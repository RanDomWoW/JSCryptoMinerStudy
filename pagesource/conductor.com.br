<script>
  $(function () {
    ContagemFraudes();
  });

  function ContagemFraudes() {
    var InicioAno = 2016;
    var TotalMes = 3300;
    var TotalAno = 12 * TotalMes;
    var ano = new Date().getFullYear();
    var mes = new Date().getMonth();
    var dia = new Date().getDate();

            //Total ate o ano passado
            var Total = (ano - InicioAno) * TotalAno;
            //Se começou no ano atual faz um ajuste pra nao ficar negativo
            if (Total <= 0)
              Total = 0;

            //Total ate o mes passado
            Total += (mes * TotalMes);

            //Quantidade de dias no mes
            var diasmes = new Date(ano, mes, 0).getDate();

            //Total por dia.
            var TotalDia = TotalMes / diasmes;

            //Total até ontem
            Total += (dia - 1) * TotalDia;

            var TotalSegundo = ((TotalDia / 24) / 60) / 60;

            var dt = new Date();
            var secs = dt.getSeconds() + (60 * dt.getMinutes()) + (60 * 60 * dt.getHours());

            Total += secs * TotalSegundo;

            setInterval(function () {
                Total += Math.random() * 5; //(TotalSegundo)
                $('#cnt-fraude').text(Total.toFixed(0));

              }, 800)
          }
        </script>
      
  <script type="text/javascript">
    (function ($) {
            //$('#idioma-Select').change(function () {
            //    $(this).parents("form").submit();
            //});

            // Contador
            var counter = function () {
              $('.js-counter').countTo({
                formatter: function (value, options) {
                  return value.toFixed(options.decimals);
                },
              });
            };
            var counterWayPoint = function () {
              if ($('#numbers').length > 0) {
                $('#numbers').waypoint(function (direction) {

                  if (direction === 'down' && !$(this.element).hasClass('animated')) {
                    setTimeout(counter, 400);
                    $(this.element).addClass('animated');
                  }
                }, { offset: '90%' });
              }
            };
            counterWayPoint();

            // Carousel Home Banner
            var ownCarouselHomeBanner = function () {
                var owl = $('.owl-carousel-home-banner');

              owl.owlCarousel({
                items: 1,
                autoplay:true,
                loop: true,
                mouseDrag: false,
                animateOut: 'fadeOut',
              });
            }
            ownCarouselHomeBanner();

            // Carousel Home Clients
            var ownCarouselHomeClients = function () {
              var owl = $('.owl-carousel-clients');

              owl.owlCarousel({
                autoWidth: true,
                loop: false,
                margin: 20,
                responsiveClass: true,
                dots: false,
                nav: true,
                navText: [
                '<button class="btn-arrow btn-prev"><</button>',
                '<button class="btn-arrow btn-next">></button>'
                ],
                responsive: {
                  0: {
                    items: 1,
                  },
                  600: {
                    items: 1,
                  },
                  1000: {
                    items: 6,
                  }
                }
              });
            }
            ownCarouselHomeClients();

            // Carousel Segmento
            $(".client-type__carousel-info[data-client='blue']").show();
            $("#coverflow").flipster({
              style: 'flat',
              spacing: -0.50,
              start: 0,
              buttons: true,
              keyboard: false,
              scrollwheel: false,
              nav: 'after',
              onItemSwitch: function (currentItem, previousItem) {
                $(".client-type__carousel-info").hide();

                switch ($(currentItem).attr('data-client')) {
                  case 'blue':
                  $(".client-type__carousel-info[data-client='blue']").show();
                  break;
                  case 'purpple':
                  $(".client-type__carousel-info[data-client='purpple']").show();
                  break;
                  case 'orange':
                  $(".client-type__carousel-info[data-client='orange']").show();
                  break;
                  case 'green':
                  $(".client-type__carousel-info[data-client='green']").show();
                  break;
                  default:
                  $(".client-type__carousel-info[data-client='blue']").show();
                  break;
                }
              }
            });

            // Select item
            $('#sel-idiomas.cs-select').click(function (e) {
              $(this).toggleClass("cs-active");
            });

            $('#sel-idiomas.cs-select .cs-options li').click(function (e) {
              $('#sel-idiomas.cs-select #culture').val($(this).data('value'));
              $('#formCultura').submit();
            });

            if (window.innerWidth <= 768) {
              $('#menu__wrapper').removeClass('wrapper').removeClass('container-fluid').addClass('container');
              $('.nav.navbar-nav ').addClass('navbar-right');
            }
          })(jQuery);
        </script>

