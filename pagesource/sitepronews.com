<script type="text/javascript">
jQuery(document).ready(function($) {
	
	$.each($('.ad'), function(){
		var id = $(this).data('id');
		var ad = this;
		$.get( 'http://www.sitepronews.com/getad/', { adid: id}, function(data){
			$(ad).html(data);
		});
	});

	
		$('#search_header').hide();
	$('input:radio[name=search_form]').change( function(){
		if($('input:radio[name=search_form]:checked').val() == 1){
			// Display site search form
			$('#searchform').show();
			$('#search_header').hide();
		} else {
			// Display exactseek form
			$('#searchform').hide();
			$('#search_header').show();
		}
	});
	
	$('ul.sub-menu').hide();
	$('.topnav').find('li:has(ul) > a').addClass('drop_button');
	
	// Animated DropDown Menu
	$("ul.topnav li a").mouseover(function() {

		$(this).parent().find("a.drop_button").addClass("drop_active");

		$(this).parent().find("ul.sub-menu").slideDown('fast').show();

		$(this).parent().hover(function() {

		}, function(){

			$(this).parent().find("ul.sub-menu").slideUp('fast');

			$(this).parent().find("a.drop_button").removeClass("drop_active");

		});

	});
	
	$('.single:last-child').css('margin-right', '0');
	
	// Place RSS icon inside link
	$('li.rss1 a').addClass('rss1icon');

});
</script>

 
