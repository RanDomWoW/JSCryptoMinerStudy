<script>
	//restrict content by country
	/**
	* usage: include a class called 'limit-by-country' to any html element
	* include a whitelist of allowed country ids in the same class (e.g. allow-254)
	**/
	var countryId = null;
	
	function isInArray(value, arr) {
		var l = arr.length;
		while(l--) {
			if(arr[l] == value) return true;
		}
		return false;
	}
	$(document).ready(function() {
		
		$(".limit-by-country").each(function() {
			var $this = $(this);
			var classes = $this.attr('class').replace(/\s+/, ' ').split(' ');
			var a = [];
			var l = classes.length;
			var temp;
			for(var i = 0; i < l; i++) {
				temp = classes[i].split('allow-');
				if(temp.length === 2) {
					a.push(temp[1]);
				}
			}
			if(isInArray(countryId, a)) {
				$this.addClass("is-shown").show();
				$this.removeClass("is-not-shown");
			} else {
				$this.addClass("is-not-shown").hide();
				$this.removeClass("is-shown");
			}
		});
		
		$(".limit-by-country-group").each(function() {
			var $this = $(this);
			var showDefault = true;
			$this.find(".limit-by-country").not(".allow-default").each(function() {
				//console.log($(this).attr("class"), $(this).is(":visible"));
				//if($(this).is(":visible")) {
				if($(this).hasClass("is-shown")) {
					showDefault = false;
				}
			});
			if(showDefault) {
				$this.find(".allow-default").show();
			} else {
				$this.find(".allow-default").hide();
			}
		});
	});
</script>

