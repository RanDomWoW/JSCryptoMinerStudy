					<script>
	$(document).ready(function() {
		var isBetaDomain = 0;
		var isAdmin = 0;
		var subdomain = 'lifeplus.com';
		$('.country-select-form').change(function(e) {
			
			var host = 'lifeplus.com';
			var a1 = host.split('lifeplus.com');
			
			if(a1.length > 1 || host.indexOf('lifeplus.com') !== false) {
				var selectedOption = $(this).find("option:selected");
				var sub = selectedOption.attr("data-prefix");
				var countryCode = selectedOption.attr("data-country-code");
				var languageCode = 'en';
				var request = '/';
				var a = request.split('/');
				
				if(a1[0] != sub + '.' && !isAdmin) {
					if(/(\/[a-z]{2}\/[a-z]{2}\/)/.exec(request) || /(\/[a-z]{2}\/[a-z]{2}$)/.exec(request) || /(\/[a-z]{2}-[a-z]{2}\/[a-z]{2}\/)/.exec(request) || /(\/[a-z]{2}-[a-z]{2}\/[a-z]{2}$)/.exec(request)) {
						var hasReplaced = false;
						var l = a.length;
						if(l > 5) {
							l = 5;
						}
						while(l--) {
						//for(var i = 0; i < l; i++) {
							var i = l;
							if(!hasReplaced && a[i].length == 2) {
							//if( (a[i].length == 5 && /([a-z]{2}-[a-z]{2})/.exec(a[i])) || (a[i].length == 8 && /([a-z]{2}-[a-z]{2}-[a-z]{2})/.exec(a[i])) ) {
								//a[i] = countryCode + '/' + languageCode;
								a[i] = languageCode;
								a[i - 1] = countryCode;
								hasReplaced = true;
							}
						}
					} else {
						if(a[1].indexOf('-') == -1) {
							a.splice(2, 0, countryCode + '/' + languageCode);
						} else {
							a.splice(1, 0, countryCode + '/' + languageCode);
						}
					}
					/*
					if(isBetaDomain) {
						if(sub == 'uk') {
							sub = 'ww1';
						} else if(sub == 'us') {
							sub = 'ww2';
						}
					}
					*/
					request = a.join('/');
					window.location = 'https://' + sub + '.lifeplus.com' + request;
					
				} else {
					$(this).submit();
				}
			}
		});
	});
</script>

