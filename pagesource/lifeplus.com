<script>
	$(document).ready(function() {
		if(!$.cookie("__dcn")) {
			$("#cookie-notice").show('slow');
		}
		$("#cookie-notice .close-button").click(function() {
			$.cookie("__dcn", "true"); //__dcn = Dismiss Cookie Notice
			$("#cookie-notice").hide('slow');
		});
	});
</script>
<script>
	/**
	 *
	 *  Dual View Slideshow code:
	 *
	 **/
	 
	$(document).ready(function() {
	
		var slideshowCount = 0;
		var $dualViews = $(".dual-view-slideshow");
		
		$dualViews.each(function() {
		
			var $thisSlideshow = $(this);
			var controlCount = 0;
			var $pageButtons = $thisSlideshow.find(".page-buttons");
			var $leftArrow = $thisSlideshow.find(".left-arrow-button");
			var $rightArrow = $thisSlideshow.find(".right-arrow-button");
			
			/**
			 * Image Group Methods
			 * - used to find where an image is relative to another image
			 *   in order to determine animation directions
			 **/
			var $imageGroup = $thisSlideshow.find(".image-group");
			var imageIndex = function(src) {
				/* function used to find where a particular image 'src' is referenced in the .image-group */
				var $images = $imageGroup.find("img");
				var l = $images.length;
				for(var i = 0; i < l; i++) {
					if($($images[i]).attr("src") === src) {
						return i;
					}
				}
				return -1;
			}
			var animationDirection = function(src1, src2) {
				/* uses the above defined imageIndex() to determine the animation direction */
				if( imageIndex(src1) - imageIndex(src2) > 0 ) {
					return ["right", "left"];
				} else {
					return ["left", "right"];
				}
			}

			/**
			 * Info Group Methods
			 * - used to find where the HTML text is relative to the other HTML quotes
			 *   in order to determine animation directions
			 **/
			var $infoGroup = $thisSlideshow.find(".info-group");
			var infoIndex = function(src) {
				/* function used to find where a particular image 'src' is referenced in the .info-group */
				var $infos = $infoGroup.find(".info");
				var l = $infos.length;
				for(var i = 0; i < l; i++) {
					if($($infos[i]).attr("data-img") === src) {
						return i;
					}
				}
				return -1;
			}
			var verticalAnimationDirection = function(src1, src2) {
				/* uses the above defined infoIndex() to determine the animation direction */
				if( infoIndex(src1) - infoIndex(src2) > 0 ) {
					return ["down", "up"];
				} else {
					return ["up", "down"];
				}
			}
			
			var pageTo = function(src) {
				var $selected = $pageButtons.find(".selected");
				var selectedSrc = $selected.attr('data-img');
				var direction = animationDirection(src, selectedSrc);
				var directionVertical = verticalAnimationDirection(src, selectedSrc);
				$imageGroup.find("img[src='" + selectedSrc + "']").hide(
					"slide",
					{
						direction: direction[1], 
						easing: $thisSlideshow.attr('data-easing-method')
					}, 
					Number($thisSlideshow.attr('data-transition-length'))
				);
				$imageGroup.find("img[src='" + src + "']").show(
					"slide",
					{
						direction: direction[0], 
						easing: $thisSlideshow.attr('data-easing-method')
					}, 
					Number($thisSlideshow.attr('data-transition-length'))
				);
				$infoGroup.find(".info[data-img='" + selectedSrc + "']").hide(
					"slide",
					{
						direction: directionVertical[1], 
						easing: $thisSlideshow.attr('data-easing-method')
					}, 
					Number($thisSlideshow.attr('data-transition-length'))
				);
				$infoGroup.find(".info[data-img='" + src + "']").show(
					"slide",
					{
						direction: directionVertical[0], 
						easing: $thisSlideshow.attr('data-easing-method')
					}, 
					Number($thisSlideshow.attr('data-transition-length'))
				);
				$selected.removeClass("selected");
				$pageButtons.find(".button[data-img='" + src + "']").addClass("selected");
			}
			var getPreviousSrc = function() {
				var selectedSrc = $pageButtons.find(".selected").attr('data-img');
				var index = imageIndex(selectedSrc);
				var $images = $imageGroup.find("img");
				if(index === 0) {
					return $($images[$images.length - 1]).attr('src');
				}
				var l = $images.length;
				for(var i = 0; i < l; i++) {
					if($($images[i]).attr("src") === selectedSrc) {
						return $($images[i - 1]).attr('src');
					}
				}
			}
			var getNextSrc = function() {
				var selectedSrc = $pageButtons.find(".selected").attr('data-img');
				var index = imageIndex(selectedSrc);
				var $images = $imageGroup.find("img");
				if(index === $images.length - 1) {
					return $($images[0]).attr('src');
				}
				var l = $images.length;
				for(var i = 0; i < l; i++) {
					if($($images[i]).attr("src") === selectedSrc) {
						return $($images[i + 1]).attr('src');
					}
				}
			}
			var pageBack = function() {
				pageTo(getPreviousSrc());
			}
			var pageForward = function() {
				pageTo(getNextSrc());
			}
			
			$leftArrow.click(pageBack);
			$rightArrow.click(pageForward);
			
			/**
			 * Set up the conrol buttons for individual slides (the rouund buttons that click to an image)
			 **/
			$imageGroup.each(function() {
				var $thisImageGroup = $(this);
				var $img = $thisImageGroup.find("img");
				$img.each(function() {
					var $thisImage = $(this);
					var href = $thisImage.attr("data-href");
					if(href) {
						$thisImage.attr("style", "cursor: pointer");
						$thisImage.click(function() {
							window.location.href = href;
						});
					}
					var $btn = $('<div class="button"></div>');
					var src = $thisImage.attr('src');
					$btn.attr('data-img', src);
					if(controlCount === 0) {
						$btn.addClass("selected");
						$thisSlideshow.find(".info[data-img='" + src + "']").show();
					} else {
						$thisImage.hide();
						$thisSlideshow.find(".info[data-img='" + src + "']").hide();
					}
					$pageButtons.append($btn);
					$btn.click(function() {
						pageTo($(this).attr("data-img"));
					});
					++controlCount;
				});
			});
			var isMouseOver = false;
			$thisSlideshow.mouseover(function() { isMouseOver = true; });
			$thisSlideshow.mouseout(function() { isMouseOver = false; });
			
			setInterval(function(){
				if(!isMouseOver) {
					pageForward();
				}
			}, $thisSlideshow.attr("data-timer-interval"));
			++slideshowCount;
			
		});
	});
</script>

