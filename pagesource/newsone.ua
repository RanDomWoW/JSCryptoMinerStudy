           $(div).css('display', 'block').animate({opacity: 1}, 200); 
         });
     });

      close.click( function(){ 
        modal.animate({opacity: 0}, 200, 
         function(){ 
           $(this).css('display', 'none');
           overlay.fadeOut(200); 
         }
         );
      });
    });

  });
</script>

