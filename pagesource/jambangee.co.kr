    })
    $('.arrow-right2').on('click', function(e){
        e.preventDefault()
        mySwiper2.swipeNext()
    })
    $('.swiper-main').on('mouseover',function() { mySwiper2.stopAutoplay(); }); $('.swiper-main').on('mouseout',function() { mySwiper2.startAutoplay(); });
</script>

