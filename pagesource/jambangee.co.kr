        mySwiper.swipeNext()
    })
    var mySwiper2 = new Swiper('.swiper-main',{
        pagination: '.pagination',
        loop:true,
        grabCursor: true,
        paginationClickable: true,
        autoplay: 6000,
        speed: 1600,
