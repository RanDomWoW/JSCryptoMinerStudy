<script type="text/javascript">
  $(document).ready(function(){
   $("#boton").click(function () {   
   
   //   $(this).attr("src", "images/element_down.png");
	  $("#abajo").each(function() {
		imagePath = $("#boton").attr("src");
    	if(imagePath == "images/element_delete.png"){
        $("#boton").attr("src", "images/element_down.png");
	    }else{
        $("#boton").attr("src", "images/element_delete.png");        
	    }
		displaying = $(this).css("display");
        
		if(displaying == "block") {
		$(this).slideUp();
          $(this).fadeOut('slow',function() {
           $(this).css("display","none");
          });
        } else {     
    	$(this).slideDown();
          $(this).fadeIn('slow',function() {
  		   
			$(this).css("display","block");		
          });
        }
      });
    });
  });
  </script>
