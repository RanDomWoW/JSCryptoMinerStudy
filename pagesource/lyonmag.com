<script type="text/javascript">
$(function() {
  	$("#newslider #newslider_controls ul li a:not(.last)").live("mouseenter", function() {
  		if ( ! $(this).hasClass("current")) {
  			var index = $(this).data("index");
	  		// add and remove current class
	  		$("#newslider #newslider_controls ul li a").each( function() { $(this).removeClass("current"); });
	  		$(this).addClass("current");
	  		// animate the requested item
  			$("#newslider #newslider_items .newslider_item.current").stop().animate({ "top": "-736px", "opacity": 0 }).removeClass("current");
  			$("#newslider #newslider_items .newslider_item.index-"+index).stop().animate({ "top": 0, "opacity": 1 }, 300).addClass("current");
  		}
  	});

  	$("#newslider .newslider_item").live("click", function() {
  		window.location = $(this).find("a").attr("href");
  	});

  	var autoSlideUne = setInterval( function() {
  		var goto = ($("#newslider #newslider_controls ul li a.current").data("index") < 4) ? ($("#newslider #newslider_controls ul li a.current").data("index")+1) : 0;
		// add and remove current class
  		$("#newslider #newslider_controls ul li a.current.newslider_control_"+$("#newslider #newslider_controls ul li a.current").data("index")).removeClass("current");
  		$("#newslider #newslider_controls ul li a.newslider_control_"+goto).addClass("current");
  		// animate the requested item
 		$("#newslider #newslider_items .newslider_item.current").stop().animate({ "top": "-736px", "opacity": 0 }).removeClass("current");
 		$("#newslider #newslider_items .newslider_item.index-"+goto).stop().animate({ "top": 0, "opacity": 1 }, 300).addClass("current");
  	}, 8000);
});
</script>
