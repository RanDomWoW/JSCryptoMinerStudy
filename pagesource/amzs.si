                        markersD[drustva[i][4]] = [];
                        markersD[drustva[i][4]].push(markerD);
                    } else {
                        // exist
                        markersD[drustva[i][4]].push(markerD);
                    }

                    // Allow each marker to have an info window
                    google.maps.event.addListener(markerD, 'click', (function (markerD, i) {
                        return function () {
                            infoWindowD.setContent(drustva[i][3]);
                            infoWindowD.open(mapD, markerD);
                        }
                    })(markerD, i));

                    // Automatically center the map fitting all markers on the screen
                    mapD.fitBounds(boundsD);
