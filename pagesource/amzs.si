                });

                google.maps.event.addListener(map, 'click', function (e) {
                    infoWindow.close();
                });

                function showHideMarkers(storitveIds) {
                    if (storitveIds.length != 0) {
                        for (var key in markers) {
                            var isVisible = markers[key].getVisible();
                            var count = 0;

                            keyArr = markers[key]['id'];
                            for (var i = 0; i < storitveIds.length; i++) {
                                if (keyArr.indexOf(storitveIds[i].toString()) > -1) count++;
                            }

                            if (count == storitveIds.length) {
                                markers[key].setVisible(true);
                            } else {
                                markers[key].setVisible(false);
                            }
                        }
                    } else {
                        for (var key in markers) {
                            markers[key].setVisible(true);
                        }
                    }
                }

                // filter
                jQuery('.selectOverlay ul li:not(.more) a').on('click', function (e) {
                    e.preventDefault();
                    var thisElem = jQuery(this);
                    var id = parseInt(thisElem.parent().attr('id'));
                    var index = storitveIds.indexOf(id);

                    if (thisElem.parent().hasClass('selected')) {
                        thisElem.parent().removeClass('selected');
                        // remove id from array
                        if (index > -1) storitveIds.splice(index, 1);
                    } else {
                        thisElem.parent().addClass('selected');
                        // add id to array
                        if (index == -1) storitveIds.push(id);
                    }
                    showHideMarkers(storitveIds);

                    if (jQuery('.selectOverlay li:not(.more)').length == jQuery('.selectOverlay li.selected').length){
                        jQuery('.selectOverlay li.more a span').text('Počisti vse');
                    } else {
                        jQuery('.selectOverlay li.more a span').text('Izberi vse');
                    }
                });

                //select all
                jQuery('.selectOverlay ul li.more a').on('click', function (e) {
                    e.preventDefault();
