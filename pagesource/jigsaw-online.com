		    onefromgroup: {
		      fn: function (value, requirement) {

		         var count = false;
		         $("."+requirement).each(function() {
		         	if($(this).val().length > 0) {
		         		var ParsleyField = $("."+requirement).not($(this)).parsley();
		         		window.ParsleyUI.removeError(ParsleyField, "onefromgroup");
		         		$("."+requirement).not($(this)).removeClass("parsley-error");
		         		count = true;
		         	}
		         });

		         return count;

		      },
		      priority: 32
		    },
			cardexpirydate: {
				fn: function (value, dates) {

					var month = parseInt($('select[data-js-expiry-date="month"]').val(), 10),
