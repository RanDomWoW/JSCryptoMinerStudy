<style>
    .error-window{
        font-family:'Open Sans';
        width:540px;
    }
    .error-window .modal-content{
        border-radius:0 !important;
        border:none;
        border-top:3px solid #bf151e;
    }
    .error-window .modal-header h4{
        font-weight:bold;
        font-size:15px;
        text-transform:uppercase !important;
    }
    .error-window .error-text,
    .error-window .error-text label{
        color:#bf151e;
        font-size:14px !important;
        overflow:hidden;
        font-weight:normal !important;
    }
    .error-window .error-text .image{
        padding-right:15px;
    }
    .error-window .error-text .info{
        color:black;
        font-size:14px;
    }
    .error-window .control .btn{
        font-size:14px;
        text-transform:uppercase;
        border-radius:0 !important;
        border:none;
        font-weight:bold;

    }
        .error-window .control .send {
            box-shadow: #800e14 2px 2px;
            background-color: #bf151e;
            color: white;
        }
        .error-window .control .cancel {
            box-shadow: #cccccc 2px 2px;
            background-color: #e6e6e6;
            color: #808080;
            float:right;
            margin-right:2px;
        }
        #inptext:active{
            border:1px solid #ccc2a3;
        }
        #inptext:hover{
            border:1px solid #808080;
        }
</style>


    <div id="mistakeOnPage" class="modal fade" role="dialog">
        <div class="modal-dialog error-window">
            <div class="modal-content">
                <div class="modal-header">

                    <h4 class="modal-title">Орфографическая ошибка в тексте</h4>
                </div>
                <div class="modal-body">
                    <div class="error-text" style="display:table">
                        <div style="display:table-row">
                            <div class="image" style="display:table-cell;vertical-align:top">
                                <img alt="" src="/img/info.png" />
                            </div>
                            <div style="display:table-cell">
                                <div class="text" style="margin-bottom:15px;">
                                    <label id="txtError"></label>
                                </div>
                                <div class="info" style="margin-bottom:15px;">
                                    <b>Послать сообщение об ошибке автору?</b><br />
                                    Ваш браузер останется на той же странице.
                                </div>

                                <p><span style="font-size:11px;color:#808080">Комментарий для автора (необязательно)</span><br/ >
                                <textarea id="inptext" name="comment" style="border:1px solid #cccccc;font-size:13px;width:100%;color:black"></textarea></p>

                                <div class="desc" style="font-size:13px;color:black;margin-bottom:15px;">
                                    <b>Спасибо!</b> Ваше сообщение будет направлено администратору сайта, для его дальнейшей проверки и при необходимости, внесения изменений в материалы сайта.
                                </div>

                                <div class="control" style="margin-bottom:5px;">
                                    <button id="submit" type="button" class="btn btn-default send" data-dismiss="modal">Сообщить</button>
                                    <button type="button" class="btn btn-default cancel" data-dismiss="modal">Отмена</button>
                                </div>
                            </div>
                        </div>
                        
                    </div>
                </div>

            </div>

        </div>
    </div>

    
    <div class="modal fade" id="responseError" role="dialog">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">                
                <div class="modal-body">
                    
                    <label id="responseErrorText"></label>
                </div>                
            </div>
        </div>
    </div>

    <script>
        $(document).ready(function () {
            $("#submit").on("click", function () {
                CallHandler();
            });

            function CallHandler() {
                var _data = { "comment": $("#inptext").val(),"error":$("#txtError").text() };
                $.ajax({
                    url: "/MistakeOnPage.ashx",
                    contentType: "application/json; charset=utf-8",
                    type: 'POST',
                    dataType: "json",
                    data: JSON.stringify(_data),
                    success: OnComplete,
                    error: OnFail
                });
                return false;
            }

            function OnComplete(result) {
                //debugger;
                $("#responseErrorText").text("Спасибо за помощь.")
                $("#responseError").modal("show");
                setTimeout(function () {
                    $("#responseError").modal("hide");
                }, 3000);
                //alert(result);
            }
            function OnFail(result) {
                //debugger;
                $("#responseErrorText").text("Произошла ошибка. Попробуйте еще раз")
                $("#responseError").modal("show");
                setTimeout(function () {
                    $("#responseError").modal("hide");
                }, 3000);
                //alert('Request Failed');
            }

            // функция для получение выделенного текста
            function getSelectedText() {
                var text = "";
                if (window.getSelection) {
                    text = window.getSelection();
                } else if (document.getSelection) {
                    text = document.getSelection();
                } else if (document.selection) {
                    text = document.selection.createRange().text;
                }
                return text;
            }

            var isCtrl = false;
            $(document).keyup(function (e) {
                if (e.which == 17) isCtrl = false;
            }).keydown(function (e) {
                if (e.which == 17) isCtrl = true;
                if (e.which == 13 && isCtrl == true) {
                    // получаем и показываем выделенный текст
                    //alert(getSelectedText());
                    if (getSelectedText() != '') {
                        $("#txtError").text(getSelectedText());
                        $("#mistakeOnPage").modal("show");
                    }
                }
            });

        });
    </script>
