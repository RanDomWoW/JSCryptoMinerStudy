  background: #0c7fb0;
}
#cssmemu ul ul li:hover > a {
  color: #ffffff;
}
#cssmemu .has-sub .has-sub:hover > ul {
  display: block;
}
#cssmemu .has-sub .has-sub ul {
  display: none;
  position: absolute;
  left: 100%;
  top: 0;
}
#cssmemu.align-right .has-sub .has-sub ul,
#cssmemu.align-right ul ul ul {
  left: auto;
  right: 100%;
}
#cssmemu .has-sub .has-sub ul li a {
  background: #0c7fb0;
  border-bottom: 1px dotted #31b7f1;
}
#cssmemu .has-sub .has-sub ul li a:hover {
  background: #0a6d98;
}
#cssmemu ul ul li.last > a,
#cssmemu ul ul li:last-child > a,
#cssmemu ul ul ul li.last > a,
#cssmemu ul ul ul li:last-child > a,
#cssmemu .has-sub ul li:last-child > a,
#cssmemu .has-sub ul li.last > a {
