								//console.log("Calling loadfile ", url, img);
								var xhr = new XMLHttpRequest();
								xhr.open("GET", url);
								xhr.responseType = "arraybuffer";
								xhr.onreadystatechange = function(){
									//console.log("readystatechange: ", xhr);
