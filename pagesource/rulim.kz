<script>
$(document).ready(function(){
	$("#vin_search_popup_button").click(function(){
		if ($("#vin_search_popup_container").attr("is_open") == "0"){
			$("#vin_search_popup_container").animate({
				marginLeft: "0px"
			}, 800 );
			
			$("#vin_search_popup_container").attr("is_open", "1");
		}
		else{
			$("#vin_search_popup_container").animate({
				marginLeft: "-263px"
			}, 800 );
			
			$("#vin_search_popup_container").attr("is_open", "0");
		}
	});
	
	$("#close_vin_popup").click(function(){
		$("#vin_search_popup_container").animate({
			marginLeft: "-263px"
		}, 800 );
		
		$("#vin_search_popup_container").attr("is_open", "0");
	});
	
	var angle = 0;
	setInterval(function(){
		  angle+=3;
		 $("#vin_image_round").rotate(angle);
	},50);
	
});
</script>

<style>
#close_vin_popup{
	color: #fff; 
	border-bottom: 1px dashed #fff; 
	cursor: pointer;
}

#vin_image_round{
	position: absolute;
	top: 100%;
	margin: -40px 0 0 5px;
}

#vin_search_popup_container{
	position: fixed;
	top: 100%;
	margin: -300px 0 0 -263px;
	width: 300px;
	z-index: 99;
}

#vin_search_popup_button{
	height: 213px;
	width: 37px;
	background: url(/images/help_search_vin_button.png) 0% 0 no-repeat;
	cursor: pointer;
}
</style>

