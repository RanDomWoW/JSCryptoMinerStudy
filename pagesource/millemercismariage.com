    <script type="text/javascript">
        jQuery(document).ready(function () {
            jQuery('.cookieClose').click(function () {
                jQuery('.cookieBandeau').fadeOut('slow');

                if (!readCookie('authorizedCookies')) {
                    createCookie('authorizedCookies', 'true', 390);
                }
            });

            if (!readCookie('authorizedCookies')) {
                jQuery('body').on('click', '*', function () {
                    createCookie('authorizedCookies', 'true', 390);
                    return true;
                }, 5000);

                jQuery(window).scroll(function () {
                    createCookie('authorizedCookies', 'true', 390);
                });

                jQuery('.cookieBandeau').css({ display: 'block' });
            }
        });


        /*Animation de la navInvite et du header home */
        var isInAnimation = false;
        var offset = 0;
        var isInScroll = false;
        var offsetInvite = 0;
        var hdinv = jQuery('.header_invite');
        if (hdinv.length) {
            offsetInvite = hdinv.offset().top;
        }

        jQuery(document).ready(function () {
            if (jQuery('.header_invite') != null) {
                jQuery(window).scroll(function () {
                    if (jQuery(window).scrollTop() >= offsetInvite) {
                        jQuery('.header_invite').css('position', 'fixed');
                        jQuery('.header_invite').css('top', '0px');
                        jQuery('.header_invite').css('width', '100%');
                        jQuery('.header_invite').css('height', '50px');
                        jQuery('.header_invite').css('z-index', '99');
                        jQuery('#navHome').css('width', '1260px');
                        jQuery('.mmm-logo-nav').css('display', 'block');
                        jQuery('.header_invite').addClass('header_invite_shadow');
                    }
                    else {
                        jQuery('.header_invite').css('position', 'relative');
                        jQuery('.header_invite').removeClass('header_invite_shadow');
                        jQuery('.global_container div.body_container').css('margin-top', '0px');
                        jQuery('#navHome').css('width', '1010px');
                        jQuery('.mmm-logo-nav').css('display', 'none');
                    }
                });
            }

            jQuery(function () {
                jQuery("body").on("click", ".error", function () { jQuery(this).val("").removeClass("error").removeAttr("placeholder"); });
            });
        });

        function createCookie(name, value, days) {
            if (days) {
                var date = new Date();
                date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                var expires = "; expires=" + date.toGMTString();
            }
            else var expires = "";
            document.cookie = name + "=" + value + expires + "; path=/";
        }

        function readCookie(name) {
            var nameEQ = name + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ')
                    c = c.substring(1, c.length);
                if (c.indexOf(nameEQ) == 0)
                    return c.substring(nameEQ.length, c.length);
            }
            return null;
        }

        function eraseCookie(name) {
            createCookie(name, "", -1);
        }

        function ValidatorValidate(val, validationGroup, event) {
            var badBrowser = false;
            if (navigator.appName.indexOf("Internet Explorer") != -1) {
                badBrowser = true;
            }

            val.isvalid = true;
            if ((typeof (val.enabled) == "undefined" || val.enabled != false) && IsValidationGroupMatch(val, validationGroup)) {
                if (typeof (val.evaluationfunction) == "function") {
                    val.isvalid = val.evaluationfunction(val);
                    if (!val.isvalid && Page_InvalidControlToBeFocused == null &&
                        typeof (val.focusOnError) == "string" && val.focusOnError == "t") {
                        ValidatorSetFocus(val, event);
                    }
                }
                if (!val.isvalid) {
                    jQuery("#" + val.controltovalidate).addClass("error");
                    if (badBrowser) {
                        jQuery("#" + val.controltovalidate).val(val.errormessage);
                    } else {
                        jQuery("#" + val.controltovalidate).attr("placeholder", val.errormessage);
                    }
                } else {
                    if (jQuery("#" + val.controltovalidate).val() == val.errormessage && val.errormessage != "") {
                        jQuery("#" + val.controltovalidate).addClass("error");

                        if (badBrowser) {
                            jQuery("#" + val.controltovalidate).val(val.errormessage);
                        } else {
                            jQuery("#" + val.controltovalidate).attr("placeholder", val.errormessage);
                        }
                        val.isvalid = false;
                    } else {
                        jQuery("#" + val.controltovalidate).removeClass("error");
                        jQuery("#" + val.controltovalidate).removeAttr("placeholder");
                    }
                }
            }
            ValidatorUpdateDisplay(val);
        }

        jQuery(function () {
            jQuery("body").on("click", ".error", function () { jQuery(this).val("").removeClass("error").removeAttr("placeholder"); });
        });

    </script>
