        function ValidCompatString_Mobile(oSrc, args) {
            args.IsValid = true
            var value = args.Value;
            var len = value.length;

            if (len != 11) {
                args.IsValid = false;
            }//End IF


            var n = value.charAt(0);
            var b = value.charAt(1);
            if (n != '0' && b != '9') {
                args.IsValid = false;
            }

        }
        function ValidCompatString_Tell(oSrc, args) {
            args.IsValid = true
            var value = args.Value;
            var len = value.length;

            if (len != 11) {
                args.IsValid = false;
            }//End IF


            var n = value.charAt(0);

            if (n != '0') {
                args.IsValid = false;
            }

        }
