		check_chinaPhone:function(phone) //检测中国电话号码
		{
			var myreg = /1[3-8]+\d{9}/;
            var myreg = /^[\d\+\-\s]*$/;
		    if(!myreg.test(phone))
		    {
		    	user_data._err("mulPhone","手机格式错误，请重新填写");
			 	return false;
		    }
		    else
		    {
		    	return true;
		    }
		},
		check_otherPhone:function(phone) //检测外国电话号码
		{
			if($.trim(phone) == "")
			{
				user_data._err("mulPhone","手机号不能为空");
