<script type="text/javascript">
$(document).ready(function() {
	
	$("#login").click(function() {
	
		var action = $("#form1").attr('action');
		var form_data = {
			username: $("#username").val(),
			password: $("#password").val(),
			is_ajax: 1
		};
		
		$.ajax({
			type: "POST",
			url: action,
			data: form_data,
			success: function(response)
			{
				if(response == 'success')
					$("#form1").slideUp('slow', function() {
						$("#message").html("<div id='authMessage' class='success-msg'>ورود موفقیت آمیز بود ... در حال انتقال به پنل کاربری !</div>");
setTimeout(function() {
  window.location.href = "desktop/index.php";
}, 2000);

					});
				else
					$("#message").html("<div id='authMessage' class='message'>نام کاربری یا رمز عبور اشتباه است.</div>");	
			}
		});
		
		return false;
	});
	
});
</script>
