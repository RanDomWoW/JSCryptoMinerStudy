<script type="text/javascript">
function getCookie(c_name)
{
var i,x,y,ARRcookies=document.cookie.split(";");
for (i=0;i<ARRcookies.length;i++)
{
  x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
  y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
  x=x.replace(/^\s+|\s+$/g,"");
  if (x==c_name)
    {
    return unescape(y);
    }
  }
}

function setCookie(c_name,value,exdays)
{
var exdate=new Date();
exdate.setDate(exdate.getDate() + exdays);
var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
document.cookie=c_name + "=" + c_value;
}

function showframe(){
document.getElementById("tnchatdiv").style.display = "inline"
document.getElementById("tnchatiframe").src = "http://webchat.irchighway.net/?channels=turkernation"
setCookie("framestate",document.getElementById("tnchatdiv").style.display,365)
document.getElementById("togglechatlink").innerHTML = "Hide chat app"
}

function hideframe(){
document.getElementById("tnchatdiv").style.display = "none"
setCookie("framestate",document.getElementById("tnchatdiv").style.display,365)
document.getElementById("togglechatlink").innerHTML = "Click here to view the chatroom"
}

function loaded(){

if (getCookie("framestate") === undefined){
setCookie("framestate",document.getElementById("tnchatdiv").style.display,365)
}

document.getElementById("togglechatlink").onclick = 
function(){
if (getCookie("framestate") === "none"){
showframe()
}
else{
hideframe()
}
return false;
}

if (getCookie("framestate") === "inline"){
showframe()
}
else{
hideframe()
}
}

if (window.addEventListener){
window.addEventListener('load',loaded,false)
}
else {
window.attachEvent("onload",loaded)
}

</script>

