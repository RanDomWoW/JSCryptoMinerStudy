  function getNewsLetter(obj) {
    emailRE = /^([\w]+)(.[\w]+)*@([\w-]+\.){1,5}([A-Za-z]){2,4}$/;
    email = document.getElementById("newsletter_email").value;
    // checks the email using the regular expression above
    if ( !emailRE.test(email) ) {
      document.getElementById('errorNewsLetterFill').style.display = "";
      intervalID = setInterval ( "clearMessage()", 3000 );
    } else {
      document.getElementById('errorNewsLetterFill').style.display = "none";
      var poststr = "email=" + email;
      makeRequest('service/ajax/serv_newsletterAdd.php', poststr, successNewsLetter);
    }
  }

  function clearMessage() {
    clearInterval(intervalID);
    document.getElementById("errorNewsLetterFill").style.display = "none";
    document.getElementById("errorNewsLetterAdd").style.display = "none";
    document.getElementById("successNewsLetterAdd").style.display = "none";
  }
</script>


