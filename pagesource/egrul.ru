		   }
		   
		   if (!checkEmailOfflineCountry()) {
	            errorsList += "<br /> - Заполните электронный адрес";
	       }
		   
		   var cDogovorObj = $("#emailPanel input[name=getDogovor]");
           if (cDogovorObj.length > 0) {
        	   if( !cDogovorObj.prop("checked") ) {
        		   errorsList += "<br /> - Вы должны согласиться с договором-офертой";   
        	   }
        	   
        	   
           }
		   
		   if (errorsList == "" && !sendOfflineCountryFormInProgress) {
			   
		   sendOfflineCountryFormInProgress = true;
		   $(".butSubmitWrap").addClass("showPreloader");
			   
		   var dataArr = formObj.serializeArray();
		   
	      dataArr.push(
	    		{ name: 'method', value: 'ajax' },
	         { name: '_action', value: 'sendOfflineCountry' },
	         { name: 'cEmail', value: cEmail }
	      );
	      
	      var urlAjax = '/index.html?method=ajax';
	      if (sourceType == 1) urlAjax = '/ebr.html?action=searchF&fa=1';
	      
		   $.ajax({
	            url: urlAjax,
	            type: "POST",
	            data: dataArr,
	            dataType: "json",
	            success: function(res) {
	                if (res.success) {
	                	$("#emailPanel").hide();
	                	$(".butSubmitWrap").show();
	                	//$.fallr.show({ content: "Ваша заявка принята"});
	                }
	                
	                if (res.messText) {
	                	eval(res.messText);
	                }
	            },     
	            complete: function() {
	            	sendOfflineCountryFormInProgress = false;
	            	$(".butSubmitWrap").removeClass("showPreloader");
	            },
	            error: function() {
	               //alert('Ошибка обработки данных. Попробуйте повторить попытку.')
	            },
	            cache:false
	      });
		   }
		   
		   if (errorsList != "") {
				$("#formDocError").html(" Внимание:" + errorsList);
				$("#formDocError").slideDown("slow");
