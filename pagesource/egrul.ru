<script>
var mouseX = 0;
var mouseY = 0;
var curPopupNum = 0;
var hidePopupTimerID = null;

    getMouseXY = function(e) {
        if (!e) e = window.event;

        if ( e.pageX == null && e.clientX != null ) {
            var doc = document.documentElement, body = document.body;
            e.pageX = e.clientX + (doc && doc.scrollLeft || body && body.scrollLeft || 0) - (doc.clientLeft || 0);
            e.pageY = e.clientY + (doc && doc.scrollTop || body && body.scrollTop || 0) - (doc.clientTop || 0);
        }

        mouseX = e.pageX;
        mouseY = e.pageY;
    };
    
    setPopup = function(e, curLineObj, num) {
       $(curLineObj).addClass("selected");
       
       clearTimeout(hidePopupTimerID);
       getMouseXY(e);

       if (num != curPopupNum) {
          if (curPopupNum) {
             if (curPoputObj = document.getElementById("mainPopupDiv" + curPopupNum)) {
                curPoputObj.style.display = 'none';
             }
          }
       
          curPopupNum = num;       
          if (popupDivObj = document.getElementById("mainPopupDiv" + num)) {
             popupDivObj.style.display = "block";
             popupDivObj.style.right = "10px";
          }
       }
    }
    
    hidePopup = function() {
       if (popupDivObj = document.getElementById("mainPopupDiv" + curPopupNum)) {
          popupDivObj.style.display = "none";
          curPopupNum = 0;
       }
    }
    
    timerHidePoput = function(curLineObj) {
       $(curLineObj).removeClass("selected");
       hidePopupTimerID = setTimeout(function() {hidePopup();}, 1000);
    }

</script>





