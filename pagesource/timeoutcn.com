    function ajaxLoadMoreBlogs() {
        var ajaxPlaceHolder = jQuery(".tile_tiny.tile_blogs .blogs .blog--step.ajax-placeholder");

        jQuery.ajax({
            type: "GET",
            cache: false,
            url: "/HttpHandlersNew/BlogVerticalBarNextPaginationHtmlHandler.ashx",
            data: { "CategoryCode": "", "PaginationOrdinal": ajaxPlaceHolder.attr("data-pagination-ordinal") },
                success: function (htmlSegment) {
                    if (htmlSegment == "")
                        ajaxPlaceHolder.hide();
                    else{
                        ajaxPlaceHolder.replaceWith(htmlSegment);
                        jQuery(".tile_tiny.tile_blogs .blogs").on("scroll", checkIfLoadMoreBlogs);
                    }
                },
                error: function () {
                    ajaxPlaceHolder.hide();
                },
                beforeSend: function () {
                    jQuery(".tile_tiny.tile_blogs .blogs").off("scroll");
                    ajaxPlaceHolder.show();
                }
                
           });
        }
</script>
 
