<script type="text/javascript">
(function() {
  var globals_socket;
  var total_article_count_interval;
  var globals_connected = function() {
    this.emit("join");
    this.emit("global", "total_article_count");
  };

  var on_global_variable = function (msg) {
    if ("total_article_count" in msg) {
      var total_article_count = msg["total_article_count"];
      total_article_count = total_article_count.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")
      document.getElementById('article_num').innerText = total_article_count;
      total_article_count_interval = setTimeout(function(){
        globals_socket.emit("global", "total_article_count");
      }, 1000);
    }
  };

  globals_socket = io('/globals');
  globals_socket.on('connect', globals_connected);
  globals_socket.on('global', on_global_variable);
})();
</script>
