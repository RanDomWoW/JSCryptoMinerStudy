function updateHeadInfo(){
	$.ajax({
		type: "post",
		url: "/user/loginStatus",
		success: function(user){
			if(!user.status){
                $("#loginZtAfter").hide();
                $("#loginZt").show();
			}else{
                $(".car_nub").text(user.courseNum);
                $("#loginZt").hide();
				$("#realNameHeader").text(user.username);
				$("#loginZtAfter").show();
				if(user.imgUrl){
					$("#userPhotoHeader").attr("src",user.imgUrl);
				}
			}
		}
	});
}

