        function autoregular_font_size(){
            var i = 13;
            var o = 11;  
            var u = 12; 
            $('td.variable').attr('style','font-size:9px!important');
            $('td.datos').attr('style','font-size:9px!important');
            $('.btninei').css('font-size','9px');
            $('.preguntascat div').css('font-size','11px');

            $('#cuerpo').attr('style','font-size:'+i+'px!important');
            $('div#contenido').attr('style','font-size:'+i+'x!important');
            $('td.variable').attr('style','font-size:'+o+'px!important');
            $('td.datos').attr('style','font-size:'+o+'px!important');
            $('.btninei').css('font-size',o+'px');
            $('.resumen_indicadores').css('font-size',u+'px');
            $('.preguntascat div').css('font-size',u+'px');
            
            $('div#font_upper').click(function(){
                i = i<15?++i:15;    
                o = o<13?++o:13;  
                u = u<14?++u:14;  
                $('#cuerpo').attr('style','font-size:'+i+'px!important');
                $('div#contenido').attr('style','font-size:'+i+'x!important');
                $('td.variable').attr('style','font-size:'+o+'px!important');
                $('td.datos').attr('style','font-size:'+o+'px!important');
                $('.btninei').css('font-size',o+'px');
                $('.resumen_indicadores').css('font-size',u+'px');
                $('.preguntascat div').css('font-size',u+'px');
            });  

            $('div#font_lower').click(function(){
                i = i>13?--i:13;    
                o = o>11?--o:11;  
                u = u>12?--u:12;      
                $('#cuerpo').attr('style','font-size:'+i+'px!important');
                $('div#contenido').attr('style','font-size:'+i+'x!important');
                $('td.variable').attr('style','font-size:'+o+'px!important');
                $('td.datos').attr('style','font-size:'+o+'px!important');
                $('.btninei').css('font-size',o+'px');
                $('.resumen_indicadores').css('font-size',u+'px');
                $('.preguntascat div').css('font-size',u+'px');
            });      
        } 

