            function corregirMenu(){
                var n = 0;
                var $leftmenu = $("#menuLateral");
                $leftmenu.find("a.primernivel").each(function(){

                    if($(this).parent().find(".current_link").length || $(this).find(".current_link").length) {
                                                $(this).next().slideUp('normal');

                        $(this).attr('id','first');
                        $(this).children().attr('class', 'menuActivo');
                        $(this).next().slideUp('normal');
                        n++;
                    }else {
                        $(this).attr('id','second');
                        $(this).next().hide(0);
                    }
                });
                if(n==0){
                    $leftmenu.find("a.primernivel:first").attr('id','first');					
