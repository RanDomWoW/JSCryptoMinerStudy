    $.ajax({
        url: url,
        data: {},
        dataType: 'jsonp',
        jsonp: 'callback',
        jsonpCallback: 'callback',
        success: function(data) {
            var itera=0;
            $( data['channel']['days'][0]['events']).each(function(index, programma) {
                itera++;
                if(itera<6){
                    var program_name=programma.name;
                    if(program_name.length>35){
                       program_name=program_name.substring(0,35)+'...';
                    }

                $('#print_tv_show').append("<p><span>"+programma.time.substring(0,5)+" </span>    "+program_name+"</p>");}

            });

        }
    });
</script>
