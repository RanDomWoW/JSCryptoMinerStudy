                            }

                            if (item.region) {
                                countryName += item.region;
                            }

                            if (item.country) {
                                countryName += countryName == '' ? item.country : ', ' + item.country;
                            }

                            return '<p class="--new-select__city-name">' + cityName + '</p><p class="citysearch__new-select__country-name">' + countryName + '</p>';
                        }
                    }
