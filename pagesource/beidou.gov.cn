    	}else{
    	   $(this).attr('name','none');
           $('.fieldUlBox ul').fadeOut();
    	} 
    })   
    
   $('.fieldUlBox ul li a').click(function(){
        $('.fieldUlTopA span').html($(this).html())
        $('.fieldUlBox .fieldUlTopA').attr('name','none');
        $('.fieldUlBox ul').fadeOut();
   }) 

</script>

