      var time = 20000;
      var allLong = 0;
      //在复制节点之前，统计所有图片的宽度
      $(".latestNewsRD ul li").each(function(){
        //侦察
        allLong = allLong + $(this).outerWidth(true);
      });

      //复制所有的li
      $(".latestNewsRD ul li").clone().appendTo(".latestNewsRD ul");
      dongyici(time); //调用函数

      //console.log(allLong);

      $(".latestNewsRD").mouseenter(function(){
        $(".latestNewsRD ul").stop();
      });

      $(".latestNewsRD").mouseleave(function(){
        //剩余的动画分配多少时间？
        //按比例来！
        //如果现在还没有走的路程，比较短；那么就分配剩余的时间少一些。
        //如果现在还没有走的路程，比较长；那么就分配剩余的时间长一些。
        //已经走过的路程。取绝对值。
        // Math.abs 绝对值
        // parseFloat 去掉px单位
        var x = Math.abs(parseFloat($(".latestNewsRD ul").css("left"))); 
        var t = (allLong - x) / allLong * time; //剩余应该分配的时间
        dongyici(t);
      });

      function dongyici(shijian){
        //开跑
        $(".latestNewsRD ul").animate({"left":-allLong},shijian,"easieLinear", function(){
          $(this).css("left",0);
          dongyici(time); //迭代。自己调用自己。
        });
      }
})


