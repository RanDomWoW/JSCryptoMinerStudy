        var isCmsDropdown = $row.parents( ".dropdown" ).attr( "id" ) != "productsNavBar";
        var minHeight;

        if ( isCmsDropdown ) {
            minHeight = $row.parents( "ul" ).parents( ".cms_section" ).parents( ".dropdown-menu" ).outerHeight() - 4;
        } else {
            $submenu.css( {
                width: columns * 200 + 320
            } );

            minHeight = height - 4
            $submenu.find( ".subcategories" ).css( {
                width: columns * 185 + 10,
                height: minHeight
            } );

            if ( $submenu.find( ".featuredInDD" ).length <= 0 ) {
                $submenu.find( ".regularSubcategories" ).css( {
                    width: columns * 185 + 10,
                    height: minHeight
                } );
            } else {

                var numberOfFeaturedCats = $submenu.find( ".featuredInDD" ).children( "div.subMenu" ).length;
                $submenu.find( ".regularSubcategories" ).css( {
                    width: columns * 185 + 10,
                    height: minHeight - ( Math.ceil( numberOfFeaturedCats / columns ) * 40 )
                } );
            }
        }

        $submenu.css( {
            display: "block",
            top: -1,
            left: ( width - 1 ) + "px",
            minHeight: minHeight,
            maxHeight: 500
        } );

        $submenu.children( "div:first-child" ).css( {
            maxHeight: 500,
            minHeight: minHeight
        } );
    }

