    function changeDropDownWidths() {

        $( "#navigationHeader_Inner.desktopNav > li" ).each( function () {
            var navSectionWidth = $( this ).outerWidth();

            if ( navSectionWidth == 0 ) {
                navSectionWidth = 200;
            }

            $( this ).children( ".dropdown-menu" ).css( "min-width", ( navSectionWidth + 4 ) + "px" ).attr( "data-setwidth", navSectionWidth );
        } );

    }

    function InitMegaMenu() {
        //**** FANCY GEOMETRY MEGA-MENU HOVER PLUGIN ***********
        setTimeout( function () {
            $( "#navigationHeader_Inner.desktopNav > .dropdown > .dropdown-menu > ul, #navigationHeader_Inner.desktopNav > .dropdown > .dropdown-menu > div.cms_section > ul" ).menuAim( {
                exitMenu: function () { return true; },
                activate: activateSubmenu,
                deactivate: deactivateSubmenu,
                tolerance: 150
            } );
        }, 500 );
    }

    function LoadNavigationBar() {
        var desktopVersion = $( "#bootstrap-visible-xs" ).is( ":hidden" );
        if ( desktopVersion ) {
            if ( $( ".desktopNav #productsNavBar #browse-nav-container" ).data( "isloaded" ) == false ) {
                $.ajax( {
                    type: "GET",
                    url: "/homes/browsenav/",
                    async: true,
                    success: function ( result ) {
                        $( ".desktopNav .browse-nav-container" ).replaceWith( result );
                        changeDropDownWidths();
                    }
                } ).done( function () {
                    $( '.desktopNav .browse-nav .popover' ).popover();
                    InitMegaMenu();
                } );
            }

            
        } else {
            if ( $( ".mobileNav #productsNavBar #browse-nav-container" ).data( "isloaded" ) == false || $( ".showcaseMobileNav" ).is( ":empty" ) ) {
                $.get("/homes/browsenav_mobile/", function ( result ) {
                    $( ".mobileNav .browse-nav-container" ).replaceWith( result );
                    changeDropDownWidths();
                } );
            }
        }
    }

    function OpenOffCanvas() {
        $( "#offCanvasContent" ).show();
        $( "body" ).addClass( "offCanvasOpen" ).append( "<div id='offCanvasOverlay'></div>" );
    }

    function CloseOffCanvas() {
        $( "body" ).removeClass( "offCanvasOpen" );
        $( "#offCanvasOverlay" ).remove();
    }

    var offCanvasClicksInitialized = false;
    function InitOffCanvasAndSwipe() {

        if ( !offCanvasClicksInitialized ) {
            $( ".mobileMenuBtn" ).live( "click", function () {
                OpenOffCanvas();
            } );

            $( "#closeOffCanvas, #offCanvasOverlay" ).live( "click", function () {
                CloseOffCanvas();
            } );
        }

        offCanvasClicksInitialized = true;

        $( 'html.touch' ).swipe( {
            swipeLeft: function () {
                CloseOffCanvas();
            },
            swipeRight: function () {
                OpenOffCanvas();
            },
            threshold: 245,
            excludedElements: ".owl-carousel, .carousel-inner, input"
        } );
    }

