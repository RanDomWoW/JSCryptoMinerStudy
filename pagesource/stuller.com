    <script>
        requireReady( function () {
            require( ["knockout", "jquery", "ui/context"], function ( ko, $, ctx ) {
                
                if ( ctx.default.CartCount() <= 50 ) {
                    ctx.default.LoadCart( function () {
                        var cartSidebarCookieName = 'showCartSidebar';
                        var cartSidebarCookie = readCookie( cartSidebarCookieName );
                        var currentScreenWidth = $( window ).width();

                        var vm = ctx.default.Cart();
                        vm.NumOfCartItemsToShow = ko.observable( 15 );
                        vm.ShowFullCartLink = ko.observable( vm.CartCount() > vm.NumOfCartItemsToShow() );

                        vm.ShowSidebar =
                            ko.observable(( cartSidebarCookie == null || cartSidebarCookie == "true" ) &&
                                currentScreenWidth > 1730 );

                        vm.ShowSidebar.subscribe( function ( value ) {
                            createCookie( cartSidebarCookieName, value, 365 );
                        } );
                        vm.Ctx = ctx.default;

                        vm.CartLabel = ko.computed( function () {
                            if (vm.Ctx.IsInJewelerShowcase() && !vm.Ctx.IsOrderApprovalByPassCheckout())
                                return "Quote";
                            else {
                                return "Cart";
                            }
                        } );

                        vm.PostProcessing = function () {
                            $( '.cartSidebar' ).affix( {
                                offset: {
                                    top: function () { return ( this.top = $( '#header' ).offset().top ) }
                                }
                            } );
                        }
                        var cartDrawerElement = document.getElementById( "cartSidebarContainer" );
                                    if (cartDrawerElement != null) {
                            ko.applyBindings( vm, cartDrawerElement );

                                        //prevent page scroll when scrolling the cart sidebar items
                                        $(".cartSidebar .fancyScroll").on('mousewheel DOMMouseScroll', function (e) {
                                            var e0 = e.originalEvent;
                                            var delta = e0.wheelDelta || -e0.detail;

                                            this.scrollTop += (delta < 0 ? 1 : -1) * 30;
                                            e.preventDefault();
                                        });
                                    }
                    } );
                }
            } );
        } );
    </script>

