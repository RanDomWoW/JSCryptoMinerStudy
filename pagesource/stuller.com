    function initResponsiveTable( table ) {
        var head_col_count = $( table ).find( 'table thead tr th' ).size();
        for ( j = 0; j <= head_col_count; j++ ) {
            var head_col_label = $( table ).find( '> table > thead th:nth-child(' + j + ')' )
                .clone()    //clone the element
                .children() //select all the children
                .remove()   //remove all the children
                .end()  //again go back to selected element
                .text();
            $( table ).find( 'table tr td:nth-child(' + j + ')' ).attr( 'data-title', head_col_label );
        }
    }

