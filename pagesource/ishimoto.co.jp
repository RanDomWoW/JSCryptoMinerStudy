function pagetopbtn(){
    var pagetopnav = jQuery('#pagetopnav');
    pagetopnav.hide();
    jQuery(window).scroll(function(){
        var sh = jQuery(document).height();
        var sp = jQuery(window).height() + jQuery(window).scrollTop();
        if ( jQuery(window).scrollTop() > 300 ) {
            pagetopnav.fadeIn(200);
            if ( (sh - sp) < 105 ) {
                var ps = 105 - (sh - sp);
                if( ps > 20 ) {
                    pagetopnav.css('bottom', ps);
                } else if (ps < 0) {
                    pagetopnav.css('bottom', '20');
                }
            }
        } else {
            pagetopnav.fadeOut(200);
        }
    });
}
