function rankLstWing(rankCode, lstPage, lstSize, searchType, searchStr){
    
    if(!rankCode) rankCode = '0';
    if(!lstPage) lstPage = '1';
    if(!lstSize) lstSize = '10';
	$.ajax({
		type : 'get',
		url : 'http://sys1.dotoritv.kr:9001/jsonp/api/cast/rankLst.asp',
		dataType : 'jsonp',
		jsonp : 'HYJSONP',
		jsonpCallback : 'HYJSONP',
		cashe : false,
		//async: false,
		data : {
			rankCode: rankCode,
			lstPage: lstPage,
			lstSize: lstSize,
			searchType : searchType,
			searchStr : searchStr
		},
		success: function (data){
		    console.log(data.rankLst.rstCode);
			if(data.rankLst.rstCode == 0){

				var rank_point = "";
				var inner_content="";
				var inner_paging=[];
				var mc_pFileName ="";
				var rank_num ="";
				var mc_pTitle="";
				var mc_code="";
				var k = makeRandom(1,11);

				for ( var i = 0; i < data.lstData.length; i++) {
                    rank_num        = parseInt((lstPage-1)*lstSize)+parseInt(i+1);
                    mc_signId       = data.lstData[i].mc_signId;
                    mc_code         = data.lstData[i].mc_partnerCode;
                    mc_recommend    = data.lstData[i].mc_recommend;
                    mc_castStatus   = data.lstData[i].mc_castStatus;
                    mc_pTitle       = data.lstData[i].mc_pTitle;
                    mc_pFileName    = data.lstData[i].mc_pFileName;
                    mc_nickName     = data.lstData[i].mc_nickName;
                    mc_rankPoint    = data.lstData[i].mc_rankPoint;
                    mc_watchCnt     = data.lstData[i].mc_watchCnt;
					

					if(mc_pFileName == "https://thumb.popcast.co.kr/POPCAST_THUMB/sys_Thumb/P-00013/defaultProfile.jpg"){
						//mc_pFileName ="/images/default_img"+makeRandom(1,11)+".png";
						mc_pFileName ="/images/default_img"+k+".png";
						k = k+1;
						if(k > 10) k = k -10;
					}

					if(mc_pTitle.length > 12){
						mc_pTitle = mc_pTitle.substr(0,12) + "...";
					}

					/* 2016.08.24 성인관련 문자열 치환 추가
					발기찬, 야한, 꼴릿한, 야성녀, 야설
					*/
					// title
                    mc_pTitle = mc_pTitle.replace("발기찬","***");
                    mc_pTitle = mc_pTitle.replace("야한","**");
                    mc_pTitle = mc_pTitle.replace("꼴릿한","***");
                    mc_pTitle = mc_pTitle.replace("야성녀","***");
                    mc_pTitle = mc_pTitle.replace("야설","**");
                    // nickname
                    mc_nickName = data.lstData[i].mc_nickName;
                    mc_nickName = mc_nickName.replace("발기찬","***");
                    mc_nickName = mc_nickName.replace("야한","**");
                    mc_nickName = mc_nickName.replace("꼴릿한","***");
                    mc_nickName = mc_nickName.replace("야성녀","***");
                    mc_nickName = mc_nickName.replace("야설","**");
                    
                    //inner_content [i] ="<ul class=\"rank_tit\"><li>순위</li><li>BJ</li><li>인기지수</li><li>시청점수</li><li>추천점수</li><li>즐겨찾기</li></ul>";
                    
                    cls_live = "";
                    disp_live = "";
					if(mc_castStatus == 0){ //방송 on
                        cls_live = "live_on";
                        disp_live = "LIVE";
					} else {  //방송 off
                        cls_live = "live_off";
                        disp_live = "OFF";
					}
					
					cls_1st_rank = "";
					if(rank_num == 1) {
					    cls_1st_rank = "rank_list_1";
					} else {
					    cls_1st_rank = "";
					}
					
					//inner_content = inner_content + "<ul class='rank_list "+cls_1st_rank+"'><li>"+rank_num+"</li><li><a href='#'><span class='"+cls_live+"'>"+disp_live+"</span><span class='rank_list_img'><img src='"+mc_pFileName+"' alt='bj이미지' /></span><span class='rank_list_tit'>"+mc_pTitle+"</span><span class='rank_list_name'>"+mc_nickName+"</span><span class='rank_list_id'>("+mc_signId+")</span></a></li><li>"+rank_point+"</li><li>"+mc_watchCnt+"</li><li>"+mc_recommend+"</li><li><button type='button' onclick=favorites('0','"+mc_code+"','"+mc_signId+"')>즐겨찾기 등록</button></li></ul>";
					
					inner_content = inner_content + "<li><a href='#' onClick=\"javascript:mychPopupLank('"+data.lstData[i].mc_signId+"','"+data.lstData[i].mc_partnerCode+"');\"><span class='side_rank_count'>"+rank_num+"</span><span class='side_rank_img'><img src='"+mc_pFileName+"' alt='bj 사진' /></span><span class='side_rank_name'>"+mc_nickName+"</span></a></li>";
				}

				//console.log(data.rankLst.rstMsg);
				if(data.rankLst.rstMsg =="순위 목록이 없습니다."){
					//inner_content=[];
					inner_content = inner_content + "<li class='no_search'>검색된 결과가 없습니다.</li>";
				}
				
				$("#rank_wing").html(inner_content);
				inner_content = "";
			}else{
				console.log(data.rankLst.rstMsg);
			}

		},
		error : function(){
			//alert('서버에 연결할 수 없습니다(r).');
			console.log('서버에 연결할 수 없습니다(r).');
		}
	});
}

