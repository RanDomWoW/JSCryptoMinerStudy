function iframe_resize_fun(){
	var old_px = 0;
	var demo_resize = function (event) {
	var tmp_data = event.data.toString();
	if(tmp_data.substring(0,3) == "UN|") {
		use_nacho(event.data);
	} else {
		var demo_iframe = document.getElementById('demo_iframe');
		var if_height = "870";
		//var if_height =  event.data;

			if (demo_iframe) {
				if(event.data >870){
				if_height =	event.data;

				}

				demo_iframe.style.height = if_height + "px";
				if(old_px != event.data) {
					window.scrollTo(0,0);
					old_px = event.data;
				}
			}
	}
	};

	if (window.addEventListener) {
		console.log(1);
		window.addEventListener("message", demo_resize, false);
	} else if (window.attachEvent) {
		console.log(2);
		window.attachEvent("onmessage", demo_resize);
	}

}

