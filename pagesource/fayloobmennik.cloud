	function accept_form() {
		if (document.getElementById('accept').checked == true) {
			document.getElementById('sending').disabled = false;
		}
		else {
			document.getElementById('sending').disabled = true;
		}
	}
	
	function beginUpload() {
		document.getElementById('sending').disabled = true;
		document.getElementById('iframe').className='nohint';
		$("#uploadprogressbar").fadeIn();

		var i = setInterval(function() {
            $.ajax({
                url: "/files_apc.php?APC_UPLOAD_PROGRESS=5ab070903387b",
