<script>
    var iam_data = {"st": "baunetz", "cp": "cp_bnw", "sv": "ke", "co": ""};

    //Dynamisch beim Seitenwechsel aufrufen
    if (typeof jQuery !== 'undefined') {
        (function ($) {
            /*
             * Slider (Artikelbilder) tracken
             * event: slick.changed
             */
            $(window).on('slick.changed', function() {
                if (typeof iam_data !== 'undefined' && typeof iom !== 'undefined') {
                    iom.c(iam_data, 1);
                }
            });

            /*
             * Seitenwechsel tracken
             */
            $(window).on('PageChange', function (e, state) {
                // SZM V. 2.0
                if (typeof iam_data !== 'undefined' && typeof iom !== 'undefined') {
                    //Infoseiten cp: inf, andere cp: cp
                    if (state.current_view == 'Newsletter' || state.current_view == 'Default') {
                        iam_data.cp = 'inf';
                    } else {
                        iam_data.cp = 'cp_bnw';
                    }
                    //Startseite extra
                    if (state.current_view == 'Home') {
                        iam_data.cp = 'bnw_mul';
                    }

                    iom.c(iam_data, 1);
                }
            });
        })(jQuery);
    } else {
        iom.c(iam_data, 1);
    }
