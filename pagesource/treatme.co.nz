function setup_shoppting_cart_countdown(d){
  var difference;
  var now = new Date();

  if($("#time-left").length == 0) {
    return;
  }
  if(typeof d != "undefined") {
    start = new Date();
    time_left = d;
    return;
  }
  difference = time_left - (now.getTime() - start.getTime())/1000;

	if(difference > 60 && difference <= 60 + shopping_cart_timer_delay/1000){
			$.ajax({
			type: 'GET',
			url: "/shopping_cart/view?current_deal_id=" + product_deal_id,
			success: function(data){ 
				$('#shopping-cart-popup-wrapper').html(data);
			}
		});
		return;
		}
    
  if(difference <= 0 && cartExpired == false) {
    if($('.cardList').html() == '') return;
    $('.cardList').html('');
    $('#shopping-cart-total-count, #shopping-cart-total-count-nav, .shopping-cart-total-count').text('0');
    $('.bex_cart_item_count').html('');
      $.ajax({
          type: 'GET',
          url: '/shopping_cart/flush/set_favourites/1',
          success: function(data){
              $.ajax({
                  type: 'GET',
                  url: "/shopping_cart/view",
                  success: function(data){
                      $('#shopping-cart-popup-wrapper').html(data);
                                            var nav_popup_html = $('#shopping-cart-popup').html()
                          .replace('id="shopping-cart-is-empty"', 'id="shopping-cart-is-empty-nav"')
                          .replace('id="continueShopping"', 'id="continueShoppingNav"');
                      if($('#shopping-cart-popup-nav').length > 0) {
                          $('#shopping-cart-popup-nav').html(nav_popup_html);
                      }
                      if ($('#cartInfoHiddenBlock').length > 0) {
                          $('#cartInfoHiddenBlock').html(nav_popup_html);
                      }
                                        }
              });
          }
      });
            tb_show("Time-expired","/layovers/get_layover?url=restore_cart_confirmation&width=540&height=250&modal=true");
      $.ajax({
          type: 'GET',
          url: "/layovers/get_layover?url=restore_cart_confirmation&width=540&height=250&modal=true",
          success: function(data){
              $("#TB_window").html('<div id="TB_ajaxContent" class="TB_modal" style="width:540px;height:245px;">'+data+'</div>');
              $("#TB_window").attr('style', 'margin-left: -285px; width: 570px; margin-top: -145px; display: block;');
          }
      });
      cartExpired = true;
  }

	var minutesDifference=Math.floor(difference/60);
	var secondsDifference=Math.floor(difference) - minutesDifference*60;
	
	var timer_array = [];
	
	timer_array['m'] = (minutesDifference < 10 ? '0'+ minutesDifference : minutesDifference);
	timer_array['s'] = (secondsDifference < 10 ? '0'+secondsDifference : secondsDifference);
	
	
	if(minutesDifference == 0){
		timer_array['m'] = '00';
		if(secondsDifference == 0){
			timer_array['s'] = '00';
		}
	}
    if (!cartExpired){
        $("#time-left, #time-left2, .js-shopping-catr-time-left").html(timer_array['m'] + ' : ' + timer_array['s']);
    }

	
	}

