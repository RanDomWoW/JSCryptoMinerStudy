<script type="text/javascript">
function shopping_cart_view() {
	if(typeof product_deal_id !== 'undefined' && app_name == "frontend"){
			var d = new Date();
			$.ajax({
				type: 'GET',
				url: "/shopping_cart/view?product_deal_id="+ product_deal_id +"&time" + d.getTime(),
				async: true,
				success: function(data){ 
					$('#shopping-cart-popup-wrapper').html(data);
				}
			});
	}
}

</script>

