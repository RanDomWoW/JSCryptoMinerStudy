        var signup_top_form_validate = false;
        
        function signup_top_form(){
            $('#subscribe-error').html('');
            $('#subscribe-success').html('');
            
            $('#subscribe-error').hide();
            $('#subscribe-success').hide();

            $('#ajax_subscribe_id').hide();
            $('#ajax_loader_subscribe').show();
            
            $.ajax({
              type: 'POST',
              url: '/subscription/validate',
              data: $('#subscribe-form').serialize(),
              success: function(data){
                var obj = $.parseJSON(data);

                $('#ajax_subscribe_id').show();
                $('#ajax_loader_subscribe').hide();
              
                if('already_subscribed' == obj.status){
                    $('#subscribe-error').show();
                    $('#subscribe-error').html(obj.message);
                }else if('success' == obj.status  || 'already_has_account' == obj.status){
                    signup_top_form_validate = true;
                    $.ajax({
                      type: 'POST',
                      url: '/subscription/ajax_subscribe',
                      data: $('#subscribe-form').serialize(),
                      success: function(data){
                        if('already_has_account' == obj.status){
                            $('#subscribe-success').show();
                            $('#subscribe-success').html(obj.message);
                        }else{
                            $('#subscribe-success').show();
                            $('#subscribe-success').html('Success subscribed');
                        }
                      },
                      statusCode: {
                		    404: function() {
                		    	location.reload();
                		    },
                		    500: function() {
                		    	location.reload();
                		    }
                		  }
                   });
                }else{
                    $('#subscribe-error').show();
                    $('#subscribe-error').html(obj.message);
                }
              },
              statusCode: {
      		    404: function() {
      		    	location.reload();
      		    },
      		    500: function() {
      		    	location.reload();
      		    }
      		  }
            });
        }
        
        $(document).ready(function(){
            $('.hover-off').each(function(number, element){
                $(element).mouseover(function(){
                    if('shopping-cart-popup' != element.id){
                        $('#shopping-cart-popup').hide();
                    }
                    $('#feedItem').removeClass('active');
                });
            });
        });
        
        $('#feedItem .header-tooltip').mouseout(function() {
            if ($(':focus').attr('name') == 'email' || $(':focus').attr('name') == 'postal_code') {
                $(this).parent().addClass('active');
                return false;
            } else {
                $(this).parent().removeClass('active');
            }
        });


