            $('#shopping-cart-popup').show();
            clearTimeout(shopping_cart_visible_timeout);
        }
        
        $('#shopping-cart-popup-wrapper').mouseout(function() {
            shopping_cart_available_hide = true;
            hide_shopping_cart();
