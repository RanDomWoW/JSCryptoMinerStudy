          <script type = "text/javascript">
function deal_time_left(placeholder, difference) {
    
    var wrapperClass = '';
    
    if(difference <= 0){
        return;
    }

    var daysDifference=Math.floor(difference/60/60/24);
    var hoursDifference=Math.floor(difference/60/60) - daysDifference*24;
    var minutesDifference=Math.floor(difference/60) - daysDifference*60*24 - hoursDifference*60;
    var secondsDifference=Math.floor(difference) - daysDifference*60*60*24 - hoursDifference*60*60 - minutesDifference*60;

    var timer_html = '';

    if(daysDifference > 0){

        timer_html = 'Available for <span class="time-left" >' + daysDifference;
        
        if(daysDifference == 1){
    		timer_html += ' more day </span>';
            wrapperClass = 'one-day-left';
        }else{
    		timer_html += ' more days </span>';
            wrapperClass = 'some-days-left';
        }
    }else{
        if(hoursDifference > 0){
            if(minutesDifference > 0){
                hoursDifference++;
            }
            timer_html = 'Available for <span class="time-left" >' + hoursDifference;
            
            if(hoursDifference == 1){
            	timer_html += ' more hour </span>';
                wrapperClass = 'one-hour-left';
            }else{
            	timer_html += ' more hours </span>';
                wrapperClass = 'some-hours-left';
            }
        }else{
            if(minutesDifference > 0) {
                var text_minutes = 'more minutes';
                if (minutesDifference == 1) {
                    text_minutes = 'more minute';
                }
                timer_html = 'Available for <span class="time-left" >' + minutesDifference + ' ' + text_minutes + ' </span>';
                wrapperClass = 'some-minutes-left';
            }else{
                timer_html = '<span class="time-left" >Less than 1 minute left</span>';
                wrapperClass = 'less-than-minute';
            }
        }
    }

    $("#"+placeholder).html(timer_html);
    $("#"+placeholder).addClass(wrapperClass);

    if(typeof deal_time_left == 'function') setTimeout("deal_time_left('" + placeholder + "', " + (difference - 1) + ")",1000);
}

function deal_time_left_alt_batch() {
  $('.deal-counter').each(function(i, e){
    e = $(e);
    var placeholder = e;
    var difference = e.attr('data-difference');
    e.attr('data-difference', difference - 1);
    deal_time_left_alt(placeholder, difference - deal_time_left_timestamp);
  });
//   if(typeof deal_time_left_alt_batch == 'function')  setTimeout("deal_time_left_alt_batch()",1000);
}

function deal_time_left_alt(placeholder, difference) {
    
    var wrapperClass = '';
    
    if(difference <= 0){
        return;
    }

    var daysDifference=Math.floor(difference/60/60/24);
    var hoursDifference=Math.floor(difference/60/60) - daysDifference*24;
    var minutesDifference=Math.floor(difference/60) - daysDifference*60*24 - hoursDifference*60;

    var timer_html = '';

    if(daysDifference > 0){
        timer_html = daysDifference;
        
        if(daysDifference == 1){
    		timer_html += ' day left';
            wrapperClass = 'one-day-left';
        }else{
    		timer_html += ' days left';
            wrapperClass = 'some-days-left';
        }
    }else{
        if(hoursDifference > 0){
            if(minutesDifference > 0){
                hoursDifference++;
            }
            timer_html = hoursDifference;
            
            if(hoursDifference == 1){
            	timer_html += ' hour left';
                wrapperClass = 'one-hour-left';
            }else{
            	timer_html += ' hours left';
                wrapperClass = 'some-hours-left';
            }
        }else{
            if(minutesDifference > 0) {
