    <script>
        var sbiVal = '';
        var IdbiVal = '';
        var payuVal = '';
        $(document).ready(function () {
            $('input[name=optgraph]').change(function () {
                if ($('input[type=radio][name=optgraph]').is(':checked') == true) {
                    var opt = $('input[type=radio][name=optgraph]:checked').val();
                    bindChartt();
                }
            });
            //bindChartt();
        });

        function bindChartt() {
            var opt = $('input[type=radio][name=optgraph]:checked').val();
            $.ajax({
                url: "Default.aspx/SucessRate_Bnk",
                type: "POST",
                data: "{'opt':'" + opt + "'}",
                contentType: 'application/json; charset=utf-8',
                dataType: 'json',
                failure: function (data, status, error) {
                    alert(data.responseText + "|" + status + "|" + error);
                },
                error: function (data, status, error) {
                    alert(data.responseText + "|" + status + "|" + error);
                },
                success: function (data) {
                    debugger;
                    var dDatat = data.d;
                    $.each(dDatat, function (i, e) {
                        debugger;
                        var BnkNm = '';
                        if (e.split('-')[0] == '0001509') {
                            BnkNm = 'SBI Aggregator';
                            sbiVal = e.split('-')[1];
                        }
                        else if (e.split('-')[0] == '0300997') {
                            BnkNm = 'IDBI Aggregator';
                            IdbiVal = e.split('-')[1];
                        }
                        else if (e.split('-')[0] == '0300999') {
                            BnkNm = 'PNB Aggregator';
                            payuVal = e.split('-')[1];
                        }
                    });
                    bindChart();
                }
            });
            return false;
        }

        function bindChart() {
            var chart = AmCharts.makeChart("chartdiv", {
                "type": "serial",
                "dataProvider": [{
                    "Bank": "SBI Aggregator",
                    "ActualVal": sbiVal,
                    "color": "#8085E9",
                    "label": sbiVal + '%'
                }, {
                    "Bank": "IDBI Aggregator",
                    "ActualVal": IdbiVal,
                    "color": "#7CB5EC",
                    "label": IdbiVal + '%'
                },
                 {
                     "Bank": "PNB Aggregator",
                     "ActualVal": payuVal,
                     "color": "#90ED7D",
                     "label": payuVal + '%'
                 }
                ],
                "valueAxes": [{
                    "ActualVal": 100,
                    "unit": "%",
                    "minimum": 0,
                    "maximum": 100,
                    "position": "left",
                    "title": "e-transaction success rate"
                }],
                "startDuration": 1,
                "graphs": [{
                    "balloonText": "e-transaction success rate in <br> [[category]] (2017): <b>[[value]]</b>",
                    "fillAlphas": 0.9,
                    "lineAlpha": 0.2,
                    "title": "2017",
                    "colorField": "color",
                    "type": "column",
                    "labelText": "[[label]]",
                    "labelPosition": "inside",
                    "valueField": "ActualVal",
                    "fixedColumnWidth": 70
                }],
                "plotAreaFillAlphas": 0.1,
                "depth3D": 50,
                "angle": 25,
                "categoryField": "Bank",
                "categoryAxis": {
                    "gridPosition": "start"
                },
                "export": {
                    "enabled": true
                }
            });
            jQuery('.chart-input').off().on('input change', function () {
                var property = jQuery(this).data('property');
                var target = chart;
                chart.startDuration = 0;

                if (property == 'topRadius') {
                    target = chart.graphs[0];
                    if (this.value == 0) {
                        this.value = undefined;
                    }
                }
                target[property] = this.value;
                chart.validateNow();
            });
        }
    </script>
