            $(document).ready(function () {
                $('.bxslider').bxSlider({
                    mode: 'fade',
                    captions: true
                });
            });
            setInterval(function () {
                var sliders = $('.slider-li');
                var first = $('.slider-li:first-child');
                var active = sliders.filter('.active'); //$('.slider.active');

                active.removeClass('active');
                var next = active.next();
                if (next.length != 0) {


                    next.addClass('active');
                } else {

                    first.addClass('active');
                }

                $('.main-slider .title').text($('.slider-li.active').find('span').text());
                $('.main-slider .leading').text(sliders.filter('.active').find('p').text());
                $('.main-slider a').attr('href', sliders.filter('.active').find('a').attr('href'));
                $('.main-slider img').attr('src', sliders.filter('.active').find('img').attr('src'));
//console.log('1');

            }
            , 8000);
            //var msinterval = setInterval(mainslider, 11000);



            setInterval(function () {
                var first = $('#adv1.ads-slider:first-child');
                var active = $('#adv1.ads-slider.active');
                var next = active.next();
                if (next.length !== 0) {
                    next.addClass('active').removeClass('hide');
                    active.removeClass('active').addClass('hide');
                } else {
                    first.addClass('active').removeClass('hide');
                    active.removeClass('active').addClass('hide');
                }
                //console.log('b');
            }
            , 8000);




