	<script>
		// Contents GET and Filter script
		(function() {
			var fetchStart = 0;
			var fetchState = true;
			var History = window.History;
			var backState = false;
			
			$(document).on('ready', function() {
				var data = History.getState().data;
				
				$('#range1').rangeSlider({
					max: 500,
					defaultMax: data.search ? data.search.maxPeople : 500,
					defaultMin: data.search ? data.search.minPeople : 0,
					step: 50,
					measure: '명',
					onBlur: function() {
	                    getList(1);
	                }
				});
				$('#range2').rangeSlider({
					max: 100000,
					defaultMax: data.search ? data.search.maxPrice : 100000,
					defaultMin: data.search ? data.search.minPrice : 0,
					step: 5000,
					measure: '원',
					onBlur: function() {
						getList(1);
	                }
				});
				
				// custom select box end
				var $regionSelectWrap1 = $('#city'); 
				var $regionSelectWrap2 = $('#street');
				regionSelect1 = new CustomSelect( $regionSelectWrap1 );
				regionSelect2 = new CustomSelect( $regionSelectWrap2 );
				
				$regionSelectWrap1.on('change', function() {
					regionSelect2.setItemsByRegion( $(this).find('option:selected').text() );
					if($('#street').length == 0){
						//도시만 있을경우
						getList(1);
					}
					// getList(1); wrap2에 click 이벤트 발생 되어서 2번에서 getList 호출합니다. 
				});
				
				$regionSelectWrap2.on('change', function() {
					getList(1);
				});
				
				if ( data.html ) {
					if (data.uuid != 'cdcb0b6d-f1d8-4301-8af8-67178b5ca978' ) {
						getList(1);
					}
					else {
						backState = true;
						_setSearch(data.search);
						
						$('.item-list').html(data.html);
						backState = false;
					}
				}else{
					getList(1);
				}
				
				
				$(document).on('click', '.btn-filter', function() {
					$('body').addClass('open-filter').css({'overflow-y': 'hidden'});
				});
				$(document).on('click', '.btn-filter-close', function() {
					$('body').removeClass('open-filter').css('overflow-y', 'auto');
				});
				
				$(document).on('click', '.refresh', function() {
					getList(1);
				});
				
				$(window).scroll(function() {
					var currentScroll = $(window).scrollTop() + $(window).height() - 30;
					var rHeight = $(".item-list").height();

					if(currentScroll >= rHeight && fetchState){
						fetchState = false;
						getList(0);
					}
				});

				
			});
			
			function getList(status){
				if(backState) return false;
				
				if(status == 1){
					// 초기화
					fetchStart = 0;
					fetchState = true;
					$(".item-list").children().remove();
				}
				var q = $("#gnb > ul > li.on").find("li.on").text();
				var city = $("#city option:selected").val() == undefined ? "" : $("#city option:selected").val();
				var street = $("#street option:selected").val() == undefined ? "" : $("#street option:selected").val();
				var food = '';
				
				$("input[name=food]:checked").each(function (index) {
					if(food != '') food += ',';
					food += $(this).val();  
			    });
				
				var minPeople = $("#range1").attr("data-min") == undefined ? 0 : $("#range1").attr("data-min");
				var maxPeople = $("#range1").attr("data-max") == undefined ? 500 : $("#range1").attr("data-max");
				
				var minPrice = $("#range2").attr("data-min") == undefined ? 0 : $("#range2").attr("data-min");
				var maxPrice = $("#range2").attr("data-max") == undefined ? 100000 : $("#range2").attr("data-max");
				
				var params = "type=&q="+encodeURIComponent($.trim(q))+"&city="+encodeURIComponent(city)+"&street="
				+encodeURIComponent(street)+"&food="+encodeURIComponent(food)+"&minPeople="+minPeople+"&maxPeople="+maxPeople
				+"&minPrice="+minPrice+"&maxPrice="+maxPrice+"&fetchStart="+fetchStart;
				$.ajax({
		    		type : "GET",
		    		url : "/api/partnerV2",
		    		data : params,
		    		success: function(data){
		    			fetchStart += 21;
		    			var items = data.items;
		    			
		    			if(items.length >= 21){
		    				fetchState = true;
		    			}
		    			
						if(items.length > 0){
		    				
		    				for(var i=0; i<items.length; i++){
		    					var v = items[i];
		    					var html = getItem(v);
								$(".item-list").append(html);
		    				}
		    				
		    				var search = new Object();
		    				search.q = q;
		    				search.city = city;
		    				search.street = street;
		    				search.food = food;
		    				search.minPeople = minPeople;
		    				search.maxPeople = maxPeople;
		    				search.minPrice = minPrice;
		    				search.maxPrice = maxPrice;
		    				search.fetchStart = fetchStart;
		    				
		    				var o = {};
							o.html = $('.item-list').html();
							o.uuid = 'cdcb0b6d-f1d8-4301-8af8-67178b5ca978';
							o.search = search;
							History.replaceState( o, null, location.pathname + location.search);
		    			}
						
		    		}
		    	});
			}
			
			function getItem(item){
				var html = '';
				var coverImage = "";
				if(item.coverUrl != null){
					coverImage = 'style="background-image:url(https://imgs.h2m.io/?url='+item.coverUrl+'&w=1200);"';
				}
				
				var region = item.regionV2 == "" ? '-' : item.regionV2;
				
				var reviewCount = 0;
				if(item.reviewCount != null) reviewCount = item.reviewCount;
				
				var rating = item.rating/2;
				var star = "";
				if(rating != null && rating >= 0){
					for(var i=0; i<Math.round(rating); i++){
						if(i == (Math.round(rating)-1)){
							if(rating == Math.round(rating)){
								star += '<li class="star full"></li>';
							}else{
								star += '<li class="star half"></li>';
							}
						}else{
							star += '<li class="star full"></li>';
						}
					}
					
					var idx = 5 - Math.round(rating);
					for(var i=0; i<idx; i++){
						star += '<li class="star"></li>';
					}
					
				}
				
				var tagItems = "";
				var tags = item.tags;
				if(tags != null && tags.length > 0){
					for(var i=0; i<tags.length; i++){
						var tag = tags[i];
						if(tag.name != null && tag.name != ""){
							tagItems += "<span>"+tag.name+"</span>";
						}
					}
				}
				
				var bItems = "";
				var benefits = item.benefits;
				if(benefits != null && benefits.length > 0){
					for(var i=0; i<benefits.length; i++){
						var benefit = benefits[i];
						if(benefit.badge != null && benefit.badge != ""){
							bItems += "<span>"+benefit.badge+"</span> ";
						}
					}
				}
				
				var profile = item.profile;
				if(profile == null || profile == ""){
					profile = item.uuid;
				}
				
				html += '<li class="company-item">';
				html += '	<a href="/page/'+profile+'">';
				html += '		<div class="company-bg-wrap lazy" '+coverImage+'></div>';
				html += '		<div class="company-info-wrap">';
				html += '			<h2>'+item.name+'</h2>';
				html += '			<span class="company-region">'+region+'</span>';
				html += '			<div class="company-reputation">';
				html += '				<span class="only-sr">평점 3.5</span>';
				html += '				<ul class="star-wrap clearfix">'+star+'</ul>';
				html += '				<span class="company-review-count">후기 '+reviewCount+'개</span>';
				html += '			</div>';
				html += '			<div class="company-benefits">'+bItems+'</div>';
				html += '		</div>';
				html += '	</a>';
				html += '</li>';
				
				return html;
			}
			
			function _setSearch(search){
				fetchStart = search.fetchStart;
				var q = $.trim(search.q);
				var city = search.city;
				var street = search.street;
				var food = search.food;
				food = food.split(',');
				
				if(q){
					$("#gnb>ul>li.on .gnb-depth2 li.on").removeClass("on");
					$("#gnb>ul>li.on .gnb-depth2 li[data-text='"+q+"']").addClass("on");
				}
				
				if( city ) {
					regionSelect1.setRegion( city );
				}
				if( street ) {
					regionSelect2.setRegion( street );
				}
				if( food ) {
					$.each( food, function(i, v) {
						$('input[name="food"][value="'+v+'"]').prop('checked', true);
					});
				}
			}	
		})(window);
		
	</script>

