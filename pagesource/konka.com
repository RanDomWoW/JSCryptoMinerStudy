                        _html+='<div class="col-xs-3" data-url="a/'+ke.activity.activity_id+'/c/'+ke.cpns_list[0].cpns_id+'/k/'+ke.cpns_list[0].cpns_key+'">'+
                            '<div class="you-left"><p class="price">'+ke.activity.name+'</p><p class="top">'+ke.cpns_list[0].description+'</p><span class="time">使用时期:'+ke.activity.fromtime+'-'+ke.activity.totime+'</span></div>'+
                            '<div class="immediately"><span>立即领取</span></div></div>';
                    }
                });
                $('.you').append(_html);
                $('.you .col-xs-3').on('click',function(e){
                    if(!$.cookie('UNAME') || !$.cookie('MEMBER_IDENT')){
                    location.href = "/passport-login.html";
                        return;
                    }
                    var url = $(this).attr('data-url');
                    if( url ){
                        var url = 'http://www.konka.com/index.php/openapi/acpns/achieve/' + url;
                        $.getJSON(url,function(re){
                            if( re.result == 'success'){
                                alert('领取成功');
                                location.reload();
                            }else if(re.msg){
                                alert("领取失败\n"+re.msg);
                            }else{
                                alert('请稍后重试');
                            }
                        });
                    }
                });
            });
            var carParent=$('.carousel .carousel-indicators').parent().parent();
            carParent.map(function(){
                var _this=$(this).attr('id');
            	$(this).find('.carousel .carousel-inner .item:first-child').addClass('active');
            	$(this).find('.carousel .carousel-indicators li:first-child').addClass('active');
            	$(this).find('.carousel ').removeClass('television-generic').addClass(_this+'-generic')
            	$(this).find('ol li').attr('data-target','.'+_this+'-generic')
            })
            //商品banner轮播
            $('.carousel').carousel();
            var goodsInd=$('.carousel .carousel-indicators');
            goodsInd.map(function(){
                var IndLeng=$(this).find('li').length;
                if(IndLeng<=1){
                    $(this).hide();
                }
            });
            //类别评论
            $('#comment .col-xs-3').hover(function(){
                var satisShow=$(this).find('.text-left').find('.satis-show').html();
                if(satisShow==""||satisShow==null){
                    $(this).find('.wrapper').hide();
                }else{
                    $(this).find('.wrapper').stop().slideDown("slow");
                }
            },function(){
                $('#comment .col-xs-3').find('.wrapper').stop().slideUp("500");
            });
            //懒加载
            $('img.layz').lazyload({
                effect : "fadeIn"
            });
            /*
             *锚点
             */
            var ancCon=$('.anchor li span');
            ancCon.each(function(){
                //动态更改行高
                if($(this).text().length>2){
                    $(this).parents('li').css('lineHeight','17px');
                }else{
                     $(this).parents('li').css('lineHeight','35px');
                }
            });
            
            function anchor(){
                var frist=$(window).height();
                var screenWid=$(window).width();
                var pisLeft=(screenWid-1140)/2-100+'px';
                //首屏不显示
                if($(window).scrollTop()>frist){
                    $('.anchor').fadeIn();
                    $('.anchor').css('left',pisLeft)
                }else{
                    $('.anchor').fadeOut();
                }
            }
            //计算锚点left的值
            $('.anchor').hide();
            $(window).scroll(function(){
                anchor();
            });
            $(window).resize(function(){
                anchor();
            });
            //锚点滚动
           $('.anchor ul li a[href^="#"]').click(function(e){
                e.preventDefault();
                $('html, body').animate({scrollTop: $(this.hash).offset().top-50}, 400);
            });
           //去除分类最后一个span/
           $('.more-ul li:last-child').find("span").html(" ");
           $('.more-ul li:last-child').find("a").css({"color":"#ff5b57"});
           /*
            *banner动画
            */
            $('.index-banner').mouseover(function(event) {
                $(this).find('.carousel-control').find('.glyphicon-chevron-left').stop().animate({"left":"160px","opacity":1});
                $(this).find('.carousel-control').find('.glyphicon-chevron-right').stop().animate({"right":"160px","opacity":1});
            });
            $('.index-banner').mouseout(function(){
                $(this).find('.carousel-control').find('.glyphicon-chevron-left').stop().animate({"left":"-20px"});
                $(this).find('.carousel-control').find('.glyphicon-chevron-right').stop().animate({"right":"-20px"});
            });
           /*
            *IE8兼容odd.even
            */
            var Sys = {};
            var ua = navigator.userAgent.toLowerCase();
            var s;
            (s = ua.match(/rv:([\d.]+)\) like gecko/)) ? Sys.ie = s[1] :
            (s = ua.match(/msie ([\d.]+)/)) ? Sys.ie = s[1] : 0;
            if (Sys.ie == "8.0"){
                //购买指南等
                $('.home-six .col-xs-2:nth-child(odd)').css({"background":"#f68387"});
                $('.home-six .col-xs-2:nth-child(even)').css({"background":"#f2555b"});
                //商品列表第一个
                $('.category .television .container-fluid .row .col-xs-3:nth-child(4n+1)').css({"marginLeft":0});
            };
            //更改评论区域星星
            $('.text-left').find('.satis-show').map(function(){
                var satisShow=$(this).html();
            })
            $('.text-left').find('.satisfaction').map(function(){
                var nums=parseInt($(this).attr("data-commen"));
                if(nums==" " || nums==null){
                    $(this).hide();
                }
                if(nums==1.0||nums==1.5){
                    $(this).css({"background-position":'-126px 0'});
                }
                if(nums==2.0 || nums==2.5){
                    $(this).css({"background-position":'-94px 0'});
                }
                if(nums==3.0 || nums==3.5){
                    $(this).css({"background-position":'-60px 0'});
                }
                if(nums==4.0 || nums==4.5){
                    $(this).css({"background-position":'-30px 0'});
                }
                if(nums==5.0){
                    $(this).css({"background-position":"0 0"});
                }
            });
             /*
             *相册滚动
             */
            var tempLength=0;
            var liHeight=$('.star-rolling .col-xs-4');
            var moveLength=290;//计算每次移动的长度
            var countLength=(liHeight.length-4)*moveLength;//总长度
            //var moveLength=1140;//计算每次移动的长度
            //var countLength=parseInt(liHeight.length/4)*moveLength;//总长度
            //向右
            $('.home-star .more .arr-right').on('click',function(){
                if(tempLength < countLength){
                    if((countLength-tempLength) > moveLength){
                        $('.star-rolling .row').animate({"left":"-="+moveLength+"px"});
                        tempLength+=moveLength;
                    }else{
                        $('.star-rolling .row').animate({"left":"-="+(countLength-tempLength)+"px"});
                        tempLength += (countLength - tempLength);
                    }
                }
            });
            //向左
            $('.home-star .more .arr-left').on('click',function(){
                if(tempLength>0){
                    if(tempLength>moveLength){
                        $('.star-rolling .row').animate({"left":"+="+moveLength+"px"});
                        tempLength-=moveLength;
                    }else{
                        $('.star-rolling .row').animate({"left":"+="+tempLength+"px"});
                        tempLength = 0;
                    }
                }
            });
        });
    </script>
</body>
</html>