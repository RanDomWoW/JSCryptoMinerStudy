                jQuery('.login-widget-form').on('submit', function (e) {
                    e.preventDefault();

                    var formSubmit = jQuery(this);
                    var inputs = jQuery(formSubmit).find('.form-control');
                    var values = {
                        'action': 'herzog_login'
                    };
                    var btnSubmit = jQuery(formSubmit).find("*[type='submit']")
                    var errorMessage = jQuery(formSubmit).find('.error-message');

                    inputs.each(function () {
                        values[this.name] = jQuery(this).val();

                        jQuery(this).prop('disabled', true);
                    });
                    jQuery(btnSubmit).prop('disabled', true);

                    jQuery(btnSubmit).html('טוען...');

