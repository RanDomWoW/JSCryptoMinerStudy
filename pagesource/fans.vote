function select_option(pid, aid, multi_answer, use_captcha, order_no, remove_duplicates) {

	if(typeof(remove_duplicates) == 'undefined') {
		remove_duplicates = true;
	}

	if(typeof(multi_answer) != 'undefined' && multi_answer) {

		if(typeof(selected_option[pid]) == 'undefined')
		{
			selected_option[pid] = [];
			
			if(selected_option_order && order_no)
				selected_option_order = selected_option_order + "," + order_no;
			else if(order_no)
				selected_option_order = order_no;
				
		}
		//option was not checked, add it, else remove
		var index = selected_option[pid].indexOf(aid)
		if(index == -1)
			selected_option[pid].push(aid);
		else if(remove_duplicates)
			selected_option[pid].splice(index, 1);
	}
	else 
	{
		if(order_no)
			selected_option_order = order_no;
			
		selected_option[pid] = aid;
	}

	// enable voting button if there's some
	if($('#inactive-vote-btn-'+pid).length && !$('#active-vote-btn-'+pid).is(':visible')) {
		if(typeof use_captcha!="undefined" && use_captcha) {
			// load the captcha if it's not loaded yet
			load_captcha(function(){
				if (ie8(true)) {
					$('#active-vote-btn-'+pid).show();
				} else {
					$('#active-vote-btn-'+pid).fadeIn();
				}

			});
		} else {
			if (ie8(true)) {
				$('#active-vote-btn-'+pid).show();
			} else {
				$('#active-vote-btn-'+pid).fadeIn();
			}
		}
	}

}

