function PublishEmbed(json, redirect, top_location, force) {

	if(typeof top_location=="undefined") {
		top_location = true;
	}

	if(typeof force=="undefined") {
		force = false;
	}

	var pollListenerFunction = function(event) {
		var data = JSON.parse(event.data);
		if (event.origin == API_ENDPOINT)
		{
			if (data.message == "share_request_response")
			{
				if (typeof redirect != "undefined") {
					if(top_location) {
						top.location.href = redirect;
					} else {
						location.href = redirect;
					}
				}
			}
		} else {
			if (typeof redirect != "undefined") {
				if(top_location) {
					top.location.href = redirect;
				} else {
					location.href = redirect;
				}
			}
		}
	}

	jQuery(window).unbind("message").bind("message", pollListenerFunction);

	var share_data = json;

	if($(".share-my-opinion").is(":checked") || $(".share-my-opinion").length==0 || force) {
		var msg = JSON.stringify({
			message: 'share_request',
			data: share_data,
			id: 'no_id'
		});
		$.postMessage(
				msg,
				API_ENDPOINT
		);
	} else if(typeof redirect != "undefined") {
		if(top_location) {
			top.location.href = redirect;
		} else {
			location.href = redirect;
		}
	}


}

/**
 * Function to share my vote on FB and redirect user to poll results afterwards.
 *
 * @param vote_json - data to be shared
 * @param redirect - destination URL where we should redirect after sharing
 * @param top_location - use top.location.href or just location.href
 * @param force - if true, use FB share dialog
 * @param share_action - name of the action to be tracked in SMP
 */
function PublishMyVote(pid, vote_json, redirect, top_location, force, share_action) {

	if(typeof force=="undefined") {
		force = false;
	}

	if(typeof share_action=="undefined") {
		share_action = "share";
	}

	
	var is_embed = false;

	if(!is_embed && ($(".share-my-opinion.pid"+pid).is(":checked") || $(".share-my-opinion").length==0 || force)) {

		var title_length  = vote_json.name.length;
		var options_count = vote_json.properties.length;

		for ( var i = 0; i < vote_json.properties.length; i ++ ) {
			if( vote_json.properties[i].text.length >= 60 ) {
				vote_json.properties[i].text = vote_json.properties[i].text.substring(0,60) + '...';
			}
		}

		if(options_count == 3) {
			//title must be max 60
			if(title_length > 60) {
				var hide_options = 1;
			} else {
				var hide_options = 0;
			}
		} else if(options_count == 2) {
			//title must be max 120
			if(title_length > 120) {
				var hide_options = 1;
			} else {
				var hide_options = 0;
			}
		} else {
			var hide_options = 1;
		}

		if(hide_options == 1) {
			delete vote_json.properties;
		}

		var selected_radio_quotes = get_selected_option(pid);
		if(!vote_json.description || vote_json.description.trim() == '') {
			if(selected_radio_quotes == '') {
				vote_json.description = polls_app_desc_share;
			} else {
				vote_json.description = 'I just voted ' + selected_radio_quotes + ' in this poll.';
			}
		}

		//regenerate link for sharing
		FB.api('/?id='+ document.URL +'&scrape=true', 'post', function(response) {
			FB.ui(vote_json, function(response) {

				if (response && !response.error_code) {
					var track = {
						category: 'canvas',
						action: share_action,
						label: "poll://"+pid
					};

					try {
						_gaq.push(['_trackEvent', track.category, track.action, pid]);
					} catch (err){}

					if (typeof(StarSMP) != 'undefined') {
						// TRACK: share
						StarSMP.track(track.action, track.label, function() {
							if(typeof redirect!="undefined") {
								redirectionAfterVoting(top_location, redirect);
								/*
								if(top_location) {
									top.location.href = redirect;
								} else {
									location.href = redirect;
								}*/
							}
						});
					}

				} else if(typeof redirect!="undefined") {
					redirectionAfterVoting(top_location, redirect);
					/*
					if(top_location) {
						top.location.href = redirect;
					} else {
						location.href = redirect;
					}
					*/
				}
			});
		});

	} else if(typeof redirect!="undefined") {
		redirectionAfterVoting(top_location, redirect);
	}
	
}

function redirectionAfterVoting(top_window, redirect)
{
	if(typeof redirect != "undefined" && redirect != "")
	{
					if(top_window == true)
				top.location.href = redirect;
			else
				location.href = redirect;
			}
}

