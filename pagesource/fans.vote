/**
 * Update the content of poll results based on the options user has selected.
 */
function fast_results_update(pid) {

	var options = $("#" + pid + "_fast_results").find('.bar-wrapper');
	var answers = selected_option[pid];
	if (typeof answers == "number" || typeof answers == "string") {
		answers = [answers];
	}

	var total_votes = $('#pollcoreresults').data('votes') - $("#" + pid + "_fast_results").find('.bar-value.me').length + answers.length;
	
	//console.log("pr: " + $('#pollcoreresults').data('votes'));
	//console.log("mb: " +  $("#" + pid + "_fast_results").find('.bar-value.me').length);
	//console.log("answers: " + answers.length);
	//console.log("total votes: " + total_votes);
	
	for (var i = 0; i<options.length; i++) {

		var option = options[i];
		var option_id = parseInt($(option).attr('id').replace('answer-', ''));
		var option_count = $(option).data('votes');
		var option_bar = $(option).find('.bar-value');

		// check if this has been user votes or not
		var my_past_vote = option_bar.hasClass('me');

		if ($.inArray(option_id, answers) > -1 || $.inArray(option_id.toString(), answers) > -1) {
			if (!my_past_vote)
				option_count++;
			option_bar.addClass('me');
		} else if (my_past_vote) {
			option_bar.removeClass('me');
			option_count--;
		}

		$('#pollcoreresults').data('votes', total_votes);
		$('span[itemprop="upvoteCount"]').text(total_votes);

		var option_perc = Math.round(option_count/total_votes*100);
		//console.log("option id: " + i + "/" + option_perc);
		var new_html = option_perc + '%';
					new_html += ' / <span itemprop="answerCount">' + option_count + '</span> vote' + (option_count != 1 ? 's' : '');
				
		$(option).find('.votes-count').html(new_html);
		$(option).find('.bar-value').attr("data-width", option_perc + "%");//.attr("data-new", option_perc);
		
		//console.log($(option_bar));
	}
	
	//rebuild bars
	var bars = $(".bar-value");
	for(var i=0; i<bars.length; i++)
	{
		var new_width = $(bars[i]).attr("data-width");
		$(bars[i]).animate({width: new_width}, 1500);
	}
	

	// check if some of the answers is new, if yes, then append new bar
	for (var i = 0; i<answers.length; i++) {
		var a_id = '#answer-' + answers[i];
		if($(a_id).length == 0) 
		{
			fast_results_new_option(pid, answers[i]);
		}
	}
}

