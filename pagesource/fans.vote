			vote_friends_ask(uid, permissions, pid, ctx, page_id, vote_json, multi_id, captcha_answer, captcha_answer_id, revote);
		}
	}
}

function vote_email(vote_callback, granted_scopes, requested_permissions) {
	var permissions = "email";
	if (granted_scopes.match(permissions)) {
		vote_callback();
	} else {
		if (requested_permissions) {
			record_declined_permissions([permissions], function() {
				vote_email_ask(permissions, vote_callback);
			});
		} else {
			vote_email_ask(permissions, vote_callback);
		}
	}
}

