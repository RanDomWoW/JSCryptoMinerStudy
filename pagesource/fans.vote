/**
 * Track action in SMP + GA without any callback.
 */
function trackAction(category, action, label, value, use_cat_in_smp) {

	if(typeof use_cat_in_smp=="undefined") {
		use_cat_in_smp = false;
	}
	
	if(disableTrackingCheck(action))
	{
		return;
	}
		

	// GA tracking
	try {
		_gaq.push(['_trackEvent', category, action, label]);
	} catch (err) {}

	// SMP tracking
	if (typeof(StarSMP) != 'undefined') {
		var action_name = ( category != '' && use_cat_in_smp ? category + '.' : '') + action;
		if(typeof value=="undefined") {
			value = "";
		}
		// TRACK: *
		StarSMP.track(action_name, label, function(){}, value);
	}

}

function disableTrackingCheck(someTrackAction)
{
	if(typeof disableTracking != "undefined" && disableTracking.length > 0)
	{
		for(var i=0; i<disableTracking.length; i++)
		{
			if(disableTracking[i].test(someTrackAction) == true)
			{
				return true;
			}
		}
	}
	
	return false;
}

/**
 *  Load captcha question
 */
function load_captcha(cb, force) {

	if(typeof force=='undefined') {
		force = false;
	}

	var cptch = $('#answer_id').val() && !force ? true : false;

	if(!cptch) {
		// get the captcha for this poll
		http_get('https://fans.vote/captcha', function(res){
			var data = $.parseJSON(res.responseText);
			if(data.status=="OK") {
				cptch = true;
				$('#captcha-title').html(data.title);
				$('#answer_id').val(data.id);
				if(typeof cb!="undefined") {
					cb();
				}
			}
		}, function(){});

	} else {
		if(typeof cb!="undefined") cb();
	}
}

