/**
 * Function to execute vote action.
 * @param pid
 * @param ctx
 * @param page_id
 * @param suggest_fan
 * @param require_fan
 * @param require_email
 * @param require_friends
 * @param vote_json
 * @param cid
 * @param multi_id
 * @param captcha_answer
 * @param captcha_answer_id
 */

var ajaxResultsPreload = false;
function vote(pid, ctx, page_id, suggest_fan, require_fan, require_email, require_friends, vote_json, cid, multi_id, captcha_answer, captcha_answer_id, revote) {

		//dont allow vote if reasults are still loading
	if(ajaxResultsPreload != false)
	{
		return;
	}
	/*
        console.log(revote);
        console.log("c1:" + (suggest_fan && page_id && !has_just_liked));
        console.log("c2:" + ((require_fan || require_friends) && page_id && cid==0));
        if(confirm("continue?") == false)
        {
            return;
        }
        */
	if (vote_in_progress)
		return false;

	if(typeof captcha_answer == "undefined") {
		var captcha_answer = "";
	}

	if(typeof captcha_answer_id == "undefined") {
		var captcha_answer_id = 0;
	}

	var params = {
		"pid": pid,
		"uid": "0",
		"mid": "",
		"cid": cid,
		"c_answer": captcha_answer,
		"aid": captcha_answer_id,
		"old_pid": "AAAAyIjUZZY"
	};

	var voted_option_text = get_selected_option(pid);

	vote_json.description = 'I just voted ' + voted_option_text + ' in this poll.';

	var no_require_login = 0;
	
	if (require_email)
	{
		var permissions = "email";
		FB.login(function(response) {
			if (response.authResponse)
			{
				if (response.status == "connected")
				{
					FB.api('/me', function(response) {
						checkEmail(pid, ctx, page_id, suggest_fan, require_fan, voter_pictures, vote_json);
						return false;
					});
				}
				else
				{
					// user is logged in, but did not grant any permissions
					show_error_overlay("In order to vote on this poll, you need to accept the application\'s permissions.");
				}
			}
			else
			{
				show_error_overlay("In order to vote on this poll, you need to accept the application\'s permissions.");
			}
		}, {
			scope: permissions
		});
	}
	else if (suggest_fan && page_id && !has_just_liked)
	{
		var page_name = $('#' + pid + '_author_name').val();
		var title = 'Don\'t forget to Like the page [[pagename]]!';
		title = title.replace('[[pagename]]', page_name);

		var like_content = get_like_button(pid, page_id);

		show_modal_dialog(title, like_content, null, 'Close', function(){},
			function(){
				vote(pid, ctx, page_id, 0, require_fan, require_email, require_friends, vote_json, cid, multi_id, captcha_answer, captcha_answer_id, revote)
			}
		);
		FB.XFBML.parse();
	}
	else if(require_friends && uid != 0 && no_require_login == 1)
	{
		var permissions = ['email'];

		if (require_friends)
			permissions.push('user_friends');

		check_permissions(
			function(uid, token, granted_scopes, requested_permissions){
				vote_friends(uid, pid, ctx, page_id, page_name, require_friends, vote_json, multi_id, granted_scopes, requested_permissions, captcha_answer, captcha_answer_id, revote);
			},
			'',
			permissions
		);
	}
	else if(require_fan && page_id && cid==0) //else if ((require_fan || require_friends) && page_id && cid==0)
	{
		var page_name = $('#'+pid+'_author_name').val();
		var permissions = ['email'];

		if (require_fan)
			permissions.push('user_likes');

		if (require_friends)
			permissions.push('user_friends');

		var err_message = 'This poll requires you to like the page ' + page_name;
		if (!require_fan) {
			err_message = '';
		}

		check_permissions(
			function(uid, token, granted_scopes, requested_permissions){
				if (require_fan)
					vote_like(pid, ctx, page_id, page_name, require_friends, vote_json, multi_id, granted_scopes, requested_permissions, captcha_answer, captcha_answer_id, revote);
				if (require_friends && !require_fan)
					vote_friends(uid, pid, ctx, page_id, page_name, require_friends, vote_json, multi_id, granted_scopes, requested_permissions, captcha_answer, captcha_answer_id, revote);
			},
			err_message,
			permissions
		);
	}
	else
	{
		var voted_from = '';

		/*
		 * Function called after vote ajax req has been sent.
		 */
		function finish(response) {
			vote_in_progress = false;
			if (response.status == 'OK') {

				// vote has been saved
				var voteTrackName = (revote != 0) ? 'revote' : 'vote';
				var label = "poll://"+pid;
				var track = {
					category: voted_from,
					action: (response.paid_vote) ? voteTrackName + '-paid' : voteTrackName,
					label: label,
					value: (response.paid_vote_price) ? response.paid_vote_price : ''
				};
				var params = {
					pid: pid,
					title: vote_json.name,
					image_link: vote_json.picture,
					voted_option: voted_option_text
				};

				if(selected_option_order != 0)
					track.action = track.action + "/" + selected_option_order;

				// GA tracking
				try {
					_gaq.push(['_trackEvent', track.category, track.action, pid]);
				} catch (err){}

				// SMP tracking + callback execution
				var voted_options = typeof response.voted_options == "undefined" ? "" : response.voted_options;
				trackAndShowRes(track, pid, vote_json, params, multi_id, voted_options);

			} else if(response.status == 'CAPTCHA_ERROR') {
				// user has provided incorrect captcha code or captcha has expired
				var incorrect_captcha_text = "Incorrect. Try again.";
				var expired_captcha_text = "Sorry! You took too long answering that question, here\'s a new one";
				var error_text = response.expired ? expired_captcha_text.replace('\.', '.<br/>') : incorrect_captcha_text;

				if(response.expired) {
					// load new captcha
					load_captcha(function(){
						if($('#captcha-res').length) {
							$('#captcha-res').html(error_text);
						} else {
							alert(error_text);
						}
					}, true);
				} else {
					if($('#captcha-res').length) {
						$('#captcha-res').html(error_text);
					} else {
						alert(error_text);
					}
				}


			} else if(response.status == 'ERR_POLL_INACTIVE') {
				show_error_overlay("This poll does not seem to be active.");
			} else {

				if(is_widget) {
					// show results for the widget poll
					afterVoteCallback(pid, vote_json, multi_id, true);
				} else {

					// user has already voted for this poll
					show_error_overlay("You already voted on this poll!");
									}

			}
		}

		/*
		 * Finish checking whether user can or can't vote for this poll.
		 */
		function sendVote(data) {
									if (voted_from == 'tab') {
					data += '&voted_from=tab';
				}
				top.location.href = 'https://fans.vote/vote?' + data;
							}

		/*
		 * Callback after the vote action has been unsuccessful.
		 */
		function fail() {
			vote_in_progress = false;
		}

		// check if user has selected at least one of the options
		if (selected_option[pid] == null) {
			show_error_overlay("Please choose an answer in order to register your vote.");
		} else {
			// init cid
			if (typeof cid == 'undefined')
				cid = 0;

			// set voting parameters
			params.options = selected_option[pid];
			params.ctx_public = $("#ctx_public_" + pid).val();

			var data = "";
			for (var k in params)
				data += k + "=" + params[k] + "&";

					voted_from = 'canvas';
			data += "from=poll";
			vote_in_progress = true;
			last_context[pid] = ctx;
			// check login
			check_permissions(function(uid, token, granted_permissions, requested_permissions){

				data = data.replace("uid=0", "uid="+uid);
				data += '&access_token='+token;
				vote_email(function(){
						fast_results_show(pid, true);
						http_jquery_post_jsonp("https://fans.vote/vote", data, finish, fail);
					},
					granted_permissions,
					requested_permissions
				);
			}, '', ['email']);
		
		}
	}
} // end of vote

