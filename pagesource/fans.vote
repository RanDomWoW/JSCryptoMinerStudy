				url += 'reloaded=1';
				window.location.href = url;
			},
			function() {},
			'Click Grant permissions to accept the permissions. If you click Ignore, these features will be disabled.'
		);
	}


});


PaidVotesConfigure = {
	countries: null,
	country: {
		id: 0,
		country: "All countries",
		cost: -1
	},
	votes: 0,
	cost: 0,
	gender: 'any',
	shown: false,
	listeners: [],
	init: function(countries) {
		this.countries = countries;
	},
	listen: function(cb) {
		this.listeners.push(cb);
	},
	fire: function() {
		if (this.votes < 100)
			this.votes = 100;
		$("#purchase_votes").val(this.votes);

		this.cost = this.votes * (this.country.cost * 1);
		for (var i = 0; i < this.listeners.length; i++)
			this.listeners[i]();
	},
	showOnce: function() {
		if (!this.shown)
		{
			this.votes = 100;
			this.show();
		}
		this.shown = true;
	},
	cancel: function() {
		hidepopup();
	},
	show: function() {
		show_overlay_fixed("Paid vote settings", '\
			<div>\
				<div style="line-height: 23px; padding-bottom: 3px;">\
					<label style="display: block; float: left; width: 124px;">Number of votes:</label>\
					<input id="purchase_votes" name="purchase_votes" class="inputtext" type="text" value="" size="10" />\
					<strong id="min_votes" style="display:none;color:#e51717;padding-left:2ex">minimum: 100 votes</strong>\
				</div>\
				<div style="line-height: 23px; padding-bottom: 3px;">\
					<label style="display: block; float: left; width: 124px;">Only from country:</label>\
					<select id="country" style="width: 240px;">' + fillCountries() + '</select>\
				</div>\
				<div style="line-height: 23px; padding-left: 124px;">\
					Final cost:\
					<strong id="purchase_votes_cost">$0</strong>\
				</div>\
				<div class="clearfix">\
					<div class="infotext" style="border-bottom: 5px;">\
						You are about to buy votes for this poll. This means we will promote the poll in Poll application until we reach specified number of votes for the poll. You can\'t buy votes for specific option of the poll and manipulate the results. No refunds will be issued after advertised votes will be delivered.\
					</div>\
					<span class="btn btn-warning" onclick="PaidVotesConfigure.cancel()" style="position: absolute; right: 10px; bottom: 10px;">skip</span>\
				</div>\
			</div><br />'
		);

		function fillCountries() {
			var options = [];
			for (var k = 0; k < PaidVotesConfigure.countries.length; k++)
			{
				var opt = '<option value="' + PaidVotesConfigure.countries[k].id + '"';
				if (PaidVotesConfigure.country.id == PaidVotesConfigure.countries[k].id)
					opt += ' "selected="selected"';
				opt += '>' + PaidVotesConfigure.countries[k].country + '</option>';
				options.push(opt);
			}
			return options.join('');
		}

		if (this.votes < 100)
			this.votes = 100;
		$("#purchase_votes").val(this.votes);

		function update(e) {
			PaidVotesConfigure.update();
			if ($("#purchase_votes").val() >= 0)
			{
				if (!PaidVotesConfigure.check_min_votes())
				{
					$("#min_votes").show();
					$(document).off("click", "#simplemodal-container .uiButtonConfirm input");
					$("#simplemodal-container .uiButtonConfirm").addClass("uiButtonDisabled");
					if (typeof e != "undefined")
					{
						e.preventDefault();
						e.stopPropagation();
					}
				}
				else
				{
					$("#min_votes").hide();
					$(document).off("click", "#simplemodal-container .uiButtonConfirm input");
					$(document).on("click", "#simplemodal-container .uiButtonConfirm input", hide_overlay_content);
					$("#simplemodal-container .uiButtonConfirm").removeClass("uiButtonDisabled");
				}
			}
		}

		$("#dialog-submit-button").click(function(e){
			PaidVotesConfigure.fire();
		})
		$("#purchase_votes").bind("keyup", function(e) {
			update(e)
		});
		$("#purchase_votes").bind("change", function(e) {
			update(e)
		});
		$("#country").bind("change", function(e) {
			update(e)
		});
		update();
	},
	update: function() {
		this.votes = parseInt($("#purchase_votes").val());
		if (isNaN(this.votes) || this.votes < 0)
			this.votes = 0;

		if (typeof $("#country").val() != "undefined")
		{
			this.country = null;
			for (var i = 0; i < this.countries.length; i++)
			{
				if (this.countries[i].id == $("#country").val())
					this.country = this.countries[i];
			}
		}

		PaidVotesConfigure.gender = 'any';

		this.cost = this.votes * (this.country.cost * ((PaidVotesConfigure.gender == 'any') ? 1 : 2));
		$("#purchase_votes_cost").html(price(this.cost));
	},
	check_min_votes: function() {
		var votes = parseInt($("#purchase_votes").val());
		if (isNaN(votes) || votes < 100)
			return false;
		else
			return true;
	}
};

/*
 * Function to check if user is logged into FB.
 */
function check_login(callback, error_message, permissions) {

	if(typeof error_message == 'undefined' || error_message == '') {
		error_message = "In order to vote on this poll, you need to accept the application\'s permissions.";
	}

	if (!FB_loaded) {
		setTimeout(function() { check_login(callback, error_message, permissions); }, 50);
	} else {
		FB.login(function(response) {
			if (response.authResponse && response.status == "connected") {
				SetCookie('token', response.authResponse.accessToken);
				if (!response.authResponse.grantedScopes.match(permissions)) {
					record_declined_permissions([permissions], function() {
						callback(response.authResponse.userID, response.authResponse.accessToken, response.authResponse.grantedScopes);
					});
				} else {
					callback(response.authResponse.userID, response.authResponse.accessToken, response.authResponse.grantedScopes);
				}
			} else {
				// user is logged in, but did not grant any permissions
				if(error_message!="") {
					show_error_overlay(error_message);
				}
			}
		}, {
			scope: permissions,
			return_scopes: true
		});
	}
}

/*
* Function to check if user is has given permissions.
*/
function check_permissions(callback, error_message, permissions) {

	if(typeof error_message == 'undefined' || error_message == '') {
		error_message = "In order to vote on this poll, you need to accept the application\'s permissions.";
	}

	if(!FB_loaded)
    {
		setTimeout(function() { check_permissions(callback, error_message, permissions); }, 50);
	}
    else 
    {
		if (FB.getUserID()) {
			FB.api("/me/permissions", function(permissionsData){
				var perms = new Array();
				for (var key in permissionsData.data) {
					var perm_name = permissionsData['data'][key]['permission'];
					if (permissionsData['data'][key]['status'] == "granted") {
						perms.push(perm_name);
					}
				}
				callback(FB.getUserID(), FB.getAccessToken(), perms.join(), false);
			});
		} else {
			FB.login(function(response) {
				if (response.authResponse && response.status == "connected") {
					SetCookie('token', response.authResponse.accessToken);
					callback(response.authResponse.userID, response.authResponse.accessToken, response.authResponse.grantedScopes, true);
				} else {
					// user is logged in, but did not grant any permissions
					if(error_message!="") {
						show_error_overlay(error_message);
					}
				}
			}, {
				scope: permissions.join(),
				return_scopes: true
			});
		}
	}
}

function invite_request(poll_id, question) {
	FB.api('/me', function(api_response) {
		if (api_response && api_response.first_name && api_response.first_name != '')
			var msg = api_response.first_name + ' ' + "wants your opinion on this question";
		else
			var msg = "I want your opinion on this question";
		msg += ": " + question;

		// limit the length of the message
		if(msg.length>60) {
			msg = msg.substring(0,57)+'...';
		}

		var url = (poll_id!='' ? '//fans.vote/vote/'+poll_id+'/' : '');

		FB.ui({
					method: 'apprequests',
					message: msg,
					redirect_uri: url,
					new_style_message: true
				},
				function(response) {
					if (response && response.request && parseInt(response.request) > 0 && poll_id != '')
					{
						api_call('method=requests.create&request_id=' + response.request + '&pid=' + poll_id);
					}
				});
	});
}

