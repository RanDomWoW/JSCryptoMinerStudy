				);
			});
		} else {
			permission_explanation(
				permissions,
				'Why are we asking for these permissions?',
				'Grant permissions',
				function(){
					ctx.click();
				},
				function(){},
				'Click Grant permissions to accept the permissions. If you click Ignore, these features will be disabled.'
			);
		}

	}
}

function vote_friends(uid, pid, ctx, page_id, page_name, voter_pictures, vote_json, multi_id, granted_scopes, requested_permissions, captcha_answer, captcha_answer_id, revote) {

        //console.log("vote_friends_revote:" + revote);
	var permissions = "user_friends";
	if (granted_scopes.match(permissions)) {
		vote(pid, ctx, page_id, 0, 0, 0, 0, vote_json, 0, multi_id, captcha_answer, captcha_answer_id, revote);
	} else {
		if (!granted_scopes.match("email")) {
			permissions += ",email";
		}
		if (requested_permissions) {
			record_declined_permissions(permissions.split(','), function() {
				vote_friends_ask(uid, permissions, pid, ctx, page_id, vote_json, multi_id, captcha_answer, captcha_answer_id, revote);
			});
