<script>
     
        function pop_store(){
             $('#toggle-department-menu').prop("checked",true);
             $('#toggle-sub-menu-store').prop("checked",true);
        }
	function getPlatform(){
		 var w=$(document).width();
		 switch(true){
			 case w<=768:
						return 'wap';
			 default: 
						return 'pc';
		 }
	}
        
	function getPlatform2(){
		 var w=$(document).width();
		 switch(true){
			 case w<768:
						return 'wap';
			 default: 
						return 'pc';
		 }
	}
    function getPlatform3(){
		 var w=$(document).width();
		 switch(true){
			 case w<1180:
						return 'wap';
			 default: 
						return 'pc';
		 }
	}
	function category_btn_click(){
    	$('.category-btn').click(function(){
          if(getPlatform3()!='wap'){
    	    window.location.href='all-categories.html';
    	  }
        })
       
        
	}
	
	function setCategories(){
		var w=$(document).width();
		$('.department-menu').css({width:parseInt(w-107*2)+'px'});
		$('.department-menu .menu-item').removeClass('focus-item');
		
		//var focus_el=$('.toggle-menu-list.expand');
		var cat_id=1;
		/*
		if(focus_el.length>0){
			 cat_id = focus_el.attr('id').replace('cat_','');
			 if(cat_id=='none'){
				 cat_id=1;
			 }
		}
		*/
		$('.toggle-menu-list').removeClass('expand');
		var menu_item = $('.menu-item[cat_id="'+cat_id+'"]');
		menu_item.addClass('focus-item');
		$('#cat_'+cat_id).addClass('expand');
		var target_el = $('#toggle-sub-menu-'+cat_id);
		if(target_el.length>0){
			target_el.prop("checked",true);
		}
	}
	
	function setPopup(el){
		 var p=getPlatform();
		 if(p=='wap'){
			  return;
		 }
		 el=$(el);
		 var pos = el.offset();
		 var popup=el.next();
		 var sw=$(document).width();
		 var ew=el.width();
		 var pw=popup.width();
		 el.removeClass('arrow-left');
		 if(pos.left+ew+pw+30>sw){
			  el.addClass('arrow-left');
				popup.css({left:'-'+(pw+20)+'px'});
		 }else{
			  popup.css({left:'100%'});
		 }
	}
	
	function setCartNum(qty){
			$('.cart-qty').html(qty);
			var el=$('.cart-qty').parent();
			if(qty>0){
				 el.removeClass('cart-empty');
			}else{
				 if(!el.hasClass('cart-empty')){
				    el.addClass('cart-empty');
				 }
			}
	}
	
	function setNavPos(){
		var w=$(document).width();
		var h=$('.navigation').outerHeight()+10;
		if(w>=768){
			if($(document).scrollTop()>h){
				$('.navigation').css({'position':'fixed','top':'0px'});
				$('#categories').css({'position':'fixed'});
			}else{
				$('.navigation').css({'position':'absolute'});
				$('#categories').css({'position':'absolute'});
			}
		}else{
			$('.navigation').css({'position':'fixed','top':'0px'});
			$('#categories').css({'position':'absolute'});
		}
	}
	
	$(function(){
        
        placeholder(".search");
        placeholder(".collect_email2");
        
		$('.learn-more').click(function(){
			$.scrollTo('#umart_make_bg_pos',300);
		});
		
		$('.select-store').hover(
			function(){
				$(this).find('.storeList').css({display:'block'});
				var w1=$(this).find('.storeList').width();
				var w2=$(this).width();
				var l=(w2-w1)/2;
				$(this).find('.storeList').css({left:l+'px'});
			},
			function(){
				$(this).find('.storeList').css({display:'none'});
			}
		);
		
		$('.user-account').hover(
			function(){
				$(this).find('.actionList').css({display:'block'});
			},
			function(){
				$(this).find('.actionList').css({display:'none'});
			}
		);
        
        function nav_cart_hover(){
  
              $('.nav-cart').hover(
                  
                  function(){
                   
                    if(getPlatform3()=='wap'){
                        return;
                    }
                      $('.nav-cart').find('.cartList').css({display:'block'}); 
                      $(this).find('img#loading_img').attr('src','themes/umart2018/images/loader.gif');
                      $("#control_cartlist_ajax").prop("checked",true);
                      
                      if($("#control_cartlist_ajax").prop("checked")==true){ 
                         
                           $.post("cart.php?step=nav_cartlist",function(data){
                              if(data){
                                  $('.nav-cart').find('img#loading_img').remove()
                                  $('.cartList-items-box').html(data)
                                  if($('.cartList-items-box').height()>690){
                                      $('.cartlist-items').mCustomScrollbar({
                                        setHeight:720,
                                        theme:"dark-3",
                                        scrollInertia:0
                                      });
                                  }
                              }
                          })
                          
                      }
                
                      
                    
                      
                  },
                  function(){
                     if(getPlatform3()=='wap'){
                        return;   
                     }
                      $("#control_cartlist_ajax").prop("checked",false);
                      $(this).find('img#loading_img').removeAttr("src")
                      $(this).find('.cartList').css({display:'none'});  
                      
                  }
              )
            
           
        }
		
		$('.third-item-title').on('mouseover',function(){
			 setPopup(this);
		});
		
		$('.third-item-title').click(function(e){
			/*
			 var w=$(document).width();
			 if(w<=768||w>=1200){
				  return false;
			 }
			 e=e||window.event;
			 var eg=$(e.target);
			 */
			 isSupportTouch = "ontouchend" in document ? true : false;
			 if(isSupportTouch){
				 return false;
			 }
			 setPopup(this);
		});
        
        $(document).ready(function(e) {
            nav_cart_hover();
            category_btn_click();
        });
		/*
		$(window).scroll(function(){
			setNavPos();
		});
		*/
		
		$(window).on('resize',function(){
            nav_cart_hover();
            category_btn_click();
			//setNavPos();
			if(getPlatform2()=='wap'){
				 $('.department-menu').css({width:''});
				 $('.department-menu').css({display:'block'});
			}else{
				 $('.department-menu').css({display:'none'});
				 setCategories();
				 if($('#toggle-department-menu').prop('checked')){
						var el=$('.toggle-sub-menu:checked');
						if(el.length>0){
							 var cat_id=el.attr('id').replace('toggle-sub-menu-','');
							 $('.department-menu .menu-item').removeClass('focus-item');
							 $('.menu-item[cat_id="'+cat_id+'"]').addClass('focus-item');
						}else{
							$('.department-menu').css({width:''});
						}
				 }else{
						$('.department-menu').css({width:''});
				 }
			}
			$('.department-menu .sub-menu-list').removeClass('expand');
		});
		var dmenuheight = $(window).height() - $('.navigation').height();
		$('#categories').hover(
				function(){
					if(getPlatform2()=='wap'){
						return;
					}
					setCategories();
					$('#toggle-department-menu').prop("checked",true);
					$('.department-menu').css({display:'block'});
                                        if($(".menu-list").height() > dmenuheight){
                                           $(".menu-list").css({"height":dmenuheight-50+'px',"overflow-y":'auto'})
                                         }
                                        var cat_id =  $('.department-menu .menu-item').attr('cat_id');
                                        if($("#cat_"+cat_id).height()> dmenuheight){
                                          $("#cat_"+cat_id).css({"height":dmenuheight-50+'px',"overflow-y":'auto'})
                                         }
                                        
                 
				},
				function(){
					if(getPlatform2()=='wap'){
						return;
					}
					$('#toggle-department-menu').prop("checked",false);
					$('.toggle-sub-menu').prop('checked',false);
					$('.department-menu').css({display:'none'});
					//$('.department-menu').css({width:'170px'});
					//$('.department-menu .sub-menu-list').removeClass('expand');
				}
		);
		
		$('.department-menu .menu-item').hover(
			 function(){
				 if(getPlatform2()=='wap'){
					 return;
				 }
				 //var w=$(document).width();
				 $('.department-menu .menu-item').removeClass('focus-item');
				 $('.department-menu .sub-menu-list').removeClass('expand');
				 $('.toggle-sub-menu').prop('checked',false);
				 $(this).addClass('focus-item');
				 var cat_id = $(this).attr('cat_id');
				 //$('.department-menu').css({width:parseInt(w*0.87)+'px'});
				 $('#cat_'+cat_id).addClass('expand');
				 var target_el = $('#toggle-sub-menu-'+cat_id);
				 if(target_el.length>0){
						target_el.prop("checked",true);
				 }
                                if($("#cat_"+cat_id).height()> dmenuheight){
                                   $("#cat_"+cat_id).css({"height":dmenuheight-50+'px',"overflow-y":'auto'})
                                 }
                  
			 },
			 function(){
				 if(getPlatform2()=='wap'){
					 return;
				 }
				 var cat_id = $(this).attr('cat_id');
				 var target_el = $('#toggle-sub-menu-'+cat_id);
				 if(target_el.length>0){
						target_el.prop("checked",true);
				 }
				 //$(this).removeClass('focus-item');
				 //$('.department-menu').css({width:'170px'});
			 }
		);
		
		$('.pop-sub-menu').change(function(){
				if($(this).prop('checked')){
					return;
				}
				var chk_id=$(this).attr('id');
				var key=chk_id.replace('toggle-sub-menu-','');
				var keys=['0','a','b'];
				var do_close=false;
				for(k in keys){
					if(keys[k]==key){
						do_close=true
					}
					if(do_close){
						$('#toggle-sub-menu-'+keys[k]).prop('checked',false);
					}
				}
		});
		
		$('.category-overlay').click(function(){
			$('.toggle-sub-menu').prop('checked',false);
		});
		
		$('.category-overlay').on('touchmove',function(e){
			e.preventDefault();
		});
		
		registerEventButton();
		
		$.get('cart.php?act=cartinfo&t='+(new Date()).getTime(),function(result){
			setCartNum(result.qty);
	  },'JSON');
	
	});
	
	function registerEventButton(){
			$('.second-sub').hover(
				 function(){
					 if(getPlatform2()=='wap'){
						 return;
					 }
					 $(this).addClass('sub-active');
                                          var cat_id =  $('.department-menu .menu-item').attr('cat_id');
                                          var popover = $(this).find('.popover');
                                          var oversize = popover.position().left + popover.outerWidth() - $("#cat_"+ cat_id ).width();
                                         if( oversize > 0 ){
                                            popover.css("left",popover.position().left-oversize-2);
                                         }
				 },
				 function(){
					 if(getPlatform2()=='wap'){
						 return;
					 }
					 $(this).removeClass('sub-active');
					 $(this).find('.third-item-title').removeClass('arrow-left');
				 }
			);
			
			$('.back-main').click(function(){
				 $('.toggle-sub-menu').prop('checked',false);
				 
			});
			
			$('.menu-layer').click(function(e){
				 html=$(this).find('.sub-content').html();
				 var target_id=$(this).attr('target_id');
				 target_id=target_id||0;
				 $('#cat_'+target_id).find('.sub-menu-content').html(html);
				 $('#toggle-sub-menu-'+target_id).prop('checked',true);
				 registerEventButton();
				 //var w=$(document).width();
				 e=e||window.event;
				 var eg=$(e.target);
				 isSupportTouch = "ontouchend" in document ? true : false;
				 if(isSupportTouch&&eg.hasClass('department-menu__link')){
					 return false;
				 }
			});
			
			$('.second-sub').click(function(e){
				 html=$(this).find('.third-item-body').html();
				 $('#cat_a').find('.sub-menu-content').html(html);
				 $('#toggle-sub-menu-a').prop('checked',true);
				 registerEventButton();
				 e=e||window.event;
				 var eg=$(e.target);
				 isSupportTouch = "ontouchend" in document ? true : false;
				 if(isSupportTouch){
					 return false;
				 }
			});
	}
	
	
