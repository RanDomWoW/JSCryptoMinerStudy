    function collect_email(obj){
        var email,type;
        var email_preg = /^[\w\-\.]+@[\w\-\.]+(\.\w+)+$/;
        type = $(obj).parent().siblings("span").html();
        if($(obj).parent().siblings("input").val().length>0){
            email = $(obj).parent().siblings("input").val();
        }
   
        if(!email_preg.test(email)){
            layer.msg('Email is not correct', {
              icon: 2,
              time: 3000 
            }, function(){
              $(obj).parent().siblings("input").focus();
              
            });
        }
        else{
          $.post('collect_email.php',{email:email,type:type},function(data){
                layer.msg('Email subscribe success', {
                  icon: 1,
                  time: 3000 
                }, function(){
                   $(obj).parent().siblings("input").val('');
                  
                });
                
          })
            
            
        }
    }
    </script>
    
