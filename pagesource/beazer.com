tion><option value='1009' data-url='/search-CA-sacramento' data-ispartner='false'>Sacramento, CA</option><option value='1066' data-url='/search-TX-' data-ispartner='true'>San Antonio, TX</option><option value='1072' data-url='/search-CA-san-diego' data-ispartner='false'>San Diego, CA</option><option value='1020' data-url='/search-GA-savannah' data-ispartner='false'>Savannah, GA</option><option value='1038' data-url='/search-FL-tampa' data-ispartner='false'>Tampa, FL</option><option value='1040' data-url='/search-VA-virginia--dc' data-ispartner='false'>Virginia / D.C., VA</option></select><button type='button'>GO</button></div><div class='progress hide' id='wait_listMetrosTop'><div class='indeterminate'></div></div><script>
                (function() { 
                    var init = function() {
                        
                        // Get the dropdown and set the current metroid from the cookies
                        var $select = $('#listMetrosTop').beazerDropDown();
                        var metroid = Beazer.Cookies.GetSearchSettingsMetro();

                        // If their Metro cookie is for a Metro we no longer have, blank it out
                        if (metroid != null && $select.find('option[value="' + metroid + '"]').length == 0) {
                            metroid = null;
                            Beazer.Cookies.SetSearchSettingsMetro(null);
                        }
            
                        // Set Metro dropdown value
                        $select.val(metroid).change();

                        // Handle clicks on the GO button
                        $select.closest('.go-box').find('button').click(function() {
                             
                             var $selected = $select.find('option:selected');
                             var val = $selected.attr('value');
                             var $wait = $('#wait_listMetrosTop');

                             if ($selected.attr('data-ispartner') == 'true') {
                                Beazer.Dialogs.ShowMetroPartnerInfo(val, $wait);
                                return false;
                             }
                             else {

                                // Save the selected MetroID in cookies and navigate to the search page
                                $wait.removeClass('hide');
                                Beazer.Cookies.SetSearchSettingsMetro(val);
                                window.location.href = $selected.attr('data-url');

                             }

                        });

                    };
                    if (document.readyState == 'complete') {
                        init();
                    }
                    else {
                        $(document).ready(init);
                    }
                })();
            </script>
