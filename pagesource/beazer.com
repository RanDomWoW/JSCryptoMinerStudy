    <script>
        $(document).ready(function () {

            setupSpecialOffers();

            // Function used to update the UI to reflect the supplied metroid. If the metroid is null, metro specific content should be hidden
            function refreshMetroUI(metroid) {
                // update image
                var metroImage = $.grep($('main').data().metroImages, function(mi) { return mi.metroId == metroid })[0];
                if (metroImage && metroImage.imageSrc) {
                    $('.home_hero_outer img:not(.overlay)').attr('src', metroImage.imageSrc);
                }

                // Unhide the Explore Communities section
                $(".home_explore_outer").toggleClass("hide", metroid == null);

                // Show the appropriate text just above the metro selection box
                $(".home_metro_nogeo").toggleClass("hide", metroid == null);
                $(".home_metro_geo").toggleClass("hide", metroid != null);

                // Set the Metro Listing value
                $("#listMetros").val(metroid);

                // If we have a current metro id
                if (metroid != null) {

                    // Swap Beazer Difference and Gatherings if they exist
                    var $gatherings = $(".home_gatherings_promo");
                    var $difference = $(".home_difference_outer");
                    var $differenceDesktop = $(".home_difference_desktop_outer");
                    $gatherings.insertBefore(".home_news_outer");
                    $difference.insertAfter(".home_news_outer");
                    $differenceDesktop.insertAfter(".home_news_outer");

                    // Re-load Offers in case it has Metro specific content
                    $.ajax({
                        type: 'GET',
                        url: '/Home/GetHomepageOffers',
                        data: { id: metroid }
                    }).then(function (response) {
                        $(".home_offer_outer").empty().html(response);
                        setupSpecialOffers();
                    });


                    // Load communities in that Metro
                    $("#waitExploreCommunities").show();
                    $.ajax({
                        type: 'GET',
                        url: '/Home/Explore',
                        data: { id: metroid }
                    }).then(function (response) {

                        // Hide the progress bar
                        $("#waitExploreCommunities").hide();

                        // Initialize the Explore Communities component
                        $(response).insertBefore('.home_explore_outer .home_explore_cta');

                        var $metroOption = $('#listMetrosTop option[value="' + metroid + '"]');
                        var metroState = $metroOption.text();
                        var metroName = metroState.split(',')[0];                        
                        $('.home_explore_outer .home_explore_metroname').text(metroState);
                        $('.home_explore_outer .home_explore_cta').removeClass('hide');
                        $('.home_explore_outer .home_explore_cta p').text('Want to learn more about all the ' + metroName + ' communities?');
                        $('.home_explore_outer .home_explore_cta a').attr('href', $metroOption.data().url);

                        $(".home_wherewebuild_outer").find(".card_list_outer").beazerCardList('centermap', 36.491973, -93.396484, 5);          

                        //$('form').beazerform();
                        ////Load Contact Info Data from Cookie
                        //var ContactData = Beazer.Cookies.GetContactInfo();
                        //$('div.leadform_container #FirstName').val(ContactData.FirstName);
                        //$('div.leadform_container #LastName').val(ContactData.LastName);
                        //$('div.leadform_container #EmailAddress').val(ContactData.Email);
                        //$('div.leadform_container #CompletePhoneNumber').val(ContactData.Phone);
                        //$('div.leadform_container #ZipCode').val(ContactData.Zip);
                        //Materialize.updateTextFields();

                    });

                    // If the Metro has Gatherings series, show the Gatherings promo. Otherwise, hide it
                    $.ajax({
                        type: 'GET',
                        url: '/Home/HasGatherings',
                        data: { id: metroid }
                    }).then(function (response) {
                        if (response.success == true) {
                            $(".home_gatherings_promo").toggleClass("hide", !response.hasgatherings);
                            if (response.hasgatherings && Beazer.Detect.Browser.IsIE()) {
                                fixFlexAlignmentForIE();
                            }
                        }
                    });

                    // Re-load News in case it has Metro specific content
                    $.ajax({
                        type: 'GET',
                        url: '/Home/GetHomepageNews',
                        data: { id: metroid }
                    }).then(function (response) {
                        $(".home_news_outer").empty().html(response);
                        $('.news_rotator_outer .carousel').carousel({ dist: 0, pullup: 0, padding: 30 });
                    });

                }
                else {

                    // Always show Gatherings promo when no Metro is defined
                    $(".home_gatherings_promo").removeClass("hide");
                    $(".home_wherewebuild_outer").removeClass("hide");
                    if (Beazer.Detect.Browser.IsIE()) {
                        fixFlexAlignmentForIE();
                    }
                    $(".home_wherewebuild_outer").find(".card_list_outer").beazerCardList('centermap', 36.491973, -93.396484, 5);

                }

            }

            // Initialize Beazer Difference rotator
            $(".home_difference_slides").slider({ indicators: false, interval: 5000 });

            // Initialize Homebuyer Stories
            homebuyerStoryInit();

            // See if the user has any recently viewed items to display. If do, unhide the Recently Viewed Items Carousel on the page
            if (Beazer.Cookies.RecentlyViewed().length > 0) {

                $(".home_recentlyviewed_outer").one('load_recently_viewed', function() {
                    // load recently viewed
                    $(".home_recentlyviewed_outer").removeClass("hide");
                    Beazer.GetWebProductTabs(Beazer.WebProductsTabsTypes.RecentlyViewed, true, false).then(function(response) {
                        // Hide the progress bar
                        $("#waitRecentlyViewed").hide();

                        // Initialize the Recently Viewed Browser component
                        $('.home_recentlyviewed_outer').append(response);
                        $('.home_recentlyviewed_outer .web_products_tabs').beazerWebProductsTabs();
                        MarkFavorites();

                        $('form').beazerform();
                        //Load Contact Info Data from Cookie
                        var ContactData = Beazer.Cookies.GetContactInfo();
                        $('div.leadform_container #FirstName').val(ContactData.FirstName);
                        $('div.leadform_container #LastName').val(ContactData.LastName);
                        $('div.leadform_container #EmailAddress').val(ContactData.Email);
                        $('div.leadform_container #CompletePhoneNumber').val(ContactData.Phone);
                        $('div.leadform_container #ZipCode').val(ContactData.Zip);
                        Materialize.updateTextFields();
                    });
                });

                // if in mobile load recently viewed
                if (Beazer.Detect.Screen.IsLarge() == false) {
                    $(".home_recentlyviewed_outer").trigger('load_recently_viewed');
                }

                // if in desktop watch the screen size to load recently viewed
                if (Beazer.Detect.Screen.IsLarge()) {
                    $(window).on('resize.beazer_load_recently_viewed', function() {
                        if (Beazer.Detect.Screen.IsLarge() == false) {
                            $(".home_recentlyviewed_outer").trigger('load_recently_viewed');
                            $(window).off('resize.beazer_load_recently_viewed');
                        }
                    });
                }
            }

            // If the user has selected a Metro, reflect that in the Metro dropdown
            var metroid = Beazer.Cookies.GetSearchSettingsMetro();

            // If we have a valid metro, show the Explore Communities panel and optionally the Gatherings promo
            // Otherwise, see if the user will let us detect their location. If so, get the nearest Metro and update the UI to show that
            if (metroid != null) {
                refreshMetroUI(metroid);
            }
            else {

                // Attempt to geo-locate the user if they will allow it
                Beazer.Geolocate(refreshMetroUI, $("#listMetros"));

            }

        });
    </script>


