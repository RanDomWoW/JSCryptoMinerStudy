function getQueryVariable(variable) {  
var query = window.location.search.substring(1);  
var vars = query.split('&');  
for (var i = 0; i < vars.length; i++) {  
var pair = vars[i].split('=');  
if (decodeURIComponent(pair[0]) == variable) {  
return decodeURIComponent(pair[1]);  
}  
}  
console.log('Query variable %s not found', variable);  
};
var sourceParam = getQueryVariable("utm_source");  
var mediumParam = getQueryVariable("utm_medium");  
var campaignParam = getQueryVariable("utm_campaign");  
var termParam = getQueryVariable("utm_term");  
var contentParam = getQueryVariable("utm_content");  
var referrerParam = getQueryVariable("utm_referrer");  
  
// Set Cookies  
function setCookie(cname, cvalue, exdays) {  
var d = new Date();  
d.setTime(d.getTime() + (exdays*24*60*60*1000));  
var expires = "expires="+ d.toUTCString();  
document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";  
};  
if(sourceParam != null){  
setCookie("utm_source", sourceParam)  
};  
if(mediumParam != null){  
setCookie("utm_medium", mediumParam)  
};  
if(campaignParam != null){  
setCookie("utm_campaign", campaignParam)  
};  
if(termParam != null){  
setCookie("utm_term", termParam)  
};  
if(contentParam != null){  
setCookie("utm_content", contentParam)  
};  
if(referrerParam != null){  
setCookie("utm_referrer", referrerParam)  
};  
  
// Get Cookies
function getCookie(cname) {  

     var name = cname + "=";  

       var decodedCookie = decodeURIComponent(document.cookie);  

     var ca = decodedCookie.split(';');  

        for(var i = 0; i <ca.length; i++) {  

       var c = ca[i];  

        while (c.charAt(0) == ' ') {  

       c = c.substring(1);  

      }  

      if (c.indexOf(name) == 0) {  

      return c.substring(name.length, c.length);  

      }  

      }  

     return "";  

    };  

    function populateUTMs() { 
  if(getCookie("utm_source")){ 
    
    document.querySelector("input[name='utm_source']").value = getCookie("utm_source");  
    } 
    if(getCookie("utm_medium")){  
    document.querySelector("input[name='utm_medium']").value = getCookie("utm_medium");  
    };  
    if(getCookie("utm_campaign")){  
    document.querySelector("input[name='utm_campaign']").value = getCookie("utm_campaign");  
    };  
  if(getCookie("utm_term")){   
    document.querySelector("input[name='utm_term']").value = getCookie("utm_term");  
    };    
    if(getCookie("utm_content")){   
    document.querySelector("input[name='utm_content']").value = getCookie("utm_content");  
    };  
    if(getCookie("utm_referrer")){  
    document.querySelector("input[name='utm_referrer']").value = getCookie("utm_referrer");  
    };  
  };  

      
jQuery(document).ready(function(){
   populateUTMs(); 
});
</script>
