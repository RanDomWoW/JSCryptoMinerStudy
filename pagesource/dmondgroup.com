                                else if (href.includes('facebook')) {
                                    $(this).find('i').addClass('fa-facebook');
                                }
				else if (href.includes('t.me')) {
				    $(this).find('i').addClass('fa-telegram');
				}
                                else {
                                    $(this).find('i').addClass('fa-linkedin');
                                }
                            });
                        });
                    </script>

                </ul>
            </div>
        </div>
    </div>
</footer>
