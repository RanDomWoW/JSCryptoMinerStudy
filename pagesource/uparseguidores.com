		function scroll(elem_id){
			setTimeout(function(){
				if (navigator.userAgent.match(/(iPod|iPhone|iPad|Android)/)) {           
					window.scrollTo(0,$(elem_id).offset().top-100); // first value for left offset, second value for top offset
					document.getElementById(elem_id.substr(1)).scrollIntoView();
				}else{

					$('html, body').animate({ scrollTop: $(elem_id).offset().top-100 }, 1000);
				}
			},100);
			return false;
		}
