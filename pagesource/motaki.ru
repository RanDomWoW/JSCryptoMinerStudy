<link rel="stylesheet" type="text/css" href="catalog/view/theme/default/stylesheet/sphinxAutocomplete.css">
				    <script type="text/javascript">
		var wait;
		var selector = '#search input';
		var catTitle = 'Категории';
		var prodTitle = 'Товары';
		var viewAllTitle = 'Показать все';
		var noResTitle = 'Ничего не найдено';
		
		$(document).ready(function(){
			$(selector).after('<div class="sphinxsearch"></div>').blur(function(){
				clearTimeout(wait);
				wait = setTimeout(hideSearch, 300);
			}).keydown(function(e){
				if ($('.sphinx-product').length && e.which == 38) {
					e.preventDefault();
					return false;
				}
				if(e.which == 13) {
					xhr.abort();
					var url = 'https://motaki.ru/search/?wildcard=true&search=' + encodeURIComponent($(selector).val());
					if(url != undefined) {
						location.href = url;
					}
					return false;
				}
			}).keyup(function(e){
				if (!$(selector).val()) {
					clearTimeout(wait);
					wait = setTimeout(hideSearch, 300);
				}
				if ($(selector).val().replace(/^\s+|\s+$/g, '') && (e.which == 8 || (47 < e.which && e.which < 112) || e.which > 185)) {
					clearTimeout(wait);
					wait = setTimeout(sphinxSearch, 300);
				}
				if ($(selector).val() == '') {
					hideSearch();
				}
			});
		});
		
		function sphinxSearch() {
			$(selector).next().html('<div class="sphinx-viewall"><img src="catalog/view/theme/default/image/loading.gif" /></div>').show();
			xhr = $.ajax({
				url: 'https://motaki.ru/index.php?route=module/sphinxautocomplete&search=' + $(selector).val(),
				dataType: 'json',
				success: function(data) {
					var html = '';
					
					//Categories
					
					if (data.categories.length) {
						html += '<div class="categories"><span>'+catTitle+'</span>';
						var categories = data.categories;
						for (i = 0; i < categories.length; i++) {
							html += '<a class="sphinx-product-cat" href="' + categories[i]['href'] + '">';
								if(categories[i]['image'] != '') {
									html += '<img src="' + categories[i]['image'] + '" />';
								}
								html += categories[i]['name'];
								html += '<br />';
							html += '</a>';
						}
						html += '</div>';
					}
					
					//Products
					if (data.products.length) {
							
						if (data.categories.length) {html += '<div class="categories"><span>'+prodTitle+'</span></div>';   }
						var products = data.products;
						for (i = 0; i < products.length; i++) {
							html += '<a class="sphinx-product" href="' + products[i]['href'] + '">';
								
								html += '<img src="' + products[i]['image'] + '" />';
								html += '<div class="product-name ">';
								html += products[i]['name'];
								html += '</div>';
								html += '<div>';
									if (products[i]['quantity'] > 0) { 
										html += '<span class="yes quantity">В наличии</span>';
									} else {
										html += '<span class="no quantity">Нет в наличии</span>';
									}
								if (products[i]['special']) {
									html += '<span class="price-old">' + products[i]['price'] + '</span>';	
									html += '<span class="price">' + products[i]['special'] + '</span>';
								} else {
									html += '<span class="price">' + products[i]['price'] + '</span>';
								}
								html += '</div>'
							html += '</a>';
						}
					//	if (data.categories.length) {  html += '</div>'; }
						html += '<div class="sphinx-viewall"><a id="view-all" href="https://motaki.ru/search/?wildcard=true&amp;search=' + encodeURIComponent($(selector).val()) + '">'+viewAllTitle+'</a></div>';
					} else {
						html = '<div class="sphinx-viewall">'+noResTitle+'</div>';
					}
					$(selector).next().html(html);
				}
			});
		}
		
		function hideSearch() {
			$('.sphinxsearch').hide();
		}
	</script>

