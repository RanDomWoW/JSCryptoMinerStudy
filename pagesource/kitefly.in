<script type="text/javascript">
		var autocomplete; 
		function initialize() {
			var input = document.getElementById('city_autocomplete_input');
			var options = {
				types: ['(regions)']
			};
			autocomplete = new google.maps.places.Autocomplete(input,options);
			autocomplete.addListener('place_changed', fillInAddress);
		}
		// google.maps.event.addDomListener(window, 'load', initialize);
		function fillInAddress() {
			// Get the place details from the autocomplete object.
			var place = autocomplete.getPlace();
			var data = {};
			// Pull out country code, city and subcity from address
			for (var i = 0; i < place.address_components.length; i++) {
				var component = place.address_components[i] ;
				var addressType = component.types[0];
				if (addressType == "country"){
					data["country_code"] = component.short_name; 
					data["country"] = component.long_name; 
				} else if (addressType == "locality"){
					data["city"] = component.long_name; 
				} else if (addressType == "sublocality" || addressType == "sublocality_level_1"){
					data["subcity"] = component.long_name; 
				} else if (addressType == "administrative_area_level_1"){
					data["state_code"] = component.short_name; 
					data["state"] = component.long_name; 
				}
			}
			data["place_id"] = place.place_id ;
			data["name"] = place.name ;
			data["lat"] = place.geometry.location.lat() ;
			data["lon"] = place.geometry.location.lng() ;
			for (var key in data) {
				$("#city_"+key).val(data[key]); 
			}
	   }

	</script>