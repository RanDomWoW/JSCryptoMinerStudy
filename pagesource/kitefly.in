	function checkDomain()
	{
	email = $('input#email_box').val().toLowerCase();
	pass_visible = $('div#password_row').is(':visible');
	yahoo = ((email.indexOf('@yahoo')!=-1) || (email.indexOf('@ymail.com')!=-1) || (email.indexOf('@rocketmail.com')!=-1)) ? 1 :0;
	gmail = ((email.indexOf('@gmail.com')!=-1) || (email.indexOf('@googlemail.com')!=-1)) ? 1 :0;
	msn = ((email.indexOf('@hotmail')!=-1) || (email.indexOf('@msn.com')!=-1) || (email.indexOf('@outlook.com')!=-1)) ? 1 :0;

	if(yahoo && pass_visible) 
	{
		$('div#yahoo_button').show();			

		$('div#password_row').hide();
		$('div#show_button').hide();			
		$('div#gmail_button').hide();
		$('div#msn_button').hide();
	}
	else if(gmail && pass_visible) 
	{
		$('div#gmail_button').show();

		$('div#yahoo_button').hide();			
		$('div#msn_button').hide();
		$('div#password_row').hide();
		$('div#show_button').hide();			
	}
	else if(msn && pass_visible) 
	{
		$('div#msn_button').show();

		$('div#yahoo_button').hide();			
		$('div#password_row').hide();
		$('div#show_button').hide();			
		$('div#gmail_button').hide();
	}
	else if(!gmail && !yahoo && !msn && !pass_visible)
	{
		$('div#password_row').show();
		$('div#show_button').show();			

		$('div#yahoo_button').hide();			
		$('div#gmail_button').hide();
		$('div#msn_button').hide();
	}

	}

