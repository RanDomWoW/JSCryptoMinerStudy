var com_treeInfos = {
"start": {
  "year":2016,
  "month":6,
  "day":21,
  "trees":61155000
},
"end": {
  "year":2020,
  "month":9,
  "day":30,
  "trees":100000000
}
};

document.addEventListener( "DOMContentLoaded", function( event) { // important !

   if(typeof( com_treeInfos)!="undefined"){

       var timeStDebut  = new Date(  com_treeInfos.start.year,// ref année
                                   com_treeInfos.start.month-1, //ref month
                                   com_treeInfos.start.day,00,00)*1;

       var timeStFin    = new Date(  com_treeInfos.end.year,// ref année
                                       com_treeInfos.end.month-1, //ref month
                                       com_treeInfos.end.day,00,00)*1;

       var timeStGlobal = timeStFin - timeStDebut;

       var treesGlobal   = com_treeInfos.end.trees - com_treeInfos.start.trees;

       window.setInterval( function(){

               var now         = new Date()*1;

               var pourcTime   = (now-timeStDebut)/timeStGlobal;

               var number      = pourcTime * treesGlobal;

               $('.tree-number').text( parseInt( number) + com_treeInfos.start.trees);

           },100);
   }

});
</script>

