                        </li></ul>                        <div id="controles">
                            <i class="fa fa-angle-left locked" aria-hidden="true"></i>
                            <i class="fa fa-angle-right" aria-hidden="true"></i>
                        </div>
                    </div>
            <script>
                $(document).ready(function(){
                    var largeur;
                    var x = 1;
                    var maxX = $('#temoignages li').length;     // gère les débordements
                    var automatisation;

                    $( window ).resize(function() {
                        placement();
                    });

                    caroussel($('#temoignages li'));               // init

                    $('#temoignages .fa-angle-left').click(function(){  // écouteurs sur la navigation
                        decalageGauche();
                        clearInterval(automatisation);
                        init();
                    })
                    $('#temoignages .fa-angle-right').click(function(){
                        decalageDroite();
                        clearInterval(automatisation);
                        init();
                    })

                    function caroussel(target){
                        placement();
                        target.click(function(index){
                            decalageDroite();                   // écouteur supplementaire sur les LI
                            clearInterval(automatisation);
                            init();
                        });
                        init();
                    }
                    function placement(){
                        //largeur = $('#temoignages').outerWidth();    //surveille la largeur
                        largeur = $(window).width();
                        x = 1;                                  // reset l'index
                        $('#temoignages .fa-angle-left').addClass('locked');
                        $('#temoignages .fa-angle-right').removeClass('locked');
                        var currentPosY;
                        $('#temoignages li').each(function( index ) {
                            currentPosY = index*largeur;
                            $(this).css({"position":"absolute","left":currentPosY+"px"});   // dipose les éléments au début
                        });
                    }

                    function decalageDroite(){
                        if(x < maxX){
                            x++;
                            $('#temoignages li').animate({left: "-="+largeur}, 500);
                            $('#temoignages .fa-angle-left').removeClass('locked');
                            if( x == maxX){
                                $('#temoignages .fa-angle-right').addClass('locked');
                            }else{
                                $('#temoignages .fa-angle-right').removeClass('locked');
                            }
                        }
                    }
                    function decalageGauche(){
                        if(x > 1){
                            x--;
                            $('#temoignages li').animate({left: "+="+largeur}, 500);
                            $('#temoignages .fa-angle-right').removeClass('locked');
                            if( x == 1){
                                $('#temoignages .fa-angle-left').addClass('locked');
                            }else{
                                $('#temoignages .fa-angle-left').removeClass('locked');
                            }
                        }
                    }

                    function init(){
                        automatisation = setInterval(function(){         // automatisation
                            if(x < maxX){
                                decalageDroite();
                            }else{
                                decalageGauche();
                            }
                        }, 6000);
                    }
                    
