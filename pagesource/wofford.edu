    <script type="text/javascript">
        $(document).ready(function () {

            var siteURL = "http://wofford.edu";

            $("input.txt").keydown(function (event) {
                value = $(this).val();
                var myArray = value.split(' ');
                var newValue = '';
                for (var i = 0; i < (myArray.length - 1) ; i++) {
                    newValue += myArray[i];
                    newValue += '+';
                }
                newValue += myArray[myArray.length - 1];
                if ((event.keyCode == 13 && newValue != '+')) {
                    event.preventDefault();
                    window.location = siteURL + "/search/searchSiteResults.aspx?keyword=" + newValue;
                }
            });
            $("input.submit").click(function (e) {
                value = $("input.txt").val();
                var myArray = value.split(' ');
                var newValue = '';
                for (var i = 0; i < (myArray.length - 1) ; i++) {
                    newValue += myArray[i];
                    newValue += '+';
                }
                newValue += myArray[myArray.length - 1];
                if ((newValue != '+')) {
                    e.preventDefault();
                    window.location = siteURL + "/search/searchSiteResults.aspx?keyword=" + newValue;
                }
            });

            $('.flexslider').flexslider({
                animation: "slide",
                controlNav: false,
                slideshow: false,
                after: function (slider) {
                    var theCurrentSlide = slider.currentSlide;
                    $(".col-l img").removeClass("activeTagline");
                    $(".col-l img").eq(theCurrentSlide).addClass("activeTagline");
                    $(".col-r p").removeClass("activeCaption");
                    $(".col-r p").eq(theCurrentSlide).addClass("activeCaption");
                }
            });

            //Corrects all http://.pdf OR .doc OR .docx OR .xls OR .xlsx to https://.pdf OR .doc OR .docx OR .xls OR .xlsx
            $("a").each(function () {
                var fileExtension = $(this).prop("href").substr($(this).prop("href").lastIndexOf('.')).toLowerCase();

                if (fileExtension == '.pdf' || fileExtension == '.doc' || fileExtension == '.docx' || fileExtension == '.xls' || fileExtension == '.xlsx') {
                    if ($(this).prop("href").substring(0, 5) != 'https' && $(this).prop("href").indexOf("webs.wofford.edu") == -1) {
                        $(this).prop("href", 'https' + $(this).prop("href").substring(4));
                    }
                }
            });

        });

    </script>
