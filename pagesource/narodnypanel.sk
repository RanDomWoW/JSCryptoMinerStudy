	//additional validation methods
	function validateDate(date)
	{
		dateRegex = /^(((0?[1-9]|1\d|2[0-8])\.(0?[1-9]|1[012])|(29|30)\.(0?[13456789]|1[012])|31\.(0?[13578]|1[02]))\.(19|[2-9]\d)\d{2}|29\.0?2\.((19|[2-9]\d)(0[48]|[2468][048]|[13579][26])|(([2468][048]|[3579][26])00)))$/
		return date.match(dateRegex);
	}

	function validateTime(time)
	{
		timeRegex = /^(([0-9])|([0-1][0-9])|([2][0-3])):(([0-9])|([0-5][0-9]))(:[0-5]\d)?$/;
		return time.match(timeRegex);
	}
	function validateBirthCode(birthCode)
	{
		birthCodeRegex = /^[0-9]{6}\/[0-9]{3,4}$/;
		return birthCode.match(birthCodeRegex);
	}
		
	function validateBirthDate(birthDate)
	{	
		birthDateRegex = /^(0?[1-9]|[12][0-9]|3[01])\.(0?[1-9]|1[012])\.((19|20)\d\d)$/;
		return birthDate.match(birthDateRegex);
	}

	function validateAccountPartNumber(partNumber)
	{
		accountPartNumberRegex = /^[0-9]{4,10}$/;
		return partNumber.match(accountPartNumberRegex);
	}

	function validateBankCode(bankCode)
	{
		bankCodeNumberRegex = /^[0-9]{4}$/;
		return bankCode.match(bankCodeNumberRegex);
	}


	function validatePhoneNumber(phoneNumber)
	{	
		var locale="sk";
		// cs, sk and pl all have the same phone formats
		switch (locale){
			case "sk":
				phoneNumberRegex = /^(0?[0-9]{9})$/;
				return phoneNumber.match(phoneNumberRegex);						
				break;
			default:
				phoneNumberRegex = /^([0-9]{9})$/;
				return phoneNumber.match(phoneNumberRegex);
				break;
		}
	}

	function validateInteger(integer)
	{
		integerRegex = /^((\+)|(-))?[0-9]*$/;
		return integer.match(integerRegex);
	}

	function validateNationality(element) {
		return validateOtherOption(element, "nationalityOther", 1)
	}
	function validateNation(element) {
		return validateOtherOption(element, "nationOther", 1)
	}				

	/*
	 * Validates radio buttons where one option is something like "other" and means another field has to be filled.
	 *@param element - one (any) of a set of radio buttons with same "name" parameter
	 *@param id - id of the field which has to be filled
	 *@param otherIndex  of the "other" radio button FROM THE END of set of radio buttons, zero-based (i.e. last element has otherIndex==0)
	 */
	function validateOtherOption(element, id, otherIndex)	{
		//jQuery Validate always tries to validate the first radio button=>we must find the last visible one (the very last one is hidden)...
		var radioButtons=$('input[type="radio"][name="'+$(element).prop("name")+'"]');				
		var lastErrorLabel=$(radioButtons.get(radioButtons.length-1-otherIndex));
		//option "Other" was selected
		if ($(lastErrorLabel).prop("checked")==true) {
			var inputValue=$("#"+id).prop("value");
			return ( inputValue!=''  );
		}
		else return true;
	}


$(document).ready(function() {
          
	/**
	* Because we are sending the form using form.submit, we have to add hidden input
	* on valid click, because form.submit cant send an input button we are submitting the
	* form with
	*/
   $('input[type=submit]').click(function (e) {
	   // remove all previously manually added hidden items
	   $('.my-added-hidden-item').remove();
	   // check whether the form is valid
	   var form = $(this).parents('form');
	   if ($(form).valid()) {
		   var newElement = '<input type="hidden" class="my-added-hidden-item" name="' + $(this).prop('name') + '" value="' + $(this).prop('value') + '">'
		   $('form').append(newElement);
	   } else {

	   }
   });

   //setting validation for the forms
   $("#form").validate({
		   focusInvalid: true,
		   onfocusout: true,
		   onfocus: true,
		   onfocusout: function(element) { $(element).valid(); },   
		   submitHandler: function(form) {
			   if(!this.wasSent) {
				   this.wasSent = true;
				   safeSubmit('');
				   $(':submit', form).val('Čakajte...')
									 .prop('disabled', 'disabled')
									 .addClass('disabled');
				   form.submit();
			   } else {
				   return false;
			   }
		   },
			errorPlacement: function(error, element) {
				//jQuery.validate locates the element by name, not id. In case of Radio buttons, it therefore puts the error message after the first
				//radio button=>we must move it manually. 		
				if( element.prop("type")=="radio") {
				   var forID=element.prop("name");
				   var inputs=$('input[type="radio"][name="'+forID+'"]:not(.hiddenRadio)');
				   var lastInput = $(inputs.get(inputs.length-1));
				   var lastLabel = $('label[for="'+ lastInput.attr("id") +'"]');
				   if (lastLabel.length == 0) {
					   error.appendTo(lastInput.parent());
					   error.css("display", "inline");
					} else {
						error.insertAfter(lastLabel);
					}
				   
					//correct display of elements
					$('input[type="radio"][name="'+forID+'"]').css("display","inline");
					$('input[type="radio"][name="'+forID+'"].hiddenRadio').hide();
				}
				else { 
					error.insertAfter(element);
				}
			}
   });


	

});
