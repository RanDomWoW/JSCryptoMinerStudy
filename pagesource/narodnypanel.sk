	$.validator.addMethod(
	   'birth_code',
	   function(value, element) {
		   return (value == '' && this.optional(element)) || validateBirthCode(value);
	   },
	   '`Nesprávny formát rodného čísla. Rodné číslo zadávajte s lomítkom (napr. 770225/2365).'
	);
	
	$.validator.addMethod(
	   'datetime_cz',
	   function(value, element) {
		   dateTime = value.split(' ', 2);
		   date = dateTime[0];
		   time = dateTime[1];
		   return (value == '' && this.optional(element)) || (validateDate(date) && validateTime(time));
	   },
	   'Nesprávny formát dátumu alebo času.'
   );

   
   $.validator.addMethod(
	   'birth_date',
