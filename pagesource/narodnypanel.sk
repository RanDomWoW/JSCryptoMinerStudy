   $.validator.addMethod(
	   'radioButton',
	   function(value, element) {								
		   //jQuery Validate always takes the value of the FIRST radio button=>check manually if 
		   //the hidden radio button is selected == user did not select anything
		   var radioButtons=$('input[type="radio"][name="'+$(element).prop("name")+'"].hiddenRadio');	
		   var lastButton=$(radioButtons.get(radioButtons.length-1));							
		   //no value was selected
