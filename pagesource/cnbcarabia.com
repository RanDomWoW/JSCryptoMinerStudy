    $(window).resize(function() {
        fixTakeovers();
    });

    function fixTakeovers(){
        windowWidth = $(window).width();
        siteWidth = $(".container").first().width();
        if((windowWidth - siteWidth) < 220){
            $(".takeoverDiv1").hide();
            $(".takeoverDiv2").hide();
        }else{
            $(".takeoverDiv1").show();
            $(".takeoverDiv2").show();

            $(".takeoverDiv1").css("left",(((windowWidth - siteWidth)/2)-125)+"px");
            $(".takeoverDiv2").css("right",(((windowWidth - siteWidth)/2)-125)+"px");
        }
    }
</script>



