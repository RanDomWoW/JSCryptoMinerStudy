        $('li[data-target="#bottomCarousel"]').each(function () {
            $(this).click(function () {

                slideToIndex = $(this).attr("data-slide-to");
                divLength = $('div.item[data-id="bottom-id-' + slideToIndex + '"]').length;
                $('div.item[data-id^="bottom-id-"]').each(function () {
                    $(this).removeClass("active");
                });

                $('li[data-target="#bottomCarousel"]').each(function () {
                    $(this).removeClass("active");
                });
                $(this).addClass("active");
                $('div.item[data-id="bottom-id-' + slideToIndex + '"]').addClass('active');
                return false;
            });
        });

        $("#bottomCarousel").swiperight(function () {

            $(this).carousel('prev');
        });
        $("#bottomCarousel").swipeleft(function () {

