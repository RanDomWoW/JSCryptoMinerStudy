<script>

    $(document).ready(function () {

        $(".common-nav-li").find(".dropdown-toggle").each(function(){
            $(this).click(function(){
                window.location = $(this).attr("href");
            });
        });


        $('#header-search')
                .on('input', function () {
                    searchKeyword = $(this).val();

                    $('#search-keyword-a').attr("href", "/search?search=" + searchKeyword);

                    $('#search-keyword').empty().html(searchKeyword);

                    var i = 0;
                    if (searchKeyword.length >= 1) {
                        $('ul#content').removeClass('hidden');
                        if ($('ul#content').html()) {
                            $('ul#content .search-market').empty();
                            $.post('/market-search', {keywords: searchKeyword}, function (data) {
                                $.each(data, function (index, obj) {
                                    if($("#searchMarket"+obj.id).length == 0) {
                                        $('ul#content').prepend(
                                                '<li class="search-market" id="searchMarket'+obj.id+'" style="border-bottom:1px solid rgba(0, 0, 0, 0.1);float:right;width:90%;padding-left:5%;padding-right:5%;font-size:12px;"><a href="/markets/ticker/' + obj.id + '">' +
                                                '<div class="search-market-title col-xs-3 text-right" style="padding: 5px 10px;border:0; text-align: left;">' + obj.title + '</div>' +
                                                '<div class="search-market-desc col-xs-9 text-left" style="padding:0; text-align: right; border:0;">' + obj.description + ' - ' + obj.marketsymbol + '</div></a></li>');
                                    }
                                });
                            }, "json");
                        }
                    } else {
                        $('ul#content').empty().addClass('hidden');
                    }
                })
                .on('keyup', function () {
                    $('span#search-keyword').text($(this).val());
                    $('li a#search-link').attr('href', '/search?search=' + $(this).val());
                });
    });

    $(document).scroll(function () {
        var y = $(this).scrollTop();
        if (y > 103) {
            $('.menu-top').addClass('fixed-menu');
        } else {
            $('.menu-top').removeClass('fixed-menu');
        }
    });

</script>
