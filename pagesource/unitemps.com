        }

        $('.location-icon')
            .on('click',
                function () {

                    $('.location-input').val(userLocation);
                    navigator.geolocation.getCurrentPosition(function (position) {

                        geocoder.geocode({ 'latLng': new google.maps.LatLng(position.coords.latitude, position.coords.longitude) }, function (results, status) {
                            if (status == google.maps.GeocoderStatus.OK) {

                                var country = '';

                                for (var i = 0; i < results[0].address_components.length; i++) {
                                    if ((results[0].address_components[i].short_name === 'UK' || results[0].address_components[i].short_name === 'GB') && results[0].address_components[i].long_name === 'United Kingdom') country = 'UK';
                                }

                                if (country === 'UK') {
                                    $('#RegionId').val(1);
                                    $('#Distance > option[value="2"]').html('2 miles')
                                    $('#Distance > option[value="5"]').html('5 miles')
                                    $('#Distance > option[value="10"]').html('10 miles')
                                    $('#Distance > option[value="25"]').html('25 miles')
                                }
                                else {
                                    $('#RegionId').val(2);
                                    $('#Distance > option[value="2"]').html('2 km')
                                    $('#Distance > option[value="5"]').html('5 km')
                                    $('#Distance > option[value="10"]').html('10 km')
                                    $('#Distance > option[value="25"]').html('25 km')
                                }
                                $('#Distance').removeAttr('disabled');
                                $('#Distance option[value="5"]').prop('selected', true);
                            }

                        });


                        $('#Latitude').val(position.coords.latitude);
                        $('#Longitude').val(position.coords.longitude);
                    });
                    $('#RegionIds').prop('disabled', ($(this).val() == ''));
                    $('#Distance').removeAttr('disabled');
                    $('#Distance option[value="5"]').prop('selected', true);

                });

        $('.location-input').on('input', function () {
            var address = $(this).val();

            $('#Latitude').val('0');
            $('#Longitude').val('0');

            $('#Distance').val("")
            $('#Distance').attr('disabled', true);
        });

        $('#search-form').on('submit', function () {
            var location = $('#Location').val();
            var latitude = $('#Latitude').val();
            var longitude = $('#Longitude').val();

            if (location && (latitude == '0' || longitude == '0')) {
                alert('Please select a location');
                return false;
            }
        });


    </script>


