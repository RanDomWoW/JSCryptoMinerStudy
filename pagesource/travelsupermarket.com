    <script>
    if(document.getElementById('googleDfpControl')){
        var googleDfpControl = JSON.parse(document.getElementById('googleDfpControl').title),
        adsEnabled = googleDfpControl.enabled,
        heroAdDesktopEnabled = googleDfpControl.opts.page.hero,
        heroAdMobileEnabled = googleDfpControl.opts.page.heroMobile,
        desktopColor = googleDfpControl.opts.page.backgroundcolor,
        mobileColor = googleDfpControl.opts.mobile.backgroundcolor,
        isMobile = document.body.clientWidth <= 900,
        superHomeDiv = document.getElementsByClassName('section-content main-bg--hero-section hero-section hero-section-wrapper')[0],
        landingDiv = document.getElementsByClassName('hero-section clearfix hero-section--partial-height main-bg--hero-section')[0],
        runHeroAd = false;

        if((heroAdDesktopEnabled && !isMobile) || (heroAdMobileEnabled && isMobile)){
            runHeroAd = true;
        }

        if(heroAdDesktopEnabled && runHeroAd){
            if(superHomeDiv){
                superHomeDiv.style.setProperty("background-color", desktopColor);
                superHomeDiv.style.setProperty("background-image", "none");
            }
            else if(landingDiv){
                landingDiv.style.setProperty("background-color", desktopColor);
                landingDiv.style.setProperty("background-image", "none");
            }
        }
    }
</script></div>
