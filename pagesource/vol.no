function getIdFromUrl(url){
   var id = 0;
    var afterArticleStr = url.substring(url.lastIndexOf("article")+"article".length);
    if (afterArticleStr.indexOf(".ece") > -1) {
       id = afterArticleStr.substring(0,afterArticleStr.lastIndexOf(".ece"));
    } else if (afterArticleStr.indexOf(".snd")) {
       id = afterArticleStr.substring(0,afterArticleStr.lastIndexOf(".snd"));
    }
    return id;
}

requireReady(['jquery','lib/jquery.cookie'], function () {
var variant = $.cookie("variant");
if (typeof variant==="undefined") {
    var randomOf2 = Math.floor(Math.random()*2);
    variant = "A";
    if (randomOf2 == 1) {
        variant = "B";
    }
    $.cookie("variant",variant); 
    /*console.log("variant cookie set to: " + variant);*/
} else {
    /*console.log("variant cookie has value: " + variant);*/
}

if (variant=="B") {
    $(".title-b").each(function(index,value){
        var titleb = $(value).text(); 
        if ($.trim(titleb.length) > 0) {
            if (titleb.indexOf("#") > -1) {
               var titleSizeClass = "t" + titleb.substring(titleb.lastIndexOf("#")+1,titleb.length);
               titleb =  titleb.substring(0,titleb.lastIndexOf("#"));
               $(value).parent().parent().find(".headline span").first().attr("class",titleSizeClass);
            }
            $(value).parent().parent().find(".headline span a").first().text(titleb);
        }
    });
}

$(".title-b").each(function(index,value){
    if ($.trim($(value).text().length) > 0) {
        var url = $(value).parent().find(".headline span a").first().attr("href");
        var id = getIdFromUrl(url);
        var title = $.trim($(value).parent().find(".headline span a").first().text());
        var eventAction = "Visning " + variant + ": " + title;
        /*console.log(eventAction);*/
        _gaq.push(['_trackEvent', 'AB', eventAction, id]);
    }    
});

$(".article").click(function(e){
    var url = $(this).find(".headline span a").attr("href");
    var id = getIdFromUrl(url);
    var titleInUse = $.trim($(this).find(".headline span a").first().text());
    if ($(this).find(".title-b").length > 0) {
        var eventAction  = "Klikk " + variant + ": " + titleInUse;
        /*console.log(eventAction);*/
        _gaq.push(['_trackEvent', 'AB', eventAction, id]);
    }
});
});


