            topPosition = (window.screen.height / 2) - ((320 / 2) + 50);
            windowFeatures = "status=no,height=" + 320 + ",width=" + 520 + ",resizable=yes,left=" + leftPosition + ",top=" + topPosition + ",screenX=" + leftPosition + ",screenY=" + topPosition + ",toolbar=no,menubar=no,scrollbars=no,location=no,directories=no";
