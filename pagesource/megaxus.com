            if(sharer === "fb") urlShare = "https://www.facebook.com/sharer.php?u="+encodeURIComponent(urlToShare)+"&t="+encodeURIComponent(title);
            else if(sharer === "tw") urlShare = "https://twitter.com/intent/tweet?text="+encodeURIComponent(title).substr(0,118)+"&url="+encodeURIComponent(urlToShare);
            else if(sharer === "g+") urlShare = "https://plus.google.com/share?url="+encodeURIComponent(urlToShare);
            
            window.open(urlShare, sharer, windowFeatures);
            
            return false;
        }
