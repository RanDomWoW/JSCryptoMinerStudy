						if ($("#location-slug").val()){
							window.location = "/listings/" + $("#location-slug").val() + "/" + $category.val();
						}
					}
				}
				else if(citySlug){
					window.location = "/listings/" + citySlug;
				}
				else{
					window.location = "/listings/loveland-co-usa";
				}
			});
		
});</script>
