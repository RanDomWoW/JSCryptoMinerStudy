	<script type="text/javascript">
	function returnObjDate(sDate) {
		if((sDate != undefined) && (sDate != "")) {
			var sData = sDate.split("T")[0];
			var sTime = sDate.split("T")[1] || "";
			var sYear = parseInt(sDate.substring(0, 4));
			var sMonth = parseInt(sDate.substring(4, 6) - 1);
			var sDay = parseInt(sDate.substring(6,8));

			var sHours = (sTime != "") ? sTime.substring(0, 2) : 00;
			var sMinutes = (sTime != "") ? sTime.substring(2, 4) : 00;

			return new Date(sYear, sMonth, sDay, sHours, sMinutes);
		}
		else {
			return "";
		}
	}

	function checkVisibility(jEl) {
		var dToday = new Date();
		var bIsAdmin = (typeof fContextMenu == "function");
		for(var x = 0; x != jEl.length; x++) {
			var jThis = $(jEl[x]);
			var dFrom = returnObjDate( jThis.attr("data-visible-from") );
			var dTo = returnObjDate( jThis.attr("data-visible-to") );
			var bVisible = jThis.is(":visible");
			var hadToBeVisible = ((dFrom <= dToday) || (dFrom == "")) && ((dTo >= dToday) || (dTo == ""));
			
			if(!bVisible && hadToBeVisible || bIsAdmin) {
				jThis.removeClass("hidden");
			}
			else if(bVisible && !hadToBeVisible && !bIsAdmin) {
				jThis.addClass("hidden");
			}
		}
	}
  
  	function checkStopBubbling() {
		$("a.stopPropagation").click(function(e){
			e.stopPropagation();
		});
	}

	$(function() {
		checkVisibility($("*[data-visibility]"));
	  	checkStopBubbling();
	});
</script>
	<script type="text/javascript">
function init() {
	window.addEventListener('scroll', function(e){
		if($(window).outerWidth() <= 767) {
			var distanceY = window.pageYOffset || document.documentElement.scrollTop,
			shrinkOn = 50,
			header = $("#fascia-logo");
			$("#main-menu .navbar-collapse").css({
				"position": "fixed",
				"width": "100%",
				"z-index": 999
			});
			if (distanceY > shrinkOn) {
				$(".col-xs-10", header).removeClass("col-xs-10").addClass("col-xs-7");
				header.css("box-shadow", "0 2px 3px black");
				$("#main-menu .buttonText").addClass("hidden");
			} else {
				$(".col-xs-7", header).removeClass("col-xs-7").addClass("col-xs-10");
				header.css("box-shadow", "0 none");
				$("#main-menu .buttonText").removeClass("hidden");
			}
		}
	});
}
window.onload = init();
</script>
