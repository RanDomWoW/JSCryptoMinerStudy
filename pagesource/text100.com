<script>

jQuery(document).ready(function($){

    // Detect when scroll reach the bottom of the page
    //function bindScroll() {
    //    if($(window).scrollTop() + $(window).height() == $(document).height()) {
    //        $(window).unbind('scroll');
    //        if ($('#load-more').length > 0) {
    //            loadMore();
    //        }
    //    }
    //}
    //$(window).scroll(bindScroll);
    
    $('.load-more').on('click', function(){
        $('.article-list').addClass('fade');
        loadMore();        
    });

    // Load more items ajax call
    function loadMore() {
        
        var next_page = $('#next_page').val();
        var search = $('#search').val();
        var filters = $('#filters').val();
        var json_filters = JSON.parse(filters);

        var data = {
            'search' : search,
            'paged' : next_page,
            'filters' : filters,
            'class' : 'ajax'
        };

        var auth_obj = {};
        if (window.location.hostname === 'text-100.bugfix.repubdev.com' ||
         window.location.hostname === 'text-100.feature.repubdev.com' ||
         window.location.hostname === 'text-100.staging.repubdev.com'
         ) {
            
            var username = 'text-100';
            var password = 'text-100.text100!';

            auth_obj = {
                beforeSend: function (xhr) {
                    xhr.setRequestHeader("Authorization", "Basic " + btoa(username + ":" + password));
                },
            };
        }

        var request = $.ajax({
            url: 'https://www.text100.com/wp-content/themes/2016/partials/ajax/archive.php',
            type: "post",
            data: data,
            auth_obj
        });

        // Callback handler that will be called on success
        request.success(function (response, textStatus, jqXHR){

            $('#next_page').val(parseInt(next_page)+1);

            // Hide load more button if no more results
            var article_loaded = 0;
            $.each($(response), function() {
                if ($(this).is('article')) {
                    article_loaded++;
                }
            });

            if (article_loaded !== json_filters.posts_per_page) {
                $('.load-more').hide();
            }

            setTimeout(function(){
                $("#load-more").append(response);
                $('.article-list').removeClass('fade');
            }, 300);

            return;
        });

        // Callback handler that will be called on failure
        request.fail(function (jqXHR, textStatus, errorThrown){
            console.error(
                "The following error occurred: "+
                textStatus, errorThrown
            );
        });

        // Callback handler that will be called regardless
        // if the request failed or succeeded
        //request.always(function () {
        //    // Reenable the inputs
        //    $inputs.prop("disabled", false);
        //});

        //$(window).bind('scroll', bindScroll);

    }

});

</script>


