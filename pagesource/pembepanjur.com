/**
 * Generic function to help with posting to pp services
 *
 * custom_success_function parameters_object'te gönderildiğinde istenilen bir success function çağrılabilir
 **/
function generic_ajax_service(action,parameters_object){

    //todo: bu üç kütüphaneden birini kullanmalıyız jquery yerine.
    //https://github.com/github/fetch
    //https://github.com/visionmedia/superagent
    //https://github.com/request/request

    //todo: Eğer IE8 desteğinde sorun olursa o zaman IE8 için belki generic_ajax_service jquery yükleyerek çalışabilir
    //todo: bunu sağlamak için http://requirejs.org/ deneyebiliriz

    ajax_running=true;//A global variable to help detect if a page has an ajax query running


    //Default sorgu türü GET
    ajax_type="GET";

    //Bu servislerin ajax_type'ı POST.
    // Bunların temel özelliği içinde boşluk, quote vb karakterler içerebilecek inputlar kabul ediyor olmaları
    // Ayrıca password, kredi kartı vb bilgi içeren requestler de POST ile gönderiliyor.
    if(
        action=="admin_script_updatefield" ||
        action=="admin_script_updatefieldfieldwithtableandquery" ||
        action=="admin_script_profile_approval" ||
        action=="admin_ajax_script_add_user_action_note" ||
        action=="admin_ajax_script_approve_payment" ||
        action=="admin_ajax_script_chargeback" ||
        action=="admin_ajax_script_refund" ||
        action=="accept_chat" ||
        action=="camera_accept" ||
        action=="camera_decline" ||
        action=="camera_invitation" ||
        action=="camera_invitation_cannot_accept" ||
        action=="camera_invitation_closed" ||
        action=="change_password" ||
        action=="change_forgotten_password" ||
        action=="decline_chat" ||
        action=="delete_user_with_delay" ||
        action=="fetch_reverse_image_search_count_from_google" ||
        action=="get_search_result" ||
        action=="get_search_result_seo" ||
        action=="get_search_result_v2" ||//todonowhaluksearch: Bunu kullanan var mi?
        action=="get_searchresult_v2" ||//todonowhaluksearch: Bunu kullanan var mi?
        action=="hide_user_account" ||
        action=="invite_chat" ||
        action=="insert_success_story_leads_detail" ||
        action=="registration_update" ||
        action=="report_error" ||
        action=="search_record_insert" ||
        action=="send_message" ||
        action=="send_password_reset_email" ||
        action=="set_settings"||
        action=="start_chat" ||
        action=="start_new_popup" ||
        action=="status_update" ||
        action=="submitcomplaint" ||
        action=="translations_insert" ||
        action=="translations_update" ||
        action=="update_caption" ||
        action=="update_success_story" ||
        action=="update_press_story" ||
        action=="update_admin_guide" ||
        action=="update_faq" ||
        action=="update_success_story_leads" ||
        action=="user_left_chat" ||
        action=="write_status_chat_message" ||
        action=="admin_script_updatefieldfieldwithtableandquery"||
        action=="admin_script_updatefield"
    ){
        ajax_type="POST";
    }

    base_url="";
    //Eğer base_url gelirse parameters_object'te onu kullanıyoruz.
    if(typeof parameters_object.base_url != "undefined" && parameters_object.base_url.length>5){
        base_url="//"+parameters_object.base_url;
    }




    if(action.length>1){
        //all good
    }else{
        //Hata loglanır
        report_error_js('no_action_in_ajax','no_action_in_ajax');
        return false;//Invalid action provided
    }

    var post_data=parameters_object;

    var parameters_object_length=Object.keys(parameters_object).length;

    if (parameters_object_length>0){
        //All good
    }else{
        //Hata loglanır
        report_error_js('empty_parameters_object','empty_parameters_object');

        return false;//Something is wrong, post_data is empty
    }



    //isadmin tanımlı
    if(typeof isadmin != "undefined"
        && isadmin>0 //
        && typeof admin_notifications_enabled != "undefined"
        && admin_notifications_enabled==true
    ){

        var n = noty({
                text: action,
                template: '<div class="noty_message"><span class="noty_text" style="word-break: break-all; display:block;"></span><div class="noty_close"></div></div>',
                layout: 'topRight',
                type: 'alert',
                timeout: 4000
            }
        );


    }


    $.ajax({
        type: ajax_type,
        url: base_url+"/app/phpscripts/services/"+action+"/?",
        cache: false,
        data: post_data,
        success: function(data){

            ajax_running=false;
            if(typeof parameters_object.custom_success_function != "undefined" && typeof window[parameters_object.custom_success_function] === 'function') {
                window[parameters_object.custom_success_function](data,parameters_object);
            } else if (typeof window["success_"+action] === "function"){
                window["success_"+action](data,parameters_object);//Pass the returned object to the success function

            }else if(typeof window["success_"+action] == "undefined"){
                send_js_error("undefined success_"+action,"missing_success_function");
                admin_console_log("undefined success_"+action);
            }else{
                //Do nothing
            }

        },
        error: function(data){
            ajax_running=false;
            if(typeof parameters_object.custom_error_function != "undefined" && typeof window[parameters_object.custom_error_function] === 'function') {
                window[parameters_object.custom_error_function](data,parameters_object);
            } else if(typeof window["error_"+action] === "function"){
                window["error_"+action](data,parameters_object);//Pass the returned object to the error function
            }else{
                report_error_js("ajax_error","servis: "+action+" . Ajaxta hata"+"AJAX error in request: " + JSON.stringify(data, null, 2));
                //Do nothing
            }


        }
    });
}

/**
 *
 * @param min
 * @param max
 * @returns {number}
 *
 * İki integer arasında rasgele bir integer verir.
 */
function randomIntFromInterval(min,max){
    return Math.floor((Math.random()*(max-min+1))+min);
}


/**
 *
 * n digit bir rasgele sayı yaratmak için kullanılır. Örnek 10 digit: 2416712743
 * @param digit, default: 10
 * @returns {*}
 */
function get_n_digit_random_integer(digit){
    digit = Number(digit); //Number cast

    //Default değer 10
    if(!digit>0){
        digit=10;
    }

    var min_int=1;
    var max_int=9;
    if(digit>1){
        min_int=Math.pow(10, (digit-1));
        max_int=Math.pow(10, digit)-1;
    }

    return randomIntFromInterval(min_int,max_int);

}


/**
 *
 * @param field
 * @param value
 */
function set_session_only_cookie_js(field,value){

    //Bu uc bilgi de zorunlu
    if (
        typeof cookie_url !== "undefined" &&
        typeof field !== "undefined" &&
        typeof value !== "undefined"
    ) {
        Cookies.set(field, value, { expires: 0, path: "/", domain: "."+cookie_url });
    }else{
        send_js_error("set_session_only_cookiede eksik bilgi cookie_url:"+String(cookie_url)+" field:"+String(field)+" value:"+String(value),"set_session_only_cookie_js");
    }

}

