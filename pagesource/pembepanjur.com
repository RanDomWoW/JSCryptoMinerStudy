var pp_popup_opened = false;


if (!Object.keys) {
    Object.keys = (function () {

        var hasOwnProperty = Object.prototype.hasOwnProperty,
            hasDontEnumBug = !({toString: null}).propertyIsEnumerable('toString'),
            dontEnums = [
                'toString',
                'toLocaleString',
                'valueOf',
                'hasOwnProperty',
                'isPrototypeOf',
                'propertyIsEnumerable',
                'constructor'
            ],
            dontEnumsLength = dontEnums.length;

        return function (obj) {
            if (typeof obj !== 'object' && (typeof obj !== 'function' || obj === null)) {
                throw new TypeError('Object.keys called on non-object');
            }

            var result = [], prop, i;

            for (prop in obj) {
                if (hasOwnProperty.call(obj, prop)) {
                    result.push(prop);
                }
            }

            if (hasDontEnumBug) {
                for (i = 0; i < dontEnumsLength; i++) {
                    if (hasOwnProperty.call(obj, dontEnums[i])) {
                        result.push(dontEnums[i]);
                    }
                }
            }
            return result;
        };
    }());
}



//Start report js errors

var no_of_js_errors_submitted=0;

function send_js_error(error_string,error_type){

    if (typeof isadmin !== "undefined") {
        // all good isadmin is defined
    }else{
        isadmin=0;
    }


    var limit_per_page=2;

    if (typeof page !== "undefined") {
        //page is defined
        if(page=="landing_page"||page=="login_page"){
            limit_per_page=1;//Sometimes we might prefer more errors on landing and login pages, particularly to log front-end user submission errors
        }
    }

    if(no_of_js_errors_submitted>limit_per_page&&isadmin==0){
        return false;//Only two errors per page allowed
    }else{
        no_of_js_errors_submitted++;
    }

    //Check if session_user_id_variable is defined
    if (typeof pp_session_user_id!="undefined"){
        user_id=pp_session_user_id;
    }else if(typeof hd_session_user_id!="undefined"){
        user_id=hd_session_user_id;
    }else{
        user_id=1;
    }



    if (typeof session_gender=="undefined"){
        session_gender=-1;
    }

    if (typeof pp_pst=="undefined"){
        pp_pst=-1;
    }

    $.ajax({
        type: "POST",
        url: "/app/phpscripts/ajax/send_js_errors.php",
        data: "error_string=" + error_string +
        "&user_id="+user_id +
        "&session_gender="+session_gender +
        "&error_type="+error_type +
        "&pp_pst="+pp_pst +
        "&url="+encodeURIComponent(document.URL)
    });
}



