    var number_displayed = false;
    var number_displayed_previous = false;

    function init(initial_number)
    {
        show_number(initial_number);

        for (var i = 0; i < 7; i ++)
        {
            var left_pos = (i * 26) + 'px';
            if(document.getElementById('digit_top_' + i) !== null) { document.getElementById('digit_top_' + i).style.left = left_pos; }
            if(document.getElementById('digit_top_bg_' + i) !== null) { document.getElementById('digit_top_bg_' + i).style.left = left_pos; }
            if(document.getElementById('digit_bottom_' + i) !== null) { document.getElementById('digit_bottom_' + i).style.left = left_pos; }
            if(document.getElementById('digit_bottom_bg_' + i) !== null) { document.getElementById('digit_bottom_bg_' + i).style.left = left_pos; }
        }

        setInterval('inc_number(Math.floor(number_displayed) + (Math.floor(Math.random() * 2) + 1));', 5000);
    }

    function show_number(number)
    {
        number_displayed_previous = number_displayed;

        number = pad(number, 7);

        number_displayed = number;

        if (!number_displayed_previous) number_displayed_previous = number;

        flip_digits_top(0);
    }

    function pad(number_to_pad, new_length)
    {
        number_to_pad += '';

        while (number_to_pad.length < new_length)
        {
            number_to_pad = '0' + number_to_pad;
        }

        return number_to_pad;
    }

    function flip_digits_top(frame)
    {
        for (var i = 0; i < 7; i ++)
        {
            if (frame == 0&&number_displayed[i]>=0)
            {
                position_pixels=(24 * number_displayed[i]);

                if(document.getElementById('digit_top_' + i) !== null) { document.getElementById('digit_top_' + i).style.backgroundPosition = '-' + position_pixels + 'px 0px'; }
                if(document.getElementById('digit_top_bg_' + i) !== null) { document.getElementById('digit_top_bg_' + i).style.backgroundPosition = '-' + position_pixels + 'px 0px'; }
                if(document.getElementById('digit_top_' + i) !== null) { document.getElementById('digit_top_' + i).style.height = '0px';}
                if(document.getElementById('digit_top_bg_' + i) !== null) { document.getElementById('digit_top_bg_' + i).style.height = '18px';}
                if(document.getElementById('digit_bottom_' + i) !== null) { document.getElementById('digit_bottom_' + i).style.height = '0px';}
                if(document.getElementById('digit_bottom_' + i) !== null) { document.getElementById('digit_bottom_' + i).style.backgroundPosition = '-' + position_pixels + 'px -18px';}
            }

            if (number_displayed[i] != number_displayed_previous[i])
            {
                if(document.getElementById('digit_top_' + i) !== null) { document.getElementById('digit_top_' + i).style.marginTop = (frame) + 'px'; }
                if(document.getElementById('digit_top_' + i) !== null) { document.getElementById('digit_top_' + i).style.height = (18 - frame) + 'px'; }
            }
            else
            {
                if (number_displayed[i]>=0){
                    position_pixels=(24 * number_displayed[i]);
                    if(document.getElementById('digit_top_' + i) !== null) { document.getElementById('digit_top_' + i).style.backgroundPosition = '-' + position_pixels + 'px 0px'; }
                    if(document.getElementById('digit_top_bg_' + i) !== null) { document.getElementById('digit_top_bg_' + i).style.backgroundPosition = '-' + position_pixels + 'px 0px'; }
                    if(document.getElementById('digit_top_' + i) !== null) { document.getElementById('digit_top_' + i).style.height = '0px'; }
                    if(document.getElementById('digit_top_bg_' + i) !== null) { document.getElementById('digit_top_bg_' + i).style.height = '18px'; }
                    if(document.getElementById('digit_bottom_' + i) !== null) { document.getElementById('digit_bottom_' + i).style.height = '0px'; }
                    if(document.getElementById('digit_bottom_' + i) !== null) { document.getElementById('digit_bottom_' + i).style.backgroundPosition = '-' + position_pixels + 'px -18px'; }
                }
            }
        }

        frame += 4;

        if (frame < 17)
        {
            setTimeout('flip_digits_top(' + frame + ');', 20);
        }
        else
        {
            flip_digits_bottom(0);
        }
    }

    function flip_digits_bottom(frame)
    {
        for (var i = 0; i < 7; i ++)
        {
            if (number_displayed[i] != number_displayed_previous[i])
            {
                if (number_displayed[i]>=0){
                    if(document.getElementById('digit_bottom_' + i) !== null) { document.getElementById('digit_bottom_' + i).style.backgroundPosition = '-' + (24 * number_displayed[i]) + 'px ' + (-34 + frame) + 'px'; }
                    if(document.getElementById('digit_bottom_' + i) !== null) { document.getElementById('digit_bottom_' + i).style.height = frame + 'px'; }
                }
            }
        }

        frame += 4;

        if (frame < 20)
        {
            setTimeout('flip_digits_bottom(' + frame + ');', 20);
        }
        else
        {
            for (var i = 0; i < 7; i ++)
            {
                if (number_displayed[i]>=0){
                    if(document.getElementById('digit_top_' + i) !== null) { document.getElementById('digit_top_' + i).style.backgroundPosition = '-' + (24 * number_displayed[i]) + 'px 0px'; }
                    if(document.getElementById('digit_bottom_bg_' + i) !== null) { document.getElementById('digit_bottom_bg_' + i).style.backgroundPosition = '-' + (24 * number_displayed[i]) + 'px -18px'; }
                }
            }
        }
    }

    function inc_number(target_number)
    {
        if (number_displayed < target_number) show_number(Math.floor(number_displayed) + 1);
        if (number_displayed < target_number) setTimeout('inc_number(' + target_number + ');', 500);
