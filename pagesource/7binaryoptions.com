			jQuery('.tablepress thead').show();
			jQuery('.tablepress .column-2').show();
			jQuery('.tablepress .column-3').show();
			jQuery('.tablepress .column-4').show();
			if(isEnglish)
				jQuery('.tablepress .column-5').show();
		}

		if( jQuery(window).width() < 540 + widthModifier ){
			jQuery('.tablepress td').css('padding-right', '0px');
			jQuery('.tablepress td').css('padding-left', '0px');
		} else {
			jQuery('.tablepress td').css('padding', '8px');
		}

		if( jQuery(window).width() < 420 + widthModifier){
			jQuery('.tablepress .column-'+tablepressColAmount).hide();
		} else {
			jQuery('.tablepress .column-'+tablepressColAmount).show();
		}
	}

