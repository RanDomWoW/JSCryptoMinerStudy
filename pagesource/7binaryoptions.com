		function tablepressResize(){	
		var tablepressColAmount = jQuery('.tablepress th').length;
		var isEnglish = tablepressColAmount == 7;
		var widthModifier = (isEnglish)?0:50;
		// tablepress column removals on mobile
		if( jQuery(window).width() < 1200 ){
			jQuery('.tablepress img').css('margin-left', '0px');
			jQuery('.tablepress img').removeClass('alignright');

			jQuery('.tablepress thead').hide();
