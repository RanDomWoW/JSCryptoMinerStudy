$(function () {
	emailEsistente = false;
	
	$("#email").change(function(){
	  $.get("_controllaEmail.php?e=" + $(this).val(),function(data,status){
		  	if (data > 0){
				$('#passwordDimenticata').show();
				$('#testoPassword').replaceWith('<span id="testoPassword">La tua password:</span>');
				$('#password').attr('title','Il tuo indirizzo eMail risulta già iscritto! Inserisci la tua password per aggiungere questo sfogliabile alla tua collezione');
				$('#password').attr('data-original-title','Il tuo indirizzo eMail risulta già iscritto! Inserisci la tua password per aggiungere questo sfogliabile alla tua collezione');
				$('#password').tooltip();
				emailEsistente = true;
			} else {
								
				$('#passwordDimenticata').hide();
				$('#testoPassword').replaceWith('<span id="testoPassword">Scegli una password:</span>');
				$('#password').attr('title','Scegli una password di almeno 6 caratteri, ti servirà per gestire tutti i tuoi sfogliabili.');
				$('#password').attr('data-original-title','Scegli una password di almeno 6 caratteri, ti servirà per gestire tutti i tuoi sfogliabili.');
				$('#password').tooltip();
				emailEsistente = false;
			}
	  });
	  $("#password").val('');
	  $('#password').removeClass('formNO');
	  $('#password').removeClass('formOK');	
	  passwordErrata = false;
	});



	$("#password").change(function(){
		if (emailEsistente){	
		 	$.get("_controllaUtenteEsistente.php?e=" + $('#email').val() + "&p=" + $(this).val(),function(data,status){
				if (data > 0){
					$('#password').closest('.form-group').removeClass('has-error');
					passwordErrata = false;
				} else {
					$('#password').closest('.form-group').addClass('has-error');
					passwordErrata = true;
				}
	  		});
	  	}
	});

 	
    'use strict';
    // Change this to the location of your server-side upload handler:
    var url = '/jQuery-File-Upload/server/php/';
    $('#fileupload').fileupload({
        autoUpload: false,
		//maxChunkSize: 	80000000, //80MB
		maxFileSize: 		1000000000, // 1000 MB
		maxNumberOfFiles: 1,
		acceptFileTypes: /(\.|\/)(pdf)$/i,
		url: url,
        dataType: 'json',
		add: function (e, data) {            
			$(".upload_div").off('click').on('click', function () {
				console.log('valido form');
				$("#caricamento_frm").validationEngine({showOneMessage:true, scroll: false});
				
				if($("#caricamento_frm").validationEngine('validate')){
					data.submit();
				} else {
					alert('inserisci tutti i dati');
					return false;
				}
			});
		},
		done: function (e, data) {
            $.each(data.result.files, function (index, file) {
                $('<p/>').text(file.name).appendTo('#files');
            });
        },
        progressall: function (e, data) {
            var progress = parseInt(data.loaded / data.total * 100, 10);
            $('#progress .progress-bar').css(
                'width',
                progress + '%'
            );
        }
    }).prop('disabled', !$.support.fileInput)
      .parent().addClass($.support.fileInput ? undefined : 'disabled');
	  
	  
	  
	  
	  
	  
		
		
		
	$('#fileupload').bind('fileuploadadd', function (e, data) {
		$('.upload_div').show();
		$('.select_div').hide();
	})

	$('#fileupload').bind('fileuploadstart', function (e, data) {
		$('#upload_lock').fadeIn();
	})



	$('#fileupload').bind('fileuploaddone', function (e, data) {
		window.location.href = "https://www.sfogliami.it/library/";	})
});
</script>

