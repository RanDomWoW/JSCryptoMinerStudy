	function NewsKeyDown(event)
	{
		if (event.which == 13)
		{
			event.preventDefault();
			NewsSubscribe();
		}
	}
	function NewsSubscribe() {
		var email = $('#campo-news').attr('value');
		if (IsEmail(email)) {
			$('#footer-news').load("/libri/AsyncWidgets/Newsletter?email=" + encodeURI(email),
				function () { }
			);
		}
		else {
			$('#news-msg').css("color","red").html("Indirizzo e-mail non valido");
		}
		return false;
	}

	function IsEmail(emailAddress) {
		var pattern = new RegExp(/^([\w\.\-]+)@([\w\-]+)((\.(\w){2,4})+)$/i);
		return pattern.test(emailAddress);
	}
</script>
