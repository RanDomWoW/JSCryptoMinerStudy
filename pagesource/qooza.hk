<script>

$(document).ready(function(){

	var color = [ '#000000','#000000','#000000','#ffffff','#000000','#000000'];	
  	$('.bxslider').bxSlider({
		slideWidth: 1600,
		mode: 'fade',
		auto: true,
		bgcolor:color,
		speed: 4000
  	});

  	$('.container_slider').css({
		'background-color': color[0],
		'height': '500px' 
	});

	$(".btn_login").click(function(){
  	 	$( "#login_form" ).submit();
  	});
  	

  	$(".slider_go").click(function(){

  	  	var _href = $(this).attr('data-href');
  	 	window.open(_href,'_blank');
  	});
  	$(".today-arrow").click(function(){

  		var cover = "";
  		var is_left = $(this).hasClass('arrow-left');
		var _text = "";
  		$('.today_cover_image').each(function () {
  			
  			var _find = $(this).hasClass('show');
  			if(_find)
  			{
				if(is_left)
				{
					cover = $(this).attr('data-before-value');
				}
				else
				{
					cover = $(this).attr('data-next-value');
				}
				_text = $(this).attr('data-text');
				$(this).addClass('hidden');
				$(this).removeClass('show');
  			}
  		});

  		$('.today_cover_'+cover).addClass('show');
		$('.today_cover_'+cover).removeClass('hidden');

		$('.today_cover_detail').html( _text );
  	});
  	
  	

  	$(window).scroll(function()
  	{
  	  	var ypos = $(window).scrollTop();
  	  	var _pos =0;

  		if(ypos > -1)
  		{
  	  		_pos = (120 - ypos) + 'px';
  	  		$('.slider_img').css({ 'background-position': '50% '+ _pos });
  		}else{
  			$('.slider_img').css({ 'background-position': '50% 100px' });
  		}

  	});


	$('#username').keyup(function(e){
		if(e.keyCode == 13) {
			if($('#password').val()=='') {
				$('#password').focus();
			} else {
				$('#login_form').submit();
			}
		}
	});
	$('#password').keyup(function(e){
		if(e.keyCode == 13) $('#login_form').submit();
	});
	
});
</script>





