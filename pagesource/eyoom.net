$(document).ready(function() {
	var currentSlide = 0,
	    $slideContainer = $('.slide-container'),
	    $slide = $('.slide-li'),
	    slideCount = $slide.length,
	    animationTime = 300;

	function setSlideDimensions() {
		var windowWidth = $(window).width();
			slideContentWidth = $('.slide-container-wrap').width();
		$slideContainer.width(slideContentWidth * slideCount);
		$slide.width(slideContentWidth);
	}

	function generatePagination() {
		var $pagination = $('.pagination-step');
		for(var i = 0; i < slideCount; i ++){
			var $indicator = $('<div>').addClass('indicator-circle'),
			    $progressBarContainer = $('<div>').addClass('progress-bar-container'),
			    $progressBar = $('<div>').addClass('progress-bar-step'),
			    indicatorTagText = $slide.eq(i).attr('data-tag'),
			    $tag = $('<div>').addClass('step-tag').text(indicatorTagText);
			$indicator.append($tag);
			$progressBarContainer.append($progressBar);
			$pagination.append($indicator).append($progressBarContainer);
		}
		$pagination.find('.indicator-circle').eq(0).addClass('active');
	}

	function goToNextSlide() {
		if(currentSlide >= slideCount - 1) return;
		var windowWidth = $(window).width();
			slideContentWidth = $('.slide-container-wrap').width();
		currentSlide++;
		$slideContainer.animate({
			left: -(slideContentWidth * currentSlide)
		});
		setActiveIndicator();
		$('.progress-bar-step').eq(currentSlide - 1).animate({
			width: '100%'
		}, animationTime);
	}

	function goToPreviousSlide() {
		if(currentSlide <= 0) return;
		var windowWidth = $(window).width();
			slideContentWidth = $('.slide-container-wrap').width();
		currentSlide--;
		$slideContainer.animate({
			left: -(slideContentWidth * currentSlide)
		}, animationTime);
		setActiveIndicator();
		$('.progress-bar-step').eq(currentSlide).animate({
			width: '0%'
		}, animationTime);
	}

	function postitionSlides() {
		var windowWidth = $(window).width();
			slideContentWidth = $('.slide-container-wrap').width();
		setSlideDimensions();
		$slideContainer.css({
			left: -(slideContentWidth * currentSlide)
		}, animationTime);
	}

	function setActiveIndicator() {
		var $indicator = $('.indicator-circle');
		$indicator.removeClass('active').removeClass('step-complete');
		$indicator.eq(currentSlide).addClass('active');
		for(var i = 0; i < currentSlide+1; i++){
			$indicator.eq(i).addClass('step-complete');
		}
	}

	setSlideDimensions();
	generatePagination();
	$(window).resize(postitionSlides);
	$('.next-step-btn').on('click', goToNextSlide);
	$('.previous-step-btn').on('click', goToPreviousSlide);
});
</script>
