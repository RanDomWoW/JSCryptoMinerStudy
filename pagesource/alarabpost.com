            var currentTime = new Date('3/22/2018 9:43:44 PM');
            var hours = currentTime.getHours();
            var minutes = currentTime.getMinutes();
            var seconds = currentTime.getSeconds();
            function clockSet() {
                seconds++;
                if (seconds > 59) {
                    seconds = 0;
                    minutes++;
                }
                if (minutes > 59) {
                    minutes = 0;
                    hours++;
                }
                if (hours > 23) {
                    hours = 0;
                }
                $('#time').text('');
                $('#time').text(hours + ':' + minutes + ':' + seconds);
            }
            setInterval("clockSet()", 1000);
            clockSet();

            var showDateIndex = 0;
            function showDate() {
                if (showDateIndex >= 3)
                    showDateIndex = 0;
                $(".DataSpan").hide();
                $($(".DataSpan").get(showDateIndex)).fadeIn();
                showDateIndex++
            }
            showDate();
            setInterval("showDate()", 5000);
