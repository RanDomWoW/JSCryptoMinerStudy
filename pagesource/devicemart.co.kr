<iframe name="search_hidden_frame" style="display:none;"></iframe>
<script>
//-- livesearch
function livesearchAction(obj){
	var keycode = event.keyCode;
	var value = obj.value;
    if (!value)
	{
		closeSearch();
		return;
    }
	if(keycode == 13){
		search_chk();
	}
    if (value)
	{

		jq.ajax({
			type: 'post',
			url: '/livesearch.php',
			data: 'sword=' + value,
			dataType: 'json',
			success: function(data){
				jq("#livesearch_layer div.inner_search_wa").empty();
				jq("#livesearch_layer div.inner_search_goods").empty();
				jq("#livesearch_layer div.cate").empty();
				if(data.keyword){
					jq(data.keyword).each(function(){
						var keyword = this.keyword.replace(value, '<b>'+value+'</b>');
						jq("#livesearch_layer div.inner_search_wa").append('<a href="javascript:selectSearch_top(\''+this.keyword+'\');"><span>'+keyword+'</span></a>');
					});
				}
				if(data.category){
					jq(data.category).each(function(){
						var category = '';
						if(this.one!= this.category){
							category += '<a href="/goods/list.php?category='+this.one+'">'+(this.onecatename.replace(value, '<b>'+value+'</b>'))+'</a> > ';
						}
						if(this.two!= this.category){
							category += '<a href="/goods/list.php?category='+this.two+'">'+(this.twocatename.replace(value, '<b>'+value+'</b>'))+'</a> > ';
						}
						if(this.three!= this.category){
							category += '<a href="/goods/list.php?category='+this.three+'">'+(this.threecatename.replace(value, '<b>'+value+'</b>'))+'</a> > ';
						}
						category += '<a href="'+(this.url ? this.url : '/goods/list.php?category='+this.category )+'">'+(this.catename.replace(value, '<b>'+value+'</b>')) + '</a>';

						jq("#livesearch_layer div.cate").append('<div class="cata_list"><span>카테고리</span>'+category+'</div>');
					});
				}
				if(data.goods){
					jq(data.goods).each(function(){
						this.brand = this.brand ? this.brand : 'Any Vendor';
						var html = '<div class="search_pr">';
						html += '		<span class="img"><img src="'+(this.image_list ? this.image_list : '/admin/images/no_img60.gif')+'" /></span>';
						html += '			<ul>';
						html += '				<li>['+(this.brand.replace(value, '<b>'+value+'</b>'))+']</li>';
						html += '				<li><a href="/'+this.seq+'">'+(this.name.replace(value, '<b>'+value+'</b>'))+'</a></li>';
						html += '				<li class="sum">'+this.price+'원 ';
						html += '					<span data-price-option="'+this.price_option+'" data-order-min="'+this.order_min_count+'" data-goodstype="'+this.goodstype+'">';
						html += '						<img src="/skin/default/img/top/btn_search_buy.gif" value="'+this.seq+'" name="btn-search-buy" onclick="buy_search_goods(this);" />';
						html += '						<img src="/skin/default/img/top/btn_search_cart.gif" value="'+this.seq+'" name="btn-search-cart" onclick="cart_search_goods(this);"  />';
						html += '					</span>';
						html += '				</li>';
						html += '			</ul>';
						html += '</div>';
						jq("#livesearch_layer div.inner_search_goods").append(html);
					});
				}
			},
			 error:function(request,status,error){
		        //alert("code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error);
				closeSearch();  
			}
		});

		jq("#livesearch_layer").show();
    }
}	
//-- liveserach key event effect 

function buy_search_goods(obj){
	if(jq(obj).parent().attr('data-price-option') == 'SINGLE'){
		//단일옵션
		jq.ajax({
			url : '/goods/process.php',
			type : 'post',
			dataType : 'json',
			data : {goods_seq : jq(obj).attr('value'), mode: "modOpt1_opt", option1_text: '', option2_text: '', width: '', height: ''},
			success : function(data){
				jq("#search_hidden_form").attr('target', '');
				jq("#search_hidden_form input[name=mode]").val('direct');
				jq("#search_hidden_form input[name=seq]").val(jq(obj).attr('value'));
				jq("#search_hidden_form input[name=price_option]").val('SINGLE');
				jq("#search_hidden_form input[name=eq]").val(jq(obj).parent().attr('data-order-min'));
				jq("#search_hidden_form input[name=price_seq]").val(data.seq);
				jq("#search_hidden_form input[name=goodstype]").val(jq(obj).parent().attr('data-goodstype'));
				jq("#search_hidden_form").attr('action', '/order/process.php');
				jq("#search_hidden_form").get(0).submit();
			},
			error : function(){
				alert('가격정보를 정상적으로 가져오지 못했습니다.\n상품 상세 페이지에서 구매해주세요.');
			}
		});
	}else{
		//가격옵션 있음
		if(confirm('필수옵션이 있는 상품입니다.\n상품 상세 페이지로 이동하시겠습니까?')){
			location.href= jq(obj).val();
		}
	}
}

function cart_search_goods(obj){
	if(jq(obj).parent().attr('data-price-option') == 'SINGLE'){
		jq.ajax({
			url : '/goods/process.php',
			type : 'post',
			dataType : 'json',
			data : {goods_seq : jq(obj).attr('value'), mode: "modOpt1_opt", option1_text: '', option2_text: '', width: '', height: ''},
			success : function(data){
				jq("#search_hidden_form").attr('target', 'search_hidden_frame');
				jq("#search_hidden_form input[name=mode]").val('into_cart');
				jq("#search_hidden_form input[name=seq]").val(jq(obj).attr('value'));
				jq("#search_hidden_form input[name=price_option]").val('SINGLE');
				jq("#search_hidden_form input[name=eq]").val(jq(obj).parent().attr('data-order-min'));
				jq("#search_hidden_form input[name=price_seq]").val(data.seq);
				jq("#search_hidden_form input[name=goodstype]").val(jq(obj).parent().attr('data-goodstype'));
				jq("#search_hidden_form").attr('action', '/order/process.php');
				jq("#search_hidden_form").get(0).submit();
				if(confirm("상품이 장바구니에 담겼습니다. 장바구니로 이동하시겠습니까_?"))
				{
					//document.location.href="/order/cart.php";
					document.location.href="/order/cart.php?action=redirect";
				}
			},
			error : function(){
				alert('가격정보를 정상적으로 가져오지 못했습니다.\n상품 상세 페이지에서 구매해주세요.');
			}
		});
