 function cateView(mode){
    if (mode=="on"){
	    jq("#vOn").css("display","none");
	    jq("#vOff").css("display","block");
	    jq("#allCate").css("display","block");
    } else {
	    jq("#vOn").css("display","block");
	    jq("#vOff").css("display","none");
	    jq("#allCate").css("display","none");
    }    
}

var now_sub = '';
function view_cate_sub(cate){
	if(now_sub == cate){
		jq('div.sub2_list').hide('slide');
		now_sub = '';
	}else{
		jq('div.sub2_list').hide();
		jq('#sub_'+cate).show('slide');
		now_sub = cate;
	}
}

