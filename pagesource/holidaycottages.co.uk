        function DoLazy1(resize) {
            var imgprefix = "";
            if ($('.device-xs').is(':visible')) {
                imgprefix = "data-original-xs";
            }
            if ($('.device-sm').is(':visible')) {
                imgprefix = "data-original-sm";
            }
            if ($('.device-md').is(':visible')) {
                imgprefix = "data-original-md";
            }
            if ($('.device-lg').is(':visible')) {
                imgprefix = "data-original-lg";
            }
            if (resize) {
                $(".bglazy").each(function () {
                    $(this).attr('style', 'display:block;background-image: url("' + $(this).attr('data-image') + $(this).attr(imgprefix) + '")');
                }
               );
            } else {
                $(".bglazy").each(function () {
                    $(this).attr('data-original', $(this).attr('data-image') + $(this).attr(imgprefix));
                }
                );

                if ($('.device-xs').is(':visible') || $('.device-sm').is(':visible')) {
                    $(".bglazy").each(function () {
                        $(this).attr('style', 'display:block;background-image: url("' + $(this).attr('data-image') + $(this).attr(imgprefix) + '")');
                    }
               );
                } else {
                    $(".bglazy").lazyload({
                        effect: "fadeIn"
                    });
                }
            }
        }

