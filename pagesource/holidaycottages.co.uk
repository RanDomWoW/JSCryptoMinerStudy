<script type="text/javascript">



    var prm = Sys.WebForms.PageRequestManager.getInstance();
    prm.add_beginRequest(BeginRequestHandler);
    prm.add_endRequest(EndRequestHandler);

    function BeginRequestHandler(sender, args) {
        $("*[id$='ddlFrom']").attr('disabled', true);
        $("*[id$='ddlLocation']").attr('disabled', true);
        $("*[id$='ddlPartySize']").attr('disabled', true);
        $("*[id$='ddlDuration']").attr('disabled', true);
        $("*[id$='Button1']").attr('disabled', true);
        $("#datestext").attr('style', 'background-color: #eeeeee; cursor: pointer;');
        $('.proptext input').popover('destroy');
    }

    function EndRequestHandler(sender, args) {
        $("*[id$='ddlFrom']").removeAttr('disabled');
        $("*[id$='ddlLocation']").removeAttr('disabled');
        $("*[id$='ddlPartySize']").removeAttr('disabled');
        $("*[id$='ddlDuration']").removeAttr('disabled');
        $("*[id$='Button1']").removeAttr('disabled');
        $("#datestext").attr('style', 'background-color: #fff; cursor: pointer;');

        if ($('#propsearchError').length > 0) {
            $("input[id$='propertySearch']").popover('destroy');
            $("input[id$='propertySearch']").popover({ placement: 'bottom', trigger: 'manual', title: 'Oops, we’re sorry!', content: 'We couldn’t find a property with this name or property code' }).popover('show');
            $(".popover.bottom").css("margin-top", "32px");
        }

        $("*[id$='ddlFrom']").removeAttr('disabled');

        $("#datestext").text(document.getElementById("hiddate").value);
        if ($("#datestext").text == "") {
            $('#cleardate').hide();
            $("#datestext").removeClass("nobgi");
        } else {
            $('#cleardate').show();
            $("#datestext").addClass("nobgi");
        }

        $(document).ready(function () {
            init();
            if ($("#ContentPlaceHolder1_SearchPanel1_propertySearch").val() != "Enter property name or code" && $("#ContentPlaceHolder1_SearchPanel1_propertySearch").val() != "") {
                $(".spanelswitch a").click(function () {
                    $(".fullsearch").hide();
                    $(".property-code-search").show();
                });
                $(".pcpanelswitch a").click(function () {
                    $(".fullsearch").show();
                    $(".property-code-search").hide();
                    $("#ContentPlaceHolder1_SearchPanel1_propertySearch").val("");
                });
                $(".fullsearch").hide();
                $(".property-code-search").show();
            } else {

                $('.spanelswitch a').on('click', function (e) {
                    $(".fullsearch").hide();
                    $(".property-code-search").show();
                });
                $('.pcpanelswitch a').on('click', function (e) {
                    $(".fullsearch").show();
                    $(".property-code-search").hide();
                });

                $(".fullsearch").show();
                $(".property-code-search").hide();
            }

        });
        
    }

    function showdd(dd, button) {
        $("#" + dd).toggle();
        $(button).addClass("selbg");
        $("#datesdd").hide();

        $(document).mouseup(function (e) {
            var container = $("#" + dd);
            if (!container.is(e.target) // if the target of the click isn't the container...
                && container.has(e.target).length === 0 && e.target != button) // ... nor a descendant of the container
            {
                container.hide();
                $(button).removeClass("selbg");
                $(document).unbind('mouseup');
            }
        });
    }

    $(".searchpanel .btn").on("click", function () {
        var totalAdults = parseInt($("#hid_AdultsSearchBar").val(), 10);
        var totalChildren = parseInt($("#hid_ChildrenSearchBar").val(), 10);
        var totalInfants = parseInt($("#hid_InfantsSearchBar").val(), 10);

        var totalGuests = totalAdults + totalChildren + totalInfants;
        $("#hid_GuestsSearchBar").val(totalGuests);
    });


    function FormatDateSelection(DateIn, ShouldFormat) {
        if (DateIn == null || DateIn == "") return "Select your dates";
        var datesel = "";
        var datestr = "";
        var formattedDate = "";
        if (ShouldFormat) {
            formattedDate = ("0" + DateIn.date.getDate()).slice(-2);
            datesel = dayNames[DateIn.date.getDay()] + " " + formattedDate + " " + monthNames[DateIn.date.getMonth()] + " " + DateIn.date.getFullYear().toString().substr(2, 2);
            datestr = dayNames[DateIn.date.getDay()] + " " + formattedDate + " " + monthNames[DateIn.date.getMonth()] + " " + DateIn.date.getFullYear().toString().substr(2, 2);
            $("#hiddate").val(datestr);
            $(".cleardatelink").removeClass("hide");
        } else {
            datesel = DateIn;
        }
        return datesel;



    }

    function GetNightsSelected() {
        var ntstext = " nights";
        if ($("#refNightsSearchBar").val() == "1") ntstext = " night";
        return $("#refNightsSearchBar").val() + ntstext;
    }

    function GetFlexSelected() {
        var flextext = "";
        if ($("#refFlexibilitySearchBar").val() == "1") {
            flextext = $("#refFlexibilitySearchBar").val() + " day";
        } else if ($("#refFlexibilitySearchBar").val() == "0") {
            flextext = "Exact days";
        } else {
            flextext = $("#refFlexibilitySearchBar").val() + " days";
        }
        return " <span>(" + flextext + ")</span>";
    }
</script>

