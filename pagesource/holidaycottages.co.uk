        });

        function guestCount(initialGuestTotal) {

            //Third version of script, calculate adults and children as guests, infants separate

            var guestCount = initialGuestTotal - newValinfants;

            if (guestCount == 1 && newValinfants == 1) {
                $("#guestUpdate").html(guestCount + " Guest &#183; " + newValinfants + " Infant");
            }
            else if (guestCount > 1 && newValinfants == 1) {
                $("#guestUpdate").html(guestCount + " Guests &#183; " + newValinfants + " Infant");
            }
            else if (guestCount == 1 && newValinfants > 1) {
                $("#guestUpdate").html(guestCount + " Guest &#183; " + newValinfants + " Infants");
            }
            else if (guestCount > 1 && newValinfants > 1) {
                $("#guestUpdate").html(guestCount + " Guests &#183; " + newValinfants + " Infants");
            }
            else if (guestCount == 1 && newValinfants < 1) {
                $("#guestUpdate").html(guestCount + " Guest");
            }
            else {
                $("#guestUpdate").html(guestCount + " Guests");
            }

            $(".clearguestslink").removeClass("hide");

            $('#search_link').attr("href", UpdateQueryString("Adults", newValadults, $('#search_link').attr("href")));
            $('#search_link').attr("href", UpdateQueryString("Children", newValchildren, $('#search_link').attr("href")));
            $('#search_link').attr("href", UpdateQueryString("Infants", newValinfants, $('#search_link').attr("href")));
            $('#search_link').attr("href", UpdateQueryString("PartySize", guestTotal, $('#search_link').attr("href")));

            if (($('.device-xs').is(':visible')) || ($('.device-sm').is(':visible')) || ($('.device-md').is(':visible'))) {
                $('.homepage #guestlinkbox i').hide();
            }
        }

        $(".clearguestslink").on("click", function (e) {
            e.preventDefault();
            clearGuests();

        });

        if ($("#SelMinPrice").length > 0) {
            $(this).change(function () {
                $("#hidMinPrice").val($("#SelMinPrice").val());
            });   
        }

        if ($("#SelMaxPrice").length > 0) {
            $(this).change(function () {
                $("#hidMaxPrice").val($("#SelMaxPrice").val());
            });   
        }

    });

    window.addEventListener("pageshow", function (evt) {
        if (evt.persisted) {
            setTimeout(function () {
                if ($(".device-sm").is(':visible')) {
                    clearsearch();
                }
            }, 10);
        }
    }, false);

    var dayNames = ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"];
    var monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];

    function showcal(dd, button) {

        $("#" + dd).toggle();
        $("#guestdd").hide();
        $("#datelinkbox").addClass("selbg");

        loadCalendar();

        $(document).mouseup(function (e) {
            var container = $("#" + dd);
            if (!container.is(e.target) // if the target of the click isn't the container...
                && container.has(e.target).length === 0 && e.target != button) // ... nor a descendant of the container
            {
                container.hide();
                $(button).removeClass("selbg");
                $(document).unbind('mouseup');
                if ($("#hiddate").val() !== "") {
                    $("#datelinkbox").addClass("hasval");
                }

            }
        });
    }

    function clearGuests() {
        $("#guestUpdate").text("Guests");
        $("#hid_GuestsSearchBar").val("");

        $("#refAdultsSearchBar").val("0");
        $("#refChildrenSearchBar").val("0");
        $("#refInfantsSearchBar").val("0");

        $("#hid_AdultsSearchBar").val("0");
        $("#hid_ChildrenSearchBar").val("0");
        $("#hid_InfantsSearchBar").val("0");

        $("#guestlinkbox").removeClass("active");

        $(".clearguestslink").addClass("hide");

        $('#search_link').attr("href", UpdateQueryString("Adults", "", $('#search_link').attr("href")));
        $('#search_link').attr("href", UpdateQueryString("Children", "", $('#search_link').attr("href")));
        $('#search_link').attr("href", UpdateQueryString("Infants", "", $('#search_link').attr("href")));
        $('#search_link').attr("href", UpdateQueryString("PartySize", "", $('#search_link').attr("href")));

        if (($('.device-xs').is(':visible')) || ($('.device-sm').is(':visible')) || ($('.device-md').is(':visible'))) {
            $('.homepage #guestlinkbox i').show();
        }

        initialGuestTotal = 0;
        newValinfants = 0;
    }

    function pad(n) { return n < 10 ? '0' + n : n }
    function ISODateString(d) {
        function pad(n) { return n < 10 ? '0' + n : n }

        return pad(d.getDate()) + '/' + pad(d.getMonth() + 1) + '/' + d.getUTCFullYear();
    }

    function loadCalendar() {
        var calwidth = $("#datelinkbox").width() + 30;

        var sdate = null;
        if ($('#hiddate').val() != "") {

            var dateString = $('#hiddate').val();
            if (dateString.indexOf("/") !== -1) {
                var splitDate = dateString.split('/');
                var month = splitDate[1] - 1; //Javascript months are 0-11

                sdate = new Date(splitDate[2], month, splitDate[0]);
            } else {
                sdate = Date.parse($('#hiddate').val());
            }
        }


        $('#dpickerSearchBar').mobiscroll().calendar({
            theme: 'ios',
            display: 'inline',
            swipeDirection: 'vertical',
            startYear: new Date().getFullYear(),
            endYear: maxDate.getFullYear(),
            firstDay: 1,
            setText: "",
            dateFormat: 'D dd M y',
            minDate: new Date(),
            maxDate: maxDate,
            calendarWidth: calwidth,
            closeOnSelect: false,
            cancelText: '',
            defaultValue: sdate,
            onSetDate: function (day, inst) {
                if (day.control == "calendar") {
                    $("#datelinkbox").addClass("active");
                    $("#dateUpdate").text(FormatDateSelection(day, true));

                    var d = ISODateString(day.date);
                    $('#search_link').attr("href", UpdateQueryString("From", d, $('#search_link').attr("href")));
                    //Periods
                    for (var key in refPeriods) {
                        if (key == d) {
                            $('#search_link').attr("href", UpdateQueryString("period", refPeriods[key], $('#search_link').attr("href")));
                        }
                    }
                    $('#search_link').attr("href", UpdateQueryString("year", day.date.getFullYear(), $('#search_link').attr("href")));

                    if (($('.device-xs').is(':visible')) || ($('.device-sm').is(':visible')) || ($('.device-md').is(':visible'))) {
                        $('.homepage #datelinkbox i').hide();
                    }

                }
            }
        });
    }

    function cleardate() {

        $("#dateUpdate").text("Select dates");
        $("#hiddate").val("");
        $("#datelinkbox").removeClass("active");
        $(".cleardatelink").addClass("hide");

        $('#search_link').attr("href", UpdateQueryString("From", "", $('#search_link').attr("href")));
        $('#search_link').attr("href", UpdateQueryString("period", "", $('#search_link').attr("href")));
        $('#search_link').attr("href", UpdateQueryString("year", "", $('#search_link').attr("href")));

        if (($('.device-xs').is(':visible')) || ($('.device-sm').is(':visible')) || ($('.device-md').is(':visible'))) {
            $('.homepage #datelinkbox i').show();
        }
    }
</script>
