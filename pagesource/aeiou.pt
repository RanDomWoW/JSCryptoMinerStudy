<script type="text/javascript">
var TB = {}; TB.id = null; TB.status = "down"; TB.anim = false; TB.globalAnim = 200; TB.mouseoutmenu = null;
if (jQuery.browser.msie && parseInt(jQuery.browser.version, 10) <= 7) TB.globalAnim = 0;
jQuery("#aeiou-toolbar a[id^=aeiou-menu]").click(function(){
	if(TB.anim) return false;
	TB.anim = true;
	//check for other open menu
	tempid = jQuery(this).attr('id').split('-')
	tempid = tempid[2];
	
	if ((TB.id != null) && (TB.id != tempid)){
		//close the previous menu
		fade = true;
		if (jQuery.browser.msie && parseInt(jQuery.browser.version, 10) <= 8) fade = false;
		jQuery("#aeiou-toolbar #aeiou-menu-"+TB.id).removeClass('txactive');
		jQuery("#aeiou-toolbar .aeiou-menu-"+TB.id).hide();
	}else{
		fade = false;
	}
	TB.id = tempid;
	if (jQuery(this).hasClass('txactive')){
		TB.status = "down";
		
		jQuery("#aeiou-toolbar .aeiou-links-left").animate({"top":"0px"},TB.globalAnim,function(){
			jQuery("#aeiou-toolbar #aeiou-close-menu").hide();
			jQuery("#aeiou-toolbar #aeiou-menu-"+TB.id).removeClass('txactive');
			jQuery("#aeiou-toolbar .aeiou-menu-"+TB.id).hide();
			TB.anim = false;
			TB.id = null;
		});
	}else{
		TB.status = "up";
		
		jQuery("#aeiou-toolbar #aeiou-menu-"+TB.id).addClass('txactive');
		if (!fade)
			jQuery("#aeiou-toolbar .aeiou-menu-"+TB.id).show();
		else
			jQuery("#aeiou-toolbar .aeiou-menu-"+TB.id).fadeIn(TB.globalAnim);
		
		//position box
		w = jQuery(this).outerWidth()/2;
		p = jQuery(this).position().left + w;
		we = jQuery("#aeiou-toolbar .aeiou-menu-"+TB.id).outerWidth();
		pe = p-(we/2);
		
		//check min value
		if(pe <= 14) pe = 14; //14 from left padding.
		//check max value
		jQuery("#aeiou-toolbar #aeiou-close-menu").show();
		pm = jQuery("#aeiou-toolbar #aeiou-close-menu").position().left;
		if((pe + we) > pm) pe = pm - we;
		
		jQuery("#aeiou-toolbar .aeiou-menu-"+TB.id).css("left",pe);
		jQuery("#aeiou-toolbar .aeiou-links-left").animate({"top":"-30px"},TB.globalAnim,function(){
			TB.anim = false;
		});
	}
	return false;
});
jQuery("#aeiou-toolbar").bind('mouseenter',function(){
	if (TB.status == "up"){
		clearTimeout(TB.mouseoutmenu);
	}
}).bind('mouseleave',function(){
	if (TB.status == "up"){
		clearTimeout(TB.mouseoutmenu);
		TB.mouseoutmenu = setTimeout("timeoutMenu()",1500);
	}
});
jQuery("#aeiou-toolbar #aeiou-close-menu").click(function(){
	if (TB.status == "up") timeoutMenu();
});
function timeoutMenu(){
	jQuery("#aeiou-toolbar #aeiou-menu-"+TB.id).trigger('click');
}
//update links for analytics
TB.domain = document.domain;
TB.track = function(url){
	jQuery("body").append("<iframe src='"+url+"' style='width:0px; height:0px; display:none;'></iframe>");
}
jQuery("#aeiou-toolbar .aeiou-links-left a").each(function(){
	href = jQuery(this).attr('href');
	if(href.indexOf('utm') >= 0){
		href += "&utm_source="+TB.domain;
		jQuery(this).attr('href',href);
	}
	
	if(jQuery(this).attr('rel')){
		jQuery(this).click(function(){
			rel = jQuery(this).attr('rel');
			TB.track(rel)
		});
	}
});
var sethomepage = function(obj){
var url = obj.href;
if( jQuery.browser.msie ) {
obj.style.behavior='url(#default#homepage)';
obj.setHomePage(url);
}else{
alert('Arraste o link Faça do AEIOU a sua Homepage para o ícone de Home do seu Browser para fazer do AEIOU a sua Homepage');
}
return false;
}
