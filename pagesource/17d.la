<script language="javascript">
function make_url(){
	var map_url = getId('map_url').value.strtrim();
	if(map_url==''){
		atips('请输入您要缩短的原始网址');
		getId('map_url').focus();
		return false;
	}
	getId('s1_btn').disabled = true;
	$('#s1_btn_loading').show();
	$.ajax({
		type : 'post',
		url : 'ajax.php',
		data : 'action=make_url&map_url='+encodeURIComponent(map_url)+'&t='+Math.random(),
		dataType : 'text',
		success:function(msg){	
			var arr = msg.split('|');
			if(arr[0]=='true'){
				$('#s1_btn_loading').hide();
				$('#s1_tips').html('<p>'+arr[1]+'</p>');
								setTimeout(function(){document.location = 'mycp.php?item=url&action=manage';},2250);
							}else{
				atips(msg);
			}
			getId('s1_btn').disabled = false;
			$('#s1_btn_loading').hide();
			$('#s1_tips').show();
						setTimeout(function(){$('#s1_tips').hide();},3000);
					},
		error:function(){
		}

	});
}
function docode(){
	if(document.all){
		if(13 == event.keyCode){
			make_url();
		}
	}
}
</script>
<div class="idx_tips_box">
<input type="text" id="map_url" value="" maxlength="255" style="width:440px; padding:3px;" onkeydown="docode();" />&nbsp;<input type="button" id="s1_btn" style="padding:3px" value="生成短址" onclick="make_url();" /><img id="s1_btn_loading" src="images/ajax_loading.gif" border="0" style="display:none" /><br /><span id="s1_tips" style="display:none" class="txtred f12"></span>
