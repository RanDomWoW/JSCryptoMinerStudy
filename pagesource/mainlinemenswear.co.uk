<script type="text/javascript">
    var gaEventSent = false;
    function gaSendEvent( gaCategory, gaAction, gaLabel, gaUrl, e ) {
		if (typeof(ga) !== 'undefined' && gaUrl == '' && gaEventSent === false) {
            if (e.currentTarget.href !== '' && typeof(e.currentTarget.href) !== 'undefined') { gaUrl = e.currentTarget.href; }
            if (e.target.href !== '' && typeof(e.target.href) !== 'undefined') { gaUrl = e.target.href; }
        }
        if (typeof(ga) !== 'undefined' && gaUrl !== '' && gaEventSent === false) {
            e.preventDefault();
        }
        if (gaEventSent === true && gaUrl !== "") {
            window.location = gaUrl;
			return false;
        }
        if (gaEventSent === false && typeof(ga) !== 'undefined' && gaUrl !== '') {
            ga('send','event',gaCategory,gaAction,gaLabel, {
                hitCallback: function() {
                    window.location = gaUrl;
					return false;
                }
            });
            gaEventSent = true;
            setTimeout( gaSendEvent( gaCategory, gaAction, gaLabel, gaUrl, e ),1000);
			return false;
        }
        if (gaEventSent === false && typeof(ga) !== 'undefined' && gaUrl == '') {
            ga('send','event',gaCategory,gaAction,gaLabel);
        }
    }
</script>

