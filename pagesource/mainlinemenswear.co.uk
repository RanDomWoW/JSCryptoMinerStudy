// filter settings
if(currentPage == "product"){
    window.onbeforeunload = function(e){
        setPageInfo(currentPage);
    };
}
function setPageInfo(cookieText){
    var prePreviousPage = readCookie('previousPage');
    if(!prePreviousPage){
        prePreviousPage = '';
    }
    newSetCookie('previousPage',cookieText,15);
    newSetCookie('prePreviousPage',prePreviousPage,15);
}
function newSetCookie(cookieName,cookieText,expiresMin) {
    var d = new Date();
    d.setTime(d.getTime() + (expiresMin*60*1000*5));
    var expires = "expires="+ d.toGMTString();
    document.cookie = cookieName + "=" + cookieText + "; " + expires + ";domain=.mainlinemenswear.co.uk;path=/";
}
function setCookie(c_name, value, exdays) {
    var exdate = new Date();
    exdate.setDate(exdate.getDate() + exdays);
    var c_value = escape(value) + ((exdays == null) ? "" : "; expires=" + exdate.toUTCString());
    document.cookie = c_name + "=" + c_value;
}

function readCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') c = c.substring(1, c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
    }
    return null;
}

/*Lazy Loading Images*/
registerListener('load', lazyLoad);
registerListener('scroll', lazyLoad);

function lazyLoad(){
  [].forEach.call(document.querySelectorAll('img[data-hires]'), function(img) {
    if(isInViewport(img)){
    	img.setAttribute('src', img.getAttribute('data-hires'));
    	img.onload = function() {
    		img.removeAttribute('data-hires');
    	};
    }
  });
}

function registerListener(event, func) {
  if (window.addEventListener) {
      window.addEventListener(event, func)
  } else {
      window.attachEvent('on' + event, func)
  }
}

function isInViewport(el){
  var rect = el.getBoundingClientRect();
  return (
      rect.bottom >= 0 &&
      rect.right >= 0 &&

      rect.top <= (
      window.innerHeight ||
      document.documentElement.clientHeight) &&

      rect.left <= (
      window.innerWidth ||
      document.documentElement.clientWidth)
  );
}
