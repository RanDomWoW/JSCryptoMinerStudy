<script type="text/javascript">
    $("#subscribe_form_footer").validate({
        onclick: true,
        focusInvalid: true,
        errorClass: "error_msg_newsletter_form",
        errorLabelContainer: "#msgBox",
        rules: {
            nl_first_name: {
                required: true,
                minlength: 3
            },
            nl_email: {
                required: true,
                email: true
            }
        },
        messages: {
            nl_first_name: {
                required: "Παρακαλώ εισάγετε το όνομα σας.",
                minlength: jQuery.validator.format("Το όνομα δεν μπορεί να είναι μικρότερη από {0} χαρακτήρες.")
            },
            nl_email: {
                required: "Παρακαλούμε, εισάγετε e-mail.",
                email: "Παρακαλώ εισάγετε ένα έγκυρο e-mail."
            }
        },
        submitHandler: function (form) {

            $.ajax({
                type: "POST",
                url: 'https://woomie.gr/subscribe.php',
                data: $(form).serialize(),
                timeout: 3000,
                success: function (resp) {
                    var r = resp.split('?')[1];
                    if (r === '1') {
                        $('#msgBox').empty().css({display: "inline"}).append('<label class="subscribe_success_msg">Εγγραφτήκατε επιτυχώς.<label>');
                        $('#subscribe_form_footer').css({display: "none"});
                        ga('send', 'event', 'Newsletter', 'Opt-in', 'Footer-form', 1);
                        fbq('track', 'CompleteRegistration');


                    } else if (r === '2') {
                        $('#msgBox').empty().css({display: "inline"}).append('<label class="subscribe_error_msg">Έχετε ήδη εγγραφτεί με αυτό το e-mail.<label>');
                        ga('send', 'event', 'Newsletter', 'Already Opted-in', 'Footer-form', 1);
                    } else {
                        $('#msgBox').empty().css({display: "inline"}).append('<label class="subscribe_error_msg">Παρουσιάστηκε σφάλμα, δοκιμάστε ξανά αργότερα.<label>');

                    }
                },
                error: function () {
                    $('#msgBox').empty().css({display: "inline"}).append('<label class="subscribe_error_msg">Παρουσιάστηκε σφάλμα, δοκιμάστε ξανά αργότερα.<label>');

                }
            });
            return false;
        }

    });

</script>
  

