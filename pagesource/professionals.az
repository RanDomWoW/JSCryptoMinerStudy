<script type="text/javascript">
function onLinkedInAuth() {
    var fields = ["firstName","lastName","email-address","interests","summary","headline","date-of-birth","phone-numbers","honors-awards","positions","educations","certifications","courses"];

    IN.API.Profile("me").fields(fields).result(function(data) {
        console.log(data.values[0]);
        $.ajax({
            type:"POST",
        	url: '/register/loginWithLinkedin',
			data:{
				data: data.values[0]
				},
				success:function(data,textStatus){
					window.location = "/"
					},
		        error:function(XMLHttpRequest,textStatus,errorThrown){
		        	 alert("Linkedin ilə sistemə daxil olarkən xəta baş verdi.Lütfən bizimlə əlaqə saxlayın.");
			        }
            });
    }).error(function(data) {
        console.log(data); 
    });
};
</script>
