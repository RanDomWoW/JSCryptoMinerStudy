  <script type="text/javascript">
  var gtmDLPush = {
    dataLayer: undefined,
    list: function (event, action, currency, impressions, pars) {
      this.dataLayer.push({
        'event': event,
        'eventCategory': 'ecommerce',
        'eventAction': action,
        'glami_ids': pars,
        'ecommerce': {
          'currencyCode': currency,
          'impressions': impressions
        }
              });
    },
    product: function (product) {
      this.dataLayer.push({
        'event': 'productClick',
        'ecommerce': {
          'click': {
            'actionField': {
              'list': 'Product List'
            },
            'products': [product]
          }
        }
      });
    },
    interest: function (product) {
      this.dataLayer.push({
        'event': 'productClick',
        'ecommerce': {
          'click': {
            'actionField': {
              'list': 'Mohlo by Vás zajímat'
            },
            'products': [product]
          }
        }
      });
    },
    addPromo: function (promotion) {
        this.dataLayer.push({
            'event': 'addPromo',
            'ecommerce': {
                'promoView': {
                    'promotions': [promotion]
                }
            }
        });
    },
    promoClick: function (promotion) {
      this.dataLayer.push({
        'event': 'promotionClick',
        'ecommerce': {
          'promoClick': {
            'promotions': [promotion]
          }
        }
      });
    },
    addToCart: function (currency, product, par)  {
      this.dataLayer.push({
        'event': 'addToCart',
        'glami_ids': par,
        'ecommerce': {
          'currencyCode': currency,
          'add': {
            'products': [product]
          }
        }
      });
    },
    popupCart: function (products) {
      this.dataLayer.push({
        'event': 'checkout',
        'ecommerce': {
          'checkout': {
            'actionField': {
              'step': 1
            },
            'products': products
          }
        }
              });
    },
    inCart: function (products, dimension16, dimension18) {
      this.dataLayer.push({
        'event': 'checkout',
        'ecommerce': {
          'checkout': {
            'actionField': {
              'step': 1
            },
            'products': products
          }
        },
        'dimension16': dimension16,
        'dimension18': dimension18
              });
    },
    remove: function (product) {
      this.dataLayer.push({
        'event': 'removeFromCart',
        'ecommerce': {
          'remove': {
            'products': [product]
          }
        }
      });
    },
    send: function (orderId, totalPrice, tax, shipping, coupon, products, shippingPayment, discount, metric2, pars) {
      this.dataLayer.push({
        'event': 'purchase',
        'dimension15': discount,
        'dimension17': shippingPayment,
        'metric2': metric2,
        'glami_ids': pars,
        'ecommerce': {
          'purchase': {
            'actionField': {
              'id': orderId,
              'affiliation': 'krasa.cz',
              'revenue': totalPrice,
              'tax': tax,
              'shipping': shipping,
              'coupon': coupon
            },
            'products': products
          }
        }
              });
    },
    shipment: function(shipping, cod, packing) {
        this.dataLayer.push({
            'event': 'shipmentVars',
            'productsShipment': [{
                id: 'BALNE',
                name: 'BALNE',
                price: packing,
                quantity: 1
            },
            {
                id: 'POSTOVNE',
                    name: 'POSTOVNE',
                price: shipping,
                quantity: 1
            },
            {
                id: 'DOBERECNE',
                name: 'DOBERECNE',
                price: cod,
                quantity: 1
            }]
        })
    },
    upsell: function(products) {
        this.dataLayer.push({
            'event': 'lightbox-sell',
            'eventLabel': products
        })
    },
    doubleClick: {
      dataLayer: undefined,
      product: function(prefix, price, category, brand, availability, sale) {
        this.dataLayer.push({
          'product prefix': prefix,
          'product price': price,
          'product category': category,
          'product brand': brand,
          'availability': availability,
          'sale': sale,
          'page type': 'Product Detail'
                  });
      },
      inCart: function (amount) {
        this.dataLayer.push({
          'amount': amount,
          'page type': 'Basket'
                  });
      },
      send: function (sale, amount, transport, payment, discount, town, sum, euSum) {
        this.dataLayer.push({
          'sale': sale,
          'amount': amount,
          'transport type': transport,
          'payment type': payment,
          'discount id': discount,
          'town': town,
          'price': sum,
          'page type': 'Conversion',
          'euprice': euSum
                  });
      },
      other: function (pageType) {
        this.dataLayer.push({
          'page type': pageType
                  });
      }
    }
  };

  var dataLayer = [{ 'pageType':'Homepage', 'ecommerce':{ 'promoView':{ 'promotions':[{ 'id':'https://www.krasa.cz/maybelline/','name':'Maybelline doprava zdarma','creative':'Homepage Carousel','position':1},{ 'id':'https://www.krasa.cz/avene/','name':'Kosmetika Avène','creative':'home-advert-small','position':1},{ 'id':'https://www.krasa.cz/toni-guy/','name':'Sleva 20% na heslo','creative':'home-advert-small','position':2},{ 'id':'https://www.krasa.cz/soaphoria/','name':'Dárek k nákupu','creative':'home-advert-small','position':3}]}} }];
  gtmDLPush.dataLayer = dataLayer;

    var HPcarousel = {       2: { 'id':'https://www.krasa.cz/neutrogena/','name':'Neutrogena dárek k nákupu','creative':'Homepage Carousel','position':2},        3: { 'id':'https://www.krasa.cz/sleva-na-vybrany-produkt-znacky-eucerin/','name':'Eucerin sleva 100 Kč na heslo','creative':'Homepage Carousel','position':3},        4: { 'id':'https://www.krasa.cz/barber-shop/','name':'Péče o vousy','creative':'Homepage Carousel','position':4},        5: { 'id':'https://www.krasa.cz/facevolution/','name':'FacEvolution - péče o řasy','creative':'Homepage Carousel','position':5}   };
  
  
      gtmDLPush.doubleClick.dataLayer = dataLayer;
          gtmDLPush.doubleClick.other('HP');
      </script>

