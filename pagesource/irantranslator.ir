        <script type="text/javascript">
            $(document).ready(function()
            {


                

                $(".form-control").change(function(event) {
            var total = 0; //majmo
            var count = parseInt($("#count").val()); //tedad
            var type  = parseInt($("#type").val()); //paper .. word .. text
            var font  = parseInt($("#fonttype").val()); //mamoli riz khyliriz
            if (parseInt($("#type").val())==250){
                //page
                $(".ftp").css("display", "block"); 
                $(".textcount").css("display", "none");
                $(".countresult").css("display", "none");
                $("#count").css("display", "block");
            }
            else if (parseInt($("#type").val())==0)
            {
                //text
                $(".textcount").css("display", "block");
                $(".countresult").css("display", "block");
                $(".ftp").css("display", "none");
                $("#count").css("display", "none");

            }
            else {
                //word
                font = 0;
                $(".ftp").css("display", "none"); //mamoli riz khyliriz
                $("#count").css("display", "block");
                $(".textcount").css("display", "none");
                $(".countresult").css("display", "none");
            }
            var type  = type + font;
            var sum   = count*type; //tedad safhe ya kalame b dast miad
            
            function time (word){
                var realword;
                var day;
                var hour;
                var hole;

                if (word < 250){
                    realword = 250;
                }
                else {
                    realword = word;

                }
                sum = realword;
                realword /= 250;
                realword=realword*6;
                if (realword >= 24 )
                {
                    day = Math.floor(realword / 24);
                    hour = Math.floor(realword % 24);

                }
                else {
                    day = 0;
                    hour = Math.floor(realword);
                }
                hole = day + " روز " + hour + " ساعت ";
                return hole;
                
            }
            
            
            $(".time").html(time(sum));

            $(".form-value option:selected").each(function() {
                total += parseInt($(this).val());
            });
            total-=parseInt($("#type").val());
            if (sum != 0) { 
             
                
                if (sum <= 2250 && sum > 0){            
                    $("#asilver").html(Math.floor((( sum*total*1.168* 90) / 100)-(( sum*total* 5) / 100) ));
                    $("#agold").html(Math.floor(((sum*total*1.82* 90) / 100)-(( sum*total* 4) / 100) ));
                    $("#aplatinum").html(Math.floor(((sum*total*3* 90) / 100)-(( sum*total* 4) / 100) ));
                    $(".off").html("5%");

                }
                else if (sum <= 6250 && sum > 2250){            

                    $("#asilver").html(Math.floor((( sum*total*1.168* 90) / 100)-(( sum*total* 9) / 100) ));
                    $("#agold").html(Math.floor(((sum*total*1.82* 90) / 100)-(( sum*total* 9) / 100) ));
                    $("#aplatinum").html(Math.floor(((sum*total*3* 90) / 100)-(( sum*total* 9) / 100) ));
                    $(".off").html("10%");
                }
                else if (sum <= 12500 && sum > 6250){           

                    $("#asilver").html(Math.floor((( sum*total*1.168* 90) / 100)-(( sum*total* 14) / 100) ));
                    $("#agold").html(Math.floor(((sum*total*1.82* 90) / 100)-(( sum*total* 14) / 100) ));
                    $("#aplatinum").html(Math.floor(((sum*total*3* 90) / 100)-(( sum*total* 14) / 100) ));
                    $(".off").html("15%");
                }
                else if (sum <= 25000 && sum > 12500){          

                    $("#asilver").html(Math.floor((( sum*total*1.168* 90) / 100)-(( sum*total* 17) / 100) ));
                    $("#agold").html(Math.floor(((sum*total*1.82* 90) / 100)-(( sum*total* 17) / 100) ));
                    $("#aplatinum").html(Math.floor(((sum*total*3* 90) / 100)-(( sum*total* 17) / 100) ));
                    $(".off").html("18%");
                }
                else if (sum <= 50000 && sum > 25000){          

                    $("#asilver").html(Math.floor((( sum*total*1.168* 90) / 100)-(( sum*total* 21) / 100) ));
                    $("#agold").html(Math.floor(((sum*total*1.82* 90) / 100)-(( sum*total* 21) / 100) ));
                    $("#aplatinum").html(Math.floor(((sum*total*3* 90) / 100)-(( sum*total* 21) / 100) ));
                    $(".off").html("22%");
                }
                else if (sum <= 125000 && sum > 50000){         

                    $("#asilver").html(Math.floor((( sum*total*1.168* 90) / 100)-(( sum*total* 26) / 100) ));
                    $("#agold").html(Math.floor(((sum*total*1.82* 90) / 100)-(( sum*total* 26) / 100) ));
                    $("#aplatinum").html(Math.floor(((sum*total*3* 90) / 100)-(( sum*total* 26) / 100) ));
                    $(".off").html("27%");
                }
                else if (sum <= 400000 && sum > 125000){            

                    $("#asilver").html(Math.floor((( sum*total*1.168* 90) / 100)-(( sum*total* 34) / 100) ));
                    $("#agold").html(Math.floor(((sum*total*1.82* 90) / 100)-(( sum*total* 34) / 100) ));
                    $("#aplatinum").html(Math.floor(((sum*total*3* 90) / 100)-(( sum*total* 34) / 100) ));
                    $(".off").html("35%");
                }
            }

            function textcount(){
                var value = $('#textcountarea').val();
                if (value.length == 0) {
                    $('#wordCount').html(0);
                    $('#totalChars').html(0);
                    $('#charCount').html(0);
                    $('#charCountNoSpace').html(0);
                    return;
                }

                var regex = /\s+/gi;
                var wordCount = value.trim().replace(regex, ' ').split(' ').length;
                var totalChars = value.length;
                var charCount = value.trim().replace(/\n$/gm, '').split(/\n/).length;
                
                var charCountNoSpace = value.replace(regex, '').length;
                
                $('#wordCount').html(wordCount);
                $('#totalChars').html(totalChars);
                $('#charCount').html(charCount);
                $('#charCountNoSpace').html(charCountNoSpace);

                $("#asilver").html(Math.floor( wordCount*2.08*total* 90) / 100);
                $("#agold").html(Math.floor( wordCount*2.08*total*1.65* 90) / 100);
                $("#aplatinum").html(Math.floor(wordCount*2.08*total*3* 90) / 100);
                $(".time").html(time(wordCount));
            }

            $('#textcountarea').change(textcount);
        });
    });     
</script>
