<script type="text/javascript">
    function open_nav_menu () {
        $('.mobile-nav-container').fadeIn('fast');
        $( ".mobile-nav-list" ).animate({
            marginLeft: "0",
        }, 200, function() {
            return;
        });
		$('body').addClass('overflowNone');
    }
    var menu_state = 0;
    function close_nav_menu (e) {
        var container = $('.mobile-nav-list');
        if (!container.is(e.target) && container.has(e.target).length === 0) // ... nor a descendant of the container
        {
            container.animate({
                marginLeft: "-80%",
            }, 500, function() {
                return;
            });
            $('.mobile-nav-container').fadeOut();
			$('body').removeClass('overflowNone');
            menu_state = 0;
        }
    }
    $(document).bind( "mouseup touchend", function(ele){
        if( menu_state == 0) return 0;
        close_nav_menu(ele);
    });
    $('.nav-menu').click(function (){
        open_nav_menu();
        menu_state = 1;
    });
    
</script>

