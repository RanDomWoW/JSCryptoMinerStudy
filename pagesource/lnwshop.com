function button_wait(bele){
	$(bele).css('width',($(bele).outerWidth())+'px');
	$(bele).css('height',($(bele).outerHeight())+'px');
	if($(bele).attr('value') != undefined){
		$(bele).attr('_value',$(bele).attr('value'));
		$(bele).attr('value','');
	}
	$(bele).addClass('b-wait').attr('disabled','disabled');
}
function button_normal(bele){
	$(bele).removeClass('b-wait').removeAttr('disabled')
	if($(bele).attr('_value') != undefined){
		$(bele).attr('value',$(bele).attr('_value'));
		$(bele).removeAttr('_value');
	}
	$(bele).css('width','');
	$(bele).css('height','');
}
			function validate_contact(form) {
				if(form.name && form.name.value.length == 0) {
					alert('กรุณากรอก' + 'ชื่อผู้ติดต่อ');
					return false;
				}
				if(form.telno && form.telno.value.length == 0) {
					alert('กรุณากรอก' + 'เบอร์โทรศัพท์');
					return false;
				}
				if(form.email && form.email.value.length == 0) {
					alert('กรุณากรอก' + 'Email');
					return false;
				}
				if(form.subject && form.subject.value.length == 0) {
					alert('กรุณากรอก' + 'เรื่อง');
					return false;
				}
				if(form.message && form.message.value.length == 0) {
					alert('กรุณากรอก' + 'ข้อความ');
					return false;
				}
				return true;
			}

			$(document).ready(function() {
				var form = $("form[name=contact_form]");
				var hash_contact = {
					name: 'ชื่อของคุณ',
					telno: 'เบอร์ติดต่อ',
					email: 'Email ของคุณ',
					subject: 'เรื่อง',
					message: 'ข้อความ และ URL ร้าน (ถ้ามี)'
				};
				for(var key in hash_contact) {
					var value = hash_contact[key];
					var obj = form.find('[name=' + key + ']');
					if(obj.length == 0)continue;
					obj.val(value);
					obj.focus(function() {
						var name = $(this).attr('name');
						if($(this).val() == hash_contact[name]) {
							$(this).val('').addClass('active');
						}
					});
					obj.blur(function() {
						var name = $(this).attr('name');
						if($(this).val() == '') {
							$(this).val(hash_contact[name]).removeClass('active');
						}
					});
				}

				form.submit(function() {
					for(var key in hash_contact) {
						var value = hash_contact[key];
						var obj = $(this).find('[name=' + key + ']');
						if(obj.length == 0)continue;
						if(obj.val() == value) {
							obj.val('');
						}
					}

					if(!validate_contact(this)) {
						for(var key in hash_contact) {
							var value = hash_contact[key];
							var obj = $(this).find('[name=' + key + ']');
							if(obj.length == 0)continue;
							if(obj.val() == '') {
								obj.val(value);
							}
						}
						return false;
					}
					if(typeof button_wait == 'function'){
						button_wait($('.button_new_message',form));
					}
					$.lnwajax.run('help',true,{
						type: 'POST',
						url: 'https' + '://' + 'www.lnwshop.com/help',
						data: $(this).serialize(),
						dataType: 'json',
						start: function(){
						},
						success: function(response){
							if(response.success){
								if(typeof response.message != 'undefined' && response.message != ''){
									alert(response.message);
								}
								form.get(0).reset();
								form.find('input,textarea').blur();
							}else{
								alert(response.key);
							}
							if(typeof button_normal == 'function'){
								button_normal($('.button_new_message',form));
							}
						},
						error: function(){
							if(typeof button_normal == 'function'){
								button_normal($('.button_new_message',form));
							}
							alert('Error,เกิดความผิดพลาดในการติดต่อกรุณาลองรีเฟรชหน้าแล้วค่อยดำเนินการใหม่ด้วยค่ะ');
						}
					});
					return false;
				});
			});
		</script>
