					<script type="text/javascript">
						var setT;
						var setTL;
						var listePhrases = new Array("Dub","Ska","Julia Holter","Joe Pilgrim","Hadouk","Rumba","East Of Eli","Texas","Solside","Reggae","Pillac","Sebastien Tellier","Mitch Ryder & Detroit","Caraïbes","Malene Mortensen","Buzz Booster Ile-de-france","311 New Morning Japan","House","Herve Celcal Quartet","Hundred Seventy Split","Taj Mahal","Sarah Morrow","Sfjazz Collective","Cubanismo","Nina Van Horn","Sam Palladio","Arabo/andalou","Sylvain Beuf","Samoëla","Mambo","Idol","Rock","Paula Morelenbaum","Elza Soares","Gnawa","Rita Marley","Achille's Family","Christian Scott Septet","Mighty Sam Mcclain","Spoek Mathambo","Votre recherche");
							
						var currentIndex = listePhrases.length, temporaryValue, randomIndex;
						while (0 !== currentIndex) {							
							randomIndex = Math.floor(Math.random() * currentIndex);
							currentIndex -= 1;
							temporaryValue = listePhrases[currentIndex];
							listePhrases[currentIndex] = listePhrases[randomIndex];
							listePhrases[randomIndex] = temporaryValue;
						}
	
						$.each(listePhrases, function(i,e) {
							setT = setTimeout(function() {							
								if($("#rechercheSite").data('clicked')) {
									return false;
									clearTimeout(setT);
								}														
								var time = (i*300)+(listePhrases.length*300);									
								var size = e.length;	
								for (var t = 0; t < (size+1); t++) {							
									(function(t) {
										setTL = setTimeout(function() {
											if($("#rechercheSite").data('clicked')) {
												$("#rechercheSite").val('');
												return false;
												clearTimeout(setTL);
											}
											$("#rechercheSite").val(e.substring(0, t));										
										}, 100 * t);
									})(t);
								}
							}, i*6000);
						});//each							
								
						$("#rechercheSite").click(function(){
							$(this).data('clicked', true);
							$("#rechercheSite").val('');
						});						
					</script>										
