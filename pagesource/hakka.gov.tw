    <!--menu 鍵盤控制-->
    <script type="text/javascript">
        function hakka_menu_for_keyboard() {
      
            var focus = 0;
            jQuery("li.menu-item-has-children").attr({ "aria-haspopup": "true", "aria-expanded": "false" });


            jQuery('[role="menubar"]').on('focus.aria  mouseenter.aria', '[aria-haspopup="true"]', function (ev) {
                  
              //打開特定(選中)的menu
                jQuery(ev.currentTarget).attr('aria-expanded', true);
                jQuery(ev.currentTarget).children(".sub-nav").attr('id', 'open-nav-bar')
                jQuery(ev.currentTarget).addClass("dt-hovered");
                jQuery(ev.currentTarget).children(".sub-nav").css({ 'visibility': 'visible', 'opacity': 1 });

            });

            
            /// Properly update the ARIA states on blur (keyboard)關閉menu 
            $('[role="menubar"]').on('blur.aria', '[aria-haspopup="true"]', function (ev) {
              
                //tab鍵
                $(this).children(".sub-nav").children("ul").children("li").last().focusout(function (e) {

                    $(e.currentTarget).parent("ul").parent(".sub-nav").css({ 'visibility': 'hidden', 'opacity': 0 });

                })

                if ($(this).children(".sub-nav").children("ul").children("li").find(".sub-nav").length > 0)
                {
                    $(this).children(".sub-nav").children("ul").children("li").find(".sub-nav").children("ul").children("li").last().focusout(function (e) {
                 
                         $(e.currentTarget).parent("ul").parent(".sub-nav").css({ 'visibility': 'hidden', 'opacity': 0 });
            
                   })
                }                  
            });

         // mouse out events 關閉menu
            $('[role="menubar"]').on('mouseleave.aria', '[aria-haspopup="true"]', function (ev) {
     
                   jQuery(ev.currentTarget).attr('aria-expanded', false);
                  jQuery(ev.currentTarget).children(".sub-nav").css({ 'visibility': 'hidden', 'opacity': 0 });
                  
            })
        }

        jQuery(document).ready(function () {
            hakka_menu_for_keyboard();
        })
    </script>
