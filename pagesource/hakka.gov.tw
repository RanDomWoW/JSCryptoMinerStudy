    <!--主要是調整畫面UI排版-->
    <script type="text/javascript">
        jQuery(document).ready(function ($) {

            chooseMenu();

            $(window).resize(function () {

                chooseMenu();
            })

            // select menu for screen
            function chooseMenu() {

                if (window.innerWidth >= 768) {
                    $("#myOptions").show();
                }else{
                    $("#myOptions").hide();
                }
            }
        });

        //若是首頁則拿掉麵包穴 去除廣告橫幅上方留白
        $(function () {
            $('#breadcrumbs').remove();

        })

        PanelAdjust();
        //設定社群專區的panel高度 動態調整第一排panel高度
        $(function () {
          
            
           
            $(window).load(function () {
                
                PanelAdjust();
                FB.XFBML.parse();
            })
            $(window).resize(function () {
                //重新載入fb套件api(rwd調整大小)
                
                PanelAdjust();
                FB.XFBML.parse();
            })
        })

        function PanelAdjust() {
            $('#dwnload').css("height", "");
            $('#PanelFlag').css("height", "");
            $('#fb').css("height", "");

            var hdwn = $('#dwnload').height();
            var h = $('#PanelFlag').height();
            
            if (window.innerWidth >= 768) {
                if (h > hdwn) {
                    $('#fb').height(h);
                    $('#dwnload').height(h);
                }
                else {
                    $('#fb').height(hdwn);
                    $('#PanelFlag').height(hdwn);
                }
            }
            else
            {
                $('#fb').css("height", "550px");
            }
        }
    </script>
