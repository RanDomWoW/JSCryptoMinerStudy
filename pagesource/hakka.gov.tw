        var Setyoutubetitle = window.setInterval(function () {
            AddTitleText();
        }, 3000);


        function AddTitleText() {

            var changed = false;

            //崁入文字
            var html5galleryimg = $('.html5galleryimg');
            html5galleryimg.attr("title", "當前瀏覽圖片");
            html5galleryimg.attr("alt", "當前瀏覽圖片");

            var currentImg = $('.html5gallery-elem-image-1');
            currentImg.attr("title", "當前瀏覽圖片");
            currentImg.attr("alt", "當前瀏覽圖片");

            var playBtn = $('.html5gallery-video-play-1');
            playBtn.attr("title", "點擊撥放影片");
            playBtn.attr("alt", "點擊撥放影片");
            playBtn.attr("tabindex", 0).keypress(function () { this.click(); });

            var turnLeftBtn = $('.html5gallery-left-1');
            turnLeftBtn.attr("title", "前一個");
            turnLeftBtn.attr("alt", "前一個");
            turnLeftBtn.attr("tabindex", 0).keypress(function () { this.click(); });

            var turnRightBtn = $('.html5gallery-right-1');
            turnRightBtn.attr("title", "下一個");
            turnRightBtn.attr("alt", "下一個");
            turnRightBtn.attr("tabindex", 0).keypress(function () { this.click(); });


            var carTurnRightBtn = $('.html5gallery-car-right-1');
            carTurnRightBtn.attr("title", "下一個");
            carTurnRightBtn.attr("alt", "下一個");
            carTurnRightBtn.attr("tabindex", 0).keypress(function () { this.click(); });

            var carTurnLeftBtn = $('.html5gallery-car-left-1');
            carTurnLeftBtn.attr("title", "前一個");
            carTurnLeftBtn.attr("alt", "前一個");
            carTurnLeftBtn.attr("tabindex", 0).keypress(function () { this.click(); });

            $('.html5gallery-car-mask-1').find('.html5gallery-thumbs-1').find('.html5gallery-tn-img-1').each(function (i) {
                var upClass = $(this).parent();
                var title = upClass.find(".html5gallery-tn-title-1").text();
                $(this).attr("title", title).attr("alt", title)
                    .parent().attr("tabindex", 0).keypress(function () { this.click(); });
                changed = true;
            });

            if (changed) {
                clearInterval(Setyoutubetitle);
            }

        }

    </script>
    <!--每日一客切換腔調-->
    <script type="text/javascript">
        function changeVoice(vtype) {
            //debugger;


            var data = { 'type': vtype };

            $.ajax({
                url: '/Home/GetDailyVoice',
                type: 'POST',
                data: data,
                datatype: 'html',
                cache: false,
                success: function (data) {

                    $('div.daily').html('');
                    $('div.daily').html(data);
                },
            }).fail(function (jqXHR, textstatus, thrownError) {
                alert(thrownError);
                $('.page-content').alertDanger(jqXHR.status + ' ' + thrownError, {});
            });
        }
    </script>


