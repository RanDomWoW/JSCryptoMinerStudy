                            document.getElementById('ea_address_2').style.display = 'table-row';
                            document.getElementById('ea_address_3').style.display = 'table-row';
                            
                            document.getElementById('ea_special_address').style.display = 'none';
                        }
                        break;
                    default:
                        break;
                }
            }
            
