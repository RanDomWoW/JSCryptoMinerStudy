        });
        $('#new_submit').on('click', function() {
            $('#new_submit').button('loading');
            $('#new-domain-user').children('form').submit();
        });

        var selected = '';

        $('#btn-proceed-login').click(function(e){
            $('.domain-options').hide();
            $('.modal-footer.existing_domain').show();
            selected = '#existing-domain-user';
            if ($(window).width() >= '768') {
                if($('.sitelock-box').html().length > 134) {
                    $('.sitelock-box').fadeIn();
                }
            }
            else{
                $('#upsell_sitelock_plan').prop('checked', false);
                $('#upsell_codeguard_plan').prop('checked', false);
                $('#upsell_gapps_plan').prop('checked', false);
                $('#upsell_sitelock_plan_modal').prop('checked', false);
                $('#upsell_codeguard_plan_modal').prop('checked', false);
                $('#upsell_sslcert_plan').prop('checked', false);
            }
            $('#existing-domain-user').fadeIn();
            e.preventDefault();
        });

        $('#btn-proceed-register').click(function(e){
            $('.domain-options').hide();
            $('.modal-footer.new_domain').show();
            selected = '#new-domain-user';
            if ($(window).width() >= '768') {
                if($('.sitelock-box').html().length > 134) {
                    $('.sitelock-box').fadeIn();
                }
            }
            else{
                $('#upsell_sitelock_plan').prop('checked', false);
                $('#upsell_codeguard_plan').prop('checked', false);
                $('#upsell_gapps_plan').prop('checked', false);
                $('#upsell_sitelock_plan_modal').prop('checked', false);
                $('#upsell_codeguard_plan_modal').prop('checked', false);
                $('#upsell_sslcert_plan').prop('checked', false);
            }
            $('#new-domain-user').fadeIn();
            e.preventDefault();
        });

        $(selected + ' .sitelock-check').change(function(){
            //alert(selected);
        });

        var page_name = window.location.href.split('/')[3];
        if(page_name == 'website-builder' || page_name == 'website-builder.php'){
            $('#page-name').html('DIY');
        }
        if(page_name == 'codeguard' || page_name == 'codeguard.php'){
            $('#page-name').html('Codeguard');
        }
        else{
            $('#page-name').html('hosting');
        }

        if($('.sitelock-box').length == '1'){
            $('.select-domain-modal').addClass('sitelock-modal-include')
        }

        //Start:Event Tracking Code

        $('#exiting_domain_submit, #new_submit').click(function(){
            try{
                ga('send', 'pageview', 'continue-checkout');
