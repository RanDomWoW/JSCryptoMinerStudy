    function continue_to_add_order()
    {
        $('#exiting_domain_submit').button('loading');

        var actionURL = "/hosting.php";

        if($("#type_id").val() == 'sblite')
        {
            form_name = 'sitebuilderplan_'+plan_id;
            var product = 'diy';
        }
        else if($("#type_id").val() == 'email')
        {
            form_name = 'hostingplan_'+plan_id;
            var product = 'email';
        }
        else if($("#type_id").val() == 'bifm')
        {
                form_name = 'bifm_form';
                var product = 'bifm';
        }
        else if($("#type_id").val() == 'enterpriseemail'){
            form_name = 'enterpriseemail_form';
            var product = 'enterpriseemail';
        }
        else if($("#type_id").val() == 'codeguard'){
            form_name = 'codeguard_form';
            var product = 'codeguard';
        }
        else if($("#type_id").val() == 'sitelock'){
            form_name = 'sitelock_form';
            var product = 'sitelock';
        }
        else if($("#type_id").val() == 'sslcert'){
            form_name = 'comodo_form';
            var product = 'comodo';
        }
        else if($("#type_id").val() == 'eelite'){
            form_name = 'enterpriseemail_lite_form';
            var product = 'eelite';
        }
        else if($("#type_id").val() == 'gapps'){
            form_name = 'gapps_form';
            var product = 'gapps';
        }
        else if($("#type_id").val() == 'impressly'){
            form_name = 'impressly_form';
            var product = 'impressly';
        }
        else if ($("#type_id").val() == 'cloudsiteslinuxus') {
            form_name = 'cloudsite_'+plan_id;
            var product = 'cloudsiteslinuxus';
        }
        else if ($("#type_id").val() == 'weebly') {
            form_name = 'weebly_form';
            var product = 'weebly';
            actionURL = "/weebly.php"
        }
        else
        {
            form_name = 'hostingplan_'+plan_id;
            var product = 'hosting';
        }

        try {
            _gaq.push(['_trackPageview', '/modal/' + product + '/existing_domain_selected']);
        }
        catch(e){ };

        var domainname = "";

        if($("#domainname_opt_id").val() === undefined)
        {
            $('#'+form_name+' input[name="domain_name"]').val($("#existing_domain_name").val());
            domainname = $("#existing_domain_name").val();
        }
        else
        {
            if($("#domainname_opt_id").val() == 'other')
            {
                $('#'+form_name+' input[name="domain_name"]').val($("#existing_domain_name").val());
                domainname = $("#existing_domain_name").val();
            }
            else
            {
                $('#'+form_name+' input[name="domain_name"]').val($("#domainname_opt_id").val());
                domainname = $("#domainname_opt_id").val();
            }
        }

        var location = $('#'+form_name+' input[name="location"]').val();
        var type = $('#'+form_name+' input[name="type"]').val();

        $('#'+form_name+' input[name="action"]').val('check_domain_availability');
        var post_data = $('#'+form_name).serialize();

        //$('#gapps_form input[name="location"]').val('in');

        $.ajax({
            type: "POST",
            url: actionURL,
            data: post_data,
            dataType:'json',
            success: function(msg){
                if(msg.status == 'failed')
                {
                    $("#error").html(msg.value);
                    $("#error").show();
                    $('#exiting_domain_submit').button('reset');
                    return false;
                }
                else if(msg.status == 'upgrade')
                {
                    document.upgradeform.order_id.value = msg.order_id;
                    document.upgradeform.planid.value = msg.planid;
                    document.upgradeform.type.value = msg.type;
                    document.upgradeform.domain_name.value = msg.domain_name;
                    document.upgradeform.submit();
                }
                else if(msg.status== 'cart_conflict'){
                    populate_details_in_conflict_modal(msg.cart_product_details, msg.new_product_details);
                    $("#select_domain").hide();
                    $("#cart_conflict").show();
                    return false;
                }
                else
                {
                    $('#'+form_name+' input[name="action"]').val('add');
                    $('#'+form_name+' input[name="upsell_sitelock"]').val($("#upsell_sitelock_plan").is(":checked"));
                    $('#'+form_name+' input[name="upsell_codeguard"]').val($("#upsell_codeguard_plan").is(":checked"));
                    if($('#'+form_name+" input[name=upsell_gapps]").length > 0)
                    {
                        $('#'+form_name+' input[name="upsell_gapps"]').val($("#upsell_gapps_plan").is(":checked"));
                        console.log("this is fif")
                    }
                    else
                    {
                        $('<input>').attr({
                            type: 'hidden',
                            id: 'upsell_gapps',
                            name: 'upsell_gapps',
                            value: $("#upsell_gapps_plan").is(":checked")
                        }).appendTo( $('#'+form_name) );
                        console.log("this is elese")
                    }
                    if($('#'+form_name+" input[name=upsell_sslcert]").length > 0)
                    {
                        $('#'+form_name+' input[name="upsell_sslcert"]').val($("#upsell_sslcert_plan").is(":checked"));
                        console.log("this is fif")
                    }
                    else
                    {
                        $('<input>').attr({
                            type: 'hidden',
                            id: 'upsell_sslcert',
                            name: 'upsell_sslcert',
                            value: $("#upsell_sslcert_plan").is(":checked")
                        }).appendTo( $('#'+form_name) );
                        console.log("this is elese")
                    }
                    if($('#'+form_name+" input[name=upsell_sitelock_modal]").length > 0)
                    {
                        $('#'+form_name+' input[name="upsell_sitelock_modal"]').val($("#upsell_sitelock_plan_modal").is(":checked"));
                    }
                    else
                    {
                        $('<input>').attr({
                            type: 'hidden',
                            id: 'upsell_sitelock_modal',
                            name: 'upsell_sitelock_modal',
                            value: $("#upsell_sitelock_plan_modal").is(":checked")
                        }).appendTo( $('#'+form_name) );
                    }
                    if($('#'+form_name+" input[name=upsell_codeguard_modal]").length > 0)
                    {
                        $('#'+form_name+' input[name="upsell_codeguard_modal"]').val($("#upsell_codeguard_plan_modal").is(":checked"));
                    }
                    else
                    {
                        $('<input>').attr({
                            type: 'hidden',
                            id: 'upsell_codeguard_modal',
                            name: 'upsell_codeguard_modal',
                            value: $("#upsell_codeguard_plan_modal").is(":checked")
                        }).appendTo( $('#'+form_name) );
                    }

                    $('#'+form_name).submit();
                }
            }
        });

        $('#exiting_domain_submit').button('reset');

        return false;
    }

    function show_otherbox_for_domain_name()
    {
        if($('#domainname_opt_id').val() == 'other'){
            $('#other_domain_name').show();
        }
        else{
            $('#other_domain_name').hide();
        }
    }

    function check_domainname_submit()
    {
        var domainname = $("#new-domain-name").val();
        domainname = jQuery.trim(domainname);

        if(domainname == '')
        {
            $("#domain_error").html('Please enter domain name');
            $("#domain_error").show();
            $('#new_submit').button('reset');
            return false;
        }

        $('#hostingplan_with_domain input[name="upsell_sitelock"]').val($("#upsell_sitelock_plan").is(":checked"));
        $('#hostingplan_with_domain input[name="upsell_codeguard"]').val($("#upsell_codeguard_plan").is(":checked"));
        $('#hostingplan_with_domain input[name="upsell_sslcert"]').val($("#upsell_sslcert_plan").is(":checked"));
        $('#hostingplan_with_domain input[name="upsell_gapps"]').val($("#upsell_gapps_plan").is(":checked"));
        $('#hostingplan_with_domain input[name="upsell_codeguard_modal"]').val($("#upsell_codeguard_plan_modal").is(":checked"));
        $('#hostingplan_with_domain input[name="upsell_sitelock_modal"]').val($("#upsell_sitelock_plan_modal").is(":checked"));
        return true;
    }

    function close_domain_modal(id){
        closeModal();
        $('#existing-domain-user').hide();
        $('#new-domain-user').hide();
        $('.domain-options').show();
        $('.sitelock-box').hide();
    }
</script>
