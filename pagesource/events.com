<script type="text/javascript">
    var ajaxUrl = 'https://events.com/wp-admin/admin-ajax.php';

    jQuery(document).ready( function() {

        jQuery("#lead-form-hubspot").validate({
            rules: {
                firstname: "required",
                lastname: "required",
                event_name__c: "required",
                number_of_participants: "required",
                email: {
                    required: true,
                    email: true
                }
            },
            /*messages: {
                firstname: "Please complete this mandatory field.",
                lastname: "Please complete this mandatory field.",
                email: "Please complete this mandatory field."
            },*/
            submitHandler: function(form) {
                jQuery('#talkToSpecialistSubmitButton').prop('disabled', true);
                var data = {
                    action: 'talk_to_specialist_submit',
                    firstname: jQuery('#firstname').val(),
                    lastname: jQuery('#lastname').val(),
                    email: jQuery('#email').val(),
                    eventname: jQuery('#event_name__c').val(),
                    participants: jQuery('#number_of_participants').val(),
                    nonce: jQuery('#nonce').val()
                };

                jQuery.ajax({
                    type : "post",
                    dataType : "json",
                    url : ajaxUrl,
                    data : data,
                    success: function(response) {
                        if(response.type == "success") {
                            showSuccess();
                        } else {
                            showError(response.message);
                        }
                    },
                    error: function() {
                        showError('Unexpected error. Please try again later.');
                    }
                })

                return false;
            }
        });

        $('#talkToSpecialistModal').on('hidden.bs.modal', function () {
            cleanForm();
        });
    })

    function showSuccess() {
        jQuery('#talkToSpecialistSuccessWrapper').show();
       jQuery('#talkToSpecialistFormWrapper').hide();

        jQuery('#error-panel').hide();
    }

    function showError(message) {
       jQuery('#talkToSpecialistFormWrapper').show();
       jQuery('#error-message').text(message);
       jQuery('#error-panel').show();
       jQuery('#talkToSpecialistSuccessWrapper').hide();
       jQuery('#talkToSpecialistSubmitButton').prop('disabled', false);
    }

    function cleanForm()
    {
        jQuery('#firstname').val('');
        jQuery('#lastname').val('');
        jQuery('#email').val('');
        jQuery('#event_name__c').val('');
        jQuery('#number_of_participants').val('');
        jQuery('#talkToSpecialistSubmitButton').prop('disabled', false);
        jQuery('#talkToSpecialistSuccessWrapper').hide();
        jQuery('#talkToSpecialistFormWrapper').show();
        jQuery('#error-panel').hide();
    }

</script>

