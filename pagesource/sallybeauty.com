		var email = "null";		
		//jQuery("input[name=dwfrm_profile_customer_emailsubscribe]").val(email);
		
		jQuery.validator.addMethod(
		        "regex",
		        function(value, element, regexp) {
		            var re = new RegExp(regexp);
		            return this.optional(element) || re.test(value);
		        },
		        "Please enter a valid email."
		);
		
		jQuery('.emailsignup input[type="text"]').on("focus", function(){
			if(jQuery("#emailthankyou").is(":visible")){
				jQuery("#emailthankyou").fadeOut("fast");
			}
		});
		
			
		jQuery(".emailsignupinfo").on("submit",function (e) {
		e.preventDefault();
			
			jQuery(this).validate();
			
			jQuery("input[name=dwfrm_profile_customer_emailsubscribe]").rules("add", {regex:"^[\\w-\\.\\']{1,}\\@([\\da-zA-Z-]{1,}\\.){1,}[\\da-zA-Z-]{2,3}$"});		
						
			if (jQuery(this).valid()) {
				var link = jQuery(".emailsignupinfo").attr("action");
				var data = jQuery(this).serialize();			
				jQuery.ajax({
					dataType: "html",
					url		: link,	
					type: "post",
					data : data,
					success : function (response){
					 $("#emailthankyou").html(response).show();
					}
				});
				
				
			}
			return false;
			
		});
		
	});
		
