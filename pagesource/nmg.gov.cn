<script>
function pbtest(testval1){   

	var pattern = new RegExp("[%--`~!@#$^&*()=|{}':;'\\[\\].<>/?~]")
	var rs = "";
	for (var i = 0; i < testval1.length; i++) {
		rs = rs+testval1.substr(i, 1).replace(pattern, '');
	}
	
	var testval=document.getElementById("keywords");

	testval.value=rs;   
	return rs;
}
</script>
