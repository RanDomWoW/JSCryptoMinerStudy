<script>
var gtmDataLayerUtil = {
  "walletLoaded": function () {
    dataLayer.push({
      "event": "LoadWallet",
    });
  },

  "userLoggedIn": function (id) {
    dataLayer.push({
      "userID": id,
    });
  },

  "userRegistered": function () {
    dataLayer.push({
      "event": "Register",
    });
  },
    
  "addedToCart": function (gameId, price, quantity) {
    if(!window.GTM_GAMES_NOT_RAPID[gameId]) {
      return;
    }

    var gameAdded = window.GTM_GAMES_NOT_RAPID[gameId];
    gameAdded.quantity = quantity || 1;
    gameAdded.price = price || window.GTM_GAMES_NOT_RAPID[gameId].price;
    var addedToCart = [gameAdded];
    dataLayer.push({
      "event": "addToCart",
      "ecommerce": {
        "currencyCode": "EUR",
        "add": {
          "products": addedToCart
        }
      }
    });
  },

  "removedFromCart": function (gameId, price) {
    if(!window.GTM_GAMES_NOT_RAPID[gameId]) {
      return;
    }

    var gameRemoved = window.GTM_GAMES_NOT_RAPID[gameId];
    gameRemoved.price = price || window.GTM_GAMES_NOT_RAPID[gameId].price;
    var removedFromCart = [gameRemoved];
    dataLayer.push({
      "event": "removeFromCart",
      "ecommerce": {
        "remove": {
          "products": removedFromCart
        }
      }
    });
  },

  "checkedOut": function (gameId, price, quantity, step, variant) {
    if(!window.GTM_GAMES_NOT_RAPID[gameId]) {
      return;
    }

    variant = variant ||"";
    var gameCheckedOut = window.GTM_GAMES_NOT_RAPID[gameId];
    quantity = quantity || 1;
    gameCheckedOut.price = price || window.GTM_GAMES_NOT_RAPID[gameId].price;
    gameCheckedOut.quantity = quantity;
    gameCheckedOut.variant = variant;
    var checkedOutGames = [gameCheckedOut];
    dataLayer.push({
      "event": "checkout",
      "ecommerce": {
        "checkout": {
          "actionField": {
            "step": step
          },
          "products": checkedOutGames
        }
      }
    });
  },
  
  "purchased": function (gameId, serialId, price, quantity, couponType, variant, totalPrice) {
    if(!window.GTM_GAMES_NOT_RAPID[gameId]) {
      return;
    }

    variant = variant || "";
    price = price || window.GTM_GAMES_NOT_RAPID[gameId];
    totalPrice = totalPrice || price;
    var purchasedGame = window.GTM_GAMES_NOT_RAPID[gameId];
    purchasedGame.quantity = quantity || 1;
    purchasedGame.price = price;
    purchasedGame.coupon = couponType || "";
    purchasedGame.variant = variant;
    var purchasedGames = [purchasedGame];
    dataLayer.push({
      "event": "purchase",
      "ecommerce": {
        "purchase": {
          "actionField": {
            "id": serialId,
            "revenue": totalPrice
          },
          "products": purchasedGames
        }
      }
    })
  },

  "internalPromotion": function (id, name, creative, position) {
    var promotions = [{
      "id": id,
      "name": name,
      "creative": creative,
      "position": position
    }]

    dataLayer.push({
      "event": "promoview",
      "ecommerce": {
        "promoView": {
          "promotions": promotions
        }
      }
    });
  },

  "productDetails": function(gameId) {
    if(!window.GTM_GAMES_NOT_RAPID[gameId]) {
      return;
    }

    var gameDetails = window.GTM_GAMES_NOT_RAPID[gameId];
    var detailsList = [gameDetails];
    dataLayer.push({
      "event": "detail",
      "ecommerce": {
        "detail": {
          "products": detailsList
        }
      }
    });
  },

  "productImpressions": function() {
    var gamesInMenu = Object.keys(window.GTM_GAMES_NOT_RAPID).map(function(gameId) {
      return window.GTM_GAMES_NOT_RAPID[gameId];
    });
    
    gamesInMenu.forEach(function(el, index){
        el.position = index.toString();
        el.list = "Home";
    }); 
    
    dataLayer.push({
      "event": "impressions", 
      "ecommerce": {
        "currencyCode": "EUR",                      
        "impressions": gamesInMenu
      }
    });
  },

  "productClick": function(gameId, position) {
    if(!window.GTM_GAMES_NOT_RAPID[gameId]) {
      return;
    }
    
    var gameClicked = window.GTM_GAMES_NOT_RAPID[gameId];
    gameClicked.position = position;
    var productsClicked = [gameClicked];
    dataLayer.push({
      "event": "product click",
      "ecommerce": {
        "click": {
          "actionField": {
            "list": "Menu"
          },
          "products": productsClicked
        }
      }
    });
  },

  "promotionClick": function (element, url) {
    var c = element.parentNode.children, i = 0, index = -1;
    for (; i < c.length; i++) {
      if (c[i] == element) index = i;
    }

    dataLayer.push({
      'event': 'promotionClick',
      'eccommerce': {
        'promoClick': {
          'promotions': [
            {
              'id': url,
              'name': 'Special Product',
              'creative': 'banner' + index,
              'position': 'slot' + index
            }
          ]
        }
      }
    });
  }
};
</script>
