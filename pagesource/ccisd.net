function adjustImgs(){
   var imgTarget = $('#main-slider > ul.slides > li > div > div.image > img');
   var imgTargetParent = $('#main-slider > ul.slides > li > div > div.image');

   // Function check W:H of each image and adjusts accordingly
   imgTarget.each(function(){

       var imgW = $(this).width();
       var imgH = $(this).height();
       var imgWHr = imgW / imgH;

       var outW = imgTargetParent.width();
       var outH = imgTargetParent.height();
       var outWHr = outW / outH;

       if( outWHr >= imgWHr ){    
           $(this).css({
               'width' : '100%',
               'height' : 'auto'
           });
       }
       else {
           $(this).css({
               'width' : 'auto',
               'height' : '100%'
           });    
       }
        });
}

