<script type="text/javascript">
$(function() {
  var nav = $('.top-hidden-banner');
  var navTop = nav.offset().top+1100;
  var navHeight = nav.height()+10;
  var showFlag = false;
  var closePermanently = false;
  var bannerTextExist = false;
  nav.css('top', -navHeight+'px');

  $(window).scroll(function () {
    if (closePermanently == false && $(".top-hidden-banner a").length) {
      var winTop = $(this).scrollTop();
      if (winTop >= navTop) {
        if (showFlag == false) {
          showFlag = true;
          nav.addClass('top-hidden-banner-fixed')
              .stop().animate({'top' : '0px'}, 200);
          }
      } else if (winTop <= navTop) {
        if (showFlag) {
          showFlag = false;
          nav.stop().animate({'top' : -navHeight+'px'}, 200, function(){
          nav.removeClass('top-hidden-banner-fixed');
          });
        }
      }
    }
  });

  $('#top-hidden-banner-close').click(function() {
    if (closePermanently == false && $(".top-hidden-banner a").length) {
      if (showFlag) {
        showFlag = false;
        closePermanently = true;
        nav.stop().animate({'top' : -navHeight+'px'}, 200, function(){
          nav.removeClass('top-hidden-banner-fixed');
        });
      }
    }
  });
});
</script>

