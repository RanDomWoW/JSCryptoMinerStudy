<script type="text/javascript">
$(document).ready(function() {
  var totWidth = 0;
  var positions = new Array();
  $('#HeadlineContainer .HeadLineSlide').each(function (i) {
    positions[i] = totWidth;
    totWidth += $(this).width();
    if (!$(this).width()) { return false; }
  });
  $('#HeadlineContainer').width(totWidth);
  $('#NavigateContainer span a').mouseover(function (e, keepScroll) {
    $('span.menuItem').removeClass('act').addClass('inact');
    $(this).parent().addClass('act');
    var pos = $(this).parent().prevAll('.menuItem').length;
    if(!(current % $('#NavigateContainer span a').length)) {
      if(current == 0) {
        current = pos;
        $('#HeadlineContainer').stop().animate({ marginLeft: -positions[pos] + 'px' }, 450);
      } else {
        $('#HeadlineContainer').stop().css({ marginLeft: '0px' }, 0);
        current = 0;
        $('#NavigateContainer span a').eq($('#NavigateContainer span a').length - 5).trigger('mouseover', [true]);
      }
    } else {
      current = pos;
      $('#HeadlineContainer').stop().animate({ marginLeft: -positions[pos] + 'px' }, 450);
      if(current == $('#NavigateContainer span a').length - 1) {
        $('span.menuItem:first').addClass('inact');
      }
    }
    e.preventDefault();
    if (!keepScroll) clearInterval(itvl);
  });
  $('#NavigateContainer span.menuItem:first').addClass('act').siblings().addClass('inact');
  $('#NavigateContainer span a').mouseout(function (e, keepScroll) {
    if (!keepScroll) itvl = setInterval(function () { autoAdvance() }, 5 * 750);
  });
  var current = 1;
  function autoAdvance() {
    $('#NavigateContainer span a').eq(current % $('#NavigateContainer span a').length).trigger('mouseover', [true]);
    current++;
  }
  var itvl = setInterval(function () { autoAdvance() }, 5 * 750);
});
</script>
