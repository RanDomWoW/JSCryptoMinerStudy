    <script>
        $(document).ready(function () {
            $('table').each(function () {
                $(this).find('td[valign=top]').css("vertical-align", "top");
                $(this).find('th[valign=top]').css("vertical-align", "top");

                $(this).find('td[valign=bottom]').css("vertical-align", "bottom");
                $(this).find('th[valign=bottom]').css("vertical-align", "bottom");
            });
            $("table.tablesorter").tablesorter({
                sortList: [[0, 0]]
            });

            $('blockquote').each(function (index) {
                var cite = $(this).attr("cite");
                var title = $(this).attr("title");
                var add = "";
                if (title)
                {
                    add = "<span class='bq-title'>" + title + "</span>";
                }
                if (cite) {
                    $(this).append("<cite>" + cite + add+ "</cite>");
                } else if (title)
                {
                    $(this).append("<cite>"  + add + "</cite>");

                }
               
              
            });
        });
       
                               
                          

