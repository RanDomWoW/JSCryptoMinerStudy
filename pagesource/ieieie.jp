<script type="text/javascript">
	$(function(){

        //ie&edgeでobject-fit適応
        objectFitImages('.img_fit');

		var topBtn = $('#pagetop');

		$(window).scroll(function () {
			if ($(this).scrollTop() > 200) {
				topBtn.fadeIn();
			} else {
				topBtn.fadeOut();
			}
		});
	
		$('a[href^=#]').click(function() {
			var href= $(this).attr("href");
			var target = $(href == "#" || href == "" ? 'html' : href);
			var header_h = $('#header_top').height();
			$('body, html').animate({ scrollTop: target.offset().top - header_h }, 500);
			return false;
		});

		$('a img.imgover').hover(function(){
			$(this).attr('src', $(this).attr('src').replace('_out', '_over'));
				}, function(){
				   if (!$(this).hasClass('current')) {
				   $(this).attr('src', $(this).attr('src').replace('_over', '_out'));
			}
		});

		//$(".overall_link").mouseover(function(){ //cloneで作られたもの対策
			/*$(".overall_link").click(function(){
				if( $(this).find('a').attr('target') == '_blank'){
					window.open().location = $(this).find('a').attr("href");
				}else{
					window.location = $(this).find('a').attr("href");
				}
				return false;
			});*/
		//});
	});

	$(window).load(function(){
		$(".overall_link").click(function(){
			if( $(this).find('a').attr('target') == '_blank'){
				window.open().location = $(this).find('a').attr("href");
			}else{
				window.location = $(this).find('a').attr("href");
			}
			return false;
		});
	});
	
	//ヘッダーボタンの色変更
	$(function(){
		
		//ヘッダートップ固定
		$(window).scroll(function(){
			var header_top_h = $('#header_top').height();
			var scroll_top = $(window).scrollTop();
			if(scroll_top > header_top_h){
				$('#header_top').addClass('fixed').stop().animate({'top':0});
				$('#h_top_logo').show();
			}else{
				$('#header_top').removeClass('fixed').stop().animate({'top': -header_top_h});
				$('#h_top_logo').hide();
			}
		});
	});
</script>
