function extractUrlParams(url){
   var t = url.substring(1).split('&');
   var f = [];
   for (var i=0; i<t.length; i++){
      var x = t[ i ].split('=');
      f[x[0]]=x[1];
   }
   return f;
}
function envoieRequete(url,id){
	var xhr_object = null; var position = id; var page='';
