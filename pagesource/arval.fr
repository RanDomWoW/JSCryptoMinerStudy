    <script>
    (function ($){
    var baseURL = "/findmycarportlet?p_p_id=arvalwsquotesearchbasic_WAR_arvalwsportlet_INSTANCE_f7pkwlcFzAVf&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&_arvalwsquotesearchbasic_WAR_arvalwsportlet_INSTANCE_f7pkwlcFzAVf_sourceSystemRef=QOT001&p_p_resource_id=";    
    var makesEndPoint = 'getMakes';
    var modelsEndPoint = 'getModels';
    var specsEndPoint = 'getSpecs';

    function cleanUpSpecialChars(str) {
	str = str.replace(/[éè]/g,"e");
	str = str.replace(/[ò]/g,"o");
	str = str.replace(/[+_']/g,"-");
	str = str.replace(/[()]/g, "");
	return str;
    }
 
    function getMakes() {
    	$('error-veh-type').hide();
    	callAjax(baseURL + makesEndPoint, {}, function(makes) {
    		reset($('#makeRef'), makes.length, "");
    		for (var i=0; i<makes.length; i++) {
    			createOption($('#makeRef'), makes[i].name, makes[i].makeRef);

    		}
    	});
    }
    
    function createOption($select, text, value) {
    	$option = $('<option></option>');
    	
        if(value == "none"){
            $option.text("Selectionnez");
        }else{
            $option.text(text);
        }
    	$option.attr("value",value);
    	$select.append($option);
    }
    
    function reset($select, length, addText) {
    	$select.empty();
    	$optionNone = $('<option></option>');
    	if(length > 0) {
    		$optionNone.text('Sélectionnez');
    		$select.prop("disabled", false);
    	} else {
    		$optionNone.text('Sélectionnez un ' + addText);
    		$select.prop("disabled", true);
    	}
    	$optionNone.attr("value","none");
    	$select.append($optionNone);
    }
    
    function getModels() {
    	$('#error-make').hide();
    	callAjax(baseURL + modelsEndPoint, {}, function(models) {
    		reset($('#modelRef'), models.length, "make");
    		reset($('#specificationRef'), 0, "model");
    		for (var i=0; i<models.length; i++) {
    			createOption($('#modelRef'), models[i].name, models[i].modelRef);
    		}
    	});
    }
    
    function getSpecs() {
    	$('#error-model').hide();
    	var params = {};
    	params.modelRef = $('#modelRef').val();
    	callAjax(baseURL + specsEndPoint, params, function(specs) {
    		reset($('#specificationRef'), specs.length, "model");
    		for (var i=0; i<specs.length; i++) {
    			createOption($('#specificationRef'), specs[i].specificationName, specs[i].vehicleRef);
    		}
    	});
    }
    
    function callAjax(url, params, callback) {
    	//params.vehicleTypeRef = $('#vehicleTypeRef').val();
	params._arvalwsquotesearchbasic_WAR_arvalwsportlet_INSTANCE_f7pkwlcFzAVf_vehicleTypeRef = $('#vehicleTypeRef').val();
    	params.makeRef = $('#makeRef').val();

    	if(params.modelRef == "none") params.makeRef = "none";
    	
    	$.ajax({
    		url : url,
    		type : 'POST',
    		data : params,
    		datatype : 'json',
    		async : true,
    		success : function(json) {
    
    		$('#results').html(json);	
    			callback(eval('(' + json + ')'));
    		},
    		error : function() {
    			// alert('JSON error x');
    		}
    	});
    }
    function submitForm() {
    	var url;
        $('#error-make').hide();

        if($('#makeRef').val() == 'none'){
            $('#error-make').show();

        }else{
        	if($('#modelRef').val() != 'none'){
		        url = '/location/'+$('#makeRef option:selected').text()+'-'+$('#makeRef').val()+'/'+$('#modelRef option:selected').text()+'-'+$('#modelRef').val();
        	}else{
		        url = '/location/'+$('#makeRef option:selected').text()+'-'+$('#makeRef').val()+'?vehicleTypeRef='+$('#vehicleTypeRef').val();
        	}

	    url = cleanUpSpecialChars(url);

            url = url.replace(/ /g, '-');
        	$(location).attr('href',url);
        }
    }
    
    $(document).ready(function() {
       	getMakes();
	$('#vehicleTypeRef').live('change',function(){
                getMakes();
        });
        $('#makeRef').live('change',function(){
                getModels();
        });
        $('#modelRef').live('change',function(){
                getSpecs();
        });
        $('#submitOffer').live('click',function(){
                submitForm();
        });
    });

    })(jQuery);
    </script>
