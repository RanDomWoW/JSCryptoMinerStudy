					$this = $(this);
					if (event.type == "mouseover" && $this.text() == '已加入购物车') {//鼠标悬浮
						$this.addClass('btn_cart_o').html('去结算').attr('href',
								'/show/shopcart/list.do').attr('target',
								'_blank');
					} else if (event.type == "mouseout"
							&& $this.text() == '去结算') {//鼠标离开
						$this.removeClass('btn_cart_o').html('已加入购物车');
					}
				})
	})
</script>

