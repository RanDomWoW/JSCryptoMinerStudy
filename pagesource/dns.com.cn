<script type="text/javascript">
	function GetQueryString(name) {
		var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
		var r = window.location.search.substr(1).match(reg);
		if (r != null)
			return unescape(r[2]);
		return null;
	}

	function setNavCurClass(urlpara) {
		var locatStr = window.location;
		if (locatStr == 'http://dev.dns.com.cn/' || locatStr == 'http://www.dns.com.cn/' || locatStr == 'http://www.dns.com.cn'
				|| locatStr == 'https://www.dns.com.cn') {
			$('#nav_index').addClass('index_cur');
		}
		if (locatStr.toString().indexOf('beian_flow_mod') != -1) {
			$('#HeaderNav li').removeClass('index_cur');
			$('#nav_webhosting').addClass('index_cur');
		}
        var _local = locatStr.pathname.split('/');
		var mypara = _local[2];
			
		var cur_index = 0;
		if (mypara != null && mypara.toString().length > 1) {
			if (mypara.indexOf('webhosting') != -1) {
				mypara = 'webhosting';
			}
			switch (mypara) {
			case 'home':
				cur_index = 0;
				break;
			case 'domain':
				cur_index = 1;
				break;
			case 'host':
				cur_index = 2;
				break;
			case 'bce':
				cur_index = 3;
				break;
			case 'buildsite':
				cur_index = 4;
				break;
			case 'mailcloud':
			case 'mailxun':
 
			  
				cur_index = 5;

				break;
			case 'rent':
				cur_index = 6;
				break;
			case 'mobilead':
				cur_index = 7;
				break;
			case 'crm':
				cur_index = 8;
				break;
			default: 
				cur_index = 9
				break;
			}
			$('#HeaderNav li').eq(cur_index).addClass('index_cur').siblings().removeClass('index_cur');
		}
		;
	}
	setNavCurClass('module');
</script>






