        var keynum;
        if (window.event) // IE  
        {
            keynum = window.event.keyCode
        }
        else if (event.which) // Netscape/Firefox/Opera  
        {
            keynum = event.which
        }
        if (keynum == 13) {
            {
                search();
            }
            if (window.event)
                window.event.returnValue = false;
            else
                event.preventDefault(); //for firefox  
        }
    }
