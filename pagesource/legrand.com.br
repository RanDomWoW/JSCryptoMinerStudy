		<script>
			function dados_setCookie(cname, cvalue, exdays) {
				var d = new Date();
				d.setTime(d.getTime() + (exdays*24*60*60*1000));
				var expires = "expires="+ d.toUTCString()+"; path=/";
				document.cookie = cname + "=" + cvalue + "; " + expires;
			}

			function dados_getCookie(cname) {
				var name = cname + "=";
				var ca = document.cookie.split(';');
				for(var i = 0; i <ca.length; i++) {
					var c = ca[i];
					while (c.charAt(0)==' ') {
						c = c.substring(1);
					}
					if (c.indexOf(name) == 0) {
						return c.substring(name.length,c.length);
					}
				}
				return "";
			}

			var nome = dados_getCookie('nome');
			if(nome!==undefined && nome!==''){
				dataLayer.push({
					'cliente': {
						'tipo': 'Newsletter',
						'nome': dados_getCookie('nome'),
						'email': dados_getCookie('email'),
						'perfil': dados_getCookie('perfil')
					}
				});
			}
		</script>
