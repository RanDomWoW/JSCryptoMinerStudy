  <script type="text/javascript">
                function subscribe() {
                    if(useragentid !=null){
                        OneSignal.setSubscription(true);
                    } else {
                        OneSignal.registerForPushNotifications({
                            modalPrompt: true
                        });
                    }
                }
                
                function unsubscribe() {
                    $("#notification-bubble").removeClass("subscribed");
                    $("#notification-bubble").addClass("to-unsubscribe");
                    $(document).off('click', "#notification-bubble");
                    $(document).on('click', "#notification-bubble", function() {unsubscribeConfirm()});
                }
                
                function unsubscribeConfirm() {
                    $("#notification-bubble").removeClass("to-unsubscribe");
                    $(document).off('click', "#notification-bubble");
                    $(document).on('click', "#notification-bubble", function() {subscribe()});
                    OneSignal.setSubscription(false);
                    $("#notification-bubble").append('<span class="unsubscribe-confirmation">Již nebudete dostávat žádné další notifikace</span>');
                    $(".unsubscribe-confirmation").fadeOut(3000, function() { $(".unsubscribe-confirmation").remove(); });
                }

                $(document).ready(function() {					
                    $("html").on("click", function(e) {
                        if (!$(e.target).hasClass("to-unsubscribe")) { if ($("#notification-bubble.to-unsubscribe").length) {
                            $("#notification-bubble").removeClass("to-unsubscribe");
                            $("#notification-bubble").addClass("subscribed");
                            $(document).off('click', "#notification-bubble");
                            $(document).on('click', "#notification-bubble", function() {unsubscribe()});
                        }}
                    });
                });
                
                window.onload = function() {
                    OneSignal.push(function() {
                      
                        if (!OneSignal.isPushNotificationsSupported()) {
                            return;
                        }
                        OneSignal.getUserId().then(function(userId) {                
                            if(userId == null){
                            } else {
                                useragentid = userId;
                                OneSignal.push(["getNotificationPermission", function(permission){}]);
                            }
                            OneSignal.isPushNotificationsEnabled(function(isEnabled) {
                                if (isEnabled) {
                                    $("#notification-bubble").addClass("subscribed")
                                    $(document).on('click', "#notification-bubble", function() {unsubscribe()});
                                } else {
                                    OneSignal.showHttpPrompt();
                                    $(document).on('click', "#notification-bubble", function() {subscribe()});
                                }
                                $("#notification-bubble").show();
                            });
                        });
                        
                        OneSignal.on('subscriptionChange', function (isSubscribed) {
                            if (isSubscribed) {
                                OneSignal.getUserId().then(function(userId) {
                                    useragentid = userId;
                                })
                                $("#notification-bubble").addClass("subscribed");
                                $(document).off('click', "#notification-bubble");
                                $(document).on('click', "#notification-bubble", function() {unsubscribe()});
                            } else {
                                OneSignal.getUserId().then(function(userId) {
                                    useragentid = userId;
                                })
                                $("#notification-bubble").removeClass("subscribed");
                                $(document).off('click', "#notification-bubble");
                                $(document).on('click', "#notification-bubble", function() {subscribe()});
                            }
                        });
