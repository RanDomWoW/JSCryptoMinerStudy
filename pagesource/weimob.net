<script>
    $("#actBanBg").on("click", function(e){
        e = e || window.event;
        var $this = $(this);

        var obj = getBannerObj($this);

        if(e.target && e.target.tagName.toUpperCase() == "A") {
            return ;
            wmDot.bannerDot("banner_rgst" , obj.name)
        };

        wmDot.bannerDot("banner" , obj.name)
        var link = obj.link;
        window.open(link);
    });

    function getBannerObj($this){
        var obj = { name: '微商城', link: 'http://wp.weimob.net'}
        if($this.hasClass('kld')){
            obj.name = "客来店";
            obj.link = "http://kld.weimob.net";
        }else if($this.hasClass('shop')){
            obj.name = "小店";
            obj.link = "http://xd.weimob.net";
        }else if($this.hasClass('zd')){
            obj.name = "智店";
            obj.link = "http://zd.weimob.net/";
        }else if($this.hasClass('buss')){
            obj.name = "微商城";
            obj.link = "http://wp.weimob.net";
        }else if($this.hasClass('met')){
            obj.name = "会务";
            obj.link = "http://hw.weimob.net";
        }else if($this.hasClass('canting')){
            obj.name = "智慧餐厅";
            obj.link = "http://canting.weimob.net/";
        }else if($this.hasClass('store')){
            obj.name = "微盟旺铺";
            obj.link = "http://wmwp.weimob.net/";
        }else if($this.hasClass('beauty')){
            obj.name = "智慧美业";
            obj.link = "http://meiye.weimob.net/";
        }
        return obj;
    }

    $(function(){
        //获取商户数
        $.ajax({
            url: "/website//api/getBusinessCount",
            dataType: "JSON",
            type:"post",
            success:function(res){
                if(res){
                    countScroll[countScroll.length-1].countUp.update(res.count);
                }
            },error:function(res){

            }
        })
    })
</script>

