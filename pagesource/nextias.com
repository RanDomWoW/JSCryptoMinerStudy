               $('.fancybox-thumb'+numIndex).click(function(e) {
                 var uniques = {};
                 $(this).attr('rel', 'fancybox-thumb');
                 uniques[$(this).attr('href')] = true;
                 $('.fancybox-thumb'+numIndex).not(this).each(function() {
                     href = $(this).attr('href');
