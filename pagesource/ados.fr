	<script type="text/javascript">

$(document).ready(

	function (){

        if($('#wrapper').html()!=null){
    	    var posTop=$('#wrapper').offset().top;
        }

		if(posTop!=' '){
			$(window).bind('scroll',function(){
				if($(window).scrollTop()>=posTop){
					$('#tools .tools_subnav').stop().animate({height:'37px'},300,function(){
						$('#tools .tools_subnav').addClass('tools_subnav_open');
					});
				}else{
					$('#tools .tools_subnav').stop().animate({height:'0'},300,function(){
						$('#tools .tools_subnav').removeClass('tools_subnav_open');
					});
				}
			});
		}

		var elt_larg=0;
		for(var i=0; i<=$('#tools .tools_subnav ul li').length ;i++){
			elt_larg=parseInt($('#tools .tools_subnav ul li:eq('+i+')').width()+20)+elt_larg;
		}

		$('#tools .tools_subnav ul').width(elt_larg);

		if(elt_larg>parseInt($('#tools .tools_subnav .liste .liste_cnt').width())){
			$('#tools .tools_subnav .bt').css({display:'block'});
		}

		$('#tools .tools_subnav .bt_prec').bind('click',function(){
			$('#tools .tools_subnav ul li:last').prependTo($('#tools .tools_subnav ul'));
		});

		$('#tools .tools_subnav .bt_suiv').bind('click',function(){
			$('#tools .tools_subnav ul li:first').appendTo($('#tools .tools_subnav ul'));
		});


        if($.browser.msie){

            $.fn.placeholder = function() {

                var elt=$(this),
                    elt_placeholder=elt.attr('placeholder');

                $('head').append('<style type="text/css">input.placeholder{color:#a06d87}</style>');

                elt.val(elt_placeholder).addClass('placeholder');

                elt.bind('focus',function(){
                    if(elt.val()===elt_placeholder){
                        elt.val('').removeClass('placeholder');
                    }
                }).bind('blur',function(){
                    if(elt.val()===''){
                        elt.val(elt_placeholder).addClass('placeholder');
                    }
                });

            }

            $('input[type="text"]').each(function(){
                $(this).placeholder();
            });

        }

	}

);

</script>









