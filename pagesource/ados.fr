		$('.bloc_home_news').onglet_listing();


		$.fn.slideshow = function() {

			var elt=$(this);
			elt.find('.slide ul li:first').css({zIndex:'1',display:'block'});

			elt.find('.slide ul li').each(function(){
                var url=$(this).find('.titre a').attr('href');
                $(this).find('.titre a').replaceWith($(this).find('.titre a').html());
                $(this).find('img, .texte').wrapAll('<a href="'+url+'"></a>');
            });

			if(elt.find('.slide ul li').length>1){

				var pos_slide=0,
				    slideshow='';

				elt.find('.miniature ul li:eq('+pos_slide+')').addClass('on');

				function launch_slideshow(){
					slideshow=setInterval(function(){
						pos_slide++;
						if(pos_slide==elt.find('.slide ul li').length){
							pos_slide=0;
						}
						show_slide();
					},4000);
				}

				function show_slide(){
					elt.find('.slide ul li').not(':eq('+pos_slide+')').stop().dequeue().css({zIndex:'0',opacity:'1'});
					elt.find('.miniature ul li').not(':eq('+pos_slide+')').removeClass('on');
					elt.find('.miniature ul li:eq('+pos_slide+')').addClass('on');
					elt.find('.slide ul li:eq('+pos_slide+')').css({zIndex:'1'}).fadeIn(500,function(){
						elt.find('.slide ul li').not(':eq('+pos_slide+')').css({display:'none'});
					});
				}

				elt.find('.miniature ul li').bind('mouseenter',function(){
					pos_slide=elt.find('.miniature ul li').index(this);
					show_slide();
				});

				elt.hover(function(){
					clearInterval(slideshow);
				},function(){
					clearInterval(slideshow);
					launch_slideshow();
				});

				launch_slideshow();

			}

		};

		$('.bloc_home_alaune').slideshow();

	}
);

</script>


