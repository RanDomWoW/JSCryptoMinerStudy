<script type="text/javascript" >

 $.fn.sticky = function () {

            var elt = $(this),
                    habHeight = parseInt($('#header').offset().top, 10),
                    posTop = parseInt(elt.offset().top, 10) - (habHeight) - $('#tools').height();

            $(window).bind('scroll', function () {

                habHeight = parseInt($('#header').offset().top, 10);

                var elt_height = parseInt(elt.height() + 20, 10),
                        elt_parentheight = elt.parents('#conteneur').height(),
                        posBottom = posTop + habHeight + elt_parentheight;

                console.log(elt_parentheight );

                if ($(window).scrollTop() >= (posTop + habHeight)) {
                    if (parseInt($(window).scrollTop() + elt_height, 10) >= posBottom) {
                        elt.css({top: parseInt(elt_parentheight - elt_height, 10) + 'px'});
                    } else {
                        elt.css({top: parseInt($(window).scrollTop() - (posTop + habHeight), 10) + 'px'});
                    }
                } else {
                    elt.css({top: '0'});
                }
            });

        };

$(document).ready(function(){   

        if ($(window).width()>1024) {
            $('.right_col').sticky();
        }
});

</script>


