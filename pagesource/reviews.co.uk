    <script language='javascript'>
        function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires="+d.toUTCString();
            var domain = "domain=.reviews.co.uk";
            document.cookie = cname + "=" + cvalue + ";" + expires + ";" + domain + ";path=/";
        }

        function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for(var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return JSON.parse(c.substring(name.length, c.length));
                }
            }
            return new Array();
        }

        var path = getCookie('reviewPath');
        path.push(document.location.href);
        var journey = path.filter(function(i, p){
            return path.indexOf(i)==p;
        });

        setCookie('reviewPath', JSON.stringify(journey), 1);

        $(document).ready(function(){
            $('input[name=signup_journey]').val(journey);
        });


        var RECAPTURE_KEY = '6LdSuhITAAAAADz49Larze6UhZ_jFC3_jENWZXRJ';

