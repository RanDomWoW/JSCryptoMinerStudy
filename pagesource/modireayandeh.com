      jQuery(this).parent().hasClass("active") ? (jQuery(this).parent().find("ul").slideUp("150"), 
      jQuery(this).parent().removeClass("active")) : (jQuery(this).parent().find("ul.sub_topics").slideDown("150"),
      jQuery(this).parent().addClass("active"))
}
</script>
