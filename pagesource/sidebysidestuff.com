$(function(){
	var leftNavElem = $('#leftnav');
	var leftNavParentElem = leftNavElem.parent();
	var parentPos = leftNavElem.parent().offset().top;
	window.navPos = leftNavElem.offset().top;
	var parentHeight = leftNavParentElem.height();
	window.maxTop = parentHeight - leftNavElem.height() - 20;

	document.addEventListener("resize", function(){
		parentPos = leftNavElem.parent().offset().top;
		window.navPos = leftNavElem.offset().top;
		parentHeight = leftNavParentElem.height();
		window.maxTop = parentHeight - leftNavElem.height() - 20;
	});

	document.addEventListener("scroll", function(){
		var scrollPos = Math.max($('html').scrollTop(),$('body').scrollTop());
		var topOffset = scrollPos - window.navPos;
		if( scrollPos > window.navPos && topOffset < window.maxTop ){
			$('#leftnav').css({'top': topOffset + 'px'});
		}else if( scrollPos > window.navPos ){
			$('#leftnav').css({'top': window.maxTop + 'px'});
		}else{
			$('#leftnav').css({'top': '0px'});
		}
	});
});
