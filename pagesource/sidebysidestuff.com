	jQuery(document).ready(function(){
	/*remove default floating cart functionality*/
		jQuery('#footerlinks li.last').on('click', function(){
			window.location.href = jQuery(this).find('a').attr('href');	
		});
	});

	/* CUSTOM CODE disruptive-advertising ADDED BY YTIMES on 03/07/2018 */
	var whenExists = (function(){
		var processElements = function(elements){
			for (var i = 0; i < elements.length; i++) {
				if (document.querySelectorAll(elements[i]).length) {
					elements.splice(i,1);
					i--;
				}
			}
			return elements;
		};
		return function(elements,callback){
			elements = elements.split(',');
			elements = processElements(elements);
			if (elements.length === 0) {
				callback();
			}else{
				var interval = setInterval(function() {
					elements = processElements(elements);
					if (elements.length === 0) {
						callback();
						clearInterval(interval);
					}
				}, 100);
			}
		};
	})();
	// active switcher
	var activePage = false;
	if ( window.location.href.match( /sidebysidestuff\.com\/?((#.*)|(\?.*))?$/i ) ){ activePage = 'home'; }
	if ( window.location.href.match('index.html') ){ activePage = 'home'; }
	if ( window.location.href.match('shop-by-vehicle.html') ){ activePage = 'vehicles'; }
	if ( window.location.href.match('featured-brands.html') ){ activePage = 'brands'; }
	if ( window.location.href.match('shop-by-category.html') ){ activePage = 'categories'; }
	if ( window.location.href.match( /secure\.sidebysidestuff\.com\//i ) ){ activePage = 'cart'; }
	if( activePage == 'cart'){
	  jQuery('body').append('\
	  <mobile-app-nav>\
		<div>\
		  <a class="vehicles'+(activePage == 'vehicles' ? ' active':'')+'" href="shop-by-vehicle.html"><icon ></icon>Vehicles</a>\
		  <a class="brands'+(activePage == 'brands' ? ' active':'')+'" href="featured-brands.html"><icon ></icon>Brands</a>\
		  <a class="categories'+(activePage == 'categories' ? ' active':'')+'" href="shop-by-category.html"><icon ></icon>Categories</a>\
		  <a class="cart'+(activePage == 'cart' ? ' active':'')+'" href="#"><icon ></icon>Cart</a>\
		</div>\
	  </mobile-app-nav>\
	  ');
	}else{
	  whenExists('.shopbtn > .shpbtnlnk > a, #cartTotal', function(){
		var cartLink = jQuery('.shopbtn > .shpbtnlnk > a').attr('href');

		jQuery('body').append('\
		<mobile-app-nav>\
		  <div>\
			<a class="vehicles'+(activePage == 'vehicles' ? ' active':'')+'" href="shop-by-vehicle.html"><icon ></icon>Vehicles</a>\
			<a class="brands'+(activePage == 'brands' ? ' active':'')+'" href="featured-brands.html"><icon ></icon>Brands</a>\
			<a class="categories'+(activePage == 'categories' ? ' active':'')+'" href="shop-by-category.html"><icon ></icon>Categories</a>\
			<a class="cart'+(activePage == 'cart' ? ' active':'')+'" href="'+cartLink+'"><icon ></icon><cart-item-count>0</cart-item-count>Cart</a>\
		  </div>\
		</mobile-app-nav>\
		');

		var updateInterval;
		var updateCartItemCount = function(){
		  var cartItemCount = jQuery('#cartTotal').html().match( /(\d+)\sItem/i )[1];

		  if(cartItemCount > 0){
			clearInterval(updateInterval);
			jQuery('cart-item-count').html(cartItemCount);
		  }
		};
		updateCartItemCount();
		updateInterval = setInterval(updateCartItemCount,500);
	  });
	}
</script>

