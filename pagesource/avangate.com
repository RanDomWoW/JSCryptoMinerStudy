<script type="text/javascript">

	/* Home Page */
	
	$(function(){
		$('.homePage .normalView .title, .homePage .hoverView .title, .resourcesContent .resourcesTitle h3 a, .resourcesContent .resourcesDetails, .homePage .latestNewsPreview').ellipsis({
			row: 4,
			onlyFullWords: true,
			position: 'tail',
			char: '...'
		});
	});

	$(document).ready(function(){
		
		if ($(window).width() > 768 ) {
			$(".ajax").colorbox({
				maxWidth:"760",
				scrolling: false, 
				onComplete : function() { 
					$(this).colorbox.resize(); 
				}
			});
		}
		if ($(window).width() < 767 ) {
			$(".ajax").colorbox({
				maxWidth:"90%",
				scrolling: false, 
				onComplete : function() { 
					$(this).colorbox.resize(); 
				}
			});
			jQuery(window).resize(function() {
				jQuery.colorbox.resize({width:"90%"});
			});
		}
		
		//Animate Number...
		$(window).load(function(){
			$('.timer').each(function(){
				$(this).one('inview', function (event, visible) {
					if(visible === true) {
						var val = $(this).attr('data-value');
						var initialVal = 0;
						var comma_separator_number_step = $.animateNumber.numberStepFactories.separator(',');
						$(this)
						.prop('number', initialVal)
						.animateNumber({ 
							number: val,
							numberStep: comma_separator_number_step
						}, 750);
					}
				});
			});
			$('.donut, .bounceIn').each(function(){
				$(this).one('inview', function (event, visible) {
					if(visible === true) {
						var val = $(this).addClass('active');
					}
				});
			});
		});		
		
		//check if header has class "header cookie-buffer"
	
		if ($(".header").hasClass('cookie-buffer')) {
			$('.wrap_all').addClass('cookie-buffer');
		} else {
			$('.wrap_all').removeClass('cookie-buffer');
		}
		
		$('#CookieClear').on('click', function(){
			$('.wrap_all').removeClass('cookie-buffer');
		});
		
	});

	// Typed function
	$(function(){
		$(".element").typed({
			strings: ["Digital Goods Sales", "Software Sales", "SaaS Sales"],
			typeSpeed: 100,
			loop: true
		});
	});
  
</script>


