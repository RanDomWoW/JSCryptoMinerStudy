         else if(lang === "ru" && currLang != "ru"){
            document.location.href = '/?ru_lang';
        }

    }

    function getCookie(name) {
        var value = "; " + document.cookie;
        var parts = value.split("; " + name + "=");
        if (parts.length === 2) return parts.pop().split(";").shift();
    }

    function changeLanguage(language) {
        var resturl = "";
        var lang = "ru"
        if (lang == "kz") {

            resturl = "/kaz/lang/ChangeLanguageController/changeLanguage";
        }
        else {

            resturl = "/lang/ChangeLanguageController/changeLanguage"
        }


        $.ajax({
            url: resturl,
            data: {language: language},
            type: 'GET',
            success: function (data) {
                console.log("changeLanguage new method " + language);
                location.reload();
            },
            error: function (jxhr, msg, err) {
                console.log("could not changeLanguage new method " + language);
            }
        });
    }

    window.onload = function () {
        var siteLang = getCookie("i18next");

        if (siteLang === 'ru') {
            //$('#kaz-lang-button').css('background', 'none');
            //$('#rus-lang-button').css('background', 'lightgrey');

            $('#td-d-header-language').val(siteLang);
        }
        if (siteLang === 'kz') {
            //$('#rus-lang-button').css('background', 'none');
            //$('#kaz-lang-button').css('background', 'lightgrey');

            $('#td-d-header-language').val(siteLang);
        }

        $('#td-d-header-language').on('change', function(e) {
            var option_selected = $("option:selected", this);
            var value_selected = this.value;

            changeLang(value_selected);
        });
    };



</script>




