    jQuery.ab.fn.applyHotelsListingBookNow = function(hotels, trackingA, trackingB) {
        try {
            var rffridPattern = /(rffrid=[a-z]{3}\.[a-z]{4}\.[a-z]{2}\.[0-9]{3}\.[0-9]{3}\.[0-9]{2})\.([0-9]{3})\.([0-9A-Za-z]{3})\.([0-9]{3})/;
