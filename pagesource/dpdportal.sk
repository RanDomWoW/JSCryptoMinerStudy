<script type="text/javascript">

	var form;

	// init subscribe formicky
	function initSubscribeForm() {
		form = $('#mc-embedded-subscribe-form-mailchimp');

		// override submit handlera, pre zachovanie browser validacii
		form.submit(function() {
			register(form.attr('method'), form.attr('action'), form.serialize());

			return false;
		});
	}

	// zobrazi success message
	function displaySuccessMessage() {
		$('.message', form).hide();
		form.find('.success-message').show();
	}

	// zobrazi error message
	function displayErrorMessage() {
		$('.message', form).hide();
		form.find('.error-message').show();
	}

	// registration handler
	function register(method, action, data) {
		try {
			$.ajax({
				type: method,
				url: action,
				data: data,
				cache: false,
				dataType: 'json',
				contentType: "application/json; charset=utf-8",
				error: function (err) {
					displayErrorMessage();
				},
				success: function (data) {
					if (data.result && data.result === 'success') {
						displaySuccessMessage();
					}
					else {
						displayErrorMessage();
					}
				}
			});
		}
		catch (ex) {
			displayErrorMessage()
		}
	}

	$(document).ready(function() {
		initSubscribeForm();
	});


</script>                </div>
