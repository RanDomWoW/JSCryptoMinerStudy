<script type="text/javascript">
    $(document).ready(function() {

        var cod = $("input[name='is_cod']");
        var target = $("input[name='cod_amount']").parent().parent().parent();
        cod.click(function () {
            if (cod.prop('checked'))
            {
                target.removeClass('hidden');
            } else {
                target.addClass('hidden');
            }
        });
        if (cod.prop('checked')) {
            target.removeClass('hidden');
        }


        $("select[name='id_country_to']").change(function () {

            var countries = { "703" : "EUR" };

            if (countries[$(this).val()])
            {
                target.find(".input-group-addon").html(countries[$(this).val()]);
                target.show();
                cod.parent().parent().show();
            } else {
                target.hide();
                cod.prop('checked', false);
                cod.parent().parent().hide();
            }
        });
    });
</script>


