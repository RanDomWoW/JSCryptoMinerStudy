                                            <script type="text/javascript">
$(function(){
	getValChk();

	$('#deitailSearch_h a').click(function(){
		$os = $('#optionSearch_h');

		if($os.hasClass('active')){
			$(this).text('詳細検索');
		} else {
			$(this).text('閉じる');
		}
		$(this).toggleClass('active');
		$os.toggleClass('active');
	});

	$('#optionSearch_h .priceBox').bind('keyup change',function(){
		var $that = $(this);
		charCode($that);
	});

	$('#optionSearch input[name="contact_no"]').bind('keyup change',function(){
		var $that = $(this);
		charCode($that);
	});
});

function getValChk(){
	var url = location.href,
		parameters = url.split('?');
	
	if(parameters[1] != undefined){
		var params = parameters[1].split('&');
		var paramsArray = [];
		for(i = 0; i < params.length; i++){
			var neet = params[i].split('=');
			paramsArray.push(neet[0]);
			paramsArray[neet[0]] = neet[1];
		}

		if(paramsArray['price_l'] || paramsArray['price_h'] || paramsArray['contact_no']){
			$('#deitailSearch a').addClass('active').text('閉じる');
			$('#optionSearch').addClass('active');
		}
	}
}

function charCode(that){
	var text = $(that).val();
	var hen = text.replace(/[Ａ-Ｚａ-ｚ０-９]/g,function(s){
		return String.fromCharCode(s.charCodeAt(0)-0xFEE0);
	});
	$(that).val(hen);
}
</script>

