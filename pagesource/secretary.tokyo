<script>
  $(function() {
    var snapper = new Snap({
      element: document.getElementById('body'), 
      dragger: document.getElementById('body'), 
      navLeft: document.getElementById('snap-nav-left'),
      navRight: document.getElementById('snap-nav-right'),
    
      
      transitionSpeed: 0.3, 
      easing: 'ease', 
      maxPosition: 300, 
      hyperextensible: false,
      minPosition: -300, 
      tapToClose: true, 
      touchToDrag: true 
    });

    var addEvent = function addEvent(element, eventName, func) {
      if (element.addEventListener) {
        return element.addEventListener(eventName, func, false);
      } else if (element.attachEvent) {
        return element.attachEvent("on" + eventName, func);
      }
    };

    addEvent(document.getElementById('snap-open-left'), 'click', function(){
      snapper.open('left');
    });
    addEvent(document.getElementById('snap-open-right'), 'click', function(){
      snapper.open('right');
    });
  });
</script>

