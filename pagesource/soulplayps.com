    /* simple number format plugin, not need to wait for dom to load */
    $.fn.number_format = function(){ 
        return this.each(function(){ 
            $(this).text( $(this).text().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,") ); 
        })
    }

    /* Let the dom load before doing any jquery stuff
    ** most of the time this stuff aint needed unless dom is ready 
    */
    jQuery(function ($) {

        /* flexy menu */
	    $(".flexy-menu").flexymenu({speed: 400});

        /* Which nav item to open and highlight for hiscores */
        if ($('#menu-hiscores').length) {

            var current_url = window.location.href,
                star_icon   = '<i class="fa fa-star"></i> ';

            $('#menu-hiscores li a').each(function() {
                var href = $(this).attr('href');

                if(href == current_url || current_url.indexOf(href.replace('https://www.soulplayps.com/', '')) > -1) {
                    $(this).prepend(star_icon).parent().addClass('active').siblings().removeClass('active');
                }
            });

        }

        /* When clicking on the button close or the mask layer the popup closed */
		$('a.close, #mask').on('click', function() {
			$('#mask , .login-popup').fadeOut(300, function() {
				$('#mask').remove();
			});
			return false;
		});

        /* jQuery activation and setting options for first tabs*/
        $("#tabbed-nav").zozoTabs({
            position: "top-left",
            multiline: true,
            style: "clean",
            theme: "flat-sun-flower",
            spaced: true,
            rounded: true,
            bordered: true,
            dark: true,
            animation: {
                easing: "easeInOutExpo",
                duration: 400,
                effects: "slideH"
            }
        });

        /* carousel */
		var owl = $('.owl-carousel');
		owl.owlCarousel({
			items:4,
			loop:true,
			margin:10,
			autoplay:true,
			autoplayTimeout:5000,
			autoplayHoverPause:true
		});
		$('.play').on('click',function(){
			owl.trigger('autoplay.play.owl',[5000])
		});
		$('.stop').on('click',function(){
			owl.trigger('autoplay.stop.owl')
		});
		
		/* carousel */
		var owl = $('.owl-carousel2');
		owl.owlCarousel({
			items:1,
