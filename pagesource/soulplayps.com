var currentTime = new Date();
var endDate = new Date(currentTime.getTime() + remainingTimeMillisecond);

$('#clock').countdown(endDate, function(event) {
  var $this = $(this).html(event.strftime(''
    + '<span>%d</span> days '
    + '<span>%H</span> hr '
    + '<span>%M</span> min '
    + '<span>%S</span> sec'));
});
