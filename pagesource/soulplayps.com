	    $().UItoTop({ easingType: 'easeOutQuart' });


        /* Animated progress bar with current / max display */
        $('.progress-bar-container').each(function () {
 
            var $container         = $(this),
                $step_container    = $container.find('.bar-steps'),
                $percent_container = $container.find('.bar-percent'),
                max_step           = Math.ceil($step_container.data('steps')),
                max_percent        = $percent_container.data('percent');

            $percent_container.animate({width: max_percent + '%'}, {
                duration: 2000,
                queue: false,
                easing:'linear'
            }).prop({count_step: 0}).animate({count_step: max_step}, {
                duration: 2000,
                queue: false,
                easing:'linear',
                step: function(now) {
                    $step_container.find('span').text(Math.ceil(now)).number_format();
                }
            });

        });

