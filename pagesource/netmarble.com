						speed: 1000, 
						auto: false,
						pause:4000, 
						mode: 'fade',
						randomStart:'boolean'
					});
				}, error : function(e) {
					location.href="/500";
				}
			
			});
			
			$.ajax({
				url : "/api/banner/list/ko",
				type : "GET",
				async : true,
				contentType : "application/json" ,
				success : function(result) {
					$("#tmpl_official_banner").tmpl( result ).appendTo("#bxSlider1");
					$('#bxSlider1').bxSlider({
						speed: 1000, 
						auto: true,
						pause:4000
					});
