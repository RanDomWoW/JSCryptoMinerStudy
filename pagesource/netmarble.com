	<script type="text/javascript">
	
		$(document).ready(function() {
			$.ajax({
				url : "/api/main/contents/ko",
				type : "GET",
				async : true,
				contentType : "application/json" ,
				success : function(result) {
					$("#tmpl_official_main_bg").tmpl( result.mainList ).appendTo("#bxSlider0");
					$("#tmpl_official_game").tmpl( result.gameList.slice(0,2) ).appendTo("#gameList");
