					location.href="/500";
				}
			});
			
			
			$.ajax({
				url : '/api/articles/3991/1',
				type : 'GET',
				async : true,
				contentType : 'application/json',
				success : function(result) {
					var bestArticles = result.bestArticles;
					var articles = result.articleExtendedResponse.articles;
					var bestCnt = 0;
					var bestSeqList = [];
					var deleteSeqList = []; 
					var z = 0;
					if(bestArticles != null){
						$.each(bestArticles, function(i, barticle) {
							barticle.regDateString = dateConvert("ko", barticle.regDate);
							bestCnt = bestCnt + 1;
							bestSeqList[i] = barticle.seq;
						});
					}					
					$.each(articles, function(i, article) {						
						article.regDateString = dateConvert("ko", article.regDate);
						$.each(bestSeqList, function(k, bestSeq) {
							if(article.seq == bestSeq){
								deleteSeqList[z] =i;
								z = z +1;
							}
						});
					});
					deleteSeqList.sort();
					deleteSeqList.reverse();
					if(deleteSeqList.length > 0){
						$.each(deleteSeqList, function(i, deleteSeq) {
							articles.splice(deleteSeq,1);
						});
					}										
					if(bestCnt > 0){
						$('#tmpl_official_news').tmpl( bestArticles.splice(0, bestCnt) ).appendTo('#newsList');
					}					
					$('#tmpl_official_news').tmpl( articles.splice(0, 5-bestCnt) ).appendTo('#newsList');
					$('.nws_list ul li .list').ellipsis({ lines : 1});
				}, error : function(e) {
					location.href="/500";
				}
				
			});
		});
		
		
	
	</script>

