    $.ajax({
        type: "POST",
        url: "/code/activate",
        data: data,
        beforeSend: function() {
            //
        },
        success: function(data) {
            var obj = $.parseJSON(data);
        
            if (obj.error == 0) {
                $('#codeBody').html('<br /><br />Код успешно активирован<br /><br />');
            } else {
                $('#codeFormError').html(obj.message);
                $('#codeFormError').show();
            }
        },
        error: function() {
            //
        },
    });
}
