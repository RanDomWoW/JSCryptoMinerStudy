          $("#thumbCarousel-1016878").on("click", ".owl-item", function(e){
              e.preventDefault();
              var number = $(this).find('.item').data('id');
              sync1.trigger("to.owl.carousel", [number, 200, true]);
          });


          sync1.on("changed.owl.carousel", function (event){
            var current = event.item.index+1;
