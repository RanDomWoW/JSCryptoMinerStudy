    <p>Inscrivez-vous aux courriels de la Sépaq et soyez le premier à connaître nos nouveautés, nos offres et nos promotions&nbsp;spéciales.</p>
    <form method="post" action="/dotCMS/abonnement/courriels/ajax" class="subscribe-form clearfix w-66">
      <p>
        <label for="subscribeEmail" class="form-label is-visuallyhidden">Courriel</label>
        <input id="subscribeEmail" name="subscribeEmail" type="email" value="" class="form-input bg-white" required autocomplete="email" placeholder="Courriel">
        <button class="bouton is-action">S'inscrire</button>
      </p>
      <input type="hidden" name="dispatch" value="subscribe" />
      <input type="hidden" name="communications" value="1" />
      <input type="hidden" name="communications" value="3" />
    </form>
  </div>
</section>
  
<script>
function initSubscribeForm(form, text, subtext, confirmButtonText) {
  form.on("submit", function(){
    jQuery.ajax({
      data: jQuery(this).serialize(),
      type: jQuery(this).attr("method"),
      url: jQuery(this).attr("action"),
      beforeSend: function(){
        showLoading();
      },
      success: function(data, textStatus, jqXHR) {
        hideLoading();
        if(jqXHR.status == 200){
          customAlert({
              text: text,
              subtext: subtext,
              confirmButtonText: confirmButtonText,
              onConfirm: function() {
                jQuery(".form-input", form).val("");
              }
          });
        } else if(jqXHR.status == 400){
          customAlert({
              text: "Erreur ",
              subtext: "Votre adresse de courriel n'est pas valide ",
              confirmButtonText: confirmButtonText,
              onConfirm: function() {
                jQuery(".form-input", form).val("");
              }
          });
        }
        _gaq.push(['_trackEvent', 'Acquisition', 'Inscription', 'Footer']);
      },
      error: function(){
        hideLoading();
        customAlert({
          text: "Erreur ",   
          subtext: "Une erreur système est survenue. Veuillez&nbsp;réessayer. ",  
          confirmButtonText: confirmButtonText,
          onConfirm: function() {
            jQuery(".form-input", form).val("");
          }
        });
      }
    });
    
    return false;
  });
}

jQuery(function() {
  initSubscribeForm(jQuery(".subscribe-form"), "Merci!", "Vous êtes maintenant abonné au bulletin La Sépaq vous informe et aux infolettres de la&nbsp;Sépaq.", "Ok");
});
</script>
      
      
  
  
