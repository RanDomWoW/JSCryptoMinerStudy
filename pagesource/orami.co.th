<script>
    jQuery(document).ready(function(){
        if (jQuery(window).width() < 575) {
            jQuery('#tglr').click(function(event) {
                window.location.href='https://www.orami.co.th/th/checkout/cart/';
            });
        } else {
            jQuery('#tglr').popover({
                html : true,
                content: function() {
                    var div_id =  "tmp-id-" + jQuery.now();
                    return details_in_popup(div_id);
                }
            });
        }
    });

    function details_in_popup(div_id){
        jQuery.ajax({
            url: "https://www.orami.co.th/th/improvement/cart/getcart/",
            type: "POST",
            success: function (data) {
                var cartData = JSON.parse(data);
                var arr = Object.keys(cartData).map(function (key) { return cartData[key]; });
                if (arr.length > 0) {
                    var string = "<div class='pop-item-section'>";
                     for(var i = 0; i < arr.length; i++){
                        string +=  "<div class='clearfix pop-item'>" + "<div class='pop-img'>" + "<img" + " "  + "src='/media/catalog/product" + arr[i]['image'] + "'/></div><div class='pop-content'>"  + arr[i]['name']  +"</br><b>"
                        +arr[i]['price'] + "</b> X <b>" +arr[i]['quantity'] + "</b> ชิ้น" +"</div></div>";
                        total = "<b class='pop-cart-total'>ทั้งหมด " + arr[i]['total'] + "</b>" ;
                    }
                    string += "</div>";
                    jQuery('#' + div_id).html(string+total);
                }else {
                    jQuery('#' + div_id).html("Your Cart Is Empty");
                }

            },
            error: function (data) {
                alert('Some Problem Happen. Cannot View Cart');
            }
        });
        return '<div id="' + div_id + '" class="pop-cart-content">Loading...</div>'
            + '<div class="pop-cart-footer">'
                + '<a href="https://www.orami.co.th/th/checkout/cart/" class="pop-cart-btn secondary funnel_viewcart">View Cart</a>'
                + '<a href="https://www.orami.co.th/th/checkout/onepage/" class="pop-cart-btn funnel_checkout">Checkout</a>'
            + '</div>';
    }

    jQuery('body').on('click', function (e) {
        jQuery('[data-toggle="popover"]').each(function () {
            if (!jQuery(this).is(e.target) && jQuery(this).has(e.target).length === 0 && jQuery('.popover').has(e.target).length === 0) {
                jQuery(this).popover('hide');
            }
        });
    });
</script>
