function set_banner(no,arr){
    var v=$.cookie(cookie_prefix+no)
    if (v==null){
        v=-1;
    }
    v++;
    if (v>=arr.length){
        v=0;
    }
    $.cookie(cookie_prefix+no,v);
    if(arr[v][0]=='flash'){
        $(banner_id_prefix+no).flashembed({src:arr[v][1], width:arr[v][2], height:arr[v][3], wmode:'opaque'});
    }
    else{
        $(banner_id_prefix+no).html('<a class="banner_link" href="'+arr[v][2]+'" target="_blank"><img class="banner_img" src="'+arr[v][1]+'" alt="'+arr[v][3]+'" title="'+arr[v][3]+'"></a>')
    }

}

