function generate_numbers() {
    $('.numbig').each(function () {
        var number = $(this).html().replace(/^\s+|\s+$/g, '');
        $(this).html('');
        number = '' + number;
        number = number.split('');
        for (var i = 0; i < number.length; i++) {
            if (number[i] == ',') {
                $(this).append('<div class="num-comma"></div>');
            } else {
                $(this).append('<div class="num-' + number[i] + '"></div>');
            }
        }
    });
}

