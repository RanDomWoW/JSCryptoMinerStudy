var Activity = {

  start: function () {
    var self = this;

    this.interval = setInterval(function() {
	    var activityFeed = $('#indexActivityFeed');
            var activityId = activityFeed.find('.actionRow:first').attr('id');
            var actOldId = activityId.replace('actId_','');

            var activityData = {
             'activityId': activityId
            }

            $.ajax('/loadindexactivityfeed.php', {
             type: 'POST',
             data: {
                'activityData': JSON.stringify(activityData)
             },
             success: function(jsonRequest) {
                var mediaData = JSON.parse(jsonRequest);
                if(mediaData.result=='success') {
			activityFeed.prepend(mediaData.data);
                        activityFeed.find('.actionRow').filter(function() {
                          return parseInt( this.id.replace('actId_',''), 10) > actOldId;
                        }).fadeIn(700);
			activityFeed.find('.actionRow .actionDetails .timeago').timeago();
			activityFeed.find('.actionRow:last').remove();
                }
             },
             error: function() {

             }
            });
        }, 20000);
  },

  pause: function () {
    clearInterval(this.interval);
    delete this.interval;
    $('#activityState .pause').text('Jatka');
    $('#activityStatus').text('Fiidi on pysäytetty');
    $('#activityState .pause').attr('class', 'resume');
  },

  resume: function () {
    $('#activityState .resume').text('Pysäytä');
    $('#activityStatus').text('');
    $('#activityState .resume').attr('class', 'pause');
    if (!this.interval) this.start();
  }
};

Activity.start();

$('#activityState button').on('click', function() {
 if($('#activityState button').hasClass('pause')) {
  Activity.pause();
 }
 else {
  Activity.resume();
 }
