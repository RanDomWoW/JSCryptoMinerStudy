				var blog_position=parseInt((Math.abs(parseInt($('.blogs-mod-list').css('right')))+1)/blogBoxWidth);
				var blog_width=parseInt($('.blogs-mod-list').children().length)/6*blogBoxWidth-blogBoxWidth-1;
				$('.blogs-mod-scroller').removeClass('disabled');
				if( $(this).hasClass('left') ){
					blog_position++;
					if( blog_position*blogBoxWidth>=blog_width ) $(this).addClass('disabled');
					$('.blogs-mod-list').animate({'right':-blog_position*blogBoxWidth},'slow');
				}else{
					blog_position--;
					if(blog_position<=0) $(this).addClass('disabled');
					$('.blogs-mod-list').animate({'right':-blog_position*blogBoxWidth},'slow');
				}
			}
			return false;
		});
