					}
				});

				$("tr.stock").each(function() {

					var id = this.id;

					eval("var value = hq_str_" + id + " || null;");

					if(value)

					{

						var data = value.split(',');

						$(this).find("td").each(function(index) {

							switch(index)

							{

								case 0:
									$(this).html(data[0]);
									break;

								case 1:
									$(this).html(data[2]);
									break;

								case 2:
									$(this).html(data[3]);
									break;

								case 3:

									var current = parseFloat(data[2]),
										start = parseFloat(data[3]);

									if(current > 0 && start > 0)

									{

										var change = ((start - current) * 100 / current).toFixed(2);

										if(change > 0)

										{

											$(this).html('<span class="red">' + change + '%</span>');

										} else if(change == 0)

										{

											$(this).html('<span class="blue">' + change + '%</span>');

										} else

										{

											$(this).html('<span class="green">' + change + '%</span>');

										}

									}

									break;

							}

						});

					}

				});

