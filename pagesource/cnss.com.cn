					var name = strs[1];

					$.getJSON('/caches/task/exponent/' + name + '/day.json?v=' + new Date().getTime(), null, function(json)

						{

							var change = parseInt(json.change_count);

							var id = "#exponent_" + name;

							if(change > 0)

							{

								$(id).removeClass().addClass("red");

							} else if(change == 0)

							{

								$(id).removeClass().addClass("blue");

							} else

							{

								$(id).removeClass().addClass("green");

							}

							$(id + " b").html(json.index);

						});

				});

