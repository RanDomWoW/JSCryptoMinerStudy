					function searchport() {

						var flag = 1;

						var portval = $('#port').val();

						if(portval == '') {

							alert('请填写查询港口...');
							$('#port').focus();
							return false;

						} else {

							for(var i in availableTags)

							{

								if(new RegExp("^" + portval + "").test(availableTags[i].value)) {

									$('#portid').val(availableTags[i].id);

									flag = 0;

									break;

								}

							}

							if(flag) {

								alert("抱歉，您查询的港口“" + portval + "”暂无信息...");

								portval = NULL;

								return false;

							}

						}

					}
