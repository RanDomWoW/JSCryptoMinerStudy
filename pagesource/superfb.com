    	</li> \
        ';
        $('#twitter .twitterboxlist').append(htmlText);

    });
    if (jsonTweets.pageCount > 1) {
        $('#twitter .tbnav span').show(); 
        if (jsonTweets.currentPage == 1) {
            $('#twitter .tbnav span.left').hide();
        }
        if (jsonTweets.currentPage == jsonTweets.pageCount) {
            $('#twitter .tbnav span.right').hide();
        }
    } else {
        $('#twitter .tbnav span').hide();
    }
}
