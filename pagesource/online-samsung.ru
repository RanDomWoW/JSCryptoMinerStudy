      var productItemsCart = [];
      
      // Изменение кол-ва товаров в корзине (увеличение/уменьшение)
      jQ('.form-item .spinner input').on(
        'change',function(){

          let thisProductSku = jQ(this).parents('.line-item-row').find('.views-field-title a + div').text();
          var thisProductCount = jQ(this).val();
          for(let i=0; i < Object.keys(productItemsCart).length; i++){

            // Если артикулы совпали
            let productSku = productItemsCart[i].productSku;
            if(thisProductSku === productSku){

              // Добавили в корзину
              if(thisProductCount > productItemsCart[i].productCount){
                // Считаем сколько товаров положили в корзину
                let productNewCount = thisProductCount - productItemsCart[i].productCount;
                productItemsCart[i].productCount = Number(productItemsCart[i].productCount) + Number(productNewCount);

                let productItem = {
                  'id': productItemsCart[i].productId,
                  'price': productItemsCart[i].productPrice,
                  'count': productNewCount
                };
                ra_flocktory_add_product(productItem);
              }

              // Удалили из корзины
              if(thisProductCount < productItemsCart[i].productCount){
                // Считаем сколько товаров удалили из корзины
                let productNewCount = Number(productItemsCart[i].productCount) - Number(thisProductCount);
                productItemsCart[i].productCount = productItemsCart[i].productCount - productNewCount;

                let productItem = {
                  'id': productItemsCart[i].productId,
                  'price': productItemsCart[i].productPrice,
                  'count': productNewCount
                };
                ra_flocky_remove_product(productItem);
