							type : "post",
							dataType : "html",
							url : parametros.ajaxurl,
							data : { action:act, userdata:m80_res, back_url:parametros.back_url, post_id:parametros.post_id },
							success : function(html) {
								jQuery('#iuu-comentarios').append(html)
							}
						});
					}

