	<script>
		/***** Popup coupon *****/
		if (Clipboard.isSupported()) {
			var client = new Clipboard("#popup-wrap .copyButtonJS");

			client.on( "success", function(event) {
				event.clearSelection();
				$("#popup-wrap .copyButtonJS").addClass('copied');
				window.setTimeout(function() {
					$("#popup-wrap .copyButtonJS").removeClass('copied');
				}, 1000);
			});

			client.on( "error", function(event) {
				event.clearSelection();
				event.trigger.textContent = 'Ошибка';
				window.setTimeout(function() {
					event.trigger.textContent = 'Копировать';
				}, 1000);
			});
		} else {
			$(".copyButtonJS").hide();
		}

		if($('.coupon-popup-modal').length) {
			$('#couponModal').eq(0).appendTo($('#popup-wrap')).modal('show');
            var expectDate = $('#defaultCountdown').data('expect');
            if (expectDate) {
                var untilDate = new Date(Date.parse(expectDate));
                $('#popup-wrap #defaultCountdown').countdown({until: untilDate, expiryUrl: '/', format: 'dHMS', timezone: +3});
            }
		}

		$(function () {
			$(document).on("click", "a.popup", function() {
				var couponId = $(this).data("id");
				var couponParam = $(this).closest('div.coupon').data("get");
				var clearURL  = window.location.href.split('#')[0];

				$(this).attr("href", clearURL.split('?')[0] + '?' + couponParam + '=' + couponId);

				$.post('/coupons/' + couponId, function(response) {
					window.location = response.gotolink;
				}, 'JSON');

			});
            $('.offers_rating').first().rating({
                fx: 'full',
                image: '/themes/default/img/stars_20.png',
                stars: 5,
                url: '/api/rating-offers/'+$('.offers_rating').first().data("id"),
                callback: function(responce){
                    this.vote_success.delay(3000).slideUp('slow');
                }
            });
		});
	</script>

	
