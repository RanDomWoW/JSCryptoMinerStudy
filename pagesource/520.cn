        }
    
    </script>
    <script type="text/javascript">
        $(function () {
            var d = new Date();
            var y = d.getFullYear();
            var m = d.getMonth() + 1;
            var dd = d.getDate();
            var h = d.getHours();
            var mm = d.getMinutes();
            var sec = d.getSeconds();
            var curDateStr = y + "/" + m + "/" + dd + " " + h + ":" + mm + ":" + sec;
            if (Date.parse(curDateStr) >= Date.parse("2016/2/6 12:00:00") && Date.parse(curDateStr) <= Date.parse("2016/2/14 9:00:00")) {
                $.messageBox({ timeout: 20000, title: '公告：15秒后自动关闭……', msg: '<img src="http://resource.520.cn/img/Home/homeIndex/2016chunjiefangjia.jpg" alt="春节放假公告"  />'
                })
            }
        })

        //判断 年-月-日，大小  
        function compareDate(DateOne, DateTwo) {
            var str = "-";

            var OneMonth = DateOne.substring(5, DateOne.lastIndexOf(str));
            var OneDay = DateOne.substring(DateOne.length, DateOne.lastIndexOf(str) + 1);
            var OneYear = DateOne.substring(0, DateOne.indexOf(str));

            var TwoMonth = DateTwo.substring(5, DateTwo.lastIndexOf(str));
            var TwoDay = DateTwo.substring(DateTwo.length, DateTwo.lastIndexOf(str) + 1);
            var TwoYear = DateTwo.substring(0, DateTwo.indexOf(str));

            if (Date.parse(OneMonth + "/" + OneDay + "/" + OneYear) >
        Date.parse(TwoMonth + "/" + TwoDay + "/" + TwoYear)) {
                return true;
            }
            else {
                return false;
            }

        }  
    </script>
    <script type="text/javascript">
        function NextBanner() {
            var sliderTrigger = $(".slider-panel a.BannerON");
            var index = sliderTrigger.index();
            var nextIndex = 0;

            //默认值空，向右滑
            if (arguments.length == 0 || arguments[0])
                nextIndex = index == 7 ? 0 : index + 1;
            else
                nextIndex = index == 0 ? 7 : index - 1;

            sliderTrigger.removeClass("BannerON");
            $(".slider-panel a").eq(nextIndex).addClass("BannerON");
            $(".slider-panel").css({ "top": "-" + nextIndex * 480 + "px" });

            $(".slider-trigger span").removeClass("act-slider").eq(nextIndex).addClass("act-slider");
        }
    </script>
