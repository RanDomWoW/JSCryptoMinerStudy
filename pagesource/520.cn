            );

            $(".slider-trigger span").mouseover(function(){
                var sliderTrigger = $(".slider-panel a.BannerON");

                var index = $(this).index();
                sliderTrigger.removeClass("BannerON");
                $(".slider-panel a").eq(index).addClass("BannerON");
                $(".slider-panel").css({ "top": "-" + index * 480 + "px" });
                
                $(".slider-trigger span").removeClass("act-slider").eq(index).addClass("act-slider");
            });

            $(".extend .extend-left").click(function(){
                NextBanner(false);
            });
            $(".extend .extend-right").click(function(){
                NextBanner();
            });
