									sh.el.css({'-webkit-transform':'translate(0,0)','-webkit-transition':'all 0.25s ease-in-out'});
									setTimeout(
										function() {
											console.log(even.target.nodeName);
											if(even.target.nodeName=='IMG') {
												var linkel = $(even.target).parent();
											} else if (even.target.nodeName=='A') {
												var linkel = $(even.target);
											}
											location.href = linkel.attr('href');
											sh.el.css({'-webkit-transition':'none'});
											deltaX=0;
										},
										275
									);
								}
								sh.el.off(
									'touchend',
									swipeend
								);
							}
						);
					}
				}
			);
		}

	}

	sh.compute = function() {
		var sliderPicHeight = Math.ceil( ((87.5 / 100) * sw)/1.777777777777778);
		if (sw < 769) {
			var diff = (sw < 480) ? 63 : 94;
			sh.el.css( 'height', sliderPicHeight);
			sh.arrowNext.css('top',((sliderPicHeight-80)/2)+diff);
			sh.arrowPrev.css('top',((sliderPicHeight-80)/2)+diff);
		}
	}

	sh.goToNext = function() {
		sh.el.addClass('goLeft');
		setTimeout(
          function() {
            $.each(
              sh.slides,
              function(n,v) {
                var el = $(v),
                pos = (sh.is_firefox) ? parseInt(el.attr('class').replace('slide','').replace('ff','').replace('s','')) : parseInt(el.attr('class').replace('slide s',''));
                el.removeClass('s'+pos);
                pos = (pos==1) ? sh.slidescount : pos-1;

                el.addClass('s'+pos);
              }
            );
            sh.el.removeClass('goLeft');
          },550
        );
	}

    sh.goToPrev = function() {
		sh.el.addClass('goRight');
		setTimeout(
			function() {
				$.each(
					sh.slides,
					function(n,v) {
						var el = $(v),
						pos = (sh.is_firefox) ? parseInt(el.attr('class').replace('slide','').replace('ff','').replace('s','')) : parseInt(el.attr('class').replace('slide s',''));
						el.removeClass('s'+pos);
						pos = (pos==sh.slidescount) ? 1 : pos+1;
						el.addClass('s'+pos);
					}
				);
				sh.el.removeClass('goRight');
			},550
		);
    }
}

function hpscroll() {

	var hs = this;

	hs.ts 		= $('#ts');
	hs.bs 		= $('#bs');
	hs.pb 		= $('#pb');
	hs.ft 		= $('#footer');
	hs.tadv 	= $('#gpt-300250-container');
	hs.badv 	= $('#movehere');
	hs.isDown 	= false;

	hs.init = function() {
		hs.sw  		= document.documentElement.clientWidth;
		hs.sh  		= document.documentElement.clientHeight;
		hs.tso 		= hs.ts.offset();
		hs.tso 		= hs.tso.top;
		hs.bso 		= hs.bs.offset();
		hs.bso 		= hs.bso.top;
		hs.tadvh 	= hs.tadv.height();
		hs.badvo	= hs.badv.offset();
		hs.badvo	= hs.badvo.top;
		hs.pbo 		= hs.pb.offset();
		hs.pbo 		= hs.pbo.top;
		hs.fto 		= hs.ft.offset();
		hs.fto 		= hs.fto.top;
		hs.limit 	= hs.pbo;
		hs.botLimit = hs.fto;
		hs.vpos 	= $(window).scrollTop()

		$(window).on('scroll',function() { hs.react(); });
	}

	hs.react = function() {
		var vpos_din  	= $(window).scrollTop(),
		tso_din 		= hs.ts.offset(),
		tso_din 		= tso_din.top,
		badvo_din 		= hs.badv.offset(),
		badvo_din 		= badvo_din.top,
		diff 			= (hs.sw>1539) ? 90 : 30,
		dir 			= (hs.vpos<vpos_din) ? 25 : 25,
		pushing 		= (isPushing) ? $('.flying-container').height() : 0;
		hs.tsh 			= hs.ts.height();
		if (!hs.isDown) {
			if ((vpos_din-hs.tso) >= (dir*-1)) {
				hs.ts.css('top',(vpos_din-hs.tso)+dir+pushing);
				if ((vpos_din+hs.tsh+dir+54)>=hs.limit-pushing) {
           			hs.ts.css('top',hs.limit-hs.tso-hs.tsh-54);
		        }
			} else {
				hs.ts.css('top',0);
			}
			if (tso_din+hs.tsh<vpos_din) {
				hs.badv.html('');
				hs.tadv.contents().appendTo('#movehere');
				hs.tadv.attr('id','movehere');
				hs.badv.attr('id','gpt-300250-container');
				if (typeof googletag === "undefined") {} else {
					googletag.cmd.push(googletag.pubads().refresh([gpt_300x250]));
					//console.debug("...push sotto!");
				}
				hs.tadv.html('<div style="width:300px;height:'+hs.tadvh+'px;"></div>');
				hs.isDown = true;
			}

		} else {
			hs.badvh = hs.badv.height();

			if ((vpos_din-hs.badvo) >= (dir*-1)) {
				hs.badv.css({'position':'fixed','top':dir+pushing});
				if ((vpos_din+hs.badvh)>=hs.botLimit-diff) {
					hs.badv.css({'position':'fixed','top':hs.botLimit-(vpos_din+hs.badvh)-diff});
				}
			} else {
				hs.badv.css({'position':'','top':''});
				if (vpos_din+hs.sh < hs.bso) {
					hs.tadv.html('');
					hs.badv.contents().appendTo(hs.tadv);
					hs.badv.attr('id','movehere');
					hs.tadv.attr('id','gpt-300250-container');
					if (typeof googletag === "undefined") {} else {
						googletag.cmd.push(googletag.pubads().refresh([gpt_300x250]));
						//console.debug("...push sopra!");
					}
					hs.badv.html('<div style="width:300px;height:'+hs.tadvh+'px;"></div>');
					hs.isDown = false;
				}
			}
		}

		hs.vpos=vpos_din;
	}

}

