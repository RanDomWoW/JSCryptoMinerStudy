function stickyTopBoxMobile() {
	var me = this;

	me.init = function(target,top) {
		console.debug('[stickyTopBoxMobile] init');
		me.target = document.querySelector(target);
		if(me.target == null) return false;
		me.targetJq = $(target);

		me.top = top;

		if (typeof device === 'undefined') {
			me.device = sw>900&&!isTouch?"desktop":900>sw&&sw>670||sw>900&&isTouch?"tablet":"smartphone";
		}  else {
			me.device = device;
		}

		if(me.device!='smartphone') return false;

		//me.wrapStickyTopBox = $(document.createElement('div')).addClass('wrapStickyTopBox')
		//me.stickyTopBox = $(document.createElement('div')).addClass('stickyTopBox');

		//me.targetJq.wrap(me.stickyTopBox);

		me.stickyTopBox = $('.stickyTopBox');
		//me.stickyTopBox.wrap(me.wrapStickyTopBox);

		//me.stickyTopBox.append($(document.createElement('div')).addClass('stickyBoxFooter'));
		me.stickyBoxFooter = $('.stickyBoxFooter');

		//me.stickyBoxFooter.append($(document.createElement('span')).addClass('stickyBoxCounter'));
		me.stickyBoxCounter = $('.stickyBoxCounter');

		//me.stickyBoxFooter.append($(document.createElement('a')).addClass('stickyBoxClose'));
		me.stickyBoxClose = $('.stickyBoxClose');

		me.wrapStickyTopBox = $('.wrapStickyTopBox');
		me.processed = new Array();

		me.checkPosition();
		me.bindClose();
		$(window).scroll(function() {
				me.checkPosition();
			})
		}

	me.checkPosition = function() {
		me.index = 0;

		me.stickyTopBox.each(function() {
			var _t = this;

			me.advHeight = $(_t).children().height();
			$(_t).parent().css('min-height',me.advHeight+'px');

			if($(window).height() <= me.advHeight) return false; //se lo schermo è meno alto dell'adv non facciamo niente

			if(me.inPosition(_t.parentNode) && me.processed[me.index] != 1 && $(_t).height()!=0) {
				me.processed[me.index] = 1;
				$(_t).addClass('inTop');
				$(_t).height(me.advHeight);
				me.sticky = 1;

				me.stickyBoxCounterVal = 3;
				me.stickyBoxCounter.text(me.stickyBoxCounterVal);
				me.intervalCounter = setInterval(function() {
					me.stickyBoxCounterVal = me.stickyBoxCounterVal-1;
					me.stickyBoxCounter.text(me.stickyBoxCounterVal);
					if(me.stickyBoxCounterVal <= 0) clearInterval(me.intervalCounter);
				},1000)

				me.timeout = setTimeout(function() {
					me.sticky = 0;
					$(_t).addClass('stickyFade');
					setTimeout(function() {
						$(_t).height('');
						$(_t).parent().css('min-height','');
						$(_t).removeClass('inTop stickyFade');
					},1000);
				},3100);
			} else if(me.sticky == 0) {
				$(_t).removeClass('inTop');
				$(_t).height('');
				$(_t).parent().css('min-height','');
			}
		})
	}

	me.bindClose = function() {
		$(document).on('click','.stickyBoxClose',function() {
			var _t = $(this);
			var target = _t.parents('.stickyTopBox').eq(0);
			target.addClass('stickyFade');
			setTimeout(function() {
				target.height('');
				target.parent().css('min-height','');
				target.removeClass('inTop stickyFade');
			},1000);
		})
	}

	me.inPosition = function(element) {
		var rect = element.getBoundingClientRect();
		return (
			rect.top <= me.top
		);
	};
}

