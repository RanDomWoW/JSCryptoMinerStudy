$(window).load(function(){
	var Count = 9;
	var FirstArr = shuffleRandom(10)
	,SecondArr = shuffleRandom(10)
	,ThirdArr = shuffleRandom(10)
	,FourthArr = shuffleRandom(10)
	,FirstNo = 0
	,SecondNo = 0
	,ThirdNo = 0
	,FourthNo = 0
	,TimeOut = 0
	,shuffleExit;
	function autoStep(){
		TimeOut += 100;
		if(TimeOut <= 2000){
			$(".counter span:first").html(FirstArr[FirstNo]);
			if(FirstNo > (Count-1)) FirstNo = 0;
			else FirstNo++;
		}else{
			$(".counter span:first").html(2);
		}

		if(TimeOut <= 1500){
			$(".counter span:eq(2)").html(SecondArr[SecondNo]);
			if(SecondNo > (Count-1)) SecondNo = 0;
			else SecondNo++;
		}else{
			$(".counter span:eq(2)").html(1);
		}

		if(TimeOut <= 1000){
			$(".counter span:eq(3)").html(ThirdArr[ThirdNo]);
			if(ThirdNo > (Count-1)) ThirdNo = 0;
			else ThirdNo++;
		}else{
			$(".counter span:eq(3)").html(6);
		}

		if(TimeOut <= 500){
			$(".counter span:last").html(FourthArr[FourthNo]);
			if(FourthNo > (Count-1)) FourthNo = 0;
			else FourthNo++;
		}else{
			$(".counter span:last").html(5);
		}

		if(TimeOut > 2501) clearTimeout(shuffleExit)
    }

    shuffleExit = window.setInterval(function () {

		autoStep();

	 }, 50);

	autoStep();
});
					</script>


