    <script>
        var getDatePickerPropertiesObj = function (target) {
            // target = objecto do jquery - enviado quando se chama a função ( ex: $("#txtName") | $(".txtName") | $(this) )
            var datePickerProps = new Object();
            datePickerProps["changeMonth"] = true;
            datePickerProps["changeYear"] = true;
            datePickerProps["yearRange"] = target.attr('dpmin') + ":" + target.attr('dpmax');
            datePickerProps["minDate"] = new Date(target.attr('dpmin'), 0, 1);
            datePickerProps["maxDate"] = new Date(target.attr('dpmax'), 11, 31);

            //START - LANGUAGE
            datePickerProps["monthNames"] = ['Janeiro', 'Fevereiro', 'Março', 'Abril', 'Maio', 'Junho', 'Julho', 'Agosto', 'Setembro', 'Outubro', 'Novembro', 'Dezembro'];
            datePickerProps["monthNamesShort"] = ['Jan', 'Fev', 'Mar', 'Abr', 'Mai', 'Jun', 'Jul', 'Ago', 'Set', 'Out', 'Nov', 'Dez'];
            datePickerProps["dayNames"] = ['Domingo', 'Segunda-feira', 'Terça-feira', 'Quarta-feira', 'Quinta-feira', 'Sexta-feira', 'Sábado'];
            datePickerProps["dayNamesShort"] = ['Dom', 'Seg', 'Ter', 'Qua', 'Qui', 'Sex', 'Sáb'];
            datePickerProps["dayNamesMin"] = ['D', 'S', 'T', 'Q', 'Q', 'S', 'S'];
            datePickerProps["dateFormat"] = 'dd-mm-yy'
            datePickerProps["firstDay"] = 0
            //datePickerProps["renderer"] =  target.defaultRenderer;
            datePickerProps["prevText"] = '&lt;Anterior';
            datePickerProps["prevStatus"] = 'Mês anterior';
            datePickerProps["prevJumpText"] = '&lt;&lt;';
            datePickerProps["prevJumpStatus"] = 'Ano anterior';
            datePickerProps["nextText"] = 'Próximo&gt;';
            datePickerProps["nextStatus"] = 'Próximo mês';
            datePickerProps["nextJumpText"] = '&gt;&gt;';
            datePickerProps["nextJumpStatus"] = 'Próximo ano';
            datePickerProps["currentText"] = 'Atual';
            datePickerProps["currentStatus"] = 'Mês atual';
            datePickerProps["todayText"] = 'Hoje';
            datePickerProps["todayStatus"] = 'Hoje';
            datePickerProps["clearText"] = 'Limpar';
            datePickerProps["clearStatus"] = 'Limpar data';
            datePickerProps["closeText"] = 'Fechar';
            datePickerProps["closeStatus"] = 'Fechar o calendário';
            datePickerProps["yearStatus"] = 'Selecionar ano';
            datePickerProps["monthStatus"] = 'Selecionar mês';
            datePickerProps["weekText"] = 's';
            datePickerProps["weekStatus"] = 'Semana do ano';
            datePickerProps["dayStatus"] = 'DD, d \'de\' M \'de\' yyyy';
            datePickerProps["defaultStatus"] = 'Selecione um dia';
            datePickerProps["isRTL"] = false
            //END LANGUAGE
            return datePickerProps;
        }

        function _setDate(objID, d) {
            document.getElementById(objID).value = d;
        }
        $(document).ready(function () {

            $(".datepicker").each(function () {
                $(this).datepicker(getDatePickerPropertiesObj($(this)));
            });
            $(".datepickerFirst").each(function () {
                var datePickerProperties = getDatePickerPropertiesObj($(this));
                datePickerProperties["beforeShowDay"] = dayOneOnly
                $(this).datepicker(datePickerProperties);
            });
            $(".datepickerLast").each(function () {
                var datePickerProperties = getDatePickerPropertiesObj($(this));
                datePickerProperties["beforeShowDay"] = lastDayOnly
                $(this).datepicker(datePickerProperties);
            });
			
			// Rc.2015.06.18
            // para resolver um bug do firefox com as dropdown do calendário dentro das modal do bootstrap
            $(".modal").each(function () {
                $(this).removeAttr("tabindex");
            });
        });
        function dayOneOnly(date) {
            dmy = date.getDate() + "-" + (date.getMonth() + 1) + "-" + date.getFullYear();
            if (date.getDate() == "1") {
                return [true, "", dmy];
            } else {
                return [false, "", ""];
            }
        }
        function lastDayOnly(date) {
            dmy = date.getDate() + "-" + (date.getMonth() + 1) + "-" + date.getFullYear();
            if (date.getDate() == new Date(date.getFullYear(), date.getMonth() + 1, 0).getDate()) {
                return [true, "", dmy];
            } else {
                return [false, "", ""];
            }
        }

    </script>

