      if ( $form.length > 0 ) {

       $form.find('#subscribe').bind('click', function ( event ) {
         
        if ( event ) event.preventDefault();
        function validate_input(){

         var testEmail = /^[A-Z0-9._%+-]+@([A-Z0-9-]+\.)+[A-Z]{2,4}$/i;
         var get_email = $form.find('#mail').val();
         if(get_email != ''){
