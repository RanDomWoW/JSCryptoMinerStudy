  $('.login_custom input[type=email]').keypress(function(e){
      if(e.keyCode === 13){
            e.preventDefault(); 
            $('.login_custom .btn-login-submit').trigger('click');       
    }  
  });
  
  $('.login_custom .btn-login-submit').click(function(e){
      e.preventDefault();

      var email_id =  $('#custom_email').val();
      if(email_id !=''){
        $.ajax({
          type:"POST",
          url: "https://s.wlfpt.co/customlogin/get_customer_detail",
          data : { shop: 'ghost', customer_email: email_id},
          crossDomain: true,
          success:function(customer){
            if(customer.status != 'enabled'){
              $('#registerLoginForm').show();
              $('.notice').show();
              $('.active_note span').html("We've sent you an email to confirm and set your new password.");
            } else {
              $('.btn-login-submit').hide();
              $('.login_cont').text("Please enter your password to sign into your account");
              $('.notice').hide();
              $('.passoword_field').show();
              $('.passoword_field #CustomerPassword').focus();
              $('#CustomerLoginForm  input[type="email"]').attr('value',email_id);
              $('#CustomerLoginForm').show();
            }
          },
          error: function(data){
            console.log(data);
            $('.notice').hide();
            $('#registerLoginForm  input[type="email"]').attr('value',email_id);
            $('#registerLoginForm').show();
            $('.notice').show();
            $('#registerLoginForm .active_note span').html('Hello There! Looks like you’re new to us. Please continue signing up if you would like to create an account.');
            $('#horizontalTab').responsiveTabs('activate', 1);
          }
        });
      }
  });   
  
    
