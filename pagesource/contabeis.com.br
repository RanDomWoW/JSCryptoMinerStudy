			_destinoAtual.find('input').focus(function(){ _destinoAtual.removeClass('dica'); });
		}

        function getCookie(name) {
           var nameEQ = name + "=";
           var ca = document.cookie.split(';');
           for(var i=0;i < ca.length;i++) {
              var c = ca[i];
              while (c.charAt(0)==' ') c = c.substring(1);
              if (c.indexOf(nameEQ) != -1) return c.substring(nameEQ.length,c.length);
           }
           return null;
        }

		$(window).scroll(function () {
			var limite = $(document).height() - $(window).height();
			var _janelaDT = $(".faixaLoginPadrao.scroll");
			if (!_janelaDT.hasClass("clicado") && getCookie("janela" + _janelaDT.attr("janela"))!="fechou") {
				if ($(this).scrollTop() > 200 && $(this).scrollTop() < limite) { _janelaDT.delay(100).addClass('aberto'); } else { _janelaDT.delay(100).removeClass('aberto');}
			}
		});

