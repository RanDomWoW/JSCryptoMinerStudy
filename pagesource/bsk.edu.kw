        function ClientDateValidationFunction(sender, args) {
            //check for update or insert
            //0 = insert form
            //1 = edit form
            var index = 0; 
            var senderId = $(sender).attr('id');
            index = 1;

            var allDayEvent = $('.advancedAllDayChk')[index].children[0].checked
            var start = $find($('.rsAdvStartDate')[index].children[0].id);
            var end = $find($('.rsAdvEndDate')[index].children[0].id);
            var startTime = $find($('.rsAdvStartTime')[index].children[0].id);
            var endTime = $find($('.rsAdvEndTime')[index].children[0].id);

            var starenddatetTimeDate = new Date();
            startTimeDate = startTime.get_selectedDate();

            var endTimeDate = new Date();
            endTimeDate = endTime.get_selectedDate();
            var date1 = new Date();
            date1 = start.get_selectedDate();
            date1.setHours(startTimeDate.getHours());
            date1.setMinutes(startTimeDate.getMinutes());
            var date2 = new Date();
            date2 = end.get_selectedDate();
            date2.setHours(endTimeDate.getHours())
            date2.setMinutes(endTimeDate.getMinutes())

            if (allDayEvent && date1 > date2) {
                args.IsValid = false;
            }

            if (date1 >= date2 && !allDayEvent) {
                args.IsValid = false;
            }
        } 
                          

        function ResponseEnd() {
            //hide the loading panel and clean up the global variables
            if (currentLoadingPanel != null)
                currentLoadingPanel.hide(currentUpdatedControl);
            currentUpdatedControl = null;
            currentLoadingPanel = null;
        }

        function applyTooltips(){
            $.each($(".eventLink"), function(index, anchor){ 
                 var categoryStr = '';
                 var subCategoryStr = '';
                 var aptCatArr = $(this).parent().find('div:nth-child(4) span').text().split(',');
                 var aptSubCatArr = $(this).parent().find('div:nth-child(5) span').text().split(',');

                 
                    $.each(aptCatArr, function(index, value) { 
                        categoryStr += jsonObject(aptCategories,parseInt(value));
                        if(index != (aptCatArr.length -1))
                        categoryStr += ',';
                    });

                    if($.trim(categoryStr) == '')
                        categoryStr = 'n/a';

                    $.each(aptSubCatArr, function(index, value) { 
                        subCategoryStr += jsonObject(aptSubCategories,parseInt(value));
                        if(index != (aptSubCatArr.length -1))
                        subCategoryStr += ',';
                    });

                        if($.trim(subCategoryStr) == '')
                        subCategoryStr = 'n/a';

                 $(this).parent().find('div:nth-child(4) span').text(categoryStr)
                 $(this).parent().find('div:nth-child(5) span').text(subCategoryStr)

                /// TODO: replace ids with names

                /// decode div into title 
                $(anchor).attr("title", htmlDecode($(this).parent().find(".eventDesc").html())); 
            
            });
        }
