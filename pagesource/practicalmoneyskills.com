<script>
$(function () {
	$('#state-standards-levels').change(function(){
		if ($(this).val()!='HighSchool') {
			$('#state-standards-state').show();
		} else {
			$('#state-standards-state').hide();
			$('#state-standards-state').next().hide();
			$('#state-standards-state option:selected').removeAttr('selected');
		}
	})

	$('#state-standards-tabs a').click(function(e){
		e.preventDefault();
		$(this).addClass('active').siblings().removeClass('active');
		var num = $(this).index();
		$('.state-standards-highschool').eq(num).show().siblings('.state-standards-highschool').hide();

	})

	$('.nominee').click(function(){
		var val = $(this).val();
		if (val=='someoneelse') {
			$('#someoneelse_fullname').show();
		} else {
			$('#someoneelse_fullname').hide().val('');
		}
	})

	$('.card a').click(function(e){
		e.stopPropagation()
	})
})
</script>

