<script>
    $(function() {
        var $loader = $('#ajaxloader');
        var $selectablePanel = $('.selectable-panel:not(.panel-disabled)');
        var panelActiveClass = 'panel-success';

        $loader.hide();

        $selectablePanel.on('click', function(event) {
            var $this = $(this);

            if ($(event.target).prop('className').indexOf('vehicle-body-types') > -1
                    || $(event.target).prop('className').indexOf('vehicle-body-type') > -1
            ) {
                // prevent event to interact with popover
                event.preventDefault();
                event.stopPropagation();
                return false;
            }

            $this.addClass(panelActiveClass);

            $selectablePanel.not($this).removeClass(panelActiveClass);

            $loader.show();

            location.href = $this.data('href');
        });

        window.getDevice = function() {
            var userAgent = navigator.userAgent || navigator.vendor || window.opera;
            var detected = null;

            if (/windows phone/i.test(userAgent)) {
                detected = "Windows Phone";
            }

            if (/android/i.test(userAgent)) {
                detected = "Android";
            }

            if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {
                detected = "iOS";
            }

            return detected;
        };

        if ('iOS' === window.getDevice()) {
            $(window).bind("pageshow", function(event) {
                if (event.originalEvent.persisted) {
                    $loader.hide();
                    location.href=location.href;
                }
            });
        }
    });
</script>
