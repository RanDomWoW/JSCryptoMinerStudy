<script>
    $(function() {
        var mq = window.matchMedia("(max-width: 767px)");

        var initial = false;
        var $panels = $('.landing-start-benefits .panel');

        var panelCollapse = function() {

            if (true === mq.matches) {
                $panels.each(function() {
                    var $this = $(this);
                    var ownHeight = $this.outerHeight();
                    var headerHeight = $this.find('.panel-heading').outerHeight();
                    var opened = false;

                    $this.data('own-height', ownHeight);

                    if (false === opened) {
                        $this.css({
                            height: headerHeight,
                            overflow: 'hidden',
                            cursor: 'pointer'
                        });
                    }

                    $this.off('click');
                    $this.on('click', function(event) {
                        $panels.not($this).css({
                            height: headerHeight,
                            cursor: 'pointer'
                        });

                        initial = true;
                        opened = true;

                        $this.css({
                            height: ownHeight,
                            cursor: 'auto'
                        });

                        if (true === initial) {
                            if ($(window).scrollTop() >= 185) {
                                setTimeout(function() {
                                    $('html, body').animate({
                                        scrollTop: $this.offset().top - 10
                                    }, 256);
                                }, 512);
                            }
                        }
                    });
                });

                if (false === initial) {
                    $panels.first().find('.panel-heading').trigger('click');
                }
            } else {

            }
        };

//        panelCollapse();
    });
</script>

