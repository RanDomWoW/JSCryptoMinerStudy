                            <script>
                                $(function() {
                                    var $expandButton = $('#header-navbar-collapse');
                                    var $collapser = $('.page-header .navbar-collapse');
                                    var $dropdowns = $('.page-header .navbar-collapse .dropdown');
                                    var dropdownBackButtonHeight = 100;
                                    var initialHeight = 0;

                                    var resetCollapserHeight = function() {
                                        setTimeout(function() {
                                            $collapser.css({
                                                height: 'auto'
                                            });
                                        }, 128);
                                    };

                                    $expandButton.on('shown.bs.collapse', function() {
                                        initialHeight = $collapser.outerHeight(true);

                                        $collapser.css({
                                            height: initialHeight
                                        });
                                    });

                                    $dropdowns.on('click', function() {
                                        var $dropdown = $(this);
                                        var $dropdownMenu = $dropdown.find('.dropdown-menu > li');

                                        var dropdownMenuOuterHeight = $dropdownMenu.outerHeight(true);

                                        $collapser.css({
                                            height: initialHeight
                                        });

                                        if ($dropdownMenu.length > 0) {
                                            var reset = false;
                                            var dropdownMenuHeight = dropdownMenuOuterHeight * $dropdownMenu.length + dropdownBackButtonHeight;

                                            if (0 === dropdownMenuHeight || initialHeight >= dropdownMenuHeight) {
                                                dropdownMenuHeight = initialHeight;
                                                reset = true;
                                            }
                                        }

                                        $collapser.css({
                                            height: dropdownMenuHeight
                                        });

                                        if (true === reset) {
                                            resetCollapserHeight();
                                        }
                                    });

                                    $dropdowns.on('hidden.bs.dropdown', function() {
                                        $collapser.css({
                                            height: initialHeight
                                        });

                                        resetCollapserHeight();
                                    });
                                });
                            </script>
