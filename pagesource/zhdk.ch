            <script type="text/html" id="comp-sub-form-template">
                <form method='POST' action='' id="{{id}}" class="form custom-form-validation l-center-s">
            
                    <div class="confirmation visuallyhidden">
                        <h3>
                            {{confirmationTitle}}
                        </h3>
                        {{{confirmationText}}}
                    </div>
                    <div class="transmissionError visuallyhidden">
                        <h3>
                            {{transmissionErrorTitle}}
                        </h3>
                        {{{transmissionErrorText}}}
                    </div>
            
                    <input type="hidden" name="recipient" value="{{recipient}}"/>
                    <input type="hidden" name="subject" value="{{emailSubject}}"/>
                    <input type="hidden" name="form_checksum" value="{{form_checksum}}"/>
                    <input type="hidden" name="content_uid" value="{{content_uid}}"/>
                    <div class="visuallyhidden" aria-hidden="true">
                        <label for="humanValidation">3+13*5</label>
                        <input type="text" id="humanValidation" name="humanValidation"/>
                    </div>
            
                    <fieldset id='group-{{id}}' class="form_group form-group-outer">
                        <h2 class="form_title">{{title}}</h2>
            
                        <div class="mandatoryMissingMessage" style="display: none;">
                            <ul class="done filled input_error_list" id="input_u2_error" nocomments="">
                                <li class="input_error" role="alert">{{mandatoryMissingMessage}}</li>
                            </ul>
                        </div>
                        <div class="fieldErrorMessageTemplate" style="display: none;">{{fieldErrorMessageTemplate}}</div>
                        <div class="form_alert form_alert-error" style="display: none;">
                            <h2 class="form_alert--heading">{{errorTextTitle}}</h2>
                            <div class="form_alert--list--content">
                                <ul class="form_alert--list"></ul>
                            </div>
                        </div>
            
            
                        {{#each properties as |property propertyId|}}
            
                        <fieldset id='{{propertyId}}' class='form_row'>
            
            
                            {{#renderModeInList options.renderMode 'textfield,textarea'}}
                                <label nocomments="true" {{#if options.hideLabel}}class="visuallyhidden"{{/if}}for="{{propertyId}}">{{title}}{{#if options.required}}<abbr title="Required">&nbsp;*</abbr>{{/if}}</label>
                                <div class="form_input form_input-text">
                            {{/renderModeInList}}
            
                            {{#renderModeInList options.renderMode 'checkbox,radio'}}
                                <legend nocomments="true" {{#if options.hideLabel}}class="visuallyhidden"{{/if}}for="{{propertyId}}" class="form_label">{{title}}{{#if options.required}}<abbr title="Required">&nbsp;*</abbr>{{/if}}</legend>
                                <ul class="{{#if options.inline}}as-inline {{/if}}form_inputgroup form_inputgroup-{{options.renderMode}}">
                            {{/renderModeInList}}
            
            
            
            
            
                            {{#renderModeInList options.renderMode 'textfield'}}
                                    <input type='{{#if options.inputtype}}{{options.inputtype}}{{else}}text{{/if}}' name='{{propertyId}}' id='{{propertyId}}' {{#if options.required}} required{{/if}}{{#each options.attributes}} {{@key}}="{{this}}"{{/each}} />
                            {{/renderModeInList}}
            
                            {{#renderModeInList options.renderMode 'textarea'}}
                                    <textarea name='{{propertyId}}' id='{{propertyId}}' {{#if options.required}} required{{/if}}{{#each options.attributes}} {{@key}}="{{this}}"{{/each}}></textarea>
                            {{/renderModeInList}}
            
                            {{#renderModeInList options.renderMode 'radio'}}
                                {{#each options.items as |item itemId|}}
                                    <li class="form_{{../options.renderMode}}"{{#each item.options.wrapperAttributes}} {{@key}}="{{this}}"{{/each}}>
                                        <input type="{{../options.renderMode}}" id="{{item.id}}" name="{{propertyId}}" value="{{item.label}}"{{#if ../options.required}} required{{/if}}{{#each item.options.attributes}} {{@key}}="{{this}}"{{/each}} />
                                        <label for="{{item.id}}">{{item.label}}</label>
                                    </li>
                                {{/each}}
                            {{/renderModeInList}}
            
                            {{#renderModeInList options.renderMode 'checkbox'}}
                                {{#each options.items as |item itemId|}}
                                    <li class="form_{{../options.renderMode}} {{item.id}}"{{#each item.options.wrapperAttributes}} {{@key}}="{{this}}"{{/each}}>
                                        <input type="{{../options.renderMode}}" id="{{item.id}}" name="{{propertyId}}" value="{{item.label}}"{{#if item.options.required}} required{{/if}}{{#each item.options.attributes}} {{@key}}="{{this}}"{{/each}} />
                                        <label for="{{item.id}}">{{item.label}}{{#if item.options.required}}<abbr title="Required">&nbsp;*</abbr>{{/if}}</label>
                                    </li>
                                {{/each}}
                            {{/renderModeInList}}
            
                            {{#renderModeInList options.renderMode 'button'}}
                                <ul class="form_buttons">
                                    {{#each options.items as |item itemId|}}
                                        <li{{#each item.options.wrapperAttributes}} {{itemId}}="{{this}}"{{/each}}>
                                            <button id="{{item.id}}" class="button form-save-button"{{#each item.options.attributes}} {{@key}}="{{this}}"{{/each}}>{{item.label}}</button>
                                        </li>
                                    {{/each}}
                                </ul>
                            {{/renderModeInList}}
            
                            {{#renderModeInList options.renderMode 'infotext'}}
                                <div class="form_text">
                                    {{{title}}}
                                </div>
                            {{/renderModeInList}}
            
            
            
            
            
                            {{#renderModeInList options.renderMode 'checkbox,radio'}}
                                </ul>
                            {{/renderModeInList}}
            
                            {{#renderModeInList options.renderMode 'textfield,textarea'}}
                                </div>
                            {{/renderModeInList}}
            
                        </fieldset>
            
                        {{/each}}
                    </fieldset>
            
                </form>
            </script>
