            var playing = false;
        } else {
            var playing = true;
        }
    
    $('#video-overlay').click(function(){
        
        $(this).toggleClass("down");
    
          if (playing == false) {
              $('#vidControl').toggleClass('fa fa-play').toggleClass('fa fa-pause');
              document.getElementById('bgvid').play();
              playing = true;
          } else {
            $('#vidControl').toggleClass('fa fa-play').toggleClass('fa fa-pause');
            document.getElementById('bgvid').pause();
            playing = false;
          }
    });
});
