    <script type="text/javascript">
    $(document).ready(function () {
        $('#subscribing').click(function () {
            var email = $('#subscriber').val();
            var emailPattern = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;

            if (email != '') {
                if (emailPattern.test(email)) {
                    $('#subscribing').hide();
                    $.post(SITE_URL + '/ajax/save_subscriber', {'email': email}, function (rst) {
                        if (rst == 'done') {
                            $("#ui_dialog").html('<p>Thank you! You are registered for newsletters successfully.</p>').dialog({modal: true, draggable: false, minHeight: 30, resizable: false, show: 'fast', title: 'Subscription Successful'});
                            $('#subscriber').val('');
                        } else if (rst == 'exists') {
                            $("#ui_dialog").html('<p>You are already registered for newsletters.</p>').dialog({modal: true, draggable: false, minHeight: 30, resizable: false, show: false, title: 'Subscription Error'});
                        } else if (rst == 'error') {
                            $("#ui_dialog").html('<p>An error occured. Please try again.</p>').dialog({modal: true, draggable: false, minHeight: 30, resizable: false, show: false, title: 'Subscription Error'});
                        } else if (rst == 'invalid_email') {
							
                            $("#ui_dialog").html('<p>You have entered an invalid email address. Please enter a genuine email address.</p>').dialog({modal: true, draggable: false, minHeight: 30, resizable: false, show: false, title: 'Subscription Error'});
                        }
                        $('#subscribing').show();
                    });
                } else {
                    $("#ui_dialog").attr('title', 'Subscription Error').html('<p>You have entered an invalid email address. Please enter a genuine email address.</p>').dialog({modal: true, draggable: false, minHeight: 30, resizable: false, show: false, title: 'Subscription Error'});
                }
            }
        });
    });
    </script>
