						   response_div.html("<p>Email format is Invalid </p>");
							break;
						case 2 :
							 
							response_div.html("<p>Sorry ! Email is already in use.</p>");
							break;
						case 3 :
							response_div.html("Congratulation, you have successfully register with cantalop.com</p>.<p> You are being redirected to the thank you page.");
							break;
						case 4 :
						   response_div.html("Something is going wrong with creating account Please try again.");
							break;
						case 5 : 
							response_div.html("Something is going wrong with creating account Please try again.");
						   
							break;
						case 6 :
							response_div.html("We are sorry! But, your account is disabled. Please contact administrator.");
							break;
							default:
							response_div.html("Please Wait ! your account is creation in process.");  

                    } 
                    
					
					var register_success_array = ['3'];
					if($.inArray(register_flag , register_success_array)){ 
						$('#register_seeker_btn').show(5);
						$('#register_seeker_loader').hide(5);
						open_cv_step_popup(fname,lname,1);
						//window.location=SITE_URL + '/users/success/';
					}else{
						
						// Open directly via API
						$.magnificPopup.open({
							items: {
								src: '#signin_signup_response_dialog', // can be a HTML string, jQuery object, or CSS selector
								type: 'inline',
								fixedContentPos: false,
								fixedBgPos: true,
								overflowY: 'auto',
								closeBtnInside: true,
								preloader: false,
								midClick: true,
								removalDelay: 300,
								mainClass: 'my-mfp-slide-bottom',
							}
						});
						
					}
                },
				error: function (xhr, ajaxOptions, thrownError) {
					//var err = eval("(" + xhr.responseText + ")");
					//alert(err.Message);
					alert('There is some error,Please try again.'); location.reload();
				}
            });
        }
    }
	
	function open_cv_step_popup(fname,lname,cv_step){
		var cv_step =  parseInt($.trim(cv_step));
		$('.step_li_info-'+cv_step).addClass('select');
		if(cv_step>=4 && cv_step<=8){
			$('.step_span_info-'+cv_step).addClass('active');
		}
		
		$.magnificPopup.instance.st.callbacks = {
			close: function() {
				$('body').removeClass('module_open');
				
				$("form#registrationSeeker2").trigger('reset'); //jquery
				$("form#login").trigger('reset'); //jquery
				$("form#forgot_password").trigger('reset'); //jquery
				
				$("form#general_info").trigger('reset'); //jquery
				$("form#experience_info").trigger('reset'); //jquery
				$("form#education_info").trigger('reset'); //jquery
				$("form#function_preferences_info").trigger('reset'); //jquery
				$("form#location_preferences_info").trigger('reset'); //jquery
				$("form#target_jobtitle_info").trigger('reset'); //jquery
				$("form#industry_preferences_info").trigger('reset'); //jquery
				$("form#career_level_info").trigger('reset'); //jquery
				$("form#invite_info").trigger('reset'); //jquery
				$("form#invite_info_individual").trigger('reset'); //jquery
				
				
				// Remove validaion error
				$('#day').css('border-color','#ff9a00');
				$('#month').css('border-color','#ff9a00');
				$('#year').css('border-color','#ff9a00');
				
				$('.cv_popup_radio').each(function() {
					if($(this).is(':checked')){
						var thisId = $(this).attr('id');
						if(thisId=='gender1' || thisId=='gender2' || thisId=='marital_status1' || thisId=='marital_status2'){
							$('#'+thisId).attr('checked', false);
							var imageUrl = $('#'+thisId).closest('.styledRadio').css('background-image');
							var imageUrl = imageUrl.replace("radio-select.png", "radio.png"); 
							$('#'+thisId).closest('.styledRadio').css('background-image', imageUrl);
						}
					}
				});
				$("input[name='gender']").closest('.styledRadio').css({"border": "0px solid red"}).removeClass('checked');
				$("input[name='marital_status']").closest('.styledRadio').css({"border": "0px solid red"}).removeClass('checked');
				$('#nationality').css('border-color','#ff9a00');
				$('#country_residence').css('border-color','#ff9a00');
				
				$('#job_title').css('border-color','#ff9a00');
				$('#employer').css('border-color','#ff9a00');
				$('#function_id').css('border-color','#ff9a00');
				$('#industry_id').css('border-color','#ff9a00');
				$('#from_month').css('border-color','#ff9a00');
				$('#from_year').css('border-color','#ff9a00');
				$('#to_month').css('border-color','#ff9a00');
				$('#to_year').css('border-color','#ff9a00');
				$('#country_id').css('border-color','#ff9a00');
				
				if($("#currently_work_here").is(':checked')){
					$('#currently_work_here').closest('.styledCheckbox').removeClass('checked');
					$('#currently_work_here').attr('checked', false); // Unchecks it
					$('#to_date_box').show();
					$('#to_present_box').hide();
				}
				
				
				$('#degree').css('border-color','#ff9a00');
				$('#school_university').css('border-color','#ff9a00');
				$('#study_field').css('border-color','#ff9a00');
				$('#from_year').css('border-color','#ff9a00');
				$('#to_year').css('border-color','#ff9a00');
				
				$('[name^="jobtitle"]').css('border-color', '#ff9a00' ); 
				$("[name^='search_type_']").closest('.styledRadio').css({"border": "0px solid red"});
			}
		}
		$('body').addClass('module_open');
		$('#cvpopupstep').val(cv_step);
		$('#cvpopupfname').val(fname);
		$('#cvpopuplname').val(lname);
		$('.cv_user_display_name').text(fname+' '+lname)
		$.magnificPopup.open({
			items: {
				src: '#signup-dialog'+cv_step, // can be a HTML string, jQuery object, or CSS selector
				type: 'inline',
				fixedContentPos: false,
				fixedBgPos: true,
				overflowY: 'auto',
				closeBtnInside: true,
				preloader: false,
				midClick: true,
				removalDelay: false,
				mainClass: 'my-mfp-slide-bottom',
				closeOnBgClick:false,
				enableEscapeKey:false,
			}
		});
		
		$('.mfp-wrap').animate({
			scrollTop: $('#signup-dialog'+cv_step).offset().top
		},100);
	}
	
	function search_cv_preference(query,ul_id)
	{   
		jQuery('#'+ul_id+' li').each(function ()
		{
			jQuery(this).find('.onlyvalue').each(function ()
			{
				var elem = jQuery(this);
				if (!elem.text().match(new RegExp('.*?' + query + '.*?', 'i'))) {
					jQuery(this).parents('li.parent_li').css('display', 'none');
					// console.log(query);
				} else {
					jQuery(this).parents('li.parent_li').css('display','');
					return false;
				}
				return ;
			});
		});
	}


