	<script type="text/javascript">
	$(function(){
		$( "select" ).live('change',function() {
			updateProgressBar();
		});
		
		$( "input" ).blur(function() {
			updateProgressBar();
		});
	});
	
	function updateProgressBar(){
		var EmployerName = $.trim($("#EmployerName").val());
		var EmployerIndustryId = $.trim($("#EmployerIndustryId").val());
		var EmployerSize = $.trim($("#EmployerSize").val());
		var EmployerCountry = $.trim($("#EmployerCountry").val());
		var EmployerCity = $.trim($("#EmployerCity").val());
		var EmployerUrl = $.trim($("#EmployerUrl").val());
		var EmployerJobTitle = $.trim($("#EmployerJobTitle").val());
		var EmployerFirstName = $.trim($("#EmployerFirstName").val());
		var EmployerLastName = $.trim($("#EmployerLastName").val());
		var EmployerCode = $.trim($("#EmployerCode").val());
		var EmployerContactNo = $.trim($("#EmployerContactNo").val());
		var EmployerEmailAddress = $.trim($("#EmployerEmailAddress").val());
		var EmployerPassword = $.trim($("#EmployerPassword").val());
		var EmployerConfirmPassword = $.trim($("#EmployerConfirmPassword").val());
		
		var complete_score = 0;
		if(EmployerName!='' && EmployerIndustryId!='' && EmployerSize!='' && EmployerCountry!='' && EmployerCity!='' && EmployerUrl!=''  && EmployerUrl!='http://'){
			complete_score = 50;
		}
		
		if(EmployerJobTitle!='' && EmployerFirstName!='' && EmployerLastName!='' && EmployerCode!='' && EmployerContactNo!='' && EmployerEmailAddress!='' && EmployerPassword!='' && EmployerConfirmPassword!=''){
			if(complete_score == 50){
				complete_score = 100;
			}else{
				complete_score = 50;
			}
		}
		
		if(complete_score==0){
			var score_class = 'empstep1';
		}else if(complete_score==50){
			var score_class = 'empstep2';
		}else{
			var score_class = 'empstep3';
		}
		$('#show_score').text(complete_score+'%');
		$( "div.firstdiv" ).replaceWith( '<div class="'+score_class+' firstdiv"></div>');
	}
</script>
