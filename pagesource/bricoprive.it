        var BricoSearch = {
            ajaxUrl   : '/modules/sensefuel/ajax.search.php',
            version   : 'it',
            v_timeout : false,
            init : function()
            {
                $('#sensefuel #global_search').bind('keyup', function(){
                    BricoSearch.search();
                });
                $('#sensefuel .search_submit').click(function() {
                    BricoSearch.search();
                });
                $(document).delegate('.sensefuelsearch-close', 'click touchstart', function(e) {
                    e.preventDefault();
                    e.stopPropagation();
                    BricoSearch.close();
                });
            },
            close: function()
            {
                $('#global_search').removeClass('active-search');
                if ($('#sensefuelsearchresult').length) {
                    $('#sensefuelsearchresult').hide();
                }
            },
            search : function()
            {
                // clear timeout
                if (typeof(BricoSearch.v_timeout) !== 'undefined' && !!BricoSearch.v_timeout) {
                    clearTimeout(BricoSearch.v_timeout);
                }
                // timeout check inout validity
                $input = $('#sensefuel #global_search');
                BricoSearch.v_timeout = setTimeout(function() {
                    BricoSearch.doSearch($input);
                }, 500);
            },
            doSearch : function(element)
            {
                $.ajax({
                    type:       "POST",
                    url:        BricoSearch.ajaxUrl,
                    cache:      false,
                    dataType:   "json",
                    data : {
                        ajax:   true,
                        method:   'doSearch',
                        search_v: BricoSearch.version,
                        search:   element.val(),
                    },
                    success : function(jsonData)
                    {
                        if (typeof(jsonData.errors) == 'undefined' ||
                            typeof(jsonData.result) == 'undefined' ||
                            jsonData.errors == true) {
                            return false;
                        }
                        if (!$('#sensefuelsearchresult').length) {
                            $('body').append($('<div>').addClass('block-white block-6 block-sm-12 block-push-sm-0 block-push-3').attr('id', 'sensefuelsearchresult'));
                        }

                        $result_header = $('<div>').addClass('text-right block-white block-6 block-sm-12 block-push-sm-0 block-push-3 result-header block-lighter').append(
                            $('<a>').addClass('sensefuelsearch-close icon-close text-dark font-size6 ease-color-highlight-dark')
                        );

                        $result_form = $('#sensefuelsearchresult');
                        if (jsonData.result.length) {
                            $result_list = $('<div>').addClass('result-list row-block text-left');
                            var price = '';
                            var old_price = '';
                            var brand = '';
                            for (var i = 0; i < jsonData.result.length; i++) {
                                $row = jsonData.result[i];
                                if (typeof($row['g:sale_price']) != 'undefined') {
                                    price = parseFloat($row['g:sale_price']);
                                    old_price = '<br/><span class="font-size1" style="text-decoration: line-through;">' + parseFloat($row['g:price']).toFixed(2) + '&nbsp;â¬</span>';
                                } else {
                                    price = parseFloat($row['g:price']);
                                }

                                if (typeof($row['g:brand'] != 'undefined') && !!$row['g:brand']) {
                                    brand = $row['g:brand'];
                                } else {
                                    brand = '&nbsp;';
                                }

                                $result_list.append($('<div>').addClass('result-line').append(
                                    $('<a>').addClass('inner-block block-12 text-dark ease-color-highlight-dark ease-bg-lighter no-margin').attr('href', $row['g:link']).append(
                                        $('<span>').addClass('block-1 block-sm-2 no-padding result-image').append(
                                            $('<img>').attr('src', $row['g:image_link'].replace('-large', '-small')).addClass('img-responsive').css('max-height', '50px')
                                        ),
                                        $('<span>').addClass('block-2 hidden-sm no-padding result-brand').html(brand),
                                        $('<span>').addClass('block-8 block-sm-7 result-title').text($row['g:title']),
                                        $('<span>').addClass('block-1 block-sm-3 text-highlight-dark no-padding  text-right result-price').html(price.toFixed(2) + '&nbsp;â¬' + old_price)
                                    )
                                ));
                            }
                            $result_form.html('').append($result_header, $result_list);
                            $('#global_search').addClass('active-search');
                            $result_form.show();
                        } else {
                            $('#global_search').removeClass('active-search');
                            $result_form.hide();
                        }
                    },
                });
            },
        }
