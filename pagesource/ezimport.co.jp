//header 找賣家
function check_search(){
  var seller = $("#sellerSearch").val();
  var content = $("#yschsp").val();
  if(!content) return ;
  if(seller == 1){
    window.location.href='/user_products.php?id='+content;
  }else{
    $("#sbn").submit();
  }
}
function FormEnterKey(e) {
     var key;
     var seller = $("#sellerSearch").val();
     var content = $("#yschsp").val();
     if(window.event)
          key = window.event.keyCode;
     else
          key = e.which;

    if(key == 13){
      if(content){
        if(seller == 1){
          window.location.href='/user_products.php?id='+content;
        }else{
         $("#sbn").submit();
        }
      }
    }
     return (key!=13);
}

