function init_ad_char(){
  var total_h = $('.ad_char .char').height()+$('.ad_char .content_list').height();
  $('.ad_char').height(total_h);
  $('.ad_char .scroll_list').css('top',-total_h/2).show();//顯示左右箭號
  $('.ad_char .content_list .content_box').css('width',($('.ad_char .content_list .content').size()+1)*$('.ad_char').width());
  $('.ad_char .content_list').scrollLeft(0);
  $('.ad_char .char').each(function(k,v){$(this).attr('k',k+1)});

  $('.ad_char .char').on('mouseenter',function(){
    ad_scroll_char($(this).attr('k'));
  });
  
  $('.ad_char').on('mouseenter',function(){
    var m = parseInt($('.ad_char .char_on').attr('k'));
    var char_size = $('.ad_char .char').size();
    if($(this).attr('class') == 'scroll left') m = m-1;
    if($(this).attr('class') == 'scroll right') m = m+1;
    if(m <= 0) m = char_size;
    if(m > char_size) m = 1;
    ad_scroll_char(m);
  });
}
function ad_scroll_char(n)
{
  var w = $('.ad_char').width();
  var now = $('.ad_char .content_list').scrollLeft();
  var num = $('.ad_char .content_list .content').size();
  var max_w = w*(num-1);
  var go_left = now+w;
  $('.ad_char .content_list').stop();
  if(n)
  {
    clearInterval(run_ad);
    go_left = w*(n-1);
  }
  if(go_left > max_w) go_left = 0;
  var char_num = go_left/w+1;
  $('.ad_char .char').removeClass('char_on');
  $('.ad_char .char[k="'+char_num+'"]').addClass('char_on');
                                                    /*500 捲動速度,愈少愈快*/
  $('.ad_char .content_list').animate({scrollLeft:go_left},60,function(){
  });
}
</script>
