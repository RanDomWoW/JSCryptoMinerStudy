function check_text_load(id){
  if ( $(id).length ) {
       limit_text(id)
  } else {setTimeout(check_text_load,2000)}
}

function limit_text(idt){
$(idt).each(function() {
   if($(this).text().length > 52){
    var newText = ($(this).text()).substr(0, 52);
    $(this).text(newText);      
   } 
 });
}

function check_desc_load(ido){
  if ( $(ido).length ) {
       limit_text_desc(ido)
  } else {setTimeout(check_desc_load,2000)}
}

function limit_text_desc(idoo){
$(idoo).each(function() {
   if($(this).text().length > 100){
    var newDesc = ($(this).text()).substr(0, 100) + '...';
    $(this).text(newDesc);      
   } 
 });
}

