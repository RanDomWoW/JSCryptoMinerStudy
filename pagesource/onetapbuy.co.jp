<script type="text/javascript">
$(function() {
    if (document.cookie.match('AUTOLOGIN')) {
        $('#AUTOLOGIN').attr("checked", true);
    }
    $('#login_submit').click(function() {
        var url = '/login/';
        var params = {
            'MEMBER_ID': $("#MEMBER_ID").val(),
            'PASSWORD': $("#PASSWORD").val(),
            'AUTOLOGIN': $("#AUTOLOGIN:checkbox:checked").val(),
            'UUID': $('#UUID').val()
        };
        $.post(url, params, function(data) {
            if (data.STATUS == false) {
                $("#msg").text(data.MESSAGE_ARRAY);
                return false;
            } else {
                $("#msg").text("");
                location.href = "/trade/";
            }
        })
    });
    $('#login').click(function(e) {
        if(!$(e.target).closest('.transaction_window').length) {
            $('#login').hide();
        }
    });
})
</script>