<script type="text/javascript">
//<![CDATA[

    jQuery(document).ready(function ($) {

        //setup an array of AJAX options, each object is an index that will specify information for a single AJAX request

        /* 
        var ajaxes = [
        { url: 'http://free.worldweatheronline.com/feed/weather.ashx?q=adelaide,australia&format=json&num_of_days=1&key=7e9691d182231652120212', city: 'Adelaide' },
        { url: 'http://free.worldweatheronline.com/feed/weather.ashx?q=Brisbane,australia&format=json&num_of_days=1&key=7e9691d182231652120212', city: 'Brisbane' },
        { url: 'http://free.worldweatheronline.com/feed/weather.ashx?q=Darwin,australia&format=json&num_of_days=1&key=7e9691d182231652120212', city: 'Darwin' },
        { url: 'http://free.worldweatheronline.com/feed/weather.ashx?q=Hobart,australia&format=json&num_of_days=1&key=7e9691d182231652120212', city: 'Hobart' },
        { url: 'http://free.worldweatheronline.com/feed/weather.ashx?q=melbourne,australia&format=json&num_of_days=1&key=7e9691d182231652120212', city: 'Melbourne' },
        { url: 'http://free.worldweatheronline.com/feed/weather.ashx?q=Perth,australia&format=json&num_of_days=1&key=7e9691d182231652120212', city: 'Perth' },
        { url: 'http://free.worldweatheronline.com/feed/weather.ashx?q=sydney,australia&format=json&num_of_days=1&key=7e9691d182231652120212', city: 'Sydney' }
        ],       
        */

        var ajaxes = [
        { url: 'http://api.worldweatheronline.com/free/v1/weather.ashx?q=adelaide,australia&format=json&num_of_days=1&key=tbz2f89fw8cnvp43en355482', city: 'Adelaide' },
        { url: 'http://api.worldweatheronline.com/free/v1/weather.ashx?q=Brisbane,australia&format=json&num_of_days=1&key=tbz2f89fw8cnvp43en355482', city: 'Brisbane' },
        { url: 'http://api.worldweatheronline.com/free/v1/weather.ashx?q=Darwin,australia&format=json&num_of_days=1&key=tbz2f89fw8cnvp43en355482', city: 'Darwin' },
        { url: 'http://api.worldweatheronline.com/free/v1/weather.ashx?q=Hobart,australia&format=json&num_of_days=1&key=tbz2f89fw8cnvp43en355482', city: 'Hobart' },
        { url: 'http://api.worldweatheronline.com/free/v1/weather.ashx?q=melbourne,australia&format=json&num_of_days=1&key=tbz2f89fw8cnvp43en355482', city: 'Melbourne' },
        { url: 'http://api.worldweatheronline.com/free/v1/weather.ashx?q=Perth,australia&format=json&num_of_days=1&key=tbz2f89fw8cnvp43en355482', city: 'Perth' },
        { url: 'http://api.worldweatheronline.com/free/v1/weather.ashx?q=sydney,australia&format=json&num_of_days=1&key=tbz2f89fw8cnvp43en355482', city: 'Sydney' }
        ],
        current = 0;
        var data = { list: [] };

        data.list.splice(0, 1);

        //declare your function to run AJAX requests
        function do_ajax() {

            //check to make sure there are more requests to make
            if (current < ajaxes.length) {

                //make the AJAX request with the given data from the 'ajaxes' array of objects
                $.ajax({
                    url: ajaxes[current].url,
                    dataType: "jsonp",
                    success: function (data_json) {
                        //alert('here');
                        //alert(data_json.data.weather[0].tempMinC);

                        //------------------------------------------------------------------------------------
                        //VALIDATE WEATHER SECTION
                        //------------------------------------------------------------------------------------
                        var strIconDesc = data_json.data.weather[0].weatherDesc[0].value.toLowerCase();

                        if (strIconDesc.indexOf("sunny") != -1) {
                            strIconDesc = "sunny";
                        } else if (strIconDesc.indexOf("drizzle") != -1) {
                            strIconDesc = "shower";
                        } else if (strIconDesc.indexOf("show") != -1) {
                            strIconDesc = "shower";
                        } else if (strIconDesc.indexOf("rain") != -1) {
                            strIconDesc = "rain";
                        } else if (strIconDesc.indexOf("thundery") != -1) {
                            strIconDesc = "thunder";
                        } else if (strIconDesc.indexOf("thunder") != -1) {
                            strIconDesc = "possible-thunder";
                        } else if (strIconDesc.indexOf("fog") != -1) {
                            strIconDesc = "fog";
                        } else if (strIconDesc.indexOf("cloudy") != -1) {
                            strIconDesc = "cloudy";
                        } else if (strIconDesc.indexOf("windy") != -1) {
                            strIconDesc = "windy";
                        } else if (strIconDesc.indexOf("overcast") != -1) {
                            strIconDesc = "windy";
                        } else {
                            strIconDesc = ""
                        }

                        var CurrentTemp = data_json.data.current_condition[0].temp_C;
                        var MaxTemp = data_json.data.weather[0].tempMaxC;

                        if (CurrentTemp > MaxTemp) {
                            MaxTemp = CurrentTemp;
                        }

                        //------------------------------------------------------------------------------------
                        // END VALIDATE WEATHER SECTION
                        //"City": ajaxes[current].city, "Current": data_json.data.current_condition[0].temp_C, "Min": data_json.data.weather[0].tempMinC, "Max": data_json.data.weather[0].tempMaxC, "Forecast": data_json.data.weather[0].weatherDesc[0].value.replace(' in nearby', '').replace('nearby', ''), "Icon": strIconDesc }
                        //------------------------------------------------------------------------------------

                        data.list.push(
                            { "City": ajaxes[current].city, "Current": CurrentTemp, "Min": data_json.data.weather[0].tempMinC, "Max": MaxTemp, "Forecast": data_json.data.weather[0].weatherDesc[0].value.replace(' in nearby', '').replace('nearby', ''), "Icon": strIconDesc }
                        );

                        //increment the 'current' counter and recursively call this function again
                        current++;
                        do_ajax();
                    }
                });
            }
        }

        //assign value to weather class
        $.weather(data);

        //run the AJAX function for the first time once 'document.ready' fires
        do_ajax();
    });

</script>
