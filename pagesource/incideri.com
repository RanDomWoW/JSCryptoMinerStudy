        <li v-if="(this.params.currentPage - 3) > 0 && params.pageCount > 5"><a href="javascript:;">...</a></li>
        <li class="{{ n == params.currentPage ? 'current' : '' }}" v-for="n in numbers" v-if="n > 0" @click="changePage(n)"><a href="javascript:;">{{ n }}</a></li>
        <li v-if="(params.pageCount <= 5 && params.pageCount - params.currentPage) >= 5"><a href="javascript:void(0);">...</a></li>
        <li v-if="(params.pageCount > 5 && (params.pageCount - params.currentPage)) >= 3"><a href="javascript:void(0);">...</a></li>
    </ul>
</div>

<div class="pagination mobile">
	<button @click="changePage(params.currentPage - 1)" class="btn hollow narrow">Önceki</button>
	{{params.currentPage}} / {{ params.pageCount }}
	<button @click="changePage(params.currentPage + 1)" class="btn hollow narrow">Sonraki</button>
</div>


</template>
