<script type="text/javascript">
var slideIdx = 0;
var slideSize = $('#slideshow .slides .content').size();
var dispInterval = 3000;
var dispDuration = 200;
var dispSlide = function(idx){

  var prevIdx = 0;
  $('#slideshow .slide-nav li').each(function(i){
    if ($(this).hasClass('current')){
      prevIdx = i;
    }
  });
  $('#slideshow .slide-nav li').removeClass('current');
  $('#slideshow .slide-nav li').eq(idx).addClass('current');
  $('#slideshow .slides .content').eq(prevIdx).hide('slide', {direction: 'right'}, dispDuration);
  $('#slideshow .slides .content').eq(idx).show('slide', {direction: 'left'}, dispDuration);
};
var getNextSlideIdx = function(idx){
  var nIdx =  idx + 1;
  if ( nIdx > slideSize - 1 ){
    nIdx = 0;
  }
  return nIdx;
} 
var getPrevSlideIdx = function(idx){
  if (idx > 0){
    var pIdx = idx - 1;
  } else {
    var pIdx = slideSize - 1 ;
  }
  return pIdx;
} 
var startSlide = function(idx){
  dispSlide(idx);
  slideIdx = getNextSlideIdx(idx);
};


$(function(){
  slideIdx = 1;
  var autoSlide = setInterval('startSlide(slideIdx)',dispInterval);
  $('#slideshow .slide-nav li').hoverIntent(
    function(){
      var n = $('#slideshow .slide-nav li').index(this);
      slideIdx = n;
      startSlide(slideIdx);
      clearInterval(autoSlide);
    },
    function(){
      autoSlide = setInterval('startSlide(slideIdx)',dispInterval);
    }
  );
  $('#slideshow .slides .content').hoverIntent(
    function(){
      var n = $('#slideshow .slides .content').index(this);
      slideIdx = n;
      clearInterval(autoSlide);
      slideIdx = getNextSlideIdx(n);
    },
    function(){
      autoSlide = setInterval('startSlide(slideIdx)',dispInterval);
    }
  );

});




</script>



