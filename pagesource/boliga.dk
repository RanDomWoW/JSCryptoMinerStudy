<script>
        var StringToInput = "";

        var landsdelsKode = "" || $("input#Search_Area").val();
        var kummuneKode = "" || $("input#Search_Region2").val();
        var postnummer = "" || $("input#Search_InZips").val();
        var postnummerMin = "" || $("input#Search_MinZip").val();
        var postnummerMax = "" || $("input#Search_MaxZip").val();
        var stednavn = "" || $("input#Search_PlaceName").val();
        var vejnavn = "" || $("input#Search_Street").val();




        //LANDSDEL
        if (landsdelsKode != '' && (kummuneKode == '' || kummuneKode == '0') && postnummer == '' && stednavn == '' && vejnavn == '') {
            if (typeof GetRegionName === 'function') {
                SetRigionNameFromCode();
            } else {
                document.getElementById("LandsdeleScript").onload = SetRigionNameFromCode;
            }
            function SetRigionNameFromCode() {
                var landsdelNavn = GetRegionName(landsdelsKode);
                StringToInput += landsdelNavn;
                makeTag("Area", landsdelsKode, StringToInput);
                setName(StringToInput);
            }
        }
        //KOMMUNE
        if (landsdelsKode == '' && kummuneKode != '' && kummuneKode != '0' && postnummer == '' && stednavn == '' && vejnavn == '') {
            if (typeof GetKommuneName === 'function') {
                SetKommuneTextFromModel();
            } else {
                document.getElementById("KommunerScript").onload = SetKommuneTextFromModel;
            }

            function SetKommuneTextFromModel() {
                var kommuneNavn = GetKommuneName(kummuneKode);
                StringToInput += kommuneNavn;
                setName(StringToInput + " kommune");
                makeTag("Region2", kummuneKode, StringToInput + " kommune");
            }
        }
        //BY
        if (landsdelsKode == '' && (kummuneKode == '' || kummuneKode == '0') && postnummer != '' && stednavn == '' && vejnavn == '') {
            var pnArray = postnummer.split(",");
            var indexz = 0;
            var loopArray = function (arr) {
                getZipcodeName(arr[indexz], function (name) {
                    // set x to next item
                    StringToInput = name;
                    makeTag("InZips", arr[indexz], StringToInput)
                    setName(StringToInput);

                    indexz++;
                    if (indexz < arr.length) {
                        loopArray(pnArray);
                    } else {
                        ToggleDropdownState();
                    }
                });
            }
            loopArray(pnArray);
        }

        //STEDNAVN
        if (landsdelsKode == '' && (kummuneKode == '' || kummuneKode == '0') && postnummer != '' && stednavn != '' && vejnavn == '') {
            var postnummernavn = getZipcodeName(postnummer, function (name) {
                StringToInput += stednavn + ", " + name;
                setName(StringToInput);
                makeTag("Region2", postnummer, StringToInput);
            });
        }

        //VEJ
        if (landsdelsKode == '' && (kummuneKode == '' || kummuneKode == '0') && postnummer != '' && stednavn == '' && vejnavn != '') {
            var postnummernavn = getZipcodeName(postnummer, function (name) {
                StringToInput += vejnavn + ", " + name;
                setName(StringToInput);
                makeTag("Street", postnummer, StringToInput);
            });
        }

        function getZipcodeName(postnummer, callback) {
            if (typeof GetCityName === 'function') {
                SetCityName(postnummer);
            } else {
                document.getElementById("PostnumreScript").onload = function () {
                    $(document).ready(function () {
                        SetCityName(postnummer);
                    });
                }
            }

            function SetCityName(zipcode) {
                if (zipcode.length === 4) {
                    return callback(zipcode + " " + GetCityName(zipcode));
                }
                else {
                    var zipArray = zipcode.split(',');
                    var zipsString = '';
                    jQuery.each(zipArray, function (i, val) {

                        if (val.indexOf("-") >= 0) {
                            //var zips = [];
                            var zips = val.split('-');
                            var CityOne = GetCityName(zips[0]);
                            var CityTwo = GetCityName(zips[1]);
                            if (CityOne == CityTwo)
                                zipsString += zips[0] + "-" + zips[1] + " " + CityTwo + ", ";
                            else
                                zipsString += "Imellem " + zips[0] + " " + CityOne + " og " + zips[1] + " " + CityTwo + ", ";
                        }
                        else {
                            zipsString += val + " " + GetCityName(val) + ", ";
                        }

                    });
                    return callback(zipsString.slice(0, -2));
                }
            }

        }

        function setName(StringToInput) {
            $("#area_to_print").text(StringToInput);
        }

    </script>
<script type="text/javascript">

        function foldMenuToggle(id) {
            if ($.isArray(id)) {
                $.each(id, function (i, val) {
                    $("#" + val).toggleClass("hiddeFold");
                });
            } else {
                $("#" + id).toggleClass("hiddeFold");
            }
            $("#" + id).siblings().find(".foldIndicator").toggleClass("rotate");
            return false;
        }

        function hideMenuToggle(id) {
            if ($.isArray(id)) {
                $.each(id, function (i, val) {
                    $("#" + val).addClass("hiddeFold");
                });
            } else {
                $("#" + id).addClass("hiddeFold");
            }
            $("#" + id).siblings().find(".foldIndicator").removeClass("rotate");
            return false;
        }

        function showMenuToggle(id) {
            if ($.isArray(id)) {
                $.each(id, function (i, val) {
                    $("#" + val).removeClass("hiddeFold");
                });
            } else {
                $("#" + id).removeClass("hiddeFold");
            }
            $("#" + id).siblings().find(".foldIndicator").addClass("rotate");
            return false;
        }

        var isSearchMenuHover = false;
        function toggleMainSearchDropdown(id) {
            isSearchMenuHover = true;
            var isHidden = $("#" + id).hasClass("d-none");
            $('[data-js="mainSearchDropdown"]').addClass("d-none");
            if (isHidden) {
                $("#" + id).removeClass("d-none");
            } else {
                $("#" + id).addClass("d-none");
            }
            return false;
        }
        $('[data-js="mainSearchDropdown"]').on("mouseenter", function () {
            isSearchMenuHover = true;
        });
        $('[data-js="mainSearchDropdown"]').on("mouseleave", function () {
            isSearchMenuHover = false;
        });

        $("body").click(function () {
            if (!isSearchMenuHover) {
                $('[data-js="mainSearchDropdown"]').addClass("d-none");
                isSearchMenuHover = false;
            }
        });

        $(document).on('touchstart', function (event) {
            if (!$(event.target).closest('[data-js="mainSearchDropdown"]').length) {
                $('[data-js="mainSearchDropdown"]').addClass("d-none");
                isSearchMenuHover = false;
            }
        });
    </script>


