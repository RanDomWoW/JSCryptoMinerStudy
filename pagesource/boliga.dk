    function positionSkyScraper(reinitialize, wallpaperLoaded) {        
        var $skyLeft = $("#banner-sky-wrapper-left"),            
            $skyRight = $("#banner-sky-wrapper-right-sticky"),            
            initialTopPos = 0       
        
        if ((($skyLeft.length == 1 && $skyLeft.width() == 0) && ($skyRight.length == 1 && $skyRight.width() == 0))) {
            retryCount++;
            if (retryCount < maxRetries) {             
                setTimeout(positionSkyScraper, 50);
            }            
            return;
        }        
        
        if (wallpaperLoaded) {                
            initialTopPos = 1240;            
        }
            
        if ($(window).scrollTop() >= initialTopPos + 180) {
            $skyRight.css({
                position: 'fixed',
                top: 0
            });
            $skyLeft.css({
                position: 'fixed',
                top: 0
            });
        } else {                
            $skyLeft.css({
                position: 'absolute',
                top: initialTopPos
            });
            $skyRight.css({
                position: 'absolute',
                top: initialTopPos
            });
        }             
    }           


    // listen for wallpaper events and adjust offset in that case        
