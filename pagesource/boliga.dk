<script type="text/javascript">
        function makeKommuneHTML(region1val) {
            var kommuneHTML = '';
            if (region1val == '') {
                $.each(municipalities, function (key, value) {
                    kommuneHTML += "<a href='' id='Region2_" + value[0] + "' js-data='region2Selector' data-regionid='" + value[0] + "'><h6>" + value[1] + "</h6></a>";
                });
            }
            else {
                var komunekoder = GetMunicipalitiesByRegionCode(region1val);
                $.each(komunekoder, function (key, value) {
                    kommuneHTML += "<a href='' id='Region2_" + value + "' js-data='region2Selector' data-regionid='" + value + "'><h6>" + GetKommuneName(value) + "</h6></a>";
                });
            }
            $("#redion1Menu").removeClass("show");
            $("#zippcodeMenu").removeClass("show");
            $("#redion2Menu").addClass("show");
            $("#redion2Wrapper").html(kommuneHTML);
        }

        function makeZipcodeHTML(region2val) {
            var postnumreHTML = "";

            if (region2val == '') {
                $("div#zippcodePageing").removeClass("d-none").addClass("d-flex");
                postnumreHTML = GetZipcodeInterval(1000, 2999);
            }
            else {
                $("div#zippcodePageing").addClass("d-none").removeClass("d-flex");
                var postnumreArray = GetZipcodesByKommunekode(region2val);

                $.each(postnumreArray, function (key, value) {
                    var CityName = GetCityName(value) || value;
                    postnumreHTML += makeZipcodeInnerHtml(value, CityName);
                });
            }

            $("#redion1Menu").removeClass("show");
            $("#redion2Menu").removeClass("show");
            $("#zippcodeMenu").addClass("show");
            $("#zippcodeWrapper").html(postnumreHTML);
            var selectedZips = $("#Search_InZips").val();
            $("#zippcodeWrapper").children('[js-data="zipcodeSelector"]').each(function () {
                if (selectedZips.indexOf($(this).attr("data-zipcode")) >= 0) {
                    $(this).addClass("zipCodeSelected");
                }
            });
        }
        function makeZipcodeInterval(from, to) {
            $("#zippcodeWrapper").html(GetZipcodeInterval(from, to));
        }

        function GetZipcodeInterval(from, to) {
            var postnumreHTML = "";
            $.each(postnumre, function (key, value) {
                if (value[0] >= from && value[0] <= to) {
                    postnumreHTML += makeZipcodeInnerHtml(value[0], value[1]);
                }
            });
            return postnumreHTML;
        }

        var downarrowSVG = '<svg class="bcon bcon-12"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/images/bcon/bcon_pilned.svg#p"></use></svg>';

        function makeZipcodeInnerHtml(code, name) {
            // 1050-1472 København K
            if (code >= 1050 && code <= 1472) {
                if (code == 1050) {
                    return "<h6><a href='' onclick='return toggleSubZipcodeMenu(\"KbhK\");'>" + downarrowSVG + "</a><a class='zipSubmenuHead' href='' id='zipecode_1050-1472' js-data='zipcodeSelector' data-zipcode='1050-1472' data-submenuname='KbhK'>1050-1472 København K</a></h6><div id='SubZipcodeMenu_KbhK' class='SubZipcodeMenu hidden'>" + makeZipcodeAnchorElement(code, name);
                }
                else if (code == 1472) {
                    return makeZipcodeAnchorElement(code, name) + "</div>";
                }
                return makeZipcodeAnchorElement(code, name);
            }
                // 1550-1799 København V
            else if (code >= 1550 && code <= 1799) {
                if (code == 1550) {
                    return "<h6><a href='' onclick='return toggleSubZipcodeMenu(\"KbhV\");'>" + downarrowSVG + "</a><a class='zipSubmenuHead' href='' id='zipecode_1550-1799' js-data='zipcodeSelector' data-zipcode='1550-1799' data-submenuname='KbhV'>1550-1799 København V</a></h6><div id='SubZipcodeMenu_KbhV' class='SubZipcodeMenu hidden'>" + makeZipcodeAnchorElement(code, name);
                }
                else if (code == 1799) {
                    return makeZipcodeAnchorElement(code, name) + "</div>";
                }
                return makeZipcodeAnchorElement(code, name);
            }
                // 1800-1974 Frederiksberg C
            else if (code >= 1800 && code <= 1974) {
                if (code == 1800) {
                    return "<h6><a href='' onclick='return toggleSubZipcodeMenu(\"FreC\");'>" + downarrowSVG + "</a><a class='zipSubmenuHead' href='' id='zipecode_1800-1974' js-data='zipcodeSelector' data-zipcode='1800-1974' data-submenuname='FreC'>1800-1974 Frederiksberg C</a></h6><div id='SubZipcodeMenu_FreC' class='SubZipcodeMenu hidden'>" + makeZipcodeAnchorElement(code, name);
                }
                else if (code == 1974) {
                    return makeZipcodeAnchorElement(code, name) + "</div>";
                }
                return makeZipcodeAnchorElement(code, name);
            }
            else {
                return makeZipcodeAnchorElement(code, name);
            }
        }

        function makeZipcodeAnchorElement(code, name) {
            return "<a href='' id='zipecode_" + code + "' js-data='zipcodeSelector' data-zipcode='" + code + "'><h6>" + code + " " + name + "</h6></a>";
        }

        function changetabMenu(elm) {
            $("#areaTabMenu").find(".active").removeClass("active");
            $("#areaTabMenu").find('a[data-js="' + elm + '"]').addClass("active");
            ul();
        }

        $('a[js-data="region1Selector"]').on("click", function (e) {
            e.preventDefault()
            var regionId = $(this).data("regionid");
            var regionName = $(this).text();

            makeKommuneHTML(regionId);
            changetabMenu('Kommuner');

            var data = {};
            data = { Type: 60, AreaId: regionId, Name: regionName, nameToFillChooseClickBtn: regionName };
            fillChooseClickBtn(data);
        });

        $('#redion2Menu').on("click","a[js-data='region2Selector']", function (e) {
            e.preventDefault()
            var regionId = $(this).data("regionid");
            var regionName = $(this).text();

            makeZipcodeHTML(regionId);
            changetabMenu('Postnummer');

            var data = {};
            if (regionName.slice(-1) === 's' || regionName !== 'København') {
                data = {
                    Type: 20,
                    Municipality: regionId,
                    Name: regionName + " kommune",
                    nameToFillChooseClickBtn: regionName + " kommune"
                };
               } else {
                data = {
                    Type: 20,
                    Municipality: regionId,
                    Name: regionName + "s kommune",
                    nameToFillChooseClickBtn: regionName + "s kommune"
                }; 
            }            fillChooseClickBtn(data);
        });

        var zipcodesArray = [];
        var zipNamesArray = [];


        $('#zippcodeMenu').on("click","a[js-data='zipcodeSelector']", function (e) {
            e.preventDefault()
            if (!$(this).hasClass("passiv")) {
                var zipcode = $(this).data("zipcode");
                var zipcodeName = $(this).text();

                if ($(this).hasClass("zipCodeSelected")) {
                    if ($(this).hasClass("zipSubmenuHead")) {
                        var submenuname = $(this).data("submenuname");
                        $("#SubZipcodeMenu_" + submenuname).children().removeClass("zipCodeSelected passiv");
                    }

                    removeTag("InZips", zipcode);
                    $(this).removeClass("zipCodeSelected");

                    zipcodesArray = $.grep(zipcodesArray, function (value) {
                        return value != zipcode;
                    });

                    zipNamesArray = $.grep(zipNamesArray, function (value) {
                        return value != zipcodeName;
                    });


                } else {
                    if ($(this).hasClass("zipSubmenuHead")) {
                        var submenuname = $(this).data("submenuname");
                        $("#SubZipcodeMenu_" + submenuname).children().addClass("zipCodeSelected passiv");
                    }

                    $(this).addClass("zipCodeSelected");
                    zipcodesArray.push(zipcode);
                    zipNamesArray.push(zipcodeName);
                    var data = { Type: 40, ZipCode: zipcode, Name: zipcodeName, nameToFillChooseClickBtn: "" };
                    PopulateInputs(data);
                }


                var zipcodesToSearch = arrayToString(zipcodesArray);
                var zipNamesToSearch = arrayToString(zipNamesArray);
                fillChooseClickBtn(data);
            }
        });


        $('a[data-js="Landsdel"]').on("click", function (e) {
            $("#zippcodeMenu").removeClass("show");
            $("#redion2Menu").removeClass("show");
            $("#redion1Menu").addClass("show");
        });
        $('a[data-js="Kommuner"]').on("click", function (e) {
            makeKommuneHTML('');
        });
        $('a[data-js="Postnummer"]').on("click", function (e) {
            makeZipcodeHTML('');
        });

        function arrayToString(array) {
            array.sort()
            var str = '';
            $.each(array, function (k, v) {
                str += v + ', ';
            });
            return str;
        }

        function emptyDropdow() {
            zipcodesArray = [];
            zipNamesArray = [];

            $("input#selectedRegion1").val("");
            $("input#selectedRegion2").val("");
            $("#zippcodeWrapper").html("");
            $("#redion2Wrapper").html("");
            showMenuToggle('redion1Menu');
            hideMenuToggle('redion2Menu');
            hideMenuToggle('zippcodeMenu');
        }

        $(document).mouseup(function (e) {
            var container = $("#AreaDropdown");
            var btn = $("toggleAreaDropdownBtn");
            if (!container.is(e.target)
                && container.has(e.target).length === 0
                && btn.is(e.target)
                && btn.has(e.target).length === 0) {
                container.addClass("hidden");
                emptyDropdow();
            }
        });

        function toggleSubZipcodeMenu(id) {
            $("#SubZipcodeMenu_" + id).toggleClass("hidden");
            return false;
        }
        $(document).on("mouseleave", ".tabMenu", function () {
            ul();
        });
        function ul(elem) {
            var activeElem = $("#areaTabMenu").find(".active");
            activeElem.each(function () {
                var elemPosition = $(this).offset();
                var parrentPosition = $(this).closest(".tabMenu").offset();

                $(this).closest(".tabMenu").find(".underline").css({
                    "width": $(elem).width() + "px",
                    "left": (elemPosition.left - parrentPosition.left) + "px"
                });
            });

        }

        var AreaClick_data = [];

        function fillChooseClickBtn(data) {
            AreaClick_data = data;
            var t = "Vælg " + data.nameToFillChooseClickBtn
            $("[data-js='choose_click']").find("h6").html(t);
        }
        $("[data-js='choose_click']").click(function () {
            $("#clickAreaDropdown").addClass("d-none");
            if (AreaClick_data.Type != 40) {
                PopulateInputs(AreaClick_data);
                resetPagingAndTimerSubmitSearch(0);
            }
        });

    </script>
