<script type="text/javascript">

        function SearchBoxSubmitSearch(destination) {
            var textbox = $("input#whereSearchTextBox").val();
            var Search_Area = $("input#Search_Area").val();
            var Search_Region2 = $("input#Search_Region2").val();
            var Search_PlaceName = $("input#Search_PlaceName").val();
            var Search_Street = $("input#Search_Street").val();
            var Search_InZips = $("input#Search_InZips").val();
            var Search_Q = $("input#Search_Q");
            var Search_QValue = Search_Q.val();

            if (textbox != '' && Search_Area == '' && Search_Region2 == '' && Search_PlaceName == '' && Search_Street == '' && Search_InZips == '') {
                var data = {
                    query: textbox,
                    areaLimit: 1,
                    adressLimit: 1,
                };
                var url = "/searchsuggestions/getsuggestions";
                $.ajax({
                    url: url,
                    data: data,
                    type: 'POST',
                    cache: false,
                    success: function (item) {
                        if (item.AreaSuggestions.length > 0) {
                            PopulateInputs(item.AreaSuggestions[0], function () {
                                submitSearchToggle(destination);
                            });
                        }
                        else if (item.BoligSuggestionsTotalCount === 1 && item.AreaSuggestions.length <= 0) {
                            window.location.href = "/bolig/" + item.BoligSuggestions[0].Id;
                        }
                        else {
                            $(".MainSearchMenu").addClass("d-none");
                            makeAreaAlert(destination);
                        }
                    },
                    error: function () {
                        $(".MainSearchMenu").addClass("d-none");
                        makeAreaAlert(destination);
                    }
                });
            } else {
                $(".MainSearchMenu").addClass("d-none");
                submitSearchToggle(destination);
            }
        }

        function submitSearchToggle(destination) {
            if (destination === "map") {
                SubmitSearchToMap();
            }
            else if(destination == "advanced")
            {
                SubmitSearchAdvanced();
            }
            else {
                //if ($.cookie('UseNewList') == null) {
                //    SubmitSearchOldList();
                //}
                //else {
                SubmitSearch();
                //}
            }
        }

        function SubmitSearchToMap() {
            var data = $('form#searchform').serialize();
            return $.ajax({
                url: '/soeg2/gemsoegning',
                type: 'POST',
                async: false, //to force browser to wait to finish, befor exit (.unload)
                data: data,
                cache: false,
                success: function (savedSearch) {
                    if (isFunction(WriteSearchToCookie)) {
                        WriteSearchToCookie(savedSearch);
                    }
                    window.location.href = '/kortsoegning/' + savedSearch.guid;
                }
            });
        }

        function SubmitSearchOldList() {
            var data = $('form#searchform').serialize();
            return $.ajax({
                url: '/soeg2/gemsoegning',
                type: 'POST',
                async: false, //to force browser to wait to finish, befor exit (.unload)
                data: data,
                cache: false,
                success: function (savedSearch) {
                    if (isFunction(WriteSearchToCookie)) {
                        WriteSearchToCookie(savedSearch);
                    }
                    window.location.href = '/soeg/resultater/' + savedSearch.guid;
                }
            });
        }

        function makeAreaAlert(destination) {
            var input = $("input#whereSearchTextBox");
            var alert = $("div#autocompleteError");
            alert.find("span").html(input.val());
            $("div.inputAutocomplete").addClass("inputAlertDotDanger");
            alert.removeClass("d-none");
            var searchedValue = $('#whereSearchTextBox').val();

            dataLayer.push({
                'eventcategory': 'autocompleteError',
                'eventlabel': '' + searchedValue + '',
                'eventaction': 'triggered',
                'event': 'autocompleteError'
            });

            $("form#searchform").find("input").on("click", function () {
                $("div.inputAutocomplete").removeClass("inputAlertDotDanger");
                alert.addClass("d-none");
            });

            $("#addToQAndSubmitSearch").unbind('click').click(function (e) {
                e.preventDefault();
                var value = $("input#Search_Q").val();
                value += " " + $("input#whereSearchTextBox").val();
                $("input#Search_Q").val(value);
                submitSearchToggle(destination);
            });
        }

        function UnderlineOrigin() {
            var activeElem = $("#searchLinkBar .active");
            var activeElemPosition = activeElem.position();
            $("#underline").css({
                "width": activeElem.width() + "px",
                "left": activeElemPosition.left + "px"
            });
        }

        $("#searchLinkBar").on("mouseenter", "a", function () {
            var elemWidth = $(this).width();
            var elemPosition = $(this).position();
            $("#underline").css({
                "width": elemWidth + "px",
                "left": elemPosition.left + "px"
            });
        });

        $("#searchLinkBar").on("mouseleave", function () {
            UnderlineOrigin();
        });

        $("#searchLinkBar").on("click", "a", function () {
            $("#searchLinkBar .active").removeClass("active");
            $(this).addClass("active");
            UnderlineOrigin();
            var type = $(this).data("type");
            toggleSearchArea(type);
        });

        $(window).on("load resize", function () {
            UnderlineOrigin();
        });

        function toggleSearchArea(type) {
            var partials = {
                'agent': '/searchbox/SearchAgentView',
                'archive': '/searchbox/SearchArchiveView',
