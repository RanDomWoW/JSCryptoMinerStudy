            }
            else if (id === "404") {
                $('div[data-js="EstateTypeTags"]').addClass('off');
                $('div[data-js="EstateTypTagsPlaceholder"]').addClass('hidden');
                $('div[data-js="EstateTypeTags"]').find('input[type="checkbox"]').prop('checked', false);
                MasterSelectorToggle("none", $(this));
            }
            ColorSearchTypeDropdown();
        });
        // add estarte type to tags
        $('div[data-js="EstateTypeTags"]').on('click', function () {
            var id = this.getAttribute('id').split("_").pop();

            var input = $('div[data-js="EstateTypeTags"] input[value="' + id + '"]');

            if (input.siblings('input[type="checkbox"]').prop('checked')) {
                input.siblings('input[type="checkbox"]').prop('checked', false);
            } else {
                input.siblings('input[type="checkbox"]').prop('checked', true);
            }

            ColorSearchTypeDropdown(id);
        });

        function MasterSelectorToggle(type, element) {
            if (type === "all") {
                element.attr("data-js", "404");
                element.find("h6").text("Fravælg alle");
            }
            if (type === "none") {
                element.attr("data-js", "0");
                element.find("h6").text("Vælg alle");
            }
        }
        function ColorSearchTypeDropdown(id) {
            $("div[data-js='EstateTypeTags']").each(function () {
                var checkbox = $(this).find('input[type="checkbox"]');
                var id = $(this).find("input[name$='.Id']").val();

                if (checkbox.prop('checked')) {
                    $(this).removeClass('off');
                    $("div[data-estatetyptagsplaceholder='" + id + "']").removeClass("hidden");
                } else {
                    $(this).addClass('off');
                    $("div[data-estatetyptagsplaceholder='" + id + "']").addClass("hidden");
                }
            });


            var totalEstateTypeTagsCount = $("div[data-js='EstateTypeTags']").length;
            var estateTypeTagsCount = $("div[data-js='EstateTypeTags'] input[type='checkbox']:checked").length;
            var text = "Ingen valgt";
            if (totalEstateTypeTagsCount === estateTypeTagsCount) {
                MasterSelectorToggle("all", $("div#typeDropdownToggle"));
                $('div[data-js="EstateTypTagsPlaceholder"]').addClass('hidden');
                text = "Alle typer";
            }
            else if (estateTypeTagsCount == 0) {
            }
            else if (estateTypeTagsCount == 1) {
                text = $("div[data-js='EstateTypeTags'] input[type='checkbox']:checked").siblings("h6").text();
            }
            else {
                MasterSelectorToggle("none", $("div#typeDropdownToggle"));
                //text = estateTypeTagsCount + " type" + (estateTypeTagsCount == 1 ? "" : "r");
                text = "";
            }
            $("#typePlaceholder").html(text)
        }
        ColorSearchTypeDropdown();

    </script>
<script type="text/javascript">
        var useArray = [];
        var specificAddressType = 900;
        $.fn.autocompleteArea = function (url, emptyPlaceholder) {
            this.autocompletegrouped({
                minLength: 1,
                source: function (request, response) {
                    var term = request.term.trim();//.split(',')[0];
                    useArray = [];

                    var isIzipIntervalSearch = false;
                    var zipIntervalSearchFirstZip;
                    var zipIntervalSearchLastZip;
                    if (term.replace(/\s/g, '').charAt(4) == "-") {
                        zipIntervalSearchFirstZip = term.split("-")[0];
                        zipIntervalSearchLastZip = term.split("-")[1];

                        term = zipIntervalSearchLastZip;
                        isIzipIntervalSearch = true;

                    }

                    if (term in a_cache && !$("input#Search_InZips").val() && !isIzipIntervalSearch) {
                        response(a_cache[term]);
                        return;
                    }

                    var queryType = 0;
                    if ($("input#Search_InZips").val()) {
                        queryType = 40;
                    }

                    var data = {
                        query: term,
                        areaLimit: 20,
                        adressLimit: 20,
                        queryType: queryType
                    };
                    var aLastEstateXhr = $.getJSON(url, data, function (data, status, xhr) {

                        $.each(data.AreaSuggestions, function (k, v) {
                            v.Name = renameAutocompleteElement(v);
                        });

                        useArray = data.AreaSuggestions;

                        if (data.BoligSuggestions && data.BoligSuggestions.length > 0) {
                            $.each(data.BoligSuggestions, function (k, v) {
                                v.Name = v.Street + ", " + v.ZipCode + " " + GetCityName(v.ZipCode);
                                v.Type = specificAddressType;
                            });
                            useArray = $.merge(data.AreaSuggestions, data.BoligSuggestions);
                            if (data.BoligSuggestionsTotalCount > data.BoligSuggestions.length) {
                                useArray.push({ "Name": "...", "Type": 900 });
                            }
                        }

                        if (isIzipIntervalSearch) {
                            var zipIntervalArray = [];
                            $.each(data.AreaSuggestions, function (k, v) {
                                var name = "Imellem " + zipIntervalSearchFirstZip + " og ";
                                if (v.Type == 40 && v.ZipCode.length <= 4) {
                                    name += v.Name;
                                    var az = parseInt(zipIntervalSearchFirstZip);
                                    var bz = (v.ZipCode.length > 4 ? v.ZipCode.split("-")[1] : v.ZipCode);
                                    var z = (az < bz ? az + "-" + bz : bz + "-" + az);
                                    zipIntervalArray.unshift({ "Name": name, "Type": 42, "ZipCode": z });
                                    a_cache[z] = [{ "Name": name, "Type": 42, "ZipCode": z }];
                                }
                            });
                            if (zipIntervalArray.length <= 0) {
                                var name = "Imellem " + zipIntervalSearchFirstZip + " " + GetCityName(zipIntervalSearchFirstZip) + " og " + (term ? term : "...");
                                zipIntervalArray.unshift({ "Name": name, "Type": 42, "ZipCode": zipIntervalSearchFirstZip + "-" + term });
                            }
                            useArray = zipIntervalArray;
                        }


                        if (!$("input#Search_InZips").val()) {
                            a_cache[term] = useArray;
                        }
                        if (xhr === aLastEstateXhr) {
                            response(useArray);
                        }

                    });
                },
                search: function (response) {
                },
                response: function (event, ui) {
                    if (ui.content.length <= 0 && $("input#Search_InZips").val() == "") {
                        clearSearch_AreaInputFields();
                    }
                    $(this).siblings('input#Suggestion').attr('placeholder', '');
                },
                focus: function (event, ui) {
                    //$(this).val(ui.item.Name);
                    $(this).attr('placeholder', '');
                    $(this).val("");
                    $(this).siblings('input#Suggestion').attr('placeholder', ui.item.Name);
                },
                open: function () {
                    var x = ($("#Search_AreaTagsWrapper").width() < ($("div#inputsArea").width() - 20) ? $("#Search_AreaTagsWrapper").width() : 137);
                    var length = $("div#inputsArea").width() - x;
                    $(".ui-widget-content").css("min-width", length);
                    $(".ui-widget-content").css("max-width", length);

                    var value = $(this).val();
                    if (typeof a_cache[value] === 'undefined' || a_cache[value] === null) {
                        return;
                    }
                    if (SearchBeginsAsZip(value)) {
                        value = value.substr(0, 4);
                    }
                    if (SearchBeginsAsZip(value)) {
                        value = value.substr(0, 4);
                    }
                    var suggestion = a_cache[value][0].Name;
                    var sugestionValue = suggestion.substring($(this).val().length);
                    if (value.toLowerCase() === suggestion.substring(0, value.length).toLowerCase()) {
                        $(this).siblings('input#Suggestion').attr('placeholder', $(this).val() + sugestionValue);
                    } else {
                        $(this).siblings('input#Suggestion').attr('placeholder', '');
                    }
                    $(".ui-autocomplete:visible").css({ top: "-=4", left: "-=8" });
                    $('.ui-autocomplete').off('menufocus hover mouseover mouseenter');
                },
                close: function () {
                    var x = ($("#Search_AreaTagsWrapper").width() < ($("div#inputsArea").width() - 20) ? $("#Search_AreaTagsWrapper").width() : 137);
                    var length = $("div#inputsArea").width() - x;
                    $(this).siblings('input#Suggestion').attr('placeholder', '');
                },
                change: function (event, ui) {
                    var value = $(this).val();
                    if (SearchBeginsAsZip(value) && a_cache) {
                        var nameFromA_cache = a_cache[value.substr(0, 4)].length > 0 ? a_cache[value.substr(0, 4)][0].Name.substr(0, $(this).val().length).toLowerCase() : '';
                        if (nameFromA_cache == $(this).val().toLowerCase()) {
                            value = value.substr(0, 4);
                        }
                    }
                    var data = a_cache[value] || a_cache[value.replace(/\s/g, '')];
                    if (data && data.length > 0) {
                        $(this).val(data[0].Name);

                        if (data[0].Type === specificAddressType) {
                            $(this).addClass("ui-autocomplete-loading");
                            window.location.href = "/bolig/" + data[0].Id;
                        }
                        else {
                            PopulateInputs(data[0]);
                        }
                    } else if (/^\d{4}\-\d{4}/.test(value.replace(/\s/g, ''))) {
                        PopulateInputs(useArray[0]);
                    } else {
                        var closest = null;
                        var tryAgainValue = value.replace(/\s/g, '');
                        var closessMatch = [];
                        var closessMatchVal = 0;
                        $.each(a_cache, function (k, v) {
                            var compareNum = 0;
                            var l = Math.min(k.length, tryAgainValue.length);
                            for (i = 0; i < l; i++) {
                                if (k.charAt(i) == tryAgainValue.charAt(i)) compareNum++;
                            }
                            if (compareNum >= closessMatchVal) {
                                closessMatchVal = compareNum;
                                closessMatch = v;
                            }
                        });
                        PopulateInputs(closessMatch[0]);
                    }

                    if ($(this).val() == "" && $("#Search_AreaTagsWrapper").html() == "") {
                        clearSearch_AreaInputFields();
                        $(this).attr('placeholder', emptyPlaceholder);
                        $(this).val('');
                    }
                },
                select: function (event, ui) {
                    if (ui.item != null) {
                        $(this).siblings('input#Suggestion').attr('placeholder', '');
                        $(this).val(ui.item.Name);

                        if (ui.item.Type === specificAddressType) {
                            $(this).addClass("ui-autocomplete-loading");
                            window.location.href = "/bolig/" + ui.item.Id;
                        }
                        else {
                            PopulateInputs(ui.item);
                        }
                    } else {
                        $(this).attr('placeholder', emptyPlaceholder);
                        $(this).val('');
                    }
                    return false;
                }
            });
        }

        // Sets the receiving inputfields
        function PopulateInputs(item, callback) {
            if (item) {
                switch (item.Type) {
                    case 10:
                        // ?
                        break;
                    case 20:
                        clearSearch_AreaInputFields();
                        $("#Search_AreaTagsWrapper").html("");
                        // Kommune
                        $('input#Search_Region2').val(item.Municipality);
                        makeTag("Region2", item.Municipality, item.Name);
                        break;
                    case 30:
                        clearSearch_AreaInputFields();
                        $("#Search_AreaTagsWrapper").html("");
                        // Stednavn
                        $('input#Search_InZips').val(item.ZipCode);
                        $('input#Search_PlaceName').val(item.Place);
                        makeTag("PlaceName", item.Municipality, item.Name);
                        break;
                    case 40:
                        // By/postdistrikt
                        if ($('input#Search_InZips').val() != '') {
                            var inZips = $('input#Search_InZips').val();
                            if (inZips.indexOf(item.ZipCode) < 0) {
                                inZips += "," + item.ZipCode;
                                makeTag("InZips", item.ZipCode, item.Name);
                                ToggleDropdownState();
                            }
                            else {
                                $("#whereSearchTextBox").val("").focus();
                            }
                        } else {
                            var inZips = item.ZipCode;
                            $("#Search_AreaTagsWrapper").html("");
                            makeTag("InZips", item.ZipCode, item.Name);
                        }
                        clearSearch_AreaInputFields();
                        $('input#Search_InZips').val(inZips);
                        $("input#whereSearchTextBox").focus().blur();
                        break;
                    case 42:
                        // By/postdistrikt INTERVAL
                        if ($('input#Search_InZips').val() != '') {
                            var inZips = $('input#Search_InZips').val();
                            if (inZips.indexOf(item.ZipCode) < 0) {
                                inZips += "," + item.ZipCode;
                                makeTag("InZips", item.ZipCode, item.Name);
                                ToggleDropdownState();
                            }
                            else {
                                $("#whereSearchTextBox").val("").focus();
                            }
                        } else {
                            var inZips = item.ZipCode;
                            $("#Search_AreaTagsWrapper").html("");
                            makeTag("InZips", item.ZipCode, item.Name);
                        }
                        clearSearch_AreaInputFields();
                        $('input#Search_InZips').val(inZips);
                        $("input#whereSearchTextBox").focus().blur();
                        break;
                    case 50:
                        clearSearch_AreaInputFields();
                        $("#Search_AreaTagsWrapper").html("");
                        // Vej
                        var streetname = item.Name.substring(0, item.Name.indexOf(item.ZipCode) - 2);
                        streetname = streetname.replace(" ", "-");
                        $('input#Search_InZips').val(item.ZipCode);
                        $('input#Search_Street').val(streetname);
                        makeTag("Street", streetname, item.Name);
                        break;
                    case 60:
                        clearSearch_AreaInputFields();
                        $("#Search_AreaTagsWrapper").html("");
                        // område/landsdel
                        $('input#Search_Area').val(item.AreaId);
                        makeTag("Area", item.AreaId, item.Name);
                        break;
                }
                return callback;
            }
        }

        function clearSearch_AreaInputFields() {
            $('input#Search_InZips').val('');
            $('input#Search_PlaceName').val('');
            $('input#Search_Region2').val('');
            $('input#Search_Street').val('');
            $('input#Search_Area').val('');
        }

        function renameAutocompleteElement(elem) {
            var areaName = '';
            switch (elem.Type) {
                case 10:
                    // ?
                    break;
                case 20:
                    // Kommune
                    areaName = elem.Name + ' kommune';
                    break;
                case 30:
                case 60:
                    // Stednavn
                    areaName = elem.Name;
                    break;
                case 40:
                    // By/postdistrikt
                    areaName = elem.ZipCode + " " + elem.Name;
                    break;
                case 50:
                    // Vej
                    areaName = elem.Name;
                    break;
            }
            return areaName;
        }

        function SearchBeginsAsZip(val) {
            var reg = /^\d{4}/;
            return reg.test(val)
        }

        function makeTag(inputField, id, name) {
            $("input#whereSearchTextBox").val("");
            $("#Search_AreaTagsWrapper").append('<div class="areaTag text-primary-dark bg-primary-light d-inline-block my-2 mx-1" id="areaTag_' + id + '" onclick="removeTag(\'' + inputField + '\',\'' + id + '\');"><h6>' + name + '</h6></div>');

            if (inputField != "InZips") {
                $("#Search_AreaWrapper").addClass("hidden");
            }
            else {
                $("#Search_AreaWrapper").removeClass("hidden");
                $('a[data-zipcode="' + id + '"]').addClass("zipCodeSelected");
            }
            $('input#Suggestion').attr('placeholder', '');
        }

        function removeTag(inputField, value) {
            if (inputField == "InZips") {
                var input = $('input#Search_' + inputField);
                var newString = input.val().replace(value, "");
                newString = newString.replace(",,", ",");
                if (newString == ",") newString = "";
                input.val(newString);
                $('a[data-zipcode="' + value + '"]').removeClass("zipCodeSelected");
            } else {
                clearSearch_AreaInputFields();
                $("#Search_AreaWrapper").removeClass("hidden").focus();
            }
            $("#areaTag_" + value).remove();
            ToggleDropdownState();
            $('input#whereSearchTextBox').blur();
        }

        var emptyPlaceholder = $('input#whereSearchTextBox').attr("placeholder");
        $('input#whereSearchTextBox').autocompleteArea('/searchsuggestions/getsuggestions', emptyPlaceholder);

        $('input#whereSearchTextBox').focus(function () {
            if ($("#Search_AreaTagsWrapper").html() == "") {
                $(this).attr("placeholder", "Indtast det område, du vil søge bolig i");
            }
            else {
                $(this).attr("placeholder", "Indtast evt. flere postnumre");
            }
        });
        $('input#whereSearchTextBox').blur(function () {
            if ($("#Search_AreaTagsWrapper").html() == "") {
                $(this).attr("placeholder", emptyPlaceholder);
            }
            else {
                $(this).attr("placeholder", "Indtast evt. flere postnumre");
            }
            var x = ($("#Search_AreaTagsWrapper").width() < ($("div#inputsArea").width() - 20) ? $("#Search_AreaTagsWrapper").width() : 137);
            var length = $("div#inputsArea").width() - x;
            $(this).siblings('input#Suggestion').attr('placeholder', '');
        });
        $('input#whereSearchTextBox').keydown(function (event) {
            if (event.keyCode == 13) { //Enter
                if ($(this).val().length == 0) {
                    event.preventDefault();
                    return false;
                } else {
                    $(".ui-autocomplete").hide();
                    $("input#whereSearchTextBox").blur().focus();
                    //SearchBoxSubmitSearch('list');
                }
            }
            if (event.keyCode == 38 || event.keyCode == 40) { //up or down
                var hover = $(".ui-state-focus").html();
                $('input#Suggestion').attr('placeholder', hover)
            }
        });


        function ToggleDropdownState() {
            var length = $(".areaTag").text().length * 8;

            var maxLength = $("#inputsArea div#Search_AreaWrapper").width() - $("#inputsArea div#areaTagMoreBtn").width();
            if (length >= maxLength && !$("#Search_AreaTagsWrapper").hasClass("dropDownMenu")) {
                toggleAreaDropdownBtn("on");
            }
            if (length < maxLength && $("#Search_AreaTagsWrapper").hasClass("dropDownMenu")) {
                toggleAreaDropdownBtn("off");
            }
            $("#areaTagMoreBtn").find("h6").html("Flere postnr. er valgt");
            //$("input#whereSearchTextBox").blur().focus();
        }

        function toggleAreaDropdownBtn(state) {
            if (state == "on") {
                $("#areaTagMoreBtn").removeClass("hidden");
                $("#Search_AreaTagsWrapper").addClass("dropDownMenu hidden");
                $("#inputsArea").css("overflow", "visible");
            }
            if (state == "off") {
                $("#areaTagMoreBtn").addClass("hidden");
                $("#Search_AreaTagsWrapper").removeClass("dropDownMenu hidden");
                $("#inputsArea").css("overflow", "hidden");
            }
        }

        function toggleAreaTagDropdow() {
            $("#Search_AreaTagsWrapper").toggleClass("hidden");
            return false;
        }

        if (window.matchMedia("(min-width: 400px)").matches) {
            $("#whereSearchTextBox").attr('placeholder', 'Søg blandt 61.631 ejendomme i hele Danmark');
        }
        else {
            $("#whereSearchTextBox").attr('placeholder', 'Søg blandt 61.631 ejendomme');
        }
    </script>
<script type="text/javascript">
