Shopify.AjaxifyCart = (function($) {
  var _config = {
    addedToCartBtnLabel: 'Thank You!',
    addingToCartBtnLabel: 'Adding...',
    soldOutBtnLabel: 'Sold Out',
    howLongTillBtnReturnsToNormal: 1000, // in milliseconds.
    cartCountSelector: '.cart-count a:first, #gocart p a, #cart .checkout em, .item-count',
    cartTotalSelector: '#cart-price',
    feedbackPosition: 'nextButton', // 'aboveForm' for top of add to cart form, 'belowForm' for below the add to cart form, and 'nextButton' for next to add to cart button.
    shopifyAjaxAddURL: '/cart/add.js',
    shopifyAjaxCartURL: '/cart.js'
  };
  var _refreshCart = function refreshCart(cart) {
    //$cartBtn = $(".cart-link");
    //if ($cartBtn) {
      //var $cartCount = $cartBtn.find('.cart_count');
      //var value = $cartCount.text() || '0';
      var cart_items_html = "";
      var $cart = $("#cart-drawer ul");
      
      /*if($cartCount && $cartCount.size()) {
        $cartCount.text(value.replace(/[0-9]+/,cart.item_count));
      } else {
        $cartBtn.prepend('<div class="cart_count">' + cart.item_count + '</div>');
      }*/
      
      $cart.find('li:not(:first)').remove();

      if (cart.item_count == 0) {
        $cart.append('<li class="mm-label empty_cart"><a href="/cart">Empty Cart</a></li>');
        //$cartCount.remove();
      } else {
        $.each(cart.items, function(index, item) {
          cart_items_html += '<li class="cart_item">' +
            '<a href="' + item.url +'">';
          if (item.image) {
            cart_items_html += '<div class="cart_image">' +
                '<img src="' + item.image.replace(/(\.[^.]*)$/, "_medium$1").replace('http:', '') + '" alt="' + item.title + '" />' +
              '</div>';
          }
           
          cart_items_html += '<div class="item_title">' + item.title +'</div>' +
              '<strong class="price">' + Shopify.formatMoney(item.price, $cart.data('money-format')) + '</strong>' +
              '<div class="quantity_selector">' +
                '<span class="minus">-</span><input type="number" min="0" class="quantity" name="updates[]" id="updates_' + item.id + '" value="' + item.quantity + '" /><span class="plus">+</span>' +
              '</div><div class="clear"></div>' +
            '</a>' +
          '</li>';
        });

        cart_items_html += '<li class="mm-label">' + '<a href="/cart">' +
              '<strong>Subtotal</strong>' +
            '</a>' +
            '<p class="mm-counter">' + Shopify.formatMoney(cart.total_price, $cart.data('money-format')) + '</p>' +
          '</li>' +
          '<li class="mm-subtitle clearfix">';

          

          cart_items_html += '<input type="submit" class="action_button right" value="Checkout" />' +
            '<a href="/cart" class="action_button edit_cart left">Edit Cart</a>' +
          '</li>';
        $cart.append(cart_items_html);

        
      }
  }
  var _config = {
    
    // What you might want to change
    addToCartBtnLabel:             'Add to cart',
    addedToCartBtnLabel:           'Thank you!',
    addingToCartBtnLabel:          'Adding...',
    soldOutBtnLabel:               'Sold Out',
    howLongTillBtnReturnsToNormal: 1000, // in milliseconds.
    cartCountSelector:             '.cart-count a span, .cart_count, #gocart p a, .item-count',
    cartTotalSelector:             '#cart-price',
    cartDrawerOpener:			   '.open-cart',
    // 'aboveForm' for top of add to cart form, 
    // 'belowForm' for below the add to cart form, and 
    // 'nextButton' for next to add to cart button.
    feedbackPosition:              'nextButton',
    
    // What you will never need to change
    addToCartBtnSelector:          '[type="submit"]',
    addToCartFormSelector:         'form[action="/cart/add"]',
    shopifyAjaxAddURL:             '/cart/add.js',
    shopifyAjaxCartURL:            '/cart.js'
  }; 
  var _showFeedback = function(success, html, addToCartForm) {
    $('.ajaxified-cart-feedback').remove();
    var feedback = '<p class="hidden ajaxified-cart-feedback ' + success + '">' + html + '</p>';
    switch (_config.feedbackPosition) {
      case 'aboveForm':
        addToCartForm.before(feedback);
        break;
      case 'belowForm':
        addToCartForm.after(feedback);
        break;
      case 'nextButton':
      default:
        addToCartForm.find('input[type="submit"]').after(feedback);
        break;   
    }
    $('.ajaxified-cart-feedback').slideDown();
  };
  function ajaxUpdateCart(){
    var addToCartBtn = $('form[action="/cart/add"]').find('input[type="submit"]');
    $.ajax({
          url: '/cart/update.js',
          dataType: 'json',
          type: 'post',
          data: $('#cart-drawer input.quantity').parents('form').serialize(),
          success: function(itemData) {
            // Update cart count and show cart link.
            $.getJSON(_config.shopifyAjaxCartURL, function(cart) {
              
              if (_config.cartCountSelector && $(_config.cartCountSelector).size()) {
                var value = $(_config.cartCountSelector).html() || '0';
                $(_config.cartCountSelector).html(value.replace(/[0-9]+/,cart.item_count));
              }
              _refreshCart(cart);              
              if (_config.cartTotalSelector && $(_config.cartTotalSelector).size()) {
                if (typeof Currency !== 'undefined' && typeof Currency.money_format !== 'undefined') {
                  var newCurrency = '';
                  if ($('[name="currencies"]').size()) {
                    newCurrency = $('[name="currencies"]').val();
                  }
                  else if ($('#currencies span.selected').size()) {
                    newCurrency = $('#currencies span.selected').attr('data-currency');
                  }
                  if (newCurrency) {
                    $(_config.cartTotalSelector).html('<span class=money>' + Shopify.formatMoney(Currency.convert(cart.total_price, "USD", newCurrency), Currency.money_format[newCurrency]) + '</span>');
                  } 
                  else {
                    $(_config.cartTotalSelector).html(Shopify.formatMoney(cart.total_price, "$ {{amount}}"));
                  }
                }
                else {
                  $(_config.cartTotalSelector).html(Shopify.formatMoney(cart.total_price, "$ {{amount}}"));
                }
              };
            });        
          }, 
          error: function(XMLHttpRequest) {
            var response = eval('(' + XMLHttpRequest.responseText + ')');
            response = response.description;
            if (response.slice(0,4) === 'All ') {
              _showFeedback('error', response.replace('All 1 ', 'All '), addToCartForm);
              addToCartBtn.removeAttr('disabled').val(_config.soldOutBtnLabel).attr('disabled','disabled');
            }
            else {
              _showFeedback('error', '<i class="fa fa-warning"></i> ' + response, addToCartForm);
              addToCartBtn.removeAttr('disabled').removeClass('disabled').removeClass('inverted').val(addToCartBtn.attr('data-label'));
            }
          }
        });
  }
  var _init = function() {   
    $(document).ready(function() { 
      
      var regular_shipping = 5000, 
      doc = $(document).on({
        'layout-addedcart': function(e, data){
        	var tmpl = $('[data-tmpl="pacart"]').html();
         ;
          
          if(data.total_price < regular_shipping) {
            data.free_shipping = [Shopify.formatMoney(regular_shipping - data.total_price), "to free shipping."].join(" ");
          }
          data.total_price = Shopify.formatMoney(data.total_price);
          $.fancybox($.tmpl(tmpl, data));
          setTimeout(function(){
            
            $.ajax({
              type: 'GET',
              url: '/cart',
              success: function(res) {
                var cart_type = $(res).find('.freeproduct').html();
                var gift_card = $(res).find('.my_gift').html();
                var other_products = $(res).find('.other_products').html();
                if ((other_products != undefined) && (cart_type == undefined)){
                  $.fancybox.close();
                  $.fancybox("#trigger");
                }
              }
            });
            
          },100);
        }
      });
      
      /* check popup added */
      var isPopupAdded = (function(rs){return rs || 0})(true);
      if(!isPopupAdded) {
      	doc.off('layout-addedcart');
      }
        
      /* end check */
      
      $('form[action="/cart/add"]').submit(function(e) {
        e.preventDefault();
        var addToCartForm = $(this);  
        // Disable add to cart button.
        var addToCartBtn = addToCartForm.find('input[type="submit"]');
        addToCartBtn.attr('data-label', addToCartBtn.val());
        addToCartBtn.val(_config.addingToCartBtnLabel).addClass('disabled').attr('disabled', 'disabled');
        // Add to cart.
        $.ajax({
          url: _config.shopifyAjaxAddURL,
          dataType: 'json',
          type: 'post',
          data: addToCartForm.serialize(),
          success: function(itemData) {
            // Re-enable add to cart button.
            addToCartBtn.addClass('inverted').val(_config.addedToCartBtnLabel);
            _showFeedback('success','<i class="fa fa-check"></i> Added to Cart <a href="/cart">Check Out Now</a> or <a href="/collections/new-at-credo">Continue Shopping</a>.',addToCartForm);
            window.setTimeout(function(){
              addToCartBtn.removeAttr('disabled').removeClass('disabled').removeClass('inverted').val(addToCartBtn.attr('data-label'));
            }, _config.howLongTillBtnReturnsToNormal);
            // Update cart count and show cart link.
            $.getJSON(_config.shopifyAjaxCartURL, function(cart) {
              
              // layout
              doc.trigger('layout-addedcart', [{
                title: itemData.title,
                price: Shopify.formatMoney(itemData.price),
                image: itemData.image,
                quantity: itemData.quantity,
                item_count: cart.item_count, 
                total_price: cart.total_price
              }]);
              
              
              if (_config.cartCountSelector && $(_config.cartCountSelector).size()) {
                var value = $(_config.cartCountSelector).html() || '0';
                $(_config.cartCountSelector).html(value.replace(/[0-9]+/,cart.item_count));
              }
              _refreshCart(cart);
              
              if (_config.cartTotalSelector && $(_config.cartTotalSelector).size()) {
                if (typeof Currency !== 'undefined' && typeof Currency.money_format !== 'undefined') {
                  var newCurrency = '';
                  if ($('[name="currencies"]').size()) {
                    newCurrency = $('[name="currencies"]').val();
                  }
                  else if ($('#currencies span.selected').size()) {
                    newCurrency = $('#currencies span.selected').attr('data-currency');
                  }
                  if (newCurrency) {
                    $(_config.cartTotalSelector).html('<span class=money>' + Shopify.formatMoney(Currency.convert(cart.total_price, "USD", newCurrency), Currency.money_format[newCurrency]) + '</span>');
                  } 
                  else {
                    $(_config.cartTotalSelector).html(Shopify.formatMoney(cart.total_price, "$ {{amount}}"));
                  }
                }
                else {
                  $(_config.cartTotalSelector).html(Shopify.formatMoney(cart.total_price, "$ {{amount}}"));
                }
              };
            });        
          }, 
          error: function(XMLHttpRequest) {
            var response = eval('(' + XMLHttpRequest.responseText + ')');
            response = response.description;
            if (response.slice(0,4) === 'All ') {
              _showFeedback('error', response.replace('All 1 ', 'All '), addToCartForm);
              addToCartBtn.removeAttr('disabled').val(_config.soldOutBtnLabel).attr('disabled','disabled');
            }
            else {
              _showFeedback('error', '<i class="fa fa-warning"></i> ' + response, addToCartForm);
              addToCartBtn.removeAttr('disabled').removeClass('disabled').removeClass('inverted').val(addToCartBtn.attr('data-label'));
            }
          }
        });   
        return false;    
      });    
      $(document).on("change", "#cart-drawer input.quantity", function() { 
        ajaxUpdateCart(); 
        return false;       
      });
      $(document).on("click", ".quantity", function(e) { 
        e.stopPropagation();
        e.preventDefault();
        return false;
      });           
      $(document).on("click", ".minus", function(e) { 
    var quantity = parseInt($(this).next("input").val()); 
    quantity -= 1;
    $(this).next("input").attr("value", quantity);
    if (quantity == 0) {
      $(this).closest('li.cart_item').slideUp();
      setTimeout(ajaxUpdateCart, 500);
    } else {
      ajaxUpdateCart();
    }
    e.stopPropagation();
    e.preventDefault();
    return false;
  });
      $(document).on("click", ".plus", function(e) { 
        var quantity = parseInt($(this).prev("input").val()); 
        quantity += 1;
        $(this).prev("input").attr("value", quantity);
        ajaxUpdateCart();
        e.stopPropagation();
        e.preventDefault();
        return false;
      });
              
    });
  };
  return {
    init: function(params) {
        // Configuration
        params = params || {};
        // Merging with defaults.
        $.extend(_config, params);
        // Action
        $(function() {
          _init();
        });
    },    
    getConfig: function() {
      return _config;
    }
  }  
})(jQuery);

Shopify.AjaxifyCart.init();

</script>

