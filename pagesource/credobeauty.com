    <script>
      $('.color_label').on('click',function(){
        var color = $(this).val();
        var url = $(this).attr('variant-id');
        var height = $('.product_all_images').outerHeight();
        $('.product_all_images').height(height);
        $('.product_all_images').addClass('loading_spinner');
        $('#product-right .swatch.clearfix').addClass('pointer_event');
        $.ajax({
          type: 'GET',
          url: url,
          async:false,
          success: function(res) {
            console.log(res);
            var myHtml = $(res).find('.product_all_images').html();
            $( '.product_all_images' ).html(myHtml);
            $('label.color_label_value').html( color );
            $('.product_all_images').removeClass('loading_spinner');
            $('.product_all_images').height('auto');
            $('#product-right .swatch.clearfix').removeClass('pointer_event');
          }
        });
      });
    </script>
    
