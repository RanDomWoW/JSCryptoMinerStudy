	 <script>
        //document.domain = 'bike-centre.ru';

        $(document).ready(function(){
            $('.map').each(function() {
                $(this).empty();
                var mapOptions = {
                    zoom: $(this).data('map_zoom'),
                    scrollwheel: $(this).data('map_scrollwheel'),
                },
                coordCenter = $(this).data('map_coord_center'),
                map = new google.maps.Map(this, mapOptions);
                var data = '<div style="width:200px;text-align:center">'+ $(this).data('map_infowindow') +'</div>';
                var infowindow = new google.maps.InfoWindow({
                    content: data
                });

                var marker = new google.maps.Marker({
                    position: new google.maps.LatLng(coordCenter[0], coordCenter[1]),
                    map: map,
                    icon: {
                        url: '/bitrix/templates/bikecenter_main/images/pin.png',
                        size: new google.maps.Size(37, 48),
                        anchor: new google.maps.Point(18, 48)
                    }
                });
                google.maps.event.addListener(marker, 'click', function() {
                    infowindow.open(map,marker);
                });
                map.setCenter(new google.maps.LatLng(coordCenter[0], coordCenter[1]));
            });
        });
    </script>
