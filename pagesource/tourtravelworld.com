<script>
function check_existing_email_ajax(emailid) {

	if (validateEmail(emailid)) {
		
		var ajax_url = 'https://www.tourtravelworld.com/index-pages/get-tour-quote-ajax.php?page=validateEmail&email='+emailid;

		$.ajax ({
			
		    type: "POST",
		    url: ajax_url,
		    cache: false,
		    success: function(msg) {
			    msg =msg.trim();
			    if(msg != 'error'){

			    	var obj = JSON.parse(msg);

					$("#name").val(obj.yourName);
					$("#mobile_cont_code").val(obj.isd_code);
					$("#search_city").val(obj.search_city);
					$("#city_id").val(obj.city_code);
					$("#mobile_number").val(obj.mobile_no);
					$("#mobile_number").intlTelInput("setCountry", obj.country);
			    }
		    },
		    error : function(msg, status) {
			    
			    alert(msg+status);
		    }
		});
	}
}

