<script type="text/ng-template" id="warenkorb-artikel-attribut.html">

	<form name="formWarenkorbArtikelAttribut">
		<div class="form-group">
			<span>from&nbsp;{{ warenkorbArtikelAttributCtrl.minValue }}</span>
			<span class="select2-style-input" ng-class="{'aw-eingabe-invalid': !formWarenkorbArtikelAttribut.awEingabe.$valid}">
				<input type="number" ng-disabled="warenkorbArtikelAttributCtrl.isLoading"
					   min="{{ warenkorbArtikelAttributCtrl.minValue }}"
					   max="{{ warenkorbArtikelAttributCtrl.maxValue}}"
					   ng-model="warenkorbArtikelAttributCtrl.value"
					   ng-change="warenkorbArtikelAttributCtrl.onValueChange()"
					   step="1"
					   ng-keyup="warenkorbArtikelAttributCtrl.showUpdateButtons ? ($event.keyCode == 13 && formWarenkorbArtikelAttribut.awEingabe.$valid && warenkorbArtikelAttributCtrl.attributwertEingabenAktualisieren()) : warenkorbArtikelAttributCtrl.noop()"
					   name="awEingabe"
				>
			</span>
			<button class="text-only"
					ng-show="warenkorbArtikelAttributCtrl.showUpdateButtons"
					ng-click="warenkorbArtikelAttributCtrl.attributwertEingabenAktualisieren()"
					ng-disabled="warenkorbArtikelAttributCtrl.isLoading || !warenkorbArtikelAttributCtrl.artikel.valid">
				<span title="Update" class="fa fa-refresh"
					  ng-class="{'fa-spin': warenkorbArtikelAttributCtrl.isLoading}"></span>
			</button>
			<span>to&nbsp;{{ warenkorbArtikelAttributCtrl.maxValue }} {{ warenkorbArtikelAttributCtrl.unit }}</span>
		</div>
	</form>
</script>
<style>button[disabled] span.fa-shopping-cart{color:gray;cursor:not-allowed}.checkbox label.checkbox-label-disabled{background:#d3d3d3!important}</style>
<script type="text/ng-template" id="warenkorb-artikel-update.html">
	<div class="form-group">
		<span class="select2-style-input">
			<input type="number" class="amount"
				   ng-model="warenkorbArtikelUpdateCtrl.menge"
				   ng-disabled="warenkorbArtikelUpdateCtrl.isLoading || (warenkorbArtikelUpdateCtrl.hasReferenzAttribute() && !warenkorbArtikelUpdateCtrl.attributwertEingabenValid())"
				   min="0"
				   max="9999"
				   step="1"
				   ng-keyup="$event.keyCode == 13 && warenkorbArtikelUpdateCtrl.mengeAktualisieren(warenkorbArtikelUpdateCtrl.showUpdateButtons)"
			>
		</span>
		<button class="text-only"
				ng-hide="warenkorbArtikelUpdateCtrl.showUpdateButtons"
				ng-click="warenkorbArtikelUpdateCtrl.mengeAktualisieren()"
				ng-disabled="!warenkorbArtikelUpdateCtrl.artikel.valid">
			<span ng-attr-title="{{ (warenkorbArtikelUpdateCtrl.artikel.valid) ? 'Add to Cart' : 'lbl_ungueltige_werte' }}"
				  class="fa fa-lg fa-shopping-cart"
				  ng-class="{'faa-burst animated': warenkorbArtikelUpdateCtrl.isLoading}"
			></span>
		</button>
		<button class="text-only"
				ng-show="warenkorbArtikelUpdateCtrl.showUpdateButtons"
				ng-click="warenkorbArtikelUpdateCtrl.mengeAktualisieren(true)"
				ng-disabled="warenkorbArtikelUpdateCtrl.isLoading || (warenkorbArtikelUpdateCtrl.hasReferenzAttribute() && !warenkorbArtikelUpdateCtrl.attributwertEingabenValid())">
			<span title="Update" class="fa fa-refresh"
				  ng-class="{'fa-spin': warenkorbArtikelUpdateCtrl.isLoading}"></span>
		</button>
		<span ng-show="warenkorbArtikelUpdateCtrl.showUpdateButtons">|</span>
		<button class="text-only"
				ng-show="warenkorbArtikelUpdateCtrl.showUpdateButtons"
				ng-click="warenkorbArtikelUpdateCtrl.artikelEntfernen()"
				ng-disabled="warenkorbArtikelUpdateCtrl.isLoading">
			<span title="Remove item" class="fa fa-remove"></span>
		</button>
	</div>
</script>
<script type="text/ng-template" id="suchergebnis.html">
	<h3>Search results</h3>
	<div style="width: 100%; min-height:80vh;">
		<form name="produktsuche">
			<div class="select2-style-input icon-button" ng-class="{'error': (produktsuche.q.$error.minlength) }">
				<input type="text" name="q"
					   ng-disabled="produktsucheCtrl.isLoading"
					   ng-model="produktsucheCtrl.q"
					   ng-keyup="$event.keyCode === 13 && produktsucheCtrl.search()"
					   ng-minlength="produktsucheCtrl.minLength"
					   ng-trim="true"
					   focus-me="produktsucheCtrl.focusInputElement"
				/>
				<span style="position: absolute; top: 2px; right: 35px;"
					  ng-show="produktsuche.q.$error.minlength">
					<i title="Enter at least 2 letters or a number." class="fa fa-warning"></i>
				</span>
				<div class="select2-style-button-with-input">
					<a href="" class="button linearize search-icon no-text"
					   ng-class="{ 'error': (produktsuche.q.$error.minlength) }"
					   ng-disabled="produktsucheCtrl.isLoading"
					   ng-click="produktsucheCtrl.search()"
					></a>
				</div>
			</div>
            <div>
                <div class="jquery-tabs tab-suche-result" style="margin: 0 0 12px 0">
                    <h4 class="tab-head">Other  ({{produktsucheCtrl.addSearch.totalCount}})</h4>
                    <div class="tab-content">
                        <div class="max-height-scroll" id="addSearchResult" style="max-height: 71vh !important;">

                            <div ng-if="produktsucheCtrl.addSearch.items.length === 0 && !produktsucheCtrl.isLoading">
                                We could not find the content you are searching.
                            </div>
                            <div ng-show="produktsucheCtrl.addSearch.items.length > 0">
                                <div
                                    infinite-scroll="produktsucheCtrl.addSearchNextPage()"
                                    infinite-scroll-distance="1"
                                    infinite-scroll-container="'#addSearchResult'">
                                    <div ng-repeat="addSearchItem in produktsucheCtrl.addSearch.items track by addSearchItem.id">
                                        <div class="ym-grid ym-grid-col-padding linearize-level-1">
                                            <div class="ym-g20 ym-gl">
                                                <div class="ym-gbox">
                                                    <a ng-href="{{addSearchItem.url}}">
                                                        <img ng-src="{{addSearchItem.images.main}}" alt="" />
                                                    </a>
                                                </div>
                                            </div>
                                            <div class="ym-g80 ym-gl">
                                                <div class="ym-gbox">
                                                    <a ng-href="{{addSearchItem.url}}">
                                                        <h4 ng-bind-html="addSearchItem.title"></h4>
                                                    </a>
                                                    <p class="pdefault" ng-bind-html="addSearchItem.highlight"></p>
                                                </div>
                                            </div>
                                        </div>
                                        <div ng-if="!$last" class="divider">
                                            <div style="border-bottom: 1px solid #00a4a4">
                                                <hr />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <h4 class="tab-head">Workshop equipment ({{produktsucheCtrl.produktgruppen.length}})</h4>
                    <div class="tab-content">
                        <div class="item-viewport-gallery max-height-scroll" style="max-height: 71vh !important;">
                            <div ng-if="produktsucheCtrl.produktgruppen.length === 0 && !produktsucheCtrl.isLoading" style="margin: 6px">
                                We could not find the content you are searching.
                            </div>
                            <ul ng-show="produktsucheCtrl.produktgruppen.length > 0">
                                <li class="produktgruppe" ng-repeat="produktgruppe in produktsucheCtrl.produktgruppen">
                                    <div class="item-viewport-tile">
                                        <div class="item-viewport-tile-content">
                                            <div class="image-wrapper">
                                                <a ecom-shop-link-handler ng-href="{{produktgruppe.url}}">
                                                    <img ng-src="{{produktgruppe.images.PIC_PRO_SET.MS || produktgruppe.images.PIC_PRO_SNG_STD.MS}}" alt="" />
                                                    <span class="square-hover"><!-- --></span>
                                                </a>
                                            </div>
                                            <div class="product-slide-text-wrapper">
                                                <div class="product-slide-text">
                                                    <h5>
                                                        <a ecom-shop-link-handler ng-href="{{produktgruppe.url}}">{{produktgruppe.produktname}}</a>
                                                    </h5>
                                                    <p>{{produktgruppe.bezeichnung}}</p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
		</form>
	</div>
</script>
