<script type="text/ng-template" id="meusburger/medienservice/merkliste/merkliste.html">
    <div id="media-cart" class="inline-popup inline-caption mfp-with-anim mfp-hide">
        <div class="grid-element-wrapper">
            <h3>Watchlist</h3>
            <div class="inline-popup-content-wrapper">
                <div ng-if="!msMerklisteCtrl.isMerklisteEmpty()">
                    <h2>Your selection</h2>
                    <div class="boxgroup-wrapper linearize-level-1">
                        <div class="ym-grid linearize-level-1 no-grid-padding">
                            <div class="ym-g33 ym-gl">
                                <div class="ym-gbox">
                                    <strong>Language</strong>
                                    <ui-select
                                            ng-model="msMerklisteCtrl.selectedSprachen"
                                            ng-disabled="msMerklisteCtrl.isAusgabeartfilterActive()"
                                            multiple theme="select2"
                                            on-remove="msMerklisteCtrl.removeMultipleMediafilesFromMerklisteBySprache($item)"
                                            on-select="msMerklisteCtrl.addMultipleMediafilesToMerklisteBySprache($item)"
                                            msb-ms-ui-select-multiple-placeholder-always-visible>
                                        <ui-select-match placeholder="Select">{{$item.spracheBezeichnung}}</ui-select-match>
                                        <ui-select-choices repeat="sprache in msMerklisteCtrl.sprachen | filter:$select.search">
                                            <span>{{sprache.spracheBezeichnung}}</span>
                                        </ui-select-choices>
                                    </ui-select>
                                </div>
                            </div>
                            <div class="ym-g33 ym-gl">
                                <div class="ym-gbox"></div>
                            </div>
                            <div class="ym-g33 ym-gr">
                                <div class="ym-gbox">
                                    <ul class="checkbox-radio-group">
                                        <li><strong>Output type</strong></li>
                                        <li ng-repeat="ausgabeart in msMerklisteCtrl.ausgabearten track by ausgabeart.ausgabeart">
                                        <span class="float-col">
                                            <span class="checkbox float-left">
                                                <input type="checkbox" id="ms-merkliste-ausgabeart-checkbox-{{ausgabeart.ausgabeart}}"
                                                       ng-disabled="msMerklisteCtrl.isSprachfilterActive()"
                                                       ng-model="ausgabeart.selected"
                                                       ng-change="msMerklisteCtrl.toggleMultipleMediafilesInMerklisteByAusgabeart(ausgabeart)" />
                                                <label for="ms-merkliste-ausgabeart-checkbox-{{ausgabeart.ausgabeart}}"
                                                       ng-class="{ 'checkbox-label-disabled': msMerklisteCtrl.isSprachfilterActive() }"
                                                ></label>
                                            </span>
                                            <span class="float-col float-col-text">{{ausgabeart.ausgabeartBezeichnung}}</span>
                                        </span>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                    <h4><span class="head">Media elements</span></h4>
                    <div ng-repeat="mediaelement in msMerklisteCtrl.merkliste track by mediaelement.id">
                        <msb-ms-mediafiles-selection merkliste-mediaelement="mediaelement"></msb-ms-mediafiles-selection>
                        <div class="divider">
                            <div>
                                <hr />
                            </div>
                        </div>
                    </div>
                    <div class="boxgroup-wrapper linearize-level-2">
                        <div class="ym-grid linearize-level-2 no-grid-padding">
                            <div class="ym-g33 ym-gl">
                                <div class="ym-gbox">
                                    <p class="select2-style-button-box">
                                        <a href="javascript:void(0)" class="button-small linearize cancel-icon"
                                           ng-class="msMerklisteCtrl.hasMediaelementsOnMerkliste() ? 'button' : 'button-inactive'"
                                           ng-click="msMerklisteCtrl.clearMerkliste()">Empty watchlist</a>
                                    </p>
                                </div>
                            </div>
                            <div class="ym-g66 ym-gr">
                                <div class="ym-gbox">
                                    <p class="select2-style-button-box-right">
                                        <!-- <a href="javascript:void(0)" class="button button-small linearize envelope-icon open-popup">lbl_merkliste_versenden</a> -->
                                        <span class="button-small linearize download-icon"
                                              ng-class="msMerklisteCtrl.hasMediafilesOnMerkliste() ? 'button' : 'button-inactive'"
                                              ng-click="msMerklisteCtrl.downloadMediafiles()">Download selected elements</span>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div ng-if="msMerklisteCtrl.isMerklisteEmpty()">
                    No entries exist
                </div>
            </div>
        </div>
    </div>
</script>
<script type="text/ng-template" id="meusburger/medienservice/merkliste/mediafiles-selection/ms-mediafiles-selection.html">
    <div class="boxgroup-wrapper linearize-level-1">
        <div class="ym-grid linearize-level-1 no-grid-padding">
            <div class="ym-g33 ym-gl">
                <div class="ym-gbox">
                    <h5>{{msMediafilesSelectionCtrl.merklisteMediaelement.name}}</h5>
                    <div class="image-wrapper">
                        <img alt="{{msMediafilesSelectionCtrl.merklisteMediaelement.name}}" ng-src="{{msMediafilesSelectionCtrl.merklisteMediaelement.imageUrl}}" />
                    </div>
                </div>
            </div>
            <div class="ym-g66 ym-gr">
                <div class="ym-gbox">
                    <div class="max-height-scroll-medienservice">
                        <table class="display responsive" style="width: 100%;"
                               datatable="ng" dt-options="msMediafilesSelectionCtrl.dtOptions">
                            <thead>
                                <tr>
                                    <th></th>
                                    <th class="all no-sort"><span class="download-icon checkbox-table-head"></span></th>
                                    <th class="all"><span>Output type</span></th>
                                    <th class="desktop b1280 b1024 b850 b768 b480"><span>Language</span></th>
                                    <th class="desktop b1280 b1024"><span>File type</span></th>
                                    <th class="desktop b1280"><span>File size</span></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="mediafile in ::msMediafilesSelectionCtrl.merklisteMediaelement.mediendateien">
                                    <td></td>
                                    <td>
                                        <span class="checkbox">
                                            <input type="checkbox" id="ms-merkliste-mediafile-checkbox-{{msMediafilesSelectionCtrl.merklisteMediaelement.id}}-{{$index}}"
                                                   ng-model="mediafile.onMerkliste"
                                                   ng-change="msMediafilesSelectionCtrl.toggleMediafileInMerkliste(mediafile)" />
                                            <label for="ms-merkliste-mediafile-checkbox-{{msMediafilesSelectionCtrl.merklisteMediaelement.id}}-{{$index}}"><!-- --></label>
                                        </span>
                                    </td>
                                    <td>{{::mediafile.ausgabeartBezeichnung}}</td>
                                    <td>{{::mediafile.spracheBezeichnung}}</td>
                                    <td>{{::mediafile.suffix}}</td>
                                    <td>{{::mediafile.dateigroesse | msbMsFilesize}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <p class="select2-style-button-box-right">
                        <a href="javascript:void(0)" class="button button-small linearize ok-icon"
                           ng-click="msMediafilesSelectionCtrl.toggleMultipleMediafilesInMerkliste()">Select all output types</a>
                        <a href="javascript:void(0)" class="button button-small linearize cancel-icon"
                           ng-click="msMediafilesSelectionCtrl.removeMediaelementFromMerkliste()">Remove media element</a>
                    </p>
                </div>
            </div>
        </div>
    </div>
</script>
