                window.location.href = spotQuery + "?spotName=" + encodeURI($('#searchSpotName').val());
            }, 200);
        });

        //모바일용
        

        //검색창 활성화
        $('#searchSpotName').on('blur', function(){
         $('.search').removeClass('focus');
         }).on('focus', function(){
         $('.search').addClass('focus');
        });

        //articleForm 내부 초기화
        initialFunctions();
    });
</script>
</body>
</html>