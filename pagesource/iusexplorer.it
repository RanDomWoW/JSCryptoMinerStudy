    // NON FARE SEND DI PAGEVIEW IN CASO DI SET DI DIMENSION COME PER LE VIEW DEI DOCUMENTI 
    $(function () {
        if (!UAUtil.TrackViewSent) {
            if (UAUtil.UID != "") ga('set', '&uid', UAUtil.UID);

            if (!UAUtil.ToTrack(document.location.search.toLowerCase())) return;
           
            var pathname = document.location.pathname.substring(1);
            var controller = pathname.substring(0, pathname.indexOf("/")).toLowerCase();
            var page = document.location.pathname;
            if ((controller == "giurisprudenza" || controller == "fontinormative" || controller == "dottrina" || controller == "bibliografia" || controller == "formulari" || controller == "codiciipertestuali" || controller == "codicicommentati" || controller == "formularicommentati") && $("#pid").val() != "10")
                page = "/Dejure" + document.location.pathname;


            // workaround per risolvere gli url sotto portale publica
            var path = document.location.pathname.toLowerCase();
            if ($("#pid").val() == "10" && path.indexOf("publica") == -1) {
                if (path.indexOf("ricerca") != -1) 
                    page = "/Publica/" + controller + "/Ricerca";
                else
                    page = "/Publica" + document.location.pathname;
            }
            
            // Riscrivo gli url delle maschere di ricerca specialistiche RicercaGiurisprudenza in Giurisprudenza/Ricerca etc...
            // Usato per le maschere dei portali che sono indicati come /RicercaGiurisprudenza /RicercaFontiNormative etc...
            var regex = new RegExp(/Ricerca(([A-Z]|[a-z]){1,})/i);
            if (regex.test(page)) {
                var archivio = regex.exec(page)[1].toLowerCase();
                page = page.replace(/Ricerca(([A-Z]|[a-z]){1,})/i, "$1/Ricerca").toLowerCase();
                page = page.replace(archivio + "/" + archivio, archivio);  // questa serve per togliere un eventuale ripetizione dell'archivio sulla pagina, succede quando sei sotto un controller dejure e finisci in RicercaGiurisprudenza ad esempio se cerchi senza una fulltext
            }

            page = page.toLowerCase();
            page = page.replace("/rivista", "/riviste/rivista");

            ga('send', 'pageview', { 'page': page });
        }
        
        // mettere dei blocchi di controllo di errore tipo trycatch
        $(document).delegate("a", "click", function () {
            if ($(this).attr("href") == undefined) return;
            if ($(this).attr("data-ga-no-exit") != undefined) return;
            var url = $(this).attr("href").toLowerCase();
            if (url.indexOf("javascript") != -1) return;
            var currentUrl = 'iusexplorer.it';
            currentUrl = currentUrl.toLowerCase();
            if (url.indexOf(currentUrl) != -1 || url.indexOf("/") == 0 || url.indexOf("http") == -1) return;
            var title = $(this).text();
            if (title.length > 50) title = title.substring(0, 50);
            if (UAUtil.UID != "") ga('set', '&uid', UAUtil.UID);
            ga('send', 'pageview', { 'page': '/exit/' + url.replace("https://", "").replace("http://", "").replace("www.", ""), 'title': title });
        });

        $("#fTipoCont :checkbox").one("click", function() { UAUtil.TipologiaContenutiChanged = true; });

        // in caso di modifica della ricerca, controllo se l'utente aveva già interagito con le tipologie di contenuti altrimenti non potrei saperlo 
        // nel reset del searchprotocol il valore di session.InterazioniRicerca viene azzerato
        

        $("form.maschera").on("submit", function () {
            var fieldsets = $("form.maschera fieldset");
            var interazioni = "";
            for (var i = 0; i < fieldsets.length; i++) {
                var elements = $(fieldsets[i]).find("select, input:not(:checkbox):not(:hidden)").filter(function () { return this.value; });
                if (elements.length != 0 && $(fieldsets[i]).find(".maschera-fieldset-header").length != 0)
                    interazioni += "|" + $(fieldsets[i]).find(".maschera-fieldset-header:visible").text();
            };

            if ($("#lemmSelected").html() != "" && $("#lemmSelected").html() != undefined && interazioni.toLowerCase().indexOf("classificazione") == -1) interazioni += "|Classificazione";
            if ($("#provvTestoInGu").is(":checked")) interazioni += "|Provvedimento";
            if (UAUtil.TipologiaContenutiChanged) interazioni += "|Tipologie di contenuti";
            if ($("input[name='FullText']") != undefined && $("input[name='FullText']").val() != "") interazioni += "|Testo";
            if (interazioni != "") interazioni += "|";

            if (interazioni != "") {
                $("#interazioni").remove();
                $("form.maschera").append("<input type='hidden' name='interazioni' id='interazioni' value='" + interazioni + "' />");
            }
        });

    });


</script>



