        }

        // apre i menu

        function getChildren(n, skipMe) {
            var r = [];
            for (; n; n = n.nextSibling)
                if (n.nodeType == 1 && n != skipMe)
                    r.push(n);
            return r;
        }

        function getSiblings(n) {
            return getChildren(n.parentNode.firstChild, n);
        }

        function toggleClass(className, elem) {
            var added = false;
            if (elem.className)
                var classArr = elem.className.split(" ");
            var pos = classArr.indexOf(className);
            if (pos > -1) {
                classArr.splice(pos, 1);
                added = false;
            }
            else {
                classArr.push(className);
                added = true;
            }
            elem.className = classArr.join(' ');
            return added;
        }

        function removeClass(className, elem) {
            if (elem.className)
                var classArr = elem.className.split(" ");
            var pos = classArr.indexOf(className);
            if (pos > -1) {
                classArr.splice(pos, 1);
                elem.className = classArr.join(' ');
            }
        }

        //var elem = document.getElementsByClassName("nu-has-sub");
        var elem = $(".nu-has-sub");
        for (var i = 0; i < elem.length; i++) {
            elem[i].onclick = function () {
                if (toggleClass('nu-opened', this.parentElement)) {
                    var siblings = getSiblings(this.parentElement);
                    for (var j = 0; j < siblings.length; j++)
                        removeClass('nu-opened', siblings[j]);
                }
            };
        }

    });

</script>

