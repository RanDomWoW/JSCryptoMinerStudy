<script>
    function LoginByIp() {
        ShowWait();
        var user = $("#id-cliente").val();
        var pwd = $("#password").val();
        var remamber = $("#rememberMe").is(":checked");
        var url = "/Access/LoginByIp?" + "userid=" + user + "&password=" + pwd + "&remamberMe=" + remamber;
        ga('send', 'event', 'IPLogin', 'Tentativo', document.location.pathname.toLowerCase(), { 'nonInteraction': 1 });
        $.post('/Access/LoginByIp', { userid: user, password: pwd, remamberMe: remamber },
                function (data) {
                    CheckLoginByIpResult(data);
                    if (data.success) {
                        var address = window.location.href;
                        var indexExt = address.indexOf(".it");
                        var extension = "";
                        if (address.charAt(indexExt + 3) == ".") {
                            extension = address.substring(indexExt + 3);
                            extension = extension.slice(0, extension.indexOf("/"));
                            window.location = data.returnUrl.substring(0, data.returnUrl.lastIndexOf("/")) + extension + "/";
                        }
                        else
                            window.location = data.returnUrl;
                    }
                    else {
                        ShowLoginByIpFailed(data.errorMessage);
                        HideWait();
                    }
                });
    }

    $(document).ready(function () {
        var loginRequestRunning = false;
        $("#loginForm").submit(function (e) {
            if (loginRequestRunning) return false;
            loginRequestRunning = true;
            e.preventDefault();
            ShowWait();
            $.post('/Access/Login', { userid: $("#id-cliente").val(), password: $("#password").val(), remamberMe: $("#rememberMe").is(":checked") },
               function (data) {
                   ga('send', 'event', 'Login', 'Tentativo', document.location.pathname.toLowerCase(), { 'nonInteraction': 1 });
                   loginRequestRunning = false;
                   if (data.alreadylogged) {
                       window.location.reload();
                       return;
                   }
                     
                   if (data.success) {
                       $("#ssousername").val(data.ssoData.SSOusername);
                       $("#ssopassword").val(data.ssoData.SSOpassword);
                       $("#site2pstoretoken").val(data.ssoData.Site2pstoretoken);
                       $("#flogin").attr("action", data.ssoData.SzAuthRedirect);
                       $("#flogin").submit();
                   } else {
                       ShowLoginByIpFailed(data.ssoData.ValidationDescription);
                       HideWait();
                   }
               });
        });
         

        $('#loginForm input').keyup(function (e) {
            if (e.keyCode == 13) {
                $("#loginForm").submit();
            }
        });

        

        //$("#passwordRecover").click(function() { if (UAUtil.UID != "") ga('set', '&uid', UAUtil.UID); ga('send', 'pageview', { 'page': '/Login/RecuperaPassword', 'title':'Recupera Password' }); });
        //$("#gestioneLicenze").click(function() { if (UAUtil.UID != "") ga('set', '&uid', UAUtil.UID); ga('send', 'pageview', { 'page': '/Login/GestioneLicenze', 'title':'Gestione Licenze' }); });
        
    });


    function CheckLoginByIpResult(data) {
        if (!data.success) {
            if (data.errorcode == 3)
                ga('send', 'event', 'IPLogin', 'ACCESSI ESAURITI', document.location.pathname.toLowerCase(), { 'nonInteraction': 1 });
            if (data.errorcode == 5)
                ga('send', 'event', 'IPLogin', 'LICENZA NON ESISTENTE', document.location.pathname.toLowerCase(), { 'nonInteraction': 1 });
            if (data.errorcode == 2 || data.errorcode == 1)
                ga('send', 'event', 'IPLogin', 'LICENZA SCADUTA', document.location.pathname.toLowerCase(), { 'nonInteraction': 1 });
        }
    }

