    <script type="text/javascript">
        function checkTime(i) {
            if (i < 10) {
                i = "0" + i;
            }
            return i;
        }

        function createDateText(d) {
            seconds = d.getSeconds().toString().length == 1 ? '0' + d.getSeconds() : d.getSeconds();
            minutes = d.getMinutes().toString().length == 1 ? '0' + d.getMinutes() : d.getMinutes();
            hours = d.getHours().toString().length == 1 ? '0' + d.getHours() : d.getHours();
            ampm = d.getHours() >= 12 ? 'pm' : 'am';
            months = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];
            days = ['Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat'];
            var curTime = d.getDate() + ' ' + months[d.getMonth()] + ' ' + hours + ':' + minutes; // + ':' + seconds + ampm;
            //var curTime = days[d.getDay()] + ' ' + d.getDate() + ' ' + months[d.getMonth()] + ' ' + d.getFullYear() + ' &nbsp;&nbsp;&nbsp; ' + hours + ':' + minutes; // + ':' + seconds + ampm;
            return curTime;
        }

        function startTime() {
            var d = new Date();
            var localTime = d.getTime();
            var localOffset = d.getTimezoneOffset() * 60000;
            var utc = localTime + localOffset;

            var mel = new Date(utc + (3600000 * 11));
            var teh = new Date(utc + (3600000 * 3.5));
            var van = new Date(utc + (3600000 * -6));

            document.getElementById('timeMel').innerHTML = createDateText(mel);
            document.getElementById('timeTeh').innerHTML = createDateText(teh);
            document.getElementById('timeVan').innerHTML = createDateText(van);

            t = setTimeout(function () {
                startTime()
            }, 20000);
        }
        startTime();

