    .global_site ul li a:hover {color:rgba(255,255,255,0.9); text-shadow:0 0 #021344;  }
    .message {position:absolute; bottom:0; width:100%; background:rgba(0,0,0,0.2); }
    .message p {text-align:center; margin:0; padding:20px 0; font-size:1.3em; font-weight:200; color:rgba(3,73,133,1); }
    .message p br {display:none; }


    .transparantIE {color: white;}
    .transparantSTD {color: transparent;}

    #ourmission {
        font-family:Nanum Barun Gothic;
        font-size:1.0em;
        padding-top:160px;
        width: 350px;
        margin:0 auto;
        text-align:center;
        -webkit-animation: none;
        -moz-animation: none;
        -ms-animation: none;
        animation: none;
        opacity: 1;
        /*color: transparent;*/
        text-shadow: 0px 0px 1px #fff;
    }

    #ourmission span{
        opacity: 0;
    }


    .startKiller {
        -webkit-animation-name: blurFadeIn; /* Chrome, Safari, Opera */
        -webkit-animation-duration: 300s; /* Chrome, Safari, Opera */
        -webkit-animation-iteration-count: infinite; /* Chrome, Safari, Opera */
        animation-name: blurFadeIn;
        animation-duration: 300s;
        animation-iteration-count: infinite;
    }

    .startTheEnd {
        font-family:Nanum Barun Gothic;
        font-size:22px;
        /*-webkit-animation: blurFadeInOut 3s ease-in 4s backwards;
        -moz-animation: blurFadeInOut 3s ease-in 4s backwards;
        -ms-animation: blurFadeInOut 3s ease-in 4s backwards;
        animation: blurFadeInOut 3s ease-in 4s backwards;*/
        -webkit-animation: blurFadeIn;
        -moz-animation: blurFadeIn;
        -ms-animation: blurFadeIn;
        animation: blurFadeIn;
        -webkit-animation-duration: 300s;
        animation-duration: 300s;
        -webkit-animation-iteration-count: infinite; /* Chrome, Safari, Opera */
        animation-iteration-count: infinite;
        /*color: transparent;*/
        /*color: white;*/ /* IE는 color: 속성에 transparent 값을 사용하면 글자가 아예 투명해져버린다.  */
        opacity: 0.8;
        text-shadow: 0px 0px 1px #fff;
        z-index:10;
    }


    /* Chrome, Safari, Opera */
    @-webkit-keyframes startKiller {
        0%   {opacity:0;}
        50% {opacity:0.9;}
        100% {opacity:1.0;}
    }
    /* Standard syntax */
    @keyframes startKiller {
        0%   {opacity:0;}
        50% {opacity:0.9;}
        100% {opacity:1.0;}
    }
    /* Chrome, Safari, Opera */
    @-webkit-keyframes blurFadeIn{
        0%{
            opacity: 0;
            text-shadow: 0px 0px 40px #fff;
            -webkit-transform: scale(1.0);
        }
        1%,5%{
            opacity: 0.9;
            text-shadow: 0px 0px 1px #fff;
            -webkit-transform: scale(1.0);
        }
        100%{
            opacity: 1.0;
            text-shadow: 0px 0px 0px #fff;
            -webkit-transform: scale(1.0);
        }
    }

    @keyframes blurFadeIn{
        0%{
            opacity: 0;
            text-shadow: 0px 0px 40px #fff;
            transform: scale(1.0);
        }
        1%,5%{
            opacity: 0.9;
            text-shadow: 0px 0px 1px #fff;
            transform: scale(1.0);
        }
        100%{
            opacity: 1.0;
            text-shadow: 0px 0px 0px #fff;
            transform: scale(1.0);
        }
    }


    @-webkit-keyframes blurFadeInOut{
        0%{
            opacity: 0;
            text-shadow: 0px 0px 40px #fff;
            -webkit-transform: scale(1.0);
        }
        5%,95%{
            opacity: 0.9;
            text-shadow: 0px 0px 1px #fff;
            -webkit-transform: scale(1.0);
        }
        100%{
            opacity: 0;
            text-shadow: 0px 0px 50px #fff;
            -webkit-transform: scale(1.0);
        }
    }

    @keyframes blurFadeInOut{
        0%{
            opacity: 0;
            text-shadow: 0px 0px 40px #fff;
            transform: scale(1.0);
        }
        5%,95%{
            opacity: 0.9;
            text-shadow: 0px 0px 1px #fff;
            transform: scale(1.0);
        }
        100%{
            opacity: 0;
            text-shadow: 0px 0px 50px #fff;
            transform: scale(1.0);
        }
    }


    /*  Media Queries 768 */
    @media only screen and (max-width: 828px) {
