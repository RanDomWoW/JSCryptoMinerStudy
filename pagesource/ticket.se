<script type="text/javascript">
    <!--
    	var externalMinDate_CALENDAR = new Date().getDate() + 0;
    	var externalMaxDate_CALENDAR = new Date().getDate() + 350;
    	var externalMinDate_CAR = new Date().getDate() + 3;
