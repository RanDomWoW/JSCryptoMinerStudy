            $(this).on("blur", function() {
                var menu = $(this).data('ui-autocomplete').menu;
                var el = $(menu.element);

                if (el.is(':visible')) {
                    if ($(".ui-state-focus", el).length == 0) {
                        menu.focus(null, el.find(".ui-menu-item:first"));
                    }
                    menu.select();
                    resetDestinationField(this);
