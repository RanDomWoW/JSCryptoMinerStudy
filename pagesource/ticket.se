                        .append( "<a>"+ item.label + "</a>" )
                        .appendTo( ul );
            };
            $(this).keydown(function (event) {
                var e = event.originalEvent;
                if (e.keyCode == $.ui.keyCode.ESCAPE) {
                    $(this).val("");
                    $(this).autocomplete("widget").hide();
                }
