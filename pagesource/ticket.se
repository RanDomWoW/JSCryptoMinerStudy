<script type="text/javascript">
    function readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1, c.length);
            }
            if (c.indexOf(nameEQ) == 0) {
                return c.substring(nameEQ.length, c.length);
            }
        }
        return null;
    }

    function isMobileDevice() {
        return (navigator.userAgent.match(/Android/i) && !navigator.userAgent.match(/Android 1/i) && !navigator.userAgent.match(/Android 2/i) && !navigator.userAgent.match(/Android 3/i) && navigator.userAgent.match(/Mobile/i))
                || navigator.userAgent.match(/webOS/i)
                || navigator.userAgent.match(/iPhone/i)
                || navigator.userAgent.match(/iPod/i)
                || navigator.userAgent.match(/BlackBerry/i)
                || navigator.userAgent.match(/Windows Phone/i);
    }

    if (location.search && location.search.toLowerCase().indexOf('site_preference=normal') != -1) {
        // Set SitePreference cookie
        var d = new Date();
        d.setTime(d.getTime() + (30 * 60 * 1000));
        document.cookie = "site_preference=NORMAL; expires=" + d.toUTCString();
    }

    try {
        if (isMobileDevice()
                && (document.cookie.indexOf("site_preference") == -1 || readCookie("site_preference") != "NORMAL")
                && location.hostname.match(/^www\./)) {
            var mobilePathName = location.pathname;
            var pathNamesJson = {"sista-minuten.html": "/sista-minuten/","charter.html": "/charter/"};
            if (pathNamesJson[location.pathname]) {
                mobilePathName = pathNamesJson[location.pathname];
            }
            // Redirect to mobile
            var mobileHostname = location.hostname.replace(/^www/, 'm');
            location.href = location.protocol + '//' + mobileHostname + mobilePathName + location.search;
        }
    } catch (err) {
    }
</script>
