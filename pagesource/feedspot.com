function postRPC(r,url,params,method){
	var m = "POST";
	if(method=="GET"){m = method;}
	if(m=="GET"){ 
		url = url+'?'+params;
	}
	r.open(m, url, true);	
	r.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
	r.send(params);
}

function myAjax(){
	if(window.XMLHttpRequest){
