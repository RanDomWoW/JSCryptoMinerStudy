		<script>
		/**
		 * Poll for wallpaper ad; once one is discovered, add the class and stop polling
		 * A wallpaper ad is detected when the body element has a background
		 * image that is not on the same domain (location.hostname) as the current page.
		 */
		(function(){
			var interval_ms = 250;
			var class_name = "wallpaper-ad";
			var interval_id;
			var computed_style = (function (el) {
				if (!!document.defaultView && !!document.defaultView.getComputedStyle) {
					return document.defaultView.getComputedStyle(el, null);
				}
				else if (el.currentStyle) {
					return el.currentStyle;
				}
				else {
					return null;
				}
			}(document.body));
			if (!computed_style) {
				return;
			}

			var parse_hostname = (function () {
				var link = document.createElement('a');
				return function (url) {
					link.href = url;
					return link.hostname;
				};
			}());

			var check_for_wallpaper_ad = function () {
				var matches = computed_style.backgroundImage.match(/url\(['"]?(.*?)['"]?\)/);
				if (matches && parse_hostname(matches[1]) !== location.hostname) {
					document.body.className += ' ' + class_name;
					computed_style = null;
				}
				else {
					setTimeout(check_for_wallpaper_ad, interval_ms);
				}
			};
			check_for_wallpaper_ad();
		}());
		</script>
