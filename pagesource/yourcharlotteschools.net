				$(".item").each(function(i) {
					if($(this).height() > CarouselHeight) {
						CarouselHeight = $(this).height();
					}
				}).css('min-height', CarouselHeight + "px");
			}
			$(document).ready(function() {
				$('.carousel').carousel({
					interval: 8000
				});
