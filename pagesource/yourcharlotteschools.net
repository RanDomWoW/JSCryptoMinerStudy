	<script type="text/javascript">
		var ApplicationDomain = "http://yourcharlotteschools.net/";
		function QuickLinksHeight() {
			var container = $("#QuickLinks>.panel-heading").height(),
					links = $("#QuickLinksMenu").height();
			$("#QuickLinks").css('height', (container + links + 30) + 'px');
			$("#QuickLinksMenu").css('top', ($("#QuickLinks .panel-heading").height() + 25) + 'px');
		}
		function checkCollapse() {
			if(window.innerWidth > 991) {
				$("#Calendar").collapse('show');
				$("#News").collapse('show');
			} else {	
				$("#Calendar").collapse('hide');
				$("#News").collapse('hide');
			}
		}
		function resizeElements() {
			$('#News').css('max-height', "none");
			$('#Calendar').css('max-height', "none");
			$("#News ul").css('padding-bottom', '0');
			$("#NewsContainer>.panel").css('height', 'auto');
			$("#QuickLinks").css('height', 'auto');
			QuickLinksHeight();
			$("#CalendarBox").css('height', 'auto');
			if(window.innerWidth > 991) {
				var LinksHeight = $("#LinksContainer").height(),
						LinksHeaderHeight = $("#QuickLinks > .panel-heading").height(),
						EventsHeight = $("#CalendarContainer").height(),
						EventsHeaderHeight = $("#CalendarBox > .panel-heading").height(),
						NewsHeight = $("#NewsContainer").height(),
						NewsHeaderHeight = $("#NewsContainer > .panel > .panel-heading").height(),
						NewsFooterHeight = $("#NewsContainer > .panel > .panel-footer").height();
				if(NewsHeight < LinksHeight) {
					$('#News').css('height', (((LinksHeight - NewsHeaderHeight) - NewsFooterHeight) - 30) + "px");
				} else {
					$('#News').css('max-height', (((LinksHeight - NewsHeaderHeight) - NewsFooterHeight) - 30) + "px");
				}
				if(EventsHeight < LinksHeight) {
					$('#Calendar').css('height', ((LinksHeight - EventsHeaderHeight) - 23) + "px");
				} else {
					$('#Calendar').css('max-height', ((LinksHeight - EventsHeaderHeight) - 23) + "px");
				}
				
				if($("#News ul").height() < $("#News").height()) {
					$("#NewsContainer").removeClass('noscroll').removeClass('scroll').addClass('noscroll');
					$("#News ul").css('padding-bottom', '0');
				} else {
					$("#NewsContainer").removeClass('scroll').removeClass('noscroll').addClass('scroll');
					$("#News ul").css('padding-bottom', '15px');
				}
				
				if($("#Calendar ul").height() < $("#Calendar").height()) {
					$("#CalendarBox").removeClass('noscroll').removeClass('scroll').addClass('noscroll');
					$("#Calendar ul").css('padding-bottom', '0');
				} else {
					$("#CalendarBox").removeClass('scroll').removeClass('noscroll').addClass('scroll');
					$("#Calendar ul").css('padding-bottom', '20px');
				}
				$('#LinksContainer').insertBefore('#NewsContainer');
				$('#QuickLinksMenu').insertAfter('#LinksContainer');
			} else {
				$('#QuickLinksMenu').appendTo('#QuickLinks .panel-body');
				$('#LinksContainer').insertAfter('#CalendarContainer');
			}
			checkCollapse();
			checkCollapse = function() {
				if(window.innerWidth > 991) {
					$("#Calendar").collapse('show');
					$("#News").collapse('show');
				}
			};
		}
		$(document).ready(function() {
			loadEvents();
			$(window).resize(function() {
				resizeElements();
			});
			$('#google_translate_element').tooltip({placement: 'top', container: '#Footer', template: '<div class="tooltip" role="tooltip"><div class="tooltip-arrow"></div><div class="tooltip-inner footer-tooltip-inner"></div></div>'});
			/**
			 * Select All Elements with Class of well that have a heading as their first child.
			 * Remove top padding.
			 */
			$('.well h1:first-child').parent().css('padding-top', '0');
			$('.well h2:first-child').parent().css('padding-top', '0');
			$('.well h3:first-child').parent().css('padding-top', '0');
			
			/* Append Timestamp to the end of all URL's that lead to PDF Files to prevent browser caching from displaying old files */
			var linkTimestamp = new Date()
			$('a').mouseover(function(){
				var url = $(this).attr('href');
				var extension = url.substr( (url.lastIndexOf('.') +1) );
				if (extension == 'pdf') {
					$(this).attr('href', url + '?t=' + linkTimestamp.getTime());
				}
			});
			/* Scroll to content when tab is clicked */
			$('a[data-toggle="tab"]').on('shown.bs.tab', function(e) {
				var container = $(this).attr('aria-controls');
				$('html, body').animate({
					scrollTop: $('#' + container).offset().top
				}, 500);
			});
			/* Hide Mobile Menu after link in menu is clicked */
			$('.sidebar').on('click', 'a:not(.dropdown-toggle)', function(e) {
				$('.sidebar').sidebar('hide');
			});
			$('.chevron-toggle').click(function() {
				if($(this).find('.glyphicon').hasClass('glyphicon-triangle-top')) {
					$(this).find('.glyphicon').removeClass('glyphicon-triangle-top').addClass('glyphicon-triangle-bottom');
				} else {
					$(this).find('.glyphicon').removeClass('glyphicon-triangle-bottom').addClass('glyphicon-triangle-top');
				}
			});
		});
		/**
		 * Function to load a page. This is necessary because bootstraps tabs override the default link behavior.
		 * This is used so that tabs on included files will act as normal links if the tabs are for a different page than the one the file is included in.
		 * @param {String} url URL of the page to load
		 */
		function loadPage(url) {
			window.location = url;
		}
		//bootstrap breakpoints detection
		function isBreakpoint(alias) {
			return $('.device-' + alias).is(':visible');
		}
		//lightweight window resize listener
		var waitForFinalEvent=function(){var b={};return function(c,d,a){a||(a="I am a banana!");b[a]&&clearTimeout(b[a]);b[a]=setTimeout(c,d)}}();
		var fullDateString = new Date();
	</script>
