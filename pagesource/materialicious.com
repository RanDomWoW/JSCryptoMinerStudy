	/* Image size validation script */
	$(document).ready(function(){
		$('.with-images input[type="submit"]').click( function(e) {
			e.stopImmediatePropagation();
		    //check whether browser fully supports all File API
		    if (window.File && window.FileReader && window.FileList && window.Blob)
		    {	
		    	var error_count = 0;
		    	$('input[type="file"]').each(function(index){
		    		if($(this).val().length > 0){
			    		//get the file size and file type from file input field
				        var fsize = $(this)[0].files[0].size;
				       
				        if(fsize>2097152) //do something if file size more than 1 mb (1048576)
				        {
				          error_count =+1;
				        }
				    }
		    	});
		    	if(error_count > 0){
		    		if($('.img-error').length == 0){
		    			$('.content').prepend('<div class="img-error" style="background-color:#cc3333;color:white;margin-bottom:22px;padding:6px;font-size:1.2em;">Image(s) must be less than 2 MB.</div>');
		    		}
			    	$(window).scrollTop('slow');
			    	return false;
		    	}else{
		    		$(this).submit();
		    	}
		    			        
		    }		    
		});
	});
	</script>
	
