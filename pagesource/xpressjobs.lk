                if (emailVal != "") {
                    $("#uploadcv_validation_email").hide();
                    if (validateEmail(emailVal) == false) {
                        $("#uploadcv_validation_email_format").show();
                        isValid = false;
                    } else {
                        $("#uploadcv_validation_email_format").hide();
                    }
                }
                $('#Name').on('click focusin', function () {
                    $("#uploadcv_validation_name").hide();
                });
                $('#Email').on('click focusin', function () {
                    $("#uploadcv_validation_email").hide();
                    $("#uploadcv_validation_email_format").hide();
                });
                $('#cv').on('click focusin', function () {
                    $("#uploadcv_validation_resume").hide();
                });
                if (isValid == false) {
                    e.preventDefault();
                }
            });

            $("#btnSearchJob").click(function (e) {
                $("#Action").val('Search');
                $("#Sectors").val(GetSelectedSectors());
                $("#Locations").val(GetSelectedLocations());
            });

            $.getJSON('/home/getlocations', function (data) {
                var items = "";
                $.each(data, function (i, location) {
                    items += "<strong id='" + location.LocationId + "' class='main_location'><input type='checkbox' id='" + "chbParentLocation_" + location.LocationId + "'><label>" + location.LocationName + "</label></strong><br/>";
                });
                $('#ddlLocations').html(items);
            });
            $.getJSON('/home/getsectors', function (data) {
                var items = "";
                $.each(data, function (i, sector) {
                    items += "<strong id='" + sector.SectorId + "' class='main_sector'><input type='checkbox' id='" + "chbParentSector_" + sector.SectorId + "'><label>" + sector.SectorName + "</label></strong><br/>";
                });
                $('#ddlSectors').html(items);
            });

            $(".ddcLocations").dropDownContent();
            $(".ddcSectors").dropDownContent();
            $("div.ddcLocations > div.ddc_selector").html("All Job Locations");
            $("div.ddcSectors > div.ddc_selector").html("All Job Categories");
        });



        //******* GET CHECKED ITEMS IDS FROM JQUERY AS COMMA SEPERATED*************************

        function GetSelectedLocations() {
            var locationIds = '';

            //SELECTED PARENT LOCATIONS
            $(".main_location input").each(function () {
                if ($(this).is(":checked")) {
                    var parentLocationId = $(this).parent().attr("id");
                    locationIds += parentLocationId + ',';
                }
            });

            //SELECTED SUB LOCATIONS
            $("#div_locations input").each(function () {
                if ($(this).is(":checked") && parseInt($(this).val())) {
                    locationIds += $(this).val() + ',';
                }
            });
            locationIds = locationIds.substring(0, locationIds.length - 1);
            return locationIds;
        }

        function GetSelectedSectors() {
            var sectorIds = '';

            //SELECTED PARENT LOCATIONS
            $(".main_sector input").each(function () {
                if ($(this).is(":checked")) {
                    var parentLocationId = $(this).parent().attr("id");
                    sectorIds += parentLocationId + ',';
                }
            });

            //SELECTED SUB LOCATIONS
            $("#div_sectors input").each(function () {
                if ($(this).is(":checked") && parseInt($(this).val())) {
                    sectorIds += $(this).val() + ',';
                }
            });

            sectorIds = sectorIds.substring(0, sectorIds.length - 1);
            return sectorIds;
        }

