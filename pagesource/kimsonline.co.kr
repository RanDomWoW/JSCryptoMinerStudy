            for (var i = 0; i < data.length; i++) {
                _html += "<div class=\"ph_inner_box\">";
                _html += "<div class=\"hidden\">" + data[i].PicID + "</div>";
                _html += "<a href=\"#\" onclick=\"ADClick_ProductHighlights('', '" + data[i].KIMSCode + "'); document.location.href='/drugcenter/search/druginfo/" + data[i].KIMSCode + "';\">";
                _html += "<div class=\"product_name\">" + cutByLen(data[i].ProductName, 17) + "</div>"; //GetSubstringByte(d.ProductName, 17)
                _html += "<div class=\"main_gen\">" + data[i].MainGen + "</div>";
                _html += "<div class=\"product_image\">";
                if (data[i].PicID == "") {
                    _html += "<img alt=\"\" src=\"http://www.kimsonline.co.kr/Data/DrugPictures_PH/img_ph_default.png\" class=\"phimg\" />";
                } else {
                    _html += "<img alt=\"\" src=\"http://www.kimsonline.co.kr/data/DrugPictures_PH/" + data[i].PicID + ".png\" class=\"phimg\" />";
                }
                _html += "</div><div class=\"indication\">" + cutByLen(data[i].Indication, 200) + "</div>"; //
                _html += "<div class=\"clearBoth\"></div>";
                _html += "</a></div>";
            }
        }

        $("#slides_ph").html(_html);

        $("#slides_ph").slidesjs({
            play: {
                active: false,
                effect: "slide",
                interval: 4000,
                auto: true,
                swap: true,
                pauseOnHover: true
            },
            pagination: {
                active: false
            }
        });
    }

