    //이메일주소 체크
    function btnMemberEmailChk() {
        var email = $.trim($("#txt_subs_mail").val());

        var arr_email = email.split('@');

        var id = arr_email[0];
        var url = arr_email[1];

        if (arr_email.length < 2 || id.length < 1 || url.length < 1) {
            alert("이메일을 정확하게 입력해주세요.");
            $("#txt_subs_mail").focus();
            return false;
        }
        else {
            var regex = /[0-9a-zA-Z][_0-9a-zA-Z-]*@[_0-9a-zA-Z-]+(\.[_0-9a-zA-Z-]+){1,2}$/;
            var input = StringFormat("{0}@{1}", id, url);
            if (!regex.test(input)) {
                alert("이메일을 정확하게 입력해주세요.");
                $("#txt_subs_mail").focus();
                return false;
            } else {
                return true;
            }
        }
    }

