    //학술센터 최근게시물 리스트 렌더링
    function fn_rcs_recent_list_tag(data) {
        var _html = "";
        var _new = "<img alt=\"new\" src=\"/Images/medical/main/icon_new.png\" style=\"margin-left:5px;\" />";

        if (data != null) {
            var list_recent;
            var list_recent_top;
            {
                list_recent = $(data)[0].ListRecent;

                list_recent_top = list_recent[0];

                _html += "";

                _html += "<table class=\"tbl_main_rcs\">";
                _html += "<colgroup><col width=\"60\" /><col /><col /></colgroup>";
                _html += "<tbody>";

                _html += "<tr>";
                _html += "<td class=\"pad_left10\"><img alt=\"\" src=\"/Images/medical/main/icon_" + list_recent_top.ResourceID + ".png\" /></td>";
                _html += "<td colspan=\"2\" class=\"title\">";
                _html += "<a href=\"#\" onclick=\"fn_link('" + list_recent_top.ResourceID + "', '" + list_recent_top.ContentID + "'); return false;\">";
                _html += cutByLen(list_recent_top.Title, 70);
                _html += (list_recent_top.NewCon == "N") ? _new : "";
                _html += "</a></td>";
                _html += "</tr>";
                _html += "<tr style=\"border-bottom:1px dashed #C0C0C0\">";
                _html += "<td colspan=\"2\" class=\"pad_left10\">";
                _html += "<img alt=\"\" src=\"" + fn_rcs_image_url(list_recent_top.ResourceID, list_recent_top.SubCategory, list_recent_top.ImageFile) + "\" style=\"width:92px; height:42px; margin-right:8px;\" />";
                _html += "</td>";
                _html += "<td style=\"padding-bottom:8px\">";
                _html += cutByLen(list_recent_top.Contents, 180);
                _html += "</td>";
                _html += "</tr>";

                for (var i = 1; i < list_recent.length; i++) {
                    _html += "<tr>";
                    _html += "<td class=\"pad_left10\"><img alt=\"\" src=\"/Images/medical/main/icon_" + list_recent[i].ResourceID + ".png\" /></td>";
                    _html += "<td colspan=\"2\" class=\"title\">";
                    _html += "<a href=\"#\" onclick=\"fn_link('" + list_recent[i].ResourceID + "', '" + list_recent[i].ContentID + "'); return false;\">";
                    _html += cutByLen(list_recent[i].Title, 70);
                    _html += (list_recent[i].NewCon == "N") ? _new : "";
                    _html += "</a></td>";
                    _html += "</tr>";
                    _html += "";
                }

                _html += "</tbody>";
                _html += "</table>";

                $("#list_recent").html(_html);
            }
        }
    }

    //학술센터 이미지 경로
    function fn_rcs_image_url(rcs_id, subcategory, image_file) {
        var _pre_image = "http://manage.kimsonline.co.kr/Images/";

        switch (rcs_id) {
            case "CMEA":
                _pre_image += "CME/" + subcategory + "/";
                break;
            case "GBHN":
                _pre_image += "WorldNews/";
                break;
            case "MDFS":
                _pre_image += "MedicalFocus/";
                break;
            case "BBET":
            case "BBOT":
                _pre_image += "BookInBook/";
                break;
        }

        _pre_image += image_file;

        return _pre_image;
    }

