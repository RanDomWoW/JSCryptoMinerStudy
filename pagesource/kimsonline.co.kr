    function LoginCheck() {
        var frm = $("#frmLogin");
        var id = $.trim($("#user_id").val());
        var pw = $.trim($("#user_pw").val());

        if (id == "") {
            alert("아이디를 입력하세요"); $("#user_id").focus();// return false;
        }
        else if (pw == "") {
            alert("패스워드를 입력하세요"); $("#user_pw").focus();// return false;
        }
        else {
            if ($("#id_save").is(":checked")) {
                $.cookie("mID", id, { path: '/' });
            }
            else {
                $.cookie("mID", id, { expires: -1 });
            }

            $("#frmLogin").submit();
        }
    }
