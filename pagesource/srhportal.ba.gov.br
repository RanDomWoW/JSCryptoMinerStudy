/// Checks the field of forms.
var valOK=0;
function isMatric()
  {   
	var str = document.forms[0].matricula.value;   
    var comp= str.length;
	var acum='';
	var concat='';
	// Return false if number field is blank.   
	if (str == "")
    {         
		alert("\nO campo Matrícula está em branco.\n\nPor favor entre com o número de sua matrícula funcional de 9 dígitos!");         
		document.forms[0].matricula.focus();         
		return false;         
	}   
    else
     if (comp < "9")
     {
		alert("\nO campo Matrícula está incompleto.\n\n Por favor digite o número de sua matrícula de 9 dígitos!.");         

		document.forms[0].elements[1].focus();         

		return false;         
     }    
	// Return false if characters are not '0-9'.    

	for (var i = 0; i < str.length; i++)
	{      
		var ch = str.substring(i, i + 1);      
		if (ch < "0" || "9" < ch)
		{         
			alert("\nO campo Matrícula aceita apenas números. \n\nPor favor digite o número de sua matrícula de 9 dígitos!.");         
			document.forms[0].elements[1].select();         
			document.forms[0].elements[1].focus();         
			return false;         
		}      

	}
    if (comp == "9" || (comp >"9" && comp <"13"))
		{
				difer= 13 - comp ;
				if (difer >'0')
					{	
						for (var t = 1; t <= difer; t++)
							{
								acum+=0;
			
							}
						concat=str + acum ;
						document.forms[0].matricula.value=concat;   
        			}     
		} 
	return true;   
}
function click() {
	if (event.button==2){
		alert('Direitos Reservados para www.portaldoservidor.ba.gov.br.') }
}
document.onmousedown=click
</script>
