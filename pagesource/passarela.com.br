        <script type="text/javascript">

            function valida_form_home() {

                /* Coletando dados */

                // E-mail
                var email_home = document.querySelector("#email-newsletter-home").value;

                // Genero
                if (document.querySelector("#mulher-newsletter-home").checked == true) {
                    var genero_home = document.querySelector("#mulher-newsletter-home").value;
                };
                if (document.querySelector("#homem-newsletter-home").checked == true) {
                    var genero_home = document.querySelector("#homem-newsletter-home").value;
                };

                // Origem
                var origem_home = document.querySelector("#newsletter-home #ORIGEM-newsletter").value;

                // RI
                var _ri_home = document.querySelector("#newsletter-home #_ri_-newsletter").value;

                // Permissão Opt-in
                var permissao_home = document.querySelector("#newsletter-home #EMAIL_PERMISSION_STATUS_").value;

                /* Validando */

                // Valida Gênero
                if (genero_home == null) {
                    $('#newsletter-home #genero_popover_home').popover('show').delay(2200).queue(function(next){
                        $(this).popover("hide");
                        next();
                    });
                    return false;
                } else {
                    $('#newsletter-home #newsletter-table #genero_popover_home').popover('hide');
                }

                // Valida E-mail
                var filter_home = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
                if (!filter_home.test(document.querySelector("#email-newsletter-home").value)) {
                    $("#email-newsletter-home").popover('show').delay(2200).queue(function(next){
                        $(this).popover('hide');
                        next();
                    });
                        document.querySelector("#email-newsletter-home").focus();
                        return false;
                    } else {
                    $("#email-newsletter-home").popover('hide');
                }

                /* construindo url */
                var urlData_home ="&EMAIL_ADDRESS_=" + email_home +
                    "&GENDER=" + genero_home +
                    "&ORIGEM=" + origem_home +
                    "&EMAIL_PERMISSION_STATUS_=" + permissao_home +
                    "&_ri_=" + _ri_home;

                $.ajax({
                    type: "POST",
                    url: "http://newsletter.passarela.com.br/pub/rf",
                    async: true,
                    data: urlData_home,
                    success: function() {

                    },
                    beforeSend: function() {

                    },
                    complete: function() {
                        $('#newsletter-home #nome-newsletter-home').val('');
                        $('#newsletter-home #email-newsletter-home').val('');
                        $('#newsletter-home #enviar_form_newsletter_home').popover('show');
                        $('#newsletter-home #enviar_form_newsletter_home').delay(2200).queue(function(next){
                        $(this).popover('hide');
                        next();
                        });
                    }
                });
            };
</script>

