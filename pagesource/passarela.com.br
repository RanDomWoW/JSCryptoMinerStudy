                        cep = cep.replace(/[^0-9]/g,'');

                        if ( cep.length == 8 ) {
                            var i = 0;
                            for (c = 0; c < tabela[i].length; c++) {
                                if ( cep >= parseInt( tabela[i][c].faixaInicio.replace(/[^0-9]/g,'') ) && cep <= parseInt( tabela[i][c].faixaFim.replace(/[^0-9]/g,'') ) ) {
                                    exibePositivoTurbo();
                                    resultadoPositivo = 1;
                                    break;
                                } 
                            } 
                            if ( resultadoPositivo == 0 ) {
