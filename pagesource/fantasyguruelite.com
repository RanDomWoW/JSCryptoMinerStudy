		_plugins[plugin] = true;
		var body 			= document.getElementsByTagName('body')[0];
		var script 		= document.createElement('script');
		script.type 	= 'text/javascript';
		script.src 		= plugin;
		script.onload = callback;
		body.appendChild(script);
	} else if (callback) {
		callback();
	}
};

/**
 * _loadStylesheet - Dynamically load stylesheets
 * 
 * @param {string} - Stylesheet name
 * @param {callback} - 
 * @return value
 */
function _loadStylesheet(stylesheet) {
	if (!_styles[stylesheet]) {
		_styles[stylesheet] = true;
		$("link:last").before('<link rel="stylesheet" src="'+stylesheet+'" type="text/css">');
	}
};

/**
 * _loadProperty - Get property saved in HTML5's localStorage
 * 
 * @param {string} - Property name
 * @return value
 */
function _loadProperty(_property) {
  return localStorage.getItem(_property);
}

/**
 * _saveProperty - Save property in HTML5's localStorage
 *
 * @param {string} - Property name
 * @param {string|number} - Property Value
 */
function _saveProperty(_property,_value) {
  localStorage.setItem(_property, _value);
}

/**
 * _deleteProperty - Delete property in HTML5's localStorage
 *
 * @param {string} - Property name
 */
function _deleteProperty(_property) {
  localStorage.removeItem(_property);
}

/**
 * _backToTop() - Display button to jump to top of page (requires font-awesome)
 */
function _backToTop(triggerHeight) {
  var backToTop = $('<a>', {id: 'back-to-top', href: '#top'});
  var icon = $('<i>', {class: 'fa fa-chevron-up'});
  
  backToTop.appendTo('body');
  icon.appendTo(backToTop);
  
  backToTop.hide();
  
  $(window).scroll(function() {
    if($(this).scrollTop() > triggerHeight) {
      backToTop.fadeIn();
    } else {
      backToTop.fadeOut();
    }
  });
  
  backToTop.click(function(e) {
    e.preventDefault();
    $('body, html').animate({
      scrollTop: 0
    }, 600);
  });
  return true;
}

/**
 * _responsiveFooter()
 */
function _responsiveFooter() {
  var triptychHeight = $('.triptych').outerHeight();
	var brandHeight = $('.brand').outerHeight()+200;
}

/**
 * _autocollapse() - Collapse extra tabs/buttons into a dropdown
 */
function _autocollapse() {
  
  var menu = $('#main-menu');
  var menu_tree = $('#main-menu-tree');
  
  if($(document).width() > 767) {
    
    var menuHeight = menu.innerHeight();
    
    if (menuHeight > 40) {
      while(menuHeight > 40) {
        var children = menu_tree.children('li:not(:last-child)');
        var count = children.size();
        $(children[count-1]).prependTo('#collapsed');
        menuHeight = menu.innerHeight();
        
        var collapsed = $('#collapsed').children('li');
        if(collapsed.size() > 0) $('#lastTab').show();
      }
    }
    else {
      var collapsed = $('#collapsed').children('li');
      var count = collapsed.size();
      if(count == 0) $('#lastTab').hide();
      $(collapsed[0]).insertBefore(menu_tree.children('li:last-child'));
      menuHeight = menu.innerHeight();
      
      if (menuHeight > 40) { // double check height again
      	_autocollapse();
      }
    }
  } else {
    var collapsed = $('#collapsed').children('li');
    var count = collapsed.size();
    var timeout = 0;
    if(count > 0) {
      while(count > 0) {
        $(collapsed[0]).insertBefore(menu_tree.children('li:last-child'));
        count--;
        timeout++;
        if(timeout > 100) return;
      }
    }
    $('#lastTab').hide();
  }
}

