						<script>
							$(function(){
								$('.tweet-text').each (function(){
									
									var tweetText = $(this).html();
									var tweetDataArr = tweetText.split(' ');
									
									for (var i = 0; i <= tweetDataArr.length; i++ ) {
										
										if( tweetDataArr[i] != undefined ){
											
											if(is_valid_url(tweetDataArr[i])){
												var textLink = '<a href="'+tweetDataArr[i]+'" target="_blank" >'+tweetDataArr[i]+'<a/>';
												tweetText = tweetText.replace(tweetDataArr[i], textLink);
											}//is_valid_url()
											
											else if( tweetDataArr[i].charAt(0) == '@' && cleanHashtag(tweetDataArr[i]).length > 1 ){
												var textLink = '<a href="http://www.twitter.com/'+cleanHashtag(tweetDataArr[i])+'" target="_blank">&#64;'+cleanHashtag(tweetDataArr[i])+'<a/>';
												tweetText = tweetText.replace('@'+cleanHashtag(tweetDataArr[i]), textLink);
											}//USER
											
											else if(is_valid_hashtag(tweetDataArr[i])){
												if( tweetDataArr[i].charAt(0) == '#' && cleanHashtag(tweetDataArr[i]).length > 1 ){
													var textLink = '<a href="http://www.twitter.com/#'+cleanHashtag(tweetDataArr[i])+'" target="_blank">&#35;'+cleanHashtag(tweetDataArr[i])+'<a/>';
													tweetText = tweetText.replace(tweetDataArr[i], textLink);
												}
											}//is_valid_hashtag()
											
										}//not undefined
										
									} //for each
									
									$(this).html(tweetText);
								});
								
							})
							
							function is_valid_url(urls) {
								var URLPatterns = /^(http(s)?:\/\/)?(www\.)?[a-z0-9]+([\-\.]{1}[a-z0-9]+)*\.[a-z]{2,5}(:[0-9]{1,5})?(\/.*)?$/;
								return URLPatterns.test(urls);  
							}
							function is_valid_hashtag(tag) {
								var hashtagPatterns = /[^a-zA-Z0-9_]/g;
								return hashtagPatterns.test(tag);  
							}
							function cleanHashtag(tag){
								var pattern = /[^a-zA-Z0-9_]/g;
								return (tag.replace(pattern, ""));
							}
							
						</script>
