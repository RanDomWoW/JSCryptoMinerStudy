<script>
$(window).on('load', function () {

	// もっと見るボタン
	$("#bt_reload").click(function () {
		reload_contents();
		scroll_set()
	});

	// コンテンツ読み込み処理
	function reload_contents(){

		$('#bt_reload').hide();
		$('#loader-bg ,#loader').show();
		var obj = $(this);
		if (!obj.data('loading')) {

			obj.data('loading', true);
			var now_num = $("#now_num").val();
			var zen_num = $("#zen_num").val();
			var gen_num = parseInt(now_num) + 6;
			$("#now_num").val(gen_num);
			$.ajax({
				type: "POST",
				url: "index_more.php",
				data: {
					now: now_num,
					all: zen_num
				},
				cache: false,
				success: function(html){
					$("ul#add_contents").empty();
					$("ul#add_contents").append(html);
					obj.data('loading', false);

					$('#bt_reload').show();
				},
				complete: function(data){
					if(parseInt($("#now_num").val()) < parseInt($("#zen_num").val())){
						$('#bt_reload').show();
					}
					$('#loader-bg ,#loader').hide();
				}
			});

		}

	}
	// 指定位置までスクロール
	function scroll_set(){
		var p = $('#box4_2_pickup').offset().top;
		$('html,body').animate({ scrollTop: p }, 300);
		return false;
	}

	// ロード中画像の表示
	function loadingView(flag) {
		$('#loading-view').remove();
		if(!flag) return;
		$('<div id="loading-view" />').appendTo('body');
	}

});
</script>

<style>
