    <script type="text/javascript">
        function openAnounce () {
            let header = document.getElementsByClassName("site-header")[0];
            let buyNowButton = header.getElementsByClassName('anounce-link')[0];
            
            if (header && buyNowButton){   
                let getCookie = function(name) {
                    match = document.cookie.match(new RegExp(name + '=([^;]+)'));
                    if (match) return match[1];
                }

                let capitalize = function(s) {
                    return s.toLowerCase().replace( /\b./g, function(a){ return a.toUpperCase(); } ); 
                };

                let timeNow = new Date();

                if ( timeNow > new Date(getCookie('voucherBanner')) ) {
                    header.className += ' open';
                    expiryTime = new Date(timeNow.getTime() + 5*60*1000)
                    document.cookie = "voucherBanner =" + expiryTime + "; path=/ ; expires=" + expiryTime + ";"
                } else if (getCookie('voucherBanner') === undefined) {
                    header.className += ' open';
                    expiryTime = new Date(timeNow.getTime() + 5*60*1000)
                    document.cookie = "voucherBanner =" + expiryTime + "; path=/ ; expires=" + expiryTime + ";"
                }

                let city = getCookie('city') !== "undefined" ? getCookie('city').trim().replace(" ", "-") : "New-Delhi"
                city = city.replace(/["]+/g,"")

                city = capitalize("new-delhi");
                link = "https://magicpin.in/india/" + city + "/vouchers/?utm_source=website&utm_medium=top_dropdown_banner&category=flat%2050%"

                buyNowButton.href = link;
            }
        };

        function closeAnounce(){
            var header = document.getElementsByClassName("site-header")[0];
            header.classList.remove('open');
            ga('send', 'event', 'buy_now_dropdown', 'click_close_buynow_banner');
        }
    </script>

    <script>
        var lazy = document.getElementsByClassName('lazy');
        var afterLoadElements = document.getElementsByClassName('after-load');
        var removeLazy = [];

        registerListener('load', lazyLoad);
        registerListener('scroll', lazyLoad);
        registerListener('resize', lazyLoad);
        registerListener('load', afterLoad);
        registerListener('load', openAnounce);

        function lazyLoad(){
            lazy = document.getElementsByClassName('lazy');
            for(var i=0; i<lazy.length; i++){
                if(isInViewport(lazy[i])){
                    if (lazy[i].getAttribute('data-src')){
                        lazy[i].src = lazy[i].getAttribute('data-src');
                        lazy[i].style.backgroundImage = lazy[i].getAttribute('data-src');
                        removeLazy.push(lazy[i]);
                        lazy[i].removeAttribute('data-src');
                    }
                }
            }
            cleanLazy();
        }

        function cleanLazy() {
            for(var i=0; i<removeLazy.length; i++){
                removeLazy[i].classList.remove('lazy');
            }
            removeLazy = [];
            lazy = Array.prototype.filter.call(lazy, function(l){ return l.getAttribute('data-src');});
        }

        function isInViewport(el){
            var rect = el.getBoundingClientRect();
            
            return (
                rect.bottom >= 0 && 
                rect.right >= 0 && 
                rect.top <= (window.innerHeight || document.documentElement.clientHeight) && 
                rect.left <= (window.innerWidth || document.documentElement.clientWidth)
             );
        }

        function registerListener(event, func) {
            if (window.addEventListener) {
                window.addEventListener(event, func)
            } else {
                window.attachEvent('on' + event, func)
            }
        }

        function afterLoad() {
            for(var i=0; i<afterLoadElements.length; i++){
                afterLoadElements[i].src = afterLoadElements[i].getAttribute('data-src');
            }
        }
    </script>

    
