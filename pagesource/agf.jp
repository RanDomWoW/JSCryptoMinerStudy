      <script>
	  	
		var mainImage_id=0;
		var mainImage_max=$('.cover_img li').length;
		var mainImage_history=0;
		var mainImage_lock=false;
		mainimageTimer = setTimeout('timeAction()',7000);
		
		$(function(){
				createBtn();
				$('.btn_prev').bind('mousedown',function(){
						btnMove(-1);
					});
				$('.btn_next').bind('mousedown',function(){
						btnMove(1);
					});
				$('.cover_img li').eq(mainImage_id).animate({opacity:1},1000);
		})
		function btnMove(args){
				if(mainImage_lock)return;
				mainImage_lock=true;
				clearTimeout(mainimageTimer);
			 	mainimageTimer = setTimeout('timeAction()',8000);
				mainImage_history=mainImage_id;
				mainImage_id+=args;
				if(mainImage_id < 0)mainImage_id=mainImage_max-1;
				if(mainImage_id > mainImage_max-1)mainImage_id=0;
				changeMove();
		}
		
		function timeAction(){
			clearTimeout(mainimageTimer); //2016.05.12
			 btnMove(1);
			 mainimageTimer = setTimeout('timeAction()',7000);
		}
		
		function changeMove(){
				$('.control_btn li img').eq(mainImage_id).attr('src','img/top/control_btn1.png');
				$('.control_btn li img').eq(mainImage_history).attr('src','img/top/control_btn2.png');
                                

				$('.cover_img li').eq(mainImage_history).animate({opacity:0},500,function(){
                                    $('.cover_img li').hide();
                                    $('.cover_img li').eq(mainImage_id).show();
                                        $('.cover_img li').eq(mainImage_id).animate({opacity:1},500,function(){
								mainImage_lock=false;
                                                                
						});
					});
		}
		
	function createBtn(){
		$('.control_btn').html('');
		for(var i=0; i<mainImage_max;i++){
			var li = document.createElement('li');
				li.nid=i;
			var a = document.createElement('a');
			var img = document.createElement('img');
			li.appendChild(a);
			a.appendChild(img);
			img.setAttribute('src','img/top/control_btn2.png');
			$(li).mousedown(function(){   directAction( this.nid)     });
			$('.control_btn').append(li);
		}
		$('.control_btn li img').eq(mainImage_id).attr('src','img/top/control_btn1.png');
	}
	
	function directAction(nid){
		if( mainImage_id ==  nid )return;
		if(mainImage_lock)return;
		mainImage_lock=true;
		clearTimeout(mainimageTimer); //2016.05.12
		mainimageTimer = setTimeout('timeAction()',8000);
		mainImage_history=mainImage_id;
		mainImage_id=nid;
		changeMove();
	}
	
	  </script>
