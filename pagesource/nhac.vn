<script type="text/javascript">
/*<![CDATA[*/

            function LoadEventStream(){
                $.ajax({
                    type:'GET',
                    url:'/LiveStreamEvent/getEvent',
                    dataType:'json',
                    success:function(res){
                        if(res.errorCode==0){
                        	var data = res.data;
                        	var html = '<img src="'+data.cover+'" />';
                        	html +='<div id="event-info">';
                        	if(data.main_title!=''){
	                        	html +='<h3 class="c-main-title">'+data.main_title.toUpperCase()+'</h3>';
	                        }
                        	if(data.sub_title!=''){
	                        	html +='<h3 class="c-sub-title">'+data.sub_title.toUpperCase()+'</h3>';
	                        }
                        	html +='<div class="c-upgrade-time">THỜI GIAN CHỈ CÒN</div>';
                        	var defaultCountDown = '<span class="cday">00</span>' + '<span class="cline"></span>' + 
							    '<span class="chour">00</span>' + '<span class="twodot"></span>' + 
							    '<span class="cmin">00</span>' + '<span class="twodot"></span>' + 
							    '<span class="csecond">00</span>';

                        	html += '<div id="countdown_event" class="timer-event-countdown">'+defaultCountDown+'</div>';
                        	html +='<div id="btn-livestream"><span class="btn-stream-live"></span></div>';
                        	html +='</div>';
                        	$('#event-container').html(html);
                        	var start_time = data.start_time;
                        	var startTimestamp = Math.round(new Date(start_time).getTime())/1000;
                        	var currTime = Math.round(new Date().getTime()/1000);
                        	var upgradeTime = startTimestamp-currTime;
							var seconds = upgradeTime;
							if(seconds>0){
	                        	var countdownTimer = setInterval(function(){
	                        		var days        = Math.floor(seconds/24/60/60);
								    var hoursLeft   = Math.floor((seconds) - (days*86400));
								    var hours       = Math.floor(hoursLeft/3600);
								    var minutesLeft = Math.floor((hoursLeft) - (hours*3600));
								    var minutes     = Math.floor(minutesLeft/60);
								    var remainingSeconds = seconds % 60;
								    if (remainingSeconds < 10) {
								        remainingSeconds = "0" + remainingSeconds; 
								    }
								    document.getElementById('countdown_event').innerHTML = '<span class="cday">'+days+'</span>' + '<span class="cline"></span>' + 
								    '<span class="chour">'+hours+'</span>' + '<span class="twodot"></span>' + 
								    '<span class="cmin">'+minutes+'</span>' + '<span class="twodot"></span>' + 
								    '<span class="csecond">'+remainingSeconds+'</span>';

								    if (seconds == 0) {
								        clearInterval(countdownTimer);
								        document.getElementById('countdown_event').innerHTML = "Sự kiện đang diễn ra";
								        //$('#countdown_event').hide();
	                        			$('.c-upgrade-time').hide();
								        $('#btn-livestream').find('.btn-stream-live').addClass('on');
								        $('#btn-livestream').on('click', function(){
								        	window.location.href=data.url_event;
								        })
								    } else {
								        seconds--;
								    }
	                        	}, 1000);
	                        }else{
	                        	$('#countdown_event').hide();
	                        	$('.c-upgrade-time').hide();
	                        	$('#btn-livestream').find('.btn-stream-live').addClass('on');
						        $('#btn-livestream').on('click', function(){
						        	window.location.href=data.url_event;
						        })
	                        }
                        	$('#event-container').show();
                        }
                    }
                });
            }
            
            
LoadEventStream();
