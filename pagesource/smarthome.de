  $.each($('.countdown-container'), function(index, item) {
    var finalDate = $(this).data('countdown'),
        labels = ['hours', 'minutes', 'seconds'],
        currDate = '00:00:00:00:00',
        nextDate = '00:00:00:00:00',
        parser = /([0-9]{2,3})/gi;
    function strfobj(str) {
      var parsed = str.match(parser),
          obj = {};
      labels.forEach(function(label, i) {
        obj[label] = parsed[i]
      });
      return obj;
    }
    
    function diff(obj1, obj2) {
      var diff = [];
      labels.forEach(function(key) {
        if (obj1[key] !== obj2[key]) {
          diff.push(key);
        }
      });
      return diff;
    }
    
    $(this).countdown(finalDate, function(event) {
      var _this = this,
          newDate = event.strftime('%H:%M:%S'),
          totalHours,
          data;
      if(event.offset.totalDays > 0) {
        totalHours = event.offset.totalDays * 24 + event.offset.hours;
        newDate = event.strftime(totalHours+':%M:%S');
        
      }
      
      if (newDate !== nextDate) {
        currDate = nextDate;
        nextDate = newDate;
        // Setup the data
        data = {
          'curr': strfobj(currDate),
          'next': strfobj(nextDate)
        };
        // Apply the new values to each node that changed
        diff(data.curr, data.next).forEach(function(label) {
          var selector = '.%s'.replace(/%s/, label),
              $node = $(_this).find(selector);
          // Update the node
          $node.removeClass('flip');
          $node.find('.curr').html('<span>'+data.curr[label]+'</span>');
          $node.find('.next').html('<span>'+data.next[label]+'</span>');
          // Wait for a repaint to then flip
          setTimeout(function() {
            $node.addClass('flip');
          }, 50);
        });
      }
    });

  });
  navigator.userAgent.toLowerCase()
});


