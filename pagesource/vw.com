<script>
//events for tagging

var $meetCar = $('.modelChooser').find('.meetCar'),
	$findMatch = $('.modelChooser').find('.buildYours'),
	$learnMore = $('.floatingText .cta-container').find('a').addClass('learnMore');

var ctaTagging = function (button, tagName) {
	button.on('click', function (e) { 

		var model = $(this).parents('.car').attr('data-model');
		
		VW.radio(tagName).broadcast({ 
			triggerElement: $(e.currentTarget),
			modelName: model
		}); 
	});
};

var learnmoreTagging = function (button, tagName) {
	button.on('click', function (e) { 
		var title = $(this).parents('.floatingText').find('h2').text() + ' -> Learn More';
		
		VW.radio(tagName).broadcast({ 
			triggerElement: $(e.currentTarget),
			feature: title
		}); 
	});
};

if ($('body').hasClass('vwSEOContent')) {
	ctaTagging($meetCar,'seo:goModelBucketCta');
	ctaTagging($findMatch,'seo:goFindMatch');
	learnmoreTagging($learnMore, 'seo:feature:learnMore');

	$('.modelChooser').find('buildYours').on('click', function (e) { 
		VW.radio('seo:goBuilderCta').broadcast({ 
			triggerElement: $(e.currentTarget)
		}); 
	});

}

</script></div>
