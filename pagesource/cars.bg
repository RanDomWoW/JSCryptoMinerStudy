<script>
function showLoginPanel(){
	if($('#login_panel').is(":visible")){ 
		$('#login_panel').hide();
		$('#publish_panel').hide();
		$('#edit_panel').hide();
		destroyOverlayTrans();
    } else {
    	createOverlayTrans();
		$('#login_panel').show();
		$('#publish_panel').hide();
		$('#edit_panel').hide();
    }
   
}
function showPublishPanel(){
	if($('#publish_panel').is(":visible")){ 
		$('#publish_panel').hide();
		$('#login_panel').hide();
		$('#edit_panel').hide();
		destroyOverlayTrans();
    } else {
    	createOverlayTrans();
		$('#publish_panel').show();
		$('#login_panel').hide();
		$('#edit_panel').hide();
    }
    
}
function showEditPanel(){
	if($('#edit_panel').is(":visible")){ 
		$('#edit_panel').hide();
		$('#login_panel').hide();
		$('#publish_panel').hide();
		destroyOverlayTrans();
    } else {
    	createOverlayTrans();
		$('#edit_panel').show();
		$('#login_panel').hide();
		$('#publish_panel').hide();
    }
    
}
function createOverlayTrans(){
var overlay = $('#overlay_trans');

 if(overlay.length == 0) {
 	overlay = $('<div id="overlay_trans">&nbsp;</div>');
    $('body').append (overlay);
      overlay.bind ('click', 
	   function () {
	     $('#publish_panel').hide();
		 $('#login_panel').hide();
		 $('#edit_panel').hide();
		 $('#search_history_panel').hide();
		 overlay.hide();
	   }
	  );

 } else {
 	overlay.show();
 }

}

function destroyOverlayTrans(){
  // var overlay = $('#overlay_trans');
   //overlay.remove();
}

$ (document).on("click", "#overlay_trans" ,function(){
$('#publish_panel').hide();
$('#login_panel').hide();
$('#edit_panel').hide();
$('#search_history_panel').hide();
destroyOverlayTrans();
});

</script>

