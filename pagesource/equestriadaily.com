	-webkit-box-sizing:border-box;
	-moz-box-sizing:border-box;
	vertical-align:bottom;
}

.nav-bar .widget:first-child
{
	border-left-color:rgba(0,0,0,0.3);
}

.nav-bar .widget:last-child
{
}

