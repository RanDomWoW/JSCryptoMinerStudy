<script type="text/javascript">
jQuery(function(){
	$.fn.startCarousel = function(){
  		var bodywidth = $('body').width();
		if(bodywidth > 1250){
			bodywidth = 820;
		} else {
			bodywidth  = bodywidth - 400;
		}

		var itemwidth = $('.mycarousel li').outerWidth(true);
		var mycontwidth = bodywidth > itemwidth ? bodywidth - bodywidth%itemwidth : itemwidth;
		var licount = $('.mycarousel li').size();
		var jscroll = 1;
			
		if(licount > mycontwidth/itemwidth){
			jscroll =  mycontwidth/itemwidth;
		} else {
			jscroll = 0;
			mycontwidth = licount * itemwidth;
		}
		
		$('.mycont').width(mycontwidth);
			
    	$('.mycarousel').jcarousel({
			scroll:jscroll
		});
  	};
	
	$(this).startCarousel();
	
	$(window).resize(function(){
		$(this).startCarousel();
	}); 

});
</script>

