function swapLangs(field1, field2) {

if ($('#'+field1).val() != 'auto') {
var tmp = $('#'+field1).val();
$('#'+field1).val($('#'+field2).val());
$('#'+field2).val(tmp);
$('#'+field1).trigger("liszt:updated");
$('#'+field2).trigger("liszt:updated");
}

}
