var translation = $("#new_translation").val();
var src_lang = $("#src_lang").val();
var dst_lang = $("#dst_lang").val();


var dataString = 'source='+ source + '&translation=' + translation + '&src_lang=' + src_lang + '&dst_lang=' + dst_lang;  
 
