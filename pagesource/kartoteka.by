	// fixed-menu nav to drop
	function fixedMenuLast(){
		if ( $(window).width() > 767 && $(window).width() < 992 ){
			if ( $('.js-todrop').parent('.navbar-nav').length ) {
				$('.js-todrop').prependTo($('.js-todrop-drop'));
			}
		} else {
			if ( !$('.js-todrop').parent('.navbar-nav').length ) {
				$('.js-todrop').appendTo($('.fixed-header .navbar-nav'));
				$('.fixed-header .navbar-nav > .dropdown').insertAfter($('.js-todrop:last-child'));
			}
		}
	}
	fixedMenuLast();
	$(window).resize(function(){
		fixedMenuLast();
	});
	
	// regionListLast
	function regionListLast(){
		if ( $(window).width() < 768 )
			$('.js-region-mobile-first').prependTo($('#region-menu'));
		else
			$('.js-region-mobile-first').appendTo($('#region-menu'));
	}
	regionListLast();
	$(window).resize(function(){
		regionListLast();
	});
    


