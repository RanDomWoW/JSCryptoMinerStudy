            var checkUrl = function(el){
                if($(el).attr("data-region") == "BY" ){
                    $(el).closest("form").attr("action", "https://kartoteka.by/search").find(".search-input").attr("placeholder", "Введите УНП, наименование компании или ФИО")
                } else if($(el).attr("data-region") == "KZ" ){
                    $(el).closest("form").attr("action", "https://kartoteka.by/searchKz").find(".search-input").attr("placeholder", "Введите БИН или наименование компании")
                } else if($(el).attr("data-region") == "UA" ){
                    $(el).closest("form").attr("action", "https://kartoteka.by/searchUa").find(".search-input").attr("placeholder", "Введите ЕДПРОУ, наименование, ФИО или адрес")
                } else if ($(el).attr("data-region") == "RU" ) {
                    $(el).closest("form").attr("action", "https://kartoteka.by/searchRussian").find(".search-input").attr("placeholder", "Введите ИНН или наименование компании")
                } else if ($(el).attr("data-region").length > 0) {
                    $(el).closest("form").find("#search-euro-region").val($(el).attr("data-region"))
                    $(el).closest("form").attr("action", "https://kartoteka.by/searchEu?region=" + $(el).attr("data-region")).find(".search-input").attr("placeholder", "Введите наименование компании на английском")
                }
