<script>
    $(document).ready(function () {
        $("#tariffsBox").on("click", ".validate-link", function () {
            var link = $(this)
            $.ajax({
                url: link.attr("href"),
                method: "POST",
                dataType: "JSON",
                async: false,
                success: function (response) {
                    if (response.success) {
                        window.location.href = link.attr("href")
                    } else {
                        $("#successModalInfo .modal-content").html(response.message)
                        $("#successModalInfo").modal("show")
                    }
                }
            })
            return false;
        })
        $('#tariffsBox').on('click', '.validate-form input[type="submit"]', function () {
            var form = $(this).closest("form")
            $.ajax({
                url: form.attr('action'),
                method: "POST",
                dataType: "JSON",
                data: form.serialize(),
                async: false,
                success: function (response) {
                    if (response.success) {
                        form.submit()
                    } else {
                        $("#successModalInfo .modal-content").html(response.message)
                        $("#successModalInfo").modal("show")
                    }
                }
