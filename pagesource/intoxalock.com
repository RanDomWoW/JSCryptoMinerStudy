<script type="text/javascript">
    //Save Coockaie
    function saveCookie(name, value, days) {
        var date, expires;
        if (days) {
            date = new Date();
            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
            expires = "; expires=" + date.toGMTString();
        } else {
            expires = "";
        }
        document.cookie = name + "=" + value + expires + "; path=/";
    }


    var geocoder;
    function getLocation() {
        document.getElementById("Loader").style.display = "none";
        saveCookie("ixState", "", 30);
        saveCookie("ixlatlng", "", 30);
        document.getElementById("divLocationText").innerHTML = "2500+ installation locations";
        document.getElementById("divState").innerHTML = "Your location";
        document.getElementById("divState").style.display = "none";

        document.getElementById("divChangeLocationText").innerHTML = "Enter zip code";
        document.getElementById("locationDetailBox").style.display = "none";
        document.getElementById("divLocationCount").style.display = "none";
        document.getElementById("divLocationMessage").style.display = "none";
        document.getElementById("togglecontainer").style.display = "block";
        document.getElementById("togglecontainer").style.display = "none";
    }

    function successFunction(position) {
        document.getElementById("Loader").style.display = "block";
        trackQS("Success", position);
        document.getElementById("locationDetailBox").style.display = "block";
        document.getElementById("divLocationCount").style.display = "block";
        document.getElementById("divLocationMessage").style.display = "block";
        document.getElementById("togglecontainer").style.display = "none";
        var lat = position.coords.latitude;
        var lng = position.coords.longitude;
        if (lat != null && lng != null) {
            getLangLat(lng, lat);
        }
        saveCookie("ixlatlng", lat + "," + lng, 30);
    }

    function errorFunction(error) {
        trackQS("Error", error.message);
        document.getElementById("divLocationText").innerHTML = "2500+ installation locations";
        document.getElementById("divState").innerHTML = "Your location";
        document.getElementById("divChangeLocationText").innerHTML = "Enter zip code";
        document.getElementById("locationDetailBox").style.display = "none";
        document.getElementById("divLocationCount").style.display = "none";
        document.getElementById("divLocationMessage").style.display = "none";
        document.getElementById("togglecontainer").style.display = "block";
        document.getElementById("Loader").style.display = "none";
        saveCookie("ixlatlng", "", 30);
        if (!$('#mainsection').hasClass("backdrop"))
            $('#mainsection').addClass("backdrop");
    }

    function trackQS(msgtype, msg) {
        var currentUrl = window.location.href;
        var isMobile = document.getElementById("dnn_ctr13326_View_hfIsMobile").value;

        var cSource = "Default";
        var cType = "Others";
        if (getCookieValue('ixSource') != "") {
            var cSource = getCookieValue('ixSource');
            var cType = "Campaigns";
        }

        var message = "";
        if (msgtype == "Error")
            message = " Error message: " + msg;
        else
            message = " Succcess to get position at lang: " + msg.coords.longitude + " and lat: " + msg.coords.latitude;

        if (isMobile == "false") {
            if (msgtype == "Error")
                ga('send', 'event', 'No GEO code found ' + cType, 'No GEO code found from ' + cSource, 'Geolocator error page ' + currentUrl + message, 1);
            else
                ga('send', 'event', 'Yes GEO code found ' + cType, 'Yes GEO code found from ' + cSource, 'Geolocator succcess page ' + currentUrl + message, 1);
        }
        else {
            if (msgtype == "Error")
                ga('send', 'event', 'No GEO code found-Mobile ' + cType, 'No GEO code found from ' + cSource, 'Geolocator error page ' + currentUrl + message, 1);
            else
                ga('send', 'event', 'Yes GEO code found-Mobile ' + cType, 'Yes GEO code found from ' + cSource, 'Geolocator succcess page ' + currentUrl + message, 1);
        }
    }

    function getLangLat(lng, lat) {
        $.ajax({
            type: "POST",
            url: "/DesktopModules/GetMyLocation/Loc.aspx/GetLangLat",
            data: '{lng: "' + lng + '",lat: "' + lat + '" }',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: OnSuccess,
            failure: function (response) {
                alert(response.d);
            }
        });
    }

    function getLangLatState(state) {
        document.getElementById("divState").innerHTML = state;
        $.ajax({
            type: "POST",
            url: "/DesktopModules/GetMyLocation/Loc.aspx/GetLangLatState",
            data: '{state: "' + state + '" }',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: OnSuccess,
            failure: function (response) {
                alert(response.d);
            }
        });
    }

    function OnSuccess(response) {
        // loader
        document.getElementById("Loader").style.display = "block";
        var ServiceCenter = response.d;
        var divState = document.getElementById("divState");
        var divLocationCount = document.getElementById("divLocationCount");
        var divLocationMessage = document.getElementById("divLocationMessage");
        var divLocationText = document.getElementById("divLocationText");

        var divServicesCenterName = document.getElementById("divServicesCenterName");
        var divGMYAddress = document.getElementById("divGMYAddress");
        var divGMYCityState = document.getElementById("divGMYCityState");
        var divServicesCenterWorkingHours = document.getElementById("divServicesCenterWorkingHours");

        if (ServiceCenter.StateName != null) {
            divState.innerHTML = ServiceCenter.StateName.replace("-", " ");
            saveCookie("ixState", ServiceCenter.StateName.replace("-", " "), 30);
        }

        divServicesCenterName.innerHTML = ServiceCenter.ServiceCenterName;
        divGMYAddress.innerHTML = ServiceCenter.AddressLine1;
        if (ServiceCenter.AddressLine2 !== "" && ServiceCenter.AddressLine2 != null)
            divGMYAddress.innerHTML += "</br>" + ServiceCenter.AddressLine2;

        divGMYCityState.innerHTML = ServiceCenter.City + " ," + ServiceCenter.StateCode + " " + ServiceCenter.PostalCode;

        divLocationCount.innerHTML = ServiceCenter.nearByLocationCount;
        divServicesCenterWorkingHours.innerHTML = ServiceCenter.WorkingHours;

        if (divLocationCount.innerHTML == "0") {
            //alert("large");
            divLocationCount.style.display = "none";
            divLocationText.innerHTML = "No Installation location in";
            document.getElementById("divLocationMessage").style.display = "none";
            document.getElementById("divChangeLocationText").innerHTML = "Enter zip code";
            document.getElementById("locationDetailBox").style.display = "none";
            document.getElementById("topArrow").className = "orange_arrow";
            if ($(window).width() > 767) 
               $(".location_details").css("margin-top", "14px");
                //  alert("large");
            
            else 
               $(".location_details").css("margin-top", "7px");
            // alert("small");
            
        }
        else {
            $(".location_details").css("margin-top", "5px");
            divLocationCount.style.display = "block";
            document.getElementById("divChangeLocationText").innerHTML = "Change Location";
            divLocationText.innerHTML = "You are looking at";
            document.getElementById("topArrow").className = "white_arrow";
            

        }
        document.getElementById("Loader").style.display = "none";
    }

    function isNumberKey(e) {
        e.target.value = e.target.value.replace(/[^\0-9\s]/g, '');
    }

    function CheckZipCode() {
        var zipreg = /^\d+$/;
        var zip = document.getElementById('dnn_ctr13326_View_txtZipCode').value;
        if (!zipreg.test(zip)) {
            alert("Please enter 5-digit valid zip code.");
            return false;
        } else if (zip.length != 5) {
            alert("Please enter 5-digit valid zip code.");
            return false;
        }
        return true;
    }

    function invalidZip() {
        alert("Please enter 5-digit valid zip code.");
    }

    function toggleZipdropdown() {
        $('#togglecontainer').toggle();

        if (!$('#mainsection').hasClass("backdrop")) {
            $('#mainsection').addClass("backdrop");
            $(".home_input.zipcode").focus();
        } else {
            $('#mainsection').removeClass("backdrop");
        }
    }

    $(document).ready(function () {
        $('.zipcode').keypress(function (event) {
            if (isNaN(String.fromCharCode(event.which)) && event.which != 0) {
                if (event.which != 8) {
                    event.preventDefault(); //stop character from entering input
                }
            }
        });
    });
