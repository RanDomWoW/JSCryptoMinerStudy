    <!-- Initialize Swiper -->
    <script>


        $(document).ready(function($) {

            var mySwiper = undefined;
            var mySwiper2 = undefined;
            function initSwiper(slideViews) {
                var screenWidth = $(window).width();
                if(screenWidth < 768 && mySwiper == undefined) {
                    mySwiper = new Swiper('#forumhome-slidreg .swiper-container', {
                        slidesPerView: slideViews,
                        spaceBetween: 1,
                        loop: true,
                        // Responsive breakpoints
                        breakpoints: {
                            // when window width is <= 480px
                            480: {
                                slidesPerView: 1,
                                spaceBetween: 0
                            },
                            // when window width is <= 768px
                            768: {
                                // The value for guest is 1 and the value for member is 2
                                slidesPerView: 2,
                                spaceBetween: 0
                            }
                        },
                        pagination: {
                            el: '.swiper-pagination',
                            clickable: true,
                        },
                        navigation: {
                            nextEl: '.swiper-button-next',
                            prevEl: '.swiper-button-prev',
                        }
                    });
                } else if (screenWidth > 767 && mySwiper != undefined) {
                    mySwiper.destroy();
                    mySwiper = undefined;
                    console.log("destroy");
                    jQuery('#forumhome-slidreg .swiper-container .swiper-wrapper').removeAttr('style');
                    jQuery('#forumhome-slidreg .swiper-container .swiper-slide').removeAttr('style');
                }


                if(screenWidth < 640 && mySwiper2 == undefined) {

                    mySwiper2 = new Swiper('#opf-container.swiper-container', {
                        slidesPerView: 3,
                        spaceBetween: 1,
                        loop: true,
                        // Responsive breakpoints
                        breakpoints: {
                            // when window width is <= 768px
                            640: {
                                // The value for guest is 1 and the value for member is 2
                                slidesPerView: 1,
                                spaceBetween: 0
                            }
                        },
                        pagination: {
                            el: '.swiper-pagination',
                            clickable: true,
                        },
                        navigation: {
                            nextEl: '.swiper-button-next',
                            prevEl: '.swiper-button-prev',
                        }
                    });

                } else if (screenWidth > 640 && mySwiper2 != undefined) {

                    mySwiper2.destroy();
                    mySwiper2 = undefined;
                    console.log("destroy");
                    jQuery('#opf-container.swiper-container .swiper-wrapper').removeAttr('style');
                    jQuery('#opf-container.swiper-container .swiper-slide').removeAttr('style');

                }


            }
            //Swiper plugin initialization
            if($('body').hasClass('hpguestyo')) {
                var slideViews = 1;
            } else {
                var slideViews = 4;
            }
            initSwiper(slideViews);
            //Swiper plugin initialization on window resize
            $(window).on('resize', function(){
                initSwiper();
            });


        });

    </script>



<script>
    jQuery(document).ready(function(){
        // Add smooth scrolling to all links with scrollsmooth class
        jQuery("a.scrollsmooth").on('click', function(event) {
            // Make sure this.hash has a value before overriding default behavior
            if (this.hash !== "" && !this.closest(".tab")) {
                // Prevent default anchor click behavior
                event.preventDefault();
                // Store hash
                var hash = this.hash;
                // Using jQuery's animate() method to add smooth page scroll
                // The optional number (800) specifies the number of milliseconds it takes to scroll to the specified area
                jQuery('html, body').animate({
                    scrollTop: $(hash).offset().top
                }, 800, function(){
                    // Add hash (#) to URL when done scrolling (default click behavior)
                    window.location.hash = hash;
                });
            } // End if
        });
    });
</script>
