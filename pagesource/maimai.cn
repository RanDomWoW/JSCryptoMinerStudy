                    var $video_arr = $('.dp_video_img');
                    var video_length = $video_arr.length;
                    var video_left = $video_arr.eq(0).offset().left - $('.dp_banner').eq(0).offset().left;
                    $(".dp_banner_left").css('left', video_left);
					if(screen_width <= 1280){ //小屏替换video图片
						for (var i= 0; i< video_length; i++){
							$video_arr.eq(i).attr('src', 'images/dp_small_video' + (i+1) + '.png');
						}
					}
				});

				$('#dp_download_btn').mouseover(function(){
					$('#dp_blue_btn').css('display', 'none');
