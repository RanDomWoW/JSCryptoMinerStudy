    <script>
    	var exitAttempt = false;
		/*Modified from : https://julian.is/article/exit-intent-popups/ */
		$(document).ready(function() {
			exitAttempt = false;
			//console.log("document ready.");
			// Exit intent trigger
			addEvent(document, 'mouseout', function(evt) {
			  //console.log("Exit Intent.: "+evt.toElement+", "+evt.relatedTarget);
			  if (evt.toElement == null && evt.relatedTarget == null && !exitAttempt) {
			  	//console.log("Show Popup True");
			    $('#ei-popover').modal('show');
			    exitAttempt = true; // Set the exit attempt flag true
			  }
			});
			// Accordion active drawer toggle
			$('#faqs div.collapse').on('shown.bs.collapse', function(){
				$(this).siblings().find('[data-fa-i2svg]').removeClass("fa-plus").addClass("fa-minus");
			});
			$('#faqs div.collapse').on('hidden.bs.collapse', function(){
				$(this).siblings().find('[data-fa-i2svg]').removeClass("fa-minus").addClass("fa-plus");
			});
		});
		/*Source : https://julian.is/article/exit-intent-popups/ */
		// Exit intent
		function addEvent(obj, evt, fn) {
		  if (obj.addEventListener) {
		    obj.addEventListener(evt, fn, false);
		  } else if (obj.attachEvent) {
		    obj.attachEvent("on" + evt, fn);
		  }
		}
    </script>
