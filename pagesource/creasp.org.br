var onMouseOverDestaques = 0;

function carrossel(fase) {
	if(onMouseOverDestaques == 0) {
		$('#destaque_fase_'+fase).show();
		fase = fase+1
		if(fase > 4) {
			fase = 1
			$('#destaque_fase_1').hide();
			$('#destaque_fase_2').hide();
			$('#destaque_fase_3').hide();
			document.getElementById('controleX').click()
		}
	}
	setTimeout("carrossel("+fase+")", 2000);
}

function controleCarrossel(on_off) {
	switch(on_off) {
		case 'on':
			onMouseOverDestaques = 0
			break
		case 'off':
			onMouseOverDestaques = 1
			break
	}
}
