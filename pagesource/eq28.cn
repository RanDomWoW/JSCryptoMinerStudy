                        updateEfunInfo();
                    };
                    topRunTimes = 0;timeCount = 0;startTime = new Date().getTime();
                }
            }, false);
        }
        function topTimeAddo(num) {
            if (num < 10) {
                num = "0" + num;
            }
            return num;
        }
        function topUpdateTime() {
            var offset = new Date().getTime() - (startTime + timeCount * 1000);
            var nextTime = 1000 - offset;
            if (nextTime < 0) {nextTime = 0};
            var remain = topServerTime - topRunTimes * 1000;
            var lag = remain / 1000;
            if (lag >= 0) {
                var second = topTimeAddo(Math.floor(lag % 60));
                var minute = topTimeAddo(Math.floor((lag / 60) % 60));
                topSpShowMinute.html(minute);
                topSpShowSecond.html(second);
                if ($('body').hasClass('efun-ind-body')) {
                    $(".ind_set_time .spShowMinute").html(minute); //首页
                    $(".ind_set_time .spShowSecond").html(second);
                } else if ($('body').hasClass('efun-prodet-body') || $('body').hasClass('efun-share-prodet-body')) {
                    $(".setTime .spShowMinute").html(minute);  //详情页 //分享页
                    $(".setTime .spShowSecond").html(second);
                } else if ($('body').hasClass('efun-recharge-body')) {
                    $(".mtb20 .spShowMinute").html($(".time-lottery-wrap .spShowMinute").html());  //充值页
                    $(".mtb20 .spShowSecond").html($(".time-lottery-wrap .spShowSecond").html());
                } else if ($('body').hasClass('efun-shop-ind')) {
                    $(".setTime").html("<strong>" + minute + "&nbsp;:&nbsp;</strong><strong>" + second + "</strong>"); //店铺首页
                };
                timeCount++;topRunTimes++;timerCounter = setTimeout(topUpdateTime, nextTime);
            } else {

                updateAttendStatus();//更新提示状态
                if ($('body').hasClass('efun-ind-body')) {
                    topServerTime = 59000;topRunTimes = 0;topUpdateTime();
                    efunIndLotteryTime();    //首页
                } else if ($('body').hasClass('efun-prodet-body')) {
                    topServerTime = 59000;topRunTimes = 0;topUpdateTime();
                    efunProDetLotteryTime();  //详情页
                } else if ($('body').hasClass('efun-shop-ind')) {
                    topServerTime = 59000;topRunTimes = 0;topUpdateTime();
                    efunShopLotteryTime();  //店铺首页
                } else if ($('body').hasClass('efun-buy-body')){
                    topServerTime = 59000;topRunTimes = 0;topUpdateTime();
                    updateEfunInfo();   //一折购页面
                } else if($('body').hasClass('efunpay-success-body')){
                    setTimeout(function () {
                        location.href = postPath + "/user/efunOrder/winDiscountList";
                    },1600);  //pay success page
                };

            }
        }
        if (topServerTime >= 0) {topUpdateTime();}
    });

