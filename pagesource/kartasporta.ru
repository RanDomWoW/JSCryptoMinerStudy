<script>
$('.new-filter-left li a').each(function(){
	if( $(this).hasClass('selected') ) {
		var ulId = $(this).attr('id') + '-content';
		$('#' + ulId).css('display','block');
	}
	$(this).click(function(){
		$('.new-filter-left li a').removeClass('selected');
		$(this).addClass('selected');
		var ulId = $(this).attr('id') + '-content';
		$('.new-filter-right').css('display','none');
		$('#' + ulId).css('display','block');
		return false;
	});
	$('.new-filter-right li a').click(function (){
		var sel = $(this).parent().parent().parent().parent().find('.new-filter-in');
		sel.html( $(this).html() );
