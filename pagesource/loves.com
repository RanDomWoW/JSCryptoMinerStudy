<script type="text/javascript">
    $(document).ready(function () {
        var languageselector = $.cookie("selectedlanguagecookie");
        if (languageselector != null && languageselector != undefined) {
            var selectedLanguage = languageselector;
            if (selectedLanguage != '' && selectedLanguage != null && selectedLanguage != undefined) {
                var myDropDown = $('<i class="fa fa-chevron-down fa-sm"></i>')
                $('[data-dropdown="languagesMobile"]').text(selectedLanguage).append(myDropDown);
            }
        }
    });

    function saveLanguageCookie(selectedLanguage, displayName) {
        $.ajax({
            url: '/api/sitecore/LanguageSelector/SetLanguage',
            data: { 'selectedLanguage': selectedLanguage, 'displayName': displayName },
            type: "post",
            cache: false,
            success: function (data) {
                var hostUrl = window.location.origin;
                var pagePath = window.location.pathname;
                var lang = pagePath.split('/')[1];
                var pageName = "";
                if (lang != "") {
                    pageName = pagePath.split(lang)[1];
                }
                var targetUrl = hostUrl + "/" + selectedLanguage + pageName;
                window.location.href = targetUrl;
            }
        });
    }
</script>        
