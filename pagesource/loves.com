<script type="text/javascript">
    function saveCustomerTypeCookie(cType, navigateUrl, event) {
		if (typeof(event) != 'undefined' && event.preventDefault) {
				event.preventDefault();
		}

		var selectedCustomer = cType;
		var selectedUrl = navigateUrl;
		$("#customers").val(cType);
		$.ajax({
			url: '/api/sitecore/login/setcustomer',
			data: { 'customerType': selectedCustomer, 'customerLink': selectedUrl },
			type: "post",
			cache: false,
			success: function (data) {
				location.reload(true);
			}
		});
	}
</script>
