<script type="text/javascript">
    $j(document).ready(function(){
        var utm_source      = getParamSalesOrigin('utm_source');
        var utm_campaign    = getParamSalesOrigin('utm_campaign');

        if(utm_source || utm_campaign){
            var expireAt = new Date();

            var lifetime = parseInt('86400');
            if (lifetime > 0) {
                expireAt.setTime(expireAt.getTime() + (lifetime * 1000));
            }

            var origin_url      = document.referrer;
            var utm_medium      = getParamSalesOrigin('utm_medium');
            var utm_term        = getParamSalesOrigin('utm_term');
            var utm_content     = getParamSalesOrigin('utm_content');

            var parameters = {
                origin_url:origin_url,
                utm_campaign:utm_campaign,
                utm_source:utm_source,
                utm_medium:utm_medium,
                utm_term:utm_term,
                utm_content:utm_content
            };

            $j.each(parameters,function(code,param){
                if ( param.length > 0 ){
                    Mage.Cookies.set(code,param,expireAt);
                }
            });
            
        }

    });

    function getParamSalesOrigin(key){
        var result = new RegExp(key + "=([^&]*)", "i").exec(window.location.search);
        return result && encodeURI(result[1]) || "";
    }
</script>
