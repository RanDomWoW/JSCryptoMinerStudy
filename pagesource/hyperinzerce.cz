<script type="text/javascript">
	$(document).ready(function () {
		$('.podkategorie_nadpis').click(function () {
			if ($(this).hasClass('close')) {
				console.log($(this).find('.podkategorie_zoznam'));
				$(this).find('.podkategorie_zoznam').css('display', 'block').hide().slideDown();
				$(this).removeClass('close').addClass('open');
				$(this).find('.arrow').addClass('rotated');
			} else if ($(this).hasClass('open')) {
				$(this).find('.podkategorie_zoznam').css('display', 'none').show().slideUp();
				$(this).addClass('close').removeClass('open');
				$(this).find('.arrow').removeClass('rotated');
			}
		})
		$('.vyhoda').click(function () {
			$(this).toggleClass('active').find('.org-list').stop().slideToggle('medium');
		})
		$('.kategorie_nadpis_button').click(function () {
			$('.kategorie_nadpis_button').toggleClass('active');
			$('.zvyhodneni').stop().slideToggle('medium')
		})
	});
</script>

