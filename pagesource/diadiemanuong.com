            function showBottomRightAds(isClick) {
                $('#show-bottom-right-ads').stop(true, true).animate({ height: isClick == true ? 'toggle' : 'show' });
                $('#hide-bottom-right-ads').stop(true, true).animate({ height: isClick == true ? 'toggle' : 'hide' });
            }

            function hideBottomRightDownloadApp(isClick) {
                try {
                $('#show-bottom-right-download-app').stop(true, true).animate({ height: isClick == true ? 'toggle' : 'hide' });
                if (isClick == true) {
                    //#region [save Cookie]
                    var dateNow = new Date();
                    Cookies.set('dateSkipBannerDownloadApp', dateNow.toString(), { expires: 1, path: '/', httponly: true });
                    //#endregion
                }
                } catch (ex) { }
            }

            function showBottomRightDownloadApp(isClick) {
                try {
                    var strDateSync = Cookies.get('dateSkipBannerDownloadApp');
                    if (strDateSync != null && strDateSync != "") {
                        var dateSync = new Date(strDateSync);
                        var dateNow = new Date();
                        if (!isNaN(dateSync)) {
                            if (Date.parse(dateNow) - Date.parse(dateSync) > (1 * 24 * 60 * 60 * 1000)) { // 1 ngày
                                $('#show-bottom-right-download-app').stop(true, true).animate({ height: isClick == true ? 'toggle' : 'show' });
                            } else {
                                $('#show-bottom-right-download-app').stop(true, true).animate({ height: isClick == true ? 'toggle' : 'hide' });
                            }
                        } else {
                            $('#show-bottom-right-download-app').stop(true, true).animate({ height: isClick == true ? 'toggle' : 'show' });
                        }
                    } else {
                        $('#show-bottom-right-download-app').stop(true, true).animate({ height: isClick == true ? 'toggle' : 'show' });
