<script type="text/template" id="schedule-time-template">
    <@
        function formatImg(url,size){
            if(!url){
                return 'http://i1.letvimg.com/lc03_iscms/201601/11/15/15/81939676d8af46c49976f18a54f3140b.png';
            }else{
                var len= url.lastIndexOf('.');
                return url.substring(0,len)+size+url.substring(len,url.length);
            }
        }
        function formatLink(obj){
            if(obj.isLive) {
                return obj.playLink;
            }else{
                return obj.isTextLive? obj.tLiveLink:obj.playLink;
            }
        }
        function getStatus(status){
            var arrStatus=['未开始', '直播中','已结束'];
            return arrStatus[status];
        }
        function getScores(status,score){
            if(status==0){
                return '-';
            }else{
                score = score?score:'0';
                return score;
            }
        }
        function getStatusCls(status){
            var arrStatus=['nostart', 'ing','end'];
            return arrStatus[status];
        }
        function formatTime(t){
            if(t){
            return t.substring(4,6)+'-'+t.substring(6,8)+' '+ t.substring(8,10)+':'+ t.substring(10,12);
            }
        }
    @>

    <@if (item.vs == 1) {@>
        <a href="<@=formatLink(item) @>" target="_blank">
            <div class="status-top">
                <span class="group"><i class="long"><@=item.cname@></i> </span>
                <@ if(item.isEpisodePay == 1) { @>
                <span class="member"></span>
                <@ } @>

                <@ if(item.status == 0) {@>
                <span class="start-time"><@=formatTime(item.startTime)@></span>
                <@ } else{ @>
                <span class="diff-common <@= getStatusCls(item.status) @>"><@= getStatus(item.status) @></span>
                <@ } @>
            </div>

            <div class="team">
                <img src="<@=item.competitors[0].picture@>">
                <span class="team-name"><@=item.competitors[0].name @></span>
                <span class="score"><@= getScores(item.status,item.competitors[0].score)@></span>
