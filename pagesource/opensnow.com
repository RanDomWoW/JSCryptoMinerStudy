<script>

    var $window = $(window),
    $stickyEl = $('#main'),
    $stickyNav = $('.site-navigation'),
    elTop = $stickyNav.offset().top;
    $window.scroll(function() {
        $stickyEl.toggleClass('sticky-nav', $window.scrollTop() > elTop);
    });

    $.widget( "custom.catcomplete", $.ui.autocomplete, {
      _create: function() {
        this._super();
        this.widget().menu( "option", "items", "> :not(.ui-autocomplete-category)" );
      },
      _renderMenu: function( ul, items ) {
        var that = this,
          currentCategory = "";
        $.each( items, function( index, item ) {
          var li;
          if ( item.category != currentCategory ) {
            ul.append( "<li class='ui-autocomplete-category'>" + item.category + "</li>" );
            currentCategory = item.category;
          }
          li = that._renderItemData( ul, item );
          if ( item.category ) {
            li.attr( "aria-label", item.category + " : " + item.label );
          }
        });
      }
    });

    $.ajax({
        url: "/snips/searchalldata",
        type: "GET",
        dataType: "json",
        cache: true
    })
    .done(function( jsondata ) {

        var isVarPicked = false;
        $("#global-search").catcomplete({
            source: jsondata,
            focus: function( event, ui ) {
                $("#global-search").val( ui.item.label );
                return false;
            },
            select: function( event, ui ) {
                $("#global-search").val( ui.item.label );
                isVarPicked = true;
                window.location.href = ui.item.value;
                return false;
            },
            close: function( event, ui ) {
                if (!isVarPicked) {
                    $("#global-search").val("");
                }
            }
        });

    });

    var user_timezone = jstz.determine();
    var user_timezone_name = user_timezone.name();
    var skiptime = 1000*60*60;
    var timezone_expire = new Date((new Date()).valueOf() +  skiptime);
    document.cookie = "usertimezone="+ user_timezone_name +";expires=" +  timezone_expire.toUTCString() + ";path=/";

    
    
    $("#neverAgainSub").click(function() {
        var skiptime = 1000*60*60*24*900;
        var skipexpires = new Date((new Date()).valueOf() +  skiptime);
        document.cookie = "subpopover=true;expires=" +  skipexpires.toUTCString() + ";path=/";
        $('#emailPopover').modal('hide');
    });
    $("#notNowSub").click(function() {
        var skiptime = 1000*60*60*24*7;
        var skipexpires = new Date((new Date()).valueOf() +  skiptime);
        document.cookie = "subpopover=true;expires=" +  skipexpires.toUTCString() + ";path=/";
        $('#emailPopover').modal('hide');
    });

    $('#mc-embedded-subscribe-form-footer').submit(function() {
        $('#mc_embed_signup-footer').hide();
        $('#mceresult-loading-footer').hide();
        $.ajax({
            url: '/snips/subscribeemail',
            type: 'GET',
            data: 'ajax=true&email=' + escape($('#mce-EMAIL-pop-footer').val()) + '&zipcode=' + escape($('#mce-ZIPCODE-pop-footer').val()),
            success: function(jsa) {
                $('#mceresult-loading-footer').hide();
                $('#mceresult-pop-footer').html('Awesome! Thanks for signing up!');
                $('#mceresult-pop-footer').removeClass();
                $('#mceresult-pop-footer').addClass("alert alert-success");
                $('#mc_embed_close-footer').show();
                var skiptime = 1000*60*60*24*900;
                var skipexpires = new Date((new Date()).valueOf() +  skiptime);
                document.cookie = "subpopover=true;expires=" +  skipexpires.toUTCString() + ";path=/";
            },
            error: function(jsa) {
                $('#mceresult-loading-footer').hide();
                $('#mc_embed_signup-footer').show();
                //$('#mceresult-pop-footer').html('Sorry, an error occurred.');
                $('#mceresult-pop-footer').html(jsa.responseJSON.msg);
                $('#mceresult-pop-footer').removeClass();
                $('#mceresult-pop-footer').addClass("alert alert-danger");
            }
        });
        return false;
    });

    $('#mc-embedded-subscribe-form-groups').submit(function() {
        $('#mc_embed_signup-groups').hide();
        $('#mceresult-loading-groups').hide();
        $.ajax({
            url: '/snips/subscribeemailgroups',
            type: 'GET',
            data: 'ajax=true&email=' + escape($('#mce-EMAIL-groups').val()) + '&zipcode=' + escape($('#mce-ZIPCODE-groups').val()) + '&group_name=' + escape($('#mce-GROUPNAME-groups').val()),
            success: function(jsa) {
                $('#mceresult-loading-groups').hide();
                $('#mceresult-groups').html('Awesome! Thanks for signing up!');
                $('#mceresult-groups').removeClass();
                $('#mceresult-groups').addClass('alert alert-success');
            },
            error: function(jsa) {
                $('#mceresult-loading-groups').hide();
                $('#mc_embed_signup-groups').show();
                $('#mceresult-groups').html(jsa.responseJSON.msg);
                $('#mceresult-groups').removeClass();
                $('#mceresult-groups').addClass('alert alert-danger');
            }
        });
        return false;
    });


    function loadOSAds() {
        var width = $( window ).width();
        if (width >= 768) {
            $('.adunit-mobile').each(function () {
                $( this ).removeClass( "adunit-active" );
            });
            $('.adunit-web').each(function () {
                $( this ).addClass( "adunit-active" );
            });
        } else {
            $('#topad').hide();
            $('.adunit-web').each(function () {
                $( this ).removeClass( "adunit-active" );
            });
            $('.adunit-mobile').each(function () {
                $( this ).addClass( "adunit-active" );
            });
        }
        $('.adunit-both').each(function () {
            $( this ).addClass( "adunit-active" );
        });

        
        $('.adunit-active').dfp({
            dfpID: '7700593',
            collapseEmptyDivs: true,
            refreshExisting: true,
            enableSingleRequest: true,
            setCentering: true,
            setTargeting: {
                "Model":"home",
                "State":"",
                "Location":"",
                "Dailysnow":"",
                "Region":"",
            },
            afterAllAdsLoaded: function(data) {
                 if (typeof sidebarScroller == 'function') {
                     sidebarScroller();
                 }
            }
        });
    }

    function adBlockDetectedMain() {
                    $('.undertext').show();
            }

    function adBlockNotDetectedMain() {
        $('.undertext').hide();
    }

    if(typeof fuckAdBlock === 'undefined') {
        adBlockDetectedMain();
    } else {
        fuckAdBlock.on(true, adBlockDetectedMain).onNotDetected(adBlockNotDetectedMain);
    }

    // Hide ads on certain pages
                        loadOSAds();
            

</script>

        
