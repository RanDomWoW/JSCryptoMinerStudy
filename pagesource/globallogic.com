    var $div = $(".subnav");
    if ($(window).scrollTop() > $div.data("top")) { 
        $div.addClass('subnav_fixed'); 
    }
    else {
        $div.removeClass('subnav_fixed');
    }
}

$(".subnav").data("top", $(".subnav").offset().top); // set original position on load
$(window).scroll(fixDiv);


