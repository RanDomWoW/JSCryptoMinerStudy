<script type="text/javascript" language="javascript">
    var default_city_search_value = "城市名、拼音、电话区号";
    function addOption(obj,value,name,flag){
        var opt = $("<option/>").html(name).val(value).attr("selected",flag);
        obj.append(opt);
    }
    $(document).ready(function(){
        var windowWidth = $(window).width()

        if(windowWidth > 1900){
            $(".banner").addClass("maxBanner");
        }else{
            $(".banner").removeClass("maxBanner");
        };
        var host = window.location.host;
        var arr = host.split(".");
        var ext = window.ext = (arr.length==3)?("."+arr[1]+"."+arr[2]):".haofang.net";
        var cookieinfo = $.cookie("citydomain");
        if(!!cookieinfo){
            cookieinfo = decodeURI(cookieinfo);

            arr = cookieinfo.split("|");
            var sub = arr[1].replace(/\.haofang\.net/gi,'');
            $("#sale_href").attr("href","http://"+sub+".haofang.net/sale/");
            $("#lease_href").attr("href","http://"+sub+".haofang.net/lease/");
            $("#build_href").attr("href","http://"+sub+".haofang.net/build/");
        }
        var regInt = /^\d+$/;
        var $prov = $('#id_prov');
        $prov.html("");
        addOption($prov,'', '请选择省份', true);
        var $city = $('#id_city');
        for(var prov in window.provData) {
            regInt.test(prov) && addOption($prov,prov, window.provData[prov]['name'], false);
        }
        $prov.change(function(){
            var prov = this.value;
            $city.html("");
            if (regInt.test(prov)) {
                var cityList = window.provData[prov]['city'];
                for (var i=0, n=cityList.length; i<n; ++i) {
                    addOption($city,cityList[i][2], cityList[i][1], false);
                }
            } else {
                addOption($city,'', '请选择城市', true);
            }
        });
        $('#id_select_city').click(function(){
            var domain = $city.val();
            if (domain!=='' && domain!=='请选择城市'){
                window.location = 'http://' + domain + window.ext+'/sale/';
            }
        });
        $("body").click(function(){$('#ten_citybox').hide();});
        $('#ten_citybox').delegate("li","mouseover",function(){
            $(this).addClass("ac_over");
        }).delegate("li","mouseout",function(){
            $(this).removeClass("ac_over");
        }).delegate("li","click",function(){
            $('#city_search').val($(this).html()).attr("domain",$(this).attr("domain"));
            window.location = "http://" + $(this).attr("domain") +"/sale/";
        });
        $('#city_search').val(default_city_search_value).focus(function(){
            city = $(this).val();if('' == city || default_city_search_value == city){$(this).val("");}
        }).blur(function(){
            city = $(this).val();if('' == city ){$(this).val(default_city_search_value);$('#ten_citybox').hide();}
        }).keyup(function(){
            var inValue = $('#city_search').val();
            if(inValue==""){
                $('#ten_citybox').hide();
                return;
            }
            inValue = encodeURIComponent(inValue);
            //inValue = encodeURI(inValue);//调用jquery时，只需执行一次encodeURI，内部执行了encodeURIComponent
            $.get("/Home/Index/getCity",{"in":inValue,"temp":Math.random()},function(data){
                $('#ten_cityUL').html("");
                if(data=="[]"){
                    $('#ten_cityUL').html("<li class=\"noin\">暂无此城市</li>");
                }else{
                    var json = eval(data);

                    var len = json.length;
                    $('#ten_cityUL').html("");
                    for (var i = 0; i < len &&i < 10; i++) {
                        var d = json[i].d;
                        d = d.replace(/\.myfun7\.com/gi,window.ext);
                        $("<li/>").attr("domain",d).html(json[i].n).appendTo("#ten_cityUL");
                    }
                    $('#ten_citybox').show();
                }
            });
        });
        if(host.indexOf("haofang.net")==-1){
            $("#citybox a,#cookiecity,.chose a").each(function(){
                var href = $(this).attr("href");
                $(this).attr("href",href.replace(/\.myfun7\.com/gi,ext));
            });
        }
        if(window.location.host.indexOf("haofang.net") < 0){
	        $("body").html($("body").html().replace("haofang.net","51vfang.cn"));
        }
    });
    function gotocity(){
        var d = $('#city_search').attr("domain");
        if (d!==''){
            window.location = "http://" + d +"/";
        }else{
            var inValue = $('#city_search').val();
            if(inValue=="" || inValue==default_city_search_value){
                alert("请先输入"+default_city_search_value+"匹配城市后，再点击进入城市");
                return;
            }
            inValue = encodeURI(encodeURI(inValue));
            $.get("/Home/Index/getCity",{"in":inValue,"temp":Math.random()},function(data){
                $('#ten_cityUL').html("");
                if(data=="[]"){
                    alert("没有匹配到相关城市，请重新输入");
                }else{
                    var json = eval(data);
                    var len = json.length;
                    if(len==1){
                        var d = json[i].d;
                        d = d.replace(/\.myfun7\.com/gi,window.ext);
                        window.location = "http://" + d +"/";
                    }else if(len>1){
                        $('#ten_cityUL').html("");
                        for (var i = 0; i < len &&i < 10; i++) {
                            var d = json[i].d;
                            d = d.replace(/\.myfun7\.com/gi,window.ext);
                            $("<li/>").attr("domain",d).html(json[i].n).appendTo("#ten_cityUL");
                        }
                        $('#ten_citybox').show();
                    }
                }
            });
        }
    }
