<h3 id="newsletter_process">SUBSCRIBE TO OUR NEWSLETTER</h3>
                        
                        <input type="text" placeholder="Enter Your Email; Its Free" id="newsletter_email"/>
                        <input type="button" value="Submit" onclick="if (document.getElementById('newsletter_email').value != ''){subscribe_newsletter();}else{document.getElementById('newsletter_process').innerHTML = 'Provide email address to continue';}"/>
                        <div class="push_top">
                            <input type="radio" name="subscribe" value="yes" id="subscribe"/><label for="subscribe">Subscribe</label>
                            &nbsp;&nbsp;
                            <input type="radio" name="subscribe" value="yes" id="unsubscribe"/><label for="unsubscribe">Unsubscribe</label>
                        </div>
                        
<script>
//place functions for performing the ajax requests
function subscribe_newsletter()
{
	var option = "yes";
	if(document.getElementById('unsubscribe').checked = true)
	{
		option = "no";
	}
	var url = "";
	//use php to determine where the processing is
		url = "admin_controller/misc/newsletterProcess.php";
			
	url = url + "?email="+escape(document.getElementById('newsletter_email').value)+"&option="+option;
	try
	{
		document.getElementById('newsletter_email').disabled = true;
		document.getElementById('newsletter_process').innerHTML = "Processing.. Please wait.";
		
		asyncNewsLetter = new XMLHttpRequest();		
		asyncNewsLetter.open("GET", url, true);//prepare the request
		asyncNewsLetter.onreadystatechange = handle_newsletter;
		asyncNewsLetter.send(null); //send the request
				
	}
	catch (exception)
	{
		document.getElementById("newsletter_process").innerHTML = "Request Failed!: Please check your internet connection";
	}
	
}

function handle_newsletter ()
{
	if (asyncNewsLetter.readyState == 4 && asyncNewsLetter.status == 200)
	{
		document.getElementById("newsletter_process").innerHTML = asyncNewsLetter.responseText;
		document.getElementById('newsletter_email').disabled = false;
		if (asyncNewsLetter.responseText == "Newsletter subscription successful")
		{
			document.getElementById('newsletter_email').value = '';
			setTimeout(function(){document.getElementById("newsletter_process").innerHTML = "SUBSCRIBE TO OUR NEWSLETTER";},10000);
		}
	}
}
</script>                    </div>
