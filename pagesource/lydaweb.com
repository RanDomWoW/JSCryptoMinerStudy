                    1200:{
                        items:4
                    }
                }
            });
        });

    </script>
<script>
    window.onload = function () {
        $('.lazy').lazy();
    };
    /* login with Oauth */
    function oauthLogin(service,popup) {
        var popup=popup==undefined?false:popup;

        switch(service){
            case 'google': _url='https://www.lydaweb.com/oauth/google';break;
            case 'github': _url='https://www.lydaweb.com/oauth/github';break;
            case 'linkedin': _url='https://www.lydaweb.com/oauth/linkedin';break;
        }

        var win = window.open(_url, "windowfzToYIqZ8XNlDYUd", 'width=800, height=600');

        var pollTimer = window.setInterval(function() {
            try {
                console.log(win.document.URL);
                if (win.document.URL.indexOf('wasLogin') != -1) {
                    // user is logged in
                    window.clearInterval(pollTimer);
                    win.close();
                    if(popup==true)
                        document.location.href=document.location.href;
                    else
                        document.location.href='https://www.lydaweb.com';
                }
            } catch(e) {
            }
        }, 100);
    }
    function openAuth(type) {
        type=type!=undefined?type:'login';
        $modal=$('#auth-modal');
        if(type=='login'){
            $modal.find('.title').html('ورود به سایت');
        }
        else{
            $modal.find('.title').html('ثبت نام در لیدواب');
        }
        $modal.modal('show');
        $modal.find('.modal-body').find('.content').html('');
        $modal.find('.modal-body').find('.loading').show();
        $.ajax({
            url:'/auth/get-form',
            data:{type:type,_token:window.Laravel.csrfToken},
            type: 'post',
            success:function(data) {
                $modal.find('.modal-body').find('.content').html(data);
                $modal.find('.modal-body').find('.loading').hide();
            },
            error:function (error) {
                console.log('server error ...');
                $modal.find('.modal-body').find('.loading').hide();
            }
        });
    }

    $(".onlyInputEn").keypress(function(event){
        message='';
        if($(this).data('message'))
            message=$(this).data('message');

        var ew = event.which;
        if(ew > 128){
            alert(message);
            return false;
        }
    });
    $(".onlyInputFA").keypress(function(event){
        message='';
        if($(this).data('message'))
            message=$(this).data('message');

        var ew = event.which;
        if(ew >= 65 && ew < 128){
            alert(message);
            return false;
        }
    });

    function back_top(){
        $('html , body').stop().animate({
            scrollTop: 0
        }, 300);
    }

    $(window).scroll(function(){
        var height = $(document).scrollTop();
        if(height>10)
        {
            $(".btn_back_top").css("right","15px");
            $(".btn_back_top").css("bottom","20px");
        }
        else if(height<10)
        {
            $(".btn_back_top").css("right","-50px");
            $(".btn_back_top").css("bottom","-50px");
        }
    });
 	var wow = new WOW(
            {
                boxClass:     'wow',      // animated element css class (default is wow)
                animateClass: 'animated', // animation css class (default is animated)
                offset:       0,          // distance to the element when triggering the animation (default is 0)
                mobile:       true,       // trigger animations on mobile devices (default is true)
                live:         true,       // act on asynchronously loaded content (default is true)
                callback:     function(box) {
                    // the callback is fired every time an animation is started
                    // the argument that is passed in is the DOM node being animated
                },
                scrollContainer: null // optional scroll container selector, otherwise use window
            }
    );

    wow.init();


    (function ($) {
        // owl carousel
        var $megaCarousel= $('.mega-carousel');
        $megaCarousel.owlCarousel({
            rtl:true,
            loop:true,
            margin:10,
            nav:true,
            dots: false,
            navText: ["<i class='glyphicon glyphicon-chevron-right'></i>","<i class='glyphicon glyphicon-chevron-left'></i>"],
            responsive:{
                0:{
                    items:1
                },
                600:{
                    items:3
                },
                1000:{
                    items:4
                },
                1200:{
                    items:5
                }
            }
        });
        $megaCarousel.show(1000);

        // css menu for sidebar
        $('#cssmenu li.active').addClass('open').children('ul').show();
        $('#cssmenu li.has-sub>a').on('click', function(){
            $(this).removeAttr('href');
            var element = $(this).parent('li');
            if (element.hasClass('open')) {
                element.removeClass('open');
                element.find('li').removeClass('open');
                element.find('ul').slideUp(200);
            }
            else {
                element.addClass('open');
                element.children('ul').slideDown(200);
                element.siblings('li').children('ul').slideUp(200);
                element.siblings('li').removeClass('open');
                element.siblings('li').find('li').removeClass('open');
                element.siblings('li').find('ul').slideUp(200);
            }
        });
        $('[data-scroll="ns"]').niceScroll();
        $('.lyda-sidebar-menu > #cssmenu ').niceScroll({
            railalign: 'left',
            cursorcolor: "#c8c8c8", // change cursor color in hex
            autohidemode: false,
            cursorminheight: 32, // set the minimum cursor height (pixel)
            cursorwidth: "7px", // cursor width in pixel (you can also write "5px")
            railpadding: { top: 0, left: 3, bottom: 0 }// set padding for rail bar
        });

        $('[data-toggle="tooltip"]').tooltip();
        $('[data-toggle="lyda-modal"]').click(function () {
            $($(this).data('target')).show();
        });

        $('.lyda-modal .close').click(function () {
            $('.lyda-modal').fadeOut(200);
        });
        $("#toggle-menu").click(function () {
            SideBarMenu();
        });


        $('.lydaweb-search ,.lydaweb-search *').click(function() {
            changeElementsStayleForSearch('open',true);
        });
        $('.lydaweb-search .input').focus(function() {
            changeElementsStayleForSearch('open');
        });
        $('.lydaweb-search .input').blur(function() {
            changeElementsStayleForSearch('close');
        });
    })(jQuery);

    function changeElementsStayleForSearch(type,otherClick) {
        if(type=='open'){
            $('#lydaweb-search').addClass('lydaweb-search-active',500);

            if(otherClick==true)
                $('.lydaweb-search .input').focus();
        }
        else{
            $('#lydaweb-search').removeClass('lydaweb-search-active ',100);
        }
    }


    var toggleSideBarMenu=false;
    var lydaSidebarMenu=$('.lyda-sidebar-menu');

    function SideBarMenu(id,nclass) {

        if(toggleSideBarMenu==false)
        {
            lydaShowMask(id,true);
            lydaSidebarMenu.addClass('show');
            if(nclass!=undefined)
                lydaSidebarMenu.addClass(nclass);

            toggleSideBarMenu=true;
        }
        else
        {
            lydaShowMask(id,false);
            lydaSidebarMenu.removeClass('show');
            if(nclass!=undefined)
                lydaSidebarMenu.removeClass(nclass);
            toggleSideBarMenu=false;
        }
    }

    function lydaShowMask(id,action){
        if(action==false)
            $("#"+id).fadeOut(100);
        else
            $("#"+id).fadeIn(100);
    }

</script>
</body>
</html>