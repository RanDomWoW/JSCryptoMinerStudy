				function execute_wwmfun(e) { 
					var description =jQuery(e).data("content");
					var utm = jQuery(e).data("utmdata");

					var img_url = jQuery(e).data("img");
					//var post_url =document.querySelector("link[rel='canonical']").getAttribute("href");
					var post_url = jQuery(e).data("permalink");
					
					var i = WWWM_FilterData(jQuery('meta[name="og:title"]').attr("content"));
					
					//----------------------------------------------------------- pinterest 
					
					if (jQuery(e).hasClass("pinterest")) {
						var pinterestshareurl = "//www.pinterest.com/pin/create/button/" + "?url=" + post_url+utm  + "&media=" + img_url + "&description=" + description;							
						window.open(pinterestshareurl, "", "menubar=no,toolbar=no,resizable=yes,scrollbars=no,height=400,width=600");
						return false
					}
					
					//----------------------------------------------------------- facebook
					
					if (jQuery(e).hasClass("facebook_custom")) {
						window.open(jQuery(e).attr('href'), 'fbShareWindow', 'height=450, width=550, top=' + (jQuery(window).height() / 2 - 275) + ', left=' + (jQuery(window).width() / 2 - 225) + ', toolbar=0, location=0, menubar=0, directories=0, scrollbars=0');
						return false;
					}
					//----------------------------------------------------------- twitter
					 if (jQuery(e).hasClass("wwm_twitter")) {
						var twittwr_url = "https://twitter.com/intent/tweet?url=" + post_url + utm + "&text=" + description + "%20%7C";
						window.open(twittwr_url, "","menubar=no,toolbar=no,resizable=yes,scrollbars=no,height=400,width=600");
						return false;
					 }
				}
				
				function WWWM_FilterData(e) {
					if (jQuery.trim(e) != "") 
						return e.replace(/[^\w\sàâäôéèëêïîçùûüÿæœÀÂÄÔÉÈËÊÏÎŸÇÙÛÜÆŒ]/g, "");
					else return "";
				}
					
