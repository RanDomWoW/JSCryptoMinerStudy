    function GetHolidayList_StockMarketTicker() {
        
        $.ajax({
            url: 'https://www.dsij.in/desktopmodules/services/api/StockTicker/DsijHoliday',
            //url: 'http://192.168.1.91/dnndalalstreetjournal/desktopmodules/services/api/StockTicker/DsijHoliday',
            data: "Exchange=50",
            dataType: "json", type: "GET", contentType: "application/json; charset=utf-8",
            success: function (data) {
                var resposehold = JSON.parse(data);
                var temp = resposehold[0].HolidaySatus;
                if (temp == 1) {
                    GetIntradayPrices_StockMarketTicker();
                }
                else {
                    //print its holiday
                    //alert("Holiday");
                    $('#graph2').css('display', 'block');
                    $('#SowHolidayId').css('display', 'block');

                    $('#graph1').css('display', 'none');
                    
                    //document.getElementById("SowHolidayId").innerText = "Holiday";                    
                }
            },
            error: function (response) {
                //alert(response);
            },
            failure: function (response) {
                //alert(response);
            }
        });
    }
    function GetIntradayPrices_StockMarketTicker() {
        $.ajax({
            url: 'https://www.dsij.in/desktopmodules/services/api/StockTicker/GetIntradayPrices',
            //url: 'http://192.168.1.91/dnndalalstreetjournal/desktopmodules/services/api/StockTicker/GetIntradayPrices',
            data: "CompanyCode=0&Exchange=" + $("#market").html(),
            dataType: "json",
            type: "GET",
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                data = JSON.parse(data);
                var oilPrices = "[";
                var exchangeRates = "[";
                $.each(data, function (index, value) {
                    oilPrices = oilPrices + "[" + (value.DateTime).replace(/\D/g, "") + "," + value.Current + "],";
                    exchangeRates = exchangeRates + "[" + (value.DateTime).replace(/\D/g, "") + "," + value.CMP + "],";
                })
                exchangeRates = exchangeRates.slice(0, -1) + "]";
                oilPrices = oilPrices.slice(0, -1) + "]";
                DrawGraph_StockMarketTicker(JSON.parse(oilPrices), JSON.parse(exchangeRates));
            },
            error: function (response) {
                //alert(response);
            },
            failure: function (response) {
                //alert(response);
            }
        });
    }

    function DrawGraph_StockMarketTicker(oilPrices, exchangeRates)
    {
        //;
        var gcolor;
        if ($('#dnn_dnnSTOCKMARKETTICKER_lblchg').html() >= 0)
            gcolor = '#279800'
        else
            gcolor = '#e14c4c'
        var data = [
         { data: oilPrices, color: gcolor },
         { data: exchangeRates,color:'#ccc' }
        ];

        var options = {
            canvas: false,
            xaxes: [
                {
                    mode: "time",
                }
            ],
            xaxis: {
                show: false,
                ticks: 0
            },
            yaxis: {
                show: true,
                ticks: 0
            },
            grid: {
                borderColor: 'white',
                borderWidth: {
                    top: 0,
                    right: 0,
                    bottom: 0,
                    left: 1
                }

            }
        }
        $.plot("#placeholder", data,options);
    }
</script>

