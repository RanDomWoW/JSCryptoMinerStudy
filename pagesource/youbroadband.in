<script type="text/javascript">
	function validateAmazonForm(){
//		console.log("function called");
		var custinterest = $("input[type='radio'][name='interested']:checked").val();
		var custoffer = $("input[type='radio'][name='chooseoffer']:checked").val();
		var acdetail = $("#custdetail").val();
		if(custinterest == null && custoffer == null && acdetail == "") {
			alert("Please select options.");
		}
 		if(custinterest == null){
			alert("Please select interest.");
			$("input[type='radio'][name='interested']").focus();
			return false;
		}
		if(custoffer == null){
			alert("Please select any offer.");
			$("input[type='radio'][name='chooseoffer']").focus();
			return false;
		}
		if(acdetail == ""){
			alert("Please enter account number / login id / register mobile number.");
			$("#custdetail").focus();
			return false;
		}
		
		var ajaxUrl = "https://itapps.youbroadband.in/default/survey_wl/AmazonFireTVAction.jsp";
		var custdata = "op1="+custinterest+"&op2="+custoffer+"&op3="+acdetail;
		var http_request;
		try{
			http_request = new XMLHttpRequest();
//			http_request.withCredentials = true;
		} catch (e){
		// Internet Explorer Browsers
		try{
			http_request = new ActiveXObject("Msxml2.XMLHTTP");
		}catch (e) {
			try{
				http_request = new ActiveXObject("Microsoft.XMLHTTP");
			}catch (e){
				// Something went wrong
				alert("Your browser broke!");
				return false;
			}
		}
	}
		
		http_request.onreadystatechange = function(){
			if (http_request.readyState == 4 && http_request.status == 200){
//				console.log("http_request status : "+http_request.status);
					try{
						var jsonObj = JSON.parse(http_request.responseText);
//						console.log("jsonObj : "+jsonObj);
//						console.log("jsonObj msg : "+jsonObj.msg);
						if(jsonObj.msg === "Success") { 
							typeof _ga_send_ === "function" && (_ga_send_(["AmazonFireTV","Survey","done"]));
							document.getElementById("message").innerHTML = "Thank you for your response. Your inputs will help us in developing plans that suit your need.";
							$(".form-class").hide();
							$(".suc-form").show();
							$("#message").removeClass("red").addClass("green");
						} else if(typeof(jsonObj.msg) === "undefined"){
							typeof _ga_send_ === "function" && (_ga_send_(["AmazonFireTV","Survey","error"]));
							document.getElementById("message").innerHTML = "Oops! something went wrong. Please try again.";
							$(".suc-form").show();
							$("#message").addClass("red");
						} else {
							typeof _ga_send_ === "function" && (_ga_send_(["AmazonFireTV","Survey",jsonObj.msg]));
							document.getElementById("message").innerHTML = jsonObj.msg;
							$(".suc-form").show();
							$("#message").addClass("red");
						}
					} catch(e){
						alert("Something went wrong. Please try again");
						return false;
					}
			}
		}
		
		http_request.open("POST", ajaxUrl, true);
		http_request.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
		http_request.send(custdata);
		return false; 
	}
</script>

