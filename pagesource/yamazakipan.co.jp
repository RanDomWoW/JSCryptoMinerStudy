    <script>
        //ウインドウ幅調整
        $(function() {
            $(window).bind("RESIZE", resize);
            $("#modal").hide();
            resize();
            if (BrowserInfo.iphone || (BrowserInfo.android && BrowserInfo.mobile)) {
                $(".nopc").show();
            }
        })

        function resize() {
            var w = $(window).width()
            if (w > 950) {
                $("#wrapper").width("100%");
                $("#modal").width("100%");
            } else {
                $("#wrapper").width("950px");
                $("#modal").width("950px");
            }
            $("#modal").css({
                opacity: 0
            });
            $("#modal").height($(window).height());
        }
        //カルーセル
        var total;
        var nowPos = 0;
        var interval = 7000;
        $(function() {
            total = $("#mainvisual .carousel li").length;
            $("#mainvisual .carousel li").each(function(i) {
                this.id = i;
                this.pos = i;
                if (this.pos >= (total - 2)) this.pos -= total;
                $(this).css({
                    left: this.pos * 100
                });
                if (this.pos == 0) {
                    $("img", this).stop(true).css({
                        opacity: 1
                    });
                    $(this).css("zIndex", "999");
                } else {
                    $("img", this).stop(true).css({
                        opacity: 0
                    });
                    $(this).css("zIndex", "auto");
                }
            })
            for (var i = 0; i < total; i++) {
                var html = '<li><a href="javascript:void(0)" class="opacityover">' + i + '</a></li>';
                var obj = $(html);
                obj[0].id = i;
                obj.bind("click", function() {
                    jump(this.id);
                });
                $("#mainvisual .currentarea .current").append(obj)
            }
            checkCurrent();
            $("#wrapper").stop(true).animate({
                opacity: 1
            }, interval, "linear", next);
            $("#mainvisual").each(function() {
                $(this).bind("mouseover", function() {
                    $("#wrapper").stop(true);
                });
                $(this).bind("mouseout", function() {
                    $("#wrapper").stop(true).animate({
                        opacity: 1
                    }, interval, "linear", next);
                });
            })
        });

        function next() {
            nowPos += 1;
            if (nowPos >= total) nowPos = 0;
            $("#mainvisual .carousel li").each(function(i) {
                if (this.id == nowPos) {
                    $(this).css("zIndex", "999");
                    $(this).stop(true).animate({
                        left: 100
                    }, 0);
                    $(this).stop(true).animate({
                        left: 0
                    }, 1500, "easeInOutQuart");
                    $("img", this).stop(true).animate({
                        opacity: 1
                    }, 1500, "easeInOutQuart");
                } else {
                    $(this).css("zIndex", "auto");
                    $(this).stop(true).animate({
                        left: -100
                    }, 1500, "easeInOutQuart");
                    $("img", this).stop(true).animate({
                        opacity: 0
                    }, 1500, "easeInOutQuart");
                }
            });
            checkCurrent();
            $("#wrapper").stop(true).animate({
                opacity: 1
            }, interval, "linear", next);
        }

        function prev() {
            nowPos -= 1;
            if (nowPos < 0) nowPos = total - 1;
            $("#mainvisual .carousel li").each(function(i) {
                if (this.id == nowPos) {
                    $(this).css("zIndex", "999");
                    $(this).stop(true).animate({
                        left: -100
                    }, 0);
                    $(this).stop(true).animate({
                        left: 0
                    }, 1500, "easeInOutQuart");
                    $("img", this).stop(true).animate({
                        opacity: 1
                    }, 1500, "easeInOutQuart");
                } else {
                    $(this).css("zIndex", "auto");
                    $(this).stop(true).animate({
                        left: 100
                    }, 1500, "easeInOutQuart");
                    $("img", this).stop(true).animate({
                        opacity: 0
                    }, 1500, "easeInOutQuart");
                }
            });
            checkCurrent();
            $("#wrapper").stop(true).animate({
                opacity: 1
            }, interval, "linear", next);
        }

        function jump(num) {
            var n = num - nowPos;
            if (n > 0) {
                $("#mainvisual .carousel li").each(function(i) {
                    if (this.id == num) {
                        $(this).css("zIndex", "999");
                        $(this).stop(true).animate({
                            left: 100
                        }, 0);
                        $(this).stop(true).animate({
                            left: 0
                        }, 1500, "easeInOutQuart");
                        $("img", this).stop(true).animate({
                            opacity: 1
                        }, 1500, "easeInOutQuart");
                    } else {
                        $(this).css("zIndex", "auto");
                        $(this).stop(true).animate({
                            left: -100
                        }, 1500, "easeInOutQuart");
                        $("img", this).stop(true).animate({
                            opacity: 0
                        }, 1500, "easeInOutQuart");
                    }
                });
            } else if (n < 0) {
                $("#mainvisual .carousel li").each(function(i) {
                    if (this.id == num) {
                        $(this).css("zIndex", "999");
                        $(this).stop(true).animate({
                            left: -100
                        }, 0);
                        $(this).stop(true).animate({
                            left: 0
                        }, 1500, "easeInOutQuart");
                        $("img", this).stop(true).animate({
                            opacity: 1
                        }, 1500, "easeInOutQuart");
                    } else {
                        $(this).css("zIndex", "auto");
                        $(this).stop(true).animate({
                            left: 100
                        }, 1500, "easeInOutQuart");
                        $("img", this).stop(true).animate({
                            opacity: 0
                        }, 1500, "easeInOutQuart");
                    }
                });
            }
            nowPos = Number(num);
            checkCurrent();
            $("#wrapper").stop(true).animate({
                opacity: 1
            }, interval, "linear", next);
        }

        function checkCurrent() {
            $("#mainvisual .currentarea .current li").each(function() {
                $("a", this).removeClass("active");
                if (nowPos == this.id) $("a", this).addClass("active");
            })
        }
        // RSS別ウインドウ指定
        function wopen04(WO) {
            NewWin = window.open("", "click", "toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no,WIDTH=700,height=700");
            NewWin.parent.location.href = WO;
        }
        //ニュース調整
        $(function() {
            $(".newsarea .news .items").each(function() {
                $(".item:last", this).css("border-bottom", "none");
            })
        })
        //トピックの文字位置調整
        $(function() {
            $(window).bind("RESIZEFONT", reposTopic);
            reposTopic();
        })

        function reposTopic() {
            $("#maincontent .topic").each(function() {
                $(".text", this).css("margin-top", "auto");
                var h = $(this).height() - $(".text", this).height();
                $(".text", this).css("margin-top", ((h - 20) / 2) + "px");
            })
        }
        //メガメニュー表示
        var megaHeight = 0;
        $(function() {
            $(".mega .inner").css("overflow", "hidden");
            $(window).load(function() {
                $(".mega").show();
                megaHeight = $(".mega .inner").height();
                $(".mega .inner").height(0);
                $(".mega").hide();
                $(".lineuparea .lineup a").bind("mouseover", function() {
                    openMega();
                })
                $(".lineuparea .lineup a").bind("mouseout", function() {
                    closeMega();
                });
                $(".lineuparea .mega").bind("mouseover", function() {
                    openMega();
                });
                $(".lineuparea .mega").bind("mouseout", function() {
                    closeMega();
                });
            })
        })

        function openMega() {
            if (!closeMotion) {
                if (!(BrowserInfo.ie && BrowserInfo.ieVersion <= 7)) $("#modal").show();
                $("#modal").stop(true).animate({
                    opacity: 1
                }, 200, "linear");
                $(".mega").show();
                $(".lineuparea .lineup a").addClass("active");
                $(".mega .inner").stop(true).animate({
                    height: megaHeight
                }, 400, "easeOutQuart");
            }
        }

        function closeMega() {
            if (!closeMotion) {
                $("#modal").hide();
                $("#modal").stop(true).animate({
                    opacity: 0
                }, 200, "linear");
                $(".mega .inner").stop(true).animate({
                    height: 0
                }, 400, "easeOutQuart", function() {
                    $(".lineuparea .lineup a").removeClass("active");
                    $(".mega").hide();
                });
            }
        }
        var closeMotion = false;

        function closeMega2() {
            closeMotion = true;
            $("#modal").hide();
            $("#modal").stop(true).animate({
                opacity: 0
            }, 200, "linear");
            $(".mega .inner").stop(true).animate({
                height: 0
            }, 400, "easeOutQuart", function() {
                $(".lineuparea .lineup a").removeClass("active");
                $(".mega").hide();
                closeMotion = false;
            });
            /**/
        }

    </script>

