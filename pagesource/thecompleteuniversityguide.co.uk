    <script>
        function isIOS() {
            return /^iP(od|ad|hone)/.test(navigator.platform);
        }

        var cug = {
            isLoggedIn: false,
            openHoverover: undefined,
            onMediaSizeChange: [],
            registerMediaSizeFunction: function(f) { cug.onMediaSizeChange.push(f); f(cug.currentMediaSize); },
            mediaSizes: [{ name: "narrow", query: "(max-width: 480px)"}, { name: "wide", query: "all" }],
            currentMediaSize: undefined,
            updateMediaSize:
                function () {
                    var newSize;

                    for (var i = 0; i < cug.mediaSizes.length; i++) {
                        if (Modernizr.mq(cug.mediaSizes[i].query)) { newSize = cug.mediaSizes[i].name; break; }
                    }

                    if (newSize != cug.currentMediaSize) {
                        var oldSize = cug.currentMediaSize;
                        cug.currentMediaSize = newSize;

                        $("html")
                            .removeClass("media-" + oldSize)
                            .addClass("media-" + newSize);

                        for(var i = 0; i < cug.onMediaSizeChange.length; i++) {
                            cug.onMediaSizeChange[i](newSize, oldSize);
                        }
                    }

                    return cug.currentMediaSize;
                },

            stickyHeaderMediaSize: undefined,
            stickyHeaderConfig: undefined, 

            checkStickyHeader: function(force) {
                if(!cug.stickyHeaderConfig) return;

                if (cug.stickyHeaderMediaSize && (cug.stickyHeaderMediaSize !== cug.currentMediaSize || force)) {
                    cug.stickyHeaderConfig[cug.stickyHeaderMediaSize].unstick();
                    cug.stickyHeaderMediaSize = undefined;
                }
                
                var currentConfig = cug.stickyHeaderConfig[cug.currentMediaSize];

                if(currentConfig)
                {
                    var shouldBeSticky = $(window).scrollTop() > currentConfig.stickyPos();

                    if (shouldBeSticky && !cug.stickyHeaderMediaSize) {
                        currentConfig.stick();
                        cug.stickyHeaderMediaSize = cug.currentMediaSize;
                    } else if (cug.stickyHeaderMediaSize && !shouldBeSticky) {
                        currentConfig.unstick();
                        cug.stickyHeaderMediaSize = undefined;
                    }
                }
            },

            setLoginState: function(loggedIn, realName) {
                cug.isLoggedIn = !!loggedIn;
                $("#header-profile-link").text(realName);
                $("html")
                    .toggleClass("logged-in", !!loggedIn)
                    .toggleClass("not-logged-in", !loggedIn);
            },

            setModelDialog: function($content) {
                $("html").addClass("modal-dialog-open");
                $(".modal-dialog-content").empty().append($content);
            },
            clearModalDialog: function() {
                $("html").removeClass("modal-dialog-open");
                $(".modal-dialog-content").empty();
            },

            ajaxLoginSettings: {
                method: "POST",
                cache: false,
                success: function(data) {
                    cug.setLoginState(true, data.realName);
                    cug.clearModalDialog();
                    if(cug.ajaxLoginSettings.triggeringElement) cug.ajaxLoginSettings.triggeringElement.click();
                },
                error: function(xhr) {
                    cug.setLoginState(false);
                    cug.setModelDialog($(xhr.responseText));
                }
            }
        }

        $(window).on("resize orientationchange", cug.updateMediaSize);
        cug.updateMediaSize();

        $(window).on("scroll", function () { cug.checkStickyHeader(); });
        $(window).on("resize", function () { cug.checkStickyHeader(true); });
        cug.registerMediaSizeFunction(cug.checkStickyHeader);
        
        function closeHoverover() {
            if (cug.openHoverover) { cug.openHoverover.removeClass("open"); }

            if(isIOS()) {
                $(document.body).css("cursor", "");
            }

            cug.openHoverover = undefined;
        }

        $(document).on("click", closeHoverover);

        $(document).on("mouseenter", ".hoverover", function (e) {
            if (!$(this).is(cug.openHoverover)) {
                closeHoverover();
            }
        });

        $(document).on("click", ".hoverover", function (e) {
            if (cug.openHoverover) { cug.openHoverover.removeClass("open"); }
            $(this).addClass("open");
            cug.openHoverover = $(this);

            if(isIOS()) {
                $(document.body).css("cursor", "pointer");
            }

            e.stopImmediatePropagation();
        })

        $(document).on("click", ".registration-link", function (e) {
            if(!cug.isLoggedIn) {
                cug.ajaxLoginSettings.triggeringElement = this;

                var $triggeringElement = $(this);
                var registrationType = $triggeringElement.data("registration-type");
                _gaq.push(["_trackEvent", "Registration lightbox", "Open", registrationType])

                $.ajax($.extend({},
                    {
                        url: "/Login/AjaxLoginRegister",
                        data: {
                            type: registrationType,
                            institutionId: $triggeringElement.data("institution-id"),
                            subjectId: $triggeringElement.data("subject-ids")
                        },
                    },
                    cug.ajaxLoginSettings));

                e.stopImmediatePropagation();
                e.preventDefault();
            }
        });

        $(document).on("click", ".modal-dialog-wrapper, .modal-dialog-close", function (event) {
            if (event.target == this) cug.clearModalDialog();
        });

        $(document).on("click", ".page-tab.in-page", function () {
            var $tabContainer = $(this).closest(".page-tab-container");
            var clickedTab = $(this).data("tab-name");
            
            $.each($tabContainer.find(".page-tab, .page-tab-content"), function() {
                $(this).toggleClass("current", $(this).data("tab-name") === clickedTab);
            })
        });
        
        $(document).on("focus", ".compact-field-text input", function () {
            $(this).closest(".compact-field").addClass("active").addClass("had-focus");
        });

        $(document).on("blur input change", ".compact-field-text input", function() {
            if (!$(this).val()) {
                $(this).closest(".compact-field").removeClass("active");
            } else {
                $(this).closest(".compact-field").addClass("active");
            }
        });
    </script>

