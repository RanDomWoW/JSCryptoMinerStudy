<script type="text/javascript">
$(document).ready(function(){  
  $('.feedback-submit').on('click',function(){  
        var name = $('#template-op-form-name').val();
        var email = $('#template-op-form-email').val();
        var phone = $('#template-op-form-phone').val();
        var message = $('#template-op-form-message').val();
        var department = $('#template-op-form-department').val();
        var captcha = $('#template-op-form-captcha').val();
      $.ajax({
        url:"https://www.ntc.net.np/feedback/form",
        data:{name:name, email:email, phone:phone, message:message, department:department,captcha:captcha},
        type:"POST",
        success: function(msg){
          var res = JSON.parse(msg);
          if(res.status == 'success'){
            $('#feedback-msg').html(res.message);
            $('#feedback-form')[0].reset();
          }else if(res.status == 'error'){
            $('#feedback-msg').html(res.message);
          }
        }
      });    
    });
});    
</script>
