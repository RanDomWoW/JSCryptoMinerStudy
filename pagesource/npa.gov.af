    <script>
        var actualUrl = window.location.href;

        function shouldRedirect(protocol, actualUrl) {
            return actualUrl.indexOf(protocol + '://npa.gov.af') > -1;
        }
        function handleUrlToRedirect(protocol, actualUrl) {
            var urlParts = actualUrl.split(protocol + '://');
            window.location.href = protocol + '://www.' + urlParts[1];
        }

        if (shouldRedirect('http', actualUrl)) {
            handleUrlToRedirect('http', actualUrl);
        } else if (shouldRedirect('https', actualUrl)) {
            handleUrlToRedirect('https', actualUrl);
        }
    </script>
