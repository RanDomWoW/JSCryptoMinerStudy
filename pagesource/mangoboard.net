		if (direction == "down") {
			if ( 0 <= st && st < 682  )
				value = 682;
			else if ( 682 <= st && st < 1364 )
				value = 1364;
			else if ( 1364 <= st && st < 2046 )
				value = 2046;
			else if ( 2046 <= st && st < 2728 )
				value = 2728;
			else if ( 2728 <= st && st < 3410 )
				value = 3410;
			else if ( st >= 3410 )
				value = st + 200;
		} else if (direction == "up") {
			if ( 0 <= st && st <= 682  )
				value = 0; 
			else if ( 682 < st && st <= 1364 )
				value = 682;
			else if ( 1364 < st && st <= 2046 )
				value = 1364;
			else if ( 2046 < st && st <= 2728 )
				value = 2046;
			else if ( 2728 < st && st <= 3410 )
				value = 2728;
			else if ( st >= 3410 )
				value = st - 200;
		}
		return value;
	}

	(function($){
        $.aniPage=function(e){
        	
        	var wheelData;
    		if ( e.originalEvent.wheelDelta ) {
    			wheelData = e.originalEvent.wheelDelta;
    		} else if ( e.originalEvent.deltaY ) {
    			wheelData = e.originalEvent.deltaY;
    			if ( wheelData < 0)
    				wheelData = Math.abs(wheelData);
    			else
    				wheelData = wheelData * -1;
    		} else {
    			wheelData = e.originalEvent.detail;
    			if ( wheelData < 0)
    				wheelData = Math.abs(wheelData);
    			else
    				wheelData = wheelData * -1;
    		}
        	
        	// scroll up
            if(wheelData > 0){ 
                $("body, html").animate({
                	scrollTop:findSection("up")
                },300,function(){
               		//console.log("scrollTop: "+$(window).scrollTop()+" section: "+findSection("up"));
					$.aniOk = 0;
                });
            }else{
            	// scroll down
