			wheelData = e.originalEvent.wheelDelta;
		else if ( e.originalEvent.deltaY )
			wheelData = e.originalEvent.deltaY;
		else
			wheelData = e.originalEvent.detail;
		
		if ((wheelData && wheelData > 0) || (e.originalEvent.detail && e.originalEvent.detail < 0)) {
			e.preventDefault();
	        if($.aniOk == 0){
	            $.aniPage(e);
	            $.aniOk = 1;
	        }
		} else {
			e.preventDefault();
	        if($.aniOk == 0){
	            $.aniPage(e);
	            $.aniOk = 1;
	        }
		}
    });
