               Math.round(googleComplete) + ':' +
               Math.round(finished); */
    var cVal = Math.round(finished);
    var t = new Date();
    t.setMilliseconds(t.getMilliseconds() + 600000);

    $.cookie('adtiming', cVal, {path: '/', domain: '.southwalesargus.co.uk', expires: t});
