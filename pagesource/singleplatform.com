<script>
  var slidepause=false;
  var pinclick=false;
  var videourl='';
  var windowWidth=0;
  var activeSlide=1;
  var homeSlider;
  var backgroundpositionx=21;
  function adjustBanner (video, banner) {
    video.css({
      height: banner.css('height'),
      width: banner.css('width'),
      top: banner.css('top'),
      left: banner.css('left'),
      position: 'relative',
      'object-fit': 'inherit'
    });
  }
  function adjustCards () {
    var w=$('body').width()/2;
    var h = w*0.25;
    var p = h;
    $('.card').css('height',h+'px');
  }

  function advanceSlide(newSlide){
    var myleft=(newSlide-1)*windowWidth;  
    $('.indicator').removeClass('active');
    $('.slidestrip').stop().animate({
      'left':'-'+myleft
    },600);
    $('.indicator:nth-child('+newSlide+')').addClass('active'); 
  }
  function resetSlide() {
    $('.indicator').removeClass('active');
    $('.slidestrip').stop().animate({
      'left':'0'
    },600);
    $('.indicator:nth-child(1)').addClass('active');
  }

  function nextSlide(activeslide) {
    var newslide; 
    if(slidepause||pinclick){
      return activeslide; 
    }
    if(activeslide<5){
      newslide=activeslide+1;
      advanceSlide(newslide);
    } else {
      newslide=1;
      resetSlide();
    }
    $('img.pin').removeClass('active').mouseleave();
    switch(newslide){
      case 1:
        $('img.pinr').addClass('active').mouseenter();
        break;
      case 2:
        $('img.pino').addClass('active').mouseenter();
        break;
      case 3:
        $('img.pinh').addClass('active').mouseenter();
        break;
      case 4:
        $('img.pinp').addClass('active').mouseenter();
        break;
      case 5:
        $('img.pins').addClass('active').mouseenter();
        break;
    }

    $('#block-yui_3_17_2_36_1450423791484_5139').mouseleave();

    return newslide;
  }

  function rotatePhoneSlider() {
    /*(console.log($('.phoneslider').css("background-position"));
        //var newvalue=$('.phoneslider').css("background-position-x")-231;
        backgroundpositionx=backgroundpositionx-252;
        console.log(backgroundpositionx);
        $('.phoneslider').stop().animate({
            "background-position": backgroundpositionx+'px 72px'
        },250);
        console.log($('.phoneslider').css("background-position"));*/
    backgroundpositionx=backgroundpositionx-252;
    $('.phoneslider').animateBG(backgroundpositionx, 72, 250);
  }

  function popModal(html,clickme){
    $('body').on('click',clickme,function(){
      $('body').append('<div id="modal" class="active"><div class="modalbody"><div class="close"><span class="fa fa-times fa-lg"></span></div></div></div>');
      $('#modal .modalbody').append(html);
      $('body').on('click','#modal .close',function(){
        $('#modal').detach();
      });
      $('body').on('click','#modal',function(){
        $('#modal').detach();
      });
      return false;
    });

  }

  function doOnOrientationChange() {
    clearInterval(homeSlider);
    switch(window.orientation)
    {
      case -90:
      case 90:
        /*'landscape'*/
        break;
      default:
        /*'portrait: '*/
    }
    var oldwidth = windowWidth;
    windowWidth=$(window).width();
    if(windowWidth>527){
      windowWidth=527;
    }
    $('.slide').css('width',windowWidth+'px');
    var myleft=(activeSlide-1)*windowWidth;  
    $('.slidestrip').stop().animate({
      'left':'-'+myleft
    },200);
    homeSlider = setInterval(function(){
      activeSlide=nextSlide(activeSlide);
    },5000);
  }


  $(document).ready(function(){
    /* FF bg animation fix */
    $.fn.animateBG = function(x, y, speed) {
      var pos = this.css('background-position').split(' ');
      this.x = parseInt(pos[0]) || 0;
      this.y = parseInt(pos[1]) || 0;
      $.Animation( this, {
        x: x,
        y: y
      }, { 
        duration: speed
      }).progress(function(e) {
        this.css('background-position', e.tweens[0].now+'px '+e.tweens[1].now+'px');
      });
      return this;
    }

    /* do things when mobile orientation changes */
    window.addEventListener('orientationchange', doOnOrientationChange);

    /* blog mobile-only category select changer */
    $('#catselect').change(function(){
      var cat=$(this).find("option:selected").attr('value');
      switch(cat) {
