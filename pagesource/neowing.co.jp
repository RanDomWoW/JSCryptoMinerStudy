            $('#rr-side-data').append(d[0]);
            $('#rr-side-title').html(d[1]);
          }

          save_width2 = slwidth('.cal_list');
          rrload_stop[d[2]] = 1;
          rrsliders[d[2]] = $('#detail-recommend-item' + d[2]).bxSlider(
