<script type="text/javascript">
    function verifyUser() {
        var emailId = $('#Emailid').val();
        var verifyEmailid = $('#VerifyEmailid').val();

        if (emailId != '' && verifyEmailid != '') {
            $.ajax({
                url: '/Account/VerifyRegUserEmail',
                type: "GET",
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                data: { VerifyEmailid: verifyEmailid },
                success: function (message) {
                    //debugger;
                    if (message.message == 'false') {
                        $('#spnEmailidexist').html("This email address has already been registered. Please use a different email address");
                        $('#spnEmailidexist').show();
                    }
                    else {
                        $("#VerifyTypePopup").modal("hide");
                        $('#spnEmailidexist').hide();
                    }
                },
                error: function () {
                }

            });
        } else {
            $('#spnEmailid').show();
            $('#spnEmailid').html('Please enter valid email id');
            $('#spnVerifyEmailid').show();
            $('#spnVerifyEmailid').html('Please enter valid email id');
        }
    }

</script>

