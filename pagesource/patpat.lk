                //$('#filter-ads').html(searchvalue);
                var postdata = a;

                $.ajax({
                    type: 'post',
                    url: 'ajax-filter-ads.php',
                    data: {udata: postdata},
                    success: function (data) {
                        $('#jaximage').css("display", "none");
                        $('#filter-ads').html(data).fadeIn("slow");
                        $('#filter-ads .selectpicker').selectpicker();
                    }
                });
            }
