	if(host=="www.idsp.gov.in" || host=="idsp.gov.in" || host=="www.idsp.nic.in" || host=="idsp.nic.in"){
		window.location.href=url;
	}else{
		if(confirm("We are redirecting extranal domain")==true){
			 window.open(url,"_blank");
		}
	}
}
