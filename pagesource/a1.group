            if(regMatchMailto && regMatchMailto != 'undefined') { _gaq.push(['_trackEvent', 'Mail', regMatchMailto[1], $.trim(pageTitle) + ' (' + window.location.pathname + ')']); }
        });
        $('#mainContent .contAccordion').on('click', '.item:not(.open) .item-heading', function(){
            // Track accordion and tabs
            _gaq.push(['_trackEvent', 'Akkordeon/Tab', $.trim(pageTitle), $.trim($(this).text())]);
        });
    });
    </script>
    </body>
</html>