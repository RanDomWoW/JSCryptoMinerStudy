<script language="javascript">
	var menuvis='';
	function toggle_visibility(id) {
		var e = document.getElementById(id);
		
		// if menuvis has a value, hide that block and show mobcontent
		// otherwise show e and hide mobcontent
		if (menuvis != ''){	
			hide_visibility (menuvis);
			show_visibility ('mobcontent');
			menuvis='';
		} else {
			hide_visibility ('mobcontent');
			show_visibility(id);
		};
	}
	
	function hide_visibility(id) {
		var e = document.getElementById(id);
		e.style.display = 'none';
		menuvis='';
	}
	
	function show_visibility(id) {
		var e = document.getElementById(id);
		menuvis=id;
		e.style.display = 'block';
	}
	
	var handleMediaChange = function (mediaQueryList) {
    	if (mediaQueryList.matches) {
        	if (menuvis != ''){	
				hide_visibility (menuvis);
				show_visibility ('mobcontent');
				menuvis='';
			};
		}
	}
	var mql = window.matchMedia("(min-width: 760px)");
	mql.addListener(handleMediaChange);
	handleMediaChange(mql);

