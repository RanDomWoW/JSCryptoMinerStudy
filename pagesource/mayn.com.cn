        auto: true,//是否自动轮播 true:是 false：否
        mode: 'fade',//轮播形式  fade：淡入淡出   slide ：  滑出滑入
        controller: $('#bannerCtrl'),//轮播控制元素
        sign: "cir",  //下部翻页控件样式 cir: 圆形  strap ：长条形
    });
    $('#banner_tabs').hover(function(){
        $('.flex-prev').removeClass('hide');
        $('.flex-next').removeClass('hide');
    },function(){
        $('.flex-prev').addClass('hide');
        $('.flex-next').addClass('hide');
    });
    $('#banner_tabs .flex-prev').click(function () {
        bannerSlider.prev()//上一页
    });
    $('#banner_tabs .flex-next').click(function () {
        bannerSlider.next()//下一页
    });
</script>
