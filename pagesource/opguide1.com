<script>
//알림글을 ajax로 불러온다
function ajax_pc_msgload () {
	$.post(
		g5_url+"/plugin/srd-pushmsg/ajax.list_pushmsg.php",
		{},
		function(data) {
			$('#dd_arm_pc').empty();
			$('#dd_arm_pc').append(data);
				if ($('#msg_count').val() > 0) {
					$('#arm_cnt_pc').html();
				}
		}
	);
}

//해당글을 읽음처리이후 링크이동
function msg_link_pc (msg_link, msg_type, msg_id){
	var blank = false;
	if (msg_type == 'memo') {
		blank = true;
	}
	$.post(
		g5_url+"/plugin/srd-pushmsg/ajax.read_pushmsg.php",
	{
		'g_ids':msg_id
	},
		function(data) {
			if(blank){
				win_memo( msg_link );
			} else {
				document.location.href = msg_link;
			}
		}
	);
}

//선택글 삭제 (목록에서 삭제이기 때문에 읽음표시로 처리됨)
function msg_pc_del (msg_id){
	$.post(
		g5_url+"/plugin/srd-pushmsg/ajax.read_pushmsg.php",
	{
		'g_ids':msg_id
	},
		function(data) {
			ajax_pc_msgload();
			$("#dd_arm_pc").css("z-index:100000");
		}
	);
}

$("#ol_arm_pc").on("click",function(e){
	ajax_pc_msgload();
});

</script>
