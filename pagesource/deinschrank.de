	<script type="text/javascript">
		jQuery(document).ready(function($) {
			function preload(arrayOfImages) {
				$(arrayOfImages).each(function(){
					$('<img/>')[0].src = this;
				});
			}
			function getHoverStateBackgroundUrl(el) {
				el.addClass('hover');
				var bg = getBackgroundUrl(el)
				console.log(bg);
				el.removeClass('hover');
				return bg;
			}
			function getBackgroundUrl(el) {
				var bg = el.eq(0).css('background-image');
				try {
					bg = bg.replace(/.*\s?url\([\'\"]?/, '').replace(/[\'\"]?\).*/, '')	
				} catch(e) {
					console.log('getBackgroundUrl: '+e)
				}
				return bg;
			}
		});
	</script>
			
				

