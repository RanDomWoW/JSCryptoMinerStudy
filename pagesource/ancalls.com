<script type="text/javascript">
$( document ).ready(function() {
	//카테고리 이미지 변경
	$('.cate').mouseenter(function() {
		var a = $(this).children().eq(0)
		var n = a.find('img').attr('src')
		var cut = n.substring(0, 22);
		var add = n.match(cut) + '_on.gif';

		$(this).addClass('on')
		a.find('img').attr('src', add);
	});
	$('.cate').mouseleave(function() {
		var a = $(this).children().eq(0)
		var n = a.find('img').attr('src')
		var cut = n.substring(0, 22);
		var remove = n.replace('_on.gif', '.gif');

		$(this).removeClass('on')
		a.find('img').attr('src', remove);
	});
});

