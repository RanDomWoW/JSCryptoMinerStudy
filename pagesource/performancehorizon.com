<script type="text/javascript">
    (function ($) {
        $(document).ready(function () {
            if (CCM_EDIT_MODE) {
                return;
            }
            var this_uID = 'ph-banner_2759_8dc9ca31ecedcf05d568ff059f72fe4a';
            var this_block = $('#' + this_uID);

            if (this_block.parent().is('.ccm-custom-style-container')) {
                this_block = this_block.parent();
                this_block.css(
                        {
                            'background-size': 'cover',
                            'background-position': 'center center'
                        }
                );
            }

            /*
             * Look at a banner and see if it needs to be promoted to cover the width of the parent,
             * This involves slicing up the parent and recreating areas either side of the banner.
             */
            var promote_banner = function (this_block) {
                var bs_container = this_block.closest('.container');
                if (bs_container.length < 1) {
                    return;
                }

                var bs_row = bs_container.children('.row');
                if (bs_row.length < 1) {
                    return;
                }

                /*
                 * Clone and empty the container/row pair
                 */
                var new_bs_container = bs_container.clone();
                new_bs_container.find('.row').empty();

                /*
                 * We slice the existing into before, this and after,
                 * Then build them into a sequence of cloned containers
                 * and replace the existing container
                 * 
                 * prevAll scans backwards through the DOM, so gets blocks in reverse!
                 * Hence we need to reverse it back to the starting point.
                 */
                var prev_blocks = $(this_block.prevAll().get().reverse());
                var next_blocks = this_block.nextAll();

                var replacement_sequence = $('<div>');

                if (prev_blocks.length > 0) {
                    var before_me = new_bs_container.clone();
                    before_me.find('.row').html(prev_blocks);
                    replacement_sequence.append(before_me);
                }
                replacement_sequence.append(this_block);

                if (next_blocks.length > 0) {
                    var after_me = new_bs_container.clone();
                    after_me.find('.row').html(next_blocks);
                    replacement_sequence.append(after_me);
                }
                bs_container.replaceWith(replacement_sequence.html());
            };

            /*
             * Move related ctas (or any following blocks) in the same area 
             * into the banner as a sidebar
             */
            var make_banner_sidebar = function (this_block) {
                /*
                 * Only do this if we are in a banner area, by itself or on
                 * a tabbed page. Both have this class.
                 */
                var banner_area = this_block.closest('.area-banner');
                if (banner_area.length < 1) {
                    return;
                }

                /*
                 * Find the blocks to promote. Exclude blocks we don't want
                 * such as sliders and anything banner-sidebar-exclude.
                 */
                var exclusion = '.owl-carousel, .banner-sidebar-exclude';
                var following_blocks = this_block.nextAll().filter(function (ix, el) {
                    var this_el = $(el);
                    if (this_el.is(exclusion)) {
                        return false;
                    }
                    if (this_el.is('script,style,link')) {
                        return false;
                    }
                    if (this_el.find(exclusion).length) {
                        return false;
                    }
                    return true;
                });
                if (following_blocks.length < 1) {
                    return;
                }

                /*
                 * Change the grid within the banner block to create
                 * a sidebar area
                 */
                var this_block_content = this_block.find('.row').children('.col-xs-12').first();
                this_block_content.removeClass('col-xs-12').addClass('col-sm-10');
                this_block_content.after('<div class="banner-sidebar col-xs-12 col-sm-5"></div>');
                var banner_sidebar = this_block.find('.banner-sidebar');

                /*
                 * Move following blocks into the new grid sidebar.
                 */
                following_blocks.each(function (ix, el) {
                    var sb_block = $(this).show().wrap('<div class="banner-sidebar-block clearfix"></div>').
                            parent().hide();
                    var sb_grid = sb_block.children().first();
                    if (sb_grid.hasClass('ccm-custom-style-container')) {
                        sb_grid = sb_grid.children().first();
                    }
                    /*
                     * Strip out any grid classes
                     */
                    sb_grid.removeClass(function (index, class_name) {
                        return (class_name.match(/(^|\s)col-\S+/g) || []).join(' ');
                    });

                    sb_block.fadeIn(2000).appendTo(banner_sidebar);
                });

            }

            /*
             * Promote where necessary.
             */
            promote_banner(this_block);

            /*
             * Add a sidebar where necessary.
             */
            make_banner_sidebar(this_block);

        });
    })(jQuery);
</script>
