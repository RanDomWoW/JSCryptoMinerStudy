<div id="ph-mq-detector" style="visibility:hidden;">
    <span class="visible-xs"></span>
    <span class="visible-sm"></span>
    <span class="visible-md"></span>
    <span class="visible-lg"></span>
</div>
<script type="text/javascript">
    /*
     * Global function, enquires of the above
     */
    var pH_media_size = function () {
        if (typeof jQuery === 'undefined') {
            return 'xs';
        }
        if (jQuery('#ph-mq-detector .visible-lg').is(':visible')) {
            return 'lg';
        }
        if (jQuery('#ph-mq-detector .visible-md').is(':visible')) {
            return 'md';
        }
        if (jQuery('#ph-mq-detector .visible-sm').is(':visible')) {
            return 'sm';
        }
        return 'xs';
    }

    /*
     * Allows a context switch, duration is irrelevant.
     * 10ms.
     */
    setTimeout(function () {
        (function ($) {
            var supports_hover = function () {
                var scr_size = pH_media_size();
                if (scr_size === 'xs') {
                    return false;
                }
                try {
                    document.createEvent("TouchEvent");
                    return false;
                } catch (e) {
                   null;
                }
                return true;
            };

            setup_flippers = function () {
                var flippers = $('.flip-container').removeClass('click-flippable active');
                if (flippers.length < 1 || supports_hover()) {
                    return;
                }
                flippers.addClass('click-flippable').find('.front,.back,.flipper-mask').on('click', function (ev) {
                    ev.preventDefault();
                    var this_container = $(this).closest('.flip-container');
                    var was_active = this_container.hasClass('active');
                    flippers.removeClass('active');
                    this_container.toggleClass('active',!was_active);
                });
            };

            $(document).ready(function () {
                setup_flippers();
                $(window).on('resize', function () {
                    setup_flippers();
                });
            });

        })(jQuery);
    }, 10);
</script>
