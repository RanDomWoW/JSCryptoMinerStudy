<!-- Owl Carousel - parent link inherit from child btn -->
<script>
    $( document ).ready(function() {
        $(".item-btn").map((index,element) => {
            var classHasALink = $(element).hasClass('item-btn');
            if (classHasALink) {
                var hrefText = $(element).attr('href');
                var elementToUpdateHref = $(element).parent().parent().parent();
                elementToUpdateHref.attr('href',hrefText);
                elementToUpdateHref.click(function(){window.location.href=hrefText});
                elementToUpdateHref.css("cursor", "pointer");
            }
        });

        // Swap dots in Carousel with images
        $.each($(".image-right-slider ul li img"), function(index, element) {
            $("a.rslides1_s"+(index+1)).css('background-image', 'url('+element.src+')');
            $("a.rslides1_s"+(index+1))
        })
    });
</script>