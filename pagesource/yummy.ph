<script type="text/javascript">
    var currPage = 1, feedLink = 'article-feed';

    var loading = false;

            $(window).scroll(function() {
            var currentPosition = $(window).scrollTop() + $(window).height();
            var targetElement = $(".feed-home");
            var lastDIvTargetPos = targetElement.last().position().top + targetElement.last().outerHeight();
            if(currentPosition >= lastDIvTargetPos && loading === false) {
               $("[data-loadmore]").trigger('click');
           }
        });
    
    $(document).on('click','[data-loadmore]',function(e){
        e.preventDefault();

        var endDate = $('.lastdate:last').data('value');
        var prevId = $('.prev_id:last').data('value');
        var btn = $(this);

        btn.hide();

        var requestUrl = {
            url: "https://www.yummy.ph/api/"+feedLink+"/"+currPage+"/"+CHANNEL+"/"+SLUG,
            type: 'GET',
            beforeSend: function( xhr ) { loading = true; }
        };

        if(endDate){
            requestUrl.data = {
                end : endDate,
                prev_id : prevId
            }
        }

        $.ajax(requestUrl)
            .done(function(response){
                console.log(response);
                $('.gtm-feedView'+currPage).addClass('gtm-feedView'+(currPage+1)).removeClass('gtm-feedView'+currPage);
                currPage = response.currentPage;
                console.log('Load more page: '+currPage);

                btn.before(response.html);
                $('img.lazy').lazyload({threshold:300, effect:'fadeIn'}).removeClass('lazy');

                $.BrandedContent({
                    siteName            : "yummy.ph",
                    flashboxAnimated    : false
                });

                                    if (response.end ){

                        $('.gtm-feedView'+currPage).removeClass("gtm-feedView"+currPage).addClass("gtm-feedArchive");

                        btn.removeAttr('data-loadmore').removeClass('none').click(function(){
                            return true;
                        });
                    };
                
                loading = false;

                return false;
            });
    });

  </script>


<script>
$(document).ready(function(){
    //----Subscribe widget start

    $('.subscribe-form').on('submit',function(event){

      event.preventDefault();

      var thisForm = $( this );

    if (thisForm.data('submitted') === true) {
    // Previously submitted - don't submit again
    } else {
    // Mark it so that the next submit can be ignored
    thisForm.data('submitted', true);

        var errorMsgClass = 'newsletter-error';
        
        var errorMsg = $("."+errorMsgClass);

        var newsletterUrl = "https://tracker.summitmedia-digital.com/v1/newsletter/authenticate";

        var inputs = {
            'email' : thisForm.find('input[name ="email"]').val(),
            'siteId' : 5
        };

        var errorMsgs = {
            "saved" : "Thank you for subscribing.",
            "exist" : "Sorry, your email address is already subscribed to this list.",
            "error" : "Are you sure you typed the correct email address? Please try again."
        };

          $.ajax({
              type        : 'POST', // define the type of HTTP verb we want to use (POST for our form)
              url         : newsletterUrl, // the url where we want to POST
              data        : $.param(inputs), // our data object
              dataType    : 'json', // what type of data do we expect back from the server

              }).done(function(data) {
                  console.log(data);

                  $('.newsletter-error').remove();
                  
                  if(data.message == 'saved'){
                    thisForm.closest('.subscription').html(errorMsgs.saved);
                  }else if(data.message == 'exist'){
                    $("<div class='"+errorMsgClass+"'>"+errorMsgs.exist+"</div>").insertBefore( thisForm );
                  }else if(data.message == 'error'){
                    $("<div class='"+errorMsgClass+"'>"+errorMsgs.error+"</div>").insertBefore( thisForm );
                  }else{
                    $("<div class='"+errorMsgClass+"'>"+data.message+"</div>").insertBefore( thisForm );
                  }

                  thisForm.data('submitted', false);

              });
    }

    });
    //----Subscribe widget end
});
</script>



