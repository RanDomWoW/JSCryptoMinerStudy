    function homeTabs(tab) {

   if (tab != "COPERTINA") {
   	$('#results-tab').load("homeTabs.php #" + tab);
   	$('#results-tab').show();
	$('#newspicker a').removeClass("selected");
	$('#'+tab+'-link').addClass("selected");
	$('#copertina-tab').hide();
	}
	else {
		$('#results-tab').hide();
		$('#copertina-tab').show();
		$('#newspicker a').removeClass("selected");
		$('#copertina-link').addClass("selected");
	}
}

