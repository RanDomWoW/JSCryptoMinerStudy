<script>
	/**
	 * Init dependencies.
	 */

	var debug = require('debug')('initialize');
	var dtm = require('dtm');


	/**
	 * Select all components.
	 */

	var components = document.querySelectorAll('[data-require]');


	for (var i = 0, l = components.length; i < l; i++) {
		var component = components[i];
		var name = component.getAttribute('data-require');
		debug(name);
		var compOptions = options(name, component);
		var fn = require(name);
		if (typeof fn === 'function' ) fn(component, compOptions);
	}


	/**
	 * Get component options.
	 *
	 * @note should query selector all
	 * and merge options.
	 *
	 * @return {Object}
	 * @api private
	 */

	function options(name, component) {

		var optionsJSON;
		var json = component.querySelector('[data-require="' + name + '"] > .' + name + '-options');
		if (json) {
			optionsJSON = JSON.parse(json.innerHTML);
			//set the dtm values of the component to the data layer.
			dtm.set(optionsJSON.dtm);
		}
		return json && optionsJSON;
	}


	require('svgshim')();


</script>
