                        setTimeout(function() {
                                $.magnificPopup.close();
                            },
                            (startdelay + timeout));

                        var expireDate = new Date();
                        expireDate.setDate(expireDate.getDate() + 30);

                        document.cookie = cookieIdentification +
                            "=1;expires=" +
                            expireDate.toUTCString() +
                            ";path=/";
                    }
                }
            };

            function getCookie(name) {
                var dc = document.cookie;
                var prefix = name + "=";
                var begin = dc.indexOf("; " + prefix);
                if (begin == -1) {
                    begin = dc.indexOf(prefix);
                    if (begin != 0) return null;
                }
                else
                {
                    begin += 2;
                    var end = document.cookie.indexOf(";", begin);
                    if (end == -1) {
                        end = dc.length;
                    }
                }
                // because unescape has been deprecated, replaced with decodeURI
                //return unescape(dc.substring(begin + prefix.length, end));
                return decodeURI(dc.substring(begin + prefix.length, end));
            };

            // Maximum screen size to show mobile mode
            var mql = window.matchMedia("(min-width: 53.125em)"); 

            // Function to handle the browser mode changes from desktop to mobile and vice versa
            var handleMediaChange = function (mediaQueryList) { 
                if (mediaQueryList.matches) {
                    // The browser window is at least 53.125em wide.
                    // Check cookie to prevent showing the overlay twice when browser mode changes.
                    if(getCookie(cookieIdentification) == null){ 
                        handleOverlay();
                    }
                }
                else {
                    // The browser window is less than 53.125em wide.
                    if(getCookie(cookieIdentification) == null){
                        handleOverlay();
                    }
                }
            }

            mql.addListener(handleMediaChange);
            handleMediaChange(mql);

        })(window.jQuery);

    </script>
 

    
