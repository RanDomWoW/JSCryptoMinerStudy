        (function($) {
            

            var cookieIdentification = "NewRecruitCampaign" + 'e5a627db-1a3a-4efe-b994-a67835f27ff6';

            var handleOverlay = function() {

                var timeout = 30 * 1000,
                    startdelay = 1000,
                    $overlay = $('#recruit-overlay');

                if ($overlay.length) {
                    $overlay.removeClass('mfp-hide');

                    if ($overlay.is(":visible")) {
