                        soloida = true;
                    }    

                    if(soloida===false){
                        $('#sHora fieldset.vuelta').show();
                    }else{
                        $('#sHora fieldset.vuelta').hide();
                        $('#sHora fieldset.vuelta').hide();
                    }
                    
                    // textos label origen->destino
                    var origen='';
                    var destino='';
                    if($('#rtipoTrenTR0').is(':checked')){ // tren
