            }
        }
        
        if (document.getElementById('rtipoTrenTR1').checked==true){
            if(document.getElementById('bOrigenTRAve').value!="0" && document.getElementById('bOrigenTRAve').value!=""){
                if (document.getElementById('bDestinoTRAve').value=="0" || document.getElementById('bDestinoTRAve').value==""){
                    apprise("Seleccione un destino", null, function() {document.getElementById('bDestinoTRAve').focus();});
                    return false;
                }
            }
            else{
                apprise("Seleccione un origen", null, function() {document.getElementById('bOrigenTRAve').focus();});
                return false;
            }
        }
        
        if (document.getElementById('rtipoTrenTR2').checked==true){
            if(document.getElementById('bOrigenTRInt').value!="0" && document.getElementById('bOrigenTRInt').value!=""){
                if (document.getElementById('bDestinoTRInt').value=="0" || document.getElementById('bDestinoTRInt').value==""){
                    apprise("Seleccione un destino", null, function() {document.getElementById('bDestinoTRInt').focus();});
                    return false;
                }
            }
            else{
                apprise("Seleccione un origen", null, function() {document.getElementById('bOrigenTRInt').focus();});
                return false;
            }
        }
        
        if (validarFecha(document.getElementById('datepickerI').value)==false){
            apprise("La fecha de ida no es correcta", null, function() {document.getElementById('datepickerI').focus();});
            return false;
        }                
        
        if (document.getElementById('rvueltaTR').checked==true){            
            if (validarFecha(document.getElementById('datepickerV').value)===false){
                javascript:IVida();
                document.getElementById('ridaTR').checked=true;
            }
            else {
                if (compare_dates(document.getElementById('datepickerI').value, document.getElementById('datepickerV').value)){
                    apprise("La fecha de vuelta no puede ser inferior a la de ida", null, function() {document.getElementById('datepickerV').focus();});
                    return false;
                }
