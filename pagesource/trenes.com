        }
        
        if (parseInt(document.getElementById('bAdultosTR').value)<1 || parseInt(document.getElementById('bAdultosTR').value)>9){
            apprise("El número de adultos ha de ser entre 1 y 9", null, function() {document.getElementById('bAdultosTR').select(); document.getElementById('bAdultosTR').focus();});
            return false;
        }
        
        if ((parseInt(document.getElementById('n1').value)+parseInt(document.getElementById('n2').value)+parseInt(document.getElementById('n3').value)+parseInt(document.getElementById('bAdultosTR').value))>9){
            apprise("Como máximo solo se puede comprar 9 billetes con plaza por transacción", null, function() {});
            return false;
        }
        
        if ((parseInt(document.getElementById('n1').value)+parseInt(document.getElementById('n2').value)+parseInt(document.getElementById('n3').value)+parseInt(document.getElementById('bAdultosTR').value))<parseInt(document.getElementById('n0').value)){
            apprise("El número máximo de niños menores de 4 años que no ocupen plaza debe de ser igual o inferior al número de billetes reservados con plaza", null, function() {$.fancybox("#verDetallesNinios");});
            return false;
        }
        
        if ((parseInt(document.getElementById('bDoradaTR').value)+parseInt(document.getElementById('bJovenTR').value))>parseInt(document.getElementById('bAdultosTR').value)){
            apprise("El número máximo de adultos con Tarjeta Dorada y Carnet Joven < 26 debe de ser igual o inferior al número de adultos", null, function() {document.getElementById('bAdultosTR').focus();});
            return false;
        }
        
        // selector hora
        var showHora = false;
        if($('#rtipoTrenTR0').is(':checked')){ // tren
            if($('input[name=ot]').data('shora') && $('input[name=dt]').data('shora')){
                showHora = true;
            }
        }else if($('#rtipoTrenTR1').is(':checked')){ // AVE
            if($('input[name=oa]').data('shora') && $('input[name=da]').data('shora')){
                showHora = true;
            }
        }else if($('#rtipoTrenTR2').is(':checked')){ // INT
            if($('input[name=oi]').data('shora') && $('input[name=di]').data('shora')){
                showHora = true;
            }
        }
        
        if(showHora){
            $.fancybox("#sHora",{
                fitToView   : false,
                autoSize    : true,
                closeClick  : false,
                beforeLoad: function() {
                    
                    // ¿ida y vuelta?
