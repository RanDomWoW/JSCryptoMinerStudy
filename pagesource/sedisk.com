                            //앨범타이틀 2개 불러오기
                            for(var i = 0; i < 2; i ++){

                                music_title  = decodeURIComponent(response[i].title);
                                music_artist = decodeURIComponent(response[i].subtitle);

                                musicListHtml += "<li>";
                                musicListHtml += "<a href=\""+response[i]['click_url']+"\">";
                                musicListHtml += "<span class=\"thumb\"><img src=\""+response[i]['img_url']+"\" ></span>";
                                musicListHtml += "<span class=\"box_s\">";
                                musicListHtml += "<span class=\"t\">"+music_title+"</span>";
                                musicListHtml += "<span class=\"s\">"+music_artist+"</span>";
                                musicListHtml += "</span>";
                                musicListHtml += "</a>";
                                musicListHtml += "</li>";

                            }

                            //텍스트 타이틀 5개 불러오기
                            for(var k = 2; k < 7; k++){

                                var liClass = "num1";
                                if(k > 4){
                                    liClass = "num";
                                }

                                music_title  = decodeURIComponent(response[k].title);
                                music_artist = decodeURIComponent(response[k].subtitle);

                                var alover = "";
                                if(music_artist.length > 4){
                                    alover = ".."
                                }
                                music_artist = music_artist.substring(0,4);


                                musicTextHtml += "<ul onclick=\"window.location.href='"+response[k]['click_url']+"'\">";
                                musicTextHtml += "<li class=\""+liClass+"\">"+(k - 1)+"</li>";
                                musicTextHtml += "<li class=\"subject\" style=\"width:160px;\"><span><b>"+music_title+"</b></span></li>";
                                musicTextHtml += "<li class=\"cash\" style=\"width:70px;\">"+music_artist+alover+"</li>";
                                musicTextHtml += "<li class=\"cate\"><img src=\"/views/images/sedisk/001/music_download.gif\" alt=\"\" style=\"cursor:pointer;\"></li>";
                                musicTextHtml += "</ul>";

                            }

                            $(".music_list").html(musicListHtml).promise().done(function() {

                                delete musicListHtml;

                            });

                            $(".music_txt_list").html(musicTextHtml).promise().done(function() {

                                delete response;
                                delete musicTextHtml;

                            });


                        }
                    });

                }

                musicListLoad();


