	var layer_url = "/eventLayer?act=" + eventCode+'&sact='+cate;
	
	jQuery.ajax({
		type:"post"
		,url:layer_url
		,dataType:"html"
		,async:false
		,success:function(response) {

			jQuery("#"+obj).html(response).promise().done(function() {
				delete response;
			});
