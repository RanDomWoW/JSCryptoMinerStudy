function get_phone_code_side_bar(country)
{	
	if(country == "")
	{
		return;
	}

	jQueryNew.ajax({
	type: "POST",
	url: "/ajax.php?action=common.get_country_code&return=json",
	data: "country="+country,
	dataType: "json",
	beforeSend: function() {
        jQueryNew("#loading_code").show();
    },
	complete: function() {
		jQueryNew("#loading_code").hide();
    },
	success: function(msg)
	{
		if (!msg.description[0].msg)
			msg.description[0].msg = "1";
	
		jQueryNew("#sidebar_inquiry_form #phone1").val(+msg.description[0].msg);
	}
	});
}

function isNumberKey(evt){
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if(charCode == 116){
        return true;
    }
    return !(charCode > 31 && (charCode < 48 || charCode > 57));
}
	 	
