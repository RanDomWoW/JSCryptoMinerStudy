<script language="JavaScript">
	$j(function()
	{
		var caches = { product: {}, selloffer: {}, profile: {} },
		lastXhr;
		$j( "#searchbar_keyword" ).autocomplete(
		{
		   	minLength: 2,
			search: function(event, ui)
			{
				if ($j('#searchbar_action').val() != "product_search" && $j('#searchbar_action').val() != "selloffer_search" && $j('#searchbar_action').val() != "profile_search")
				{
					return false;
				}
			},

			source: function( request, response )
			{
				var term = request.term;

				if ($j('#searchbar_action').val() == "product_search")
				{
					cache = caches.product;
				}
				else if($j('#searchbar_action').val() == "selloffer_search")
				{
					cache = caches.selloffer;
				}
				else
				{
					cache = caches.profile;
				}
				if ( term in cache )
				{
					response( cache[ term ] );
					return;
				}
				lastXhr = $j.getJSON( "http://www.tradekey.com/ajax.php?action=autocomplete.getresult"+"&type="+$j('#searchbar_action').val(), request, function( data, status, xhr )
				{
					if ($j('#searchbar_action').val() == "product_search")
						caches.product[ term ] = data;
					else if($j('#searchbar_action').val() == "selloffer_search")
						caches.selloffer[ term ] = data;
					else
						caches.profile[ term ] = data;
					
					//Bold all searched text
					//jQuery.each(data, function( index, value ) {
						//data[index] = value.replace(term, "<strong>"+term+"</strong>");
					//});
					response( data );
					
				});
			},
			select: function(event, ui)
			{
				if(ui.item)
				{
					tk_track_ga_event('Search Header','Suggest auto complete','Suggest auto complete');
					$j("#searchbar_keyword").val(ui.item.value);
					$j("#search").submit();
				}
			}
		});

		$j("#searchbar_keyword").keyup(function ()
		{
			$j('#search_panel_extended').css("display","none");
		});

		jQuery.ui.autocomplete.prototype._renderItem = function (ul, item) {
            item.label = item.label.replace(new RegExp("(?![^&;]+;)(?!<[^<>]*)(" + jQuery.ui.autocomplete.escapeRegex(this.term) + ")(?![^<>]*>)(?![^&;]+;)", "gi"), "<strong>$1</strong>");
            return jQuery("<li></li>").data("item.autocomplete", item).append("<a>" + item.label + "</a>").appendTo(ul);
        };
	});
	
</script>
