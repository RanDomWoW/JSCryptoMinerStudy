                            <link rel="stylesheet" type="text/css" media="all" href="/_assets/css/callme.css" />
<script>
    (function($)
    {
        $(document).ready(function()
        {
            $('.callmeback')
                .each(function(i, el)
                {
                    $(this).attr('data-id', 'popup-' + i);
                })
                .bind('click', function(ev)
                {
                    ev.preventDefault();
                    var self        = $(this);
                    var id            = self.attr('data-id');
                    var pop_el        = $('#' + id);

                    $('.callmeback-popup:not(#'+id+')').fadeOut();

                    if(pop_el.length)
                    {
                        if(pop_el.is(':visible'))
                        {
                            pop_el.fadeOut();
                        }
                        else
                        {
                            pop_el.fadeIn();
                            var top            = $(window).scrollTop();
                            var y            = pop_el.position().top;
                            if(y < top)
                            {
                                $('html, body').animate({scrollTop : y - 20}, 400);
                            }
                        }
                        return;
                    }
                    var href        = self.attr('href');
                    var query        = href.split('?');
                    var url            = '/call-me-back/';
                    if(query.length == 2)
                    {
                        url           += "?" + query[1];
                    }
                    var html        = '<div id="'+id+'" class="callmeback-popup"><iframe src="'+url+'"></iframe><a href="#"></a></div>';
                    var popup        = $(html);
                    $('body').append(popup);
                    popup.bind('reposition', function()
                    {
                        popup.removeClass('callmeback-popup--notip');
                        var position    = self.offset();
                        var x            = position.left - ((popup.width() - self.width()) / 2),
                            y            = position.top - (popup.height()) - 20;
                        if(y < 10)
                        {
                            y            = 10;
                            popup.addClass('callmeback-popup--notip');
                        }
                        if(x < 10)
                        {
                            x            = 10;
                            popup.addClass('callmeback-popup--notip');
                        }
                        popup.css(
                            {
                                'top'        : y,
                                'left'        : x
                            }
                        )
                    });
                    popup.trigger('reposition');
                    $(window).on('resize', function()
                    {
                        popup.trigger('reposition');
                    });
                    popup.hide().fadeIn();
                    var top            = $(window).scrollTop();
                    var y            = popup.position().top;
                    if(y < top)
                    {
                        $('html, body').animate({scrollTop : y - 20}, 400);
                    }
                    popup.find('a').bind('click', function(ev)
                    {
                        ev.preventDefault();
                        popup.fadeOut();
                    });
                });
        });
    })(jQuery);
</script>
 

                                

