            var navAs = [];
            var navLis = [];
            var lis = [];
            return {
                Initialise: function () {
                    navAs = document.getElementById("headerutilnav").getElementsByTagName("A");
                    navLis = document.getElementById("headerutilnav").getElementsByTagName("LI");
                    for (var i = 0; i < navLis.length; i++) {
                        if (navLis[i].className.indexOf("headerutil") >= 0) {
                            lis[navLis[i].className.split('headerutil-')[1].match(/[0-9]+/)] = ({
                                delayed: false,
                                cancel: true,
                                el: navLis[i],
                                id: navLis[i].className.split('headerutil-')[1].match(/[0-9]+/),
                                addHover: function () {
                                    this.delayed = false;
                                    if (!this.cancel) {
                                        this.el.className = this.el.className.replace(hoverAClass, "");
                                        this.el.className += " navhover";
                                    }
                                }
                            });
                            $(navLis[i]).hover(
                                function () {
                                    var id = this.className.split('headerutil-')[1].match(/[0-9]+/) + '';
                                    lis[id].delayed = true;
                                    lis[id].cancel = false;
                                    var delayed = function () {
                                        lis[id].addHover();
                                    }
                                    setTimeout(delayed, hoverDelay);

                                },
                                function () {
                                    this.className = this.className.replace(hoverAClass, "");
                                    lis[this.className.split('headerutil-')[1].match(/[0-9]+/) + ''].cancel = true;
                                }
                            );
                        }
                    }
                    for (var i = 0; i < navAs.length; i++) {
                        if (navAs[i].className.indexOf("headerutillink") >= 0) {
                            navAs[i].onfocus = function () {
                                this.className = this.className.replace(focusAClass, "");
                                this.className += " focused";
                                CravenNavigation.CheckFocus();
                            }
                            navAs[i].onblur = function () {
                                this.className = this.className.replace(focusAClass, "");
                                setTimeout(CravenNavigation.CheckBlur, 750);
                            }
                        }
                    }
                },
                CheckFocus: function () {
                    var focusedID = CravenNavigation.GetFocusedID();
                    for (var i in lis) {
                        if ((lis[i].id * 1) == (focusedID * 1)) {
                            lis[i].el.className = lis[i].el.className.replace(focusClass, "");
                            lis[i].el.className += " navfocus";
                        } else {
                            lis[i].el.className = lis[i].el.className.replace(focusClass, "");
                        }
                    }
                },
                CheckBlur: function () {
                    var focusedID = CravenNavigation.GetFocusedID();
                    for (var i in lis) {
                        if ((lis[i].id * 1) != (focusedID * 1)) {
                            lis[i].el.className = lis[i].el.className.replace(focusClass, "");
                        }
                    }
                },
                GetFocusedID: function () {
                    var id = "";
                    for (var i = 0; i < navAs.length; i++) {
                        if (navAs[i].className.indexOf("focused") >= 0) {
                            id = navAs[i].className.split('headerutillink-')[1].match(/[0-9]+/);
                            break;
                        }
                    }
                    return id;
                }
            }
        }();

