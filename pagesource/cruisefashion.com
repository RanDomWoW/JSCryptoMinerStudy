<script>
	$(document).ready(function(){
		$('#bx-pager a').click(function(e){
			e.preventDefault();
		});			
		var imageRoot = '',
			pageLoadDone = false;
		var deferredSliderImages = $('.bxslider-wrap img[data-src]'),
			deferredSliderImageCount = deferredSliderImages.length,
			deferredSliderImagesLoadedCount = 0,
			sliderStarted = false;
		
		deferredSliderImages.load(function(){
			deferredSliderImagesLoadedCount++;
			checkSlider();
		});	
		$(window).load(function(){
			pageLoadDone = true;
			checkSlider();
		});	
		deferredSliderImages.each(function(){
			$this = $(this);
			$this.attr('src', imageRoot + $this.data('src'));
		});	
		setTimeout(startSlider, 10000);	
		function checkSlider() {
			if(!pageLoadDone)
				return;
			if(deferredSliderImageCount > deferredSliderImagesLoadedCount)
				return;
			startSlider();
		}		
		function startSlider(){
			try{
				if(sliderStarted)
					return;				
				sliderStarted = true;			
				deferredSliderImages.css('display', 'block');
				$('.deferredTxt').css('display', 'block');
				var slider = $('.bxslider').bxSlider({
				    mode: 'fade',
					auto: true,
				    infiniteLoop: true,
				    speed: 0,
				    useCSS: false,
				    pager: false,	
				    touchEnabled: true,
				    swipeThreshold: 100,
				    oneToOneTouch: false,
				    hideControlOnEnd: false,
				    adaptiveHeight: true
				});
			}
			catch(e)
			{
				console.log(e);
			}			
		}
	});
</script>
