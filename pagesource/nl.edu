	function getBlogPosts()
	{

		
		var maxrecordstodisplay = 8;
		var myhtml = '';
    
    
    
        myhtml = '<li style="padding-top: 8px"><div class="titledisplay"><a target="_blank" href="https://www.nl.edu/media/pressreleases/1_16_18%20announcement%20press%20release.pdf">National Louis University and Laureate Education, Inc. Announce Transfer Agreement of Kendall College&apos;s Programs and Other Assets</a></div><hr class="fadinghr" /></li>';


		$.ajax({
			url: "/blogfeed/getblogposts/",
			async: true,
			contentType: "application/x-www-form-urlencoded",
			datatype: "json",
			type: "POST",
			data: {

			},
			success: function (response) {

				var jsonobject = JSON.parse(response);


				if(jsonobject.ROWCOUNT > 0)
				{

					for(i = 0; i < jsonobject.ROWCOUNT && i < maxrecordstodisplay; i++)
					{

							myhtml += '<li style="padding-top: 8px"><div class="titledisplay"><a href="' + jsonobject.DATA.RSSLINK[i] + '">' + jsonobject.DATA.TITLE[i] + '</a></div><hr class="fadinghr" /></li>';

					}

				}
				else
				{
					myhtml += '<li style="color:white;">Currently unavailable</li>';
				}

				if(myhtml == '')
				{

					myhtml = '<li style="color:white;">No blog posts are currently unavailable</li>';
				}

			},
			error: function (response) {
				myhtml += '<li style="color:white;">Currently unavailable</li>';
			},
			complete: function() {

				myhtml = '<ul>' + myhtml + '</ul>';

				$('#blogposts').html(myhtml);
			}
		}); // Dean note: end ajax call


	}



