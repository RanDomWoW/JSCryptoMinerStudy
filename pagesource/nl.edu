  // animate numbers in "NLU at a glance" section

  function animateNumber(startN,endN,delayN,location) {
 
    var locH2 = location + ' h2';
    var locH3 = location + ' h3';
  
    $({myValue: startN}).delay(delayN).animate({myValue: endN}, {
	    duration: 1500,
	    step: function() { 
		  // Update the element's text with rounded-up value:
		  $(locH2).text(Math.round(this.myValue));
	    },
	    complete: function() {
		  $(locH2).fadeOut(50).fadeIn(50);
		  $(locH3).fadeIn(500);
	    }
    });
 
  } // end animateNumber func

