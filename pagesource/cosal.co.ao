    <script>
        function Slider(container, interval) {
            this.container = container;
            this.interval  = interval;

            this.cycling = false;
            this.cyclingInterval;

            this.advance = function() {
                this.container.backstretch('next');
            };

            this.cycle = function() {
                this.cycling = true;

                this.cyclingInterval = window.setInterval(function() {
                    this.advance();
                }.bind(this), this.interval);
            }.bind(this);

            this.stop = function() {
                window.clearInterval(this.cyclingInterval);
                this.cycling = false;
            };

            container.on('mouseenter', function() {
                if (!this.cycling) this.advance(); this.cycle();
            }.bind(this));

            container.on('mouseleave', function() {
                if (this.cycling) this.stop();
            }.bind(this));
        }
    </script>
    
