			function initTwitch() {
				var options = {
					width: 939,
					height: 480,
					channel: "piranhagames",
					playsinline: false,
					autoplay: false,
				};

				var player = new Twitch.Player("twitch-player", options);

				player.setVolume(0.5);

				player.addEventListener(Twitch.Player.OFFLINE, function() { console.log('stream offline'); $('.twitch-stream').hide(); $('#carouselSeparatorBar').show(); });
				player.addEventListener(Twitch.Player.ENDED, function() { console.log('stream ended'); $('.twitch-stream').hide(); $('#carouselSeparatorBar').show(); });
				player.addEventListener(Twitch.Player.ONLINE, function() {
					console.log('stream online');
					$('#twitch-stream-bar').show();
					$('#carouselSeparatorBar').hide();
				});

				$('#twitch-stream-bar').click(function() {
					$('#twitch-player').show();
					$('#twitch-stream-bar').hide();
					$('#carouselSeparatorBar').show();
					player.play();
				});
			}

			jQuery(document).ready(function() {
				if (!document.cookie.replace(/(?:(?:^|.*;\s*)hidePopup2\s*\=\s*([^;]*).*$)|^.*$/, "$1")) {
					$('.marketing-popup').animate( { left: "+=315" }, 400);
				}

				$('.close-button').click(function() {
					var date = new Date();
					date.setTime(date.getTime()+(24*1*60*60*1000));
					document.cookie = "hidePopup2=true; expires="+date.toGMTString();
					$('.marketing-popup').animate( { left: "-=300" }, 400, function() {
						$('.marketing-popup').hide();
					});
				});

				MWO.SITE.initHome();
				initTwitch();

			});

