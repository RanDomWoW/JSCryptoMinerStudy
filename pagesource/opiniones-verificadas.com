	<!-- Animations -->
	<script>
	$(document).ready(function() {
		// Animations nuages logos
		function nuageLogos(timer) {
			var nuage = $('.nuage:not(.show)').first();
			if(nuage.length == 0){ clearInterval(timer); }
			$(nuage).show().addClass('show');
		}
		$('.bloc_nuage').appear();
		var appearNuages = false;
		$(document.body).on('appear', '.bloc_nuage', function(e, $affected) {
			if(appearNuages == false) {
				appearNuages = true;
				var timer;
				timer = setInterval(function(){ nuageLogos(timer); }, 100);
			}
		});
		// Animations Schéma Bénéfices
		function AffichageBenefices(timerBenefices) {
			var nuage = $('.grille_benefices li:not(.show)').first();
			if(nuage.length == 0){ clearInterval(timerBenefices); }
			$(nuage).show().addClass('show');
		}
                
		$('.grille_benefices').appear();
                
                if ($(window).height() > 700){
                    var timerBenefices;
                    timerBenefices = setInterval(function(){ AffichageBenefices(timerBenefices); }, 300);
                }else{
                    var appearBenefices = false;
                    $(document.body).on('appear', '.grille_benefices', function(e, $affected) {
                            if(appearBenefices == false) {
                                    appearBenefices = true;
                                    var timerBenefices;
                                    timerBenefices = setInterval(function(){ AffichageBenefices(timerBenefices); }, 300);
                            }
                    });
                }
	});
    </script>
