<script type="text/javascript">
jQuery(document).ready(function($){

	Dropdown.initialise();
	Dropdown.applyTo('menu-main-menu');
	$('#menu-main-menu > li').each(function(){
		if ($(this).children('ul').length > 0)
			$(this).addClass('hasDropdown');
	});

	function setThemeID(themeId)
	{

        $('#page').attr('data-theme-id', themeId);
		$('#theme-id-iframe').attr('src', 'http://www.keyfood.com/ajax/theme-id/' + themeId);
		$.cookie('key-food-theme-id', themeId, { path: '/' });
		$("a[href*='http://keyfood.m.mywebgrocer.com']").each(function(){
			var url = $(this).attr('href').split('#')[0];
			$(this).attr('href', url + '#' + themeId);
		});
		$("a[href*='http://keyfood.mywebgrocer.com']").each(function(){
			var url = $(this).attr('href').split('#')[0];
			$(this).attr('href', url + '#' + themeId);
		});
		$("a[href*='https://secure3.mywebgrocer.com']").each(function(){
			var url = $(this).attr('href').split('#')[0];
			$(this).attr('href', url + '#' + themeId);
		});
		if ($('#home-sign-up-form').length>0) {
			var action = $('#home-sign-up-form').attr('action').split('#')[0];
			$('#home-sign-up-form').attr('action', action + '#' + themeId);
		}


		// Hack to force IE8 to update
		$('#logo, #header-bar').css('background', 'none').attr('style', '');
	}

    $('.banner-theme-icon').click(function(){
        var themeId = $(this).attr('data-theme-id');
		setThemeID(themeId);
    });	
	var domain = document.domain;
	var referrer = document.referrer;
    if (domain.match(/fooddynasty/i) || referrer.match(/fooddynasty/i)) {
		themeId = 'food-dynasty';
    } else if (domain.match(/thefoodemporium.com/i)  || domain.match(/thefoodemporiumtradingcompany.net/i)  || domain.match(/thefoodemporiumtradingcompany.com/i) || domain.match(/thefoodemporiumshoponline.com/i) || domain.match(/foodemporium.org/i) || referrer.match(/thefoodemporium.com/i)  || referrer.match(/thefoodemporiumtradingcompany.net/i)  || referrer.match(/thefoodemporiumtradingcompany.com/i) || referrer.match(/thefoodemporiumshoponline.com/i) || referrer.match(/foodemporium.org/i) ) { 
		themeId = 'food-emporium';
    } else if (domain.match(/fooduniverse/i) || referrer.match(/fooduniverse/i) || document.location.href.match('fooduniverse')) {
		themeId = 'food-universe';
    } else if (domain.match(/superfreshfood/i) || referrer.match(/superfreshfood/i) || document.location.href.match('superfresh')) { 
		themeId = 'super-fresh';
    } else {
		themeId = $.cookie('key-food-theme-id') ? $.cookie('key-food-theme-id') : 'key-food';
	}

	setThemeID(themeId);
});
</script>
