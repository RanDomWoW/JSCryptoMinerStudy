        e.preventDefault();
        $(".loader_ajax").show();
        $.ajax({
            url : "//www.tatatiago.com/",
            type: "POST",
            data : $("#stickyform").serialize(),
            success: function(data, textStatus, jqXHR)
            {
                $(".loader_ajax").hide();
                var fetchResponse = $.parseJSON(data);
                if(fetchResponse.status == "failure")
                {
                    $.each(fetchResponse.error, function (i, v)
                    {
                        $('.'+i+'_error').html(v);
                        if(i == 'captcha_code' && v == "Robot verification failed, please try again.")
                        {   
                            grecaptcha.reset();
                        }
                    });
                }
                else
                {
                    $.each(fetchResponse.error, function (i, v)
                    {
                        $('.'+i+'_error').html(v);
                    });
                    $("#leadvisitForm").toggleClass("dis-none"); 
                    $("#stickyform")[0].reset();
                    $(".error").html("");
                    window.location.href = "//www.tatatiago.com/thankyou";
                }
            },
            error: function (jqXHR, textStatus, errorThrown)
            {
                $(".loader_ajax").hide();
            }
        });
