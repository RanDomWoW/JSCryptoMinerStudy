<script>
//  safe ES6 endsWith usage
if (typeof String.prototype.endsWith != 'function') {
    String.prototype.endsWith = function (str) {
        return this.slice(-str.length) == str;
    };
}

jQuery(document).ready(function($) {
    // find elq form and mark with custom class name
    $('form').each(function() {
        if ($(this).find('input[name=elqFormName]').length) {
            $(this).addClass('elqActivated');
        }
    });

    var bfUrl = 'https://secure.p03.eloqua.com/e/f2.aspx?';

    $(".elqActivated .submit").on("click", function(event) {
      event.preventDefault();
      var continueSubmit = true;
     
        // avoid sending data if mandatory fields are empty
        // check text inputs
        $('.elqActivated label.field').each(function() {
            var labeltxt = $(this).text();                
            if ( labeltxt.endsWith("*") ) {
              $(this).addClass('req-label');
              // check for text-inputs
              var txtinput = $(this).next('input[type=text]');
              if (txtinput.length) {
                $(this).addClass('req-text');
                if ( txtinput.val() == '' || txtinput.val() == txtinput.data('default') ) { 
                  continueSubmit = false;
                }
              }                    
            }
        });

        // check other elements
        $('.req-label:not(".req-text")').each(function(){
          // textarea
          if ( $(this).siblings('textarea').length && $(this).siblings('textarea').val() == "" ) {
            continueSubmit = false;
          }
          // select
          if ( $(this).next('.input-block').children('select:not([multiple])').length && $(this).next('.input-block').children('select:not([multiple])').val() == "" ) { 
            continueSubmit = false;
          }
          // radio + checkbox groups
          if ( $(this).next('.input-block').children('.answer').length) {  
            var type = $(this).next('.input-block').children('.answer:first').find('input').attr('type');
            var name = $(this).next('.input-block').children('.answer:first').find('input').attr('name');
            // checkbox groups + radios always have same name 
            if ( $('input[name="'+name+'"]:checked').length == 0 ) { 
              continueSubmit = false;
            }                
          }
        });

        // check recaptcha
        if ( typeof(grecaptcha) !== "undefined" ){
          if (grecaptcha.getResponse() == ""){
            continueSubmit = false;
          }
        }
        


        if (continueSubmit) {
            var formInputValues = {};

            // ## get text input, hidden and select values 
            $('.elqActivated input[type=text], .elqActivated input[type=hidden], .elqActivated select:not([multiple])').each(function() {
                var theName = $(this).attr('name');
                // must contains input with field name elqFormName elqSiteID and emailAddress
                if (theName && theName.replace(/\s/g, '').length > 0 && $(this).val().replace(/\s/g, '').length > 0) {
                    formInputValues[theName] = $(this).val().trim();
                }
            });


            // checkboxes + radio buttons can be done in one step - we only collect values of checked
            $('.elqActivated input[type=checkbox]:checked, input[type=radio]:checked').each(function() {
                // checkboxes: this is special. we have to find out if these are multiple checkboxes or single
                // if it is multiple, the name attribute will have a [] at the end, otherwise not

                var theName = $(this).attr('name');
            
                // does name end with [] ?
                if (theName && theName.replace(/\s/g, '').length > 0) {
                    if (theName.endsWith("[]")) {
                        // mark for later using classname
                        $(this).addClass('elq-multicheck');
                        // stop - we'll handle this later on
                        return true;
                    }
                    formInputValues[theName] = $(this).val().trim();
                }
            });
            // textarea
            $('.elqActivated textarea').each(function() {
                var theName = $(this).attr('name');
                if (theName && theName.replace(/\s/g, '').length > 0) {
                    formInputValues[theName] = JSON.stringify($(this).val());
                }


            });
            // multiple select
              $('.elqActivated select[multiple]').each(function(){
                if ($(this).children(':selected').length) { // if none selected, toString() will fail...
                  var theValue = $(this).val().toString().trim(); // trim replaces spaces at beginning and end
                  var theName = $(this).attr('name');
                  // append to bfUrl 
                   if (theName && theName.replace(/\s/g, '').length > 0) {
                      formInputValues[theName] = theValue;
                  }
                }
                
              });
              
            bfUrl = bfUrl + jQuery.param(formInputValues);
            // checkboxes - multiple only
             var multicheckValue = {};
            $('.elqActivated .elq-multicheck').each(function() { // only select checkboxes with name containing [] at the end
                var theName = $(this).attr('name');
                if (!multicheckValue[theName]){
                  multicheckValue[theName] = [];
                }
                if (theName && theName.replace(/\s/g, '').length > 0) {
                     multicheckValue[theName].push($(this).val().toString().trim());
                 }
            });
       
            Object.keys(multicheckValue).forEach(function(key,index) {
                if (Object.keys(formInputValues).length > 0) {
                        bfUrl += '&' + key + '=' + String(multicheckValue[key]);
                    } else {
                        bfUrl += '?' + key + '=' + String (multicheckValue[key]);
                    }

            });             


            $('body').append('<iframe id="IntegrationformFrame" frameborder="0" scrolling="no" width="1" height="1"></iframe>');                

            // send to Eloqua
            $('#IntegrationformFrame').attr('src', bfUrl).load(function() {
                $('.elqActivated').submit();
            });
             
        } // end if continuesumbit
        else {
          // we do not send to Eloqua, but form has to submit in order to display validation errors
           $('.elqActivated').submit();
        }
    });
});
</script>

