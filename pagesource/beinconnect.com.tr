<script id="template-quicksearch-item" type="text/x-handlebars-template">
        {{#each Items}}
        <li data-index="{{Index}}" data-url="{{Url}}"><a class="quicksearch-content-item" href="{{Url}}" data-id="{{Id}}" title="{{Title}}">{{Title}}</a></li>
        {{/each}}
    </script>

<script id="template-product-item" type="text/x-handlebars-template">
    {{#each Products}}    
    <a {{hrefHandler Url}} class="slider-item product-item" data-content-id="{{CmsContentId}}" data-content-name="{{Title}}">
        {{#ifCond ContentType '===' 5}}
        <span class="sp icon-bundle"></span>
        {{/ifCond}}
        <img src="{{Poster.PosterDefault}}" alt="{{Title}}" />
        <h4>{{substring Title 17}}</h4>
        <div class="detail" title="{{Title}}">
            <div class="title">
                <span>{{substring Title 19}}</span>
                {{#if_or ContentType 5 2 }}
                <span>{{substring TitleOriginal 19}}</span>
                {{/if_or}}
            </div>
            <div class="category">{{Category}}</div>
            <div class="description">{{substring Description 100}}</div>
            <div class="info">
                <div class="star-container">
                    <div class="star white">
                        <div class="star red" style="width:{{Score}}%"></div>
                    </div>
                </div>
                {{#if_or ContentType 5 2 }}
                   <span>{{Duration}}</span>
                {{/if_or}}
                
            </div>
        </div>
    </a>
    {{/each}}
</script>

<script id="template-recommendation-product-item" type="text/x-handlebars-template">
    {{#each Products}}
    <a {{hrefHandler Url}} class="slider-item product-item recommendation-product-detail" data-content-id="{{CmsContentId}}" data-content-name="{{Title}}">
        <div class="recommendation-product-detail">
            <div>
                <div class="product-actions" data-id="{{CmsContentId}}">
                    <div class="watched">
                        <img src="../Assets/images/watched.png" />
                    </div>
                    <div class="watched-hover">
                        <img src="../Assets/images/watched-hover.png" />
                    </div>
                    <div class="watched-this-content-product">
                        <span>Bu içeriği izledim</span>
                    </div>
                </div>
                <img src="{{Poster.PosterDefault}}" alt="{{Title}}" />
            </div>
            <div>
                <div class="recommendation-product-title">
                    <p>{{Title}}</p>
                    <p>{{TitleOriginal}}</p>
                </div>
                <div class="recommendation-product-description">
                    <p>{{substring DescriptionShort 85}}</p>
                </div>
                <div class="recommendation-product-detail-button">
                    Detay
                </div>
            </div>
        </div>
        <div class="recommendation-product-detail shadow">
        </div>
    </a>
    {{/each}}
</script>

<script id="template-message" type="text/x-handlebars-template">
    <div class="form {{css}}">
        <div class="form-header">
            <h2 class="title">{{title}}</h2>
            <a id="close" class="close" href="#">X</a>
        </div>
        <div class="form-content">
            {{message}}
        </div>
        <div class="form-footer">
            <a href="#" class="modal-ok">{{button}}</a>
        </div>
    </div>
</script>
    


    
    
