    (function ($) {
        var module = "#products-module-2308";
        var current_image = $(module + " #product-image-0");
        var next_image = false;

        var underlineData = new Array();
        var underlineSelectedID = -1;

        function underlineCalcItems() {
            var $items = $('.product-nav-item');
            var $parent = $items.parent();
            $items.each(function (i, dom)
            {
                var $current = $(dom);
                underlineData[i] = new Object();
                underlineData[i].width = $current.width();
                underlineData[i].left = $current.offset().left - $parent.offset().left;
            });
        }

        function underlineFindPosition() {
            var $items = $('.product-nav-item');
            $items.each(function (i, dom)
            {
                if($(dom).hasClass('active'))
                {
                    underlineSelectedID = i;
                }
            });

            var elementData = underlineData[underlineSelectedID];
            var $underline = $('.product-nav-line');
            $underline
                .css('left', elementData.left)
                .css('width', elementData.width);
        }

        var underlineResize = debounce(function()
        {
            underlineCalcItems();
            underlineFindPosition();
        }, 250);

        function setGalleryHeight(){
            var biggest_image_height = 0;
            $(module +" .products-image-wrap > div > .product-image > img").each(function(){
                if ($(this).height() > biggest_image_height ) {
                    biggest_image_height = $(this).outerHeight();
                }
            });
            $(module +" .products-image-wrap").height(biggest_image_height);
        }

        $(module +" .product-nav .product-nav-item").click(function () {
            if($(this).hasClass("active")){
                return;
            }else{
                next_image = $(module + " #product-image-" + $(this).data("id"));

                current_image.animateCss("fadeOut");
                current_image.css("opacity", "0");
                
                next_image.animateCss("fadeInRight");
                next_image.css("opacity", "1");

                current_image = next_image;

                $(module +" .product-item, "+ module +" .product-nav-item").removeClass("active");
                $(module +" #product-content-"  + $(this).data("id")).addClass("active");
                $(module +" #product-nav-item-" + $(this).data("id")).addClass("active");

                underlineFindPosition();
            }
        });

        $(window).resize(function(){
            setGalleryHeight();
            underlineResize();
        });

        $( document ).ready(function() {
            underlineCalcItems();
            underlineFindPosition();
        });

        setGalleryHeight();

        if($(window).width() > 768) { // neizmantojam animācijas uz mobilo
            $(module +" .products-image-wrap").animateCss('fadeInRight');
            $(module +" .products-image-wrap").css('opacity', "1");
            $(module +" .products-content-wrap").children().each(function (index) {
                var element = $(this);
                setTimeout(function () {
                    var button = element.find(".button-base:visible");
                    if(button.length){
                        button.css('opacity', "0");
                    }
                    element.animateCss('fadeInUp');
                    element.css('opacity', "1");
                    if(button.length){
                        setTimeout(function () {
                            button.animateCss('fadeInUp');
                            button.css('opacity', "1");
                        }, 300);
                    }
                }, (300 * index));
            });
        }else{
            $(module +" .products-image-wrap").css('opacity', "1");
            $(module +" .products-content-wrap").children().css('opacity', "1");
        }

    }
    (jQuery));
</script>
