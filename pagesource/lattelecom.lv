};

var mm_underlineCalcItemsLength = function()
{
    window.mm_items = new Array();
    var $items = jQuery('#sticky-menu-source #main-menu .moduletable ul.main-menu > li');
    $items.each(function(i, dom)
    {
        jQuery(dom).data('id', i);
        var $anchor = jQuery(dom).children('a');

        window.mm_items[i] = new Object();
        window.mm_items[i].width = $anchor.width();
        window.mm_items[i].left = $anchor.offset().left;
    });

    //make it vivible for a moment to get correct width/offset
    jQuery('#sticky-menu').addClass('sticky-visible');
    window.mm_items_sticky = new Array();
    var $items = jQuery('#sticky-menu #main-menu .moduletable ul.main-menu > li');
    $items.each(function(i, dom)
    {
        jQuery(dom).data('id', i);
        var $anchor = jQuery(dom).children('a');

        window.mm_items_sticky[i] = new Object();
        window.mm_items_sticky[i].width = $anchor.width();
        window.mm_items_sticky[i].left = $anchor.offset().left;
    });
    jQuery('#sticky-menu').removeClass('sticky-visible');
};

var mm_underlineFindPosition = function(which, init)
{
    init = init || false;

    var visibleMenu = window.mm_sticky_mode ? '#sticky-menu' : '#sticky-menu-source';
    $items = jQuery(visibleMenu + ' #main-menu .moduletable ul.main-menu > li');
    var id = -1;

    $items.each(function(i, dom)
    {
        var $li = jQuery(dom);
        if($li.hasClass(which))
        {
            id = parseInt(jQuery(dom).data('id'), 10);
        }
    });

    if(id < 0)
    {
        jQuery('.main-menu--underline').css('opacity', 0);
        window.mm_selectedMenuID = -1;
    }
    else
    {
        if(init)
        {
            mm_shiftUnderline(id, true);
            mm_shiftUnderline(id, false);
        }
        else
        {
            mm_shiftUnderline(id, window.mm_sticky_mode);
        }

        window.mm_selectedMenuID = id;
    }
};

var mm_shiftUnderline = function(id, isStickyMode)
{
    if(isStickyMode)
    {
        var elementData = window.mm_items_sticky[id];
        var $underline = jQuery('#sticky-menu .main-menu--underline');
    }
    else
    {
        var elementData = window.mm_items[id];
        var $underline = jQuery('#sticky-menu-source .main-menu--underline');
    }

    $underline
        .css('opacity', 1)
        .css('left', elementData.left)
        .css('width', elementData.width);
}

var mm_underlineResize = debounce(function()
{
    mm_underlineCalcItemsLength();
    if(window.mm_selectedMenuID > -1)
    {
        var elementData = window.mm_items[window.mm_selectedMenuID];
        var $underline = jQuery('.main-menu--underline');
        $underline
            .css('left', elementData.left)
            .css('width', elementData.width);
    }
}, 250);

