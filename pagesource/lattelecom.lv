        function loadUserAdresses(setAddress){
            setAddress = setAddress || false;
            jQuery.post( "https://www.lattelecom.lv/component/ajax",
                {'option': 'com_ajax', 'plugin': 'OpenIdAddresses', 'format' : 'raw', async: false}, function(addrs) {
                    try {
                        addrs = JSON.parse( addrs );
                        if(addrs.hasOwnProperty("data")){
                            addrs = addrs.data;
                        }
                        if(addrs.length > 0) {
                            metadata.addresses = addrs;
                            if(setAddress){
                                acf_FormPutAddr(addrs[0].id, addrs[0].value, addrs[0].id);
                            }else{
                                acf_InitAddrAutocomplete( $(".ac-form input"), acf_FormPutAddr, putaddr_fn );
                            }
                        }else{
                            metadata.addresses=0;
                        }
                    }
                    catch(err) {
                        metadata.addresses=0;
                    }
                });
        };

        window.create_popup = function(item_href, html,fancyConfig){
            if(!fancyConfig){
                fancyConfig = {
                    "href": item_href,
                    "autoDimensions": true,
                    "padding": 0,
                    "onStart"       : function() {
                        $('.subwin').css('position', 'static');
                    },
                    "onComplete"	: function(){
                        $('#fancybox-content').addClass('availability-myltc-popup');
                        $('#fancybox-close').addClass('myltc-close-moved');
                    },
                    "onClosed": function() {
                        $(item_href).remove();
                        $('#fancybox-close').removeClass('myltc-close-moved');
                        $('#fancybox-content').removeClass('availability-myltc-popup');
                        if (docCookies.hasItem( 'mltc_name' )) {
                            window.setLogedInText();
                        } else {
                            window.setLogedOutText();
                        }
                    }
                };
            }
            function custFancyBox(){
                popup.hide();
                $("html").addClass("availability-myltc-popup-open");
                $("body").append($("#fancybox-tmp"));
                $("body").append($("#fancybox-loading"));
                $("body").append($("#fancybox-overlay"));
                $("body").append($("#fancybox-wrap"));
                $("body").append("<div class='availability-myltc-popup mob-fancy-replace'></div>");
                $("body").show();
                var fancyReplace = $(".mob-fancy-replace").append("<div class='close-fancy-replace' style='float:right;margin-top:20px;margin-right:20px;'><img id='close-fancy-replace' src='" + JROOT + "/templates/forms/images/close-mob.png'></div>");
                fancyReplace.append("<div class='fancy-replace-content'></div>");
                var fancyCont = $(".fancy-replace-content");
                var width = $(window).width();
                var height = $( window ).height();
                fancyCont.append(html);
                fancyCont.css({
                    "width": (width)+'px',
                    "clear": "both",
                    "padding-top":"20px"
                });
                fancyReplace.css({
                    "position": "fixed",
                    "z-index": "2000",
                    "top": "0",
                    "background-color": "#fff",
                    "height": height+'px',
                    "width": width+'px',
                    "padding":"0",
                    "overflow":"auto"
                });
                $(".mob-fancy-replace .close-fancy-replace").click(function(){
                    $("html").removeClass("availability-myltc-popup-open");
                    popup.append($("#fancybox-tmp"));
                    popup.append($("#fancybox-loading"));
                    popup.append($("#fancybox-overlay"));
                    popup.append($("#fancybox-wrap"));
                    popup.append("<div class='availability-myltc-popup mob-fancy-replace'></div>");
                    //$("body").hide();
                    popup.show();
                    $('html, body').animate({
                        scrollTop: $(item_href).parent().offset().top - 100
                    }, 0);
                    fancyReplace.hide();
                    if($(item_href).length){
                        $(item_href).hide();
                    }
                    if(docCookies.hasItem( 'mltc_name' )) {
                        window.setLogedInText();
                    } else {
                        window.setLogedOutText();
                    }
                });
            }
            if($( window ).width() < 750){
                custFancyBox();
                $( window).resize(function(){
                    if($(".mob-fancy-replace").length !== 0){
                        var fancyCont = $(".fancy-replace-content");
                        var fancyReplace = $(".mob-fancy-replace");
                        var width = $(window).width();
                        var height = $( window ).height();
                        fancyReplace.width( width );
                        fancyReplace.height( height );
                        fancyCont .width( width );
                    }
                });
            }else{
                if($(item_href).length === 0){
                    jQuery('#main-content').append(html);
                }
                $.fancybox(fancyConfig);
            }
        };

