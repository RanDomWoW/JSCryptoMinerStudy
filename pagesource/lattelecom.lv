        window.setLogedInText = function(){
            if (acf_hasAddr()) {
                window.setAddressText()
            }
            if(!docCookies.hasItem( "user_address" )){
                loadUserAdresses(true);
            }
            $(".footer-text").html( openid.user.name + " " + openid.user.surname + ', ' + logedInText );
            $(".footer-text .myltclogin-out-trigger" ).click(function(event){
                event.preventDefault();
                openid.state.logout_by_accesscheck = true;
                openid.logout();
            });
        };

        window.setLogedOutText = function(){
            if (acf_hasAddr()) {
                window.setAddressText()
            }
            $('.footer-text').html( "Esi Lattelecom klients? Pieslēdzies ar Mans Lattelecom. <a class=\'myltclogin-trigger\' href=\'\'>Ienākt</a>" );
            $( ".myltclogin-trigger" ).click(function(event) {
                event.preventDefault();
                openid.state.login_by_accesscheck = true;
                openid.login();
            });
        };

        window.setOpenPopupAfterLogin = function(){
            if(typeof(openid.state.login_by_accesscheck) != "undefined") {
                if (openid.state.login_by_accesscheck == true) {
                    delete openid.state.login_by_accesscheck;
                    openPopup();
                    resizeSearchCont();
                }
            }
        };

        window.setOpenPopupAfterLogout = function(){
            if(typeof(openid.state.logout_by_accesscheck) != "undefined") {
                if (openid.state.logout_by_accesscheck == true) {
                    delete openid.state.logout_by_accesscheck;
                    openPopup();
                    resizeSearchCont();
                }
            }
        };

        openid.addCallback("initial", "login",  window.setOpenPopupAfterLogin);
        openid.addCallback("initial", "logout", window.setOpenPopupAfterLogout);
        openid.addCallback("recurrent", "login", window.setLogedInText);
        openid.addCallback("recurrent", "logout", window.setLogedOutText);
        openid.check();

        var logedInText = 'Tu esi pieslēdzies ar Mans Lattelecom lietotājvārdu un paroli. <a class=\"myltclogin-out-trigger\" href=\"\">Iziet</a>';

        $(document).ready(function(){

            $(window).resize(function(){
                resizeSearchCont();
                sizeAvailabilityPopup();
            });

            $(".availability-check-wrap").each(function(){
                if($(this).parents(".module").hasClass("availability-view")){
                    $(this).find(".button-base.button-grey").removeClass("button-grey").addClass("button-filled-red");
                }
            });

        });


    })( jQuery );

</script>
