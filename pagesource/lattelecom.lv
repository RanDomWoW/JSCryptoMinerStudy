                                aIMAGES[i] = $dom;
                                aIMAGES[i].css('opacity', '0');
                                setTimeout(function (param) {
                                    aIMAGES[param].animateCss('fadeInRight');
                                    aIMAGES[param].css('opacity', '1');
                                }, 300 * delayStep++, i);
                            }
                        });
