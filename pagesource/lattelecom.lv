            acf_OpenForm();
        };

        window.reloadForm = function(){
            acf_OpenForm(false, false);
        };

        function acf_OpenForm( assume_no_addr, return_form, notFirstLoad )
        {
            if (notFirstLoad == undefined || notFirstLoad == null ) notFirstLoad = true;
            if( window.acf_active ) return false;

            $(document).trigger('ppOpened',['acf']);
            /*window.pp_overlayMainMenu(function() {
             acf_CloseForm();
             });*/
            $(document).one('ppOpened', function(e,type) {
                if(type != 'acf' && window.acf_active) {
                    acf_CloseForm();
                }
            });

            var hasaddr = !assume_no_addr && acf_hasAddr();

            if( hasaddr && notFirstLoad)
            {
                //create ha-part if not already created
                if ($('.ha-part.part1').length === 0) {
                    var el = $( "<div />" );

                    el.append( $( "<div />", {'class': 'ha-part part1'} ).
                        append( $( "<div />", {'class': 'title', 'text': 'Izvēlētā adrese'} ) ).
                        append("<div class='text col-md-6 col-md-offset-3'><div><div><img class='loc-icon' src='" + JROOT + "/templates/lattelecom_responsive/images/loc-icon.png'><span class='address-text'>" + acf_getAddrText() + '</span><a class="button-base button-grey button-small change linkW32" href="javascript:;">Noņemt adresi</a></span>' + "</div>").
                        append(acf_Link( 'Turpināt', 'button-base button-filled-red button-next').attr('href',ACF_HA_SERVICES_LINK)).
                        append( $( "<div class='clear' />" ) )
                    );
                    el.append( $( "<div />", {'class': 'line'} ) );

                    if($('.ac-form').length){
                        hideInput();
                        $('.ac-form').append(el);
                    }else{
                        acf_SetForm( el );
                    }
                    $( ".ac-form .button-base.button-filled-red.button-next" ).wrap( "<div class='button-wrap'></div>" );

                }
            }
            else
            {

                var el = $( "<div />" );
                var cta = $( "<button />", {'type': 'button', 'class': 'cta', 'text': 'Meklēt'} );

                el.append( $( "<div />", {'class': 'title', 'text': 'Ieraksti adresi'} ) );
                el.append( $( "<div />", {'class': 'desc col-xs-10 col-xs-offset-1', 'text': 'Lai noskaidrotu vai pakalpojums ir pieejams, ievadi adresi'} ) );

                el.append( $( "<div />", {'class': 'iact'} ).
                    append( acf_SLTBWrap( acf_InputBox( 'addr', 'Piemērs: Nometņu 10-5, Salaspils' ) ) ).
                    append( cta ).
                    append( acf_Loader()).
                    append( acf_ErrorBox())
                );

                var input = el.find("input");
                window.acf_addresses = [];
                acf_InitAddrAutocomplete( input, acf_FormPutAddr, putaddr_fn );



                input.keypress(function(e)
                {
                    if( e.which == 13 ) {
                        if(autocomplete_open) {
                            autocomplete_open = false;
                            return;
                        }
                        cta.click();
                    }
                });

                if($('.ac-form').length){
                    showInput();
                    $('.ac-form').append(el);
                }else{
                    acf_SetForm( el );
                }
            }
            return true;
        };
        function acf_CloseForm()
        {
            if( window.acf_active && window.acf_active !== true )
            {
                var el = window.acf_active;
                window.acf_active = true;
                el.fadeOut( 0, function(){ $(this).remove(); window.acf_active = null; } );

                // set autocomplete flag to false
                autocomplete_open = false;

                safePromoResumeBy( "ac-form" );
            }
        };

        window.acf_ClickAction = function()
        {
            if( window.acf_active )
                acf_CloseForm();
            else{
                acf_OpenForm();
                jQuery(".addr.ui-autocomplete-input").focus();
            }
        };
        window.acf_GetAddressForm = function(){ return acf_OpenForm( true, true ); };

        window.acf_InitAll = function( input, button )
        {
            window.acf_addresses = [];
            var putaddr_fn = function( items )
            {
                var out = [];
                for( var i = 0; i < items.length; ++i )
                {
                    if( items[ i ].id != 0 )
                        out.push( items[ i ] );
                }
                window.acf_addresses = out;
            };
            var setaddr_fn = function( id, value, lowest_id )
            {
                window.acf_addresses = [{ id: id, value: value, lowest_id:lowest_id }];
            };
            var cta_fn = function()
            {
                var me = $(this);
                var ibox = input;
                var eact = ibox.closest('div').closest('div').closest('div');
                var ttpos = me;

                eact.find('.textbox').removeClass('error');
                acf_SetError();

                if( ibox.val().length < 4 )
                {
                    acf_SetError( TR('ACF_MSG_MINSYM'), Infinity, 0 );
                    eact.find('.textbox').addClass('error');
                    return;
                }

                if( window.acf_addresses.length )
                {
                    if( window.acf_addresses.length == 1 )
                    {
                        acf_PutAddr( window.acf_addresses[ 0 ].id, window.acf_addresses[ 0 ].value, window.acf_addresses[ 0 ].lowest_id );
                        acf_close();
                    }
                    else
                        oaf_OpenForm( window.acf_addresses );
                }
                else
                    esf_OpenForm();
            };

            acf_InitAddrAutocomplete( input, setaddr_fn, putaddr_fn );
            $('.ui-autocomplete').css('z-index',2000);
            button.click( cta_fn );
        };

        function hideInput(){
            $('.ac-form .title').hide();
