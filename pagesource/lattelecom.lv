(function($){

    window.sticky_menu_closing = false;

    $( document ).ready(function() {
        if (! $("#sticky-menu").length ) {
            $("#sticky-menu-source").clone(true).attr("id","sticky-menu").insertAfter("#sticky-menu-source");

            $(window).scroll(function(){
                if($(".banners-responsive-module").length){
                    var trigger = $("#banners");
                }else{
                    var trigger = $("#upper-section");
                }
                var sticky_menu = $("#sticky-menu");
                if( ! trigger.is(":visible")){
                    sticky_menu.removeClass("sticky-visible");
                    window.mm_sticky_mode = false;
                    window.sticky_menu_closing = false;
                }else {
                    if ($(this).scrollTop() >= (trigger.position().top + trigger.outerHeight(true) )) {
                        if (!sticky_menu.hasClass("sticky-visible")) {
                            sticky_menu.addClass("sticky-visible");
                            sticky_menu.animateCss('slideInDown');
                            window.mm_sticky_mode = true;
                        }
                    } else {
                        if (sticky_menu.hasClass("sticky-visible") && !window.sticky_menu_closing) {
                            window.sticky_menu_closing = true;
                            sticky_menu.animateCss('slideOutUp');
                            sticky_menu.one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function () {
                                if ($(this).scrollTop() < (trigger.position().top + trigger.outerHeight(true) ) && window.sticky_menu_closing) {
                                    sticky_menu.removeClass("sticky-visible");
                                }
                                window.sticky_menu_closing = false;
                            });
                            window.mm_sticky_mode = false;
                        }
                    }
                }
            });
        }

        mm_underlineCalcItemsLength();
        mm_underlineFindPosition('active', true);
    });

}(jQuery));
