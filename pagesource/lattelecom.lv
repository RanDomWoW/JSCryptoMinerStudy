            myltcapp.setBlueconicVariables();
        };

        myltcapp.openWindow = function () {
            myltcapp.opened = 1;
            myltcapp.root.children().addClass("opened");
            if ($(window).width() < 768) {
                myltcapp.root.children('.subwin').showModal();
            }
            else {
                myltcapp.root.children('.subwin').slideOut(myltcapp.root.closest('.container'));
            }
            myltcapp.root.find('.closebtn').fadeIn();
        };

        myltcapp.closeWindow = function () {
            if (myltcapp.opened == 1) {
                if ($(window).width() < 768) {
                    myltcapp.root.children('.subwin').hideModal();
                }
                else {
                    myltcapp.root.children('.subwin').slideIn(myltcapp.root.closest('.container'));
                }
                myltcapp.root.children().removeClass("opened");
                myltcapp.root.find('.closebtn').fadeOut(600);
                myltcapp.opened = 0;

            }
        };

        //close myltc if clicked outside
        jQuery(document).mousedown(function (e) {
            if (!jQuery(e.target).closest('#myltc-module').length) {
                myltcapp.closeWindow();
            }
        });

        myltcapp.root.find('.openbtn').click(function () {
            if ($(this).hasClass("opened")) {
                myltcapp.closeWindow();
                return;
            }
            myltcapp.root.find('.myltc-form').show();
            myltcapp.openWindow();
        });

        myltcapp.root.find('.closebtn, .action-close, #myltc-close-button').click(function () {
            myltcapp.closeWindow();
        });

        myltcapp.root.find("#myltc-login-button").click(function() {
            openid.state.login_by_myltc_module = true;
            openid.login();
        });

        myltcapp.root.find("#myltc-logout-button").click(function() {
            openid.logout();
        });

        $(window).resize(function(){
            if(myltcapp.opened == 1){
                myltcapp.openWindow();
            }else{
                myltcapp.closeWindow();
            }
        });

        myltcapp.openWindowAfterLogin = function(){
            if(typeof(openid.state.login_by_myltc_module) != "undefined") {
                if (openid.state.login_by_myltc_module == true) {
                    delete openid.state.login_by_myltc_module;
                    myltcapp.openWindow();
                }
            }
        };

        myltcapp.opened = 0;
        window.myltcGetToken = openid.getJoomlaSessionToken;

        openid.addCallback("initial", "login", myltcapp.openWindowAfterLogin);
        openid.addCallback("recurrent", "login", myltcapp.mltcLogin);
        openid.addCallback("recurrent", "logout", myltcapp.mltcLogout);
        openid.check();

    })(jQuery);
</script>

