var mm_enterHandler = function(event)
{
        var me = jQuery(this).parent();
        if(me.find( ".dropdown-wrap" ).children( ".container" ).length == 0)
        {
            //If the menu has no child menu elements, close the menu.
            me.parent().css("padding-bottom", 0);
            me.removeClass( 'openMenuItem' );
            me.removeClass( 'forceopen' );
            return true;
        }
        if(me.hasClass('openMenuItem'))
        {
            me.removeClass( 'forceopen' );
            me.removeClass( 'openMenuItem' );
            slideInMenu(me);
            return false;
        }
        if(viewport.is('xs') || viewport.is('sm')){
            var open = me.parent().find(".forceopen.openMenuItem").length;
            me.parents(".main-menu-cont").find(".main-menu > li.forceopen").removeClass("forceopen");
            me.parents(".main-menu-cont").find(".main-menu > li.openMenuItem").removeClass("openMenuItem");
            me.parents(".main-menu-cont").find(".main-menu > li > .dropdown-wrap > .container").hide();
            if(open) {
                me.children( ".dropdown-wrap" ).children( ".container" ).show();
            } else {
                me.children(".dropdown-wrap").children(".container").slideOut(me.parent());
            }
            me.addClass( 'forceopen' );
            me.addClass( 'openMenuItem' );
            return false;
        }
        else {
            if(isClosing || isOpening)return false;
            event.preventDefault();
            me.parent().find("li.forceopen").removeClass("forceopen");
            me.parent().find("li.openMenuItem").removeClass("openMenuItem");
            me.parent().find("> li > .dropdown-wrap > .container").hide();
            me.parent().find("> li > .dropdown-wrap > .container").css("height", 0);
            if(!isOpen){
                slideOutMenu(me);
                me.addClass('forceopen');
                me.addClass('openMenuItem');
                alignMenuElems();
            }
            else {
                me.children(".dropdown-wrap").children(".container").css("height", 349);
                me.children(".dropdown-wrap").children(".container").show();
                me.addClass('forceopen');
                me.addClass('openMenuItem');
                alignMenuElems();
                slideOutMenu(me);
            }

            mm_underlineFindPosition('forceopen');
        }
        return false;
};

var mob_mm_toggleSubMenu = function(event)
