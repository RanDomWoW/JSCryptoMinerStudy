            $("body > section#upper-section > .container").first().css("padding-bottom", "0px");
            popup.hide();
        });

        if(jQuery(".prev-step-mob").length){
            jQuery(".prev-step-mob").css("visibility","hidden");
        }

        window.max_records = '20';
        function acf_pos(){ return $(".ppx"); };

        window.acf_SetBluconicVariables = function(){
            var lowest_id = window.acf_getLowestID();
            if(typeof(user_codified_address) == "undefined" || user_codified_address == "" || lowest_id != user_codified_address){
                if(typeof(lowest_id) == "undefined"){
                    user_codified_address = "";
                }else{
                    user_codified_address = lowest_id;
                }
            }
        };
        window.acf_isAuth = function(){ return $(document.body).hasClass('mltcLoggedIn'); }
        window.acf_hasAddr = function(){
            window.acf_SetBluconicVariables();
            return docCookies.hasItem( "user_address" );
        };
        window.acf_getAddrID = function(){ return (docCookies.getItem( "user_address" )+"").split(':')[0]; }
        window.acf_getAddrText = function(){ return (docCookies.getItem( "user_address" )+"").split(':')[1]; }
        window.acf_getLowestID = function(){ return (docCookies.getItem( "user_address" )+"").split(':')[2]; };
        window.acf_setAddr = function( id, addr, lowest_id ){
            docCookies.setItem( "user_address", [id,addr,lowest_id].join(':'), Infinity, "/" );
            window.acf_SetBluconicVariables();
        }
        window.acf_unsetAddr = function(){ docCookies.removeItem( "user_address", "/" ); }

        function acf_close(){ $("#fancybox-close").click(); };
        window.acf_ResetAddr = function()
        {
            acf_unsetAddr();
            acf_CloseForm();
            //remove
            $(".ha-part.part1").remove();
            if($('.iact').length){
                showInput();
            }else{
                acf_OpenForm();
            }
            $(".addr.ui-autocomplete-input").val("").focus().blur();
        };

        window.acf_Next = function()
        {
            console.log('hello');
        };

        if( window.acf_active ){
            acf_CloseForm();
        }

        function acf_csa_addovr( el ){ el.append( $('<div class="acf-checking-overlay">') ); };
        function acf_csa_removr( el ){ el.find('.acf-checking-overlay').remove(); };
        function acf_csa_addfailovr( el )
        {
            $('.loader').hide();
            $('.cta').show();
            $( ".ac-form .desc" ).after( $('<div class="no-services row">').append('<p class="col-xs-8 col-xs-offset-2">Atvainojamies, šobrīd nav iespējams veikt pieejamības pārbaudi!') );
        };

        function acf_checkServiceAvail( el )
        {
            if(isServiceAviable()!=0){
                if(isServiceAviable()==-1){
                    acf_csa_addfailovr( el );
                }
                return;
            }

            acf_csa_addovr( el );
            $.ajax
            ({
                type: "POST",
                url: 'https://www.lattelecom.lv/components/com_accesscheck/addressjson.php?availtest=1',
                data: null,
                dataType: "text",
                success: function( data )
                {
                    data = JSON.parse( data );
                    window.acf_avail_checked = data.result;
                    window.session_token = data.token;
                    acf_csa_removr( el );
                    if( !data || !data.result )
                        acf_csa_addfailovr( el );
                },
                error: function()
                {
                    acf_csa_removr( el );
                }
            });
        };
        window.acf_extCheckServiceAvail = function(){ return acf_checkServiceAvail( $("<div>") ); };


        // disable highlighting of 100% successful autocomplete queries
        window.use_ac_highlights = false;


        var autocomplete_open = false;

        // enable in-result highlights
        $.ui.autocomplete.prototype._renderItem = function( ul, item )
        {
            var term = this.term.split(' ').join('|');
            var re = new RegExp("(" + term + ")", "gi");
            var t = item.label.replace(re,"<b>$1</b>");
            return $( "<li></li>" )
                .data( "item.autocomplete", item )
                .append( "<a><div class='text'>" + t + "</div><div class='set-address'>Izvēlēties</div><div class='clearfix'></div></a>" )
                .appendTo( ul );
        };


        jQuery.fn.ltc_add_tooltip = function( text, time, fadetime )
        {
            time = time || 5000;
            fadetime = fadetime || 5000;

            var at = jQuery(this).first();
            at.css('position','relative');

            var el = jQuery( "<div />", {'class': 'ltc-tooltip'} );
            el.css('position','absolute');
            el.append( jQuery( "<span />", {'class': 'lft', 'html': '&nbsp;'} ) );
            el.append( jQuery( "<span />", {'class': 'mid', 'html': text} ) );
            el.append( jQuery( "<span />", {'class': 'rgt', 'html': '&nbsp;'} ) );
            if( time !== Infinity )
                setTimeout( function(){ el.fadeOut( fadetime ); }, time );
            at.append( el );
        };
        jQuery.fn.ltc_remove_tooltip = function()
        {
            $(this).find('.ltc-tooltip').remove();
        };

        function safePromoPauseBy( x ) { if( typeof promoPauseBy !== 'undefined' ) promoPauseBy( x ); };
        function safePromoResumeBy( x ) { if( typeof promoResumeBy !== 'undefined' ) promoResumeBy( x ); };

        window.acf_active = null;

        function TR( text )
        {
            var tr = System.TText._(text);
            if( typeof tr === "undefined" )
                return text;
            else return tr;
        };


        function acf_InputBox( cls, def, name )
        {
            if (name == null)
            {
                name = cls;
            }
            var el = $( "<input />", {'class': cls, 'type': 'text', 'name' : name} );
            if( def )
            {
                def = TR( def );
                el.val( def );
                el.focus(function()
                {
                    var el = $(this);
                    if( el.val() == def )
                        el.val("");
                });
                el.blur(function()
                {
                    var el = $(this);
                    if( el.val() == "" )
                        el.val( def );
                });
            }
            return el;
        };
        function acf_Loader()
        {
            var el = $( "<img />", {'class': 'loader', 'src': 'https://www.lattelecom.lv/templates/forms/images/loader.gif'} );
            el.hide();
            return el;
        };

        function acf_SLTBWrap( input, cls )
        {
            cls = cls || 'textbox';
            var el = $( '<span />', {'class': cls} );
            el.append( $( '<span />', {'class': 'lft', 'html': '&nbsp;'} ) );
            el.append( $( '<span />', {'class': 'mid'} ).append( input ) );
            el.append( $( '<span />', {'class': 'rgt', 'html': '&nbsp;'} ) );
            return el;
        };
        function acf_Button( text, cls )
        {
            cls = cls || 'button';
            var el = $( '<a />', {'class': cls, 'href': 'javascript:;'} );
            el.append( $( '<span />', {'class': 'lft', 'html': '&nbsp;'} ) );
            el.append( $( '<span />', {'class': 'mid', 'text': TR(text)} ) );
            el.append( $( '<span />', {'class': 'rgt', 'html': '&nbsp;'} ) );
            return el;
        };

        function acf_Link( text, cls, icon)
        {
            cls = cls || 'button';
            var el = $( '<a />', {'class': cls, 'href': 'javascript:;'} );
            el.append( $( '<span />', { 'text': TR(text)} ) );
            return el;
        };

        function acf_ErrorBox()
        {
            var el = $( '<div />', {'class': 'error-box'} ).hide();
            return el;
        };
        function acf_SetError( err )
        {
            var ebox = $('.ac-form .error-box');
            if( !ebox.length ) ebox = $('#fancybox-wrap .error-box');
            if( !ebox.length ) ebox = $('.error-box');

            if( err !== null && err !== '' && typeof err !== 'undefined' )
                ebox.text(err).show();
            else
                ebox.hide();
        };

        function acf_FancyBox( cont )
        {
            if( typeof window._pfbid === "undefined" )
                window._pfbid = 0;
            var id = "_pfb"+(window._pfbid++);

            var link;
            var wrap = $( "<div />", {'style': 'display:none'} ).
            append( cont.attr( 'id', id ) ).
            append( link = $( "<a />", { 'href': '#'+id } ) );
            $('body').append( wrap );
            var close_fn = function(){safePromoResumeBy("ac-fancy");
                $("#fancybox-wrap").removeClass('ltcform');
                $(document).trigger('acfFancyBoxClosed');};
            link.fancybox({padding:0, onClosed:close_fn}).click();
            $("#fancybox-wrap").addClass('ltcform');
            wrap.remove();

            safePromoPauseBy( "ac-fancy" );
        };
        window.setAddressText = function()
        {
            $('.availability-check-wrap').addClass('address-set');
            $('.current-address').html("<br/>" + acf_getAddrText());
        };
        window.acf_PutAddr = function( id, addr, lowest_id )
        {
            recent_addresses = '';
            acf_setAddr( id, addr, lowest_id );
            setAddressText( addr );
        };

        function oaf_SetMyAddr()
        {
            var id = $(this).attr('rel');
            var addr = $(this).text();
            var lowest_id = $(this).attr('name');
            acf_PutAddr( id, addr, lowest_id );
            acf_close();
        };
        function oaf_OpenForm( addrlist )
        {
            recent_addresses = [];
            var el = $( "<div />", {'class': 'oa-form'} );

            var alel_1 = $( "<ul />", {'class': 'addrlist'} );
            for( var i = 0; i < addrlist.length && i < 10; ++i )
            {
                var link = $( "<a />", {'href':'javascript:;','text':addrlist[i].value,'rel':addrlist[i].id, 'name':addrlist[i].lowest_id } );
                link.click( oaf_SetMyAddr );
                alel_1.append( $( "<li />" ).append( link ) );
            }
            var alel_2 = $( "<ul />", {'class': 'addrlist'} );
            for( var i = 10; i < addrlist.length && i < 20; ++i )
            {
                var link = $( "<a />", {'href':'javascript:;','text':addrlist[i].value,'rel':addrlist[i].id, 'name':addrlist[i].lowest_id} );
                link.click( oaf_SetMyAddr );
                alel_2.append( $( "<li />" ).append( link ) );
            }


            if(typeof for_forms !== 'undefined'){
                if(for_forms){
                    el.append( $( "<div />", {'class': 'title', 'text': TR('OAF_TITLE_FOR_FORMS')} ) );
                }
            }else{
                el.append( $( "<div />", {'class': 'title', 'text': TR('OAF_TITLE')} ) );
            }
            el.append( $( "<div />", {'class': 'line'} ) );

            el.append( $( "<div />", {'class': 'desc-left', 'text': TR('OAF_DESC_LEFT')} ) );


            if(typeof for_forms !== 'undefined'){
                if(for_forms){
                    el.append( $( "<div />", {'class': 'ofwrap'} ).
                        append( acf_ErrorBox() ).
                        append( $( "<div />", {'class': 'iact'} ).
                        append( acf_SLTBWrap( acf_InputBox( 'addr' ) ) ).
                        append( $( "<button />", {'type': 'button', 'class': 'cta', 'text': TR('OAF_CTA')} ) )
                        ).
                        append( $( "<div />", {'class': 'example', 'text': 'Piemērs: Nometņu 10-5, Salaspils'} ) ).
                        append( $( "<div />", {'class': 'pane-left'} ).append( alel_1 ) ).
                        append( $( "<div />", {'class': 'pane-left'} ).append( alel_2 ) ).
                        append( $( "<div />", {'class': 'pane-right'} ).
                        append( $( "<div />", {'class': 'desc-right', 'text': TR('OAF_DESC_RIGHT')} ) ).
                        append( $( "<div />", {'class': 'iact2'} ).
                        append( $( "<button />", {'type': 'button', 'class': 'exs', 'text': TR('OAF_EXT')} ).
                        click( esf_OpenForm ) )
                        )
                        )
                    );
                }
            }else{
                el.append( $( "<div />", {'class': 'ofwrap'} ).
                    append( $( "<div />", {'class': 'pane-left'} ).append( alel_1 ) ).
                    append( $( "<div />", {'class': 'pane-left'} ).append( alel_2 ) ).
                    append( $( "<div />", {'class': 'pane-right'} ).
                    append( $( "<div />", {'class': 'iact2'} ).
                    append( $( "<button />", {'type': 'button', 'class': 'exs', 'text': TR('OAF_EXT')} ).
                    click( esf_OpenForm ) )
                    )
                    )
                );
            }

            acf_InitAll( el.find('.addr'), el.find('.cta') );
            acf_checkServiceAvail( el );
            acf_FancyBox( el );
        };

        function esf_LabeledInput( name, cls )
        {
            name = TR( name );
            var el = $( "<div />", {'class': cls + ' lblinput'} ).
            append( $( "<div />", {'class': 'label', 'text': name} ) ).
            append( acf_SLTBWrap( acf_InputBox( cls ) ) );
            return el;
        };

        window.esf_fields = {};
        function esf_StoreFields()
        {
            window.esf_fields = {};
            var domf = $('.es-form input');
            for( var i = 0; i < domf.length; ++i )
            {
                var df = $(domf[i]);
                window.esf_fields[ df.attr('name') ] = df.val();
            }
        };

        var root_address_index = 0;
        function aof_Populate_Addresses(recent_addresses_data, el){

            if(typeof recent_addresses_data[root_address_index] === 'undefined'){
                return;
            }
            if(jQuery('.house.ui-autocomplete-input').val() && jQuery('.city.ui-autocomplete-input').val() && jQuery('.street.ui-autocomplete-input').val()){
                for(var i = 0; i< recent_addresses_data.length; i++){
                    var string_1 = jQuery('.street.ui-autocomplete-input').val() + ' ' + jQuery('.house.ui-autocomplete-input').val() + ',';
                    if(recent_addresses_data[i].value.indexOf(string_1) > -1 ){
                        acf_PutAddr( recent_addresses_data[i].id, recent_addresses_data[i].value );
                        acf_close();
                        return;
                    }
                }
            }
            var custom_address = recent_addresses_data[root_address_index].value;
            var reqdata_custom = { term: custom_address, lastgroup: '', max_records: window.max_records };
            reqdata_custom[ window.session_token ] = "1";
            $.post( 'https://www.lattelecom.lv/components/com_accesscheck/addressjson.php', reqdata_custom,
                function( response ) {
                    var no_duplicate_id = true;
                    var acf_addr_count = window.acf_addresses.length;
                    for(i = 0; i< response.length; i++){
                        for(var j = 0; j< window.acf_addresses.length; j++){
                            if(window.acf_addresses[j].id == response[i].id ){
                                no_duplicate_id = false;
                                break;
                            }
                        }
                        if(response[i].id !== '0' && acf_addr_count < 20 && no_duplicate_id){
                            window.acf_addresses[acf_addr_count] = response[i];
                            acf_addr_count++;
                        }
                    }
                    root_address_index++;
                    if(acf_addr_count >= 20 || typeof recent_addresses_data[root_address_index] === 'undefined'){
                        el.find('a.button').click( oaf_OpenForm( window.acf_addresses ));
                        root_address_index = 0;
                        el.find('a.button').trigger('click');
                    }
                    else{
                        aof_Populate_Addresses(recent_addresses_data, el);
                    }
                }, 'json');
        };
        function valide_address_count(data){
            var count = 0;
            for(i = 0; i< data.length; i++){
                if(data[i].id!== '0'){
                    count++;
                }
            }
            return count;
        };

        var putaddr_fn = function( items )
        {
            var out = [];
            for( var i = 0; i < items.length; ++i )
            {
                if( items[ i ].id != 0 )
                    out.push( items[ i ] );
            }
            window.acf_addresses = out;
        };

        function esf_OpenForm()
        {
            var wrap = $( "<div />", {'class': 'es-form'} );

            var el = $( "<form />" );
            wrap.append(el);

            el.append( $( "<div />", {'class': 'title', 'text': TR('ESF_TITLE')} ) );
            el.append( $( "<div />", {'class': 'line'} ) );
            el.append( $( "<div />", {'class': 'desc', 'text': TR('ESF_DESC')} ) );

            el.append( acf_ErrorBox() );

            el.append( $( "<div />", {'class': 'pane-left'} ).
            append( esf_LabeledInput( 'ESF_LBL_TOWN', 'city' ) ).
            append( esf_LabeledInput( 'ESF_LBL_STREET', 'street' ) ).
            append( esf_LabeledInput( 'ESF_LBL_BUILDING', 'house' ) ).
            append( esf_LabeledInput( 'ESF_LBL_GROUP', 'flat' ) ));
            el.append( $( "<div />", {'class': 'button-wrap'} ).
            append( acf_Button( 'ESF_CTA' ) ));

            window.acf_addresses = [];
            if(typeof recent_addresses[0] !== 'undefined'){
                if(valide_address_count(recent_addresses) <= 20){
                    aof_Populate_Addresses(recent_addresses, el);
                }
                else{
                    el.find('a.button').click( oaf_OpenForm );
                    esf_AddAutoComplete( recent_addresses );
                    el.find('a.button').trigger('click');
                }
            }
            else{
                el.find('a.button').click( function(){
                    aof_Populate_Addresses(recent_addresses, el);
                });
                acf_checkServiceAvail( wrap );
                acf_FancyBox( wrap );
                //esf_LoadFields(); Iepriekšējo rezultātu ielāde
                esf_AddAutoComplete( putaddr_fn );
            }
        };
        var city_field_type = 'both';
        var recent_addresses = [];
        function esf_AddAutoComplete( onLOAD )
        {
            $( ".es-form input" ).autocomplete({
                minLength: 1,
                delay: 1000,
                source: function( request, response) {
                    var reqdata = $('.es-form input').serialize();
                    reqdata = reqdata + '&field=' + $(this.element).attr('name');
                    reqdata += "&" + window.session_token + "=1";
                    if(city_field_type !== 'both')
                        reqdata += "&" + 'city_field_type' + "=" + city_field_type;
                    else{
                        reqdata += "&" + 'city_field_type' + "=both";
                    }
                    $.post('https://www.lattelecom.lv/components/com_accesscheck/advancedjson.php',
                        reqdata,
                        function(data) {
                            var addrs = [];
                            for( var i = 0; i < data.length; ++i )
                                addrs = addrs.concat( data[i].addrs );
                            window.acf_addresses = addrs;
                            recent_addresses = addrs;
                            onLOAD( addrs );
                            response(data);
                        },
                        'json'
                    ).fail(function() { response({}); });
                },
                open: function(){
                    $(this).autocomplete('widget').css('z-index', 2000); // fancybox workaround
                    return false;
                },
                select: function( event, ui )
                {
                    if( $(this).hasClass('phone') )
                    {
                        acf_close();
                        acf_PutAddr( ui.item.id, ui.item.value );
                    }
                    else{
                        onLOAD( ui.item.addrs );
                        city_field_type = ui.item.city_field_type;
                    }
                }
            });
        };


        window.test1080 = function()
        {
