            $("body section.page-section").hide();
        }

        window.sizeAvailabilityPopup = function() {
            popup.css("min-height", $(window).height() + "px");
        }

        $( '.availability-popup-wrap .popup-close' ).click(function() {
            if (!acf_hasAddr()){
                $('.availability-check-wrap').removeClass('address-set');
            } else {
                window.setAddressText();
            }
            popup.removeClass( 'open' );
            $("body section.page-section").show();
            if ($(".banners-responsive-module .banner-slides").length) {
                $(".banners-responsive-module .banner-slides").get(0).slick.setPosition();
