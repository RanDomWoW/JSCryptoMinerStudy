                            var $dom = $(item);
                            if ($dom.length > 0 && $dom.is(':visible')) {
                                aELEMENTS[i] = $dom;
                                aELEMENTS[i].css('opacity', '0');
                                setTimeout(function (param) {
                                    aELEMENTS[param].animateCss('fadeInUp');
                                    aELEMENTS[param].css('opacity', '1');
                                }, 300 * delayStep++, i);
                            }
                        });

                        delayStep = 1;

                        var aIMAGES = new Array();
                        aIMAGES.push('.offer-img');
                        aIMAGES.push('.offer-img-mob');

