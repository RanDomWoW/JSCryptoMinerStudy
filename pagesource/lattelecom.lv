        var prev_val='';
        function acf_InitAddrAutocomplete( EL, onSET, onLOAD )
        {
            var lastgroup = '';
            EL.autocomplete
            ({
                minLength: 0,
                delay: 1000,
                source: function( request, respond ) {
                    var reqdata = { term: request.term, lastgroup: lastgroup, max_records: window.max_records };
                    reqdata[ window.session_token ] = "1";

                    $.post( 'https://www.lattelecom.lv/components/com_accesscheck/addressjson.php', reqdata,
                        function( response ) {
                            recent_addresses = response;
                            respond(response);
                        }, 'json');
                },
                search: function( event, ui )
                {
                    var val = EL.val();
                    prev_val=val;
                    if(val.length == 0) {


                    }
                    else if(val.length >=4){
                        jQuery('.ac-form .iact').removeClass('error');
                        jQuery('.error-box').text('');

                        jQuery('.loader').css("display", "inline-block");
                        jQuery('.cta').hide();

                        return !/^ *\d+ *$/.test( val ) || val.length > 7;
                    }else{
                        jQuery('.ac-form .iact').addClass('error');
                        jQuery('.error-box').text('Nekorekti ievadīta adresi. Ievadi pilno adresi.');
                        return false;
                    }

                },
                response: function( event, ui )
                {
                    if(docCookies.hasItem( 'user_address')) return;
                    onLOAD( ui.content );
                    var val = EL.val();
                    if(val.length == 0) {
                        //Add user adreses if not already added
                        if (metadata.addresses) {
                            for (var index = metadata.addresses.length - 1; index >= 0; --index) {
                                var item = {
                                    CITY: metadata.addresses[index].city,
                                    POS: "1",
                                    cityname: metadata.addresses[index].cityname,
                                    cityrank: metadata.addresses[index].cityrank,
                                    exact: "0",
                                    id: metadata.addresses[index].id,
                                    islike: "1",
                                    label: metadata.addresses[index].value,
                                    lowset_id: metadata.addresses[index].lowest_id,
                                    value: metadata.addresses[index].value
                                };
                                ui.content.push(item);
                            }
                        }
                    }
                    else if( ui.content.length == 1 )
                    {
                        var item = ui.content[ 0 ];
                        if( item.id != 0 )
                        {
                            onSET( item.id, item.value, item.lowest_id );
                            EL.val( item.value );
                            if( window.use_ac_highlights ) EL.css( 'background-color', 'lightgreen' );
                            ui.content.pop();

                            //hide loader
                            $('.loader').hide();
                            $('.cta').show();
                            jQuery('.ac-form .iact').removeClass('error');
                            return;
                        }
                    }
                    else if(ui.content.length==0 && val.length > 0){
                        if(isServiceAviable()==1) {
                            if(!(prev_val =='' && val.length > 3)){
                                jQuery('.ac-form .iact').addClass('error');
                                jQuery('.error-box').text('Nekas netika atrasts, mēģini vēlreiz');
                            };

                        }
                    }
                    else{
                        jQuery('.ac-form .iact').removeClass('error');
                    }
                    EL.css( 'background-color', 'transparent' );

                    //hide loader
                    $('.loader').hide();
                    $('.cta').show();
                },
                open: function( event ) {
                    autocomplete_open = true;
                },
                close: function( event )
                {
                    if( this.selectedAddr == 1 )
                    {
                        EL.caretTo( ',' );
                        EL.autocomplete( "search" );

                        this.selectedAddr = 0;
                    } else {
                        autocomplete_open = false;
                    }
                },
                select: function( event, ui )
                {
                    if( ui.item != null )
                    {
                        if( ui.item.id != 0 )
                            onSET( ui.item.id, ui.item.value, ui.item.lowest_id );
                        else
                            EL.val( ui.item.value );
                        if( ui.item.id == 0 )
                        {
                            this.selectedAddr = 1;
                            lastgroup = ui.item.raw;
                            ui.item.value = ui.item.value.replace( ',', ' ,' );
                        }
                        else {
                            if( window.use_ac_highlights ) EL.css( 'background-color', 'lightgreen' );
                            lastgroup = '';
                        }
                    }
                }
            })
        };

        function isServiceAviable(){
            //Return 1 if aviable, -1 if offline, 0 uknown statuss (not checked yet)
            if(window.session_token != null && typeof window.acf_avail_checked !== "undefined" )
            {
                if( window.acf_avail_checked ){
                    return 1;
                }else{
                    return -1;
                }
            }
            return 0;
        };

        function acf_SetForm( el )
        {
            var pos = acf_pos();
            var fw = $( "<div />", {'class': 'ac-formwrap'} );

            fw.append( $( "<div />", {'class': 'ac-formcontent'} ).
                append( $( "<div />", {'class': 'ac-formpro'} ) ).
                append( el.addClass( 'ac-form' ) ).
                append( $( "<div />", {'class': 'ac-formepi'} ) )
            );
            fw.append( $( "<div />", {'class': 'ppx'} ).
                append( $( "<div />", {'class': pos.children(".ppxbtn").attr('class')+' open'} ).
                html( pos.children(".ppxbtn").html() ).click( window.acf_ClickAction ) )
            );

            pos.css({position:'relative'}).append( fw );

            safePromoPauseBy( "ac-form" );

            acf_checkServiceAvail( el );

            window.acf_active = fw;
        };

        window.acf_FormCTA_micro = function( followup )
        {
            var me = $(this);
            var iact = me.parent();
            var ibox = iact.find('input');
            var deftext = 'Piemērs: Nometņu 10-5, Salaspils';
            var ttpos = me;

            acf_SetError();
            iact.find('.textbox').removeClass('error');
            if( ibox.val().length < 4 || ibox.val() == deftext )
            {
                acf_SetError( TR('ACF_MSG_MINSYM') );
                iact.find('.textbox').addClass('error');
                return;
