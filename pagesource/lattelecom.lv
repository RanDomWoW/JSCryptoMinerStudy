<script type="text/javascript">

    var metadata = {
        addresses: 0
    };

    JROOT = "https:\/\/www.lattelecom.lv\/";

    (function( $ ) {
        "use strict";


        const popup = $( '.availability-popup-wrap' );

        //open availability popup
        $( '.availability-check-wrap .srch-address' ).click(function() {
            openPopup();
            resizeSearchCont();
        });

        function openPopup() {
            acf_OpenForm(null, null, false);
