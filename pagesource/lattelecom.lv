<script type="text/javascript">
    (function($)
    {
        var module = {};
        module.root = $("#banners-responsive-module-2356");

        function getBannerSourceImage(slide){
            for(var i = 1; i < 4; i++){
                if (slide.attr("data-background_lg_"+ i +"x")) {
                    var background_value = slide.data("background_lg_"+ i +"x");
                    if(!(background_value.indexOf("/cache/thumbs/") >= 0)){
                        return slide.data("background_lg_"+ i +"x");
                    }
                }
            }
            return false;
        }

        function registerBannerAction(slide, event, bannerEventType){
            var bannerImageAddress = getBannerSourceImage(slide);
            var bannerLink = slide.find(".button-base").attr("href");
            var bannerPosition = slide.data("id") + 1;

            dataLayer.push({
                "event": event,
                "bannerEventType": bannerEventType,
                "bannerLink": bannerLink,
                "bannerPosition": bannerPosition,
                "bannerImageAddress": bannerImageAddress
            });
        }

        function setBackgroundPosition(background_pos_set){
            if(isMobilePageWidth()){
                module.root.find(".banner-item .banner-content > *").css('opacity', "1");
                var banner_height = ($(window).width() / 768) * 600;
                module.root.find(".banner-item" ).each(function() {
                    if(RetinaHelper.isRetina("3x") && $(this).data("background_sm_3x")){
                        var background = $(this).data("background_sm_3x");
                    }else if(RetinaHelper.isRetina("2x") && $(this).data("background_sm_2x")){
                        var background = $(this).data("background_sm_2x");
                    }else{
                        var background = $(this).data("background_sm_1x");
                    }
                    $(this).css('background-image', 'url(' + background + ')');
                    $(this).css('background-size', "100% "+ banner_height + "px" );
                });

                module.root.find(".banner-item").css("background-position", "top center");
                module.root.find(".slick-arrow").css("top", (banner_height / 2)  +"px");

                if($(window).width() < 368){
                    module.root.find(".banner-item").css("padding-top", banner_height +"px");
                    module.root.find(".banner-item").css("height", (260 + banner_height ) +"px");
                    background_pos_set = false;
                }
                else{
                    module.root.find(".banner-item").css("padding-top", banner_height +"px");
                    module.root.find(".banner-item").css("height", (235 + banner_height ) +"px");
                    background_pos_set = false;
                }
            }else{
                if(background_pos_set == false || typeof(background_pos_set) == "undefined") {
                    module.root.find(".banner-item" ).each(function() {
                        if(RetinaHelper.isRetina("3x") && $(this).data("background_lg_3x")){
                            var background = $(this).data("background_lg_3x");
                        }else if(RetinaHelper.isRetina("2x") && $(this).data("background_lg_2x")){
                            var background = $(this).data("background_lg_2x");
                        }else{
                            var background = $(this).data("background_lg_1x");
                        }
                        $(this).css('background-image', 'url(' + background + ')');
                        $(this).css('background-size', "cover" );
                    });
                    module.root.find(".banner-item").css("background-position", "center center");
                    module.root.find(".banner-item").css("padding-top", "0px");
                    background_pos_set = true;
                }

                var banner_width = $(window).width();
                var aspect_rate =  banner_width / base_width;
                var banner_height = base_height * aspect_rate;
                if(banner_height > base_max_height){
                    banner_height = base_max_height
                }
                if(banner_height < base_min_height){
                    banner_height = base_min_height
                }
                module.root.find(".slick-arrow").css("top", (banner_height /2) + "px");
                module.root.find(".banner-item").css("height", banner_height + "px");
            }
            return background_pos_set;
        }

        var fade_out_direction = "";
        var fade_in_direction = "";
        var base_width = 1920;
        var base_height = 670;
        var base_max_height = 670;
        var base_min_height = 420;
        var two_slide_direction;

        module.root.find('.banner-slides').on('init', function(event, slick){
            module.root.delay(10).queue(function(){
                $(this).css('opacity', 1);
                $(this).dequeue();
            });
        });

        module.root.find(".banner-slides").slick({
            dots: true,
            infinite: true,
            speed: 1350,
            cssEase: "cubic-bezier(0.8, 0, 0.3, 1)",
            slidesToShow: 1,
            adaptiveHeight: true,
            autoplay: true,
            autoplaySpeed: 5000,
            draggable: true,
            swipe: true
        });

        if(!isMobilePageWidth()) {
            module.root.find("#banner-0 .banner-content > *, .slick-dots").css('opacity', "0");

            module.root.find("#banner-0 .banner-content").children().each(function (index) {
                var element = $(this);
                setTimeout(function () {
                    element.animateCss('fadeInUp');
                    element.css('opacity', "1");
                }, (300 * index));
            });

            setTimeout(function () {
                module.root.find(".slick-dots").animateCss('fadeInUp');
                module.root.find(".slick-dots").css('opacity', "1");
            }, (300 * module.root.find("#banner-0 .banner-content").children().length));
        }

        $.fn.bindUp = function(type, fn) {
            type = type.split(/\s+/);
            this.each(function() {
                var len = type.length;
                while( len-- ) {
                    $(this).bind(type[len], fn);

                    var evt = $.data(this, 'events')[type[len]];
                    evt.splice(0, 0, evt.pop());
                }
            });
        };

        module.root.find(".slick-next").bindUp('click',function() {
            two_slide_direction = 'left';
        });
        module.root.find(".slick-prev").bindUp('click',function() {
            two_slide_direction = 'right';
        });

        module.root.find(".banner-slides").on('beforeChange', function(event, slick, currentSlide, nextSlide){
            if(isMobilePageWidth()){
                return;
            }
            var decl = currentSlide - nextSlide;
            var direction = (decl == 1 || decl == (slick.slideCount-1)*(-1)) ? "right" : "left";
            if(slick.slideCount == 2){
                direction = two_slide_direction;
            }
            fade_out_direction = direction === "left" ? "right" : "left";
            fade_in_direction = direction  === "left" ? "left" : "right";

            module.root.find("#banner-"+ currentSlide  + " .banner-content > *").addClass('slide-in-'   + fade_out_direction);
            module.root.find("#banner-"+ nextSlide     + " .banner-content > *").addClass('slide-from-' + fade_in_direction);
            if(currentSlide != nextSlide) {
                registerBannerAction(module.root.find("#banner-" + nextSlide), "onBannerInteraction", "impression");
            }
        });

        module.root.find(".banner-slides").on('afterChange', function(event, slick, currentSlide){
            module.root.find(".banner-slides .banner-content > *").removeClass("slide-in-left slide-in-right slide-from-left slide-from-right");
        });

        module.root.find(".banner-slides .button-base").click(function(event) {
            var href = this.href;
