jQuery( "ul.main-menu > li > ul" ).each(function(i,el)
{
    var $ = jQuery;
    var me = $(this);
    var a = me.parent().children("a");
    var right = a.position().left + a.width();
    me.css( "min-width", right + 30 );
});
/*******************
* mouse/menu tricks
******/

jQuery( ".notdropdown" ).each(function(i,el)
{
    var $ = jQuery;
    var me = $(this);
        me.children("ul").css( "display", "none" );
});

function microtime()
{
    var unixtime_ms = new Date().getTime();
    return unixtime_ms / 1000;
}

window.mm_timeMs = 1000;
var mm_log = function(txt){/*console.log(txt);*/};

window.mm_rX = 0;
window.mm_rY = 0;
window.mm_ppT = microtime();
window.mm_ppX = 0;
window.mm_ppY = 0;
window.mm_on = false;
window.mm_int = null;
window.mm_firstMoveDone = false;
window.mm_firstEventIgnored = false;
window.mm_current = null;
window.mm_items = null;
window.mm_items_sticky = null;
window.mm_selectedMenuID = -1;
window.mm_active = -1;
window.mm_sticky_mode = false;

var mm_getCurHover = function()
{
    var items = jQuery("#main-menu").find('.hover');
    if( items.length )
        return items[0];
    return null;
};

var mm_closeDisabled = function()
{
    var closed = false;
    jQuery( '#main-menu' ).find( '.forceopen' ).each(
        function(i,el)
        {
            if( el != window.mm_current )
            {
                jQuery(el).removeClass( 'forceopen' );
                closed = true;
            }

        }
    );
    if( closed )
        mm_log( "[M] close" );
};
var mm_changeOpen = function( to )
{
    /*window.mm_current = to;
    if( to )
        jQuery(to).addClass( 'forceopen' );
    mm_closeDisabled();*/
};

var mm_intHandler = function()
{
    mm_log( "[M] interval-handler" );
    mm_changeOpen( mm_getCurHover() );
};
var mm_intRestart = function()
{
    mult = ( Math.abs( window.mm_rX ) + Math.abs( window.mm_rY ) ) / 2000;
//    mm_log( "[M] restart" );
    if( window.mm_int )
        clearTimeout( window.mm_int );
    window.mm_int = setTimeout( mm_intHandler, window.mm_timeMs * mult );
};
var mm_intClear = function()
{
//    mm_log( "[M] clear" );
    if( window.mm_int )
        clearTimeout( window.mm_int );
    window.mm_int = null;
};
var mm_mouseMoveIntHandler = function( rx, ry )
{
    var numforceopens = jQuery( '#main-menu' ).find( '.forceopen.parent' ).length;
    if( !window.mm_on && window.mm_int && numforceopens != 0 && !( rx == 0 && ry == 0 ) )
    {
        if( ry > 0 )
            mm_intRestart();
        else if( ry < -1 )
            mm_intClear();
    }
    else
    {

        mm_changeOpen( mm_getCurHover() );
    }
};
var mm_mouseMoveHandler = function( e )
{
    if( !mm_firstEventIgnored )
    {
        window.mm_ppX = e.pageX;
        window.mm_ppY = e.pageY;
        window.mm_firstEventIgnored = true;
        return false;
    }
    if( window.mm_mstop )
        clearTimeout( window.mm_mstop );
    window.mm_mstop = setTimeout( function(){ mm_mouseMoveHandler({ pageX: window.mm_ppX, pageY: window.mm_ppY, fake: true }); }, 5 );

    var rx = e.pageX - window.mm_ppX;
    var ry = e.pageY - window.mm_ppY;
    if( rx && ry )
        window.mm_firstMoveDone = true;

    window.mm_ppX = e.pageX;
    window.mm_ppY = e.pageY;
    window.mm_aX += rx;
    window.mm_aY += ry;

    var ntm = microtime();
    if( ntm - window.mm_ppT > 1/60 )
    {
        var td = ntm - window.mm_ppT;
        window.mm_ppT = ntm;

        rx = window.mm_aX / td;
        ry = window.mm_aY / td;
        window.mm_rX = rx;
        window.mm_rY = ry;

        mm_mouseMoveIntHandler( rx, ry );

        window.mm_aX = 0;
        window.mm_aY = 0;
    }
};

var isOpen = false;
var isClosing = false;
var isOpening = false;
function slideOutMenu(elem){
        var parent = elem.parent();
        if (typeof parent !== 'undefined') {
            var child = elem.children(".dropdown-wrap").children(".container ");
            child.css("display", "block");
            child.css("position","");
            var blockHeight = 349;
            if(parent.css("padding-bottom") == "0px"){
                isOpening = true;
                parent.animate({
                    'padding-bottom' : blockHeight
                }, {duration: "slow", queue: false});
                child.animate({
                    'height' : blockHeight
                }, {duration: "slow", queue: false});
                child.promise().done(function(){isOpen = true; isOpening = false; });
            }
        }
}
function slideInMenu(elem){
    var parent = elem.parent();
    var child = elem.children(".dropdown-wrap").children(".container ");
    if (typeof parent !== 'undefined') {
        isClosing = true;
        parent.animate({
            'padding-bottom' : 0
        },{duration: "slow", queue: false});
        child.animate({
            'height' : 0
        }, {duration: "slow", queue: false});
        child.promise().done(function(){
            child.css("display", "none");
            isOpen = false;
            isClosing = false;
            child.height('');
        });
    }

    mm_underlineFindPosition('active');
}
jQuery(document).mousedown(function (e) {
        if(!jQuery(e.target).closest('.main-menu-2').length)
        {
            if(jQuery( '#sticky-menu-source').length) {
                if (jQuery('#sticky-menu').is(":visible")) {
                    var me = jQuery('#sticky-menu .main-menu-2 > li');
                } else {
                    var me = jQuery('#sticky-menu-source .main-menu-2 > li');
                }
            }else{
                var me = jQuery('.main-menu-2 > li');
            }
            me.removeClass( 'forceopen' );
            me.removeClass( 'openMenuItem' );
            slideInMenu(me);
        }
});
jQuery(document).on('click', '.change-open.button-base', function() {
    jQuery("#upper-section").children(".container").css("padding-bottom", 0);
});

function alignMenuElems(){
    if(viewport.is('xs') || viewport.is('sm')) return;
    var lHeight = 0;
    var elem = jQuery(".main-menu-2 > .openMenuItem > .dropdown-wrap > ul > li > a");
    elem.each(function(){
        if(jQuery(this).height() > lHeight) lHeight = jQuery(this).height();
    });
    elem.each(function(){
        jQuery(this).css("height", lHeight);
    });
    if(lHeight > 28){
        jQuery(".main-menu-2 > .openMenuItem > .dropdown-wrap > ul").addClass("multi-line");
    }else{
        jQuery(".main-menu-2 > .openMenuItem > .dropdown-wrap > ul").removeClass("multi-line");
    }
}
var mm_enterHandler = function(event)
{
        var me = jQuery(this).parent();
        if(me.find( ".dropdown-wrap" ).children( ".container" ).length == 0)
        {
            //If the menu has no child menu elements, close the menu.
            me.parent().css("padding-bottom", 0);
            me.removeClass( 'openMenuItem' );
            me.removeClass( 'forceopen' );
            return true;
        }
        if(me.hasClass('openMenuItem'))
        {
            me.removeClass( 'forceopen' );
            me.removeClass( 'openMenuItem' );
            slideInMenu(me);
            return false;
        }
        if(viewport.is('xs') || viewport.is('sm')){
            var open = me.parent().find(".forceopen.openMenuItem").length;
            me.parents(".main-menu-cont").find(".main-menu > li.forceopen").removeClass("forceopen");
            me.parents(".main-menu-cont").find(".main-menu > li.openMenuItem").removeClass("openMenuItem");
            me.parents(".main-menu-cont").find(".main-menu > li > .dropdown-wrap > .container").hide();
            if(open) {
                me.children( ".dropdown-wrap" ).children( ".container" ).show();
            } else {
                me.children(".dropdown-wrap").children(".container").slideOut(me.parent());
            }
            me.addClass( 'forceopen' );
            me.addClass( 'openMenuItem' );
            return false;
        }
        else {
            if(isClosing || isOpening)return false;
            event.preventDefault();
            me.parent().find("li.forceopen").removeClass("forceopen");
            me.parent().find("li.openMenuItem").removeClass("openMenuItem");
            me.parent().find("> li > .dropdown-wrap > .container").hide();
            me.parent().find("> li > .dropdown-wrap > .container").css("height", 0);
            if(!isOpen){
                slideOutMenu(me);
                me.addClass('forceopen');
                me.addClass('openMenuItem');
                alignMenuElems();
            }
            else {
                me.children(".dropdown-wrap").children(".container").css("height", 349);
                me.children(".dropdown-wrap").children(".container").show();
                me.addClass('forceopen');
                me.addClass('openMenuItem');
                alignMenuElems();
                slideOutMenu(me);
            }

            mm_underlineFindPosition('forceopen');
        }
        return false;
};

var mob_mm_toggleSubMenu = function(event)
{
    event.preventDefault();
    var dropdown_parent = jQuery(this).parent();
    var dropdown = dropdown_parent.children( '.dropdown-wrap' );
        //open sub-menu
        if (dropdown_parent.hasClass( 'closed' )) {
            dropdown_parent.removeClass( 'closed' );
            dropdown_parent.addClass( 'open' );
            dropdown.slideDown( 'slow' );
        }
        //close sub-menu
        else {
            dropdown_parent.addClass( 'closed' );
            dropdown_parent.removeClass( 'open' );
            dropdown.slideUp( 'slow' );
        }
};

var mob_toggleMainMenu = function()
{
    main_menu = jQuery(this).parent().siblings( '#mob-menu-cont' );

        //open main menu
        if (!main_menu.parent().hasClass( 'open' )) {
            main_menu.parent().addClass( 'open' );
            main_menu.show();
            jQuery( '#content-section' ).hide();
            jQuery( '#footer-section' ).hide();
            jQuery( '.afme-root' ).hide();
        }
        //close main menu
        else {
            main_menu.parent().removeClass( 'open' );
            main_menu.hide();
            jQuery( '#content-section' ).show();
            jQuery( '#footer-section' ).show();
            jQuery( '.afme-root' ).show();
            if(jQuery(".banners-responsive-module .banner-slides").length) {
                jQuery(".banners-responsive-module .banner-slides").get(0).slick.setPosition();
