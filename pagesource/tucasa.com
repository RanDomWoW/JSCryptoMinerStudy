<!-- Modal LOGGIN Favoritos -->
<div class="modal fade form-favorito col-xs-12" id="modal_favorito" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
	<div class="modal-dialog" style="max-width:450px;">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close pull-right btn_cerrar_modal br5" data-dismiss="modal">
					<span>X</span>
				</button>
				<span style="width: 36px;height: 34px;display: inline-block;background-image: url(/_img/icon-favorito-on.png);vertical-align:bottom;" class="btn-fav br10 on"></span> <h4 class="modal-title" style="padding-top:5px;display:inline-block">Guardar favorito</h4>
				<input type="hidden" name="idinmueble" value=""/>
				<input type="hidden" name="url" value=""/>
			</div>
			<div class="modal-body" style="display:inline-block">
				<div class="col-xs-12 description" style="font-weight:600;margin-bottom:20px;">
					Crea una lista con los anuncios que te gustan para tenerlos a mano 
						Escribe notas personales (sólo para ti) para acordarte de los detalles
				</div>
				<div class="col-xs-12 ">
						<div class="campos-form">
							<label>Tu email</label>
							<input type="email" class="form-control inputs br2" id="email" name="email" value="" />
							<p class="check-email"><img style="width:16px;" src="/_img/loader.gif" alt="comprobando email"/> Comprobando email</p>
							<label class="exists">Tu contraseña</label>
							<input type="password" class="exists form-control inputs br2" id="password" name="password" value="" />
							<label class="pass-fail">La contraseña que has puesto no coincide con tu email, comprueba que esté bien</label>
							<a class="exists recovery" href="javascript:void(0)">¿Has olvidado tu contraseña?</a>
							<!--<a class="register" href="javascript:void(0)">Nuevo usuario</a>-->
														
							<label class="no-exists">Por favor, comprueba que tu email está bien escrito</label>
							<span class="no-exists"style="padding: 10px 5px;text-align: center;width: 100%;background: #dedede;color: #d3222a;font-weight: 600;">email_de_prueba@gmail.com</span>
							
							<span class="register">Sólo falta que confirmes tu email</span>
							<label class="register"></label>
							
						</div>
				</div>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default btn-guardar-favorito tr05">Guardar favorito</button>
				<button type="button" class="btn btn-default btn-cerrar-favorito tr05" data-dismiss="modal">Cerrar</button>
			</div>
		</div>
	</div>
</div>
		
<script>
	$(document).ready(function() {

	
	function checkEmail(){
			$('#modal_favorito input[type=email]').css('border-color','none');
			$('#modal_favorito .no-exists').hide();
			$('#modal_favorito .pass-fail').hide();
		 
			if( $('#modal_favorito input[type=email]').val()!=''){
				$('#modal_favorito .check-email').show();
			}
		
			var _email = $('#modal_favorito input[type=email]').val();
			if(isValidEmailAddress(_email)){
				$('#modal_favorito .check-email').hide();		
				var parametros = {"email": _email};
				
				var _url="/ajax/check_user_exists.asp";

				$.ajax({
					type: 'post',
					data: parametros,
					url: _url,
					dataType: 'json',
					success: function (obj) {
						if(obj[0].existe){
							$('.exists').show();
							$('.no-exists').hide();
							$('#modal_favorito input[type=email]').css('border-color','none');
							return false;
						}else{
							$('.no-exists').show();
							$('.exists').hide();
							$('span.no-exists').css('display','inline-block');
							$('span.no-exists').html(_email);
						}
						console.log(obj);			
					},
					error: function (err) {
						console.log('error');
					}
				});	
			}else{
				$('.exists').hide();
				$('.no-exists').hide();
			}
		}
	
	
	
	function RecordarPassword(Usuario) {
		
		//var Usuario = '';
		//Usuario = $('#frmIdentificar').children('input[name=Usuario]').val();
		if (Usuario == '' || Usuario == null) Usuario = prompt('Introduce tu email / usuario', '');
		if (Usuario == '' || Usuario == null) return false;
		showLoadingList();
		$.post('/recordarpassword.asp', { usuario: Usuario }, function(data) { alert(unescape(data)); }, 'text')
		.complete(function(){hideLoadingList();});
		return false;
	}
				
		$('#modal_favorito a.recovery').click(function(){
			
			var _email = $('#modal_favorito input[type=email]').val();
				return RecordarPassword(_email);
		
		});
		
		$('#modal_favorito').on('hidden.bs.modal', function () {
			//location.reload();
			if($.cookie('id')==null){
				console.log('not logged!');
				$('.btn-fav').addClass('off').removeClass('on');
				$('.star-favorito').addClass("off").removeClass("on");
				$('.contenedor-mensaje-favorito').hide();
			}else{
				var _email = $('#modal_favorito input[type=email]').val();
				$('.pubicar_anuncio #acceso img').attr('src','/_img/icon-logado.png');
				$('.pubicar_anuncio #acceso').attr('href','#').addClass('dropdown-toggle titulo-desplegable').attr('data-toggle','dropdown').removeAttr('onclick');
				
				$('<ul class="dropdown-menu menu-login pull-right"><ul class="list-unstyled"><li style="font-weight: bold;margin: 0;font-size: 12px;">conectado como:</li><li style="margin:0;white-space:nowrap">'+_email+'</li><li class="divider" style="height:1px !important;"></li><li><a href="http://particulares.tucasa.com/">Tu Zona privada</a></li><li><a href="http://particulares.tucasa.com/particulares_favoritos.html">Tus Favoritos</a></li><li><a id="cerrar_sesion" href="/cerrarsesion/">Cerrar sesión</a></li></ul></ul>').insertAfter('.pubicar_anuncio #acceso');
				
				$('.pubicar_anuncio #acceso').removeAttr('id');											
			}
		})

		$('#modal_favorito .btn-guardar-favorito').click(function(){
		
			var _email = $('#modal_favorito input[type=email]').val();
				if(isValidEmailAddress(_email)){
					var parametros = {"email": _email};
					var _url="/ajax/loggin_user.asp";
					
					if($('#modal_favorito input[type=password]').is(":visible")){
						if($('#modal_favorito input[type=password]').val()!=''){
							//console.log('logamos y guardamos en favoritos');
							var _password = $('#modal_favorito input[type=password]').val();
							parametros = {"email": _email, "password": _password};
							
						}else{
							$('#modal_favorito input[type=password]').css('border-color','#e06469');
						}
					}else{
						//console.log('registramos y guardamos en favoritos');
						parametros = {"email": _email, "password": -1};
					}
					
				
					$.ajax({
						type: 'post',
						data: parametros,
						url: _url,
						dataType: 'json',
						success: function (obj) {
							var _idInmueble=$('#modal_favorito input[name=idinmueble]').val();
							var _url=$('#modal_favorito input[name=url]').val();
							
							if(obj.loggin){
								addFavs(_idInmueble,0,_url,'');
							}else{
								$('#modal_favorito .pass-fail').show();
								return false;
							}
							
							if(obj.loggin && obj.register){
								$('#modal_favorito .description').hide();
								$('#modal_favorito label.register').html('Hemos enviado un correo a '+_email+', no olvides revisar tu correo no deseado');
								$('#modal_favorito .exists').hide();
								$('#modal_favorito .no-exists').hide();
								$('#modal_favorito label').hide();
								$('#modal_favorito input[type=email]').hide();
								$('#modal_favorito button.btn-guardar-favorito').hide();
								$('#modal_favorito button.btn-cerrar-favorito').show();
								$('#modal_favorito span.register').css('display','block');
								$('#modal_favorito .register').show();	
								
							}else{
								//location.reload();
								$('#modal_favorito').modal('hide');
							}
						},
						error: function (err) {
							console.log('error');
						}
					});	
					
				}else{
					$('#modal_favorito input[type=email]').css('border-color','#e06469');
				}
		});
		
		$('#modal_favorito input[type=password]').keyup(function() {
			$('#modal_favorito input[type=password]').css('border-color','none');
		});
		
		$('#modal_favorito a.register').click(function() {
			$('#modal_favorito .exists').hide();
			$('#modal_favorito a.register').hide();
			
		});
		
		
		$('#modal_favorito input[type=email]').on('change keyup click',function(){
			$('#modal_favorito input[type=email]').css('border-color','none');
			$('#modal_favorito .no-exists').hide();
			$('#modal_favorito .pass-fail').hide();
			$('#modal_favorito .exists').show();
			$('#modal_favorito a.register').show();
		});
		
		$('#modal_favorito input[type=email]').blur(function(){
			checkEmail();
		});
});
