        <script type="text/javascript">
            $(function ($) {
                var slideLoop = $(".pc_lun .swiper-slide").length > 1 ? true : false;
                var myPCSwiper = new Swiper('.pc_lun', {
                    pagination: '.pagination_pc',
                    paginationClickable: true,
                    autoplayDisableOnInteraction: false,
                    autoplay: 5000,
                    speed: 1000,
                    loop: slideLoop
                });

                var myMobileSwiper = new Swiper('.lun_mobile', {
                    pagination: '.pagination_mobile',
                    paginationClickable: true,
                    autoplayDisableOnInteraction: false,
                    autoplay: 5000,
                    speed: 1000,
                    loop: slideLoop
                });

                myPCSwiper.params.onSlideChangeEnd = myMobileSwiper.params.onSlideEnd = function (swiper) {
                    var $slider = $(swiper.getSlide(swiper.activeIndex));
                    $slider.find("img.lazy").trigger("appear");
                };
                if (!slideLoop) {
                    $(".arrow-left, .arrow-right").hide();
                    $(".pagination").hide();
                }

                $('.arrow-leftpc').on('click', function (e) {
                    e.preventDefault();
                    myPCSwiper.swipePrev();

                });
                $('.arrow-rightpc').on('click', function (e) {
                    e.preventDefault();
                    myPCSwiper.swipeNext();
                });

                $('.arrow-leftm').on('click', function (e) {
                    e.preventDefault();
                    myMobileSwiper.swipePrev()
                });
                $('.arrow-rightm').on('click', function (e) {
                    e.preventDefault();
                    myMobileSwiper.swipeNext()
                });

                if ($(window).width() > 767) {
                    $(".content").height($(window).height() - $(".top").height());
                }

                $(window).resize(function () {
                    if ($(window).width() > 767) {
                        $(".content").height($(window).height() - $(".top").height());
                    } else {
                        $(".content").css("height", "auto");
                    }
                });
            });
