function save_screen(){
	var device_screen = {};
	if(navigator.userAgent.match(/iPad/i)){
		device_screen["device"]="iPad";
	} else if(navigator.userAgent.match(/iPhone/i)){
		device_screen["device"]="iPhone";
	} else if(navigator.userAgent.match(/Android/i)){
		device_screen["device"]="android";
	} else {
		device_screen["device"]="other";
	}
	device_screen["width"]=screen.width;
	device_screen["height"]=screen.height;
	device_screen["availWidth"]=screen.availWidth;
	device_screen["availHeight"]=screen.availHeight;
	device_screen["innerWidth"]=window.innerWidth; 
	device_screen["innerHeight"]=window.innerHeight; 
	device_screen["devicePixelRatio"]=window.devicePixelRatio;   
	$.get("http://www.3x.com.tw/save_screen.php?screen="+encodeURIComponent(JSON.stringify(device_screen)));

	
}
function setCookie(name, value, expires, path, domain, secure) {
        var curCookie = name + "=" + escape(value) +
                ((expires) ? "; expires=" + expires.toGMTString() : "") +
                ((path) ? "; path=" + path : "") +
                ((domain) ? "; domain=" + domain : "") +
                ((secure) ? "; secure" : "")
         
                document.cookie = curCookie 
                
}
function getCookie(name) {
        var prefix = name + "="
        var cookieStartIndex = document.cookie.indexOf(prefix)
        if (cookieStartIndex == -1)
                return null
        var cookieEndIndex = document.cookie.indexOf(";", cookieStartIndex + prefix.length)
        if (cookieEndIndex == -1)
                cookieEndIndex = document.cookie.length
        return unescape(document.cookie.substring(cookieStartIndex + prefix.length, cookieEndIndex))
}
function deleteCookie(name, path, domain) {
        if (getCookie(name)) {
                document.cookie = name + "=" + 
                ((path) ? "; path=" + path : "") +
                ((domain) ? "; domain=" + domain : "") +
                "; expires=Thu, 01-Jan-70 00:00:01 GMT"
        }
}
function fixDate(date) {
        var base = new Date(0);
        var skew = base.getTime();
        if (skew > 0) date.setTime(date.getTime() - skew);
}

