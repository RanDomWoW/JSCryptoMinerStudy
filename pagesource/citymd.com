// SITE LOCATION PAGE - 'show more...'
$(document).ready(function(e) {
  $('.additional-details-link').on('click', function() {
    var $state = $('.additional-details-state');
    if ($state.text() === 'More') {
      $state.text('Less');
      $('.additional-details').show();
      $('.additional-details-link').blur();
    } else {
      $state.text('More');
      $('.additional-details').hide();
      $('.additional-details-link').blur();
    }

    return false;
  });
}); 

// SERVICE DETAILS PAGE - 'show more...'
$(document).ready(function(e) {

  $('.service-additional-details-link').on('click', function() {
    var $this = $(this);
    var $label = $(this).find('.button-label');
    
    var isClosed = $label.text().toLowerCase().includes("more");
    
    if (isClosed) {
      $label.text($label.text().replace("more", "less").replace("MORE", "LESS").replace("More", "Less"));
      $('.service-additional-details').show();
      $this.blur();
    } else {
      $label.text($label.text().replace("less", "more").replace("LESS", "MORE").replace("More", "Less"));
      $('.service-additional-details').hide();
      $this.blur();
    }

    return false;
  });
}); 

