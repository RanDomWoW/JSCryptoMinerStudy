function implementSiteLogo(region) {
  var src='/images/logo.png';
  switch(region) {
    case 'NW':
      src='/images/logo-nw.png';
       $('.navbar-brand img').addClass('nw');
      break;

    case 'NE':
    default:
       $('.navbar-brand img').removeClass('nw');
      break;
  }
  $('.navbar-brand img').attr({'src': 'https://static.citymd.com' + src});
}

function activateLocationsCarousel() {
  // console.log("wiring locs carousel");
  $("#citymd-locations").swipe( {
    //Generic swipe handler for all directions
    swipeRight:function(event, direction, distance, duration, fingerCount, fingerData) {
      $(this).carousel('prev');
      event.stopImmediatePropagation();
    },
    swipeLeft:function(event, direction, distance, duration, fingerCount, fingerData) {
      $(this).carousel('next');
      event.stopImmediatePropagation();
    }
    //Default is 75px, set to 0 for demo so any distance triggers swipe
    //threshold:0
  });
}

