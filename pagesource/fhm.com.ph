<div class="srch-ovly" id="srch-ovly">
    <div class="srch-ovly__ctnt">
        <form class="form form__srch search" method="get" action="/search">
            <input type="text" name="search" placeholder="Enter keywords like women, cars, tech, etc..." id="srch-ovly-input">
            <button type="submit" class="form__btn gtm-navSearch" id="srch-ovly-submit" value="SEARCH">
                <svg class="icon icon--acrd pointer-none" id="srch-ovly-submit-svg">
                    <use xlink:href="#search"></use>
                </svg>
            </button>
        </form>
        <div class="hide label label--empty" search-overlay-failed>
                Sorry, no results were found for <strong search-keyword></strong>
        </div>
    </div>
</div>
<script type="text/javascript">
    $(document).ready(function(){
        $('#srch-ovly .form__srch').on('submit', function(){
            event.preventDefault();
            $(this).find('input[name=search]').blur();

            // Search item
            var searchTerm = $(this).find('input[name=search]').val();
            console.log($(this).serialize());
            $.ajax({
                url: 'https://search.summitmedia-digital.com/searchApi/8',
                type: 'POST',
                data: $(this).serialize(),
            })
            .done(function(response) {
                var resultData = typeof response === 'object' ? response : JSON.parse(response);

                if (!resultData.error) {
                    resultData.total = typeof resultData.total === 'undefined' ? 0 : resultData.total;

                    if(resultData.total > 0) {
                        window.location.href = '/search?search='+searchTerm;
                    } else {
                        $('[search-overlay-failed]').show();
                        $('[search-overlay-failed]').css('text-align','center');
                        $('[search-overlay-failed] [search-keyword]').html('"'+searchTerm+'"');
                    }
                }
            });

            return false;
        });
    });
</script>

