<script type="text/javascript">

$('#form-newsletter, #frmSubscribe').submit(function(event) {
$('.gtm-newsletterSubscribe ').prop('disabled',true);
event.preventDefault();

var thisForm = $( this );
var errorMsgClass = '.newsletter-error';
var errorMsg = $(errorMsgClass);


var newsletterUrl = "https://tracker.summitmedia-digital.com/v1/newsletter/authenticate";

var inputs = {
    'email' : thisForm.find('input[name ="email"]').val(),
    'siteId' : 8
}

 var errorMsgs = {
    "saved" : "Thank you for subscribing.",
    "exist" : "Sorry, your email address is already subscribed to this list.",
    "error" : "Are you sure you typed the correct email address? Please try again."
};

$.ajax({
    type        : 'POST', // define the type of HTTP verb we want to use (POST for our form)
    url         : newsletterUrl, // the url where we want to POST
    data        : $.param(inputs), // our data object
    dataType    : 'json', // what type of data do we expect back from the server

    }).done(function(data) {
        console.log(data);

        if(data.message == 'saved'){
            errorMsg.html(errorMsgs.saved);
            $('#newsletter_email, #newsletter-sub, .newsletter-form').addClass('hide');381
            $('.newsletter-msg').html(errorMsgs.saved);

            errorMsg.remove();
            $('.newsletter-redirect').removeClass('hide');
            $('.newsletter-msg').removeClass('wgt__msg').addClass('wgt__vlid');

        }else if(data.message == 'exist'){
            errorMsg.html(errorMsgs.exist);
            errorMsg.show();

        } else if(data.message == 'error') {
          errorMsg.html(errorMsgs.error);
          errorMsg.show();
        }else{
          $("<div class='"+errorMsgClass+"'>"+data.message+"</div>").insertBefore( thisForm );
        }

        $('.gtm-newsletterSubscribe ').prop('disabled',false);
    });
});

$(document).ready(function() {
    $(document).on('click', '.sns-item', function() {
        var
            data = $(this).data(),
            sns = data.sns,
            url = data.url,
            image = data.image,
            desc = data.desc,
            caption = ((data.caption) ? data.caption : 'FHM.COM.PH'),
            title = data.title,
            settings = 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600';

        switch (sns) {
        case 'facebook':
            var title = encodeURIComponent(title).replace(/'/g, "%27");
            var desc = encodeURIComponent(desc).replace(/'/g, "%27");

            FB.ui({
                method: 'feed',
                display: 'popup',
                name: decodeURIComponent(title),
                link: decodeURIComponent(url),
                picture: decodeURIComponent(image),
                caption: decodeURIComponent(caption),
                description: decodeURIComponent(desc)
            }, function(r) {
                console.log(r);
            });
            break;
        case 'twitter':
            window.open('http://twitter.com/intent/tweet?url='+url, 'twitterwindow', settings);
            break;
        case 'googleplus':
            window.open('https://plus.google.com/share?url='+url, 'googlepluswindow', settings);
            break;
        case 'pinterest':
            window.open('//pinterest.com/pin/create/button/?url='+url+'&media='+image+'&description='+title, 'pinterestwindow', settings);
            break;
        }

        event.preventDefault();
        return ;
    });

});

</script>

