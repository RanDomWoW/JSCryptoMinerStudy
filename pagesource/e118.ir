<script type="text/javascript">

    $(document).ready(function () {

        data = {
            province_id:0        }
        $("#city").prop("disabled", true);
        $.ajax({
            url: 'http://e118.ir/main/city/',
            type: "POST",
            data: data,
            dataType: "html",
            success: function (result)
            {
                $('#city').html(result);
                var usedNames = {};
                $("select[name='city'] > option").each(function () {
                    if (usedNames[this.text]) {
                        $(this).remove();
                    } else {
                        usedNames[this.text] = this.value;
                    }
                });
                $("#city").prop("disabled", false);
            },
            error: function (result)
            {
                $('#city').html('<div role="alert" class="alert alert-danger"><strong>Oops, something went wrong.</strong> Please try again, or get in touch if you need help.</div>');
            }
        });
    });</script>
<script>
    $("#state").change(function () {
        data = {
            province_id: $("#state").val(),
            check: "1"
        }
        $("#city option").remove();
        $("#city").prop("disabled", true);
        $.ajax({
            url: 'http://e118.ir/main/city/',
            type: "POST",
            data: data,
            dataType: "html",
            success: function (result)
            {
                $('#city').html(result);
                $("#city").prop("disabled", false);
            },
            error: function (result)
            {
                $('#city').html('<div role="alert" class="alert alert-danger"><strong>Oops, something went wrong.</strong> Please try again, or get in touch if you need help.</div>');
            }
        });
    });
</script>
