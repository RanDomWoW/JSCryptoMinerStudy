function MoveBackground() {
    var speed = 2;
    var direction = -1;
    var parallax = document.querySelectorAll(".parallax");
    [].slice.call(parallax).forEach(function (el, i) {

        var windowYOffset = window.pageYOffset;

        var header = document.getElementById("header_inner");
        var header_height = header.getBoundingClientRect().height;

        var bounding = el.getBoundingClientRect();
        var elemTop = bounding.top;

        if (ElementInViewport(el)) {
            var proportion = 100 * (elemTop -header_height) / (window.innerHeight);
            var parallaxTarget = el.querySelectorAll('.parallax-target');
            for (var i = 0; i < parallaxTarget.length; i++) {
                parallaxTarget[i].style.top = (direction * proportion * speed )  + 'px';
            }
        }
    });
}

function ElementInViewport(el) {
    var top = el.offsetTop;
    var left = el.offsetLeft;
    var width = el.offsetWidth;
    var height = el.offsetHeight;

    while (el.offsetParent) {
        el = el.offsetParent;
        top += el.offsetTop;
        left += el.offsetLeft;
    }

    return (top < (window.pageYOffset + window.innerHeight)
            && left < (window.pageXOffset + window.innerWidth)
            && (top + height) > window.pageYOffset && (left + width) > window.pageXOffset);
}

function setTeamTeaserBoxesForMobiles() {
    var teamBlocks = document.getElementsByClassName("team-block");
    var i;
    for (i = 0; i < teamBlocks.length; i++) {
