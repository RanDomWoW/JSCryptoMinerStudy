function initLocationMaps() {

	var latLangs = Array();
	var boxes = Array();

	var locationMapDivs = document.getElementsByClassName("location_map");
	var i;
	for (i = 0; i < locationMapDivs.length; i++) {

		var locationCoordinates = locationMapDivs[i]
				.getElementsByTagName("div");

		latLangs[i] = Array();
		var latlngbounds = new google.maps.LatLngBounds();

		var k;
		for (k = 0; k < locationCoordinates.length; k++) {
			latLangs[i][k] = Array();
			latLangs[i][k]['latlang'] = new google.maps.LatLng(
					locationCoordinates[k].getAttribute("data-latitude"),
					locationCoordinates[k].getAttribute("data-longitude"));
			latLangs[i][k]['box-id'] = locationCoordinates[k]
					.getAttribute("data-box-id");

			latlngbounds.extend(latLangs[i][k]['latlang']);
		}

		var gmap = new google.maps.Map(locationMapDivs[i], {
			center : latLangs[i][0]['latlang'],
			zoom : 17,
			mapTypeId : google.maps.MapTypeId.ROADMAP,
		});

		gmap.fitBounds(latlngbounds);
		if (window.innerWidth > 700 && window.innerWidth < 1000) {
			gmap.panBy(-200, 0);
		} else if (window.innerWidth >= 1000) {
			gmap.panBy(-100, 0);
		}

		var k;
		for (k = 0; k < latLangs[i].length; k++) {

			var image = new google.maps.MarkerImage(
					'/extension/bauer/design/xcel/images/map_pin2.png', null,
					new google.maps.Point(0, 0), new google.maps.Point(52, 118));

			var newMarker = new google.maps.Marker({
				position : latLangs[i][k]['latlang'],
				map : gmap,
				icon : image
			});

			google.maps.event.addListener(newMarker, 'click', function(boxId) {
				return function(event) {

					var activeBox = document.getElementById(boxId);

					var allBoxes = activeBox.parentNode
							.getElementsByClassName("box");
					var i;
					for (i = 0; i < allBoxes.length; i++) {
						allBoxes[i].classList.add('hidden');
						if (allBoxes[i].getAttribute("id") === boxId) {
							allBoxes[i].classList.remove('hidden');
						}
					}
				}
			}(latLangs[i][k]['box-id']));
		}

		var styleArray = [ {
			featureType : "all",
			stylers : [ {
				saturation : -100
			} ]
		} ];

		var styledMapOptions = {};

		var usRoadMapType = new google.maps.StyledMapType(styleArray,
				styledMapOptions);

		gmap.mapTypes.set('usroadatlas', usRoadMapType);
		gmap.setMapTypeId('usroadatlas');
	}
}

