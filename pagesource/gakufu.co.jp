<script type="text/javascript">
$(function(){
        if( $("#sort_key").val() == "recommend" || $("#sort_key").val() == "review_count" ){
            $("input[name='sort_order']").val(['desc']); // desc を選択
            $("input[name='sort_order'][value='asc']").prop('disabled',true);
            $(".sort_order_item").hide();
        }
        $("#sort_key").change(function(){
            var selected_value = $("#sort_key").val();
            if( selected_value == "recommend" || selected_value == "review_count" ){
                $("input[name='sort_order'][value='asc']").prop('disabled',true);
                $("input[name='sort_order']").val(['desc']); // desc を選択
                $(".sort_order_item").hide(500);
            }
            else {
                $(".sort_order_item").show(500, function(){
                    $(".col-md-4").tile(3);
                });
                $("input[name='sort_order']").removeAttr('disabled');
            }
        });
});
</script>
<script type="text/javascript">
$(function(){
        var rightPadding = 30;
        var timer        = false;
        var smp_width    = 384; // スマホの画面幅
        resize();
        $(".searchCate").change(resize);
        $(window).resize( function(){
            if( timer !== false ){
                clearTimeout(timer);
            }
            timer = setTimeout(function(){
                if( $(window).width() <= smp_width ){
                        $(".searchCate").css("display", "none");
                        $(".searchOptCate").css("display", "visible");
                    } else {
                        $(".searchCate").css("display", "visible");
                        $(".searchOptCate").css("display", "none");
                    }
                resize();
            }, 200);
        });

        function resize()
        {
            // 検索枠全体の幅
            var searchBox_width = $("#searchBox").get(0).offsetWidth;

            // セレクトボックス幅を調整
            var cateSelect_width = getStringWidth( $(".searchCate > option:selected").text() ) + rightPadding;
            $(".searchCate").width( cateSelect_width );

            // セレクトボックス調整後のマージン込みの幅を取得
            var cateSelect_width_with_margin = 0;
            if( $(window).width() > smp_width ){
                // スマホ以外の画面幅のときは計算する
                var cateSelect_width_with_margin = $(".searchCate").outerWidth( true );
            }

            // 検索ボタン幅を取得（Bootstrap の glyphicon-search を使うと Chrome のとき画像表示がされる前の幅で計算され、最終的にデザインが崩れる）
            //var searchBtn_width = $("#btn_mainsubmit").outerWidth( true );
            var searchBtn_width = $("#btn_mainsubmit").get(0).offsetWidth;

            // キーワードテキストボックスを調整
            var kw_width = searchBox_width - cateSelect_width_with_margin - searchBtn_width;
            $("#kw").outerWidth( kw_width );

            //alert( kw_width  + "=" +  searchBox_width + "-" +  cateSelect_width_with_margin + "-" + searchBtn_width);
        }
        function getStringWidth( str )
        {
            var elm = $("#ruler");
            var width = elm.text( str ) . outerWidth( true ); 
            elm.empty();
            return width;
        }
});
</script>
