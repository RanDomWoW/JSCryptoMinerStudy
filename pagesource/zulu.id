    function validateEmail(){
    	var email = $('#idEmail').val();

    	if(email === ""){
    		emailFiedReset();
    	}
		else if(emailValidate(email)){
    		emailFiedValid();
    		$(".with-error.email").find('label').text("");
    	}else{
    		emailFieldHasError();
    		$(".with-error.email").find('label').text("Sorry, this email is not valid");
    	}

    	var xhr = new XMLHttpRequest();
		xhr.open("GET", 'http://zulu.id/check-email' + '/' + email, false);
		xhr.send();

		if(xhr.responseText == 'used'){
			emailFieldHasError();
			$("span.register").attr('id', '');
			$(".with-error.email").find('label').text("Sorry, This email has been used");
			return false;
		}else{
			return true;
		}
