    function emailValidate(email) { 
    	var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    	return re.test(email);
    }

    function nameValidate(name) { 
    	var re = /^([ \u00c0-\u01ffa-zA-Z'\-])+$/;
    	return re.test(name);
    }
    
    function validatePassword() {
    	var p = document.getElementById('idPassword').value;
    	if(p === ""){
    		passwordFieldReset();
    		return false;
    	}else if (p.length < 6 || p.search(/[A-Z]/) < 0 || p.search(/[a-z]/) < 0 || p.search(/[0-9]/) < 0 ) {
    		passwordFieldHasError();
    		$(".with-error.password").find('label').text("Please enter a valid password");
    		return false;
    	}else{
    		passwordFieldValid();
    		$(".with-error.password").find('label').text("");
    		return true;
    	}
    }

    function confirmPassword() {
    	var pass1 = document.getElementById("idPassword").value;
    	var pass2 = document.getElementById("idConfirmPassword").value;
    	if(pass2 === ""){
    		confirmPasswordFieldReset();
    		return false;
    	}else if (pass1 != pass2) {
