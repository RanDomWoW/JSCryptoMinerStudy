<script>
function loadUnitAlbumsmart(target, prmcount) {

  var jsonUrl = "/lapap/albumsmartunit/";
  var data_no = 0;
  var prmstarLine = 0;
  var prmyears = '';
  $.ajax({
    type     : "GET",
    url      : jsonUrl,
    data     : {  },
    genre  : 'html',
    timeout  : 30000,
    cache    : true,
    beforeSend : function(XMLHttpRequest) {
    },
    success  : function(data, txtstatus, xhr){
      if (data == null) {
        $('#albumsmartunit_topinfo' ).html("");
        $('#albumsmartunit' ).html("");
        return;
      }

      if (xhr.getResponseHeader('x-fpg-erpg')) {
        /* $('#albumsmartunit_topinfo').html("読み込みに失敗しました"); */
        /* $('#albumsmartunit').html("読み込みに失敗しました"); */
        $('#albumsmartunit_topinfo' ).html("");
        $('#albumsmartunit' ).html("");
        return;
      }

      var topinfo = $(data).find('#topinfo').html();
      var topunit = $(data).find('#topunit').html();

      $('#albumsmartunit_topinfo').html(topinfo);
      $('#albumsmartunit').html(topunit);

    }, error    : function(XMLHttpRequest, textStatus, errorThrown){
      /* $('#albumsmartunit_topinfo').html("読み込みに失敗しました"); */
      /* $('#albumsmartunit').html("読み込みに失敗しました"); */
      $('#albumsmartunit_topinfo' ).html("");
      $('#albumsmartunit' ).html("");

    }, complete : function(XMLHttpRequest, textStatus){
    }
  });
}

$( document ).ready( function (){
  loadUnitAlbumsmart();
});

</script>
