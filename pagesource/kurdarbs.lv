<script language="Javascript">
function urlencode(str) {
	return encodeURIComponent(str).replace(/\+/g,'%2B').replace(/%20/g, '+').replace(/\*/g, '%2A').replace(/\//g, '%2F').replace(/@/g, '%40');
}


