    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDhu6v10hmZZQ_NDADaDEeldj_bN3Mo05Y"></script>
	<script>
	(function($) {
        $(function () {

            //массив координат
            var locations;

            //карта
            var map;

            //инициализация карты
            initMap();

            //загружаем координаты
            loadLocations();

            //загрузка координат
            function loadLocations() {
                $.ajax({
                    // url: '/mappingSystem/json.php',
                    url: '/mappingSystem/cache-json.php',
                    type: 'GET',
                    dataType: 'json'
                    //data: {param1: 'value1'}
                })
                    .done(function (resp) {
                        locations = resp;
                        loadMarkers();
                    });
            }

            //инициализация карты
            function initMap() {

                //создаем карту
                map = new google.maps.Map(document.getElementById('map'), {
                    zoom: 6,
                    center: {lat: 49.2269, lng: 31.2405},
                    scrollwheel: false,
                    mapTypeControlOptions: {
                        style: google.maps.MapTypeControlStyle.DROPDOWN_MENU
                    }
                });

            }

            //загрузка маркеров
            function loadMarkers() {
                //ифноокно
                var infoWindow = new google.maps.InfoWindow;

                //перебираем массив
                var markers = locations.map(function (location) {

                    //создаем маркер
                    var marker = new google.maps.Marker({
                        animation: google.maps.Animation.DROP,
                        //icon: 'https://lh3.googleusercontent.com/-XdUIqdMkCWA/AAAAAAAAAAI/AAAAAAAAAAA/4252rscbv5M/photo.jpg?sz=64',
                        position: {lat: parseFloat(location.lat), lng: parseFloat(location.lng)},
                        title: location.service + ', отд. №' + location.oid + ': ' + location.address
                    });

                    //инфоокно
                    var infowincontent = '<div><strong>' + location.service + ', отд №' + location.oid + '</strong><br>' + location.address + ' <a href="/node/' + location.nid + '">подробнее..</a></div>';

                    //обработка клика по маркеру
                    marker.addListener('click', function () {
                        infoWindow.setContent(infowincontent);
                        infoWindow.open(map, marker);
                    });

                    return marker;
                });

                //добавляем кластер маркеров
                var markerCluster = new MarkerClusterer(map, markers,
                    {imagePath: 'https://developers.google.com/maps/documentation/javascript/examples/markerclusterer/m'});
            }

        });
    }(jQuery));
    </script>
    <div id="map" style="width: 100%; height: 470px;margin-bottom: 25px;"></div>	

