    function initFormValidate(id, snippet, response) {

	  jQuery.validator.addMethod("api", function(value, element) {
		if(($('[name="' + element.name + '"]').hasClass('apierror'))){
		  return false;
		}
		return true;
	  }, function(params, element) {
	  	var msg = $('[name="' + element.name + '"]').attr('apierrormsg');
		if(typeof msg == undefined || msg == ''){
			return 'Ошибка заполения поля';
		}else{
			return msg;
		}
	  });
	  
	  $.validator.addMethod(
        "regexMatch",
        function(value, element, regexp) {
			value = value.split("'").join("`");
			$('#' + element.id).val(value);
            var re = new RegExp(regexp);
            return this.optional(element) || re.test(value);
        },
        "Введены недопустимые символы"
		);


		$.validator.addMethod(
				"regexNotMatch",
				function(value, element, regexp) {
					var re = new RegExp(regexp),
						bool = !re.test(value)

					return bool;
				}
		);
		
		$.validator.addMethod(
        "dateMoreThenToday",
        function(value, element) {
			var res = value.split("."),
			inputMs = Date.parse(res[2] + '-' + res[1] + '-' + res[0] + 'T00:00:00.00+03:00');
			
			return inputMs >= tomorrowMs;
			
        },
        "Введите дату, позднее чем сегодняшняя"
		);
		
		$.validator.addMethod(
        "dateLessThenTodayPlusYear",
        function(value, element) {
			var res = value.split("."),
			inputMs = Date.parse(res[2] + '-' + res[1] + '-' + res[0] + 'T00:00:00.00+03:00');
			
			return inputMs >= tomorrowMs &&  inputMs <= tomorrowMs + 365*24*60*60*1000 ;
        },
        "Введите дату, не позднее чем через год"
		);
		
		$.validator.addMethod(
        "dob",
        function(value, element) {
			var res = value.split("."),
			inputMs = Date.parse(res[2] + '-' + res[1] + '-' + res[0] + 'T00:00:00.00+03:00'),
			start20thCenturyMs = Date.parse('1900-01-01T00:00:00.00+03:00');
			
			return inputMs > start20thCenturyMs &&  inputMs < tomorrowMs - 24*60*60*1000 ;
			
        },
        "Некорректная дата рождения"
		);

		$.validator.addMethod(
        "ageLimit",
        function(value, element) {
			var res = value.split(".");
			var inputMs = Date.parse(res[2] + '-' + res[1] + '-' + res[0] + 'T00:00:00.00+03:00');
			var start20thCenturyMs = Date.parse('1900-01-01T00:00:00.00+03:00');
			var old18 = new Date();
			var old85 = new Date();
		  	old18.setFullYear(old18.getFullYear() - 18);
			old85.setFullYear(old85.getFullYear() - 85);
			$(element).addClass('long-error-input');

		  	return inputMs > start20thCenturyMs &&  +old85 < inputMs && inputMs < +old18 ;
        },
        "Страхователем не является человек младше 18 и старше 85 лет. Для того, чтобы оформить полис, звоните 3579"
		);

		$.validator.addMethod(
        "dateRangeFromYearAgoToTwoYearsLater",
        function(value, element) {
			var res = value.split("."),
			inputMs = Date.parse(res[2] + '-' + res[1] + '-' + res[0] + 'T00:00:00.00+03:00');
			
			return inputMs > tomorrowMs - 367*24*60*60*1000 && inputMs < tomorrowMs + 729*24*60*60*1000;
			
        },
        "Некорректная дата окончания ОСАГО"
		);

		jQuery.validator.addMethod("valueNotEquals", function (value, element, params) {
			return (value != params[0]);
		}, "Value must not equal arg.");

		
		

