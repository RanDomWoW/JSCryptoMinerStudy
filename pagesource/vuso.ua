$('#offices').on('change', function(){

	if($('[name="ReceiveType"]:checked').is('#post'))return false;

    var form_data = new FormData();
    form_data.append('config', 'representations');
    form_data.append('id', $(this).val());
    form_data.append('lang', js_lang);
    $.ajax({
        url: js_url + 'assets/components/vuso/ajax/snippet.php',
        type: "post",
        cache: false,
        contentType: false,
        processData: false,
        data: form_data,
        beforeSend: function(){
            $('#offices-pick label, #offices-pick input, #no_offices').remove();
        },
        success: function (data) {
            data = JSON.parse(data);
            if(data.length == 0){
				$('[name="officeDepartment"]').val(99999);
                $('#offices-pick .form-group__title').css({
                    'visibility': 'hidden',
                    'height' : '25px'
            	});
                if(js_lang == 'ru'){
                    $('#offices-pick').append('<p id="no_offices">К сожалению, в выбранном городе нету отделений</p>');
                } else if (js_lang == 'ua') {
                    $('#offices-pick').append('<p id="no_offices">Нажаль, у выбранному місті нема відділень</p>');

                }
            } else {

                $('#offices-pick .form-group__title').css({
                    'visibility': 'visible',
                    'height' : 'auto'
            });

                var ind = 1, checked;
                $.each(data, function (index, value) {
                    if(ind == 1){
						checked = 'checked';
						$('[name="officeDepartment"]').val(index);
					}else {checked=''}
                    var html = '';
                    html += '<input name="officeDepartmentPick" value="'+index+'" id="office-pick'+ind+'" type="radio" '+checked+'>';
                    html += '<label for="office-pick'+ind+'">'+value+'</label>';

                    $('#offices-pick').append(html);
                    ind++;
                })
            }
            $('#offices-pick').fadeIn();
			$('[name="officeDepartmentPick"]').change(function(){
				$('[name="officeDepartment"]').val($(this).val());
			})

        }
    });

});

