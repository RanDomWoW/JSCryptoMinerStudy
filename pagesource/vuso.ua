<script> //YK
var first_time=true;
function close_popup() {
    $(".popup-background").fadeOut(400);
}
function close_popup_thx() {
    $(".popup-background1").fadeOut(400);
}
function open_popup() {
    $(".popup-background").fadeIn(400);
    var win_w = $(window).width();
    var win_h = $(window).height();
    var wid = ((100 - 457/win_w * 100)/2).toFixed();
    var heigh = ((100 - 404/win_h * 100)/2).toFixed();
	if(typeof $(".popup-out")[0] != "undefined") {
	  $(".popup-out")[0].style.top = heigh+"%";
	  $(".popup-out")[0].style.left = wid+"%";
	}

}
function open_popup_thx() {
    $(".popup-background1").fadeIn(400);
    var win_w = $(window).width();
    var win_h = $(window).height();
    var wid = ((100 - 457/win_w * 100)/2).toFixed();
    var heigh = ((100 - 404/win_h * 100)/2).toFixed();
	if(typeof $(".popup-out1")[0] != "undefined") {
	  $(".popup-out1")[0].style.top = heigh+"%";
	  $(".popup-out1")[0].style.left = wid+"%";
	}

}
$(document).click(function (event) {
    if ($(event.target).closest('.popup-out').length == 0 && $(event.target).attr('class') == 'popup-background') {
        close_popup();
    }
});
$(document).click(function (event) {
    if ($(event.target).closest('.popup-out1').length == 0 && $(event.target).attr('class') == 'popup-background1') {
        close_popup_thx();
    }
});
$('body').mouseleave(function(){
    if (first_time==true) {
        open_popup();
    }
    first_time=false;
});

</script>


