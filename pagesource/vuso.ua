            submitHandler: function (form) {

				var form_data = new FormData();

				//files
				var file_data ='';
				var file_counter = 0; 
				$('input[type="file"]').each(function(){
					file_data = $(this).prop('files')[0];
					if(file_data){
						file_counter++;
						form_data.append($(this).attr('name'), file_data);
					}
				});
				console.log(file_counter);

				if(file_counter > 0){
					if(id == '#osago-save' || id == '#zcard-save'){
						  snippet = 'forms_handler';
						  response = 'ok_product_osago_mob';

						  $('#osago-save input[type="radio"]:checked, #zcard-save input[type="radio"]:checked').each(function () {
							  $(this).val($(this).next('label').text());
						  });
						  $('#osago-save select, #zcard-save select').each(function () {
							  $(this).find('option:selected').val($(this).find('option:selected').text());
						  });

					}
				}
				//files

				console.log(response);

				var params   = $(form).serializeArray();
			  
				$.each(params, function(i, val) {
				  form_data.append(val.name, val.value);
				});

				form_data.append('config', snippet);
				form_data.append('context', js_context);



				$.ajax({
				  url: js_url + 'assets/components/vuso/ajax/snippet.php',
				  type: "post",
				  cache: false,
				  contentType: false,
				  processData: false,
				  data: form_data,
				  beforeSend: function () {
					$('#form_preloader').show();
					if($('.button.pay').length > 0) {
						$('.button.pay').attr('disabled', 'disabled')
					}
					if($('.activate-button__container .button').length > 0){
						$('.activate-button__container .button').attr('disabled', 'disabled');
					}
				  },
				  success: function (data) {

				  	if(snippet == 'forms_handler'){
						$(form).find("input[type=text], textarea").val("");
					}
					$('#form_preloader').hide();

					if($('.button.pay').length > 0) {
						$('.button.pay').removeAttr('disabled')
					}
					if($('.activate-button__container .button').length > 0){
						$('.activate-button__container .button').removeAttr('disabled');
					}

					if(data == 'ip_error'){
					  ip_not_verified();
					}else{
					  //$(form).find("input[type=text], textarea").val("");
					  removeApierrors();
					  window[response](data);
					}

				  }
				});

            }
