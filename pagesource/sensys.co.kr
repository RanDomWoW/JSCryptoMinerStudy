	//검색페이지 이동 - 제품명 검색
	function linkTOurl2() {
		var URL = "/product/product_search.aspx";
		var txt_productName = document.getElementById('txt_productName').value;
		if (txt_productName != "") {
			//특수문자 입력제한			
			var chars = "`'~!@#$%^&*(),:;";
			for (var inx = 0; inx < txt_productName.length; inx++) {
				if (chars.indexOf(txt_productName.charAt(inx)) != -1) {
					alert("특수문자[ " + txt_productName.charAt(inx) + " ]가 포함되어 있습니다.");
					document.getElementById('txt_productName').focus();
					return false;
				}
			}
			URL += "?ProductName=" + txt_productName;
		}
		else {
			alert("제품명을 입력해주세요.");
			document.getElementById('txt_productName').focus();
			return false;
		}
		document.getElementById('_hidURL2').value = URL;
	}
	
