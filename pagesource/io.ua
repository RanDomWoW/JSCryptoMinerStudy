function doCheckReg(){
	nick=document.forms['f_reg'].inick;
	pass=document.forms['f_reg'].ipass1;
	email=document.forms['f_reg'].iemail;

	nickPat = /^(\w|-){3,20}$/i
	passPat = /^(\w|-){6,20}$/i
	emailPat = /^((\w|_|-)+\.)*(\w|_|-|\*)+@(((\w|_|-)+\.)*(\w|_|-)+\.(com|net|org|edu|int|mil|gov|arpa|biz|aero|name|coop|info|pro|museum|[a-z]{2})|(#\d{0,32})|(\[(\d{1,3}\.){3}\d{1,3}\]))$/i;
	wwwPat = /^www\./i;
	
	if (!nick.value.match(nickPat)){alert ('Вы выбрали недопустимое Имя сайта.\nИмя может состоять из цифр, латинских букв и тире длиной от 3 до 20 символов\nОкончание домена io.ua набирать не нужно'); nick.focus(); nick.select(); return false;}
	if (!pass.value.match(passPat)){alert ('Вы выбрали недопустимый Пароль.\nПароль может состоять из цифр, латинских букв и тире длиной от 6 до 20 символов'); pass.focus(); pass.select(); return false;}
	if (!email.value.match(emailPat) || email.value.match(wwwPat)){alert ('Вы указали нереальный E-mail'); email.focus(); email.select(); return false;}
	return true;
}
function doCheckEdit(){
	nick=document.forms['f_edit'].inick;
	pass=document.forms['f_edit'].ipass1;

	//nickPat = /^[a-zA-Zа-яА-ЯёЁ-]{2,20}$/i
	passPat = /^(\w|_|-){1,20}$/i
	
	//if (!nick.value.match(nickPat)){alert ('Вы ввели нереальное Имя сайта.\nИмя может состоять из цифр, латинских букв, тире и подчеркивания длиной от 2 до 20 символов\nОкончание домена io.ua набирать не нужно'); nick.focus(); nick.select(); return false;}
		if (!pass.value.match(passPat)){alert ('Вы ввели нереальный Пароль.\nПароль может состоять из цифр, латинских букв, тире и подчеркивания'); pass.focus(); pass.select(); return false;}
		return true;
}
function doCheckSearch(){
