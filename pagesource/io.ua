<SCRIPT>
function getBounds(element){
	var left	=  element.offsetLeft;
	var top		=  element.offsetTop;
	for (var parent = element.offsetParent; parent; parent = parent.offsetParent){
		left	+= parent.offsetLeft;
		top		+= parent.offsetTop;
	}
	return {left: left, top: top, width: element.offsetWidth, height: element.offsetHeight};
}

function doIoPanel(panel){
	obj_menu=document.getElementById("io_"+panel+"_menu");
	obj_div=document.getElementById("io_"+panel+"_div");
	bounds_menu = getBounds(obj_menu);
	bounds_div = getBounds(obj_div);
	with(obj_div.style){
		left=bounds_menu.left-bounds_menu.width/2-100;
		top=bounds_menu.top+bounds_menu.height+10;
		if(visibility=="visible") visibility = "hidden";
		else					  visibility = "visible";
	}
	if(panel=='reg'){
		document.getElementById("io_edit_div").style.visibility = "hidden";
		document.getElementById("io_search_div").style.visibility = "hidden";
	}else if(panel=='edit'){
		document.getElementById("io_reg_div").style.visibility = "hidden";
		document.getElementById("io_search_div").style.visibility = "hidden";
	}else if(panel=='search'){
		document.getElementById("io_reg_div").style.visibility = "hidden";
		document.getElementById("io_edit_div").style.visibility = "hidden";
	}
}
</SCRIPT>
