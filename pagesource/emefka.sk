                <script id="image-credits-transform">
            jQuery('.wp-caption .wp-caption-text').each(function () {
                    var el = jQuery(this);
                var text = el.text().trim();
                var html = text;

                var expression = /^https?:\/\/(www\.)?([-a-zA-Z0-9@:%._\+~#=]{2,256}\.[a-z]{2,6})\b([-a-zA-Z0-9@:%_\+.~#?&//=]*)$/;
                var regex = new RegExp(expression);
                var matches = text.match(regex);

                if(matches) {
                    // text is link
                    var title = matches[2];

                    html = '<a href="' + text + '" target="_blank">' + title + '</a>';
                } else {
                    // keep text and insert link if in parentheses
                    if(text.indexOf('(') >= 0) {
                        var urlStart = text.indexOf('(');
                        var urlEnd = text.indexOf(')');
                        var url = text.substring(urlStart+1, urlEnd);

                        html = '<a href="' + url + '" target="_blank">' + text.substr(0, urlStart-1) + '</a>';
                    }
                }

                el.html(html);
            });
        </script>
