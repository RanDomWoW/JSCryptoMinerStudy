	<script type="text/javascript">
		var map = '';
		var gmap_item_id = '';
		function initialize123(id) {
			gmap_item_id = id;
			load_google_map_js("load_google_map_js_extra", "&libraries=drawing");
		}
		function load_google_map_js_extra() {
			$.getScript("http://www.shoppingcenters.ir/_/js/googlemap/infobox.min.js", function () {
			});
			$.getScript("http://www.shoppingcenters.ir/_/js/googlemap/markerclusterer.min.js", function () {
				initialize_final(gmap_item_id);
			});
		}
		function initialize_final(id) {
			$(".map_ajax_loader").slideUp(200);

			if (map != '')
				return set_map_markers(id);

			map = new google.maps.Map(document.getElementById('map'), {
				mapTypeId: google.maps.MapTypeId.ROADMAP,
				maxZoom: 19
			});

			set_map_markers(id);
			return;
		}

		var property_mapajax_obj = '';
		var markerCluster = '';
		var markers = [];
		var infobox = [];
		var last_cur_url;
		var pr_data = [];
		function set_map_markers(id) {
			markerBounds = new google.maps.LatLngBounds();
			if (last_cur_url == cur_url) {
				if (typeof id != "undefined") {
					$(pr_data).each(function (ind, elm) {
						if (elm.id == id) {
							new google.maps.event.trigger(markers[ind], 'click');
							map.setCenter(markers[ind].getPosition());
							map.setZoom(15);
						}
					});
				}
				return;
			}
			$(".map_loading").slideDown(200);

			if (property_mapajax_obj != '')
				property_mapajax_obj.abort();
			action = cur_url;
			last_cur_url = cur_url;
			markers = [];
			infobox = [];
			property_mapajax_obj = $.ajax(
				{
					type: "GET",
					url: action,
					data: "ajax=1&json=1",
					dataType: "JSON",
					success: function (data) {
						$(".map_desc span.total-counts").html("total_count:" + data.length);
						pr_data = data;
						$(data).each(function (ind, elm) {
							randomPoint = new google.maps.LatLng(elm.location_lat, elm.location_lon);
							var icon = 'http://www.shoppingcenters.ir/css/_shoppingcenters/img/gmap-blue.png';
							var iconh = 'http://www.shoppingcenters.ir/css/_shoppingcenters/img/gmap-orange.png';
							markers[ind] = new google.maps.Marker({
								position: randomPoint,
								"icon": icon
							});
							google.maps.event.addListener(markers[ind], 'mouseover', function () {
								markers[ind].setIcon(iconh);
							});
							google.maps.event.addListener(markers[ind], 'mouseout', function () {
								markers[ind].setIcon(icon);
							});

							google.maps.event.addListener(markers[ind], 'click', function () {
								if (typeof infobox[ind] != "undefined") {
									if (infobox[ind].getVisible()) {
										infobox[ind].close();
										return;
									}
								}

								if (typeof elm.address_text == "undefined") {
									nc = "<div class='loading'></div>";
									var do_ajax = true;
								}
								else {
									nc = $("#gmap_bubble_jsrender").render(elm);
									var do_ajax = false;
								}
								infobox[ind] = new InfoBox({
									content: nc,
									disableAutoPan: false,
									maxWidth: 240,
									maxheight: 300,
									pixelOffset: new google.maps.Size(-118, -350),
									zIndex: 10000,
									boxStyle: {
										opacity: 1
										//width: "360px"
										// height: "250px"
									},
									closeBoxMargin: "0",
									closeBoxURL: 'http://www.shoppingcenters.ir/css/_shoppingcenters/img/close.png',
									infoBoxClearance: new google.maps.Size(1, 1)
								});
								infobox[ind].open(map, markers[ind]);

								if (do_ajax) {
									$.ajax(
										{
											type: "GET",
											url: "http://www.shoppingcenters.ir/centers/view/detail/" + elm.id + "/1",
											dataType: "JSON",
											success: function (data) {
												elm = data.details;
												nc = $("#gmap_bubble_jsrender").render(elm);
												infobox[ind].setContent(nc);

											}
										});
								}
							});

							if (id == elm.id) {
								new google.maps.event.trigger(markers[ind], 'click');
								map.setZoom(15);
							}

							markerBounds.extend(randomPoint);

						});

						// Clustershow
