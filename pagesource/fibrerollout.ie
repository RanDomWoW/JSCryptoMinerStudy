									function searchTown() {
				var county = document.getElementById('addressCounty').value;
  				while (document.getElementById('addressDistrict').options.length > 1) {
  					document.getElementById('addressDistrict').options.remove(document.getElementById('addressDistrict').options.length - 1);
  				};
  				if (county != '' && county != undefined) {
					var xmlHttp = new XMLHttpRequest();
				    xmlHttp.onreadystatechange = function() { 
				        if (xmlHttp.readyState == 4) {
				    		//document.getElementById('results').innerHTML = '';
				            if (xmlHttp.status != 403) {
