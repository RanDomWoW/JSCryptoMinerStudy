						  			if(value instanceof Array) {
							    		value.forEach(function (item, index) {
		 									document.getElementById('addressDistrict').options[document.getElementById('addressDistrict').options.length] = new Option(formatString(item), item);
							    		});
							  		}
								}
								catch (error) {
							    	//document.getElementById('results').innerHTML = '<p>' + xmlHttp.responseText + '</p>';
							    	console.log('error: ' + error);
							    	console.log(xmlHttp.responseText);
								}
						  	}
						}
				    }
				    //xmlHttp.open("GET", 'http://www.openeir.ie:3000/?apiservice=town&county=' + county, true); // true for asynchronous 
				    xmlHttp.open("GET", 'http://fibrelookup.openeir.ie/OpeneirLookupWebService.svc/broadband/get-town/c65a41b44fbfcba300b7f31a18b790d3/' + county, true); // true for asynchronous 
				    //document.getElementById('results').innerHTML = '<p>Searching</p>';
				    xmlHttp.send(null);
				}
			}

			function searchAddress() {
				var county = document.getElementById('addressCounty').value,
					district = document.getElementById('addressDistrict').value,
					address = document.getElementById('addressPartial').value,
					eircode = document.getElementById('prequalAddressEircode').value,
					patt = /^([a-zA-Z0-9]{3})(\s?)([a-zA-Z0-9]{4})$/g,
					alertMessage = '';
				if (county == '' || county == undefined)
					alertMessage = '<p>County is required.</p>';
				else if (address == '' || address == undefined)
					alertMessage = '<p>Address is required.</p>';
				else if (eircode == '' || eircode == undefined)
					alertMessage = '<p>Eircode is required.</p>';
				else if (!patt.test(eircode))
					alertMessage = '<p>Eircode is invalid. Eircode should be a 7 digits alpha numeric code.</p>';
				
				if (alertMessage == '') {
					document.getElementById('api').value = 'address';
					document.getElementById("checkFibreForm").submit();
				}
				else {
					var alertBox = document.getElementById('alert');
					alertBox.innerHTML = alertMessage;
					alertBox.style.display = 'block';
					setTimeout(function() {
						var alertBox = document.getElementById('alert');
						alertBox.innerHTML = '';
						alertBox.style.display = 'none';
					}, 5000);
				}
			}


			function searchPhone() {
				var areaCode = document.getElementById('prequalAreaCode').value,
					phone = document.getElementById('prequalPhoneNumber').value,
					eircode = document.getElementById('prequalPhoneEircode').value,
					pattPhone = /^([0-9]{5,7})$/g,
					pattEircode = /^([a-zA-Z0-9]{3})(\s?)([a-zA-Z0-9]{4})$/g,
					alertMessage = '';
				if (areaCode == '' || areaCode == undefined)
					alertMessage = '<p>Area Code is required.</p>';
				else if (phone == '' || phone == undefined)
					alertMessage = '<p>Phone is required.</p>';
				else if (!pattPhone.test(phone))
					alertMessage = '<p>Phone is invalid. Phone should have between 5 and 7 numeric digits.</p>';
				else if (eircode == '' || eircode == undefined)
					alertMessage = '<p>Eircode is required.</p>';
				else if (!pattEircode.test(eircode))
					alertMessage = '<p>Eircode is invalid. Eircode should be a 7 digits alpha numeric code.</p>';
				
				if (alertMessage == '') {
					document.getElementById('api').value = 'phone';
					document.getElementById("checkFibreForm").submit();
				}
				else {
					var alertBox = document.getElementById('alert');
					alertBox.innerHTML = alertMessage;
					alertBox.style.display = 'block';
					setTimeout(function() {
						var alertBox = document.getElementById('alert');
						alertBox.innerHTML = '';
						alertBox.style.display = 'none';
					}, 5000);
				}
			}
			
			function searchEircode() {
				var eircode = document.getElementById('prequalEircode').value,
					patt = /^([a-zA-Z0-9]{3})(\s?)([a-zA-Z0-9]{4})$/g,
					alertMessage = '';
				if (eircode == '' || eircode == undefined)
					alertMessage = '<p>Eircode is required.</p>';
				else if (!patt.test(eircode))
					alertMessage = '<p>Eircode is invalid. Eircode should be a 7 digits alpha numeric code.</p>';
				
				if (alertMessage == '') {
					document.getElementById('api').value = 'eircode';
					document.getElementById("checkFibreForm").submit();
				}
				else {
					var alertBox = document.getElementById('alert');
					alertBox.innerHTML = alertMessage;
					alertBox.style.display = 'block';
					setTimeout(function() {
						var alertBox = document.getElementById('alert');
						alertBox.innerHTML = '';
						alertBox.style.display = 'none';
					}, 5000);
				}
			}

			function formatString(str){
				var strFormatted = [];
				str.split(' ').forEach(function (item, index) {
					strFormatted.push(item.charAt(0).toUpperCase() + item.slice(1).toLowerCase());
				});
				return strFormatted.join(' ');
			}

			function httpGetAsync(theUrl, displayInfo, callback)
			{
				var xmlHttp = new XMLHttpRequest();
			    xmlHttp.onreadystatechange = function() { 
			        if (xmlHttp.readyState == 4)
			            callback(displayInfo, xmlHttp.status, xmlHttp.responseText);
			    }
			    xmlHttp.open("GET", theUrl, true); // true for asynchronous 
			    //document.getElementById('results').innerHTML = '<p>Searching</p>';
			    xmlHttp.send(null);
			}

