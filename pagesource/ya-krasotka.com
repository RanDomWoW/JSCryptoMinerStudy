                </figure>
            </div>
            <div class="b-modal__bg ng-cloak" ng-click="hideBlockPopup();"></div>
        </div>
    </script>

    <div ng-include src="'/popup.html'"></div>

    <script type="text/javascript">
        window.tz_detect_options = {
            offset: '',
            url: '/tz-detect/set/'
        };
    </script>
</body>
