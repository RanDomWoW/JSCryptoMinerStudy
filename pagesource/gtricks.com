//header-animate
jQuery(document).ready(function(){
	jQuery(window).scroll(function(){
		var barra = jQuery(window).scrollTop();
		var pocision = (barra * .6);
		var altura = jQuery('.hero-wrapper').offset().top - 64;

		jQuery('.hero-main').css({
			'top': "-" + pocision + "px"
		});
    
    //Cambiamos la opcidad de el div 
		jQuery('.hero-main .hero-blue').css({
			"opacity": pocision / 150
		});
    
    //Agregamos la class head_2 dinamicamente
		if (jQuery(window).scrollTop() > altura){
			jQuery(".hero").addClass('hero-head_2');
			
		} else {
			jQuery(".hero").removeClass('hero-head_2');
		}
	})
});

