    jQuery(function () {
        jQuery('#checkDomain').on('click',function (e) {

            var name = jQuery('#domainName').val();
            var tld =jQuery('#TLDName option:selected').val();
            jQuery( "#domainContent" ).html("لطفا کمی صبر کنید ...");
            var request = jQuery.ajax({
                url: "/wp-admin/admin-ajax.php",
                method: "POST",
                data: {
                    action : 'domain_ajax',
                    domain : name,
                    TLD : tld
                },
                dataType: "html"
            });

            request.done(function( msg ) {
                jQuery( "#domainContent" ).html( msg );
            });

            request.fail(function( jqXHR, textStatus ) {
                alert( "Request failed: " + textStatus );
            });
        });
    });







