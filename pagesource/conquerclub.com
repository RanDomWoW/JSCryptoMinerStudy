			<script>
				function cc_loader(header,messages,changeperiod){
					window.scrollTo(0,0);
					$('.cc_loader').addClass('cc_loader_reveal');
 					if (header) $('#cc_loader_header').html(header);
					if (messages) {

						if( typeof messages != 'string' || messages=='game'){
							cc_loader_randommessage(messages,changeperiod);
							return;
						}
						else $('#cc_loader_message').html(messages);
					}
				}

				function cc_loader_randommessage(messages,changeperiod){
				//console.log(messages,changeperiod);
					if( typeof messages == 'string' ) {
						messages = cc_loader_getmessages(messages);
					}
					if( !messages) return;
					if( !changeperiod) return;

					r = Math.floor(Math.random() * messages.length);
					message = messages[r];
					$('#cc_loader_message').html(message);
					messagesarray = messages.join('","');
					setTimeout('cc_loader_randommessage(["'+messagesarray+'"],'+changeperiod+')',changeperiod);

				}

				function cc_loader_getmessages(type){
					var messages;
					if (type=='game'){
					messages = ['Adjust Your Pants, we are Gamewards Bound','All for the love of murderous empire building','Gameplay Supports Excessive Analysis',' is our featured player!','May The Conquer Gods Rain Glory Upon Your Soul','Welcome to the Strategy Gaming Wonderland','High Definition Strategy Gaming Nirvana','Conquer Club Is The Thinking Human\'s Game Club','For those that like to think before crushing mercilessly','Bring On The Game','This Is Your Club, This Is Your Game',' joins the fight to become Conqueror','Think Before You Destroy','For Those Who Like Their Clubs Awesome','One Club, Thousands Of Warriors, One Conqueror','One day they all will chant the name... ','It\'s not about the fancy graphics, It\'s about the fancy gameplay','Unlimited Options Await','Gameplay Supports Massive Risk','Gameplay Supports Cunning Strategy','Gameplay Supports Treacherous Diplomacy'];
					}
					return messages;
				}
			</script>
