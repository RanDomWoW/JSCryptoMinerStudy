/*
* 04. Header
* ================================================================= */
header.header {
background-color: transparent;
display: block;
position: relative;
}
/* Top Bar */
.topbar {
background-color: #ffffff;
display: block;
height: 32px;
line-height: 32px;
margin-bottom: 30px;
clear: both;
border-bottom: 1px solid #eeeeee;
}
.topbar .container {
