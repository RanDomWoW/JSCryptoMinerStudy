}
/* Post Body */
.post-body {
display: block;
position: relative;
font-size: 16px;
font-weight: 300;
letter-spacing: .02em;
clear: both;
border-bottom: 1px dotted #eee;
padding-bottom: 30px;
margin-bottom: 10px;
}
.post-body div {
