}
/* Top Menu */
.topmenu {
display: block;
float: left;
}
.topmenu ul {
margin: 0;
padding: 0;
}
.topmenu ul li {
float: left;
display: flex;
}
.topmenu ul li a {
color: #121212;
display: block;
float: left;
font-size: 12px;
}
.topmenu ul li::after {
content: "";
display: block;
width: 1px;
height: 12px;
background: #eeeeee;
float: left;
margin: auto 12px;
}
.topmenu ul li:last-child::after {
display: none;
}
/* Social Links */
.social-links {
float: right;
}
.social-links ul {
margin: 0;
padding: 0;
}
.social-links ul li {
display: block;
float: left;
}
.social-links ul li a {
