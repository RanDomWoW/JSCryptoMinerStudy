text-align: center;
margin: 30px 0 !important;
}
blockquote:before {
content: "\f10d";
font-size: 14px;
margin-right: 6px;
color: #ccc;
}
blockquote:after {
content: "\f10e";
font-size: 14px;
margin-left: 6px;
color: #ccc;
}
