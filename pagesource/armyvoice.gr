.btn.blue {
background: #3C58E4;
}
.btn.orange {
background: #FF8F2F;
}
.btn.green {
background: #8EC63F;
}
.btn.violet {
background: #A956C7;
}
.btn.pink {
background: #F34665;
}
.btn.aqua {
background: #3CBDE4;
}
.btn.carrot {
background: #E43C3C;
}
.btn.light {
background: #eee;
color: #111;
}
.btn:hover {
