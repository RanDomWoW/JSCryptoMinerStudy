    function playvideo(sFile, iWidth, iHeight) {
        pv2_wd = iWidth;
        pv2_ht = iHeight;
        jQuery('#v-vidscontent').width(iWidth).height(iHeight);
        pv2_FileName = sFile;
        if (jwplayer()!=undefined) jwplayer().remove();
        var myPlayer = jwplayer('v-vidscontent').setup({
            file: pv2_FileName,
            repeat: true, autostart: false,
            'controlbar.position': 'over',
            height: pv2_ht,
            width: pv2_wd,
            modes: [
                {type: "flash", src: "/out/global/src/player.swf"},
                {type: "html5", config: {file: pv2_FileName}}
            ]
        });
        jQuery("a#v-videohidden").fancybox({
            transitionIn: 'fade',
            transitionOut: 'fade',
            autoDimensions: false,
            height: pv2_ht,
            width: pv2_wd
        });
        $("a#v-videohidden").trigger('click');
        jwplayer('v-vidscontent').play();
        myPlayer.play();
        return false;
    }
