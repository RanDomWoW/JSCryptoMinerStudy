<script>
//  start detect browser language setting - needs to go in header and not in doc ready
var pathNameHere = window.location.pathname;
var browserLang = ('').split('-')[0];
browserLang = (browserLang).split(',')[0];

var referPage = '';
//  end detect browser language setting

//  start detection of referral page to make sure English can be loaded for those with non eglish language preferrence
if ((referPage != "https://blackhawknetwork.com/chinese/") && (referPage != "https://blackhawknetwork.com/german/") && (referPage != "https://blackhawknetwork.com/japanese/") && (referPage != "https://blackhawknetwork.com/korean/") && (referPage != "https://blackhawknetwork.com/spanish/")) {
    jQuery("masthead").ready(function(){ 
  
  // based on setting browser to desired language to test
  // Chinese Simplified = "zh" or "zh-CN" (not zh_CN) for IE
  // English = "en-US"
  // German = "de" or "de-DE" for IE
  // Japanese = "ja" or "ja-JP" for IE
  // Korean = "ko" or "ko-KR" for IE
  // Spanish = "es" or "es-ES" for IE

  
    if(( browserLang == "zh") && (pathNameHere == "/") ){
      jQuery(".main-content").css("display","none");
      if( window.location.pathname == "/chinese/" ){
          jQuery(".main-content").css("display","block");
      } else {
          window.location = "/chinese/";
      }
    }
   
    if(( browserLang == "de" ) && (pathNameHere == "/")){
      jQuery(".main-content").css("display","none");
      if( window.location.pathname == "/german/" ){
          jQuery(".main-content").css("display","block");
      } else {
          window.location = "/german/";
      }
    }
        
    if(( browserLang == "ja" ) && (pathNameHere == "/")){
      jQuery(".main-content").css("display","none");
      if( window.location.pathname == "/japanese/" ){
          jQuery(".main-content").css("display","block");
      } else {
          window.location = "/japanese/";
      }
    }

    if(( browserLang == "ko" ) && (pathNameHere == "/")){
      jQuery(".main-content").css("display","none");
      if( window.location.pathname == "/korean/" ){
          jQuery(".main-content").css("display","block");
      } else {
          window.location = "/korean/";
      }
    }
  
    if(( browserLang == "es" ) && (pathNameHere == "/")){
      jQuery(".main-content").css("display","none");
      if( window.location.pathname == "/spanish/" ){
          jQuery(".main-content").css("display","block");
      } else {
          window.location = "/spanish/";
      }
    }
 
});
} else {
    jQuery(".main-content").css("display","block");
}
     
</script>
