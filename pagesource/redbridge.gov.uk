    <script src="/content/js/script.js"></script>
	<script>
        var pdfRegEx = "^.*\.(pdf|PDF)$";
        $('a').click(function () {
            var link = $(this).attr('href');
            if (typeof (link) != 'undefined') {
                if (link.match(pdfRegEx)) {
                    var linkText = $(this).text().replace(/\&nbsp;/g, ' ').replace(/\n/g, ' ').trim();
                    var pageURL = window.location.pathname;
                    //_gaq.push(['_trackEvent', 'PDF Download', linkText, pageURL]);
                    ga('send', 'event', 'PDF Download', pageURL, link);
                }
            }
        });
        $('a.icon-down-open-mini').click(function () {
            var link = $(this).attr('href');
            if (typeof (link) != 'undefined') {
                var actionText = '';
                if ($(this).parent().hasClass('open')) {
                    actionText = "Collapsed";
                } else {
                    actionText = "Expanded";
                }
                var linkText = $(this).text().trim();
                var pageURL = window.location.pathname;
                ga('send', 'event', 'Concertina ' + actionText, pageURL, link);
            }
        });
        // ReciteMe
        jQuery(".reciteme").click(function(i,e){
            loadService();
            return false;
        });
    </script>

