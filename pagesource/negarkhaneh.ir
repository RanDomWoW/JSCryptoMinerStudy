<script type="text/javascript">

    $(document).ready(function () {

        var SelectedItem;
        var panel;
        var ForbidenHide = 0

        $('#TopQuickLinks>ul>li>a').mouseenter(function () {

            $('#TopQuickLinks>ul>li>a').removeClass();
            $(this).addClass("Select");
            SelectedItem = $(this).parent().attr("id");

            switch (SelectedItem) {
                case "SearchBtn":
                    panel = $("#SearchPanel");

                    break;
                case "LoginBtn":
                    panel = $("#LoginPanel");

                    break;
                case "RegBtn":
                    panel = $("#RegPanel");


                    break;
            };
            
            if ($(panel).css('display') != "block") {
                HidePanel();
                ShowPanel(panel);
            }

        });

        $('#TopQuickLinks').children().mouseleave(function () {

            //if (ForbidenHide != 1) {
            //    HidePanel();
            //    $('#TopQuickLinks li a').removeClass();
            //}

        });

        $('html').click(function (event) {
            if ($(event.target).is("#TopQuickLinks, #TopQuickLinks *"))
                ForbidenHide = 1;
            else {
                ForbidenHide = 0;
                HidePanel();
                $('#TopQuickLinks li>a').removeClass();
            }

        });

        function HidePanel() {
            $("#SearchPanel, #LoginPanel, #RegPanel").fadeOut({ duration: 100, queue: true });
        }

        function ShowPanel(panel) {
            $(panel).show();
        }

        if ($("#username").val() != "" && $("#username").val() != 'نام کاربری (یا ایمیل)')
            $("#username").addClass("focus");
        if ($("#password").val() != "" && $("#password").val() != 'گذرواژه')
            $("#password").addClass("focus");

        $('#password').keyup(function (e) {
            if (e.keyCode == 13)
                $('#login-submit').click();
        });


        $("#username").focus(function () {
            if ($(this).val() == 'نام کاربری (یا ایمیل)')
                $(this).val('').addClass("focus");
        });

        $("#username").blur(function () {
            if ($(this).val() == '')
                $(this).val('نام کاربری (یا ایمیل)').removeClass("focus");
        });

        $("#password").focus(function () {
            if ($(this).val() == 'گذرواژه')
                $(this).val('').addClass("focus").attr({type:"password"});
        });

        $("#password").blur(function () {
            if ($(this).val() == '')
                $(this).val('گذرواژه').removeClass("focus").attr({ type: "text" });
        });


        $('#login-submit').click(function () {

            $.ajax({
                url: "/AjaxLibrary/helper2.aspx", cache: false,
                type: "POST",
                data: {
                    "act": "login",
                    "user": $('#username').val(),
                    "pass": $('#password').val(),
                    "remember": $('#chk_remember').is(':checked')
                },
                success: function (result) {
                    if (result == "0") {
                        $(".login-message").removeClass("Error").removeClass("Alert");
                        $(".login-message").addClass("Error").text("عضویت شما غیر فعال است.");
                    }
                    else if (result == "-1") {
                        $(".login-message").removeClass("Error").removeClass("Alert");
                        $(".login-message").addClass("Alert").text('گذرواژه و یا نام کاربری اشتباه است.');
                    }
                    else if (result == "-2") {
                        $(".login-message").removeClass("Error").removeClass("Alert");
                        $(".login-message").addClass("Alert").text('توجه: به علت بروز رسانی وب سایت، ورود با نام کاربری موقتا غیرفعال است.');
                    }
                    else {
                        var User = JSON.parse(result);
                        //alert(User.fname + User.lname + User.avatarUrl + User.username);
                        $("#LoginBtn, #RegBtn").fadeOut(200);


                        $.get("/Template/NegarkhanehV3-5/tmpl/HeaderUserPanel.html", function (template) {


                            var tpl = $.tmpl(template, {
                                "fname": User.fname,
                                "username": User.username,
                                "avatarUrl": User.avatarUrl,
                                "myMessageCount": User.myMessageCount
                            })//.appendTo("#TopQuickLinks");
                            $('#TopQuickLinks').after(tpl);
                            $('.TopUserPanel').fadeIn(400);

                        });


                    }
                },
                beforeSend: function () {
                    $("#login-submit").after("<span class='Spinner'></span>");
                    $("#login-submit").hide();
                    //$("#login-box").html("<div class='Spinner-Large'></div>");
                },
                error: function () {
                    $("#login-submit").show();
                    $('#login-box .Spinner').hide();
                },
                complete: function () {
                    $("#login-submit").show();
                    $('#login-box .Spinner').hide();
                }
            });
        });



    });




	
    </script>


<div id="TopQuickLinks">
    <ul>
        <li id="SearchBtn"><a>جستجو</a>
            
<script type="text/javascript">
    window.searchScroll = 0;
    function Ajax_ExecQuickSearch(textSearch) {

        $("#no-results").fadeOut(200, function () {
            if (textSearch == "" || textSearch == " ") {
                $('#item-container').fadeOut(100);
            }
        });

        $.ajax({ url: "/AjaxLibrary/AjaxQuickSearch.aspx", cache: false,
            type: "POST",
            data: { "act": "QuickSearch",
                "text": textSearch
            },
            complete: function () { $("#search-word .Spinner").fadeOut(100); },
            success: function (result) {
                var obj = JSON.parse(result);

                $("#item-container").fadeIn(100);
                var news = $("#QS_News").tmpl(obj.NewsList);
                var article = $("#QS_Article").tmpl(obj.ArticleList);
                var users = $("#QS_Users").tmpl(obj.UserList);
                var userpages = $("#QS_UserPages").tmpl(obj.UserPageList);
                var eshop = $("#QS_Eshop").tmpl(obj.EShopList);
                if (news.text() != "")
                    var newsTitle = "<div class='search-item-category'>اخبـــــــار</div>";
                else
                    var newsTitle = "";

                if (article.text() != "")
                    var articleTitle = "<div class='search-item-category'>مقـــالات</div>";
                else
                    var articleTitle = "";

                if (users.text() != "")
                    var usersTitle = "<div class='search-item-category'>افـــــــراد</div>";
                else
                    var usersTitle = "";

                if (userpages.text() != "")
                    var userpagesTitle = "<div class='search-item-category'>مجمــوعه‌ها</div>";
                else
                    var userpagesTitle = "";

                if (eshop.text() != "")
                    var eshopTitle = "<div class='search-item-category'>فـروشگاه</div>";
                else
                    var eshopTitle = "";

                $('#item-container .content').empty();
                $('#item-container .content').append(userpagesTitle).append(userpages).append(usersTitle).append(users).append(newsTitle).append(news).append(articleTitle).append(article).append(eshopTitle).append(eshop);

                if (news.text() == userpages.text() && userpages.text() == users.text() && users.text() == article.text() && article.text() == eshop.text() && eshop.text() == "") {
                    $('#item-container').hide();
                    $("#no-results").fadeIn(200);
                    $("#search-word .Spinner").fadeOut(100);
                }

                $('.nano').nanoScroller();

            },
            beforeSend: function () { if (textSearch != "" && textSearch != " ") $("#search-word .Spinner").fadeIn(100); },
            error: function () { $("#search-word .Spinner").fadeOut(100); }
        });
    }

    $(function () {
        $("#search-word #inp").keyup($.debounce(500, alerttt));
        $("#item-container").hide();
    });



    function alerttt() {
        var searchWord = $("#search-word #inp").val();
        Ajax_ExecQuickSearch(searchWord);
    }

</script>
