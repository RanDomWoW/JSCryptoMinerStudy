    function tipsyPercent() {

        $('.tipsySmallTooltip').tipsy({
            //delayIn: 100,      // delay before showing tooltip (ms)
            //delayOut: 1000,     // delay before hiding tooltip (ms)
            //fade: true,     // fade tooltips in/out?
            fallback: '',    // fallback text to use when no tooltip text
            gravity: 'n',    // gravity
            html: true,     // is tooltip content HTML?
            live: false,     // use live event support?
            offset: 0,       // pixel offset of tooltip from element
            opacity: 0.8,    // opacity of tooltip
            title: 'ToolTip',  // attribute/callback containing tooltip text
            trigger: 'hover' // how tooltip is triggered - hover | focus | manual
            //cssclass: 'tipsySmall'

        });
$(".StatBarInner").mousemove(function (e) {
            var width = $(".tipsy").width();
            $(".tipsy").css({
                position: "absolute",
                top: (e.pageY + 10) + "px",
                left: (e.pageX - (width / 2)) + "px"
            });
        });
    }
    var Cookies = {
        // Initialize by splitting the array of Cookies
        init: function () {
            var allCookies = document.cookie.split('; ');
            for (var i = 0; i < allCookies.length; i++) {
                var cookiePair = allCookies[i].split('=');
                this[cookiePair[0]] = cookiePair[1];
            }
        },
        // Create Function: Pass name of cookie, value, and days to expire
        create: function (name, value, days) {
            if (days) {
                var date = new Date();
                date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                var expires = "; expires=" + date.toGMTString();
            }
            else var expires = "";
            document.cookie = name + "=" + value + expires + "; path=/";
            this[name] = value;
        },
        // Read Function: Pass name of cookie
        read: function (name) {
            var nameEQ = name + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1, c.length);
                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
            }
            return null;
        },
        // Erase cookie by name
        erase: function (name) {
            this.create(name, '', -1);
            this[name] = undefined;
        }
    };
    function PollSend(pollid) {
        if ($('input:radio[name=PollName_' + pollid + ']:checked').length == 0)
            return;
        $.ajax({ url: "/AjaxLibrary/helper2.aspx", cache: false,
            type: "POST",
            data: { "act": "PollSend", "pollid": pollid, "value": $('input:radio[name=PollName_' + pollid + ']:checked').val() },
            success: function (result) {
                if (result == "-1") {
                    alert("خطا");
                }
                else {
                    Cookies.create("PollID-" + pollid, "", 365);
                    $('#divPoll_' + pollid).html('');
                    var sum = parseInt($('#spanPollSum_' + pollid).text());
                    $('#spanPollSum_' + pollid).text(sum + 1);
                    var obj = JSON.parse(result);
                    $.each(obj.options, function (index, value) {
                        AppendOption(pollid, value);
                    });
                    tipsyPercent();
                }
            },
            beforeSend: function () { $(".BtnSmall").hide(); $(".SmallLoading").show(); },
            error: function () { }
        });
    }
    function AppendOption(pollid, option) {
        var html = "<div class='PollAnswer'>" + option.text + "</div><div class='StatBar'><div class='StatBarInner tipsySmallTooltip' tooltip='" + option.p + "%'"
                   + "style='width:" + option.p + "%' ></div></div>";
        $('#divPoll_' + pollid).append(html);
    }
</script>

