<script type="text/javascript">
  // Global variables
	var modal = $('#supportContactModal');
	var modalContactOptions = $('[data-support-modal-button]');
  var selectMethodPage = $('[data-support-modal-page="selectMethod"]');
  var selectDepartmentPage = $('[data-support-modal-page="selectDepartment"]');
  var phoneNumbersPage = $('[data-support-modal-page="phoneNumbers"]');
  var contactFormPage = $('[data-support-modal-page="contactForm"]');
  var errorMessageContainer = $('.flash-message-container', modal);
  var successPage = $('[data-support-modal-page="success"]');
  var allPages = $('.modal-content');
  var modalForm = $('#supportContactForm');
  var formSubmitButton = $('button[type=submit]', modalForm);
  var recipientHiddenInput = $('#support_recipient', modalForm);
  var submittedForm;

  function onSubmit(token) {
    var form = submittedForm;

    if (form.id === "supportContactForm")
    {
      errorMessageContainer.hide();

      var postData = {
        method: "POST",
        url: "https://ajax.scanguard.com/support-request",
        data: {
          "subject": form['support_subject'].value,
          "name": form['support_name'].value,
          "email": form['support_email'].value,
          "message": form['support_message'].value,
          "recipient": form['support_recipient'].value,
          "g-recaptcha-response": token
        }
      };

      $.ajax(postData)
        .done(function( responseJson ) {
          var response = null;

          console.log('response here!');

          // Try to parse the response as JSON
          try {
            response = JSON.parse(responseJson);
          }
          catch (error)
          {
            console.error(error);
          }

          // If JSON wasn't successfully parsed
          if (response && response['success'] === true)
          {
            // Clear form
            form.reset();
            grecaptcha.reset();
            validationHandler.helper.resetForm(form);

            // Show success page
            allPages.hide();
            successPage.show();

            // Auto close modal after 10 seconds
            setTimeout(function() {
              modal.foundation('reveal', 'close');
            }, 10000)
          }
          else
          {
            return handleFormSubmissionError(form);
          }
        })
	      .fail(function(error) {
          console.log('fail here! ', error);
	        return handleFormSubmissionError(form);
	      }
      )
    }
  }

  // Form submission (callback function is run when validation passes)
  function validationSuccessCallback(e, form) {
    console.log('google recaptcha: ', typeof $(form).data('google-re-captcha'));
    console.log('google recaptcha-test: ', typeof $(form).data('google-re-captcha-test'));
    if (typeof $(form).data('google-re-captcha') !== "undefined")
    {
      e.preventDefault();
      submittedForm = form;
      grecaptcha.execute();
    }
  }

  // Page navigation
  modalContactOptions.on('click', function() {

    var clickedElement = $(this);
    var navigationTarget = clickedElement.data('support-modal-button');

    if (clickedElement.is('[data-dept]'))
    {
	    var selectedDepartment = clickedElement.data('dept');
	    recipientHiddenInput.val(selectedDepartment);
    }

    if (navigationTarget === 'email')
    {
      allPages.hide();
      selectDepartmentPage.show();
    }
    else if (navigationTarget === 'phone')
    {
      allPages.hide();
      phoneNumbersPage.show();
    }
    else if (navigationTarget === 'technical' || selectedDepartment === 'billing')
    {
      allPages.hide();
      contactFormPage.show();
    }
  });

	//  Back buttons
	$('[data-support-back-1]').on('click', function(){
	  allPages.hide();
    selectMethodPage.show();
	});

  $('[data-support-back-2]').on('click', function(){
    allPages.hide();
    selectDepartmentPage.show();
  });

  var handleFormSubmissionError = function(form) {
    errorMessageContainer.show();
    modalForm.removeAttr('form-submitted');
    var originalButtonText = formSubmitButton.data('validate-original-content');
    formSubmitButton.text(originalButtonText);
    validationHandler.helper.resetForm(form);
    grecaptcha.reset();
  };

	$(window).load(function(){
    $(document).on('closed.fndtn.reveal', '#supportContactModal', function (e) {
      allPages.hide();
      selectMethodPage.show();
      modalForm.removeAttr('form-submitted');
      var originalButtonText = formSubmitButton.data('validate-original-content');
      formSubmitButton.text(originalButtonText);
    });

	  $(document).on('open.fndtn.reveal', '#supportContactModal', function (e) {
	    console.log('modal opened!');
	    $("html, body").animate({ scrollTop: 0 }, "slow");
	  });
  });
</script>

</div>  
  


</div>

