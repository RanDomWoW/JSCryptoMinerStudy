                                var viewHeight = ($(window).height() - iframe.attr('height')) / 2;
                                console.log(viewWidth);
                                console.log('viewHeight'+viewHeight);
                                viewWidth = viewWidth


                                    $('.parallax-ad').find('div').css('left', viewWidth + 'px');
                                    $('.parallax-ad').find('div').css('top', viewHeight + 'px');

                            } else if (iframe.attr('height') == '800' || iframe.attr('height') == '1080') {
                                wrap.parent().addClass('parallaxContainer');
                                wrap.addClass('parallax-ad');
                                wrap.addClass('plx_h1080');
                                var viewWidth = ($(window).width() - iframe.attr('width')) / 2;
                                var viewHeight = ($(window).height() - iframe.attr('height')) / 2;
                                console.log('window width = '+$(window).width());
                                console.log('iframe width = '+iframe.attr('width'));
                                console.log('window height = '+$(window).height());
                                console.log('iframe height = '+iframe.attr('height'));
                                viewWidth = viewWidth


                                    $('.parallax-ad').find('div').css('left', viewWidth + 'px');
                                    $('.parallax-ad').find('div').css('top', viewHeight + 'px');

                                // Set CSS scale
                                //iframe.css({'transform': 'scale(' + scaleWidth + ') translate(-50%, -50%)', 'transformOrigin': '0% 0%'});
                            } else {
                            }

                        }
                    },
                    // Callback which is run after the render of all ads.
                    afterAllAdsLoaded: function () {
                    }
                })
            })(window);
        });


        $(document).scroll(function () {
            if ($('.parallaxContainer').isOnScreen()) {
                $(".animate").addClass("move");

                var windowTop = $(this).scrollTop() + 70;
                var divOffset = $('.parallaxContainer').offset();
                var divTop = divOffset.top;

                if (windowTop > divTop) {
                    $('.navbarCustom').addClass("moveMenu");
                } else {
                    $('.navbarCustom').removeClass("moveMenu");
                }

            } else {
                $(".animate").removeClass("move");
                $('.navbarCustom').removeClass("moveMenu");
            }
        });


        $.fn.isOnScreen = function () {

            var win = $(window);

            var viewport = {
                top: win.scrollTop(),
                left: win.scrollLeft()
            };
            viewport.right = viewport.left + win.width();
            viewport.bottom = viewport.top + win.height();


            if (this.length) {
                var bounds = this.offset();
                bounds.right = bounds.left + this.outerWidth();
                bounds.bottom = bounds.top + this.outerHeight();

                return (!(viewport.right < bounds.left || viewport.left > bounds.right || viewport.bottom < bounds.top || viewport.top > bounds.bottom));
            } else {
                return false;
            }
        };


    </script>
