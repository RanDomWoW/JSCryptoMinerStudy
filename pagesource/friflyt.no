    <script type="text/javascript">
        var searchBoxShow = false;
        var loginBoxShow = false;
        $(function () {
            var minimum_doc_width = 768;
            hideDropDown();

            if($(document).width()<768){
                return false;
            }
            $(".dropdown").on("mouseover", function(){
                $(this).attr('aria-expanded', 'true');
                $(this).addClass('open');
            });

            $(document).on("click", function(){
                hideDropDown();
            });

            $(".dropdown").on("mouseleave", function(){
                hideDropDown();
            });
        });

        $(document).mouseup(function (e){
            var container = $(".search_div");
            if (!container.is(e.target) && container.has(e.target).length === 0){
                searchBoxShow = false;

                if($('.search_div').css('display') == 'block'){
                    searchBoxShow = true;
                }
                container.hide();
            }

            var login_container = $(".login_div");
            if (!login_container.is(e.target) && login_container.has(e.target).length === 0){
                loginBoxShow = false;
                if($('.login_div').css('display') == 'block'){
                    loginBoxShow = true;
                }
                login_container.hide();
            }
        });

        function showInputBox(obj){

            if(!searchBoxShow) {
                $(obj).show();
                searchBoxShow = true;

            }
            else{
                $(obj).hide();
                searchBoxShow = false;
                return true;
            }
        }

        function showLoginInputBox(obj){
            if(!loginBoxShow){
                $(obj).show();
                loginBoxShow = true;
            }
            else{
                $(obj).hide();
                loginBoxShow = false;
            }
        }


        $('.navbar-collapse').on('shown.bs.collapse', function() {
            hideDropDown();
        });
        $('.navbar-collapse').on('hidden.bs.collapse', function() {
            hideDropDown();
        });
        $(window).resize(function(){
            hideDropDown();
        });

        function hideDropDown(){
            if($(document).width()<768){
                $('.dropdown').each(function(i,e){
                    $(e).addClass('open');
                    $(e).find("a").attr('aria-expanded', 'true');
                });
                return false;
            }
            $('.dropdown').each(function(i,e){
                $(e).removeClass('open');
                $(e).find("a").attr('aria-expanded', 'false');
            });
        }
    </script>

