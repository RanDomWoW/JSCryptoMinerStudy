                    $('#skel-layers-activeWrapper').insertBefore('#skel-layers-wrapper');
                    // and make position:absolute
                    $("#navButton, #header").addClass("teeter");
                    // now we bind to scroll events
                    $(document).bind("scroll", function () {
                        // near the top?
