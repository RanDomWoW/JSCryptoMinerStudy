        <script type="text/javascript">
var $socialStream;
var $grid;
var $items;
var page = 1;
var sources = new Array();
var $filterIcons;
var wookmark;

var film = '__all__';

var resizeTimer;

function hideLoadMore() {
    $socialStream.find('.load-more').hide();
}

function emptyGrid() {
    $socialStream.find('.load-more').show();
    page = 1;
    $grid.empty();
    loadData();
}

function fixLayout() {
    wookmark.initItems();
    wookmark.layout(true);
}

function loadData() {
    // Determine sources to pull
    sources = new Array();
    var $selectedSources = $socialStream.find('.filters').find('li.active');
    if ( $selectedSources.length == 0 ) { $selectedSources = $filterIcons; }
    $selectedSources.each(function() {                                   
        sources.push($(this).find('.icon').attr('class').split(' ')[0]);  
    });

    $.ajax({                                                              
        url: '/social_crawl/posts/html/',                                 
        dataType: 'html',                                                 
        data: {film:film,page:page,sources:sources},                           
        success: onLoadData,
    }); 
}

function onLoadData(data) {
    $grid.find('.wookmark-placeholder').remove();
    var $gridcache = $socialStream.find('.gridcache');
