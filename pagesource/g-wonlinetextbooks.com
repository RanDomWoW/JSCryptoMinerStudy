$.noConflict();
jQuery(document).ready(function($){
/*0 means disabled; 1 means enabled;*/
var popupStatus=0;
function loadPopup(pop){
if(popupStatus==0){
$("#backgroundPopup").css({"opacity":"0.7"});
$("#backgroundPopup").fadeIn("slow");
popupStatus=1;
}
$(pop).fadeIn("slow");
}
function disablePopup(){
if(popupStatus==1){
$("#backgroundPopup").fadeOut("slow");
$("#hiddenlogin").fadeOut("slow");
$("#loginheader").fadeOut("slow");
$("#jquerypasshead").fadeOut("slow");
$("#jquerypasstop").fadeOut("slow");
$("#jqueryreg").fadeOut("slow");
$("#jqueryregtop").fadeOut("slow");
$("#jqueryregbot").fadeOut("slow");
$("#changeinfo").fadeOut("slow");
$("#changeinfotop").fadeOut("slow");
popupStatus=0;
}
}
function centerPopup(pop,addtop){
var windowWidth=document.documentElement.clientWidth;
var windowHeight=document.documentElement.clientHeight;
var popupHeight=$(pop).height();
var popupWidth=$(pop).width();
$(pop).css({
"position":"absolute",
"top":((windowHeight/2-popupHeight/2)-addtop),
"left":windowWidth/2-popupWidth/2
});
//only need force for IE6
$("#backgroundPopup").css({"height":windowHeight});
}
function funcreg(){
centerPopup("#jqueryreg",95);
centerPopup("#jqueryregtop",290);
centerPopup("#jqueryregbot",-91);
$("#hiddenlogin").fadeOut("slow");
$("#loginheader").fadeOut("slow");
popupStatus=0;
loadPopup("#jqueryreg");
loadPopup("#jqueryregtop");
loadPopup("#jqueryregbot");
}
function funclogin(){
centerPopup("#hiddenlogin",101);
centerPopup("#loginheader",250);
popupStatus=0;
loadPopup("#hiddenlogin");
loadPopup("#loginheader");
}
function funcpass(){
centerPopup("#jquerypasshead",79);
centerPopup("#jquerypasstop",225);
$("#hiddenlogin").fadeOut("slow");
$("#loginheader").fadeOut("slow");
popupStatus=0;
loadPopup("#jquerypasshead");
loadPopup("#jquerypasstop");
}
$(document).ready(function(){
$("#btnlogin").click(function(){funclogin();});
$("#txtlogin").click(function(){funclogin();});
$("#txtlogin2").click(function(){funclogin();});
$("#hiddenlogin p.t-forgot a").click(function(){
this.href="javascript:void(0)";
this.innerHTML="Lost Password";
funcpass();
});
$("#lostpass").click(function(){
this.href="javascript:void(0)";
this.innerHTML="Lost Password";
funcpass();
});
$("#lostpassbot").click(function(){
this.href="javascript:void(0)";
this.innerHTML="Lost Password";
funcpass();
});
$("#btnreg").click(function(){
funcreg();
});
$("#hiddenlogin p.t-register a").click(function(){
this.href="javascript:void(0)";
funcreg();
});
$("#btnhomereg").click(function(){
this.href="javascript:void(0)";
funcreg();
});
$("#backgroundPopup").click(function(){
disablePopup();
});
$(".jqueryheader img").click(function(){
disablePopup();
});
});
});
//]]>
</script>
