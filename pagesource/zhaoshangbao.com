                    var AgentClass = $.trim($("#Agent_website_id_3").find("option:selected").val());
                    var ProvinceName_meg = $.trim($("#hidProvince_meg").val());
                    var CityName_meg = $.trim($("#hidCity_meg").val());
                    var AreaName_meg = $.trim($("#hidArea_meg").val());
                    var name_mes = $.trim($("#name_mes").val());
                    var phone_meg = $.trim($("#phone_meg").val());
                    var error = "";
                    if (AgentClass == "")
                        error += "请选择项目类别!\n";
                    if (ProvinceName_meg == "")
                        error += "请选择省份!\n";
                    if (CityName_meg == "")
                        error += "请选择城市!\n";
                    if (AreaName_meg == "")
                        error += "请选择县区!\n";
                    if (name_mes == "")
                        error += "请输入姓名!\n";
                    if (phone_meg == "") {
                        error += "请输入手机号!\n";
                    }
                    else {
                        var re = /^[\d-]{11}$/
                        if (!re.test(phone_meg)) {
                            error += "请填写您的手机号码。手机：13123456789\n";
                        }
                    }
                    if (error != "") {
                        alert(error);
                        return
                    }
                    else {
                        var data = "act=fixMessage&proClass=" + AgentClass + "&userName=" + escape(name_mes) + "&UserMobile=" + escape(phone_meg) + "&province=" + escape(ProvinceName_meg) + "&city=" + escape(CityName_meg) + "&area=" + escape(AreaName_meg);
                        $.ajax({
