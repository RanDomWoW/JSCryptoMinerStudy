<script language="javascript">


function setMbCookie(name,value,expiredays) 
{ 
var todayDate = new Date(); 
todayDate.setDate( todayDate.getDate() + expiredays ); 
document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";" 
}


//쿠키값가져오기
function getMbCookie (cookieName)
{ 
	var cookieValue = null; 
	var posName = document.cookie.indexOf(escape(cookieName) + '='); 
	if (posName != -1) { 
		var posValue = posName + (escape(cookieName) + '=').length; 
		var endPos = document.cookie.indexOf(';', posValue); 
		if (endPos != -1) 
			cookieValue = unescape(document.cookie.substring(posValue, endPos)); 
		else 
			cookieValue = unescape(document.cookie.substring(posValue)); 
	} 
  return cookieValue; 
}




function setmCookie(name, value)
{
	
		$.cookie(name, value, {expires:30});	
		return;
	
}


function getmCookie(name)
{
	var cookie_val = $.cookie(name);
	if ( typeof (cookie_val) == "undefined" || cookie_val == null ) return [];
	
	return cookie_val;
}


