								var popup_pic = document.getElementById("popup_pic");
								var popup_no = document.getElementById("popup_no");
								var list1 = popup_pic.getElementsByTagName("A");
								var list2 = popup_no.getElementsByTagName("IMG"); 
								var cnt = list1.length;

								for (var i=0; i < cnt; i++) {
								list1.item(i).style.display = "none";
								list2.item(i).src = list2.item(i).src.replace("_on.gif",".gif");
								}
								list1.item(idx-1).style.display = "block";
								list2.item(idx-1).src = list2.item(idx-1).src.replace(".gif","_on.gif");
								}
								function slidePopup() {
								if (++popup_curr_idx > popup_cnt) popup_curr_idx = 1;
								changePopup(popup_curr_idx);
								}
								function stopPopup() {
								if (popup_timer != null) clearTimeout(popup_timer);
								}
								function startPopup() {
								stopPopup();
								popup_timer = setInterval("slidePopup()", 5000);
								}
								function initPopup(){
								var popup_pic = document.getElementById("popup_pic");
								var popup_no = document.getElementById("popup_no");
								var list1 = popup_pic.getElementsByTagName("A");
								var list2 = popup_no.getElementsByTagName("IMG");
								list1.item(0).style.display = "block";
								list2.item(0).src = list2.item(0).src.replace(".gif", "_on.gif");

								startPopup();
								}
								initPopup();
								</script>
