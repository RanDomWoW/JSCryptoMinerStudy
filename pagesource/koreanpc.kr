	function getCookie(name) {
		returnCookie="";
		thisCookie = document.cookie.split("; ") ;
		for (i=0; i<thisCookie.length; i++) {
			// 쿠키가 발견될때까지 찾기 
			if(thisCookie[i].split("=")[0] == name) {
