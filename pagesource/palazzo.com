  </div>
  
  <script id="mod-ugc__item-template" type="text/template">
    <div class="mod-ugc__item">
      <div class="mod-ugc__item__image scaled">
        <img 
          draggable="false" 
          class="content lazyload" 
          data-src="<%= imgUrl %>"
          data-username="@<%= username %>"
          data-userpic="<%= userpic %>"
          data-caption="<%= caption %>"
          data-index="<%= index %>"
        >
        <span class="mod-ugc__item__username">@<%= username %></span>
      </div>
    </div>
  </script>
  
  <script id="mod-ugc__carousel__item-template" type="text/template">
    <div class="mod-ugc__carousel__item">
      <div class="mod-ugc__carousel__item__media">
        <a href="<%= url %>" target="_blank"><img src="<%= imgUrl %>"></a>
      </div>
      
      <div class="mod-ugc__carousel__item__text__container">
        <div class="mod-ugc__carousel__item__text">
          <div class="mod-ugc__carousel__item__userpic">
            <a href="<%= userUrl %>" target="_blank"><img src="<%= userpic %>"></a>
          </div>
          
          <div class="mod-ugc__carousel__item__caption">
            <h5 class="mod-ugc__carousel__item__username"><a href="<%= userUrl %>" target="_blank">@<%= username %></a></h5>
            <p class="mod-ugc__carousel__item__meta">
              <% if (likes) { %>
                <% if (likes === 1) { %>
                  <span class="mod-ugc__carousel__item__likes"><%= likes %> Like</span> <span class="separator">-</span> 
                <% } else { %>
                  <span class="mod-ugc__carousel__item__likes"><%= likes %> Likes</span> <span class="separator">-</span> 
                <% } %>
              <% } %>
              
              <span class="mod-ugc__carousel__item__date"><%= date %></span>
            </p>
            <p><%= caption %></p>
          </div>
        </div>
      </div>
    </div>
  </script>
</section>


