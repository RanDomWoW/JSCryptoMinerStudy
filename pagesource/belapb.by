<script type="text/javascript">
   
    
    function subm_form_online(){
        var ok = 1;

        var f_extensionField_Name = jQuery('#f_extensionField_Name').val();
        var f_extensionField_Email = jQuery('#f_extensionField_Email').val();
        var f_extensionField_PhoneNumber = "";
        var f_phone_number = jQuery('#phone_number').val();
        var f_phone_number2 = jQuery('#f_extensionField_Phone2').val();
        
        if(f_extensionField_Name.length < 2 ){
            ok = 0;
            jQuery('#ff_extensionField_Name').addClass('error-field');
        }else{
            jQuery('#ff_extensionField_Name').removeClass('error-field');
        }
        
        if(f_phone_number == "" && f_phone_number2 == ""){
            ok = 0;
            jQuery('#ff_extensionField_PhoneNumber').addClass('error-field');
        }else{
            if(f_phone_number != ""){
                if(f_phone_number.length < 7 || f_phone_number.match(/[^0-9 ]/g)){
                    ok = 0;
                    jQuery('#ff_extensionField_PhoneNumber').addClass('error-field');
                }else{
                    jQuery('#ff_extensionField_PhoneNumber').removeClass('error-field');
                    f_extensionField_PhoneNumber = "8"+jQuery('#phone_code').val()+f_phone_number;
                    jQuery('#f_extensionField_PhoneNumber').val(f_extensionField_PhoneNumber);
                }
            }
            
            if(f_phone_number2 != ""){
                if(f_phone_number2.length > 20 || f_phone_number2.match(/[^0-9 ]/g)){
                    ok = 0;
                    jQuery('#ff_extensionField_PhoneNumber').addClass('error-field');
                }else{
                    jQuery('#ff_extensionField_PhoneNumber').removeClass('error-field');
                    f_extensionField_PhoneNumber = f_phone_number2;
                    jQuery('#f_extensionField_PhoneNumber').val(f_extensionField_PhoneNumber);
                }
            }
        }

        if(f_extensionField_Email != "" && !validateEmail(f_extensionField_Email)){
            ok = 0;
            jQuery('#ff_extensionField_Email').addClass('error-field');
        }else if(f_extensionField_Email != ""){
            jQuery('#ff_extensionField_Email').removeClass('error-field');
        }

        if(ok == 1){
            
            var ws=new Date();
            ws.setMinutes(100+ws.getMinutes());
            var f_chat_id = '1521622239'+makeid(8);
            setCookie("onlajn_kons_id", f_chat_id, ws.toGMTString(), "/");
            
            //jQuery('#button_submit_ko').click();
            chat_create(f_chat_id);
        }
        return false;
    }
    
    function chat_create(onlajn_kons_id){
        if(onlajn_kons_id == false || onlajn_kons_id == ""){
            return false;
        }
        
        jQuery.post(
                '/chat.php',
                {
                    'chat_req_event':                       'create',
                    'extensionField_Name':                  jQuery('#f_extensionField_Name').val(),
                    'extensionField_PhoneNumber':           jQuery('#f_extensionField_PhoneNumber').val(),
                    'extensionField_Email':                 jQuery('#f_extensionField_Email').val(),
                    'extensionField_ccxqueuetag':           jQuery('#f_extensionField_ccxqueuetag').val(),
                    'author':                               'Customer',
                    'title':                                'ccx chat',
                    'extensionField_h_widgetName123456':    'ChatForm',
                    'extensionField_chatLogo':              encodeURIComponent("http://www.belapb.by/images/logo_chat.png"),
                    'extensionField_chatWaiting':           'Здравствуйте! Ваше обращение направляется оператору.',
                    'extensionField_chatAgentJoinTimeOut':  'В настоящее время все операторы заняты. Пожалуйста подождите или повторите попытку позже.',
                    'extensionField_chatError':             'В настоящее время все операторы заняты. Пожалуйста, повторите попытку позже'
                }, 
                
                function (data) {  
                    //alertObj(data);

                    if(data.ok == 1){
                        jQuery('.onlain_kons_block').hide();
                        jQuery('.onlain_chat_block').show();
                        get_chat_list();
                    }

        },'json');
    }
    
    function get_chat_list(){
            window.timerId = setInterval(function() {
                  /*
                  if(jQuery('.messages-list').hasClass('color1') == 1){
                      jQuery('.messages-list').removeClass('color1');
                      jQuery('.messages-list').addClass('color2');
                  }else{
                      jQuery('.messages-list').removeClass('color2');
                      jQuery('.messages-list').addClass('color1');
                  }
                  */
                  
                  chat_list();
        
            }, 2000);
    }
    
    function chat_list(){
        var onlajn_kons_id = getCookie('onlajn_kons_id');
        var was_last_event_id = 1*jQuery('#last_event_id').val();

        jQuery.post(
                '/chat.php',
                {
                    'chat_req_event': 'list'
                }, 
                function (data) {  
                    //alertObj(data);
                    
                    if(data.ok == 1){
                    
                        if(data.waiting == 1){
                            jQuery('#messages_list').html('Здравствуйте! Ваше обращение направляется оператору');
                        }else{
                            jQuery('#messages_list').html(data.data_str);
                            
                            if(data.has_messages == 1 || data.chat_start == 1){
                                if(jQuery('.message-btn .ybtn').hasClass('hbs') == 1){
                                    jQuery('.message-btn .ybtn').removeClass('hbs').attr('onclick', 'send_chat_mess();');
                                    jQuery('#cust_message').removeAttr('disabled');
                                }
                                
                                if(was_last_event_id < data.last_event_id){
                                    var elem = document.getElementById('messages_list');
                                    elem.scrollTop = elem.scrollHeight;
                                    jQuery('#last_event_id').val(data.last_event_id);
                                }
                            }else{
                                jQuery('.message-btn .ybtn').addClass('hbs');
                                jQuery('#cust_message').attr('disabled', 'disabled');
                                
                                if(data.left == 1){
                                    stop_chat();
                                }
                            }

                            
                        }
                        
                    }else{
                        if(data.error == 1){
                            stop_chat();
                            jQuery('#messages_list').html('В настоящее время все операторы заняты. Пожалуйста, повторите попытку позже.');
                        }
                    }

        },'json');
        
    }
    
    function stop_chat(){
        clearInterval(window.timerId);
        var ws=new Date();
        ws.setMinutes(30+ws.getMinutes());
        setCookie("onlajn_kons_id", "", ws.toGMTString(), "/");
    }
    
    
    function leave_chat(){
        jQuery.post(
                '/chat.php',
                {
                    'chat_req_event': 'leave'
                }, 
                function (data) {  
                    //alertObj(data);
                    
                    if(data.ok == 1){
                        
                    }
                
        },'json');
    }
    
    function send_chat_mess(){
        var message = jQuery('#cust_message').val();
        
        if(message != ""){
            
            jQuery.post(
                    '/chat.php',
                    {
                        'chat_req_event': 'message',
                        'message':        message
                    }, 

                    function (data) {  
                        //alertObj(data);

                        if(data.ok == 1){
                            jQuery('#cust_message').val('');
                        }

            },'json');
            
        }else{
            // do nothing
        }
    }
 
    function validateEmail(email) {
        var re = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
        return re.test(email);
    }
    
    function showPhoneblock(){
        if(jQuery('#ff_extensionField_Phone2Number').hasClass('show1') == 1){
            jQuery('#ff_extensionField_Phone2Number').removeClass('show1').addClass('show2');
            jQuery('#f_extensionField_Phone2').val('');
            jQuery('#extensionField_Phone_block').hide();
            jQuery('#extensionField_Phone2_block').show();
