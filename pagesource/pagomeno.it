<script type="text/javascript">


$('.tabbed_box_tabs').click(function(o) {
$(this).children().removeClass('active');
var li = $(o.target).is("li") ? o.target : o.target.parentNode;
var index = $(li).index();
var id = $(li).parent().attr('id');
$(li).addClass('active');
$(this).parent().find('> div').addClass('hidden');
$(this).parent().find('> div[data-index=' + li.getAttribute('data-index') + ']').removeClass('hidden');
if (id) {
var date = new Date();
date.setTime(date.getTime() + (3600 * 24 * 365 * 1000));
document.cookie = id + '=' + index + '; expires=' + date.toGMTString() +  '; path=/';
}
});


$('.tabbed_box_tabs').each(function(index, ul) {
var id = $(ul).attr('id');
if (id) {
var regexp = new RegExp(id + "=([0-9]+)(;|)");
var match = document.cookie.match(regexp);
if (match) {
index = parseInt(match[1], 10) + 1;
$('#' + id).find('li:nth-child(' + index + ') > div').click();
}
}
});

</script>



