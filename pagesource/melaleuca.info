        $minicartWrapper.append($adWrapper);

        $anchor.on('click', function(e){
            e.preventDefault();
            Melaleuca.Web.AjaxManager.AddProductToCart( confObj.selectedAd.sku, 1, confObj.returnSource, null, rtnAddToCart);

            var myClickCMString = 'cm_sp=minicartAds-_-' + confObj.cGroup + '-_-' + confObj.selectedAd.adname;

            cmCreateManualLinkClickTag(myClickCMString,confObj.cGroup,'MyCart');


        });

        cmCreateManualImpressionTag('MyCart', myCMString);

    }





    function getPoints() {
        return parseInt( $('.th7').find('span').text() );
    }





    function removeAd() {
        var $minicartWrapper = $('#myCartListv2');
        $minicartWrapper.find('.minicart-ad').remove();
    }





    function getSkusInCart() {
        var temp = $('td.sku').find('span').text().trim().split(' ');

        if (temp.length > 0) {
            return temp;
        } else {
            return false;
        }
    }





    function addToStorage(sku) {
        var temp = JSON.parse(sessionStorage.getItem('ads_seen'));
        if (temp){
            if (temp.indexOf(sku) === -1) {
                temp.push(sku);
            }
        }else {
            temp = [sku];
        }
        sessionStorage.setItem('ads_seen', JSON.stringify(temp));
    }





    function getStorage() {
        return JSON.parse(sessionStorage.getItem('ads_seen'));
