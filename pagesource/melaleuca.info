    function loadScripts() {
        var _ads,_jLinq;
        if (! _cache.banners) {
            _ads = $.getScript('/ResourceFiles/JS/adArray_checkout_js');
        } else {
            _ads = _cache.ads;
        }

        if (! _cache.jLinq) {
            _jLinq = $.getScript('//cdnus.melaleuca.com/JS/Adwords/myJlinqSearchTest.js');
        } else {
            _jLinq = _cache.jLinq;
        }

        return {_ads: _ads, _jLinq: _jLinq};
    }





    function configure() {

        var custPoints = getPoints();

        // increment how many ads have been seen
        confObj.adsSeen++;

        confObj.skusInCart = getSkusInCart();

        if (custPoints <= 30){
            confObj.minP = 0;
            confObj.maxP = 30;
            confObj.cGroup = 'g30';
        } else if (custPoints >= 35){
            confObj.minP = 35;
            confObj.maxP = 400;
            confObj.cGroup = 'g35';
        } else {
            confObj.cGroup = 'gInt';
        }
        confObj.hasMetPPC = parseInt(PageClientData.MonthlyPPCommitment) <= custPoints ? 1 : 0;

        // this is getting all the ads from the adArray_checkout_js page that match the skus in cart
        if(confObj.skusInCart){ confObj.numMatch = _cache.jLinq.from(_cache.banners).search('skuKeywords',confObj.skusInCart.join(' '), _cache.jLinq.UseWeightedSorting | _cache.jLinq.WholeWordOnly).equals('CMP', 'U').select();};

    }





    function selectAd(){
        if (confObj.numMatch.length === 0){
            if (confObj.hasMetPPC){
                confObj.adList = _cache.jLinq.from(_cache.banners).equals('CMP', '1').select();
            } else {
                confObj.adList = _cache.jLinq.from(_cache.banners).between('points', confObj.minP, confObj.maxP).equals('CMP', '0').select();
            }
            if (confObj.skusInCart) {
                confObj.adList = removeDuplicateSkus(confObj.adList, confObj.skusInCart);
            }
            confObj.selectedAd = confObj.adList[Math.floor(Math.random()*confObj.adList.length)];
        } else {
            confObj.adList = confObj.numMatch;
            if (confObj.skusInCart) {
                confObj.adList = removeDuplicateSkus(confObj.adList, confObj.skusInCart);
            }
            confObj.selectedAd = confObj.adList[0];
        }


        // if no ad is found use default or 7911
        // if (confObj.adList.length === 0){
        //     confObj.selectedAd = _cache.jLinq.from(_cache.banners).originalList.filter(function(o){
        //         return o.m_default;
        //     });

        //     // in case no default parameter is found fall back to 7911
        //     if (confObj.selectedAd.length === 0) {
        //         confObj.selectedAd = _cache.jLinq.from(_cache.banners).equals('sku', 9756).select()[0];
        //     } else {
        //         confObj.selectedAd = confObj.selectedAd[0];
        //     }
        // }

        if (confObj.selectedAd){
            return true;
        } else {
            return false;
        }
    }





