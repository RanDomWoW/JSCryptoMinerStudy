	<script type="text/javascript">
	  
		$('#notification1').addClass("active");


		var $btnLoad = $('#UpdateNewsNo');

		// isTabClick refer to user navigate between Tabs
		function getNews(category, isTabClick) {
		   

			var storeLoadedNewsIds = "-1";
			$('#storeSelectedTab').val('tab_' + category);
			var _getDiv = $('#tab_default_' + category);
			var _getCurrentDivNewsNo = _getDiv.attr('data-itemsNo');
			// if user click tab that already contains news ,then display news without any operation
			// if user click tab that not have any data yet , then load 6 news
			if (isTabClick == true && _getCurrentDivNewsNo != 0)
				return;
			if (!$(_getDiv).is(':empty')) {

				if (_getCurrentDivNewsNo == 24) {
					$btnLoad.button('reset');
					return;
				}
				storeLoadedNewsIds = _getDiv.attr('data-newsIds');
			}

			var _oneNews = '<div class="col-md-4 col-sm-6">';
			_oneNews += '<section>';
			_oneNews += '<a class="zoom" title="{title}" href="ViewNews/{id}/{titleWithDash}.html" data-gallery="">';
			_oneNews += '<img class="img-responsive" src="http://www.du.edu.eg//arImageHandler.ashx?img={id}"><span class="overlay">';
			_oneNews += '<i class="fa fa-eye"></i></span>';
			_oneNews += '<div class="date">';
			_oneNews += '<span class="day">{day}</span>';
			_oneNews += '<span class="month">{month}</span>';
			_oneNews += '</div>';
			_oneNews += '<span class="news-section">{cat}</span>';
			_oneNews += '</a>';
			_oneNews += '<div class="h-news-content">';
			_oneNews += '<a href="ViewNews/{id}/{titleWithDash}.html"><h3>{title}</h3></a>';
			_oneNews += '<p>';
			_oneNews += '{body} ';
			_oneNews += '</p>';
			_oneNews += '</div>';
			_oneNews += '</section>';
			_oneNews += '</div>';

			$btnLoad.button('loading');
		
			$.ajax({
				method: "post",
				url: "home_newd.aspx/getNewsData",
				contentType: "application/json",
				data: JSON.stringify({ category: category, InsideUn: $('#ctl00_ContentPlaceHolder1_UniOnly').val(), TabActiveNewsIds: storeLoadedNewsIds }),
				dataType: "json",
				success: function (data) {
					OnSuccess(data);
				},
				error: function (request, status, error) {
					$btnLoad.button('reset');
				}
			});
			function OnSuccess(data) {

			
				var _itemsId  = '' ; 
				if (_getDiv.attr('data-newsIds'))
				{
					_itemsId = _getDiv.attr('data-newsIds');
				}
				var _newsBind = '<div class="row">';
				$.each(data.d, function (index, item) {
					_itemsId += item.NewsId + ',';

					
					var createTitleWithDash = item.NewsTitle.replace(/ /g, "-")
						.replace(/\./g, "-")
						.replace(/\\/g, '-')
						.replace(/\//g, '-')
						.replace(/"/g, '-');

			
					var _element = _oneNews.replace(/{title}/g, item.NewsTitle)
						.replace(/{rand}/g, item.NewsRandomId)
						.replace(/{id}/g, item.NewsId).replace('{day}', item.NewsDayId)
						.replace('{month}', item.NewsMonthName)
						.replace('{body}', item.NewsBody)
						.replace('{cat}', item.NewsContentTitle)
						.replace(/{titleWithDash}/g, createTitleWithDash);
					_newsBind += _element;
				});
				_newsBind += '</div>';              
				_getDiv.append(_newsBind);
				_getDiv.attr('data-itemsNo', parseInt(_getCurrentDivNewsNo) + 6);
				_getDiv.attr('data-newsIds', _itemsId);
				$btnLoad.button('reset');
			}

		}

		$(document).ready(function () {
			var _ulTabs = $('#ulNewsTabs');
			var _divsnews = $('#divs_news');
			var _staticLi = '<li onclick="getNews({},true);" id="tab_{}"><a href="#tab_default_{}"  data-toggle="tab">{}</a></li>';
			var _staticDiv = '<div  class="tab-pane fade in" id="tab_default_{}" data-itemsno=0></div>'

			var _staticLiMore = '<li class="drop-cat-more dropdown pull-left" style="display:none"><a href="#" data-toggle="dropdown">المزيد<span class="caret"></span></a><ul class="dropdown-menu animated flipInX" role="menu">';
			$.ajax({

				method: "post",
				url: "home_newd_NewDevelop.aspx/getTabsData",
				contentType: "application/json",
				data: "{}",
				dataType: "json",
				success: function (data) {
					OnSuccess(data);

				},
				error: function (error) {
					alert(error);
				}
			});
			function OnSuccess(data) {

				_ulTabs.empty();

				$.each(data.d, function (index, item) {

					var _li = _staticLi.replace('{}', item.TabId).replace('{}', item.TabId).replace('{}', item.TabId).replace('{}', item.TabTitle);
					if (index >= 5)
						_staticLiMore += _li;
					else
						_ulTabs.append(_li);
					var div = _staticDiv.replace('{}', item.TabId);
					_divsnews.append(div);
				});
				_staticLiMore += '</ul></li>';
				_ulTabs.append(_staticLiMore);
				
				
				$('#tab_0').attr('class', 'active');
				$('#tab_default_0').attr('class', 'tab-pane fade in active');
				$btnLoad.button('loading');                
				$('#storeSelectedTab').val('tab_0');
				getNews(0, true);
			}
			$('#UpdateNewsNo').click(function () {
				var activeTab = $('#storeSelectedTab').val();
				var lastIndex = activeTab.lastIndexOf("_")
				var s1 = activeTab.substring(0, lastIndex);
				var getCatId = activeTab.substring(lastIndex + 1);
				var _getDiv = $('#tab_default_' + getCatId);
				var _checkCurrentDivNewsNo = _getDiv.attr('data-itemsNo');
				if (_checkCurrentDivNewsNo == $('#MaxAllowedNewsNo').val())
					window.location.href = '/newd_du/pages/newsArchieveNew.aspx';
				$btnLoad.button('loading');
				$('#' + activeTab).attr("onclick", "getNews(" + getCatId + ",false)");
				getNews(getCatId, false);
			});
		});

	</script>
	


