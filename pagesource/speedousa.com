<script>
var _searchType=10;

$(function()
{
	if(typeof SPUtil!=U.UNDEFINED)
	{
		SPUtil.submitSearchForm=U.submitSearchForm=function($searchInput,$CatalogSearchForm)
		{
			var placeholderText=$searchInput.attr(U.PLACEHOLDER);
			var val=$.trim($searchInput.val());
			if(U.isNotNull(val) && (U.isNull(placeholderText) || val!=placeholderText))
			{
				var escapedVal=U.removeSpecialCharacters(val);
				$searchInput.val(escapedVal);
				
				var spacelessWord=/^\S*$/;
				if(escapedVal.match(spacelessWord)) $CatalogSearchForm.children('input[name="searchType"]').val(_searchType);
				
				$CatalogSearchForm.submit();
				return true;
			} else
			{
				$searchInput.focus();
				return false;
			}
		}
		
		$('#CatalogSearchForm .searchInput').on('keypress',function(event)
		{
			if(event.keyCode==13)
			{
				var $this=$(this);
				var $form=$this.parents('#CatalogSearchForm');
				if(!U.submitSearchForm($this,$form)) return false;
			}
		});
	}
});
</script>
<!-- END - Search fix - B2C18Q1-573 -->


