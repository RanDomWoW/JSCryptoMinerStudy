		/* Goggles drop down - END */

		/* Sale drop down - START */
		var $sale=$(SPHeader.NAV_ITEM_SELECTOR+'.sale');
		var length=$sale.find('.mega-menu').length;
		if(length==1)
		{
			var $menu1=$sale.find(SPHeader.MENU_SELECTOR);
			var $menu2=$menu1.clone().empty();
			$menu1.after($menu2);
				
			$menu2.prepend($menu1.children().last());
