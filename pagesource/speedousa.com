		/* Accessories drop down - START */
		var $accessories=$(SPHeader.NAV_ITEM_SELECTOR+'.accessories');
		if($accessories.find('.mega-menu').length>1)
		{
			var $menu1=$accessories.find(SPHeader.MENU_SELECTOR+':first-child');
			var $menu2=$menu1.next(SPHeader.MENU_SELECTOR);
			$menu2.prepend($menu1.children(':last-child'));
			SPHeader._setDropDownWidth($accessories.children(SPHeader.DROP_DOWN_SELECTOR));
		}
		/* Accessories drop down - END */
		
		/* Goggles drop down - START */
		var $goggles=$(SPHeader.NAV_ITEM_SELECTOR+'.goggles');
		if($goggles.find('.mega-menu').length>1)
		{
			var $menu1=$goggles.find(SPHeader.MENU_SELECTOR+':first-child');
			var $menu2=$menu1.next(SPHeader.MENU_SELECTOR);
			if($menu2.length)
			{
