    function fn_initialize() {
        // 同步设置域名选项
        $('#endomain, #cndomain, #qydomain').click(function(e){
            if($(e.target).is(':checkbox')){
                var formId = "#" + e.currentTarget.id;
                var relevanceCheckbox = $(formId + " :checkbox[value='" + e.target.value + "']");
                relevanceCheckbox.each(function(){
                    this.checked = e.target.checked;
                });
            }
			showtld();
        });

        tab($(".sea-tab .tab-tit li"),$(".tab-con>li"));  //banner 域名选项卡
        s_Div($(".dom-sel"),$("body"),$(".domain-con"), "li.ind-bg");  //banner 域名搜索
        in_jd($(".sea-input input"));  //input 获得焦点与失去焦点
    }

    function fn_CheckChinese(domain) {
        return /[\u4E00-\u9FA5]+/.test(domain);
    }

    // 检查提交域名查询的参数的合法性.
    function fn_CheckWhoisForm(type, form) {
        if (!fn_CheckRequired (form.domainname , "域名" )) {
            return false;
        }

        if (type == 2) { // 中文域名
            if (!fn_CheckChinese(form.domainname.value)) {
                alert("中文域名请填写中文!!");
                return false;
            }
        }
        else if (! fn_ValidateDomainNameOnly (form.domainname , "域名" )) {
            return false;
        }
        return true;
    }

    function showtld(){
		var en = '';
		var encheck = $("#en").children("li").children("label");;
		if(encheck.children(":checkbox[checked='checked']").length > 0){
			en = encheck.children(":checkbox[checked='checked']").first().val();
		}else{
			en = encheck.children(":checkbox").first().val();
		}
		$("#enchoiced").text(en);
		var cn = '';
		var cncheck = $("#cn").children("li").children("label");;
		if(cncheck.children(":checkbox[checked='checked']").length > 0){
			cn = cncheck.children(":checkbox[checked='checked']").first().val();
		}else{
			cn = cncheck.children(":checkbox").first().val();
		}
		$("#cnchoiced").text(cn);
		var qy = '';
		var qycheck = $("#qy").children("li").children("label");;
		if(qycheck.children(":checkbox[checked='checked']").length > 0){
			qy = qycheck.children(":checkbox[checked='checked']").first().val();
		}else{
			qy = qycheck.children(":checkbox").first().val();
		}
		$("#qychoiced").text(qy);
	}

