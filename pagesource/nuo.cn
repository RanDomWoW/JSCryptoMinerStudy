		$(this).find('ul.sub-nav').addClass('displayBlock');
	})
	$('.menu-ul li.list').mouseout(function(){
		$(this).find('ul.sub-nav').removeClass('displayBlock');
	})

	$('.menu-ul li.list').eq(1).mouseover(function(){
		$(this).find('ul.sub-nav').css('left',0);
	})
