<script type="text/javascript">
	function changeToolbarSubjectList(subjectType) {
		$("#toolbarSubjectCode").hide();
		$("#toolbarSubjectCodeKeyText").hide();
		$("#toolbarSubjectCodeCredit").hide();
		$("#toolbarSubjectCodeNonCredit").hide();

		if ($("input[id='toolbarSubjectTypeCreditInd']").getValue() == "true" && $("input[id='toolbarSubjectTypeNonCreditInd']").getValue() == "true") {
			$("#toolbarSubjectCode").show();
			$("#toolbarSubjectCodeKeyText").show();
		} else {
			if ($("input[id='toolbarSubjectTypeCreditInd']").getValue() == "true" && $("input[id='toolbarSubjectTypeNonCreditInd']").getValue() == "") {
				$("#toolbarSubjectCodeCredit").show();
			} else if ($("input[id='toolbarSubjectTypeCreditInd']").getValue() == "" && $("input[id='toolbarSubjectTypeNonCreditInd']").getValue() == "true") {
				$("#toolbarSubjectCodeNonCredit").show();
			} else {
				if (subjectType == 'C') {
					$("input[id='toolbarSubjectTypeNonCreditInd']").setValue("true");
					$("#toolbarSubjectCodeNonCredit").show();
				} else {
					$("input[id='toolbarSubjectTypeCreditInd']").setValue("true");
					$("#toolbarSubjectCodeCredit").show();
				}
			}
				
		}
	}
	
</script>

