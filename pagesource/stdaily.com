    /** 获取当前时间一周前日期 **/
  function getLastWeekTimestamp(){
      return new Date(new Date()-7*24*3600*1000).getTime();
  };
  /** 获取当前时间一月前日期 **/
  function getLastMonthTimestamp(){
      var date=new Date();
      date.setMonth(date.getMonth()-1);
      return date.getTime();
  };
  /** 获取range开始时间 **/
  function getRangeStartDate(){
      var date = $('#control_date').text();
      var test = $('#searchStart').text();
      if(date == null || date.length <= 0){
          return "";
      }
      return new Date(date.replace(/-/g, "/")).getTime();
  };
  /** 获取range结束时间 **/
  function getRangeEndDate(){
      var date = $('#control_date2').text();
      var test = $('#searchEnd').text();
      if(date == null || date.length <= 0){
          return "";
      }
      return new Date(date.replace(/-/g, "/")).getTime();
  };
  /** 判断range开始结束时间是否正确 **/
  function judgeDate(startDate, endDate){
      if(startDate.length <= 0 || endDate <= 0 || startDate > endDate){
        return false;
      }
      return true;
  };
  
