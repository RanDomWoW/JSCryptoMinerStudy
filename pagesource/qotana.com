<script>
var myVideo = document.getElementById("myVideo");
$(".playBtn").click(function(){
	myVideo.style.display="block"
	$(".closeVideo").show();
	myVideo.play();
});
$(".closeVideo").click(function(){
		myVideo.currentTime = 0;
		myVideo.pause();
		myVideo.style.display="none";
		$(this).hide();
		$(".ztBtn").hide();
});
$("#myVideo").click(function(){
	if (myVideo.paused) {
		
		$(".ztBtn").hide();
		myVideo.play(); 
	}else {
		myVideo.pause(); 
		$(".ztBtn").show();
	}
}); 
</script>
<script>
navNum =0;
$(function(){
	var no = 0;
	var Len = $("#banner ul li").length;
	$("#banner ol li").click(function(){
		no = $(this).index();
		$("#banner ol li").eq(no).addClass("bannerCur").siblings().removeClass("bannerCur");
		bannerEffect();
	})
	function bannerEffect(){
		if(no<0){no = Len-1}
		if(no>Len-1){no = 0}
		$("#banner ul li").eq(no).fadeIn(800).siblings().fadeOut(800);
	}
	var t= setInterval(function(){
		no++;
		if(no>Len-1){no = 0}
		$("#banner ol li").eq(no).addClass("bannerCur").siblings().removeClass("bannerCur");
		$("#banner ul li").eq(no).fadeIn(800).siblings().fadeOut(800);	
	},5000)
})
</script>
