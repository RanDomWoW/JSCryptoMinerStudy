		}
		
		//var currentUserId = getCookieName_BeginningWith("WC_USERACTIVITY_").split("WC_USERACTIVITY_")[1];
		//if(dojo.byId('MiniShoppingCart') != null && !multiSessionEnabled && (enableToLoad == "true" || enableToLoad == true) && passwordForm == null && currentUserId != '-1002'){
		if(dojo.byId('MiniShoppingCart') != null && !multiSessionEnabled && (enableToLoad == "true" || enableToLoad == true) && passwordForm == null){
			loadMiniCart("E19","-3");
		}
	});
		
		require(["dojo/on", "dojo/dom", "dojo/domReady!"],
				function(on, dom) {
					var minishopcart = dojo.byId('MiniShoppingCart');
					minishopcart.wastouched = false;

					/** the standard event names have to be used here to maintain the order they are fired **/
					on(minishopcart, "touchstart", function(evt){
						minishopcart.wastouched = true;
					});
					on(minishopcart, "mouseenter", function(evt){
						if(!minishopcart.wastouched) {
							showMiniShopCartDropDown('widget_minishopcart', 'quick_cart_container', 'orderItemsList');
						}
					});
					on(minishopcart, "mouseleave", function(evt){
						if(!minishopcart.wastouched) {
							hideMiniShopCartDropDown('quick_cart_container');
						}
					});
					on(minishopcart, "onmouseup", function(evt){
						minishopcart.wastouched = false;
					});
				}
		);
		
</script>
