		if (miniCartContent == "true" || miniCartContent == true || passwordForm != null){
			setMiniShopCartControllerURL(getAbsoluteURL()+'MiniShopCartDisplayView?storeId=10151&catalogId=10051&langId=-3&miniCartContent=true');			
			wc.render.getRefreshControllerById("MiniShopCartContentsController").url = getAbsoluteURL()+'MiniShopCartDisplayView?storeId=10151&catalogId=10051&langId=-3&page_view=dropdown&miniCartContent=true';
