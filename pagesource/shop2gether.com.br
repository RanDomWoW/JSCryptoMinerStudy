function getFooterSize(){
	dynamicH = (jQuery('.footer-container').height() - jQuery('.footer .block-subscribe').height()) * -1;
	dynamicH += 'px';
	return dynamicH;
}

/*
 * Centraliza o Megamenu na Págna
 */
function megaCenter(){
	var megasum = 0;
	var marginChildrenLevel0 = 10;
	
	jQuery('#nav').children('.level0').each(function(){
		jQuery(this).css('margin-left', '');
		jQuery(this).css('margin-right', '');
		megasum += (jQuery(this).outerWidth() + (marginChildrenLevel0*2));
	});
	
	// Verifica se o tamanho do Megamenu é Maior que a Página
	megasum = verificaTamanhoMenu(megasum, marginChildrenLevel0);
	
	if(jQuery(window).width() < 1200){
		megasum = (jQuery(window).width() - megasum)/2;
	}else{
		megasum = (1200 - megasum)/2;
	}
	megasum += 'px';
	jQuery('.megamenu #nav li:eq(0)').css('marginLeft',megasum); 
}

/*
 * Verifica se o tamanho do Megamenu é Maior que a Página
 */
function verificaTamanhoMenu(menuWidth, marginChild){
	var megasum,
		windowWidth,
		newMenuWidth,
		newMarginWidth;
	
	windowWidth = jQuery(window).width();
	if(menuWidth > windowWidth){
		// Calcula o Novo tamanho do Menu com a Metade da Margim Lateral dos Filhos
		megasum 	   = 0;
		newMarginWidth = parseInt(marginChild/2);
		jQuery('#nav').children('.level0').each(function(){
			jQuery(this).css('margin-left', newMarginWidth);
			jQuery(this).css('margin-right', newMarginWidth);
			megasum += (jQuery(this).outerWidth() + (newMarginWidth*2));
		});
		
		newMenuWidth = verificaTamanhoMenu(megasum, newMarginWidth);
		menuWidth	 = newMenuWidth;
	}
	
	return menuWidth;
}



