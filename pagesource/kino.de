<script type="text/javascript">
require(['vendor/jquery', 'module/user', 'module/kino.api', 'module/kino'], function(jQuery, UserModule, Api, KinoFeature) {
  jQuery(document).ready(function() {
    jQuery('[data-action="render-user-data"]').each(function() {
      var node = jQuery(this);
      var tmplPath = node.attr('data-template');
      var loadingText = node.attr('data-loading');
      var randId = 'renderUserData_'+Math.round(Math.random()*1000+1);

      var wnd = jQuery(window);
      var body = jQuery('body');
      var loaded = false;

      var nodeIsReallyVisible = function(node) {
        var visible = true;
        if ( node.css('visibility') === 'hidden' || node.css('display') === 'none') {
          visible = false;
        } else {
          node.parents().each(function() {
            var parent = jQuery(this);
            if ( parent.css('visibility') === 'hidden' || parent.css('display') === 'none') {
              visible = false;
              return false; // break each
            }
          });
        }
        return visible;
      };

      var isInViewport = function() {
        var wt = wnd.scrollTop();
        var wb = wt + wnd.height();
        var et = node.offset().top;
        var eb = et + node.outerHeight(true);
        return nodeIsReallyVisible(node) && !!((wt < eb && eb < wb) || (wt < et && et < wb));
      };

      if ( loadingText ) {
        node.html(loadingText);
      }

      var load = function() {
        if ( loaded ) {
          return;
        }
        if ( !isInViewport()) {
          return;
        }

        loaded = true;
        wnd.off('scroll.'+randId);
        body.off('elementChange.'+randId);

        UserModule.addStateCallback('READY', function(/*data*/) {
          Api.request('/project/account/post-view/render/', UserModule.modifyRequestData({
            path: tmplPath
          }), {
            success: function(data) {
              node.html(data.html);
              node.show();
              KinoFeature.NodeReady(jQuery('body')); // body neu parsen.
            }
          });
        });

      };

      wnd.on('scroll.'+randId, load);
      body.on('elementChange.'+randId, load);
    });
  });
});
</script>


