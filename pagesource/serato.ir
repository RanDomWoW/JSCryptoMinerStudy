    <script type="text/javascript">
        $('.counter').each(function () {
            var $this = $(this);
            var countDown = $this.flipcountdown({
                size: 'sm',
                showDay: true,
                beforeDateTime: $this.attr('data-date')
            });

            $this.parents('.bid-container').data('countDown', countDown);
        });
</script>

<script src="/scripts/jquery.signalR-2.2.2.min.js"></script>
<script src="/signalr/hubs"></script>

<script type="text/javascript">
        
        $('.raise-btn')
            .on('click',
                function (e) {
                    e.preventDefault();
                    var $this = $(this);

                    $.ajax({
                        type: 'POST',
                        url: '/bids/raise',
                        data: { bidId: $this.attr('data-id') },
                        success: function (d) {
                        },
                        error: function (e) {
                            if (e.status === 401) {
                                document.location.href = '/login';
                                return;
                            }

                            if (e.responseJSON != null && e.responseJSON.message === 'nobalance') {
                                if (window.handleNoBalance) {
                                    window.handleNoBalance();
                                    return;
                                }

                                $('#no-balance-modal').modal();
                                return;
                            }

                            console.log(e);
                        }
                    });
                });

        $(function() {
            var bidHub = $.connection.bidHub;

            bidHub.client.received = function(ids) {
                if (!ids || ids.length === 0)
                    return;

                for (var i = 0; i < ids.length; i++) {
                    $('div[data-id=' + ids[i] + ']').find('.sold').removeClass('none');
                }
            };

            bidHub.client.updateBid = function (id, total, name, changedTime, time) {
                var bidContainer = $('.bid-container[data-id=' + id + ']');
                var bidPrice = bidContainer
                    .find('.bid-price').text(total);

                bidPrice.fadeOut(200).fadeIn(300);
                bidContainer.find('.product-title').fadeOut(200).fadeIn(300);
                bidContainer.find('.bid-name').text(name).fadeOut(200).fadeIn(300);

                if (!changedTime)
                    return;


                bidContainer.find('.counter').data().setOptions({ beforeDateTime: time });
            };


            bidHub.client.notFound = function () {
                alert('مزایده موردنظر یافت نشد.');
            };

            bidHub.client.emptyAccount = function() {
                alert('حساب شما برای انجام عملیات کافی نمی باشد.');
            };

            $.connection.hub.start()
                .done(function() {
                    $('#bids').show();
                });
        });
</script>


    <!-- Modal -->
