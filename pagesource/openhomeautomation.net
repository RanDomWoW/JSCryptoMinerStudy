    <script>
    $(document).ready(function() {

        // Debug?
        var debugMode = 'off';

        // Popup
        if ($('#exit-popup').length) {

            $.ajax({
                url: "/api/popups//show",
                type: "GET",
                crossDomain: true,
                dataType: "json",
                complete: function(data) {

                    // Get answer
                    var showPopup = data.responseJSON.show;

                    // Desktop
                    $(document).on('mousemove', function(event) {

                        // Detect exit intent
                        if (event.pageY < 10) {

                            // Check cookie
                            var popupCookie = Cookies.get('se-popup-');

                            if (showPopup && (!popupCookie || debugMode == 'on')) {

                                // Show
                                $('#exit-popup').modal('show');

                                // Set cookie for this page
                                Cookies.set('se-popup-', 'fired', {
                                    expires: parseInt('30')
                                });
                            }

                        }

                    });

                    // Mobile
                    if (/Mobi/.test(navigator.userAgent)) {

                        var scrollTrigger = false;

                        // Check scroll 
                        $(window).unbind('scroll');
                        $(window).scroll(function() {

                            // Get percent
                            var percent = $(window).scrollTop() / $(document).height() * 2 * 100;

                            // Trigger
                            if (percent > 50) {
                                scrollTrigger = true;
                            }

                            console.log(percent);

                            // Show
                            if (percent < 45) {

                                // Check cookie
                                var popupCookie = Cookies.get('se-popup-');

                                if (showPopup && scrollTrigger && !popupCookie) {

                                    // Show
                                    $('#exit-popup').modal('show');

                                    // Set cookie for this page
                                    Cookies.set('se-popup-', 'fired', {
                                        expires: parseInt('30')
                                    });
                                }
                            }

                        });

                    }

                }
            });

        }

