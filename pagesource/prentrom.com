            detectNonAdobeIE = 1;

            function setTarget($)
            {
                var status;
                
                if ($.isMinVersion)
                {
                    status = $.isMinVersion("PDFReader", 0);
                    // PDFReader is enabled
                    if (status == 0)
                    {
                      var allAnchors = document.getElementsByTagName("a");
                      for(var i = 0; i < allAnchors.length; i++) {
                        if (allAnchors[i].getAttribute('href') !== null)
                        {
                          var href = allAnchors[i].getAttribute('href');
                          // Anchor contains .pdf, change target to _blank
                          if (href.indexOf('.pdf') !== -1)
                          {
                            allAnchors[i].setAttribute('target', '_blank');
                          }
                        }
                      }
                    }
                    // PDFReader is disabled
                    else
                    {
                      var allAnchors = document.getElementsByTagName("a");
                      for(var i = 0; i < allAnchors.length; i++) {
                        if (allAnchors[i].getAttribute('href') !== null)
                        {
                          var href = allAnchors[i].getAttribute('href');
                          // Anchor contains .pdf, change target to _self
                          if (href.indexOf('.pdf') !== -1)
                          {
                            allAnchors[i].setAttribute('target', '_self');
                          }
                        }
                      }
                    }
                }
            };

            $.onDetectionDone("PDFReader", setTarget, DummyPDF, detectNonAdobeIE);

          };

