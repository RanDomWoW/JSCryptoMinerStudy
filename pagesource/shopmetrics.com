<script type="text/javascript">
	
	function isValidEmail ( strEmail )
	{
		validRegExp = /^[^@]+@[^@]+.[a-z]{2,}$/i;

		// search email text for regular exp matches
		if (strEmail.search(validRegExp) == -1) 
		{
			return false;
		} 

		return true; 
	}

	function validateForm ()
	{
		var retValue = true;

		var name = document.getElementById ( 'footer-name-field' );
		var email = document.getElementById ( 'footer-email-field' );
		var message = document.getElementById ( 'footer-message-field' );

		if ( name.value === '' )
		{
			name.style.border = '1px solid red';
			retValue = false;
		} else
		{
			name.style.border = '0px none';
		}
		
		if ( message.value === '' )
		{
			message.style.border = '1px solid red';
			retValue = false;
		} else
		{
			message.style.border = '0px none';
		}

		if ( email.value === '' )
		{
			email.style.border = '1px solid red';
			retValue = false;
		} else if ( ! isValidEmail ( email.value ) )
		{
			retValue = false;
		} else
		{
			email.style.border = '0px none';
		}

		return retValue;
	}
</script>
