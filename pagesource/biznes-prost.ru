<script>
	jQuery(document).ready(function($){
		var $menu = $("#head-nav");
			$(window).scroll(function(){
				if ( $(this).scrollTop() > 10 && $menu.hasClass("head-nav-top") ) {
					$menu.removeClass("head-nav-top").addClass("head-nav-fixed");
				} else if($(this).scrollTop() <= 10 && $menu.hasClass("head-nav-fixed")) {
						$menu.removeClass("head-nav-fixed").addClass("head-nav-top");
				}
			});//scroll
	});
	
		jQuery(document).ready(function($){
		var $menu = $("#promo");
		height = jQuery('.main-holder').height()/3;
			$(window).scroll(function(){
				if ( $(this).scrollTop() > height && $menu.hasClass("mobile-static") ) {
					$menu.removeClass("mobile-static").addClass("mobile-fixed");
				} else if($(this).scrollTop() <= height && $menu.hasClass("mobile-fixed")) {
						$menu.removeClass("mobile-fixed").addClass("mobile-static");
				}
			});//scroll
	});
</script>


