    function rotateArrow(clickElement) {
        if (!$(clickElement).find('.in').length) {
            $(clickElement).find('.btn-down').css({
                '-moz-transform': 'rotate(90deg)',
                '-webkit-transform': 'rotate(90deg)',
                '-o-transform': 'rotate(90deg)',
                '-ms-transform': 'rotate(90deg)',
                'transform': 'rotate(90deg)'
            });
        }
        else {
            $(clickElement).find('.btn-down').css({
                '-moz-transform': 'rotate(0deg)',
                '-webkit-transform': 'rotate(0deg)',
                '-o-transform': 'rotate(0deg)',
                '-ms-transform': 'rotate(0deg)',
                'transform': 'rotate(0deg)'
            });
        }
    }

    function onResize() {
        var ww = $(window).width()
        if (ww <= 440) {
            if ($("#header").hasClass("index") || $("#header").hasClass("aqua") || $("#header").hasClass("newbaogia")) {
                $("#header #logo img").attr("src", "/Themes/Portal/Default/Images/bizweb_new_logo_short.png?v=201803200719");
                $("#header .small-logo img").attr("src", "/Themes/Portal/Default/Images/bizweb_new_logo_short.png?v=201803200719");
            }
            else {
                $("#header #logo img").attr("src", "/Themes/Portal/Default/Images/bizweb_old_logo_short.png?v=201803200719");
                $("#header .small-logo img").attr("src", "/Themes/Portal/Default/Images/bizweb_old_logo_short.png?v=201803200719");
            }
        }
        else {
            if ($("#header").hasClass("index") || $("#header").hasClass("aqua") || $("#header").hasClass("newbaogia")) {
                $("#header #logo img").attr("src", "/Themes/Portal/Default/Images/newlogo/bizweb_logo_new.svg?v=201803200719");
                $("#header .small-logo img").attr("src", "/Themes/Portal/Default/Images/newlogo/bizweb_logo_new.svg?v=201803200719");
            }
            else {
                $("#header #logo img").attr("src", "/Themes/Portal/Default/Images/newlogo/bizweb_logo_old.svg?v=201803200719");
                $("#header .small-logo img").attr("src", "/Themes/Portal/Default/Images/newlogo/bizweb_logo_old.svg?v=201803200719");
            }
        }
    }
    $(window).resize(function () {
