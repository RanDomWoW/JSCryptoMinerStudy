<script>
$(document).ready(function(){
	$('#ykt').click(function(){
	  $('html, body').animate({
			 scrollTop: $("#yakittuketimihesaplama").offset().top
		}, 1000);
	});

	$(document).on('click', '#calcAvg',function(){
		var fuelPrice = parseFloat($('#selectfuel').val());
		var avgFuel = $('#avg').val();
		avgFuel = parseFloat(avgFuel.replace(',','.'));
		if(fuelPrice != '' && fuelPrice > 0 && avgFuel != '' && avgFuel > 0)
		{
			var sonuc = fuelPrice*avgFuel;
			
			$('#r1').html('Kilometrede '+sonuc.toFixed(0)+' kuruş yakar');
		}
		else
		{
			$('#r1').html('Yakıt cinsi ve ortalama yakıt alanları doldurmanız gerekmektedir.');
		}
	});

	$(document).on('click', '#distanceCalc', function(){
		fuelPrice = 0;yer1 = 0;yer2 = 0;
		var fuelPrice = parseFloat($('#selectfuel').val());
		var avgFuel = $('#avg').val();
		avgFuel = parseFloat(avgFuel.replace(',','.'));
		var yer1 = parseFloat($('#selectstartcity').val());
		var yer2 = parseFloat($('#selectfinishcity').val());
		if(yer1 != '' && yer1 > 0 && yer2 != '' && yer2 > 0 && fuelPrice != '' && fuelPrice > 0 && avgFuel != '' && avgFuel > 0)
		{
			$.ajax({
				type: 'POST',
				url: '/mesafe-hesapla',
				data: 'c1='+$('#selectstartcity').val()+'&c2='+$('#selectfinishcity').val(),
				success: function(cevap)
				{
					var avg = avgFuel*fuelPrice/100;
					$('#r2').html(cevap+"  km'lik bu yolda aracınız "+((avg.toFixed(2))*cevap).toFixed(0)+' TL yakar');
				}
			});
		}
		else
		{
			$('#r2').html('Yakıt cinsini ortalama yakıtı ve başlangıç-bitiş yerlerini seçmeniz gerekmektedir.');
		}
	});
	$(document).on('change', "#brandSelect", function(){
		var val = $(this).val();
        $.ajax({
            type: "POST",
            data: 'brandID='+val,
            url: baseUrl+"kullanici/getseri",
            success: function (data) {
                $("#seriSelectArea").html(data);
                    
                $("#modelSelectArea").html('<select id="modelSelect" name="modelSelect"><option value="">Seçiniz</option></select>');
                $("#modelSelect").selectpicker();
             } 
        });
    });

    $('#fuelAvg, #secondFuelAvg').onlyFloat();


    $("#brandSelect").selectpicker({
        onChange: function (val, inst) {
            $.ajax({
                type: "POST",
                data: 'brandID='+val,
                url: baseUrl+"kullanici/getseri",
                success: function (data) {
                    $("#seriSelectArea").html(data);
                    $("#seriSelect").selectpicker();
                    
                    $("#modelSelectArea").html('<select id="modelSelect" class="form-control selectpicker" name="modelSelect"><option value="">Seçiniz</option></select>');
                    $("#modelSelect").selectpicker();
                } 
            });
        }, 
        effect: "slide"
    });

	$(document).on('change', "#seriSelect", function(){
		var val = $(this).val();
        $.ajax({
            type: "POST",
            data: 'seriID='+val,
            url: baseUrl+"kullanici/getmodel",
            success: function (data) {
                $("#modelSelectArea").html(data);
            } 
        });
    });
    
});

