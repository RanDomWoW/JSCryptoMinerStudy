<script type="text/javascript">		
		$(document).ready(function () {
            var oTab = $(".lbox")
            var aItem = oTab.find(".bigbox");
            var aTitle = aItem.find(".titlebox");
            var aSpan = aTitle.find("span");
            var aMain = aItem.find(".conbox");
         
            function jump(index) {
                aMain.stop().slideUp();
                aSpan.html('<img src="./images/gs_ico12.gif" />');
                aItem.eq(index).find(".conbox").stop().slideDown(function () {
                    $(this).prev(".titlebox").find("span").html('<img src="./images/gs_ico11.gif" />')
                });

            }

            aTitle.click(function () {
                var _this = $(this);

                jump($(this).parent().index());
            });

            jump(0);
        });

		 window.onload = function () {
            var oNav = document.getElementById("nav");
            var aLi = oNav.getElementsByTagName("li");
  
            for (var i = 0, l = aLi.length; i < l; i++) {
                aLi[i].onmouseover = function () {
                    var oDiv = this.children.length ? this.children[1] : null;
                    if (oDiv) {
                        oDiv.style.display = "block";
                    }
                }
                aLi[i].onmouseout = function () {
                    var oDiv = this.children.length ? this.children[1] : null;
                    if (oDiv) {
                        oDiv.style.display = "none";
                    }
                }

            }

        }

    </script>
<script type="text/javascript">
    function setTab(name, cursel, n) {
        for (var i = 1; i <= n; i++) {

            var menu = document.getElementById(name + i);
            var con = document.getElementById("con_" + name + "_" + i);
            menu.className = i == cursel ? "ved" : "";
            con.style.display = i == cursel ? "block" : "none";
        }
    }
</script>
<script>
function searet_onsubmit() 
{
	
	var SQL,pword,kong;
	var sqlword = new Array();
	    pword=kong=" ";
		SQL="";
	    pword=document.forms.word.value;
		pword = pword.replace(/^\s*/g,"");
		pword = pword.replace(/\s*$/g,"");
		pword = pword.replace(/\s+/g," "); 
                pword = pword.split( kong );			
                sqlword[0] = pword[0];
			if(sqlword[0]!="")	
	        SQL = "biao_ti="+sqlword[0];
	for(var j=1;j<pword.length;j++)
	    {
	    sqlword[j] = " and biao_ti="  + pword[j];
		SQL += sqlword[j];
		
		}
			
	document.forms.searchword.value=SQL;	
	if(SQL=="")
	{
	     alert("关键词不能为空!");
	     return false;
	}
         return true;  
         
}
</script>
<script>
function policyQuery() {
