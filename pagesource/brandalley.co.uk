<script type="text/javascript">
        //<![CDATA[          
        var loginForm = new VarienForm('login-form-pop');
        var urlToValidate = 'https://www.brandalley.co.uk/flashsales/index/validateemail/';
        urlToValidate = urlToValidate.replace('https:', '');
        urlToValidate = urlToValidate.replace('http:', '');

        /* ================== New Ajax login script start ===================== */
        var loginPostUrl = 'https://www.brandalley.co.uk/customer/account/loginpostajax/';
        loginPostUrl = loginPostUrl.replace('https:', '');
        loginPostUrl = loginPostUrl.replace('http:', '');
        function loginAjax() {
            jQuery('#popup-error').parent('li').hide();
            if (!loginForm.validator.validate()) {
                return false;
            } else {
                jQuery('#send2').prop("disabled", true);
                jQuery('#login-btn-text').html('Loading...');
                jQuery.ajax({
                    url: loginPostUrl,
                    type: "POST",
                    data: jQuery('#login-form-pop').serialize(true),
                    success: function (result) {
                        var result = jQuery.parseJSON(result);
                        if (result['flag']) {
                            
                            //User exists in Achica and BA both platforms,after successful login 
                            if(achicaData.hasOwnProperty('achica_user_found') && achicaData.achica_user_found == true && achicaData.ba_user_found == true){
                               jQuery('#newLogin').removeClass('acba');
                               jQuery('#newLogin').hide();
                               jQuery('#thankyou-message').show();
                               jQuery('#thankyou-message').addClass('acba');
                               jQuery('#thankyou-message.acba .acba-message.login-thank-you').show();
                               setTimeout(function(){ window.location.reload() }, 5000);
                            }else{
                               
                               if(result['flag'] == 2 || result['flag'] == 3){
                                   //Direct login from popup and customer exists in BA OR direct login from popup and customer didn't exist in BA earlier
                                    jQuery('#newLogin').removeClass('acba');
                                    jQuery('#newLogin').hide();
                                    jQuery('#thankyou-message').show();
                                    jQuery('#thankyou-message').addClass('acba');
                                    jQuery('#thankyou-message.acba .acba-message.login-thank-you').show();
                                    setTimeout(function(){ window.location.reload() }, 5000);
                               }else {
                                    window.location.reload();
                                }
                            }
                            
                        } else {
                            var errorMsg = result['error'].toString().split(',');
                            var errorHtml = '';
                            jQuery.each(errorMsg, function (n, message) {
                                if (errorHtml.length > 0) {
                                    errorHtml = errorHtml + '<span>' + message + '</span>';
                                } else {
                                    errorHtml = '<span>' + message + '</span>'
                                }
                            });
                            jQuery('#popup-error').html(errorHtml);
                            jQuery('#popup-error').parent('li').show();
                        }
                    }
                }).done(function () {
                    jQuery('#login-btn-text').html('Login');
                    jQuery('#send2').prop("disabled", false);
                });
            }
        }
        jQuery("#login-form-pop").submit(function (e) {
            e.preventDefault();
            e.stopImmediatePropagation();
            loginAjax();
        });
        
        var resetForm = new VarienForm('new-password-form');
        //]]>

        function showBox(eventTrigger) {
            if (eventTrigger == 'login') {
                jQuery('#signup-popup-show').hide();
                jQuery('#newLogin').show(0, function () {
                    containerH = jQuery(this).children('.login-container').height();
                    checkHeight();
                  
                });
                allowScroll();
                jQuery('#login-email').focus();
            } else if (eventTrigger == 'signup') {
                jQuery('#newLogin').hide();
                jQuery('#signup-popup-show').show(0, function () {
                    containerH = jQuery(this).children('.login-container').height();
                    checkHeight();
                 
                });
                allowScroll();
                jQuery('#email_address').focus();
            }
        }
    
    
    jQuery("#register-thanks-button").unbind('click').on('click', function () {
        if(  achicaData.hasOwnProperty('achica_user_found') && achicaData.achica_user_found == true){    
            jQuery(this).html('Loading...');    
            var registerThanksContinueUrl =    'https://www.brandalley.co.uk/achicalanding/incoming/continue/';
            jQuery.get(registerThanksContinueUrl, function(result) {
                var result = jQuery.parseJSON(result);
                if (result['flag'] == 1 && result['user_id'] > 0) {
                      popupFlag = 0;
                      window.universal_variable.user.user_id = result['user_id'];
                      window.dataLayer = window.dataLayer || [];
                      window.dataLayer.push({ 'event': 'thanks-for-registering' });
                }
                window.location.reload(); 
            })
        } else {
            window.location.reload(); 
        }    
    })
    </script>
