<script>
    $( document ).ready(function() {

        var CSRF_TOKEN = $('meta[name="csrf-token"]').attr('content');



        $(".errorPhone").click(function() {
            var prof_id = $(this).attr("data-profile-id");
            var teacher_id = $(this).attr("data-teacher-id");
            var data_phone = $(this).attr("data-phone");
            var current_page = "http://vneklass.com";

            if(prof_id){
                console.log(prof_id);
                $('input#record-id').val(prof_id);
            }

            if(teacher_id){
                console.log(teacher_id);
                $('input#record-id').val(teacher_id);
            }

            if(data_phone){
                console.log(data_phone);
                $( "p#insert_phone" ).text(data_phone);
                $( "#phone-text" ).val(data_phone);
            }

            if(current_page){
                console.log(current_page);
                $( "input#url-page" ).text(current_page);
                $('#url-page').val(current_page);
            }

            $('#phone_modal').modal('show')

        });

        $("#sendPhoneMsg").click(function() {
            var msg = $('#phone_msg').val();
                if(msg){
                    var page = $('#url-page').val();
                    var phone = $('#phone-text').val();
                    var recordid = $('#record-id').val();

                    $.ajax({
                        type: "POST",
                        url: '/errorphone',
                        data: {
                            _token: CSRF_TOKEN,
                            phone: phone,
                            page: page,
                            msg: msg,
                            recordid:recordid
                        },
                        success: function( msg ) {
                            //window.alert(msg);
                            $(".modal-body").empty();
                            $(".modal-header").empty();
                            $(".modal-body").html('<p class="alert-success" style="font-size: 20px; padding-left: 5px;">'+msg+'</p>');
                        }
                    });
                }
        });



    });
</script>
