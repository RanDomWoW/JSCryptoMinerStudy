input[type="submit"]:focus,
button:active,
html input[type="button"]:active,
input[type="reset"]:active,
input[type="submit"]:active {
border-color: #066;
background: #066;
}
input[type=password],
input[type=text],
textarea {
color: #666;
border: 1px solid #ddd;
border-radius: 2px;
}
input[type=text]:focus,
textarea:focus {
color: #111;
}
input[type=password],
input[type=text] {
padding: 5px 3px;
margin-left: 5px;
}
textarea {
padding-left: 3px;
