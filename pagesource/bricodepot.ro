    <script>
      function sendMessage () {
        var abonat = 0
        var modal = $('#warpecrmModal')

        if (modal.find('input[name="abonat"]').is(':checked'))
          abonat = 1

        modal.find('.mesaj_felicitari').hide()
        modal.find('.mesaj_eroare').hide()

        var email = modal.find('input[name="email"]').val()

        $.ajax({
          'url': '/ajax/exit/message',
          'method': 'POST',
          'data': {
            'email': modal.find('input[name="email"]').val(),
            'nume': modal.find('input[name="nume"]').val(),
            'prenume': modal.find('input[name="prenume"]').val(),
            'telefon': modal.find('input[name="telefon"]').val(),
            'mesaj': modal.find('textarea[name="mesaj"]').val(),
            'depozit': 11,
            'abonat': abonat,
            'campanie': 'warpecrm_exitpopup'
          }
        }).success(function (data) {
          if (data.validated === true) {
            modal.find('input[name="email"]').val('')
            modal.find('input[name="nume"]').val('')
            modal.find('input[name="prenume"]').val('')
            modal.find('input[name="telefon"]').val('')
            modal.find('textarea[name="mesaj"]').val('')

            modal.find('.mesaj_felicitari').show()

            var date = new Date()
            date.setTime(date.getTime() + (365 * 24 * 60 * 60 * 1000))
            jQuery.cookie('warpecrm_popup', 1, {
              path: '/',
              domain: jQuery(location).attr('hostname'),
              expires: date
            })

            if (abonat === 1) {
              Warp_sendData(618, {
                'email': email,
                'depozit':11,
                'pagina': window.location.href
              })
              Warp_reportEvent(618, Warp_EventTypeConversion)
            }
          }
          else {
            modal.find('.mesaj_eroare').text(data.errors).show()
          }

        })
      }

      $(function () {
        var $visitedProduct = jQuery.cookie('warpecrm_popup_product_visited')
        var $showPopup = 1
        if (typeof $visitedProduct === 'undefined') {
          var $showPopupInterval = setInterval(showPopup, 500)
        }

        function visitedProduct () {
          $showPopup = 0
          jQuery.cookie('warpecrm_popup_product_visited', 1, {
            path: '/',
            domain: jQuery(location).attr('hostname')
          })
          $visitedProduct = 1
          clearInterval($showPopupInterval)
          showPopup()
        }

        if (typeof $visitedProduct === 'undefined') {
          var $visitedProductTimeout = setTimeout(visitedProduct, 5000)
        }

        function showPopup () {
          console.log('popup')
          var $cookie = jQuery.cookie('warpecrm_popup')
          var modal = $('#warpecrmModal')
          if (!$cookie) {
            if ($showPopup === 1 || typeof $visitedProduct === 'undefined') {
              $.exitIntent('enable')
              $(document).bind('exitintent', function () {
                modal.modal('show')
              })
            }
            else {
              $.exitIntent('disable')
            }
          }

          modal.on('hidden.bs.modal', function () {
            if (typeof jQuery.cookie('warpecrm_popup') === 'undefined') {
              var date = new Date()
              date.setTime(date.getTime() + (7 * 24 * 60 * 60 * 1000))
              jQuery.cookie('warpecrm_popup', 1, {
                path: '/',
                domain: jQuery(location).attr('hostname'),
                expires: date
              })
            }
            $.exitIntent('disable')
          })

          $(document).unbind('click').on('click', '#warpecrmModal .bt_send', function (ev) {
            ev.preventDefault()
            modal.find('.mesaj_eroare').text('').hide()
            if (modal.find('input[name="email"]').val() === '')
              modal.find('.mesaj_eroare').text('Câmpul "Adresă email" nu conține o adresă de email.').show()
            else if (!validateEmail(modal.find('input[name="email"]').val()))
              modal.find('.mesaj_eroare').text('Te rugăm să introduci adresa de email corectă.').show()
            else if (modal.find('input[name="nume"]').val() === '')
              modal.find('.mesaj_eroare').text('Câmpul "Nume" este obligatoriu.').show()
            else if (modal.find('input[name="prenume"]').val() === '')
              modal.find('.mesaj_eroare').text('Câmpul "Prenume" este obligatoriu.').show()
            else if (modal.find('textarea[name="mesaj"]').val() === '')
              modal.find('.mesaj_eroare').text('Câmpul "Mesaj" este obligatoriu.').show()
            else
              sendMessage()

          })

        }

      })
    </script>
<div class="modal" id="SSNmodal">
    <div class="modal-dialog modal_popup">
        <div class="bt_close" data-dismiss="modal"><i class="icon-close"></i></div>
        <div class="modal-content">
            <a href="#"><img id="goto" src="https://www.bricodepot.ro/assets/campaigns/incepe_ceva_nou/images/popup_SSN_smith.jpg" alt=""></a>
        </div>
    </div>
</div>
<script>
  function setSSNModalCookie () {
    let date = new Date()
    date.setTime(date.getTime() + (365 * 24 * 60 * 60 * 1000))
    jQuery.cookie('gama-noua', 'smith-and-locke', {
      path: '/',
      domain: jQuery(location).attr('hostname'),
      expires: date
    })
  }

  jQuery(document).ready(function ($) {
    setTimeout(function () {
      $('#SSNmodal').modal('show')
    }, 5000)
    $('#SSNmodal').on('hidden.bs.modal', function () {
      setSSNModalCookie()
    })
    $('#SSNmodal').on('click', '#goto', function (ev) {
      ev.preventDefault()
      setSSNModalCookie()
      document.location = '/momentul-de-inspiratie.html'
    })
  })
</script></body>
