<script type="text/javascript">
var friendList={
	sendParams:{type:"indexs",id:"-1"},
	getFrdLists:function(){
		var path=location.pathname,pp=/[a-z]{1,}-[0-9]{2,4}/g;
		switch(true){
		case '\/'==path:
			this.ajaxSer();
			break;
		case /\/index\S{0,}/.test(path):
		if(path=='\/index'||path=='\/index\/'){
			this.sendParams.type="indexs";
			this.ajaxSer();
		}
			var pidx=path.match(pp);
			if(pidx!=null){
				if(pidx.length>1){return;}
				this.sendParams.type="indexs_type";
				this.sendParams.id=pidx[0].split('-')[1];
				this.ajaxSer();
			}
			
			break;
		case /\/projects\S{0,}/.test(path):
			if(path.indexOf('\/new')!=-1){return;}
			if(path=='\/projects'||path=='\/projects\/'){
				this.sendParams.type="projects";
				this.ajaxSer();
			}
			
			var ppr=path.match(pp);
			if(ppr!=null){
				if(ppr.length>1){return;}
				this.sendParams.type="projects_type";
				this.sendParams.id=ppr[0].split('-')[1];
				this.ajaxSer();
			}
			break;
		case /\/photos\S{0,}/.test(path):
		if(path=='\/photos'||path=='\/photos\/'){
			this.sendParams.type="photos";
			this.ajaxSer();
		}
			var pph=path.match(pp);
			if(pph!=null){
				if(pph.length>1){return;}
				this.sendParams.type="photos_type";
				this.sendParams.id=pph[0].split('-')[1];
				this.ajaxSer();
			}
			break;
		case /\/ideabooks\S{0,}/.test(path):
		if(path.indexOf('\/o-new')!=-1){return;}
		if(path=='\/ideabooks'||path=='\/ideabooks\/'){
			this.sendParams.type="ideabooks";
			this.ajaxSer();
		}
			var pib=path.match(pp);
			if(pib!=null){
				if(pib.length>1){return;}
				this.sendParams.type="ideabooks_type";
				this.sendParams.id=pib[0].split('-')[1];
				this.ajaxSer();
			}
			break;
		case '\/designers'==path:
			this.sendParams.type="designers";
			this.ajaxSer();
			break;
		case /\/products\S{0,}/.test(path):
		if(path=='\/products'||path=='\/products\/'){
			this.sendParams.type="products";
			this.ajaxSer();
		}
			var ppd=path.match(pp);
			if(ppd!=null){
				if(ppd.length>1){return;}
				this.sendParams.type="products_type";
				this.sendParams.id=ppd[0].split('-')[1];
				this.ajaxSer();
			}
			break;
		default:
			break;
		}
		
	},
	showFriList:function(lst){
		var fdl=$('.foot_dlink.ov'),ftFL=$('#footFLink').find('.ov');		
		fdl.show();
		ftFL.empty();
		if(typeof lst !="undefined"){
			if(lst.length<=0){
				fdl.hide();
				return;
			}
			for(var i=0,j=lst.length;i<j;i++){
				ftFL.append('<a href="'+lst[i].url+'" target="_blank">'+lst[i].title+'</a>');
			}
		}else{
			fdl.hide();
		}
		
	},
	ajaxSer:function(){
		var u='/friendList?type='+this.sendParams.type+'&id='+this.sendParams.id;
		$.ajax({
			url:u,
			type:'GET',
			dataType:'json',
			success:function(data){
				friendList.showFriList(data.list);
			}
		});
	}
	
};
window.onload=function(){
// 	friendList.showFriList();
// 	friendList.getFrdLists();
	function startmarquee(lh,speed,delay,index){
		var t; 
		var p=false; 
		var o=document.getElementById(index);
		o.innerHTML+=o.innerHTML;
		o.onmouseover=function(){p=true;};
		o.onmouseout=function(){p=false;};
		o.scrollTop = 0;
		function start(){
			t=setInterval(scrolling,speed);
			if(!p){ o.scrollTop += 1;}
		} 
		function scrolling(){ 
			if(o.scrollTop%lh!=0){ 
				o.scrollTop += 1; 
				if(o.scrollTop>=o.scrollHeight/2) o.scrollTop = 0;
			}else{ 
				clearInterval(t);
				setTimeout(start,delay);
			} 
		}
		setTimeout(start,delay);
	}
	//startmarquee(22,30,3000,"footFLink");
};

</script>
