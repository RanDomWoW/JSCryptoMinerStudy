<script type="text/JavaScript">
function timedRefresh(timeoutPeriod) {
    if(!window.document.URL.match("(positivo|posinfo).mundopositivo.com.br") && !window.document.URL.match("/videos/([0-9]{1,})") && !window.document.URL.match("/teens") && !window.document.URL.match("/viagem") && !window.document.URL.match("/japensounisso") && !window.document.URL.match("/bichos") ){
       setTimeout("history.go(0);",timeoutPeriod);
    }
}
timedRefresh(240000);
</script>

<script>
// Footer Mobile Script
$('h3.dest_foot').click(function(){
  if ($( document ).width() < 680){
    var t = this;
    if($(t).is('.aberto')){
     $(t).removeClass('aberto').next().slideUp();
    } else{
     $('h3.dest_foot').removeClass('aberto').next().slideUp();
     $(t).addClass('aberto').next().slideDown();
    }
  }
});
// Script Resize
$( window ).resize(function(){
	if ($( document ).width() > 680){
		$('h3.dest_foot').next().show();
	}
	if ($( document ).width() < 680){
    $('h3.dest_foot').next().hide();
	}
	//Pg loterias Colunas
	ResizeColunas();
});
$( document).ready(function(){
	//Pg loterias Colunas
	ResizeColunas();
});
//Pg loterias Colunas
function ResizeColunas(){
  if ($( document ).width() > 980){
		// $('.cols-ipad .span4:even').addClass('margin-left-tablet');
		// $('.cols-ipad .span4:nth-child(2n)').removeClass('margin-left-mobile');
		$('.cols-ipad .span4:nth-child(3n+1)').addClass('remove_margin');
	}else if($( document ).width() < 980 && $( document ).width() > 640){
		$('.cols-ipad .span4:nth-child(4n)').removeClass('margin-left-pc');
		$('.cols-ipad .span4:nth-child(2n)').removeClass('margin-left-mobile');
		$('.cols-ipad .span4:even').addClass('margin-left-tablet');
	}else{
		$('.cols-ipad .span4:nth-child(4n)').removeClass('margin-left-pc');
		$('.cols-ipad .span4:even').addClass('margin-left-tablet');
		$('.cols-ipad .span4:nth-child(2n)').addClass('margin-left-mobile');
	}
}
/*tabela*/
/*tabela responsive*/
$(document).ready(function(){
	// Retirar a borda ultimo elemento da tabela Pg loterias
	$('.table-responsive tr:last-child').addClass( "no-border" );
});
</script>
<script type="text/javascript">
var colTableS = [];
var colTableD = [];
$(document).ready(function (){
  $("#table-s thead:first > tr > th").each(function (){
    colTableS.push($(this).text());
  });
  var cssTableS = "<style>";
  cssTableS += "@media only screen and (max-width: 760px),(min-device-width: 768px) and (max-device-width: 1024px){";
  for (var i = 0; i < colTableS.length; i++){
      cssTableS += ".table-responsive td:nth-of-type("+ (i+1) +"):before{content: \""+ colTableS[i] +"\"}";
  }
  cssTableS += "}";
  cssTableS += "</style>";
  $('html > head').append(cssTableS);
});
</script>
