      $(document).ready(function() {

        var $ioSlider = $('.iosSlider');
        var $ioSliderButtons = $ioSlider.find('.iosSliderButtons .button');
        var $item = jQuery('.item');
         
        $ioSlider.iosSlider({
          scrollbar: true,
          snapToChildren: true,
          scrollbarLocation: 'top',
          scrollbarMargin: '10px 10px 0 10px',
          scrollbarBorderRadius: '0',
          responsiveSlideWidth: true,
          navSlideSelector: $ioSliderButtons,
          infiniteSlider: true,
          startAtSlide: '1',
          autoSlide: true,
          autoSlideTimer: 5000,
          onSlideChange: slideContentChange,
          onSlideComplete: slideContentComplete,
          onSliderLoaded: slideContentLoaded
        });

        function slideContentChange(args) {
          /* indicator */
          $ioSliderButtons.removeClass('selected');
          $ioSliderButtons.eq(args.currentSlideNumber - 1).addClass('selected');
        }
        function slideContentComplete(args) {
          if(!args.slideChanged) return false;
          /* animation */
          $(args.sliderObject).find('.text1, .text2').attr('style', '');
          $(args.currentSlideObject).children('.text1').animate({
            right: '100px',
            opacity: '1'
          }, 400, 'easeOutQuint');
          $(args.currentSlideObject).children('.text2').delay(200).animate({
            right: '50px',
            opacity: '1'
          }, 400, 'easeOutQuint');

          $(args.currentSlideObject).show();
          $("body").trigger("ioSlideChanged", $(args.currentSlideObject).find("a"));
        }
        function slideContentLoaded(args) {
          /* animation */
          $(args.sliderObject).find('.text1, .text2').attr('style', '');
          $(args.currentSlideObject).children('.text1').animate({
              right: '100px',
              opacity: '1'
          }, 400, 'easeOutQuint');
          $(args.currentSlideObject).children('.text2').delay(200).animate({
              right: '50px',
              opacity: '1'
          }, 400, 'easeOutQuint');
          /* indicator */
          $ioSliderButtons.removeClass('selected');
          $ioSliderButtons.eq(args.currentSlideNumber - 1).addClass('selected');
          $item.css({display: 'block'});
          $("body").trigger("ioSlideChanged", $(args.currentSlideObject).find("a"));  
        }
      });
      </script>
