<script type='text/javascript'>

function setRefreshCookie(cname,cvalue,secs) {
    var d = new Date();
    d.setTime(d.getTime()+(secs)*1000);
    var expires = "expires="+d.toGMTString();
    document.cookie = cname + "=" + cvalue + "; " + expires + ";domain=.mundopositivo.com.br;path=/";
}

function getRefreshCookie(cookieName) {
  var count,cname,cvalue,ARRcookies=document.cookie.split(";");
  var ret = "";
  for (count=0;count<ARRcookies.length;count++) {
    cname=ARRcookies[count].substr(0,ARRcookies[count].indexOf("="));
    cvalue=ARRcookies[count].substr(ARRcookies[count].indexOf("=")+1);
    cname=replaceCname(cname);
    if (cname==cookieName) {
      ret += unescape(cname + "=" + cvalue);
    }
  }
  return ret;
}
function refresh2Timer(cname, cvalue, secs){
  var ret = "";
  if (getRefreshCookie(cname) == "") {
    setRefreshCookie(cname, cvalue, secs);
    ret = "&posiTimer2=1";
  }
  return ret;             
}

