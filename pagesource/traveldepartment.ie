<script>
    function BookNowlink(url) {
        var origid = $("#OrigID").val();
        
        if (origid == '' && url.indexOf("origid") > -1) {
            origid = $.url.param('origid');
        }

        var mask = "?origid=" + origid;
        if (url.indexOf("?") > -1) {
            mask = "&origid=" + origid;
        }

        var host = window.location.host;
        var domain = $("#siteurl").val();
        if ('http://' + host == domain) {
            document.location.href = url + mask;
        }
        else {
            TDActiveRedirect("Transferring you to our sister TDactive website.", domain + url + mask, true, function () { });
        }
    }

    function TDActiveRedirect(msg, url, modal) {
        $.fancybox("#TDActivefancyPauseScreen", {
            modal: modal,
            beforeShow: function () {
                $(".title").html(msg);
            },
            afterShow: function () {

                var timeout = setTimeout(function () {
                    window.location.href = url;
                }, 5000);

                $(".fancyPauseScreen").on("click", function (event) {
                    if ($(event.target).is(".not-ready")) {
                        clearTimeout(timeout);
                        $.fancybox.close();
                    }

                });
            }
        });
    }

</script>


