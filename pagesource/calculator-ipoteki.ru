					      $(this).find("option").each(function() {
					         $(container).find('.myform_select_options').append('<div data-value="'+$(this).val()+'">'+$(this).html()+'</div>');
					      });

					      $(container).find('.myform_select').click(function() {
					        $(this).parent().find('.myform_select_options').toggle();
					      });
					      $(container).find('.myform_select_options div').click(function() {
					        $(this).parent().parent().find('.myform_select').html($(this).html());
					        $(this).parent().parent().find('select').val($(this).data('value'));
					        $(this).parent().parent().find('select').trigger('change');

					        $(this).parent().hide();
					        return false;
					      });
