        .table-mobile-modiefied th, 
        .table-mobile-modiefied td, 
        .table-mobile-modiefied tr { 
            display: block; 
        }

        /* Hide table headers (but not display: none;, for accessibility) */
        .table-mobile-modiefied thead tr { 
            position: absolute;
            top: -9999px;
            left: -9999px;
        }



        .table-mobile-modiefied td { 
            /* Behave  like a "row" */
            border: none;
            border-bottom: 1px solid #eee; 
            position: relative;
            padding-left: 50%; 
            white-space: normal;
            text-align:left;
        }

        .table-mobile-modiefied td:before { 
            /* Now like a table header */
            position: absolute;
            /* Top/left values mimic padding */
            top: 6px;
            left: 6px;
            width: 100%;
            text-align: center;
            padding-right: 0px; 
            white-space: nowrap;

            font-weight: bold;
        }

        .single article table td:first-child
        {
            font-family:Roboto;
            font-size: 16px;
            font-weight: 300;
        }

        .single article table td
        {
