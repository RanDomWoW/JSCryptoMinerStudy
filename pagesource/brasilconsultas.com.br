$(document).ready(function(){
    $('a[href^="#"]').click(function(h){
        h.preventDefault();
        var target = this.hash;
        $("body").stop().animate({
            "scrollTop": $(target).offset().top
        }, 1200);
    });

    $(window).scroll(function() {
        if($(window).scrollTop() > 500){
            $('.go-up').fadeIn();
        }else{
            $('.go-up').fadeOut();
        }
    });

    $('.go-up').click(function(){
        $("body").stop().animate({
            "scrollTop" : 0
        }, 1200);
    });
});

$(document).ready(function(){

