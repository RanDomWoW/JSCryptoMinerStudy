    html,body,#page{width:100%}
    body{margin:0;padding:0;font-family:'ptsans_regular';font-size:15px;line-height:20px;background:#fff;color:#000}
    html{height:100%}
    h1{font-size:24px}
    h2{font-size:20px;}
    h3{font-size:18px;}
    a{font-family:'ptsans_regular';color:#000;cursor:pointer;outline: none;}
    a:hover{color:#000;text-decoration:none}
    strong a,
    strong{font-family:'ptsans_bold'}
    italic,
    em{font-family:'ptsans_italic'}
    em strong {font-family:'ptsans_bolditalic';}

    .center {text-align: center;}
    .hidden{display:none}
    .left{float:left}
    .right{float:right}
    .clr{clear:both}
    .none{display:none}
    .block{display:block}

    .red{color:#c93a12}
    .green{color:#008000}
    .grey{color:#808080}
    .purpur{color:#323cec}
    .old{text-decoration:line-through}        
    .fixed {position: fixed;z-index: 99;}

    .onclick{cursor: pointer}

    .interface_sprite {background-image: url(/assets/img/interface/interface_sprite.png);background-repeat: no-repeat;display: block;}
    .catalog_sprite {background-image: url(/assets/img/catalog/catalog_spritesheet.png);background-repeat: no-repeat;display: block;}



    /*-------------------------------
     top navigation
    --------------------------------*/

    #top-nav{height: 51px;position:fixed;z-index:15;background:#fff;width:100%;border-bottom: 1px solid #e3e3e3}

    @media (min-width:1200px){
        #top-nav{max-width:1140px;margin:0 auto;}
    }
    @media (min-width:992px) and (max-width:1199px){
        #top-nav{max-width:940px;}
    }
    @media (min-width:768px) and (max-width:991px){
        #top-nav{max-width:720px;}
    }
    @media (max-width:767px){
        #top-nav{position:relative;top:0px !important;}
    } 


    .head-nav ul li{line-height:50px;padding-right:35px;float:left} 
    .head-nav ul li a{color:#333;text-decoration:underline}
    .head-nav ul li a:hover{color:#000;text-decoration:none;outline:none;}
    .head-nav ul li:last-child{padding: 0px}
    .head-nav ul li:last-child a{color: green;background: url(/assets/img/interface/ico_journal.png) 0 center no-repeat;padding-left: 25px;}

    .wrap-cart-link{padding-right: 20px;margin-top: 15px;margin-right: 20px;border-right: 1px solid #e3e3e3;}
    .wrap-cart-link.cart-empty .cart-img span{width: 30px;height: 16px;background: url(/assets/img/interface/ico_cart_808080.png) 0 center no-repeat;margin: 2px 10px 2px 0px;}
    .wrap-cart-link.cart-empty .cart-title{/*color:grey;*/}
    .wrap-cart-link.cart-empty .cart-info{display: none}

    .wrap-cart-link.cart-full .cart-img span{width: 30px;height: 18px;background: url(/assets/img/interface/ico_cart_full_08000.png) 0 center no-repeat;margin: 1px 10px 1px 0px;}
    .wrap-cart-link.cart-full .cart-title{display: none}
    .wrap-cart-link.cart-full .cart-info{color:#333;}

    .wrap-login{padding-top: 14.5px;}
    .wrap-login #account-info .account{color: green;text-decoration: underline;margin-right: 15px;}
    .wrap-login #account-info .account:hover{color: #333;text-decoration: none;}
    .wrap-login a{line-height:20px;color:#333;display:block}
    .wrap-login a .sprite-ico_login {width: 20px;height: 20px;/*background-position: -389px -5px;*/background: url(/assets/img/interface/ico_login_808080.png) 0 center no-repeat;margin-right: 5px;}
    .wrap-login #log-ico{width: 18px;height: 18px;padding: 2px;background: url(/assets/img/interface/ico_man_08000.png) 0 center no-repeat;background-size: cover;display: block;}
    
    @media (max-width:991px){
        .wrap-login #account-info .account{padding-right: 20px;margin-right: 20px;border-right: 1px solid #e3e3e3;}
    }
    

    /*-------------------------------
     top shop info
    --------------------------------*/

    #shop-info{padding: 60px 0px 15px 0px;}

    #shop-info .wrap-logo{padding-top: 5px;}
    #shop-info .wrap-logo{width: 25%;float: left;} 
    #shop-info .wrap-logo a{display: block}
    #shop-info .wrap-logo a img{height: 95px}

    .wrap-search{width:50%;height:105px;background:url(/assets/img/bg_search.png) 50% -75% no-repeat;float: left}
    .wrap-search .search_wrapper{padding: 30px 0px;}
    .wrap-search form{position:relative}
    .wrap-search input#searchBox_text{width:75%;height:40px;font-size:17px;line-height:40px;padding-left:40px;margin: 0 12.5%;background:url(/assets/img/interface/ico_search_08000.png) 10px 9px #fff no-repeat}
    .wrap-search input#search-submit{}

    .wrap-search #resSearch{width: 75%;background:#FFF;border:1px solid #bfc4c5;margin: 0 12.5%;padding:15px;position:absolute;top:39px;z-index:10}
    .wrap-search #resSearch .ttl-search{padding:0px 25px;margin-top: 15px;}
    .wrap-search #resSearch .ttl-search:first-child{margin-top: 0px;}
    .wrap-search #resSearch li{list-style-type:none;margin:5px 0}
    .wrap-search #resSearch li:nth-child(odd){background-color:#f9f9f9}
    .wrap-search #resSearch a.ss{font-size:13px;color:#333;text-decoration:none;padding: 0px 25px;display: block;}
    .wrap-search #resSearch a:hover.ss{color:#000}
    .wrap-search #resSearch .more{width:100%;font-size:12px;padding:0px 25px; margin-top: 15px;border:0px;background:none;}

    #shop-info .wrap-phone{width: 25%;float: right;}
    #shop-info .wrap-phone .inner{width: 170px;text-align: left;padding: 6px 0;float: right;}
    .wrap-phone p{font-size:17px;line-height:130%;}
    .wrap-phone p.txt{font-size:14px}
    .wrap-phone p.number{padding-left: 20px;color: #333;background: url(/assets/img/interface/ico_phone.png) 0 center no-repeat;}

    @media (max-width:991px){
        #shop-info {padding: 51px 0 15px 0}

        #shop-info .wrap-logo{padding-top: 13px;} 
        #shop-info .wrap-logo {width: 100%;float: none;text-align: center}
        #shop-info .wrap-logo a img{height: 75px;}

        .wrap-search {width: 100%;height: auto;float: none;background: url(/assets/img/bg_search.png) 50% 48% no-repeat;}
        .wrap-search .search_wrapper {padding: 10px 0px 20px 0}

        #shop-info .wrap-phone{display: none}
    }
    @media (max-width:767px){
        #shop-info {padding: 0px 0 15px 0}
    }
    @media (max-width:580px){
        .wrap-search #resSearch,
        .wrap-search input#searchBox_text{width: 100%;margin: 0 0%;}
    }


    /*-------------------------------
     main menu
    --------------------------------*/

    .row-menu{position:relative;margin-bottom: 20px;}
    .row-menu .menu{width:100%;border-top: 1px solid #e3e3e3;border-bottom: 1px solid #e3e3e3;/*background: #f9f9f9;*/}

    .row-menu .menu li.menu_item{width:20%;border-right:1px solid #fff;float:left;}

    .row-menu .menu li.menu_item.act,
    .row-menu .menu li.menu_item.active{background:url(/assets/img/menu/bg_menu_active.png);/*background-position: -27px -5px;*/}

    .row-menu .menu li a.link_item{font-size:17px;text-decoration:none;color:#333;display:block;/*padding-left:68px;background-image: url(/assets/img/menu/spritesheet_menu.png);background-repeat: no-repeat;*/}
    .row-menu .menu li a.link_item:hover{color:#000}

    .title-menu-main{width: 100%;display:table;text-align: center;text-transform: uppercase;}
    .title-menu-main div{height:48px;display:table-cell;vertical-align:middle}

    .menu_item.menu_3 .title-menu-main span{height: 20px;width: 20px;padding-left: 25px;background: url(/assets/img/interface/ico_discount.png) 0 center no-repeat;}

    @media (min-width:1200px){
        .row-menu .menu li .sub-menu{width: 100%;padding: 25px;border: 1px solid #e3e3e3;/*border-top: 3px solid green;*/position: absolute;z-index: 9;left: 0;background: #fff;display:none}
        .row-menu .menu li:hover .sub-menu{display:block;}

        .sub-menu .sub-title{font-size: 15px;margin-bottom: 15px;text-transform: uppercase;}

        .sub-menu .series-menu{width: 100%;float: left;}
        .sub-menu .series-menu ul li{width:25%;float: left;padding:10px 0;}
        .sub-menu .series-menu .menu_item_sub a{color:#333;text-decoration:underline}
        .sub-menu .series-menu .menu_item_sub a:hover {color:#000;text-decoration:none}

        .sub-menu .journal-menu{width: 25%;float: right;}

        .sub-menu .subscription-menu{width: 50%;float: left;}
        .sub-menu .subscription-menu:nth-child(odd){padding-right: 12.5px}
        .sub-menu .subscription-menu:nth-child(even){padding-left: 12.5px}
    }
    @media (max-width:1199px){
        .row-menu .menu li .sub-menu{display:none}
    }
    @media (min-width:992px){
         .menu-nav{display:none}
         .row-menu .menu{display: block !important}
