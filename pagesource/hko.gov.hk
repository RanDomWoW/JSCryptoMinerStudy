var tempkey;
var isNav= (navigator.appName.indexOf("Netscape") != -1);
var isIE= (navigator.appName.indexOf("Microsoft") != -1);
// If we're in Navigator, report event details in this way.     
if (isNav) {
	window.captureEvents(Event.KEYDOWN);
	
	window.onkeydown = function(e){
		if ( e.type=="keydown" ) {
				tempkey = e.which;
				OpenURLByKey();
		}
	}
}
function iekeycode() {
	if (isIE) {
			tempkey = event.keyCode;
			OpenURLByKey();
	}
}
function OpenURLByKey() {
	if ( tempkey == 65 || tempkey == 97 || tempkey == 69 || tempkey == 101 ){   // A is 65, a is 97, E is 83, e is 115
		self.location = "audio/index.shtml";
	} else if ( tempkey == 67 || tempkey == 99 ){   // C is 67, c is 99
		self.location = "audio/indexc.shtml";
	} else if ( tempkey == 80 || tempkey == 112 ){   // P is 80, p is 112
		self.location = "audio/indexp.htm";
	} 
}
<!-- This javascript is used to handle the keyboard events (shortcut keys) of the users -->
