		}
		else if (jQuery(this).attr('alt') == 'ouvrir') {
			jQuery(this).parents('.title').siblings('.data').show();
			jQuery(this).attr('src',jQuery(this).attr('src').replace(/_ouvrir.gif/,'_fermer.gif'));
			jQuery(this).attr({alt: 'fermer'});
			return false;
		}
	});
}

