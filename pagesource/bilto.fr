<script type="text/javascript">
    
        $(document).ready(function() {

            //Initialisation
            $( ".popup-betbox" ).draggable({ containment: "document", handle: $(".popup-betbox .header") });
 
            //Fermeture
            $('.popup-betbox .close-div').bind('click', function() {
                $('.popup-betbox').hide();
            });
        });

        function openBetbox(race_id, horse_num, pari_id, race_id_leturf)
        {
            var urlLeturfBetbox = "https://www.leturf.fr/le-turf-paris-courses-pmu-pronostics-turf-et-resultats-pmu/betbox/pid70-parier.html?cible=BILTBOX";

            if (race_id_leturf) {
                urlLeturfBetbox += "&id_course=" + race_id;
            } else {
                urlLeturfBetbox += "&id_course_turf=" + race_id;
            }
            

            if (horse_num) {
                urlLeturfBetbox += "&designation=" + horse_num;
            }

            if(pari_id) {
