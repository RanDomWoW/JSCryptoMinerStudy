	swap_interval: 3000,
	current_banner: 0,
	run: function(){
		var banner_id = this.banner_ids[this.current_banner];
		var last_banner_id = this.banner_ids[(this.current_banner == 0 ? this.banner_ids.length - 1 : this.current_banner - 1)];
		var old_banner = dojo.byId(last_banner_id);
		var new_banner = dojo.byId(banner_id);
		old_banner.style.visibility="hidden";
		old_banner.style.width="0px";
		old_banner.style.height="0px";
		old_banner.style.overflow="hidden";
		
		new_banner.style.visibility="visible";
		new_banner.style.width="auto";
		new_banner.style.height="auto";
		new_banner.style.overflow="hidden";
		this.current_banner = (this.current_banner + 1) % this.banner_ids.length;
		setTimeout("position_partneri_loga.run();", this.swap_interval);
	}
}

dojo.addOnLoad(function(){
	position_partneri_loga.run();
});
