    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/packery/2.1.1/packery.pkgd.js"></script>
    <script type="text/javascript">
        (function ($) {
            var $container = $('#tiles-config-container').packery({
                // options
                itemSelector: '.grid-config-item',
                columnWidth: '.cell-sizer',
                rowHeight: '.cell-sizer',
                gutter: '.gutter-sizer',
                shiftPercentResize: true,
                percentPosition: true
                        //                resize: true
                        //            initLayout: false,
            });

            window.fitItems = function fitItems() {
                var $items = $container.find('.grid-config-item');
                $items.each(function (i, gridItem) {
                    var top = $(gridItem).attr('data-top'),
                            left = $(gridItem).attr('data-left');

                    $container.packery('fit', $(gridItem).attr('data-item-width'), left * $container[0].clientWidth, top);
                });
            }

            var tilesContainer = $('#tiles-config-container');
            tilesContainer.prepend('<div class="mobile-tiles"></div>');
            var stop = false;
            
            
            fitItems();
            updateItemsHeight();
            $container.data('packery').shiftLayout();

            $(window).resize(function () {
                if ($(window).innerWidth() > 500) {
                    updateItemsHeight();
                    $('#tiles-config-container > a').css('display', 'block');
                } else {
                    $('#tiles-config-container > a').css('display', 'none');
                    mobileWidth();
                }
            }).resize();

            function updateItemsHeight() {
                $('.mobile-tiles .grid-config-item').remove();
                stop = false;
                var gutterSizer = $('.gutter-sizer')[0].clientWidth;
                var cellSizer = $('.cell-sizer')[0].clientWidth;
                $('.grid-config-item').each(function () {
                    var $this = $(this);
                    var gridItem = $container.data('packery').getItem($this[0]);
                    var heightMultiple = $this.attr('data-item-height');
                    var multipleWidth = $this.attr('data-item-width');
                    var height = heightMultiple * cellSizer + (heightMultiple - 1) * gutterSizer;
                    var width = multipleWidth * cellSizer + (multipleWidth - 1) * gutterSizer;
                    $this.css({height: height + 'px', backgroundSize: '100%', width: width + 'px'});
                });
            }

            function mobileWidth() {
                var a = tilesContainer.find('.grid-config-item');
                a.each(function () {
                    var aspectRatio = $(this).height() / $(this).width();
                    var newW = $(this).width('80%').width();
                    $(this).height(newW * aspectRatio);
                    if (stop) {
                        return;
                    }
                    $(this).clone().appendTo($('.mobile-tiles'));
                });
                stop = true;
            }

        })(jQuery);

    </script>
