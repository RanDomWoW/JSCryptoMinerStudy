<script>

    var f_old_industry = $("#filter_industry").val() || [];
    var f_old_industry_str =f_old_industry.join(",");

function getSelectionIndustry(){
    var selectedOptions = [];
    var obj = [];
    var objval = [];
    $('#filter_industry option:selected').each(function(){

        var selval=this.value.replace(new RegExp("industry_",'g'),"");
        selval=selval.replace(new RegExp("_",'g'),"-");
        selectedOptions.push(selval);
        obj.push($(this).index());
        objval.push($(this).val());
        //alert("Selected Values=="+$(this).val());
        if(f_old_industry.indexOf($(this).val())==-1){
        this.selected=false;
        }

        });

    for (var i = 0; i < obj.length; i++) {
                if(f_old_industry.indexOf(objval[i])==-1){
                $('#filter_industry')[0].sumo.unSelectItem(obj[i]);
              }
            }

    return selectedOptions.join(",");
}

</script>

  
