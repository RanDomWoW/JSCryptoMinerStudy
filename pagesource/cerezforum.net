	<script type="text/javascript">

function addLink(e) {
	var body_element = document.getElementsByTagName('body')[0];
	var selection;
	var copytext = "{seltext}<br /><br />Kaynak: <a href='"+document.location.href+"'>"+document.title+"</a><br />Copyright &copy;CeReZFoRuM";
	if (window.getSelection) {  // all browsers, except IE before version 9
		selection = window.getSelection();
		copytext = copytext.replace("{seltext}",selection);
	}
	else {
		selection = document.selection.createRange ();
		copytext = copytext.replace("{seltext}",selection.text);

	}
	var newdiv = document.createElement('div');
	newdiv.style.position='absolute';
	newdiv.style.left='-99999px';
	body_element.appendChild(newdiv);
	newdiv.innerHTML = copytext;
	if (window.getSelection) {  // all browsers, except IE before version 9
		selection.selectAllChildren(newdiv);
	}
	else {
		selection.moveToElementText (newdiv);
		selection.select();
	}
	window.setTimeout(function() {
		body_element.removeChild(newdiv);
	},0);
}

</script>

