<script>
var trakerQuery = function(identify , data){
directCrm('identify', {
operation: 'DemoAccess',
identificator: identify, 
data: data
});
};
var getValInputs = function(inputs){
var vals = {};
inputs.each(function(i ,el){
if($(this).attr('name') === 'price[name]') vals.name = $(this).val() ? $(this).val() : '';
if($(this).attr('name') === 'price[site]') vals.site = $(this).val() ? $(this).val() : '';
if($(this).attr('name') === 'calculate[amount_users]') vals.count_contacts = $(this).val() ? $(this).val() : '';
if($(this).attr('name') === 'calculate[price]') vals.price = $(this).val() ? $(this).val() : '';
if($(this).attr('name') === 'price[tel]') vals.tel = $(this).val() ? $(this).val() : '';
if($(this).attr('name') === 'price[email]') vals.email = $(this).val() ? $(this).val() : '';
if($(this).attr('name') === 'price[desc]') vals.desc = $(this).val() ? $(this).val() : '';
});
return vals;
};
var callDirectQuery = function(inputs){
var vals = getValInputs(inputs);
var identify = {},user = {};
if(vals.email){
identify.provider = 'email';
identify.identity = vals.email;
}else if(vals.tel){
identify.provider = 'mobilePhone';
identify.identity = vals.tel;
}
if((vals.tel) || (vals.email)){
user.firstName = vals.name;
user.sitelink = vals.site;
user.usercount = vals.count_contacts;
user.taskdesc = vals.desc;
trakerQuery(identify , user);
}
}
$(function(){
$('body').on('form.send.demo-form' , function(e , data){
callDirectQuery(data.inputs);
});
$('body').on('form.send.price' , function(e , data){
callDirectQuery(data.inputs);
});
$('body').on('form.send.meet' , function(e , data){
callDirectQuery(data.inputs);
});
$('body').on('form.send.call' , function(e , data){
callDirectQuery(data.inputs);
});
// $('body').on('form.sendsuccess.demo' , function(e , data){
//     window.location.href = "http://oldmindbox.nikoland.online/thanks/";
// })
var hash = window.location.hash;
if (hash=='#price'){
$('.side-popup[data-popup-name="price-and-request"]').addClass('_open');
$('body').addClass('_side-popup-open');
}
})</script>
