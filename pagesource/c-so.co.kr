			function getBalance(){
				$.ajax({
					type:'POST'
					, url:'/seyfert/getBalanceAjax'
					, async:false
					, success: function(data){
						if(data.result =='SUCCESS'){
							var balance = data.balance;
							$('#balanceTxt').text(setComma(balance)+'원');
							commonAlert('예치금이 최신 금액으로 업데이트 되었습니다.');
						} else {
							if(data.message != ''){
								commonAlert(message);
							} else {
								commonAlert('예치금 최신 금액으로 업데이트 실패하였습니다.');
							}
						}
						
					}
					, error: function(x,o,e){ 
						commonAlert(x.status + ":" + o + ":" + e);
					}
				});
			}

			function goLinkUrl(url, type){
				if(url !=null && url != ''){
					if(type == '1'){
						window.location.href = url;
					}
					if(type == '2'){
						window.open(url);
					}
				}
			}
			
