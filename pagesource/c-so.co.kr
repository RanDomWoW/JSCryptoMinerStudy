	function leaveUser(){
		if(confirm("회원탈퇴를 진행합니다\n정말로 탈퇴하시겠습니까?")){
			$.ajax({
				type:'POST'
				, url:'/user/leaveAjax'
				, async:false
				, success: function(data){
					var result = data.result;
					if(result == 'USER_LEAVE_SUCCESS'){
						confirm(data.message)
						window.location.href = "/j_spring_security_logout";
					} else {
						commonAlert(data.message);
					}
				}
				, error: function(x,o,e){ 
					commonAlert(x.status + ":" + o + ":" + e);
				}
			});
		}
	}
	
	function moveStatistics(){
		$('html,body').animate({scrollTop: $('.main_diagram').offset().top-45},1000);
	}
	
</script>

