<script>
	var owl = $('#owl-carousel');

	owl.owlCarousel({
		items:1,
		dots: true,
		nav: true,
		loop: true,
		animateOut: 'fadeOut',
		onInitialize : function(element){
			owl.children().sort(function(){
				return Math.round(Math.random()) - 0.5;
			}).each(function(){
				$(this).appendTo(owl);
			});
		}
	});

	var $more_menu_btn = $("#imweb-navbar-collapse ._imweb_navbar_more_container_btn");
	var $imweb_navbar_more_container = $("#imweb-navbar-more-container");
	var is_slide_down = false;
	var full_down_event ='mouseover';


	$more_menu_btn.off(full_down_event+".more_menu_btn").on(full_down_event+".more_menu_btn",function(){
		if(!is_slide_down){
			$imweb_navbar_more_container.slideDown(
			function(){
				is_slide_down = true;
			});
		}

		$(document).off(full_down_event+".more_menu_btn").on(full_down_event+".more_menu_btn",function(e){
			if($(e.target).closest(".imweb-navbar-more").length == 0 && $(e.target).closest(".navbar-default").length == 0){
				if(is_slide_down){
					$imweb_navbar_more_container.slideUp(function(){
						is_slide_down = false;
					});
				}
			}
		});
	});
