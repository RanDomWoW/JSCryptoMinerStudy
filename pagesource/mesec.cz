	<script>
		(function () {
			function formatTime (days) {
				var hours = (days - Math.floor(days)) * 24.0;
				var minutes = (hours - Math.floor(hours)) * 60.0;
				var seconds = (minutes - Math.floor(minutes)) * 60.0;

				if (hours >= 1 && hours < 2) {
					hoursText = 'hodina';
				} else if (hours >= 2 && hours < 5) {
					hoursText = 'hodiny';
				} else {
					hoursText = 'hodin';
				}

				if (minutes >= 1 && minutes < 2) {
					minutesText = 'minuta';
				} else if (minutes >= 2 && minutes < 5) {
					minutesText = 'minuty';
				} else {
					minutesText = 'minut';
				}

				return (hours >= 1 ? Math.floor(hours) + ' ' + hoursText + ', ' : '')
					+ (minutes >= 1 ? Math.floor(minutes) + ' ' + minutesText + ' a ' : '')
					+ Math.floor(seconds) + ' s';
			}

			var headerCounterEndingValue = 1522792800000;
			var headerCounterEnding = new Date(headerCounterEndingValue);

			var headerCounter = document.getElementById('js-header-counter');
			var headerCounterDays = document.getElementById('js-header-counter__days');
			var headerCounterTime = document.getElementById('js-header-counter__time');


			if (window.matchMedia('(min-width: 590px)').matches) {
				if (headerCounter && headerCounterDays && headerCounterTime) {
					var headerCounterInterval = setInterval(function () {
						var now = new Date();
						var timeDifference = (headerCounterEnding.valueOf() - now.valueOf()) / 86400000;
						var headerCounterDaysText = '';

						if (timeDifference >= 1) {
							if (timeDifference >= 1 && timeDifference < 2) {
								headerCounterDaysText = 'den';
							} else if (timeDifference >= 2 && timeDifference < 5) {
								headerCounterDaysText = 'dny';
							} else {
								headerCounterDaysText = 'dnů';
							}
							headerCounterDays.innerHTML = Math.floor(timeDifference) + ' ' + headerCounterDaysText + ',';

						} else {
							headerCounterDays.innerHTML = '';
							headerCounterDays.style.display = 'none';
							if (headerCounterTime.className.indexOf('design-bar__highlight--left') === -1) {
								headerCounterTime.className = headerCounterTime.className + ' design-bar__highlight--left';
							}
						}
						headerCounterTime.innerHTML = formatTime(timeDifference);

						if (timeDifference < 0) {
							clearInterval(headerCounterInterval);
							headerCounter.style.display = 'none';
						} else {
							headerCounter.style.display = 'block';
						}
					}, 1000);
				}

			} else {
				headerCounter.style.display = 'block';
			}
		})();
	</script>
