		$(document).ready(function() {
			function jobs_select_tab(num) {
				if (num == 1) {
					$("#jobs").removeClass('purple');
					$("#seduo-ad-logo").hide();
					$("#jobs-ad-logo").show();
					$("#jobs-prace").show();
					$("#jobs-kurzy").hide();
					$("#jobs-ad-t2").removeClass("jobs-ad-active");
					$("#jobs-ad-t1").addClass("jobs-ad-active");
				} else {
					$("#jobs").addClass('purple');
					$("#jobs-ad-logo").hide();
					$("#seduo-ad-logo").show();
					$("#jobs-prace").hide();
					$("#jobs-kurzy").show();
					$("#jobs-ad-t1").removeClass("jobs-ad-active");
					$("#jobs-ad-t2").addClass("jobs-ad-active");
				}
			}
			$("#jobs-ad-t1").click( function (event) {
				event.preventDefault();
				jobs_select_tab(1);
			});
			$("#jobs-ad-t2").click( function (event) {
				event.preventDefault();
				jobs_select_tab(2);
			});

			// random choose one tab
			if ( Math.random() > 0.75 ) {
				jobs_select_tab(2);
			} else {
				jobs_select_tab(1);
			}
		});
	</script>

