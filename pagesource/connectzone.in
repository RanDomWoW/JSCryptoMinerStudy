<script>
$(document).ready(function(){

$('#packs').change(function(){ 
	if($('#packs').val() == '') {  
	 
$("#phone,#phone1, #phone2, #phone3, #phone4, #phone5, #phone6").hide();
	} 
	if($('#packs').val() == '0') {  
	   $("#phone").slideToggle(1000);
	   $('#form1Val').val($('#packs').val());
$("#phone1, #phone2, #phone3, #phone4, #phone5,#phone6").hide();
	} 
	 if($('#packs').val() == 'WIFI') { 
	 $("#phone1").slideToggle(1000);
	 $('#form2Val').val($('#packs').val());
$("#phone, #phone2, #phone3, #phone4, #phone5,#phone6").hide();
	} 
	 if($('#packs').val() == 'DSLS') { 
	   $("#phone2").slideToggle(1000);
	   $('#form3Val').val($('#packs').val());
$("#phone, #phone1, #phone3, #phone4, #phone5,#phone6").hide();
	  
	} 
	 if($('#packs').val() == 'FTTH') { 
	  $("#phone3").slideToggle(1000);
	  $('#form4Val').val($('#packs').val());
$("#phone, #phone1, #phone2, #phone4, #phone5,#phone6").hide();
		
	} 
	 if($('#packs').val() == 'TOF') { 
	 
		$("#phone4").slideToggle(1000);
		$('#form5Val').val($('#packs').val());
$("#phone, #phone1, #phone2, #phone3, #phone5,#phone6").hide(); 
		
	} 
	 if($('#packs').val() == 'EOC') { 
	 
		$("#phone5").slideToggle(1000);
		$('#form6Val').val($('#packs').val());
$("#phone, #phone1, #phone2, #phone3, #phone4,#phone6").hide(); 
		
	} 
	 if($('#packs').val() == 'VDSLS') { 
	 
		$("#phone6").slideToggle(1000);
		$('#form7Val').val($('#packs').val());
$("#phone, #phone1, #phone2, #phone3, #phone4,#phone5").hide(); 
		
	} 
	
});


});
</script>

