<script>
$(function() {

  // 動画を設定したスライドリスト
  //
  // id     → .slideBnr > li > a のid名
  // bnrcnt → モーダルウィンドウ（iframeで開くhtml）内のバナー表示数
  //
  var movieList = [
    { id : '#slideMovie01', bnrcnt: 1 },
  ];

  // 動画幅（px）
  var movieW = 600;

  // 動画高さ（px）
  var movieH = 338;

  // モーダル内のバナーの高さ（px）
  var bnrH = 200;

  if(movieList.length > 0) {
    $.each(movieList, function(idx,val){
      var self = this;
      $(this.id).colorbox({
        iframe: true,
        innerWidth: movieW+20,
        innerHeight:movieH+5+(bnrH+20)*this.bnrcnt
      });
    });
  }

});
</script>

