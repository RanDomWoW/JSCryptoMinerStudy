(function($){
    $.fn.innerhtml = function(htmlText) {
		return this.each(function() {
				this.innerHTML = htmlText;
				evalScript(this);
      });
      function evalScript(elem) {
          var scripts = $(elem).find("script");
          var evo = [], evi = [], i = 0, n = 0, j = 0, time = null;
          scripts.each(function(i,script) {
              var ret = script.innerHTML,
                  src = script.src;
               evo.push(ret);
               evi.push(src);
          });

          if (evi.length > 0) {
          	n = evi.length;
          	for (i = 0; i < n; i++) {
          		if (evi[i]) {
          			$.getScript(evi[i], function() {
                      	j++;
                      })
          		} else {
          			j++;
          		}
          	}
          	time = setInterval(function() {
					if (j == evi.length) {
						clearInterval(time);
						for (i = 0; i < n; i++) {
							if (evo[i]) {
								eval(evo[i]);
							}
						}
					}
          	}, 100);
          }

       }
    }
})(jQuery);

