$(document).ready(function()
{
	// if they visited with /?invite on the end of URL, auto-open the email form (they clicked "get invited" from a Features page)
		
	// if they visited with 'pre' cookie, show the sharrre buttons straight away
	if($('body').hasClass('pre'))
