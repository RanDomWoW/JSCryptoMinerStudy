		do_sharrre();
	}
	
	$('#trial_plan').val(localstorage_get('trial_plan'));
	$('#demographic').val(localstorage_get('demographic'));
	
	// show the appropriate benefits - default to business owner, but use fragment if provided
	var hash = window.location.hash.replace('#','');
	//console.log(hash);
	if (hash == 'business_owner' || hash == 'social_media_marketer')
	{
		show_benefits(hash);	
	}
	
	$("#submit_email_button").on("click", function(event)
	{
		event.preventDefault();
		ajax_submit_form();
	});
	
