$(document).ready(function()
{
	anim_running = false;
	
	$(window).resize(fit_to_screen);
});

function fit_to_screen()
{
	// animation is 3x2 size, designed for 900x600
	
	if(!anim_running)
	{
		return;
	}
	
	winW = $(window).width();
	winH = $(window).height() - 50 - 54; // keep 50px for top nav bar, and 54px for 'got it' buttons

	if (600 < winH && winH < 660)
	{
		winH = 600;
		$('#anim').css('transform-origin', 'top');
	}
	
	orig_animH = $('#anim').height();
	diffH = winH - orig_animH;
	if (diffH > 0)
	{
		diffH = 0; // so we don't add margin above 'got it' buttons if the window is taller than the animation
	}

	scale_width = winW / $('#anim').width();
	scale_height = winH / $('#anim').height();	
	scale = Math.min(scale_width,scale_height);

	if (scale > 1) // don't enlarge the animation
	{
		scale = 1;
	}
	
	$('#anim_container').css('transform', 'scale(' + scale + ')');
	if (scale_width < 1)
	{
		$('#anim_container').css('transform-origin', 'top left');
	}
	else if (scale_height < 1)
	{
		$('#anim_container').css('transform-origin', 'top');
	}
	
	// and set a -ve margin on the 'got it' buttons, so they move up/down with the animation, to sit just underneath it
	$('p.categories_understood').css('margin-top',diffH+'px');
}

