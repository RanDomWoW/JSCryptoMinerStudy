<script type="text/javascript">
function is_placeholder_supported()
{
	var test = document.createElement('input');
	return ('placeholder' in test);
}

function json_encode_form_data(form_id)
{
	var unindexed_array = $('#'+form_id).serializeArray();
	var indexed_array = {};
	
	$.map(unindexed_array, function(n, i)
	{
		indexed_array[n['name']] = n['value'];
	});
	
	return indexed_array;
}

function show_benefits(visitor_type)
{
	$('#benefits_choices	a:not('+visitor_type+')').removeClass('active');
	$('div.benefits_section:not('+visitor_type+')').removeClass('active');
	$('#benefits_choices	a.'+visitor_type).addClass('active');
	$('div.benefits_section.'+visitor_type).addClass('active');
}

function ajax_submit_form()
{
	// once the send button is clicked we perform some functions
	// and setup a few variables

	var emailval = $("#email").val();

	if(!isEmail(emailval))
	{
		$("#error").html("Please enter a valid email address!");
		$("#error").css("color", "#cc0000");
		$("#error").css('display','block');
	}
	else
	{
		$("#error").css("color", "#3d997a");
		$("#error").html("Just a sec...");
		$('#loading').show();
		$('#submit_email_button').hide();
		$('#no_ajax').remove();

		var action_url = $('#prelaunch_form').attr("action");
		
		$.ajax({
			type: "POST",
			url: action_url,
			data: $('#prelaunch_form').serialize(),
			timeout: 30 * 1000,
			complete: function()
			{
				$('#loading').hide();
				$('#submit_email_button').show();
				$('#submit_survey_button').removeAttr('disabled');
			},
			success: function(response) // submit via AJAX
			{
				if(response.status == 'success')
				{
					ref_code = response.ref_code; // e.g. pre_15r (need to add pre to differentiate referrals from pre-registrations and beta-members)
					pre_sq_id = response.pre_sq_id; // e.g. 1755
					trial_length = response.trial_length; // e.g. 1755
					$('#ref_link').val('https://smarterqueue.com/?ref='+ref_code);
					
					do_sharrre();
					
					//$('#take_pricing_survey .lp_pricing_survey a').attr('href','http://www.surveygizmo.com/s3/3209420/Pricing?id=pre_'+pre_sq_id);
					
					$('body').addClass('pre'); // so we hide the 'get invite' links
					
					$("#pre_submit").fadeOut(200, function()
					{
						$('#sharrre_container').show();
						
						if (typeof response.direct_signup_link != 'undefined') // they were lucky to get direct access, show them the signup link
						{
							$direct_signup = $('#direct_signup');
							$('body').addClass('invited'); // so we show the 'start your trial' buttons
							$direct_signup.find('.lp_direct_signup a').attr('href',response.direct_signup_link);
							$('#thanks, #take_pricing_survey, #survey').hide();
							
							// DEPRECATED for poor conversion - 50% of direct signups are shown our beta pricing
							//if(typeof response.show_beta_prices != 'undefined' && response.show_beta_prices == 1)
							//{
							//	$('#show_beta_prices').show();
							//}
							
							// track event if they click on direct-signup button
							$direct_signup.find('div.lp_direct_signup a').on('click',function()
							{
								arr_event = {};
								arr_event.event_type = 'Direct Signup Clicked';
								track_event_js(arr_event);
							});
							
							$direct_signup.find('div.lp_later a').on('click',function()
							{
								arr_event = {};
								arr_event.event_type = 'Direct Signup Later Clicked';
								track_event_js(arr_event);
							});
							
							// show the signup-now button in a modal dialog, so they can't miss it!
							// clone the direct_signup div into the dialog placeholder first
							$direct_signup.show();
							$('#dialog_direct_signup').append($direct_signup);
							var dialog_direct_signup = jQuery("#dialog_direct_signup").dialog({
								resizable: false,
								modal: true,
								title:'Ready to get started?',
								draggable: false,
								minWidth:300,
								width:'auto',
								maxWidth:600,
								minHeight:200,
								maxHeight:600,
								close: function()
								{
									$('#extra_info').append($direct_signup);
								}
							});
					
							jQuery(".ui-widget-overlay").click (function()
							{
								dialog_direct_signup.dialog("close");
							});
						}
						
						$('#post_submit').show();
					});
					
											mouseout_next_step = ''; // they've done all they can do for now
						arr_event = {};
						arr_event.event_type = 'Email submitted';
						arr_event.user_properties = {'email':$('#email').val(), 'pre_sq_id':pre_sq_id, 'trial_length':trial_length, 'ab_watch_demo':$('#ab_watch_demo').val()};
						if (typeof response.direct_signup_link != 'undefined')
						{
							arr_event.user_properties.pre_status = 'direct'; // so we can track whether they got direct access or were added to waitlist
							mouseout_next_step = 'direct_signup'; // we'll show a popup if they don't click direct_signup next
							if (typeof response.show_beta_prices != 'undefined')
							{
								arr_event.user_properties.show_beta_prices = response.show_beta_prices; // so we can track whether they got direct access or were added to waitlist
							}
						}
						track_event_js(arr_event);
						
						fbq('track', 'Lead'); // Facebook pixel
										}
				else if(response.status == "invalid")
				{
					notify('Invalid email address');
				}
				else if(response.status == "member")
				{
					notify('There is already an account with that email. Please <a href="/login?src=existing_email">login</a>.');
				}
				else
				{
					notify("There was an error saving your details");
				}
			},
			error: function(jqXHR,textStatus,errorThrown)
			{
				if (errorThrown == "")
				{
					errorThrown = "Could not reach server - check your connection";
				}

				$("#error").css("color", "#990000");
				$("#error").html("Error: "+errorThrown);
				$("#error").css('display','block');
				notify("Error",errorThrown);
				
				client_debug({text:'Pre-signup failed: '+errorThrown, pre_signup_form:$('#prelaunch_form').serialize(), jqXHR_responseText:jqXHR.responseText, jqXHR_status:jqXHR.status, jqXHR_statustext:jqXHR.statusText, textStatus:textStatus}); // store details about this error in 'client_side' debug log
			}
		});
	}
}

