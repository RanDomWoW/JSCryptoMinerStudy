	{
		var email_input = $('#email');
		placeholder_txt = email_input.attr('placeholder');
		email_input.bind({
			focus: function()
			{
				if(email_input.val() == placeholder_txt)
				{
					email_input.attr('placeholder',placeholder_txt);
					email_input.val('');
				}
			},
			blur: function()
			{
				if(email_input.val() == '')
				{
					email_input.val(placeholder_txt);
					email_input.attr('placeholder',placeholder_txt);
				}
			}
		});
	}

