				<script type="text/javascript">
		YUI({
			fetchCSS: false
		}).use('node', 'anim', function(Y) {
			var a;
			var i = 0;
			var n = Y.all('.cover ul li').size();
			var w = 660;
			var t;

			if (n > 1) {
				var l;

				Y.one('.cover').append('<ol></ol>');

				for (var j = 1; j <= n; j++) {
					l = document.createElement('li');
					l.id = 'cover-' + (j - 1);
					l.innerHTML = '&nbsp;'

					Y.one('.cover ol').append(l);
					Y.one(l).addClass('cover-' + (j - 1)).on('click', function(e, j) {
						clearInterval(t);

						i = j - 1;

						a.set('to', {left: -w * i, opacity: 1});

						Y.all('.cover ol li').removeClass('active');
						Y.one('.cover ol li.cover-' + i).addClass('active');

						a.run();

						t = setInterval(function() {
							a.set('to', {left: -w * i, opacity: 1});

							Y.all('.cover ol li').removeClass('active');
							Y.one('.cover ol li.cover-' + i).addClass('active');

							a.run();
						}, 5000);
					}, null, j);
				}

				Y.one('.cover ol li.cover-0').addClass('active');

				a = new Y.Anim({
					duration: 0.6,
					node: Y.one('.cover ul')
				});

				a.on('end', function(e) {
					i++;

					if (i == n) {
						i = 0;
					}
				});

				i++;

				t = setInterval(function() {
					a.set('to', {left: -w * i, opacity: 1});

					Y.all('.cover ol li').removeClass('active');
					Y.one('.cover ol li.cover-' + i).addClass('active');

					a.run();
				}, 5000);
			}
		});
