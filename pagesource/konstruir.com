<script language="JavaScript">
function abrirmenu(e,usuario){
var margin=5
	var tempX = e.clientX;
	var tempY = e.clientY;
  var box2 = document.getElementById('menu_flota'); 
  box2.style.top=(tempY+margin)+"px"
  box2.style.left=(tempX+margin-200)+"px"
  box2.style.display='block';
  document.getElementById('ver_usua').value=usuario;
}
var timer;
function cerrarmenu(){
   document.getElementById('menu_flota').style.display='none';
}
function cierre(){
timer = setTimeout('cerrarmenu()',1000);
}

</script>
