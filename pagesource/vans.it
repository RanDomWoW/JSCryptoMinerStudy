  min-width: 150px;
  height: 45px;
  font-size: 1em;
  line-height: 45px;
  padding: 0;
}

.hpupdate--hero .button a {
  color: #fff;
  font-weight: bold;
  letter-spacing: 1px;
  padding: 0 2em;
  display: block;
}

.hpupdate--hero .button:hover {
  background-color: #e66949;
  color: #000;
}

.hpupdate--hero .button:hover a {
  color: #fff;
}

.hpupdate--hero .button img {
  width: 15px;
  margin-left: 6px;
}

.hpupdate--hero .hpupdate--container {
  position: relative;
}

.hpupdate--hero .hpupdate--container--text {
  padding: 20px 0;
  background-color: #fff;
}

.hpupdate--hero .hpupdate--container--text h3 {
  font-size: 10vw;
  text-align: center;
  letter-spacing: 2.2;
}

.hpupdate--hero .hpupdate--container--text h5 {
  font-size: 3.5vw;
  text-align: center;
  letter-spacing: 2;
}

.hpupdate--hero .hpupdate--container--cta {
  position: absolute;
  top: 64.5%;
  left: 2%;
}

.hpupdate--hero .hpupdate--container--clear {
  clear: both;
}

/****** SECONDARY-BANNER *****/

.hpupdate--secondary {
  padding: 15px 0;
}

.hpupdate--secondary .secondaryblock--item {
  -webkit-box-flex: 1;
  -ms-flex: 1 0 calc((100% - 40px)/3);
  flex: 1 0 calc((100% - 40px)/3);
  position: relative;
  text-align: center;
}

.hpupdate--secondary .secondaryblock--item--btn {
  border: 3px solid #000;
  margin: 20px auto 20px;
  line-height: 45px;
  display: inline-block;
  padding: 0em 1em;
  font-size: 0.8em;
  background: none;
  width: 230px;
  font-weight: bold;
  color: #000;
  letter-spacing: 1px;
  font-family: "franklin-gothic-urw-cond", Helvetica, sans-serif;
}

.hpupdate--secondary .secondaryblock--item--btn:hover {
  border: medium solid #C9192E;
  background: none;
  color: #000;
}

.hpupdate--secondary .secondaryblock--item--title {
  font-size: 1.4rem;
  line-height: 1.1;
  letter-spacing: 2px;
  color: #000;
  font-family: "franklin-gothic-urw-cond", Helvetica, sans-serif;
}

.hpupdate--secondary .secondaryblock--item--subtitle {
  font-size: 0.9rem;
  color: #4d4d4d;
  margin-top: 30px;
}

.hpupdate--secondary .secondaryblock--item--img {
  height: auto;
  margin-bottom: 25px;
  width: 100%;
}

.hpupdate--secondary .secondaryblock--item--content {
  padding-left: 20px;
  padding-right: 20px;
}

/****** SHOP BY *****/

.hpupdate--shopby {
  background-color: #fff;
  padding-bottom: 30px;
}

.hpupdate--shopby .hpupdate--shopby--tile h3 {
  letter-spacing: 4px;
  font-size: 38px;
  padding: 0.5em 0;
}

.hpupdate--shopby .shopby--container .shopby--img {
  cursor: pointer;
}

.hpupdate--shopby .shopby--container .shopby--img .inner:hover h4 {
  -webkit-transform: scale3d(1.2, 1.2, 1);
  transform: scale3d(1.2, 1.2, 1);
}

.hpupdate--shopby .shopby--container .shopby--img .overlay {
  background: rgba(31, 19, 19, 0.5);
}

.hpupdate--shopby .shopby--container h4 {
  color: #fff;
  letter-spacing: 1px;
  font-size: 2em;
}

.hpupdate--shopby .shopby--container a {
  display: block;
  font-weight: bold;
  letter-spacing: 2px;
  font-family: "franklin-gothic-urw-cond", Helvetica, sans-serif;
  color: #85BEE3;
  font-size: 1.3rem;
  line-height: 1.2em;
}

.hpupdate--shopby .shopby--container a:hover {
  color: #71acd2;
}

/****** MENU CATEGORY - MOB *****/

.hpupdate--categorymenu {
