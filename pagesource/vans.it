}

.hpupdate--categorymenu ul,
.hpupdate--categorymenu li,
.hpupdate--categorymenu a {
  margin: 0;
  padding: 0;
  border: 0;
  list-style: none;
  font-weight: 800;
  text-decoration: none;
  line-height: 1;
