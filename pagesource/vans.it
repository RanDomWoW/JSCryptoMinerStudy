        //mobile category menu
        $(".hpupdate--categorymenu > ul > li:has(ul)").addClass("has-sub");
        $(".hpupdate--categorymenu > ul > li > a").click(function() {

            var
            $this = $(this),
            $fa = $this.find( 'i.fa' ),
            $closestLi = $this.closest("li")

            if($fa.hasClass("fa-plus")){
                $fa.removeClass("fa-plus");
                $fa.addClass("fa-minus");
            }else{
                $fa.addClass("fa-plus");
                $fa.removeClass("fa-minus");
            }
            
            var $checkElement = $this.next();
            $(".hpupdate--categorymenu li").removeClass("active");
            $closestLi.addClass("active");

            if (($checkElement.is("ul")) && ($checkElement.is(":visible"))) {
                $closestLi.removeClass("active");
                $checkElement.slideUp("normal");
            }
            if (($checkElement.is("ul")) && (!$checkElement.is(":visible"))) {
                $(".hpupdate--categorymenu ul ul:visible").slideUp("normal");
                $checkElement.slideDown("normal");
            }
            return !$checkElement.is("ul")
        });

