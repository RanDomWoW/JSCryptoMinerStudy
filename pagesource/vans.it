}

.hpupdate--categorymenu > ul > li > a {
  font-size: .9em;
  display: block;
  color: #000;
  text-transform: uppercase;
}

.hpupdate--categorymenu > ul > li > a:active {
  color: #000;
}

.hpupdate--categorymenu > ul > li > a > span {
  display: block;
  padding: .8em 1.8em;
}

.hpupdate--categorymenu > ul > li > a:hover {
  text-decoration: none;
}

.hpupdate--categorymenu > ul > li .active {
  border-bottom: none;
}

.hpupdate--categorymenu ul ul {
  display: none;
  background: #fff;
}

.hpupdate--categorymenu ul ul li {
  padding: 0;
  border-bottom: thin solid #d4d4d4;
  border-top: none;
  background: #F1F2ED;
}

.hpupdate--categorymenu ul ul li:last-child {
  border-bottom: none;
}

.hpupdate--categorymenu ul ul a {
  padding: 15px 10px;
  text-indent: 25px;
  display: block;
  color: #589BC6;
  font-size: .7em;
  font-weight: normal;
  font-family: Helvetica, Arial, sans-serif;
  letter-spacing: .15em;
  text-transform: uppercase;
}

.hpupdate--categorymenu ul ul a:hover {
  color: #C9192E;
}

.hpupdate--categorymenu > ul > li:last-child {
  border-bottom: 1px #d4d4d4 solid;
}

.hpupdate--categorymenu .fa {
