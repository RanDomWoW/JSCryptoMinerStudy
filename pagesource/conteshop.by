    <script type="text/javascript">
        (function () {
            var cities = [];
            window.onload = function () {
                                jQuery(function () {
                    setTimeout(function () {
                        jQuery('.currentcity-modal  .modal-content  .title').html(
                            "Ваш город &mdash; Минск?"
                        );
                        jQuery('.currentcity-modal  .modal-content  .comment').html(
                            "<!--noindex-->от вашего выбора зависит стоимость товара и доставки<!--/noindex-->"
                        );
                        var city_el_offset = jQuery('.b-country .city_name').offset();
                        if(jQuery(window).width()>=860) {
                            //jQuery('.currentcity-modal').show().addClass('cart_fadeInUp');
                            jQuery('.currentcity-modal').offset({
                                top: city_el_offset.top+34,
                                left: city_el_offset.left+jQuery('.b-country .city_name').width()+40
                            });
                        }
                    }, 1000);

                    jQuery('.change-city a').on('click', function (e) {
                        e.preventDefault();
                        jQuery('#choice-city-modal').modal();
                        jQuery('.currentcity-modal').hide();
                    });
                });
                                jQuery('.clear-input').on('click', function () {
                    var input = jQuery(this).prev();
                    input.val('');
                    jQuery("#city-id").val('');
                    jQuery(this).hide();
                    input.focus();
                });
                jQuery('.form-city-save').on('click', function () {
                    if (jQuery("#city-id").val().length > 0) {
                        sendChangeCity('https://conteshop.by/?'+jQuery("#city-id").parents('form').serialize());
                        jQuery('#choice-city-modal').modal('hide');
                        jQuery('.b-country .city_name').text(jQuery('#city-filter-name').val());
                    }
                });
                jQuery('#city-filter-name').on('keyup', onCityChanged);
                jQuery('.cities-modal').on('show.bs.modal', function(){
                    if (cities.length == 0) {
                        jQuery.post('https://conteshop.by/ru/region/city/get_list/')
                            .done(function (data) {
                                cities = data;
                                jQuery('#city-filter-name').autocomplete({
                                    minLength: 0,
                                    appendTo: '#menu-container',
                                    source: function (request, response) {
                                        var data = filterCities(request.term);
                                        response(data);
                                    },
                                    select: function (event, ui) {
                                        jQuery("#city-filter-name").val(ui.item.name);
                                        jQuery("#city-id").val(ui.item.city_id);
                                        return false;
                                    },
                                    messages: {
                                        results: function () {
                                            return ""
                                        },
                                        noResults: ""
                                    }
                                }).data('uiAutocomplete')._renderItem = function (ul, item) {
                                    var region = '<span>' + item.region_name + '</span>';
                                    if(!item.region_name) region = "";
                                    return jQuery('<li></li>')
                                        .data('item.autocomplete', item)
                                        .append('<a>' + item.name + region + '</a>')
                                        .appendTo(ul);
                                };
                            });
                    }
                });
                jQuery('.city_name').on('click', function () {
                    jQuery('.cities-modal').modal();
                    if (jQuery('.currentcity-modal').is(':visible'))
                        jQuery('.currentcity-modal').hide();
                });

                jQuery('.submit-city a').on('click', function (e) {
                    e.preventDefault();
                    jQuery('.currentcity-modal').hide();
                    sendChangeCity(jQuery(this).attr('href'));
                });
                jQuery('.city a').on('click', function (e) {
                    e.preventDefault();
                    sendChangeCity(jQuery(this).attr('href'));
                    jQuery('#choice-city-modal').modal('hide');
                    jQuery('.b-country .city_name').text(jQuery(this).text());
                });

                onCityChanged();
            }

            function onCityChanged() {
                jQuery("#city-id").val('');
                var btn = jQuery(this).next();
                if (jQuery('#city-filter-name').val().length > 0) {
                    btn.show();
                } else
                    btn.hide();
            }

            function sendChangeCity(url) {
                jQuery.ajax({method:'POST',url:url,dataType:'json'}).done(function (data){
//                    if(typeof data=='object' && data && data.redirect && window.location!=data.redirect) {
//                        window.location=data.redirect;
//                    } else {
                        window.location.reload();
//                    }
                });
            }
            function filterCities(name) {
                var filtered = [];
                for (var i in cities) {
                    if (cities[i].name.toLocaleLowerCase().indexOf(name.toLocaleLowerCase()) != -1)
                        filtered.push(cities[i]);
                }
                return filtered.splice(0, 10);
            }
        })()
    </script>
