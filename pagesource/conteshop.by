		var isIE7= new RegExp("MSIE 7").test(navigator.userAgent);
		var iffix="";
		if(isIE6) {iffix="<iframe id='iffix'></iframe>";}
		var iprocess='<div id="loadmask"><p class="loader"><img alt="Loading..." src="https://conteshop.by/ru/skin/frontend/default/default/sm/cartpro/images/ajax-loader-tr.gif"><br>Please wait...</p></div>'+iffix;
		var iconfirm="";
