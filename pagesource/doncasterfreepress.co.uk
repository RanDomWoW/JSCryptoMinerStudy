    function initRegistrationText(widgetId) {
        var isRegistrationReadyInterval = setInterval(function () {
            if (isTemplateReady(widgetId)) {
                addCustomTextToRegistrationTemplate(widgetId);
                clearInterval(isRegistrationReadyInterval)
            }
        }, 300);
    }

    function initLoginText(widgetId) {
        var isLoginReadyInterval = setInterval(function () {
            if (isLoginTemplateReady(widgetId)) {
                addCustomTextToLoginTemplate(widgetId);
                clearInterval(isLoginReadyInterval)
            }
        }, 300);
    }

    function initEditProfileText(widgetId) {
        var isEditProfileReadyInterval = setInterval(function () {
            if (isEditProfileTemplateReady(widgetId)) {
                addCustomTextToEditProfileTemplate(widgetId);
                clearInterval(isEditProfileReadyInterval)
            }
        }, 300);
    }

    function initCreateAccountText(widgetId) {
        var isCreateAccountReadyInterval = setInterval(function () {
            if (isCreateAccountTemplateReady(widgetId)) {
                addCustomTextToCreateAccountTemplate(widgetId);
                clearInterval(isCreateAccountReadyInterval)
            }
        }, 300);
    }

    function addCustomTextToRegistrationTemplate(widgetId) {
        addTextAfterNonAuthAttribute(widgetId, 2, "<p>Would you like to receive the latest news, news features and service updates from Doncaster Free Press via email?</p>")
        addTextAfterNonAuthAttribute(widgetId, 2, "<h4 class='ev-newsletter-title'>Newsletters</h4>")
        addTextAfterNonAuthAttribute(widgetId, 3, "<p>Would you like to receive more information from Johnston Press, via occasional emails, to keep you informed about new features, special offers, promotions, events and services that might be of interest to you?</p>")
        addFooterText(widgetId, '<p>By clicking on register, you will be creating an account with Doncaster Free Press and agreeing to Johnston Press&#39; Standard <a class="ext-link" href="http://www.johnstonpress.co.uk/terms-conditions" target="_blank" title="Terms and Conditions">Terms and Conditions</a>.</p>')
        addLinkToAnotherScreen(widgetId, '<p class="text-center">Already registered? <a onclick="EV.Event.publish(\'ev.close.modal.registration-widget\'); EV.Event.publish(\'ev.open.modal.login-widget\')">Login here.</a></p>')
    }

    function addCustomTextToLoginTemplate(widgetId) {
        addLinkToAnotherScreen(widgetId, '<p class="text-center">Don&#39;t have an account with us? <a onclick="EV.Event.publish(\'ev.close.modal.login-widget\'); EV.Event.publish(\'ev.open.modal.registration-widget\')">Create one today.</a></p>')
    }

    function addCustomTextToEditProfileTemplate(widgetId) {
        addTextAfterNonAuthAttribute(widgetId, 8, "<h4 class='ev-newsletter-title'>Newsletters</h4><br><p>Would you like to receive the latest news, news features and service updates from Doncaster Free Press via email?</p>")
