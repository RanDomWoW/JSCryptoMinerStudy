    }

    function addCustomTextToCreateAccountTemplate(widgetId) {
        addTextAfterNonAuthAttribute(widgetId, 3, "<h4 class='ev-newsletter-title'>Newsletters</h4><br><p>Would you like to receive the latest news, news features and service updates from Doncaster Free Press via email?</p>")
