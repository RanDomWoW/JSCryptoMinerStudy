                                $(".childCount1,.childCount2").show();
                            } else if ($(this).attr("value") == "3") {
                                $(".box").hide();
                                $(".childCount1,.childCount2,.childCount3").show();
                            } else if ($(this).attr("value") == "4") {
                                $(".box").hide();
                                $(".childCount1,.childCount2,.childCount3,.childCount4").show();
                            } else if ($(this).attr("value") == "5") {
                                $(".box").hide();
                                $(".childCount1,.childCount2,.childCount3,.childCount4,.childCount5").show();
                            } else if ($(this).attr("value") == "6") {
