<script>

    slickifyPanel();

    function switchTab(tabkey){
        $('.kep-5401 .inspiration-slider').hide();
        $('.kep-5401 > h2 .inspiration-menu  > span').removeClass("active");
        $('.kep-5401').find('.inspiration-slider[data-id='+tabkey+']').css('display','block');
        changeSliderClass(tabkey);
        eventTracker.fireEvent('HomePage', 'click' , tabkey + " - tab");
    }

    function changeSliderClass(tabkey) {
            currentSlide = parseInt($('.kep-5401 .inspiration-slider[data-id='+tabkey+'] > ul li.slick-current').attr("data-slick-index"));

            if (currentSlide === 9) {
                $('.kep-5401 .slider-arrows >div.next').addClass('low_opacity');
            }
            else {
                $('.kep-5401 .slider-arrows >div.next').removeClass('low_opacity');
