<script type="text/javascript">
$(function(){
	
	var $menuTimer;
	var $gnb_container = $(".gnb");
	var $gnb_menu = $gnb_container.find(">li>a");
	var $gnb_menu_sub = $gnb_container.find(">li>div");
	var $menubox_width = $gnb_container.width();
	
	$gnb_container.find(">li:not(.on)>div").hide();
	$gnb_container.find(">li.on").addClass("active");
	
	//初始化子菜单的默认位置
	$gnb_menu_sub.each(function(i){
		var pMenuLeft = $(this).parent().position().left;
		var sMenuWidth = $(this).width();

		if (pMenuLeft + sMenuWidth > $menubox_width) {
			$(this).css("left", ($menubox_width - sMenuWidth - pMenuLeft).toString(10).concat("px"));
		}else{
			$(this).css("left", "0px");
		}
	});
	
	//一级菜单绑定鼠标事件
	$gnb_menu.bind('mouseover', function(){

		$gnb_container.find(">li")
			.removeClass("on")
			.find(">div").hide("fast");

		$(this)
			.siblings("div").show("normal")
			.end()
			.parent().addClass("on");

		clearTimeout($menuTimer); 
	});
	
	$gnb_menu_sub.bind('mouseover', function(){ clearTimeout($menuTimer); });
	$gnb_menu.bind('mouseout',function(){ $menuTimer = setTimeout(timerSet,500); });
	$gnb_menu_sub.bind('mouseout', function(){ $menuTimer = setTimeout(timerSet,500); });
	
	function timerSet(){
		$gnb_container.find(">li")
			.removeClass("on")
			.find("div").hide("fast");

		$gnb_container.find(">li.active>div")
			.fadeIn("normal")
			.parent()
				.addClass("on");
	}
	
});
</script>
