<script>
    $(document).ready(function() {

        if(Cookies.get('avaz-slider')==1) {
            $('.switch').trigger('click')
            slider_on_off($('.switch'), $(".slider-activated"), $(".block1"))
            $('#reklama-1').css('height', '0')
            $('#reklama-1').css('overflow', 'hidden')
            $('#reklama-1').css('margin-top', '0')
        }

        $('.switch').change(function(){
            slider_on_off($(this), $(".slider-activated"), $(".block1"))
        });

        function slider_on_off(switch_button, slideActive, firstBlock) {
            setCookieSlider(slideActive)
            switch_button.toggleClass('checked')
            firstBlock.fadeToggle(100)
            slideActive.fadeToggle(100)
        }

        function setCookieSlider(slideActive){
            if(slideActive.css('display')=='block') {
                Cookies.set('avaz-slider', '0', { expires: 7 });
                $('#reklama-1').css('height', 'auto')
                $('#reklama-1').css('overflow', 'visible')
                $('#reklama-1').css('margin-top', '20px')
            }else{
                Cookies.set('avaz-slider', '1', { expires: 7 });
                $('#reklama-1').css('height', '0')
                $('#reklama-1').css('overflow', 'hidden')
                $('#reklama-1').css('margin-top', '0')
            }
        }

    });

</script>
