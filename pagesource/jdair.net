<script type="text/javascript">
function captchaInputChange(obj){
	var value=obj.value;
	if(value.length==4){
		if((/[\u4e00-\u9fa5]+/).test(value)) {
			value = encodeURI(encodeURI(value));
		} 
		var url = '/captchaCheck.action?j_captcha_response='+value;
		$.ajax({
	  		  type: 'POST',
	  		  url: url,
	  		  success: function(responseText, textStatus, XMLHttpRequest){
	  			    
	  			   	var result=responseText.result;
	  			 	document.getElementById(obj.id+"_message").style.display="";
	  			   	if(result){
	  			   		document.getElementById(obj.id+"_message").src="/themes/deerair7/images/frontend/captcha_message_succ.gif";
	  			   	}else{
	  			   		document.getElementById(obj.id+"_message").src="/themes/deerair7/images/frontend/captcha_message_fail.gif";
	  			   		if(responseText.type != 'nc') {
		  			   		changecode(obj);
	  			   		}
	  			   	}
	  		  }
	  		});
	}else{
		document.getElementById(obj.id+"_message").style.display="none";
	}
}
function keyDown(event,ele){
	if (event.keyCode == 13){
		checkIndexTicket();
	}
}
