                return false;
            }
            //alert(sceneryorgCity+'=='+orgCity64);
            //alert(scenerydstCity+'=='+dstCity64);
            //var url = "http://holiday.jdair.net/queryindex/";
            var url = "http://holiday.jdair.net/queryindex/";
            if(orgCity64 != ""){
                url += orgCity64 + "-u1";
            }
            if(dstCity64 != ""){
