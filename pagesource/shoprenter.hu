<script>
    $(document).ready(function(){

        new ShopRegistration();

        $('.sendskype').click(function(e) {
            e.preventDefault();
            var isOkay = 1;
            var error = [];

            var $formToValidate = $(this).parents('form:first');

            if ( $formToValidate.find( $('.skypename') ).val().length == 0 ) {
                isOkay = 0;
                error.push('Nem adta meg a nevét!');
            }

            if (!(/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,4})+$/.test( $formToValidate.find( $('.skypeemail') ).val() ) ) ) {
                isOkay = 0;
                error.push('Nem megfelelő email cím!');
            }

            if ( $formToValidate.find( $('.skypeindulas') ).val().length == 0) {
                isOkay = 0;
                error.push('Nem adta meg az indulás várható időpontját!');
            }

            if ( $formToValidate.find( $('.skypephone') ).val().length == 0) {
                isOkay = 0;
                error.push('Nem adott meg telefonszámot!');
            }

            if (isOkay == 1) {
                form_submitted = true;
                $($formToValidate).submit();
            } else {
                alert(error.join("\n"));
                return false;
            }
        });

        $('#sendShopMigration').click(function(e) {
            e.preventDefault();
            var isOkay = 1;
            var error = [];

            if ($('#migr_name').val().length == 0) {
                isOkay = 0;
                error.push('Nem adta meg a nevét!');
            }

            if (!(/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,4})+$/.test($('#migr_email').val()))) {
                isOkay = 0;
                error.push('Nem megfelelő email cím!');
            }

            if ($('#migr_phone').val().length == 0) {
                isOkay = 0;
                error.push('Nem adott meg telefonszámot!');
            }

            if ($('#migr_url').val().length == 0) {
                isOkay = 0;
                error.push('Nem adott meg webáruház URL-t!');
            }

            if (isOkay == 1) {
                form_submitted = true;
                $('#migrationForm').submit();
            } else {
                alert(error.join("\n"));
                return false;
            }
        });
    });
</script>

