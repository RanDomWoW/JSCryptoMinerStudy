/* Arama Formu */
/*#arama { position: relative; float:left; width:320px; height:40px; margin:-2px 0px 0px 0px; padding: 0px; background:url(//cdn.lody.net/images/vidyomax.png) 0px -114px no-repeat; }*/
#arama {
position: relative;
float: left;
width: 320px;
padding: 0px;
margin:0px;
}
#arama:before {
top: 0px;
width: 290px;
height: 0px;
border-width: 34px 13px 0px;
border-style: solid;
border-color: #E07F01 transparent;
z-index: 5;
}
#arama:after {
width: 280px;
height: 0px;
top:34px;
right:13px;
border-width: 2px 5px 0px;
border-style: solid;
border-color: #E07F01 transparent;
z-index: 5;
}
#arama:before, #arama:after, .search-wrap:after, .search-wrap:before {
position: absolute;
display: block;
content: "";
}
.search-wrap:before {
    background: rgba(87, 87, 87, 0.1);
    height:1px;
    right:17px;
    top:36px;
    width: 288px;
}
.search-wrap:after {
    background: rgba(87, 87, 87, 0.1);
    height:1px;
    width:282px;
    right:20px;
    top:37px;
}

.search-wrap { position: absolute; margin:-2px 0px 0px -3px; z-index: 6; width:100%;}
#arama #s { float:right; background: url(//cdn.lody.net/images/sprite.png) no-repeat 0px -72px transparent; margin: 0px 12px 0px 5px; padding:7px 37px 8px 5px; width:152px; overflow: hidden; border:none; color:#292929; font-family:  arial, sans-serif;}
#aramabuton { float:right; background: url(//cdn.lody.net/images/sprite.png) no-repeat -69px -38px transparent; width:96px; margin: 0px 0px 0px 3px; padding:5px 7px 8px 0px; border:none; cursor:pointer; font-size: 14px; font-weight:bold; font-family: arial, sans-serif; color:#fff; text-transform: uppercase;}
#aramabuton:hover { }
#arama #s:-moz-placeholder:focus {color:#ccc;}

#search_home { margin:45px 0 0 22px;}

