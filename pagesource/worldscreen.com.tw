<script>

/**
*	for index
*/
function _movie_move(self){
	//$(self).data('index', 0);
	$(self).attr('index', 0);
	var _slider = $('#item_slider', $(self));
	var _items = $('div.movie', $(self));
	var _speed = 500;
	var _num = 4;
	var _width = 114;
	if (_items.length>=5){
		$('#items', self).css({
			'width' : $(_items[0]).width()*(_items.length*10), 
			'left' : 0
		});
		$('#prevus', self).on('click', function(){
			var type = 'p';
			var _index = parseInt($(self).attr('index'));
			var _position;
			_index = _index - _num ;
			_index = Math.max(_index, 0);
			_position = -(_index*_width);
			$(self).attr('index', _index);
			$('#items', self).clearQueue().stop().animate({
				left: _position ,
			}, _speed, function(){ $(self).attr('index', _index); });	
			return false;
		});
		$('#next', self).on('click', function(){					
			var type = 'n';
			var _index = parseInt($(self).attr('index'));
			var _position;
			_index = _index + _num ;
			var max = $('div.movie', self).length+1;
			max = Math.min( Math.floor(max/_num)*_num ) - _num;
			_index = Math.min(_index, max);
			_position = -(_index*_width);
			$(self).attr('index', _index);
			$('#items', self).clearQueue().stop().animate({
				left: _position ,
			}, _speed, function(){ 
				$(self).attr('index', _index);
				var _img = $('div.movie', $(self));
				var n = _index + 8;
				for(var i=_index;i<n;i++){
					if (typeof(_img[i])!=='undefined'){ var img = $('img.lazyload', _img[i]); $(img).attr('src', $(img).data('original') ); }
				}
			});
			return false;
		});
	}
}	
$('div#movie_tops_weeks, div#movie_tops_nows, div#movie_tops_soons, div#movie_tops_news').each(function(){
	_movie_move(this);
});	


/**
*	首頁投票
*/
$("#vote", $('#index_vote')).on('change', function(){
	var option_id = $('option:selected', this).val();
	if (/^[0-9]+$/.test(option_id)){
		$("#option_id", $("#frmVote")).val(option_id);
		$("#vote_new", $("#frmVote")).val(option_id);
		document.getElementById('frmVote').submit();
		return false;
	}
	return false;
