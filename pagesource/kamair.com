    <script>

        jQuery(document).ready(function () {



            window.flightSearch = new FlightSearch();

            flightSearch.dateFormat = "dd/MM/yyyy";
            flightSearch.actionURL = "https://book-kamair.crane.aero/web/RezvEntry.xhtml";
            flightSearch.init();
            flightSearch.selectTripType("ONE_WAY");

            var startDate = new Date('01/01/2018');
            var FromEndDate = new Date();
            var ToEndDate = new Date();

            ToEndDate.setDate(ToEndDate.getDate() + 365);

            jQuery('#departureDate').datepicker({

                /*weekStart: 1,*/
                startDate: 'today',
                /*endDate: FromEndDate,*/
                todayHighlight: true,
                format: "dd/mm/yyyy",
                language: "en",
                autoclose: true
            })
                    .on('changeDate', function (selected) {
                        startDate = new Date(selected.date.valueOf());
                        startDate.setDate(startDate.getDate(new Date(selected.date.valueOf())));
                        jQuery('#returnDate').datepicker('setStartDate', startDate);
                    });
            jQuery('#returnDate').datepicker({
                /*weekStart: 1,*/
                startDate: startDate,
                /*endDate: ToEndDate,*/
                todayHighlight: true,
                format: "dd/mm/yyyy",
                language: "en",
                autoclose: true

            })
                    .on('changeDate', function (selected) {
                        FromEndDate = new Date(selected.date.valueOf());
                        FromEndDate.setDate(FromEndDate.getDate(new Date(selected.date.valueOf())));
                        jQuery('#departureDate').datepicker('setEndDate', FromEndDate);
                    });


        });

    </script>

