<!-- Inline JS -->
<script type="text/javascript">
//ADD T00338 ブランドタブ用カルーセル
function brandWheelOwl() {
	$(".pre-carousel").owlCarousel({
		loop: false,
		items: 3,
		autoplay: false,
		thumbs: false,
		itemClass: "owl-item owl_brand_wheel",
		nav: true,
		lazyLoad: true,
		stagePadding: 85,
	});
}

$(function() {
	//初期表示時タブ画像変更
	$('#tab .select img').attr('src', $('#tab .select img').attr('src').replace('_off', '_current'));

	//表示・非表示制御(ブランドから探す)
	//class="drop_shipping"
	if(location.href.match(/pc_wheel/i)) {
		$(".brand_find_wheel dl.drop_shipping").css({"display":"none"});
	}

	//タブ切替
	$("#tab li").click(function() {
		var num = $("#tab li").index(this);
		$(".content_wrap").addClass('dis_none');
		$(".content_wrap").eq(num).removeClass('dis_none');
		$("#tab li").removeClass('select');
		$(this).addClass('select');

		//タブ画像変更
		$('#tab li').each(function () {
			if(!$(this).hasClass('select')) {
				$('img', this).attr('src', $('img', this).attr('src').replace('_current', '_off'));
			}
		});
		$('#tab .select img').attr('src', $('#tab .select img').attr('src').replace('_on', '_current'));

		//ADD T00335 クッキー更新
		updateSelectCookie();
	});

	$('#tire_size_17').parent().addClass('size_select');
	$('#menu_tire_size_17').removeClass('dis_none');

	//Cookie保持時処理
	if($.cookie("select_tab") == "size_find_tab") {
		//TOP画面タブ切替
		$('#shashu_find_tab').removeClass('select');
		$('#size_find_tab').addClass('select');
		$('#brand_find_tab').removeClass('select'); //ADD T00338

		//TOP画面タブ中身切替
		$('#shashu_find_content').addClass('dis_none');
		$('#size_find_content').removeClass('dis_none');
		$('#brand_find_content').addClass('dis_none'); //ADD T00338

		if($.cookie("select_size")) {
			//最後に選択された要素の選択
			$('#menu_tire_size_17').addClass('dis_none');
			$('#menu_' + $.cookie("select_size")).removeClass('dis_none');
			$('#tire_size_tabs li').removeClass('size_select');
			//UPD STR T00557
			// $('#tire_size_tabs li').eq($.cookie("select_index")).addClass('size_select');
			if($('#'+$.cookie("select_size")).length){
				$('#'+$.cookie("select_size")).parent().addClass('size_select');
			}else{
				$('#tire_size_17').parent().addClass('size_select');
				$('#menu_tire_size_17').removeClass('dis_none');
			}
			//UPD END T00557

		}

		//タブ画像切替
		$('#tab li img').each(function () { 
			$(this).attr('src', $(this).attr('src').replace('_current', '_off'));
		});
		$('#tab .select img').attr('src', $('#tab .select img').attr('src').replace('_off', '_current'));
	} else if($.cookie("select_tab") == "shashu_find_tab") {
		//TOP画面タブ切替
		$('#shashu_find_tab').addClass('select');
		$('#size_find_tab').removeClass('select');
		$('#brand_find_tab').removeClass('select'); //ADD T00338

		//TOP画面タブ中身切替
		$('#shashu_find_content').removeClass('dis_none');
		$('#size_find_content').addClass('dis_none');
		$('#brand_find_content').addClass('dis_none'); //ADD T00338

		//タブ画像切替
		$('#tab li img').each(function () { 
			$(this).attr('src', $(this).attr('src').replace('_current', '_off'));
		});
		$('#tab .select img').attr('src', $('#tab .select img').attr('src').replace('_off', '_current'));
	} else if($.cookie("select_tab") == "brand_find_tab") { //ADD T00338
		//TOP画面tタブ切替
		$('#shashu_find_tab').removeClass('select');
		$('#size_find_tab').removeClass('select');
		$('#brand_find_tab').addClass('select');

		//TOP画面タブ中身切替
		$('#shashu_find_content').addClass('dis_none');
		$('#size_find_content').addClass('dis_none');
		$('#brand_find_content').removeClass('dis_none');

		//タブ画像切替
		$('#tab li img').each(function () { 
			$(this).attr('src', $(this).attr('src').replace('_current', '_off'));
		});
		$('#tab .select img').attr('src', $('#tab .select img').attr('src').replace('_off', '_current'));

		brandWheelOwl();
	}

	//サイズから探す画面 サイズ画像クリック時
	$('#tire_size_tabs li').click(function() {
		var number = $('#tire_size_tabs li').index(this);
		$(".select_contents").addClass('dis_none');
		$(".select_contents").eq(number).removeClass('dis_none');
		$("#tire_size_tabs li").removeClass('size_select');
		$(this).addClass('size_select');

		//ADD T00335 クッキー更新
		updateSelectCookie();

		//ページ内スクロール
		var headerHight = 100; //ヘッダ固定の高さ
		var speed = 300;
		var position = $('#size_anchor').offset().top - headerHight; //ヘッダ固定の高さ分をずらす
		$("html, body").animate({scrollTop:position}, speed, "swing");
		return false;
	});

	//ADD T00338 ブランドから探す画面 クリック時
	$('#brand_find_tab').click(function() {
		updateSelectCookie();
		brandWheelOwl();
	});

	//ADD T00335 クリック時にCookie保持するように変更
	//クリック時にCookieに選択状況保持
	function updateSelectCookie(){
		$.cookie("select_tab", $('ul#tab > li.select').attr('id'));
		if($('#tire_size_tabs li.size_select span').length != 0) {
			$.cookie("select_size", $('#tire_size_tabs li.size_select span').attr('id'));
			$.cookie("select_index", $('#tire_size_tabs li').index($('#tire_size_tabs li.size_select')));
		}
	}

	//フェード表示
	$('li#shashu_find_tab').fadeIn(300);
	$('li#size_find_tab').fadeIn(300);
	$('li#brand_find_tab').fadeIn(300); //ADD T00338
});
</script>
