<script type="text/javascript" src="//api.docodoco.jp/v5/docodoco?key=rMFlI9gZWV7qKwENfFHDHRdSsSJe2DJEmXgJ2MpzBM0QeL07rkYeUNeqDeX5xwby" charset="utf-8"></script>
<script type="text/javascript">
(function($){ $(function(){
  var isSmartPhone = function(){
    var useragents = [
      "iPhone",
      "iPod",
      "Android",
      "dream",
      "CUPCAKE",
      "blackberry9500",
      "blackberry9530",
      "blackberry9520",
      "blackberry9550",
      "blackberry9800",
      "webOS",
      "incognito",
      "webmate"
    ];
    var regexp = new RegExp(useragents.join('|'), 'i');
    return navigator.userAgent.match(regexp);
  }

  var getApiLocation = function(position){
    $.cookie("lat", position.coords.latitude, { expires: 30, path: '/' });
    $.cookie("lng", position.coords.longitude, { expires: 30, path: '/' });
    $.cookie("cf", 70, { expires: 30, path: '/' });
    $.cookie("pref", "undefined", { expires: 30, path: '/' });
    getRecommendStores(position.coords.latitude, position.coords.longitude, $.cookie("cf"));
  }

  var errorCallback = function(error){
    getAreaData();
  }

  //ADD T00416
  var getTirepitUrl = function(){
    $.ajax({
      dataType: 'json',
      async: false,
      url: "//" + location.hostname + "/form/ajax/getPrefecturesWord",
      data: { pref: $('#areaTagetingRecommendStores div').eq(0).find('.des_area input').val() },
      success: function(data) {
        var link_url = 'https://www.tirepit.jp/stores/prefecture_search/' + data.prefectures_word +'?prefecture_lat=' + $.cookie("lat") + '&prefecture_lng=' + $.cookie("lng") + '&rg=10';
        $("#areaRecommend > a").attr("href", link_url);
      }
    });
  }

  var getRecommendStores = function(lat, lng, cf, pref){
    if(typeof pref === "undefined"){ pref = ""; }
    var url = "//" + location.hostname + "/form/ajax/tirepitAreaRecommendStore?lat=" + lat + "&lng=" + lng + "&cf=" + cf + "&maxRows=" + 5 + "&pref=" + pref;

    $.getJSON( url, function(data){
      var idx = 0;

      if(data['statusmsg'] == "all"){
        $("#areaRecommend").empty();
        var htm = "\
          <div class='cf'><a href='https://www.tirepit.jp/content/publish' target='_blank' style='float:left;'><img src='https://lpcdn.autoway.jp/autoway/pc_2016/img/toppage/bn_tp2loop.jpg' alt='取付店様大募集'></a></div>";
        $("#areaRecommend").append(htm);
      }else{
        $.each(data['stores'], function() {
          var htm ="\
            <div>\
              <div class='photo_area'>\
                <a class='shop_url' href='{{url}}' target='_blank'><img width='100' alt='' src='{{storeimage}}'></a>\
              </div>\
              <div class='des_area'>\
                <a class='t_shop_url' href='{{url}}' target='_blank'>{{storename}}</a>\
                <span class='t_tel'>TEL：{{tel}}</span>\
                <dl><dt class='t_zip'>〒{{zip}}</dt><dd class='t_address'>{{address}}</dd></dl>\
                <p class='t_shop_text'>{{comment}}</p>\
                <a class='t_shop_rul_btn' href='{{url}}' target='_blank'><img src='https://lpcdn.autoway.jp/autoway/images/torituke_btn1_off.jpg' alt='取付店詳細情報' width='171' height='32'></a>\
                <input type='hidden' value='{{storeprefecture}}'>\
              </div>\
            </div>";
          htm= htm.replace('{{idx}}',idx);
          htm= htm.replace('{{storename}}',this['storename']);
          htm= htm.replace(/{{url}}/g,this['garenaviurl']);
          htm= htm.replace('{{storeimage}}',this['storeimage']);
          htm= htm.replace('{{tel}}',this['storetel']);
          htm= htm.replace('{{zip}}',this['storezip'].slice(0, 3) + '-' + this['storezip'].slice(3, this['storezip'].length));
          htm= htm.replace('{{address}}',this['storeprefecture']+this['storecity']+this['storeaddress1']+this['storeaddress2']);
          htm= htm.replace('{{comment}}',this['comment']);
          htm= htm.replace('{{storeprefecture}}',this['storeprefecture']);

          $('#areaTagetingRecommendStores').append(htm);
          idx +=1;
        });
        $('#areaTagetingRecommendStores').append("<div class='cf'><a href='https://www.tirepit.jp/content/publish' target='_blank' style='float:left;'><img src='https://lpcdn.autoway.jp/autoway/pc_2016/img/toppage/bn_tp2loop.jpg' alt='取付店様大募集'></a></div>");

        if($.cookie("cf") >= 70 && $.cookie("pref") == "undefined"){
          getTirepitUrl();
          $("#areaRecommend .shop_title").text("タイヤは直送・統一工賃で簡単取付！お近くのオススメ取付店はコチラ");
        }else if($.cookie("cf") < 70 && $.cookie("cf") >= 50){
          getTirepitUrl();
          $("#areaRecommend .shop_title").text("タイヤは直送・統一工賃で簡単取付！" + $.cookie("prefName") + "内のオススメ取付店はコチラ");
        }else if($.cookie("cf") >= 70){
          getTirepitUrl();
          $("#areaRecommend .shop_title").text("タイヤは直送・統一工賃で簡単取付！" + $.cookie("prefName") + $.cookie("city") + "付近のオススメ取付店はコチラ");
        }else{
          $("#areaRecommend > a").children().unwrap();
        }
        //exception
        if(data['statusmsg'] == "pref"){
          getTirepitUrl();
          $("#areaRecommend .shop_title").text("タイヤは直送・統一工賃で簡単取付！" + $.cookie("prefName") + "内のオススメ取付店はコチラ");
        }
      }
    })
  }

  var getAreaData = function(){
    var lat;
    var lng;
    var cf;
    var prefName;
    var pref;
    var city;

    if(typeof SURFPOINT == "undefined"){
      if(typeof $.cookie("cf") === "undefined"){
        //no data CF：0
        getRecommendStores(0, 0, 0);
      }else{
        getRecommendStores($.cookie("lat"), $.cookie("lng"), $.cookie("cf"), $.cookie("pref"));
      }
    }else{
      lat = SURFPOINT.getCityLatitude();
      lng = SURFPOINT.getCityLongitude();
      //UPD T00529 2017/05/31 k.rori
      var cityCf = SURFPOINT.getCityCF(); //市区町村
      var prefCf = SURFPOINT.getPrefCF(); //都道府県
      prefName = SURFPOINT.getPrefJName();
      pref = SURFPOINT.getPrefCode();
      city = SURFPOINT.getCityJName();

      //ADD T00529 2017/05/31 k.rori
      if(cityCf >= 71){
        cf = cityCf;
      }else if(prefCf >= 31){
        cf = 51;
      }else{
        cf = prefCf;
      }

      if(cf < 50){
        pref = 0;
      }

      //都道府県追記
      switch (pref){
        case "01":
          break;
        case "13":
          prefName = prefName + "都";
          break;
        case "26":
        case "27":
          prefName = prefName + "府";
          break;
        default:
          prefName = prefName + "県";
          break;
      }

      if(typeof $.cookie("cf") === "undefined"){
        $.cookie("lat", lat, { expires: 30 });
        $.cookie("lng", lng, { expires: 30 });
        $.cookie("cf", cf, { expires: 30 });
        $.cookie("pref", pref, { expires: 30 });
        $.cookie("prefName", prefName, { expires: 30 });
        $.cookie("city", city, { expires: 30 });
      }else{
        if(Number(cf) >= Number($.cookie("cf"))){
          $.cookie("lat", lat, { expires: 30 });
          $.cookie("lng", lng, { expires: 30 });
          $.cookie("cf", cf, { expires: 30 });
          $.cookie("pref", pref, { expires: 30 });
          $.cookie("prefName", prefName, { expires: 30 });
          $.cookie("city", city, { expires: 30 });
        }
      }

      getRecommendStores($.cookie("lat"), $.cookie("lng"), $.cookie("cf"), $.cookie("pref"));
    }
  }

  if(isSmartPhone()){
    if($.cookie("cf") != 100){
      //Present location
      navigator.geolocation.getCurrentPosition(getApiLocation, errorCallback);
    }else{
      getAreaData();
    }
    return false;
  }
  getAreaData();
}); })(jQuery);
</script>
