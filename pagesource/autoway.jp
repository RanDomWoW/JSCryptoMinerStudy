<script type="text/javascript" src="https://lpcdn.autoway.jp/autoway/js/jquery.cookie.js"></script>
<script src="https://lpcdn.autoway.jp/autoway/js/awcommon/webstorage.js"></script>
<script type="text/javascript">
// ×ボタンクリック時、localStorageをクリアして、選択中の車情報要素の非表示
$(document).on('click','#delete_car_info',function(e) {

	e.stopPropagation();
	AUTOWAY.WebStorage.delLS('select_maker_id');
	AUTOWAY.WebStorage.delLS('select_maker_name');
	AUTOWAY.WebStorage.delLS('select_shashu_id');
	AUTOWAY.WebStorage.delLS('select_shashu_name');
	AUTOWAY.WebStorage.delLS('select_model_name');

	if ($('#mycarHtml').size()) {
		$('#car_info').data('query', '');
		$("#info_text").text('選択中の車情報はありません');
	} else {
		$("#car_info").hide('slow');
	}
});

$(document).on('click', '#car_info, #mycar_list li', function() {
	let url = location.href.split('/')[0] + '//' + location.href.split('/')[2] + '/';
	let original_url = url;
	var query = $(this).data('query');
	if (query == '') {
		return false;
	}

	if (-1 < location.href.indexOf('/pc_summertireset')) {
		// サマータイヤホイールセット
		url = url + 'shashu/summertireset/';
	} else if (-1 < location.href.indexOf('/pc_summertire')) {
		// サマータイヤ単品
		url = url + 'shashu/summertire/';
	}

	if (-1 < location.href.indexOf('/pc_studlesstireset')) {
		// スタッドレスタイヤホイールセット
		url = url + 'shashu/studlesstireset/';
	} else if (-1 < location.href.indexOf('/pc_studlesstire')) {
		// スタッドレスタイヤ単品
		url = url + 'shashu/studlesstire/';
	}

	if (-1 < location.href.indexOf('/pc_wheel')) {
		// ホイール単品
		url = url + 'shashu/wheel/';
	}

	if (original_url == url) {
		// トップページで矢印押下された場合は、サマータイヤ検索
		url = url + 'shashu/summertire/';
	}
	url += '?' + query;

	location.href = url;
});

$(function(){
	$('.bottom_right_close').click(function(){
		$.removeCookie('popup_disp');
		$.cookie('popup_disp', false, { expires: 7, path: '/' });
		$('.bottom_right_popup').animate({height: 'toggle'}, 'slow', 'swing');
	});

	$(document).on('click', '#show-mycar', function(e) {
		e.stopPropagation();

		$(this).toggleClass('active');
		$('#mycar_list').slideToggle(200);
		if ($(this).hasClass('active')) {
			$(this).text('マイカー情報非表示');
			var css = {
				'border-radius': '5px 5px 0 0',
				'margin-bottom': '0'
			};
		} else {
			$(this).text('マイカー情報表示');
			var css = {
				'border-radius': '5px',
				'margin-bottom': '5px'
			};
		}
		$('#car_info').css(css);
	});

	if ((AUTOWAY.WebStorage.getLS('select_maker_name') != null && AUTOWAY.WebStorage.getLS('select_shashu_name') != null && AUTOWAY.WebStorage.getLS('select_model_name') != null) || $('#mycarHtml').size()) {
		if($('#shashu_find_content').get(0) != undefined) {
			// 車種選択情報表示
			var info_text = '', mycarTag = '', mycar_list = '', query = '';
			if (AUTOWAY.WebStorage.getLS('select_maker_name') != null && AUTOWAY.WebStorage.getLS('select_shashu_name') != null && AUTOWAY.WebStorage.getLS('select_model_name') != null) {
				info_text = AUTOWAY.WebStorage.getLS('select_maker_name')
						  + '&nbsp;&nbsp;&nbsp;' + AUTOWAY.WebStorage.getLS('select_shashu_name')
						  + '&nbsp;&nbsp;&nbsp;' + AUTOWAY.WebStorage.getLS('select_model_name');
				query = 'CarMakerID=' + AUTOWAY.WebStorage.getLS('select_maker_id') + '&CarID=' + AUTOWAY.WebStorage.getLS('select_shashu_id') + '&CarModel=' + AUTOWAY.WebStorage.getLS('select_model_name');
			} else {
				info_text = '選択中の車情報はありません';
			}
			if ($('#mycarHtml').size()) {
				mycarTag = '<span id="show-mycar">マイカー情報表示</span>';
				mycar_list = $('#mycarHtml').html();
			}
			var car_info_tag = '<div id="car_info" data-query="' + query + '">'
							 + '<span id="delete_car_info">×</span>'
							 + '<span id="info_start">選択中の車情報：</span>'
							 + '<span id="info_text">' + info_text + '</span>'
							 + '<p>'
							 + '<span class="info_end">で商品を探す</span>'
							 + mycarTag
							 + '</p>'
							 + '</div>';

			$("#shashu_find_content").prepend(car_info_tag + mycar_list);
		}
	}
});
</script>


