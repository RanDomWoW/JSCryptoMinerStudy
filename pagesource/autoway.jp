<script type="text/javascript">
var clickFlg = true; // duplicateClickCheck

//キーワード読込
AUTOWAY.keywordManager.setInitKeyword([]);

//KeywordHistory
(function() {
	//search path only
	if (location.pathname === '/search/') {
		AUTOWAY.keywordManager.saveParamToKeyword(location.search);
	}

	var keywordArr = AUTOWAY.keywordManager.loadKeyword(AUTOWAY.keywordManager.getInitKeyword(), 3, 1);
	var retVal = "";
	var idx = 0;
	//make null keyword
	var count = Object.keys(keywordArr).length;
	if (count < 3) {
		count++;
		var newKeywordArr = keywordArr;
		var keywordArr = {};
		for (count; count <= 2; count++) {
			var searchName = "search" + count;
			keywordArr[searchName] = {
				keyword: ""
			};
		}
		$.extend(keywordArr, newKeywordArr);
	}
	var currentKeyword = '';
	for (var key in keywordArr) {
		idx += 1;
		retVal += "<li>";
		if (keywordArr[key]["keyword"]) {
			currentKeyword =  AUTOWAY.keywordManager.escapeHtml(keywordArr[key]["keyword"]);
			retVal += "<a href='/search/keyword/?Keyword=" + currentKeyword + "' onclick='AUTOWAY.keywordManager.saveKeyword(\"" + currentKeyword + "\",\"\")'>" + currentKeyword + "</a>"
		}
		retVal += "</li>";
	}
	$("#keyword_history ul").append(retVal);

	//Category Select
	switch (location.pathname) {
		case '/':
			$('#search_categories').val('');
			break;
		case '/pc_summertire':
			$('#search_categories').val('CategoryTire');
			break;
		case '/pc_summertireset':
			$('#search_categories').val('CategoryTireWheelSet');
			break;
		case '/pc_studlesstire':
			$('#search_categories').val('CategoryStudlessTire');
			break;
		case '/pc_studlesstireset':
			$('#search_categories').val('CategoryStudlessTireWheelSet');
			break;
		case '/pc_wheel':
			$('#search_categories').val('CategoryWheel');
			break;
	}
}());

(function($) {
	$(function() {
		//Autocomplete
		$('#search_keywords').autocomplete({
			source: AUTOWAY.keywordManager.getAutocompleteData(),
			autoFocus: false,
			delay: 500,
			minLength: 2,
			select: function(event, ui) {
				if (event.keyCode === 13) { //EnterKey
					$("#keyword_search_form").find("#search_keywords").val(ui.item.value);
					$("#keyword_search_form").submit();
				}
			},
			open: function(event, ui) {
				if (AUTOWAY.UserAgent.isModel.Ipad || AUTOWAY.UserAgent.isModel.Iphone) {
					$('.ui-autocomplete').off('menufocus hover mouseover mouseenter'); //IOS hover problem 
				}
			}
		});

		//SizeSearch
		$(".sizesearch_save").on('click', function(event) {
			AUTOWAY.keywordManager.saveParamToKeyword($(this).attr('href'));
			return true;
		});

		//KeywordHistoryListBoxs
		$("#keyword_history_modal").colorbox({
			iframe: false,
			width: '100%',
			height: '100%',
			maxWidth: '800px',
			maxHeight: '600px',
			opacity: 0.5,
			closeButton: false,
			scrolling: false,
			className: 'search_keyword',
			onComplete: function() {
				$("#keyword_list").on('click', 'li a', function() {
					$.colorbox.close();
				});
			}
		});

		$("#frequent_search_condition_modal").colorbox({
			iframe: false,
			width: '100%',
			height: '100%',
			maxWidth: '980px',
			opacity: 0.5,
			closeButton: false,
			scrolling: true,
			fixed: true,
			className: 'frequent_search_condition',
			onOpen: function() {
				$('html').css('overflow','hidden');
				$("#cboxOverlay").css('background-color','#555');
			},
			onComplete: function() {
				$(".frequent_search_condition #pc_frequent_daialog_close_button").insertBefore("#cboxLoadedContent");
				$("#condition_list").on('click', 'li a', function() {
					$.colorbox.close();
				});
			},
			onCleanup: function(){
				$('html').css('overflow','auto');
				$("#cboxOverlay").css('background-color','#fff');
			}
		});

		// 1回のタップでリンクが開けない問題への対応(iOS11は効いていない)
		var $w = $(window), $target = $('#frequent_search_condition_modal');
		$target.on('touchstart', function(){
			var $this = $(this), isScrolling = false;
			$w.on('scroll', function(){
				isScrolling = true;
			});
			$this.on('touchend', function(){
				if(!isScrolling){
					var url = $this.attr('href');
					if(url){
						$(".favorite_pop").hide();
					}
				}
				isScrolling = false;
				$this.off('touchend');
			});
		});

		$("#search_keywords").example(function() {
			return $(this).attr("title");
		});
		var $keyword_form = $("#keyword_search_form");
		var $categories = $keyword_form.find("#search_categories");
		var $keywords = $keyword_form.find("#search_keywords");
		var $availavility = $keyword_form.find("#search_availavility");
		var $dummy = $keyword_form.find("#dummy_keyword");

		// on submit
		$keyword_form.submit(function() {
			// category -&gt; dummy
			if ($categories.val() != "") {
				$dummy.attr("name", $categories.val());
				$dummy.removeAttr("disabled");
			} else {
				$dummy.attr("disabled", "disabled");
			}
			// set availavility
			if ($categories.val() == "CategoryTireWheelSet" || $categories.val() == "CategoryStudlessTireWheelSet") {
				$availavility.val("1");
			} else {
				$availavility.val("");
			}
			// keywords
			if ($keywords.val() == $keywords.attr("title") || $keywords.val() == "") {
				alert("タイヤサイズ（例：195/65R15）などを指定してください");
				$keywords.focus();
				return false;
			}
			// duplicateClickCheck
			if (clickFlg) {
				clickFlg = false;
				setTimeout(function() {
					clickFlg = true;
				}, 3000);
			} else {
				return false;
			}

			//キーワードの保存
			AUTOWAY.keywordManager.saveKeyword($keywords.val(), "");

			return true;
		});
	});
})(jQuery);
</script>
