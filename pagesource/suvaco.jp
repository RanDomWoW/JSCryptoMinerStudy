<script type="text/javascript" src="//cdn1.suvaco.jp/common/libs/jquery.flexslider-min.js"></script>
<script src="//cdn1.suvaco.jp/common/libs/slick/slick.min.js"></script>
<script type="text/javascript">
    $(function () {
        // スライダー画像
        $(".flexslider").flexslider({
            slideshow: true,
            slideshowSpeed: 8000,
            directionNav: true,
            pauseOnHover: true
        });
        // スライダー画像にマウスオーバー時の挙動
        $(".js-slide_overlay").on({
            'mouseenter' : function(){
                $(this).find(".slide_overlay").css("display", "block");
            },
            'mouseleave' : function(){
                $(this).find(".slide_overlay").css("display", "none");
            }
        });

        // 専門家を探すエリアのタブ制御
        $(".tab_expert_work").hover(
                function () {
                    var expert_work_id = $(this).data("expert_work_id");
                    for (i=1; i<=3; i++) {
                        $("#tab_expert_work"+i).css("display", expert_work_id==i ? "block" : "none");
                    }
                    $(".tab_expert_work").removeClass("active");
                    $("#li_expert_work"+expert_work_id).addClass("active");
                }
        );

        $(".js-top-cover-work-btn").on('click', function(){
            var target_work = $(this).data("work");
            var change_state = $(this).attr("data-on")=="0" ? 1 : 0;
            var next_work = change_state==1 ? target_work : "";
            $("input[name='work']").val(next_work);

            $(".js-top-cover-work-btn").each(function(){
                if ($(this).data("work")==next_work) {
                    $(this).addClass("top-cover-work-btn--selected");
                    $(this).attr("data-on", 1);
                } else {
                    $(this).removeClass("top-cover-work-btn--selected");
                    $(this).attr("data-on", 0);
                }
            });
        });

        $("#coverExpertSearchSubmit").on('click', function(){
            var param_work = $("input[name='work']").val();
            var param_prefecture = $("select[name='prefecture']").val();

            var form_action_url = "/experts";
            if (param_work!="" && param_work!="none") { form_action_url += "/" + param_work ; }
            if (param_prefecture!="") { form_action_url += "/p--" + param_prefecture; }
            location.href=form_action_url;
        });

        // カルーセル画像
        $('.js-carousel_doc_series').slick({
            autoplay: true,
            autoplaySpeed: 2500,
            slidesToShow: 3,
            slidesToScroll: 3,
            arrows: false,
            dots: true
        });
    });
</script>

