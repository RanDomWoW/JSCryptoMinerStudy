<script type="text/javascript">
    $(function(){

        // フォローでフィルター
        $(".js-search-follow-filter").on("click", function(){

            ga('send', 'event', 'search_ui', 'click', 'follow_filter');

            if ($(this).hasClass("noLogin")) {
                $("#promotion").modal("show");
                return;
            }

            if ($(this).hasClass("js-search-follow-filter-off")) {
                setSearchFollowFilter(1);
            } else if ($(this).hasClass("js-search-follow-filter-on")) {
                setSearchFollowFilter(0);
            }
            $(".js-search-follow-filter").toggle();

            if ($(this).data("no_reload") == 1) {
                return;
            }
            location.reload();
        });

        function getSuvacoUserSetting(key)
        {
            var data = $.cookie('suvaco_user_setting');
            data = data !== undefined ? JSON.parse(data) : {};
            if (key === undefined) {
                return data;
            } else {
                if (key in data) {
                    return data[key];
                } else {
                    return null;
                }
            }
        }

        function setSuvacoUserSetting(key, value)
        {
            var data = getSuvacoUserSetting();
            data[key] = value;
            $.cookie("suvaco_user_setting", JSON.stringify(data), {expires: 7, path: "/"});
        }

        // set search follow filter value
        function setSearchFollowFilter(value)
        {
            var key = "search_follow_filter";
            setSuvacoUserSetting(key, value);
        }
        
      $(".footer-media__btn-close").on("click", function() {
        $(".footer-media").slideToggle(400);
      });
    });
</script>



