		mode: 'fade',
		pager: false
	});

	//
	var ua = navigator.userAgent;
	if((ua.indexOf('iPhone') > 0) || ua.indexOf('iPod') > 0 || (ua.indexOf('Android') > 0 && ua.indexOf('Mobile') > 0)){
			$('head').prepend('<meta name="viewport" content="width=device-width,initial-scale=1">');
	} else {
			$('head').prepend('<meta name="viewport" content="width=1200">');
	}

	$(".drawer-dropdown-menu").click(function(e) {
		e.stopPropagation();
	});
	$(".go_m").hover(function(){
		$(".m_area").slideToggle();
	});
	$(".go_s").hover(function(){
		$(".s_area").slideToggle();
	});

	// 画像入れ替え
	var ctime = 1500;
	$("#climg02").hide();
	$("#climg04").hide();
	setInterval(function() {
		if ($('#climg01').is(':visible')) {
			$("#climg01").fadeToggle(ctime, function(){	$("#climg02").fadeToggle(ctime);	});
		} else {
			$("#climg02").fadeToggle(ctime, function(){	$("#climg01").fadeToggle(ctime);	});
		}
		//
		if ($('#climg03').is(':visible')) {
			$("#climg03").fadeToggle(ctime, function(){	$("#climg04").fadeToggle(ctime);	});
		} else {
			$("#climg04").fadeToggle(ctime, function(){	$("#climg03").fadeToggle(ctime);	});
		}
	}, 5000);


	// #で始まるアンカーをクリックした場合に処理
	var headerHight = 75; //ヘッダの高さ
	$('#goToTopLink').click(function(){
//		$("html,body").animate({scrollTop:0},"300");
		var speed = 400; // ミリ秒
		var href= $(this).attr("href");
		var target = $(href == "#" || href == "" ? 'html' : href);
		var position = target.offset().top-headerHight; //ヘッダの高さ分位置をずらす
		$('body,html').animate({scrollTop:position}, speed, 'swing');
		return false;
	});

	$('#feed ul').each(function(){
		var rep = 0;
		$(this).children().each(function(){
			var itemHeight = parseInt($(this).css('height'));
			if(itemHeight > rep){
				rep = itemHeight;
			};
		});
		$(this).children().css({height:(rep)});
	});
});
</script>
