<script>
var GOOGLE_API_KEY = 'AIzaSyDoVNPXY2gkrlmjD5sl-vUQMcUs16m1hjE';

// Upon loading, the Google APIs JS client automatically invokes this callback.
googleApiClientReady = function() {
  gapi.auth.init(function() {
    gapi.client.setApiKey(GOOGLE_API_KEY);
    gapi.client.load('youtube', 'v3', function() {
      handleAPILoaded();
    });
  });
}

// Define some variables used to remember state.
var playlistId;

// After the API loads, call a function to get the uploads playlist ID.
function handleAPILoaded() {
  requestUserUploadsPlaylistId();
}

// Call the Data API to retrieve the playlist ID that uniquely identifies the
// list of videos uploaded to the user's channel.
function requestUserUploadsPlaylistId() {
  // See https://developers.google.com/youtube/v3/docs/channels/list
  var request = gapi.client.youtube.channels.list({
    forUsername: 'PricePlow',
    part: 'contentDetails'
  });
  request.execute(function(response) {
    playlistId = response.result.items[0].contentDetails.relatedPlaylists.uploads;
    requestVideoPlaylist(playlistId);
  });
}

// Retrieve the list of videos in the specified playlist.
function requestVideoPlaylist(playlistId) {
  var requestOptions = {
    playlistId: playlistId,
    part: 'snippet',
    maxResults: 3
  };

  var request = gapi.client.youtube.playlistItems.list(requestOptions);
  request.execute(function(response) {
    var playlistItems = response.result.items;


    if (playlistItems) {
      var videoIds = [];
      $.each(playlistItems, function(index, item) {
          videoIds.push(item.snippet.resourceId.videoId);
      });
      requestVideoStatistics(videoIds);
    } else {
      $('.widget-recent-videos').hide();
    }
  });
}

// Retrieve video statistics for each video in playlistItems.
function requestVideoStatistics(videoIds) {
  var requestOptions = {
    id: ','.concat(videoIds),
    part: 'snippet,statistics',
  };

  var request = gapi.client.youtube.videos.list(requestOptions);
  request.execute(function(response) {
    var videos = response.result.items;

    if (videos) {
      $.each(videos, function(index, item) {
        appendVideoResult(item);
      });
    } else {
      $('.widget-recent-videos').hide();
    }
  });
}

// String.format: http://stackoverflow.com/questions/610406/javascript-equivalent-to-printf-string-format
// First, checks if it isn't implemented yet.
if (!String.prototype.format) {
  String.prototype.format = function() {

    var args = arguments;
    var sprintfRegex = /\{(\d+)\}/g;

    var sprintf = function (match, number) {
      return number in args ? args[number] : match;
    };

    return this.replace(sprintfRegex, sprintf);
  };
}

function appendVideoResult(video) {
  // video is a Video resource with snippet and statistics populated:
  // https://developers.google.com/youtube/v3/docs/videos#resource

  // {0}: video url, {1}: thumbnail url, {2}: title, {3}: view count
  var videoTemplate = '<li><a href="{0}" class="clearfix" target="_blank"><span class="video-picture"><img src="{1}" alt="{2}"/></span><span class="video-picture-text"><span class="vpt-in"><strong>{2}</strong><br/>{3} views</span></span></a></li>';

  var title = video.snippet.title;
  var videoId = video.id;
  var videoUrl = 'http://www.youtube.com/watch?v=' + videoId;
  var thumbnailUrl = video.snippet.thumbnails.default.url;
  var viewCount = video.statistics.viewCount;
  var videoHtml = videoTemplate.format(videoUrl, thumbnailUrl, title, viewCount);

  $('.widget-recent-videos div ul').append(videoHtml);
}
</script>
