.ls-l.ls-video-layer{
box-shadow: 0px 0px 100px #888888;
}

/* Formulario de contacto */

#FSContact3 input, #FSContact3 textarea{
color: #3b5873;
border: 1px solid #3b5873;
padding:4px;
}

#FSContact3 input[type='submit']{
width: auto !important;
height: auto !important;
background-color: #0d4b6d !important;
border: 1px solid #bbcad2 !important;
border-radius: 6px !important;
color: #ffffff !important;
padding: 15px 20px 15px 20px !important;
}

/* Calendario de eventos */

#ftcalendar-div {
background: none;
border: none;
}

.thumb #ftcalendar-nav {
padding: 10px 0px 10px 0px;
border-top: 1px solid #0d4b6d;
border-bottom: 1px solid #0d4b6d;
}

.thumb #ftcalendar-nav #ftcalendar-current {
font-size: 16px; 
font-weight:bold;
text-align: center;
width: 85%;
position: absolute;
top: 15px;
}

#ftcalendar-nav #ftcalendar-prev {
float:left;
}

#ftcalendar-nav #ftcalendar-next {
float:right;
}

#ftcalendar-table tr th#ftcalendar-headings {
display: none;
}

.thumb #ftcalendar-table {
margin-top:5px;
background:none;
}

.thumb-event-div {
background: none;
}

.month #ftcalendar-table tr th, .month #ftcalendar-table tr td, .week #ftcalendar-table tr th, .week #ftcalendar-table tr td, .thumb #ftcalendar-table tr th, .thumb #ftcalendar-table tr td {
border: none;
font-weight: bold;
color:#002748;
}


.ftcalendar-event-date {
background:none;
}

.thumb .ftcalendar-event-date span.thumb-event {
background-color: #adc0cc;
padding: 4px;
}

#ftcalendar-table tr td.current_day  span.thumb-event{
background-color: #265d7b !important;
}

.thumb .ftcalendar-event-date.current_day span.thumb-event{
color:#ffffff;
}

.thumb .ftcalendar-event-date span.thumb-event.unmonth{
background: none;
color:#adc0cc;
}

.thumb #ftcalendar-table tr td.ftcalendar-event-date.unmonth {
background: none;
color:#adc0cc;
}

.thumb #ftcalendar-table tr td {
padding: 4px !important;
}

#ftcalendar-table tr td.current_day {
background:none;
}

.thumb-event-header {
padding: 5px;
background-color:#e6ecf0;
}

.thumb-event-header .thumb-event-close {
right: 6px;
top: 2px;
}

.thumb-event-div {
width: 150px;
height: 150px;
}

.thumb-event-div .thumb-events {
height: 127px;
}

div.ftcalendar-event {
background: none !important;
border: none;
margin-top: 5px;
}

.ftcalendar-event div{
background: none !important;
}

/*Cajas Centros de Pago*/
.fusion-flip-box .flip-box-front, .fusion-flip-box .flip-box-back{
padding: 10px 10px!important; 
min-height:200px!important;

}

.box-empresas{
  padding: 20px 20px!important;
  min-height:200px!important;


}



/*Fin Caja Centros de Pago*/



