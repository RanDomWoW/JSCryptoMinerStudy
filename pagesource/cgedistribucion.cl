.gnf-button{
width:auto !important;
height:auto !important;
font-weight:normal !important; 
/*background-color:#0d4b6d !important; */
text-align:center !important;
border:1px solid #bbcad2 !important; 
border-radius:6px !important;
color:#ffffff !important;
padding:15px 20px 15px 20px !important;
text-transform:none !important;
			/*background:#004165 !important;*/
			background: -webkit-linear-gradient(#105782, #082f47) !important;
   			background: -moz-linear-gradient(#105782, #082f47) !important;
   			background: -o-linear-gradient(#105782, #082f47) !important;

			
			-webkit-text-shadow: 0 0px 0 #fff;
			-moz-text-shadow: 0 0px 0 #fff;
			-ms-text-shadow: 0 0px 0 #fff;
			-o-text-shadow: 0 0px 0 #fff;
			text-shadow: 0 0px 0 #fff;
}
.gnf-button:hover
{
background: -webkit-linear-gradient(#082f47, #105782) !important;
   			background: -moz-linear-gradient(#082f47, #105782 ) !important;
   			background: -o-linear-gradient(#082f47, #105782) !important;
}
