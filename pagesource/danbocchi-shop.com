          $('#prd-opt-select').append($('.prd-opt-select').html());
        }
      }
    }
    window_size_switch_func();
    $(window).load(function () {
      $('.history-unit').tile();
    });
    $(window).resize(function () {
      window_size_switch_func();
      $('.history-unit').tile();
    });
  });
