    <script type="text/javascript">
        $(document).ready(function() {
            var div = $('#header-main');
            var start = $(div).offset().top + 100;
            $.event.add(window, 'scroll', function() {		  
                var p = $(window).scrollTop();
                if (p > start) {
                    $('body').addClass('fix-header');
                } else {
                    $('body').removeClass('fix-header');
                }
            });
        });
        $(document).ready(function () {
            $('.sec-left').hover(function () {
                $(this).find('.dropdown-menu').first().stop(true, true).slideDown(300);
            }, function () {
                $(this).find('.dropdown-menu').first().stop(true, true).slideUp(300)
            });
            $(".mobile_menu").slideMobileMenu({
                onMenuLoad: function (menu) {
                    console.log(menu)
                    console.log("menu loaded")
                },
                onMenuToggle: function (menu, opened) {
                    console.log(opened)
                }
            });
            $('.mega-dropdown').hover(function () {
                $(".leftsection").find(".catMenu").removeClass('cleftsection');
                $(".leftsection").find("." + selectedCatmenu).addClass('cleftsection');
            });
            $('.mega-dropdown-menu').hover(function () {
                $(".orlap").toggleClass('opensubmenu');
            });
            var selectedCatmenu;
            $(".leftsection").find(".active").addClass('cleftsection');
            var getHtmlData = "";
            $('.burgertop').hover(function () {
                if (getHtmlData == "") {
                    getHtmlData = $('.menuclone').html();
                    $('.upperMenuClone').html(getHtmlData);
                }
                $('.menuclone').html('');
            });
            $('.desktopmenu').hover(function () {
                if (getHtmlData != "") {
                    getHtmlData = $('.upperMenuClone').html();
                    $('.menuclone').html(getHtmlData);
                    getHtmlData = "";
                }
                $('.upperMenuClone').html('');
            });
            $('#dropdownMenuButton').click(function () {
                if ($('#loginDropdownMenu').css('display') == 'none')
                    $('#loginDropdownMenu').attr('style', 'display: block');
                else $('#loginDropdownMenu').attr('style', 'display: none')
            });
            $('.showsearch').click(function () {
                if ($('.search_show').css('display') == 'none') {
                    $('.search_show').removeClass('hidden-xs');
                    $('.search_show').removeClass('hidden-sm');
                    $('.search_show').attr('style', 'display: block');
                }
                else {
                    $('.search_show').addClass('hidden-xs');
                    $('.search_show').addClass('hidden-sm');
                    $('.search_show').attr('style', 'display: none')
                }
            });
        });
        var selectedCatmenu;
        $(document).on("mouseover", ".leftsection", function (e) {
            $(".leftsection").find(".catMenu").removeClass('cleftsection');
            $(".leftsection").find("." + selectedCatmenu).addClass('cleftsection');
        });
        $(document).on("mouseover", ".catMenu", function (e) {
            selectedCatmenu = $(this).attr('cat');
            $(".tab-content").find(".active").addClass('fade').removeClass('active');
            $(".tab-content").find("." + selectedCatmenu).addClass('active').removeClass('fade');
            $(".leftsection").find(".catMenu").removeClass('cleftsection');
            $(".leftsection").find("." + selectedCatmenu).addClass('cleftsection');
            var imgpro = $("." + selectedCatmenu).find('img').attr('src');
            $('.changepromo').css('background-image', 'url("' + imgpro + '")');
            var purl = $("." + selectedCatmenu).find('img').attr('promolink');
            $(".changepromo").attr("promourl", "" + purl + "");
        });
        $(document).on("mouseenter", ".snapshotRemove", function (e) {
            $('.snapshotRemove').attr('style', 'cursor: pointer');
        });
        $(document).on("click", ".changepromo", function (e) {
            var link = $(this).attr('promourl');
            if (link != "undefined"){window.open($(this).attr('promourl'), '_self');}
        });
        $(document).on("mouseleave", ".divcart", function (e) {
            $(".snapshot").slideUp("slow");
        });
    </script>

