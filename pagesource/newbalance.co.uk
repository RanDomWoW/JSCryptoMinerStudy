<script type="text/javascript">
/*global $*/
(function() {
  "use strict";

  function track(goal) {
    var s = window.s || {};
    s.tl = s.tl || $.noop;
    s.linkTrackVars = 'prop48';
    s.linkTrackEvents = 'None';
    s.prop48 = goal;
    s.tl(true, 'o', 'Optimizely Goal'); // static params / do not update
  }
var countrycode = $("header").attr("data-country-id").toUpperCase();
  function setupEmailLightbox() {
    $('#container').prepend('<div id="cdev-email-lightbox"><span class="icon-close"></span><iframe src="https://shop.newbalance.eu/pub/sf/ResponseForm?_ri_=X0Gzc2X%3DYQpglLjHJlYQGngTapAndgg1hhzaraiazazbRzgs9UWmyzdfGVXMtX%3DYQpglLjHJlYQGp9MXMr6U4zdKzfAFv30hbqn3GzfzaAG9rRi&_ei_=EsIn1HiNO4pEv9fEuRGyzlQ&ccode=' + countrycode + '" allowtransparency="true" frameborder="0"></iframe><button></button></div>');
    
    // delay adding the show class
    setTimeout(function() {
      $('#cdev-email-lightbox').addClass('cdev-show-lightbox');
    }, 100);

    // click actions for closing
    $("#cdev-email-lightbox .icon-close").click(function() {
      $(document).trigger('track-analytics', {
        'content_event': 'MyNB EM Subscribe: Lightbox: X: Click'
      });
      track('modal close x');
      $('#cdev-email-lightbox').remove();
    });
    $("#cdev-email-lightbox button").click(function() {
      $(document).trigger('track-analytics', {
        'content_event': 'MyNB EM Subscribe: Lightbox: No Thanks: Click'
      });
      track('modal close no thanks');
      $('#cdev-email-lightbox').remove();
    });
  }

  // gets the cookie
  function getCookie(cname) {
    var name = cname + "=",
      ca = document.cookie.split(';'),
      i,
      c;
    for (i = 0; i < ca.length; i++) {
      c = ca[i].trim();
      if (c.indexOf(name) === 0) {
        return c.substring(name.length, c.length);
      }
    }
    return "";
  }

  // sets a cookie with an expiration date in days
  function setCookie(cname, cvalue, exdays) {
    var d = new Date(),
      expires;
    d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
    expires = "expires=" + d.toGMTString();
    document.cookie = cname + "=" + cvalue + "; path=/; " + expires;
  }

  // if the cookie is not this will set the cookie
  function checkCookie() {
    if (navigator.cookieEnabled) {
      var hideEmailForm = getCookie("hideEmailLightbox");
      if (hideEmailForm !== "hide") {
        setTimeout(function() {
          setupEmailLightbox();
          setCookie("hideEmailLightbox", "hide", 365);
        }, 1000);
      }
    }
  }

  $(function() {
    checkCookie();
  });

}());
</script>
