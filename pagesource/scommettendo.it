 <script type="text/javascript">
    function clear_value(element,type){if (type==0&&element.value=="username")element.value=''; else if (type==1&&element.value=="password")element.value=''; }
    function toggle(mydiv){ var mydiv = document.getElementById(mydiv); if (mydiv.style.visibility == 'hidden') {global_bslip_div = "visible";mydiv.style.visibility = 'visible';} else {global_bslip_div = "hidden";mydiv.style.visibility = 'hidden';}}
    function toggle_balance(action){if(action == "show"){document.getElementById("bal").style.display = "inline";document.getElementById("balance").innerHTML = document.getElementById("curr_curr").value+" "+document.getElementById("curr_bal").value; document.getElementById("label").style.display = "none";}else{document.getElementById("bal").style.display = "none";document.getElementById("label").style.display = "inline";}}   
    function popup(url, name){params = "scrollbars=no,width=240,height=320";window.open(url, name, params);} 
 </script>
 


