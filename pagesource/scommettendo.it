$.fn.cycle.transitions.fadeIn = function($cont, $slides, opts) {
	opts.fxFn = function(curr, next, opts, after) {
		$(curr).css("z-index",0);
		$(next).css("z-index",1).fadeIn(opts.fadeSpeed, function() {
			$(curr).hide();
			after();
		});
	};
}; 
