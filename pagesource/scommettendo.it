 <script type="text/javascript">
    // data used in session_reminder_timer.js
    var session_reminder_timer = 'no';
    var session_reminder_timer_interval = 0;
    var session_reminder_timer_overlay = 0;
    var popup_container_border_colour = '#F9C511';
    var popup_container_font_colour = '#ffffff';
    var popup_container_background_colour = '#000000';
    var overlay_bg_img = '/images/transparent_black.png';
    var session_reminder_popup_url = '/session_reminder_timer.php?ajax';
    
    if (session_reminder_timer != 'no') {
        var session_reminder_time = session_reminder_timer * 1000;
        var srm_rt = 0 * 1000;
    }
 </script>
