            function detectScript(str) {
                str = str.toLowerCase();
                if (str.indexOf("location.href") != -1)
                    return '1';
                else {
                    var lessThanPosition = str.indexOf("<");
                    var greaterThanPosition = str.indexOf(">");

                    if (lessThanPosition == -1 || greaterThanPosition == -1) {
                        if ((lessThanPosition == -1 && greaterThanPosition > -1) || (greaterThanPosition == -1 && lessThanPosition > -1))
                            return '1';
                        return '0';
                    }
                    else {
                        if (lessThanPosition < greaterThanPosition) {
                            return '1';
                        }
                        else {
                            var nxtCheckStr = str.substring(lessThanPosition + 1, str.length);
                            if (nxtCheckStr.indexOf(">") != -1)
                                return '1';
                            return '0';
                        }
                    }
                }
            }

