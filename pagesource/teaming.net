				$.loadScript(urlExperimentScript, false, {
				    async : false,
				    success : function() {
						var urlVariationScript = "https://djg5cfn4h6wcu.cloudfront.net/resources/2.39.2-gz/js/experiment/"
							+ experiment.replace(/\W/g, '') + ".js";
						$.loadScript(urlVariationScript, true, {
