    $(function() {
        window.onload = function() {
            var carousel = $("#top_restaurant_carousel").css("position", "relative");
            var imgs = $("img", carousel);
            var h = $(imgs[0]).height();
            carousel.height(h);
            for (var i = 0; i < imgs.length; i++) {
                var img = $(imgs[i]);
                img.css("position", "absolute").css({"top":"0px", "left":"0px"});
            }
            setInterval(function() {
                $("img:last-child", carousel).animate({"opacity": "0"}, "fast", "swing", function() { $(this).css("opacity", "1").remove().prependTo($("a", carousel)); });
            }, 1500);
       };
    });
