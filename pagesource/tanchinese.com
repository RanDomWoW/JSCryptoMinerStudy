    <script>
    $(function(){
      $('#photoCarousel').carousel('pause');
      // handles the carousel thumbnails
      $('[id^=carousel-photo-]').click( function(){
        var id_photo = $(this).attr("id");
        var id = id_photo.substr(id_photo.length -1);
        id = parseInt(id);
        $('#photoCarousel').carousel(id);
        $('[id^=carousel-photo-]').removeClass('selected');
        $(this).addClass('selected');
      });
      // when the carousel slides, auto update
      $('#photoCarousel').on('slid', function (e) {
        var id = $('.item.active').data('slide-number');
        console.log(id);
        id = parseInt(id);
        $('[id^=carousel-photo-]').removeClass('selected');
        $('[id=carousel-photo-'+id+']').addClass('selected');
      });
    });
    </script>
