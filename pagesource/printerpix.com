		<script>
			var dateString = '2015/11/28 08:00:00';

			var convertToTimerString = function (date) {
				return date.getFullYear() + '/' + (date.getMonth() < 10 ? "0" + (date.getMonth() + 1).toString() : (date.getMonth() + 1).toString()) + '/' + (date.getDay() < 10 ? "0" + date.getDay().toString() : date.getDay().toString()) + ' ' + date.toLocaleTimeString();
			};

			var repeatEach = parseInt('0');

			var getResetTime = function (dateString, repeatEach) {
				var date = new Date(dateString);
				var diff = Date.now() % date.getTime();

				if ((Date.now() < date.getTime()) || repeatEach == 0)
					return dateString;

				repeatEach = repeatEach * 24 * 60 * 60 * 1000;

				if (diff > repeatEach)
					diff = diff % repeatEach;

				date = new Date();

				date.setTime(date.getTime() + repeatEach - diff - 1000);

				return convertToTimerString(date);
			};

			dateString = getResetTime(dateString, repeatEach).toString();

			var stopTime = function (dateString, countdown) {
				var date = new Date(dateString);

				if (date.getTime() < Date.now()) {
					var t = new Date();
					var now = t.getFullYear() + '/' + (t.getMonth() + 1) + '/' + t.getDate() + ' ' + t.toLocaleTimeString();

					countdown.set_options([
						'target_date', Date.now(),
						'target_future', false
					]);
					countdown.pause_anim();
				}
			}

			var setBlackFridayHomePageCountdown = function () {
				var isEvent = 'true';
				var isBlackFriday = 'true';
				var isExtended = 'false';
				var isMobile = 'false';
				var isChristmas = 'true';
				var showWebContentClock = 'true';
				var positionClock = 'true';

/*
				//console.log('isEvent: ', isEvent);
				//console.log('isBlackFriday: ', isBlackFriday);
				//console.log('isExtended: ', isExtended);
				//console.log('isMobile', isMobile);
				//console.log('isChristmas', isChristmas);
				//console.log('showWebContentClock', showWebContentClock);
				//console.log('positionClock', positionClock);
*/

				if (isEvent != 'true')
					return false;

				if (isEvent == 'true') {
					setTimeout(function () {
						//$('#slider-source').cycle('pause');
					}, 500);
				}



				var homePageCountDownExists = $('#home_page_countdown').length > 0 ? true : false;
				var baloonCountDownExists = $('#first_countdown').length > 0 ? true : false;
				var blackFridayPagesCountDownExists = $('#first_countdown2').length > 0 ? true : false;

				var counterOptions = [
						"day_digits", 1,
						"groups_spacing", 5,
						"show_dd", false,
						"spacer", "circle",
						"target_date", dateString,
						"fillStyleSymbol2", "rgba(255, 255, 255, 1)",
						"fillStylesPanel_g1_1", "rgba(16, 16, 16, 1)",
						"fillStylesPanel_g1_2", "rgba(32, 32, 32, 1)",
						"fillStylesPanel_g2_1", "rgba(16, 16, 16, 1)",
						"fillStylesPanel_g2_2", "rgba(32, 32, 32, 1)",
						"text_color", "rgba(255, 255, 255,1)",
						"text_glow", "rgba(0, 0, 0,1)",
						"target_date", dateString,
						"show_labels", "true",
						"days_long", "days",
								"days_short", "dd",
								"hours_long", "hours",
								"hours_short", "hh",
								"minutes_long", "minutes",
								"minutes_short", "mm",
								"seconds_long", "seconds",
								"seconds_short", "ss"
				];

				var cyberMondayStyleOverwrite = [
					"fillStyleSymbol1", "rgba(17, 45, 77, 1)",
					"fillStyleSymbol2", "rgba(17, 45, 77, 1)",
					"fillStylesPanel_g1_1", "rgba(255, 192, 20, 1)",
					"fillStylesPanel_g1_2", "rgba(255, 221, 0, 1)",
					"fillStylesPanel_g2_1", "rgba(255, 192, 20, 1)",
					"fillStylesPanel_g2_2", "rgba(255, 221, 0, 1)",
					"text_color", "rgba(255, 208, 20, 1)",
					"text_glow", "rgba(0,0,0,1)"
				];

				var christmasStyleOverwrite = [
					"fillStyleSymbol1", "rgba(255, 255, 255, 1)",
						"fillStyleSymbol2", "rgba(255, 255, 255, 1)",
				"fillStylesPanel_g1_2", "rgba(100, 100, 100, 1)",
				"fillStylesPanel_g1_1", "rgba(30, 30, 30, 1)",
				"fillStylesPanel_g2_2", "rgba(100, 100, 100, 1)",
				"fillStylesPanel_g2_1", "rgba(30,30,30,1)",
				"text_color", "rgba(255, 255, 255,1)",
				"text_glow", "rgba(0, 0, 0,1)"
				];

				//if (isMobile) {
				//	cyberMondayStyleOverwrite.push("show_labels");
				//	cyberMondayStyleOverwrite.push(false);
				//}

				var setClockTheme = function (clockObject) {
					if (clockObject == null)
						return false;

					//if (isBlackFriday != 'true')//diffeent clock theme for cyber monday
					//	$(clockObject).set_options(cyberMondayStyleOverwrite);

					//if (isChristmas == 'true')//diffeent clock theme for christmas
					//	$(clockObject).set_options(christmasStyleOverwrite);
				};

				setTimeout(function () {
					if ($('#home_page_countdown').length > 0) {
						var homePageCountdown = $('#home_page_countdown').ResponsiveCountdown();
						homePageCountdown.set_options(counterOptions);

						setClockTheme(homePageCountdown);

						homePageCountdown.play_anim();

						stopTime(dateString, homePageCountdown);
					}

					if ($('#first_countdown').length > 0) {
						var baloonCountDown = $('#first_countdown').ResponsiveCountdown();
						baloonCountDown.set_options(counterOptions);

						setClockTheme(baloonCountDown);

						baloonCountDown.play_anim();

						stopTime(dateString, baloonCountDown);
					}

				}, 0);

				setTimeout(function () {

					////console.log('firstctd22: ', $('#first_countdown2').length, 'showWebContentClock: ', showWebContentClock, 'positionClock: ', positionClock);

					if ($('#first_countdown2').length > 0 && (showWebContentClock == 'true' || positionClock == 'true')) {
						var blackFridayPagesCountDown = $('#first_countdown2').ResponsiveCountdown();
						blackFridayPagesCountDown.set_options(counterOptions);

						setClockTheme(blackFridayPagesCountDown);

						blackFridayPagesCountDown.play_anim();

						stopTime(dateString, blackFridayPagesCountDown);
					}
				}, 0);
			};

			$(document).ready(function () {
				setBlackFridayHomePageCountdown();
			});
		</script>

