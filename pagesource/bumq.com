<script type="text/javascript">
document.getElementById("loginUSC").value=ad_UserScreen();
function ad_UserScreen(){uscw=screen.width;usch=screen.height;if(uscw<usch){usc=usch+"x"+uscw;}else{usc=uscw+"x"+usch;}return usc;}
function loginSubmitFForm(x){
	document.getElementById("result_of").innerHTML="";
	document.getElementById("mySmallModalLabel").innerHTML="";
	document.getElementById("mySmallModalLabel_fail").innerHTML="";
	btn_try.className="contenthidden";
	btn_gotoactivate="contenthidden";
	
	if(x == "LG"){
		if(document.getElementById("LG_EMAIL").value == "" || document.getElementById("LG_PASSWORD").value == ""){
			$('#bumqModal').modal('show');
			document.getElementById("result_of").innerHTML="กรุณากรอกอีเมล์ และ รหัสผ่าน";
			document.getElementById("mySmallModalLabel_fail").innerHTML="เข้าสู่ระบบไม่สำเร็จ";
			btn_try.className="contentshow";
			result_of.className="contentshow";
			}else{
			document.getElementById("loginEMAIL").value=document.getElementById("LG_EMAIL").value;
			document.getElementById("loginPASSWORD").value=document.getElementById("LG_PASSWORD").value;
			document.getElementById("loginFForm").action="/login_action.php";
			document.getElementById("loginFForm").submit();
			}
		}else if(x == "XS"){
		if(document.getElementById("XS_EMAIL").value == "" || document.getElementById("XS_PASSWORD").value == ""){
			$('#bumqModal').modal('show');
			document.getElementById("result_of").innerHTML="กรุณากรอกอีเมล์ และ รหัสผ่าน";
			document.getElementById("mySmallModalLabel_fail").innerHTML="เข้าสู่ระบบไม่สำเร็จ";
			btn_try.className="contentshow";
			result_of.className="contentshow";
			}else{
			document.getElementById("loginEMAIL").value=document.getElementById("XS_EMAIL").value;
			document.getElementById("loginPASSWORD").value=document.getElementById("XS_PASSWORD").value;
			document.getElementById("loginFForm").action="/login_action.php";
			document.getElementById("loginFForm").submit();
			}
		}
	}
//##############################################################
function loginFFormReturn(responseText){
	if(responseText == "OKACTIVATE"){
		$('#bumqModal').modal('show');
		document.getElementById("mySmallModalLabel_fail").innerHTML="เข้าสู่ระบบไม่สำเร็จ";
		document.getElementById("result_of").innerHTML="บัญชีของท่านยังไม่ได้รับการยืนยันอีเมล์<br>โปรดยืนยันอีเมล์ก่อนจึงจะเข้าสู่ระบบได้";
		btn_gotoactivate.className="contentshow";
		result_of.className="contentshow";
		}else if(responseText == "OK"){
		top.location.href="http://www.bumq.com/members/main.php";
		}else{
		$('#bumqModal').modal('show');
		document.getElementById("mySmallModalLabel_fail").innerHTML="เข้าสู่ระบบไม่สำเร็จ";
		document.getElementById("result_of").innerHTML=responseText;
		btn_try.className="contentshow";
		result_of.className="contentshow";
		}
	}
