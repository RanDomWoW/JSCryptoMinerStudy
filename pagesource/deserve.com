			// jQuery('.aprValue').html(aprValue);
			if ((/utm_medium=org_ref/.test(window.location.search)) || (/utm_medium=cust_ref/.test(window.location.search))) {
				//grab the current URL string
    			let URL = window.location.href;

    			//Separate the Query String Params into a list
    			let params = [];
    			if (URL.split("?").length > 1) {
      				params = URL.split("?")[1].split("&");
    			}

   				if (params.length) {
      				//Loop through the list of params
      				let queryDict = {};
      				for(let i = 0; i < params.length; i++) {

        				//For each param, grab the param name and value
        				let query = params[i].split("=");
        				if (query.length != 2) continue;

        				let queryKey  = query[0];
        				let queryVal = query[1];

        				//For each param, create a dictionary to be used in mixpanel.register
        				queryDict[queryKey] = queryVal;
      				}


      				// check for a ref code and override whatever might be saved
      				if (queryDict["utm_medium"]=="org_ref" &&
          				queryDict["utm_source"]=="invite" &&
          				queryDict["utm_campaign"]) {

        				let org_code = queryDict["utm_campaign"];

        				// replace the local cookie and clear ref cookie
						Cookies.remove('ref_code');
						Cookies.set('ref_code', { org: org_code }, { expires:90 });
      				} else if (queryDict["utm_medium"]=="cust_ref" &&
                                        queryDict["utm_source"]=="invite" &&
                                        queryDict["utm_campaign"]) {

                                        let cust_code = queryDict["utm_campaign"];

                                        // replace the local cookie and clear ref cookie
                                                Cookies.remove('ref_code');
                                                Cookies.set('ref_code', cust_code, { expires:90 });
                                }
    			}
			}
		});
