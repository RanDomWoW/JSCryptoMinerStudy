// Scala - Lists 
object Demo {
  val mylist: List[Int] = List(1,2,5,8,9,6,4);
  val names: List[String] = List("Max", "Tom", "John");
	def main(args: Array[String]) {
		println(0 :: mylist);  
		println(mylist);  
		println(names);  
		println(1 :: 5:: 9 :: Nil); 
		println(mylist.head);  
		println(names.tail);   
		println(mylist.tail);   
		println(names.isEmpty);   
		println(mylist.reverse);  
		println(List.fill(5)(2)); 		
		println(mylist.max);  
		
		mylist.foreach( println )
		var sum : Int = 0;
		mylist.foreach(sum += _);
		println(sum);
		
		for (name &lt;- names) {
		  println(name);
		}
		 
		println(names(0));
   
	}
}

