    <script>
        
        $(document).ready(function () {
        
            $("#vadiro-search").click(function () {
                var province = $("#dd-province").val();
                var city = $("#dd-city").val();
                if (province > 0 || city > 0) {
                    location.href = "http://vadiro.com/Search?pi=1&ps=12&sort=1&pr=" + province + "&pc=" + city;
                } else {
                    formSwal("خطای ورودی","لطفا مقادیر استان و شهر را انتخاب کنید", false, "warning");
                }
            });
            
            $('#dd-province').change(function () {
                var relatedCityId = $(this).data('rcityid');
                var options = '';
                if (relatedCityId.length > 0) {
                    $("#" + relatedCityId).val("");
                    $.ajax({
                        type: 'POST',
                        url: '/Common/GetCities',
                        data: { 'ProvinceId': parseInt($(this).val()) },
                        success: function (data) {

                            
                            $.each(data, function (i, city) {
                                options += '<option value="' + city.LocationId + '">' + city.Name + '</option>';
                            });
                            $("#" + relatedCityId).append(options);
                            

                        },
                        error: function (xhr, status, text) {

                        }
                    });
                }

            });
        });
    </script>    

