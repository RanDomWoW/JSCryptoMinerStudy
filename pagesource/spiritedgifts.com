<script type="text/javascript">
    $j(document).ready(function () {
        // =============================================
        // Skip Links
        // =============================================

        window.paypalContext = true;
        var url = window.location.pathname.replace(/\//g,'');
        var skipContents = $j('.skip-minicart-content');
        var skipLinks = $j('.skip-minicart-link');

        skipLinks.on('click', function (e) {
            e.preventDefault();

            var self = $j(this);
            // Use the data-target-element attribute, if it exists. Fall back to href.
            var target = self.attr('data-target-element') ? self.attr('data-target-element') : self.attr('href');

            // Get target element
            var elem = $j(target);

            // Get body
            var body = $j('body');

            // Check if stub is open
            var isSkipContentOpen = elem.hasClass('skip-active') ? 1 : 0;

            // Hide all stubs
            skipLinks.removeClass('skip-active');
            skipContents.removeClass('skip-active');

            // Toggle stubs
            if (isSkipContentOpen) {
                self.removeClass('skip-active');
                body.removeClass('overlay');
            } else {
                self.addClass('skip-active');
                elem.addClass('skip-active');
                body.addClass('overlay');
            }

            if(paypalContext && url != 'onestepcheckout' && $j('#cart-sidebar').length > 0) {
                $j.getScript('//www.paypalobjects.com/api/checkout.js');
                paypalContext = false;
            }
        });

        $j('#header-minicart').on('click', '.hide-minicart', function(e) {
            var parent = $j(this).parents('.skip-minicart-content');
            var link = parent.siblings('.skip-minicart-link');
            var body = $j('body');

            parent.removeClass('skip-active');
            link.removeClass('skip-active');
            body.removeClass('overlay');

            e.preventDefault();
        });
    });
