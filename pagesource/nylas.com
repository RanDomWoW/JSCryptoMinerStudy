 <script type="text/javascript">
 $(function() {
	// Mobile Nav Toggle
	$(".menu-mobile-trigger").on("click", function() {
		$(".Header").toggleClass("is-active");
		$(this).toggleClass("is-active");

	});

	$(".TryApi pre, .code--box-content pre").on("mouseup", function() {
		var sel, range;
		var el = $(this)[0];
		if (window.getSelection && document.createRange) { //Browser compatibility
			sel = window.getSelection();
			if (sel.toString() == '') { //no text selection
				window.setTimeout(function() {
					range = document.createRange(); //range object
					range.selectNodeContents(el); //sets Range
					sel.removeAllRanges(); //remove all ranges from selection
					sel.addRange(range); //add Range to a Selection.
				}, 1);
			}
		} else if (document.selection) { //older ie
			sel = document.selection.createRange();
			if (sel.text == '') { //no text selection
				range = document.body.createTextRange(); //Creates TextRange object
				range.moveToElementText(el); //sets Range
				range.select(); //make selection.
			}
		}
	});

	// Tabbed Content
	$(".Tab--item a").on("click", function(e) {
		e.preventDefault();
		$(".Tab--item, .Tab--item a").removeClass("active");
		$(this).parent().addClass("active");
		$(this).addClass("active");
		var TabID = $(this).attr("href");
		$(".Tabbed-content").removeClass("active");
		$(TabID).addClass("active");

	});

	// Rotate and Stop tab rotating tabs via hover and waypoint
	var clickNext = function() {
		var $active = $('.Tabs--three .active');
		var $next = $active.next();
		if ( ! $next.length) {
			$next = $('.Tabs--three li:first-child');
		}
		$next.find('a').click();
		
	};

	var rotateTabs = setInterval(clickNext, 3000);

	$('.Tabs--three li').hover(function(){
		clearInterval(rotateTabs);
	});

	var waypoints = $('.Tabs--three').waypoint({
		handler: function(direction) {
			if (direction == "down") {
				clearInterval(rotateTabs);
			}
		},
		offset: 200
	})

	$('.Quote--outer').cycle({
		slides: "> div.Quote--wrap",
		fx: "fade",
		delay: 4000
	});

	// Code Box Toggle
	$(".code--box .code--body .toggle").on("click", function() {
		$(this).toggleClass("active");
		$(".code--box .code--body .response").toggleClass("active");

	});
	
	// Faq Toggle
	$(".Faq.Toggle .Faq--header").on("click", function(e){
		$(this).toggleClass("active");
		$(this).next().toggleClass("active");
	});
	
	// Team
	$('.custom-team-member-module-wrapper').parent().addClass('team-wrapper');
	$('.custom-team-member-module, .custom-team-member-module-wrapper').unwrap();

	$('.team-wrapper .hs_cos_wrapper_type_widget_container > div').wrapAll('<div class="hs-team-inner-wrap"></div>');

	$(".team-wrapper .hs-team-inner-wrap").html($(".hs-team-inner-wrap .custom-team-member-module").sort(function(){
		return Math.random()-0.5;
	}));

    // Customer Slider
    $('.custom-slider .hs_cos_gallery_main .slick-slide-inner-wrapper > img').wrap('<div class="slide-image-wrapper"></div>');
    
    $('.slick-prev').html('<img width="21px" alt="Arrow" src="//cdn2.hubspot.net/hubfs/3314308/%20Nylas%20May%202017%20/Images/arrow-left@2x.png">')
    $('.slick-next').html('<img width="21px" alt="Arrow" src="//cdn2.hubspot.net/hubfs/3314308/%20Nylas%20May%202017%20/Images/arrow-right@2x.png">')
    
    $('.custom-slider .hs_cos_gallery_main .slick-slide .slide-image-wrapper > img').each(function(){
    	var GetAlt =$(this).attr('alt');
    	$(this).addClass(GetAlt);
    });
    
    // ScrollTo
    $('.scrollTo').click(function(e) {
    	if(this.hash && $(this.hash).length) {
    		e.preventDefault()
    		$('html, body').animate({
    			scrollTop: $(this.hash).offset().top
    		}, 500)
    	}
    });

    // Empower Promo Banner
    $('.close-empower-banner').on('click', function(event) {
    	event.preventDefault();
    	$('.empower-banner').hide();
    	localStorage.setItem('hide_empower_banner', true);
    });

    if (!localStorage.getItem('hide_empower_banner')) {
    	$('.empower-banner').show();
    }

    equalheight('.Tile, .Pricing--module');


});

 function dataWord() {
 	$("[data-words]").attr("data-words", function(i, d) {
 		var $self = $(this),
 		$words = d.split("|"),
 		tot = $words.length,
 		c = 0;

		// CREATE SPANS INSIDE SPAN
		for (var i = 0; i < tot; i++) $self.append($('<span/>', { text: $words[i] }));

		// COLLECT WORDS AND HIDE
	$words = $self.find("span").hide();

		// ANIMATE AND LOOP
		(function loop() {
			$self.animate({ width: $words.eq(c).width() });
			$words.stop().fadeOut().eq(c).fadeIn().delay(3000).show(0, loop);
			c = ++c % tot;
		}());

	});
 }

 function equalheight(container){

 	var currentTallest = 0,
 	currentRowStart = 0,
 	rowDivs = new Array(),
 	$el,
 	topPosition = 0;
 	$(container).each(function() {

 		$el = $(this);
 		$($el).outerHeight('auto')
 		topPostion = $el.position().top;

 		if (currentRowStart != topPostion) {
 			for (currentDiv = 0 ; currentDiv < rowDivs.length ; currentDiv++) {
 				rowDivs[currentDiv].outerHeight(currentTallest);
 			}
		 rowDivs.length = 0; // empty the array
		 currentRowStart = topPostion;
		 currentTallest = $el.outerHeight();
		 rowDivs.push($el);
		} else {
			rowDivs.push($el);
			currentTallest = (currentTallest < $el.outerHeight()) ? ($el.outerHeight()) : (currentTallest);
		}
		for (currentDiv = 0 ; currentDiv < rowDivs.length ; currentDiv++) {
			rowDivs[currentDiv].height(currentTallest);
		}
	});
 }

 $(window).on("load", dataWord);

 $(window).resize(function(){
 	equalheight('.Tile, .Pricing--module');
 });

 </script>
 



