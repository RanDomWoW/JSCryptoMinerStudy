<script type="text/javascript">
    $(document).ready(function () {
        // Use virtual page views for GA homepage ab testing
        dataLayer.push({ 'event': 'VirtualPageView', 'virtualPageURL': '/', 'virtualPageTitle': 'home-swing-takecontrol' }); 

        var cur = 1, count = $(".linkbtns").children().length;
        var interval = 7000; // 7 secs
        var T0;
        var w = 984;
        
        var $first, $cur, $next, $last, $slider, go;
        $first = $cur = $('.selected');
        $last = $('.last');
        $slider = $("#slider");
        go = true;
            
        $(".slink").click(function () { Stop(this); return false; });
        
        $("#slider").mouseenter(function () { Pause(); });                     // pause
        $("#slider").mouseleave(function () { Continue(); });  // resume
        var $slinks = $(".linkbtns");
            
        function Stop(obj) { 
            Pause();
            if($(obj).index() == 0)
                $cur = $last;
            else
                $cur = $(obj).prev(); 
            
            Next(); 
        }    
        
        function Pause() { 
            clearTimeout(T0); 
            go = false; 
        }
        
        function Continue() {
            go = true;
            T0 = setTimeout(Next,interval);
        }
                 
        T0 = setTimeout(Next,interval+000);   // on page load, take 0sec longer.

        function Next() {
            if($cur.hasClass('last'))
                $next = $first;
            else 
                $next = $cur.next();
            
            $slider.animate({ opacity: 0 }).animate({ left: -$next.index() * w }).animate({ opacity: 1 });
            $slinks.children().removeClass("selected");
            $next.toggleClass('selected');
            $cur = $next;
            
            SetTestimonial($cur.index());
            
            if(go)
               T0 = setTimeout(Next, interval);
        }
        
        function SetTestimonial(i){
            $('#testimonial').fadeOut(550, function() {
                $(this).html($('#testimonials-hidden div').eq(i).html());
                $(this).fadeIn(1800);
            });//fadeOut
        }
    }); 
</script>

