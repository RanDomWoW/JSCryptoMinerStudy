            $(a).find('div[id^="post-"]').each(function () {
                b = $(this).find(".entry-title>a");
                t = $(this).find(".entry-meta>a");
                var arrDt2 = $(this).find(".entry-meta").text();
                var arrDt = $(this).find(".post-date");
                dt = arrDt.text();
                $description = $(this).find(".entry-content");
                imgtmp = $(this).find(".wp-post-image").attr("src");
                if (imgtmp == null) {
                    switch (Math.floor((Math.random() * 6) + 1)) {
                        case 1:
                            imgtmp = "https://cdn.newretirement.com/images/strategies/dollar_house_small.jpg";
                            break;
                        case 2:
                            imgtmp = "https://cdn.newretirement.com/images/strategies/dollar_key.jpg";
                            break;
                        case 3:
                            imgtmp = "https://cdn.newretirement.com/images/strategies/growing_money_plant.jpg";
                            break;
                        case 4:
                            imgtmp = "https://cdn.newretirement.com/images/strategies/house_on_money.jpg";
                            break;
                        case 5:
                            imgtmp = "https://cdn.newretirement.com/images/strategies/house_umbrella.jpg";
                            break;
                        case 6:
                            imgtmp = "https://cdn.newretirement.com/images/strategies/money_plant.jpg";
                            break;
                        default:
                            imgtmp = "https://cdn.newretirement.com/images/strategies/nest_egg.jpg";
                    }
                }
                post = {
                    title: b.text(),
                    description: $description.text(),
                    link: b.attr("href"),
                    imgsrc: imgtmp
                };
                topic = {
                    title: t.text(),
                    link: t.attr("href")
                };
                d.append("<div class='row post-row'><div class='col-sm-4 col-md-3 col-lg-3'><a href='" + post.link + "'><img src='" + post.imgsrc + "' alt='' /></a></div><div class='col-sm-8 col-md-9 col-lg-9'><a href='" + topic.link + "' class='topic-link'>" + topic.title + "</a><a href='" + post.link + "' class='title-link'>" + post.title + "</a></div></div>");

                if (4 <= c) return !1;
                c++;
                d.append("<div class='section-border'></div>");
            })
        },
        error: function () { },
        complete: function () { }
    })
};

FetchLatestBlogPosts();

</script>

