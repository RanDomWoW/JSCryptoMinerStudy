      $('ul.dropdown-menu').each(function () {
        var __ob = $(this)

        if (__ob.parent().hasClass('max-dropdown'))
          return false

        var __button = (__ob.attr('aria-labelledby') ? $('button#' + __ob.attr('aria-labelledby')) : __ob.prev('button'))

        $('li > a', __ob).on('click', function () {
          $('li', $(this).parents('ul.dropdown-menu')).removeClass('active')
          $(this).parent().addClass('active')
        })

        __ob.parent().on('hidden.bs.dropdown', function () {
          var __this = $(this)
          var __active = $('li.active', __this)
          var __input = $('input[type=hidden]', __this.parent())

          if (__active.length) {
            $('> span', __button).html($('a', __active).html())

            if (__input.length)
              __input.val(__active.attr('value')).attr('value', __active.attr('value'))

          } else {
            $('> span', __button).html(__button.attr('placeholder'))

            if (__input.length)
              __input.val('')
          }
        }).trigger('hidden.bs.dropdown')
      });

