    <script type="text/javascript">
        var davSize = 0; //to store current daviation from actual size. Initial value is 0               
        var defaultSize = 0;


        function ChangeFont(size) {

            defaultSize = GetBroswerFontSize();

            if (size == '0') size = -davSize; //to reset font size to its actual value
            davSize = davSize + parseInt(size);
            if (davSize > 4 || davSize < -1) { davSize = davSize - parseInt(size); return; }
            document.getElementById('ctl00_fDav').value = davSize; //to handle postback

            //$('#Form1 p').add('#Form1 li').add('#Form1 td').add('#Form1 th').add('.scroller p').add('#Form1 .link_right h3').add('#Form1 div').add('#copyright').each(function () {
            //    this.style.fontSize = (parseInt(GetFontSize(this).replace("px", "")) + parseInt(size)) + 'px';
            //});

            $('#form1 p').add('#form1 li').each(function () {
                this.style.fontSize = (parseInt(GetFontSize(this).replace("px", "")) + parseInt(size)) + 'px';
            });

            $('#form1 p').add('#form1 div').each(function () {

                this.style.fontSize = (parseInt(GetFontSize(this).replace("px", "")) + parseInt(size)) + 'px';

            });

            $('#form1 p').add('#form1 td').each(function () {
                this.style.fontSize = (parseInt(GetFontSize(this).replace("px", "")) + parseInt(size)) + 'px';
            });




            $('.Parent').add('.maincontainer').each(function () {
                this.style.fontSize = (parseInt(GetFontSize(this).replace("px", "")) + parseInt(size)) + 'px';
            });
            //  alert(this.style.fontSize);
        }
        function GetFontSize(control) {
            if (control.style.fontSize) return control.style.fontSize.toString();
            var parent = control.parentNode;
            do {
                if (parent.style.fontSize) return parent.style.fontSize.toString();
                else parent = parent.parentElement;
            } while (parent);
            return defaultSize;
        }

        function GetBroswerFontSize() {
            var measure = document.createElement('div');
            measure.style.height = '10em';
            document.body.appendChild(measure);
            var size = measure.offsetHeight / 10;
            document.body.removeChild(measure);
            return size.toString();
        }



        
    </script>



    
