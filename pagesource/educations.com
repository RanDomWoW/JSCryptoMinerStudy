            var fullComment = commentContainer.data('comment');
            var croppedComment = commentContainer.html();
            commentContainer.html(fullComment);
            commentContainer.data('comment', croppedComment);

            var replyContainer = p.find('.review-reply-box').find('.review-reply-box-text');
            var fullReply = replyContainer.data('comment');
