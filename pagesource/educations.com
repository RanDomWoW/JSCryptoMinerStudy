            commentContainer.data('comment', fullComment);
            var replyContainer = p.find('.review-reply-box').find('.review-reply-box-text');
            var croppedReply = replyContainer.data('comment');
            var fullReply = replyContainer.html();
            replyContainer.html(croppedReply);
            replyContainer.data('comment', fullReply);
            p.find('.review-questions').hide();
            p.find('.review-bottom').hide();
            p.find('.review-facebook-comments-container').hide();
            p.find('.review-single-questions').hide();

            $(this).hide();
            $('.review-single-more', p).show();
        });

        $('.customer-comment-content').click(function () {
            if ($(this).hasClass('crop-comment')) {
                $(this).removeClass('crop-comment');
            }
