<script type="text/ng-template" id="multiple-selection-list-tpl">
    <div id="{{comp_id}}">
        <input type="text" class="floating-right multiple-select-list-holder "
               placeholder="{{uiLabel}}"
               ng-click="showMenu($event)"
               ng-class="{'error': checked.length==0 && is_required && form_submitted}"/>
        <div class="field data-row normal-check-box-container">
            <div class="check-container data-row"
                 ng-repeat="option in uiDefaultOptions"
                 ng-click="toggleSelection($event)">
                <label class="floating-right">
                    {{option[value_attribute]}}
                </label>
            </div>
            <div class="check-container data-row"
                 ng-repeat="cat in sourceList"
                 ng-click="add($index)">
                <label class="floating-right">
                    {{cat[value_attribute]}}
                </label>
            </div>
        </div>
        <div class="normal-check-box-selected">
            <div class="normal-check-box-selected item" ng-repeat="item in checked">
                <a ng-click="removeByIndex($index)" title="Remove {{item[value_attribute]}}"
                   alt="Remove {{item[value_attribute]}}">x</a>
                &nbsp;
                <span>{{item.title}}</span>

            </div>
        </div>
    </div>
</script>
