<script type="text/javascript">
$(document).ready(function(){
	var Objsilde=50;	//边缘检测厚??
	var targetTop=0;	
	var targetLeft=0;
	var speed=160;	//运动速度
	var mousePos;	//鼠标对象
	var oldX; var oldY;
	var nowX; var nowY;
	
	
	$('.boxgrid.captionfull').hover(function(){
		var Objwidth=$(this).width();
		var Objheight=$(this).height();
		
		var Objtop=$(this).offset().top;
		var Objleft=$(this).offset().left;
		
		if(mousePos.y<(Objtop+ Objsilde ) && mousePos.ox>Objleft && mousePos.ox<Objleft+Objwidth){
			targetTop=-Objheight;
			targetLeft=0;
		}else if(mousePos.x>(Objleft+Objwidth-Objsilde) && mousePos.oy>Objtop && mousePos.oy<Objtop+Objheight){
			targetTop=0;
			targetLeft=Objwidth;
		}else if(mousePos.y>(Objtop+Objheight-Objsilde) && mousePos.ox>Objleft && mousePos.ox<Objleft+Objwidth){
			targetTop=Objheight;
			targetLeft=0;
		}else if (mousePos.x<(Objleft+Objsilde) && mousePos.oy>Objtop && mousePos.oy<Objtop+Objheight){
			targetTop=0;
			targetLeft=-Objwidth;
			
		}
		$(".cover", this).show();
		$(".cover", this).fadeTo("fast", 1); 
		$(".cover", this).css({top:targetTop,left:targetLeft});
		$(".cover", this).stop().animate({top:0,left:0},{queue:false,duration:speed});
	}, function() {
		$(".cover", this).fadeOut("slow");
	});
	
	//鼠标坐标
	function mouseCoords(ev) 
	{
		ev=getEvent();
		if(ev.pageX || ev.pageY){
			return {x:ev.pageX, y:ev.pageY,ox:oldX,oy:oldY}; 
		}
		
		return { 
			x:ev.clientX + document.documentElement.scrollLeft - document.documentElement.clientLeft, 
			y:ev.clientY + document.documentElement.scrollTop - document.documentElement.clientTop,
			ox:oldX,
			oy:oldY
		}; 
		
	}
	
	//鼠标移动事件
	$(document).mousemove(function(e){
	  mousePos = mouseCoords(); 
	  oldX=mousePos.x;
	  oldY=mousePos.y;
	});

	
	//获得Event
	function getEvent(){
	   if(document.all) return window.event; 
	   func=getEvent.caller; 
	   while(func!=null){ 
		 var arg0=func.arguments[0]; 
		 if(arg0){ 
		   if((arg0.constructor==Event || arg0.constructor ==MouseEvent) || (typeof(arg0)=="object" && arg0.preventDefault && arg0.stopPropagation)){ 
			 return arg0; 
		   } 
		 } 
		 func=func.caller; 
	   } 
	   return null; 
	}
	
 function adsorption(){
    var headerWrap=document.getElementById('topbar');
    var scrollTop=0;
    window.onscroll=function(){
     scrollTop=document.body.scrollTop||document.documentElement.scrollTop;
     if(scrollTop>100){
      headerWrap.className='fixed';
     }else{
      headerWrap.className='dis';
     }
    }
   }
   adsorption();
	
});

</script>


