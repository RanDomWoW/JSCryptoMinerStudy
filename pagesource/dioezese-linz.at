<script language="JavaScript" type="text/JavaScript">
$(document).ready(function() { 

	// ====== set slider height - adapt to viewport height to show mainnav ======
	var viewportHeight = $(window).height();
	var navHeight = 172;
	var sliderHeight = viewportHeight - navHeight
	
	if (sliderHeight < 350) sliderHeight = 350;
	if (sliderHeight > 450) sliderHeight = 450;
	
	$(".index #sliderBox, #indexSlide .item").css("height", sliderHeight + "px")

	// ====== init slider ======
	$('#indexSlide').carouFredSel({
		responsive: true,
		auto: {
				play: true,
				timeoutDuration: 6000
			},
		width: '100%',
		height: sliderHeight,
		direction: 'left',
		easing: 'easeOutQuart',
		//prev: '#indexSlide_prev',
		//next: '#indexSlide_next',
		items: 1,
		scroll: {
				fx: 'crossfade',
				duration: 1500,
				onBefore: function(data) {

					var $selElm = data.items.visible;
					var elmClass = $selElm.attr("class");
					elmClass = elmClass.replace("item " , "");

					animateThumb($("#indexSlide_thumbs ." + elmClass));
				},
				onAfter: function(data) {
					loadImage(data.items.old.prev());
					loadImage(data.items.visible.next());
				}
			},
		onCreate: function(data) {
				loadImage(data.items);
				loadImage(data.items.next());
			}
	});

	// ====== place first thumb at the end of list ======
	if ($("#indexSlide_thumbs li").length > 2) {
		
		// place first thumb at the end of list
		$("#indexSlide_thumbs li:first").appendTo("#indexSlide_thumbs ul");
		
		// assign click to thumbs
		$("#indexSlide_thumbs li").click(function(){
			var elmClass = $(this).attr("class");
			elmClass = elmClass.replace("item " , "");
			var $slideToElm = $("#indexSlide ." + elmClass);
			
			// slide to selected item
			$("#indexSlide").trigger("slideTo", [$slideToElm, 0, false]);
			
		});
	}
	
	// ====== stop autoplay when clicking on prev/next/thumb ======
	$('#indexSlide_next, #indexSlide_prev, #indexSlide_thumbs li').click(function(){
		$("#indexSlide").trigger("configuration", ["auto", false]);
	});
	
	// ====== assign click to next link ======
	$('#indexSlide_next').click(function(){
		$("#indexSlide_thumbs li:first").click();
	});
});

// ===--- lazy load the actual background image ---===
function loadImage($img) {
	if ($img.length > 0) {
		imgSrc = $img.attr('data-background');

		if ($img.css("backgroundImage").indexOf("storage") == -1) {
			$img.css("backgroundImage","url(" + imgSrc + ")");
		}
	}
}

// ===--- animate thumb ---===
function animateThumb($thumb) {
	if ($thumb.length > 0) {
		// first, get width of items
		var itemWidth = $("#indexSlide_thumbs li:first").width();

		// then animate
		$thumb.animate({
			top: "80px",
			opacity: 0
		}, 800, "easeOutQuart", function(){
			$(this).animate({
				width: "0px"
			}, 500, "easeOutQuart", function(){
				$(this).appendTo("#indexSlide_thumbs ul").css({
					width: itemWidth + "px",
					top: "0px",
					opacity: 1
				});
			});
		});
	}
}

</script>

