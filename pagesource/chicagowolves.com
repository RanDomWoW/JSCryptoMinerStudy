  <script type="text/javascript">

    function updateScore(){

				jQuery.getJSON( '/livescoredata.js?1997510596', function(data){

					jQuery.each(data,function () {

						if ( typeof this.HomeTeam !== 'undefined' && typeof this.VisitingTeam !== 'undefined') {


							if( this.HomeTeam.ID=='330' || this.VisitingTeam.ID=='330'){

								if( this.SmallStatus!=='Pre-Game' && this.SmallStatus.toLowerCase().indexOf("final") == -1  ){

										jQuery('.bulb').removeClass('on');

										var HomeTeamScore=this.HomeTeam.Score;
										var HomeTeamTens=Math.floor(HomeTeamScore/10);
										var HomeTeamOnes=HomeTeamScore % 10;
										if(HomeTeamTens>0){ jQuery('.points.hometeam .tens .s-'+HomeTeamTens).addClass('on'); }
										jQuery('.points.hometeam .ones .s-'+HomeTeamOnes).addClass('on');

										var AwayTeamScore=this.VisitingTeam.Score;
										var AwayTeamTens=Math.floor(AwayTeamScore/10);
										var AwayTeamOnes=AwayTeamScore % 10;
										if(AwayTeamTens>0){ jQuery('.points.visitingteam .tens .s-'+AwayTeamTens).addClass('on'); }
										jQuery('.points.visitingteam .ones .s-'+AwayTeamOnes).addClass('on');

										var period=this.Period.charAt(0);
										jQuery('.points.period .number .s-'+period).addClass('on');

										return false;

									}else if( this.SmallStatus.toLowerCase().indexOf("final") >= 0 ){

										//jQuery('.game-day-takeover').slideUp();
										jQuery('.points.period  .bulb').removeClass('on');
										jQuery('.points.period .number .final').addClass('on');

									}


							}
						}

					});

				});

			}

   function countDown(end, cur){

	   this.end_date = new Date(end);
		this.cur_date = new Date(cur);

		var context = this;

		var update = function(){
			context.cur_date.setSeconds(context.cur_date.getSeconds()+1);
			var time_diff = (context.end_date - context.cur_date)/1000;

			// Check if timer expired:
			if (time_diff<0){
				//jQuery('.hours, .minutes, .seconds').html(0);

			}

			var one_minute 	= 60; //minute unit in seconds
			var one_hour 	= 60 * 60; //hour unit in seconds
			var one_day 	= 60 * 60 * 24; //day unit in seconds
			var one_month 	= 60 * 60 * 24 * 31; //day unit in seconds

			var month_field 	= Math.floor(time_diff / one_month);
			var day_field 		= Math.floor((time_diff - month_field * one_month) / one_day);
			var hour_field 		= Math.floor((time_diff - month_field * one_month - day_field * one_day) / one_hour);
			var minute_field 	= Math.floor((time_diff - month_field * one_month - day_field * one_day - hour_field * one_hour) / one_minute);
			var second_field 	= Math.floor(time_diff - month_field * one_month - day_field * one_day - hour_field * one_hour - minute_field * one_minute);
			second_field = (second_field.toString().length == 1 )? '0' + second_field : second_field;

			jQuery('.countdown .clock .number .on').removeClass('on')

			if((hour_field==0 && minute_field==0 && second_field==0)){
				jQuery('.countdown').fadeOut( 'fast',function(){jQuery('.livescoreboard').fadeIn();});
				return false;
			}


			var hour_tens=Math.floor(hour_field/10);
			var hour_ones=hour_field % 10;
			jQuery('.hours .tens .s-'+hour_tens).addClass('on');
			jQuery('.hours .ones .s-'+hour_ones).addClass('on');

			var minute_tens=Math.floor(minute_field/10);
			var minute_ones=minute_field % 10;
			jQuery('.minutes .tens .s-'+minute_tens).addClass('on');
			jQuery('.minutes .ones .s-'+minute_ones).addClass('on');

			var second_tens=Math.floor(second_field/10);
			var second_ones=second_field % 10;
			jQuery('.seconds .tens .s-'+second_tens).addClass('on');
			jQuery('.seconds .ones .s-'+second_ones).addClass('on');


			// Call recursively
			setTimeout(update, 1000);
		};

		// Call the recursive loop
		update();

		}


   jQuery(document).ready(function($){

		setInterval("updateScore()",60000);

		$.getJSON( '/livescoredata.js', function(data){

				$.each( data, function() {



					if ( typeof this.HomeTeam !== 'undefined' && typeof this.VisitingTeam !== 'undefined') {


						if( this.VisitingTeam.ID=='330' || this.HomeTeam.ID=='330' ){

								var gamedate=this.Date.split('-');

								var monthNames = [ "Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec" ];

								var dayNames = [ "Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat" ];

								var puckdrop = new Date(monthNames[gamedate[1]-1]+' '+gamedate[2]+', '+gamedate[0]);

								var day = puckdrop.getDate();
								var dayIndex = puckdrop.getDay();
								var monthIndex = puckdrop.getMonth();
								var year = puckdrop.getFullYear();

								var gametime= this.ScheduledTime.split(':');

								var hours = gametime[0];

								if( /PM/i.test(gametime[1]) ){
									hours=parseInt(hours)+12;
								}
								var minutes=gametime[1].split(' ',1);

								var timezone=this.Timezone;
                                //console.log(timezone);

								var offset=-600;

								// Atlantic Time	UTC -4:00 / -3:00
								// Eastern Time		UTC -5:00 / -4:00
								// Central Time		UTC -6:00 / -5:00
								// Mountain Time	UTC -7:00 / -6:00
								// Pacific Time		UTC -8:00 / -7:00


                                // if(timezone=="Canada/Atlantic"){ offset='-400'; }
                                // else if(timezone=="Canada/Eastern"){ offset='-500'; }
                                // else if(timezone=="Canada/Central"){ offset='-600'; }
                                // else if(timezone=="Canada/Mountain"){ offset='-700'; }
                                // else if(timezone=="Canada/Pacific"){ offset='-800'; }

                                //DAYLIGHT SAVINGS
                                if(timezone=="Canada/Atlantic"){ offset='-400'; }
                                else if(timezone=="Canada/Eastern"){ offset='-500'; }
                                else if(timezone=="Canada/Central"){ offset='-600'; }
                                else if(timezone=="Canada/Mountain"){ offset='-700'; }
                                else if(timezone=="Canada/Pacific"){ offset='-800'; }

								// if(timezone=="AST"){ offset='-400'; }
								// else if(timezone=="EST"){ offset='-500'; }
								// else if(timezone=="EDT"){ offset='-400'; }
								// else if(timezone=="CST"){ offset='-600'; }
								// else if(timezone=="CDT"){ offset='-500'; }
								// else if(timezone=="MST"){ offset='-700'; }
								// else if(timezone=="MDT"){ offset='-600'; }
								// else if(timezone=="PST"){ offset='-800'; }
								// else if(timezone=="PDT"){ offset='-700'; }
								// else if(timezone=="AKST"){ offset='-900'; }
								// else if(timezone=="AKDT"){ offset='-800'; }
								// else if(timezone=="HST"){ offset='-1000'; }
								// else if(timezone=="HAST"){ offset='-1000'; }
								// else if(timezone=="HADT"){ offset='-900'; }
								// else if(timezone=="SST"){ offset='-1100'; }
								// else if(timezone=="SDT"){ offset='-1000'; }
								// else if(timezone=="CHST"){ offset='+1000'; }
								// else if(timezone=="NZDT"){ offset='+1300'; }

								var puckdrop = new Date(dayNames[dayIndex]+' '+monthNames[monthIndex]+' '+day+' '+year+' '+ hours+':'+minutes+':00 GMT'+offset+' ('+timezone+')' );

								var golive = new Date(puckdrop);

								golive.setHours(golive.getHours() - 12);


								var now = new Date();


								if( this.SmallStatus.toLowerCase().indexOf("final") == -1 ){

										if(golive<now){

											if( this.SmallStatus!=='Pre-Game' ){
													$('.countdown').fadeOut( 'fast',function(){ $('.livescoreboard').fadeIn(); });

													var HomeTeamScore=this.HomeTeam.Score;
													var HomeTeamTens=Math.floor(HomeTeamScore/10);
													var HomeTeamOnes=HomeTeamScore % 10;
													if(HomeTeamTens>0){ $('.points.hometeam .tens .s-'+HomeTeamTens).addClass('on'); }
													$('.points.hometeam .ones .s-'+HomeTeamOnes).addClass('on');

													var AwayTeamScore=this.VisitingTeam.Score;
													var AwayTeamTens=Math.floor(AwayTeamScore/10);
													var AwayTeamOnes=AwayTeamScore % 10;
													if(AwayTeamTens>0){ $('.points.visitingteam .tens .s-'+AwayTeamTens).addClass('on'); }
													$('.points.visitingteam .ones .s-'+AwayTeamOnes).addClass('on');

													var period=this.Period.charAt(0);
													$('.points.period .number .s-'+period).addClass('on');

													$('.watch-now h2').text('Live Now!');

												}else if(puckdrop>now){
														countDown((puckdrop.getTime()), (now));
												}else{

													console.log('PUCKDROP: '+puckdrop);
													console.log('NOW: '+now);
														$('.countdown').fadeOut( 'fast',function(){$('.livescoreboard').fadeIn();});
												}


											$('.hometeam .name').append(': '+this.HomeTeam.Code);

											$('.visitingteam .name').append(': '+this.VisitingTeam.Code);

											$('.game-day-takeover, .show-on-live-game').slideDown();

											$('.game-center-item .gallery-image').slideUp().remove();

											$('.small-calendar').parent('.game-center-item').slideUp().remove();

											$('.game-center-item').css('height','auto').equalHeights();

											}


										} else {

											if(this.VisitingTeam.ID=='330'){
												var wolvesScore=parseInt(this.VisitingTeam.Score);
												var opponentScore=parseInt(this.HomeTeam.Score);
											}else if(this.HomeTeam.ID=='330'){
												var wolvesScore=parseInt(this.HomeTeam.Score);
												var opponentScore=parseInt(this.VisitingTeam.Score);
											}

											if(wolvesScore>opponentScore){
												$('.game-day-takeover .result').html('<h4>Winners</h4><div class="scoreboard winners" style="width:100%"><div><h1>WOLVES WIN!</h1></div></div>');
												$('.game-day-takeover').css('max-height','120px');
