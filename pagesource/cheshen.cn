				if ($('#focus-pic button').length>0){
					$('#focus-pic button').hide();
					clearInterval(interval);
				}
			},1000/60);
			$('#focus-pic').mouseenter(function () {
				$('#focus-pic button').fadeIn(300);
			})
			$('#focus-pic').mouseleave(function () {
				$('#focus-pic button').fadeOut(300);
			})
			//焦点图左右按钮end
	
			$('img').imglazyload({
				fadeIn: true
			});

			//超级领队bind
			var mediaUrl = cmm.addParam('mod', 'getMedia', location.href);
			$.ajax({
				type: 'GET',
				url: mediaUrl,
				success: function (data) {
					//console.log(JSON.stringify(data));
					var _data = JSON.parse(data);
					if (_data.status == 200) {
						var list = _data.data;
						if(list.length>0) {
							for (var i=0;i<list.length;i++){

								var len = $('.cs-sidebar-super .super-con .btn_follow').length;
								for (var j=0;j<len;j++){
									var nowObj = $('.cs-sidebar-super .super-con .btn_follow').eq(j);
									if (list[i].id == nowObj.attr('data-id')){
										nowObj.attr('data-isfollow',list[i].isFollow);
										if (list[i].isFollow==1){
											nowObj.find('.iconfont').removeClass('icon-add').addClass('icon-correct');
											nowObj.find('i').html('已关注');
										}
									}
								}
							}
						}
					}else{
						sky_dialog.toast(_data.msg);
					}
				},
				complete: function () {

				}
			});
		})();
	</script>

