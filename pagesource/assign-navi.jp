	$(function() {
		//個別既読処理（リンク押下）
		$('a.internal-link').click(function(){
			var msgId = '#msg-' + $(this).data('msg-id');
			//-->既読
			$(msgId).removeClass('un_read_row');
			if ($(this).data('msg-id') == undefined) {
				return;
			}
			var url = '/activity_reports/readed/' + $(this).data('msg-id');
			var tryCount = 0;
			var retryLimit = 3;

			$.ajax({
				url: url,
				type: 'GET',
				dataType: 'json',
				success: function(data) {
				},
				error: function() {
					tryCount++;
					if (tryCount <= retryLimit) {
						$.ajax(this);
						return;
					}
				}
			});
		});
		//メッセージ既読処理

	});
</script>
