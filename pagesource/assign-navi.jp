<script type="text/javascript">
	$(function() {
		// submit
		$('#TroubleConfMailForm').on('submit', function (event) {
			event.preventDefault();
			if (!$('#trouble_mail').val()) {
				$('#trouble_conf_body_error').show();
				$("#trouble_conf_body_error").html('メッセージを入力してください。');
			} else if ($('#trouble_mail').val().length > '1000'){
				$("#trouble_conf_body_error").html('1000文字以内にしてください');
			} else {
				var textarea = $('<div>').text($('#trouble_mail').val()).html();
				$(".t_textarea").html(textarea.replace(/[\n\r]/g, "<br />"));
				$('.pull_conf_def').hide();
				$('.pull_conf_set').show();
				$('#trouble-conf-submit-button').on('click', function() {　// 送信ボタンクリック
					$('#TroubleConfMailForm').off().submit();　// イベントハンドラを解除してから再submit
					$('*').off();　// 全イベントを無効化する
				});
				//キャンセルボタン
				$('#trouble-conf-cancel-button').on('click', function() {　// キャンセルボタンクリック
					$('.pull_conf').removeClass('selected');
					$('.pull_conf').hide();　// イベントハンドラを解除してから再submit
					$('.pull_span').removeAttr('style');
					$('.pull_span').removeClass('selected');
					$('.pull_conf_def').show();
					$('.pull_conf_set').hide();
					$('#trouble_conf_body_error').hide();
				});
			}
		});

		$('#TroubleSendMailForm').on('submit', function (event) {
			event.preventDefault();
			if (!$('#trouble_send_mail').val()) {
				$('#trouble_send_body_error').show();
				$("#trouble_send_body_error").html('メッセージを入力してください。');
			} else if ($('#trouble_send_mail').val().length > '1000'){
				$("#trouble_send_body_error").html('1000文字以内にしてください');
			} else {
				var textarea = $('<div>').text($('#trouble_send_mail').val()).html();
				$(".t_textarea").html(textarea.replace(/[\n\r]/g, "<br />"));
				$('.pull_send_def').hide();
				$('.pull_send_set').show();
				$('#trouble-send-submit-button').on('click', function() {　// 送信ボタンクリック
					$('#TroubleSendMailForm').off().submit();　// イベントハンドラを解除してから再submit
					$('*').off();　// 全イベントを無効化する
				});
				//キャンセルボタン
				$('#trouble-send-cancel-button').on('click', function() {　// キャンセルボタンクリック
					$('.pull_span').removeClass('selected');
					$('.pull_send').hide();　// イベントハンドラを解除してから再submit
					$('.pull_span').removeAttr('style');
					$('.pull_conf').removeClass('selected');
					$('.pull_send_def').show();
					$('.pull_send_set').hide();
					$('#trouble_send_body_error').hide();
				});
			}
		});

		$('#trouble-conf-cancel , #trouble-send-cancel').on('click', function() {　// キャンセルボタンクリック
			$('.pull_span').removeClass('selected');
			$('.pull_conf, .pull_send').slideUp('fast');
			$('.pull_span').removeAttr('style');
			$('#trouble_send_body_error').hide();
			$('#trouble_conf_body_error').hide();
		});
	});
</script>
