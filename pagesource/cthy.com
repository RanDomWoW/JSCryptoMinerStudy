        rollink.onmouseover = function () { clearInterval(MyMar); }  //鼠标移上时清除定时器达到滚动停止的目的
        rollink.onmouseout = function () { MyMar = setInterval(Marquee, rollspeed); }  //鼠标移开时重设定时器

