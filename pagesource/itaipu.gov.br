      <script>
		$('.news-content').each(function() {
		   var content = $(this);
		   var w = content.width();
		   var paddingLeft = parseInt(content.css('padding-left')) || 0;
		   var paddingRight = parseInt(content.css('padding-right')) || 0;
		   var nw = w - (paddingLeft + paddingRight);
		   
		   content.find('table, img').each(function() {
		      var $this = $(this);
		      
		      if ($this.width() > nw) {
		           var parents = $.makeArray($this.parentsUntil(content).map(function() {
		           var parent = $(this)

		           return (parseInt(parent.css('padding-left')) || 0) + (parseInt($(this).css('padding-right')) || 0);
		         })).reduce(function(a, b) { return a + b }, 0);
		         $this.width(nw - parents);
		      }
		   })
		});
	  </script>
