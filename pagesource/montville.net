                            "#hp-upcoming-header, #hp-shortcuts-header { background-color: "+primaryColor+"; }"+
                            "h1.ui-article-title a, h3 { color: "+secondaryColor+"; }";
			
			var rules = document.createTextNode(dynStyles);
			
