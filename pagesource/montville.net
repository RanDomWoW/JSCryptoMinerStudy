   function setupCustomColors() {
		var dynStyleSheet = document.createElement('style');
		
		if(dynStyleSheet) {
			dynStyleSheet.setAttribute('type', 'text/css');
			var head = document.getElementsByTagName('head')[0];
			
			if(head) {
				head.insertBefore(dynStyleSheet, head.childNodes[0]);
			}
			
			var dynStyles = "#gb-nav-container, #gb-logo-container { background-color: "+secondaryColor+"; }"+
                            "#gb-header-gradient {"+ 
                            	"background: -moz-linear-gradient(left,  rgba("+hexToRgb(primaryColor)+",0) 0%, rgba("+hexToRgb(primaryColor)+",1) 65%, rgba("+hexToRgb(primaryColor)+",1) 100%);"+ 
								"background: -webkit-linear-gradient(left,  rgba("+hexToRgb(primaryColor)+",0) 0%, rgba("+hexToRgb(primaryColor)+",1) 65%, rgba("+hexToRgb(primaryColor)+",1) 100%);"+
								"background: linear-gradient(to right,  rgba("+hexToRgb(primaryColor)+",0) 0%, rgba("+hexToRgb(primaryColor)+",1) 65%, rgba("+hexToRgb(primaryColor)+",1) 100%);"+
