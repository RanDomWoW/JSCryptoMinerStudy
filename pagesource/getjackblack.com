<script type="text/javascript">
    $(document).ready(function () {
        var txtSearch = $('.txtSearch');
        var divPred = $('#divPredictions');
        var predictionArr = [];
        var lnkSelected;
        var q = null;

        txtSearch.blur(function () {
            setTimeout(function () { divPred.hide(); }, 300);
        });

        function searchEnter() {
            window.location = "/search.aspx?keyword=" + txtSearch.val();
        };

        $('.imgMapSearchArea').click(searchEnter);

        txtSearch.keyup(function (event) {
            if (event.which == 40 || event.which == 9) {
                // arrow down || tab
                if (lnkSelected && lnkSelected.next().length > 0) lnkSelected = lnkSelected.next();
                else lnkSelected = divPred.children().first();
                event.preventDefault();
            } else if (event.which == 38) {
                if (lnkSelected && lnkSelected.prev().length > 0) lnkSelected = lnkSelected.prev();
                // arrow up
                event.preventDefault();
            } else if (event.which == 13) {
                // enter
                searchEnter();
                event.preventDefault();
            } else {
                var refreshList = (q == null && this.value != '');
                if (this.value == '') { q = null; }
                else { q = new RegExp('^' + this.value, 'i'); }
                lnkSelected = null;
                if (refreshList) {
                    $.ajax('/predictivesearch.aspx/SearchPredictions', {
                        contentType: 'application/json; charset=utf-8',
                        type: 'POST',
                        dataType: 'json',
                        data: '{ q:"' + this.value.charAt(0) + '" }',
                        success: function (data) {
                            predictionArr = data.d;
                            showTop();
                        }
                    });
                } else {
                    showTop();
                }
            }
            divPred.children().removeClass('lnkSelected');
            if (lnkSelected) {
                lnkSelected.addClass('lnkSelected');
                txtSearch.val(lnkSelected.html());
            }
        });

        var topPrediction = '';
        var topProducts = [];
        function showTop() {
            divPred.html('').hide();
            if (q) {
                var maxResults = 5;
                var hasResults = false;
                var newResults = [];
                for (var i = 0; i < predictionArr.length && maxResults > 0; i++) {
                    if (q.test(predictionArr[i])) {
                        newResults.push(predictionArr[i]);
                        maxResults--;
                        hasResults = true;
                    }
                };

                if (hasResults) {
                    for (var i = 0; i < newResults.length; i++) {
                        divPred.append('<a class="lnkPredResult" href="/search.aspx?keyword=' + newResults[i] + '">' + newResults[i] + '</a>');
                    };

                    for (var i = 0; i < topProducts.length; i++) {
                        divPred.append('<a class="lnkPredResult" href="' + topProducts[i].ProductURL + '"><table class="tblPredProd" cellpadding="0" cellspacing="0" border="0"><tr><td class="tdPredProdImg"><img src="' + topProducts[i].ProductImage + '" /></td><td>' + topProducts[i].ProductName + '<br/>' + topProducts[i].ProductPrice + '</td></tr></table></a>');
                    };

                    divPred.show();

                    if (topPrediction != newResults[0]) {
                        topPrediction = newResults[0];
                        $.ajax('/predictivesearch.aspx/SearchProducts', {
                            contentType: 'application/json; charset=utf-8',
                            type: 'POST',
                            dataType: 'json',
                            data: '{ keyword:"' + topPrediction + '" }',
                            success: function (data) {
                                if (data.d) {
                                    topProducts = data.d;
                                    showTop();
                                }
                            }
                        });
                    }
                }
            }
        };
    });
    
    $(document).ready(function () {
        if ($('#headerTopRotator .textSlide').length > 1) {
            $('#headerTopRotator').cycle({ fx: 'fade', speed: 50, timeout: 2700, slides: '.textSlide', pause: 1 });
        }
        if ($('#headerTopRotatorMobile .textSlide').length > 1) {
            $('#headerTopRotatorMobile').cycle({ fx: 'fade', speed: 50, timeout: 2700, slides: '.textSlide', pause: 1 });
        }
    });
</script>


