                            function agendaBusca(mes, ano) {
                            $.ajax({
                            url: '/agenda-atracao-busca',
                                    dataType: 'json',
                                    type: 'get',
                                    data: {
                                    mes:mes,
                                            ano:ano
                                    },
                                    success: function (dados) {
                                    for (var i = 0; i < dados.length; i++) {
                                    var d = dados[i].data.split('-');
                                    var mes = d[1];
                                    var dia = d[2];
                                    if (mes.startsWith('0')) mes = mes.substr(1, 1);
                                    if (dia.startsWith('0')) dia = dia.substr(1, 1);
                                    $('.minical_day_' + mes + '_' + dia + '_' + d[0]).addClass('minical_today');
                                    }
                                    }
                            });
                            }






