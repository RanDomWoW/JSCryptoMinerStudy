                            $('.div-dados select').change(function(){
                            var value = $(this).val();
                            var end = $(this).parent().parent().parent().find('.end');
                            var num = $(this).parent().parent().parent().find('.num');
                            var endereco = value.split("###");
                            end.val(endereco[0]);
                            num.val(endereco[1]);
                            });
                            $('#selOrigem, #selDestino').change(function(){
                            $('#frmRotas').find('fieldset').attr('disabled', 'disabled');
                            var input = $(this);
                            var idCategoria = $(this).val();
                            var divDados = input.parent().parent().parent().find('.div-dados');
                            $('#onumero').val('');
                            if (idCategoria == 0){
                            divDados.find('input').show();
                            divDados.find('select').hide();
                            $('#frmRotas').find('fieldset').removeAttr('disabled');
                            } else{
                            divDados.find('input').hide();
                            divDados.find('select').show();
                            divDados.find('select').html('');
                            //    Buscar Dados
                            $.ajax({
                            url : 'api/unidades-e-equipamentos?categoria=' + idCategoria,
                                    dataType : 'JSON',
                                    success : function(data){
                                    divDados.find('select').append($('<option>', {value: 0, text: 'Escolha uma opção'}));
                                    $.each(data, function(index, value){
                                    divDados.find('select').append($('<option>', {value: value.endereco_sem_acento, text: value.titulo}));
                                    });
                                    $('#frmRotas').find('fieldset').removeAttr('disabled');
                                    }
                            });
                            }

                            });


