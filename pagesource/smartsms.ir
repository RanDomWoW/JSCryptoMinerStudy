            window.login_fail = function(xhr) {
                //alert('error');
            };

            $(document).ajaxStart(function() {
                var fixModal = $('.modal');
                if (fixModal.length == 0) {
                    fixModal = $('<div class="modal" />');
                    fixModal.appendTo(document.body);
                }
                fixModal.show();
                $('*').addClass('progress');
            }).ajaxStop(function() {
                $('*').removeClass('progress');
                var fixModal = $('.modal');
                fixModal.hide();
            });

            $(document).ready(function() {
                $('#keepme > label').click(function() {
                    var checkbox = $('#keepme > input[type="checkbox"]')[0],
                        self = $(this);
                    checkbox.checked = !checkbox.checked;
                });
            });
        </script>
