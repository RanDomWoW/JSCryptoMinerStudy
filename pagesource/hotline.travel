	var blockCurrentSections = {};
	function viewSubSection(blockId, sectionId, parentId, t) {
		var viewSection = 0;

		$('#custom-block-' + blockId + ' .post-title-list-item').removeClass('active');
		$('#custom-block-' + blockId + ' .dropdown-list li').removeClass('active');
		$('#custom-block-' + blockId + ' .dropdown-text').text('ЕЩЁ').css('color', '');

		if (blockCurrentSections[blockId] && blockCurrentSections[blockId] == sectionId) {
			viewSection = parentId;
			blockCurrentSections[blockId] = 0;
			console.log('Родительский раздел');
		} else {
			blockCurrentSections[blockId] = viewSection = sectionId;
			$('.sub-section-' + viewSection).parent().addClass('active');
			if ($('.sub-section-' + viewSection).is('.dropdown-item')) {
				console.log('Дочерний раздел');
				$('#custom-block-' + blockId + ' .hidden-xs .dropdown-text').text($('.hidden-xs .sub-section-' + viewSection).text()).css('color', '#e5404b');
				$('#custom-block-' + blockId + ' .visible-xs .dropdown-text').text($('.visible-xs .sub-section-' + viewSection).text()).css('color', '#e5404b');

			}
		}

		if (!viewSection) {
			return;
		}

		$('#custom-block-' + blockId).find('.overlay').show();
		$.ajax(
			{
//            url:'/new-mainpage-9567aa34dffe0129761ba7144d2a34db/custom-block-get-data.php',
				url: '/custom-block-get-data.php',
				dataType: 'json',
				type: 'post',
				data: {section: viewSection, template: t},
				success: function(d) {
					$('#custom-block-' + blockId).find('.overlay').hide();
					if (d && d.result == 1) {
						$('#custom-block-' + blockId).find('.carousel-item').remove();
						$('#custom-block-' + blockId).find('.carousel-list').append(d.html);
						$('#custom-block-' + blockId).find('.carousel-list').data('bxSlider').reloadSlider();

						cackle_ajax_init();
					} else {
						alert('Не удалось обновить данные. Повторите попытку позже.');
					}
				},
				error: function() {
					$('#custom-block-' + blockId).find('.overlay').hide();
					alert('Не удалось выполнить запрос к серверу. Повторите попытку позже.');
				}
			}
		);
	}

	var cackle_ajax_init = function() {
		if (!Cackle) {
			console.log('!Cackle');
			return;
		}

		cackle_widget = [];
		cackle_widget.push({widget: 'CommentCount', id: 29049});
		Cackle.bootstrap(true);
	};
