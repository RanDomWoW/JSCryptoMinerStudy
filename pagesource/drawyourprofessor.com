$(document).ready(function(){
$('#word_select').change(function(){
var test = $( "#word_select option:selected" ).text();
if(test == "Professor"){
$('#wordsearch').attr('placeholder', 'Find Your Professor');
}else if(test == "School"){ 
$('#wordsearch').attr('placeholder', 'Find Your School');
}
});
});

$(document).ready(function() {
var inputTimer;
$('#wordsearch').keyup(function(){
clearTimeout(inputTimer);
inputTimer = setTimeout(ajaxFunction, 800); //fires after 1 second
});

});

function ajaxFunction() {

var sel = $('#wordsearch').val();
var profSchool = $('#word_select option:selected').val();
var length = sel.length;
var data = {word : sel, prof_school : profSchool};

if(length >= 2){
var currentRequest = $.ajax({
                        type: "POST",
                        url: "wordsearch.php", 
                        data: data,
                        beforeSend : function()    {           
                if(currentRequest != null) {
                    currentRequest.abort();
                }
            },
                         
                        success: function(data) {
                            $('#drop_ul').html(data);
                      
                        }
               });

}
}

