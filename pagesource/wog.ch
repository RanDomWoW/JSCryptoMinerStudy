	<script type="text/javascript">
		$(document).ready( function() {

		

		 // Search Suggestion
		 $("#searchTerm").autocomplete({
				source: function(request, response) {
					$.ajax({
						url: "https://www.wog.ch/index.cfm/json.searchSuggestionList",
						dataType: "json",
						data: {
							term: request.term,
							type: $("#productTypeID").val()
						},
						success: function(data) {
							response(data);
						}
					});
				},
				minLength: 2,
				delay: 300,
				focus: function( event, ui ) {
					$("#searchTerm").val( ui.item.VALUE );
					return false;
				},
				select: function( event, ui ) {
					$("#searchTerm").val( ui.item.VALUE );
					$("#search-form").submit();
				}
			});
			$.ui.autocomplete.prototype._renderItem = function( ul, item){
			  var term = this.term.split(' ').join('|');
			  var re = new RegExp("(" + term + ")", "gi") ;
			  var title = item.LABEL.replace(re,'<span class="highlight">$1</span>');
			  var desc = item.DESC.replace(re,'<span class="highlight">$1</span>');
			  return $( '<li></li>' )
			     .data( 'item.autocomplete', item )
			     .append( '<a title="' + item.LABEL + '"><div class="suggested-item"><div class="cover"><img src="' + item.COVER + '" alt="' + item.LABEL + '" width="' + item.COVERWIDTH + '" height="' + item.COVERHEIGHT + '" /></div><div class="desc"><strong>' + title + '</strong><br /><small>' + desc + '</small></div><div class="clear"></div></div></a>' )
			     .appendTo( ul );
			};
		});

		function SBopen() {
			document.body.style.overflow = "hidden";
		}

		function SBclose() {
			document.body.style.overflow = "auto";
		}

