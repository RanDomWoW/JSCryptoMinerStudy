function TrackSearch(elemeno_chiave,tipoform){
	var pagina_corrente;
	var stringa=document.getElementById(elemeno_chiave).value;
	if(elemeno_chiave=='keyword_dest' || elemeno_chiave=='keyword_allog') {
		var location_ratesearch_dal=document.getElementById('location_ratesearch_dal').value;
		var location_ratesearch_al=document.getElementById('location_ratesearch_al').value;
		var s=document.getElementById('location_ratesearch_numadults');
		var location_ratesearch_numadults=s.options[s.selectedIndex].value;
		if(location_ratesearch_dal!='' && location_ratesearch_al!='' && location_ratesearch_numadults!='' ){
			pagina_corrente='datada='+location_ratesearch_dal+'&dataa='+location_ratesearch_al+'&adulti='+location_ratesearch_numadults;
		}else{
			pagina_corrente='solodestinazione';
		}
	}else pagina_corrente=location.href;;	
	if(pageThreeTracker!=null)pageThreeTracker._trackEvent(tipoform,stringa,pagina_corrente);
	return true;
}

function Migliori(elemento,var_ricezione_ris,nomeform,cercahotel) {
		if (movimento) { movimento=false; return false; }
		var stringa=var_ricezione_ris.value;
		attesoris=document.getElementById(elemento);
		if (stringa.length<3){
			if (attesoris!=null){
				attesoris.innerHTML='';attesoris.style.border="none";attesoris.style.display='none';attesoris.style.width=0;
			}
			controllotasti='';
			return false;
		}
		if (stringa==controllotasti) return false;
		controllotasti=stringa;
		form_ricezione=nomeform;
		ricezione_ris=var_ricezione_ris;	
	    var r = Math.random();
	    myRequestRSC = CreateXmlHttpReq(RispostaRSC);
	    if (cercahotel) myRequestRSC.open("GET","/inc/search_htype.php?frase_ric="+escape(stringa)+"&id_lingua=1&rand="+escape(r),true);
	    else myRequestRSC.open("GET","/inc/search_gtype.php?frase_ric="+escape(stringa)+"&id_lingua=1&rand="+escape(r),true);
	    myRequestRSC.send(null);
}

function MettiSelezione(componente,id){
	var testo=componente.innerHTML;

	//var re = new RegExp("^(.*?)<span.*?>(.*?)</span>$");
	var re = new RegExp("^(.*?)<(span|SPAN).*?>(.*?)</(SPAN|span)>$");
	var m = re.exec(testo);
	if(m!=null) testo=m[1];
				
	var parent=ricezione_ris.parentNode;
	ricezione_ris.value=testo;
	var elemeno_chiave_form=form_ricezione.elemeno_chiave_form.value;
 	var tipoform_form=form_ricezione.tipoform_form.value;
	var inputhidden;
	if( !$(parent).find('#id_suggested').length ){
		$(parent).append('<input type="hidden" name="id_suggested" id="idsuggested" value="'+id+'" />');
	}else{
		$(parent).find('#id_suggested').attr('value',id);
	}
 	TrackSearch(elemeno_chiave_form,tipoform_form);
	attesoris.innerHTML='';
	attesoris.style.border="none";
	attesoris.style.width=0;
	attesoris.style.display="none";
}


function MouseSeleziona(elemento){
	ricezione_ris.focus();
	for (p=1;p<=posizione_searc_max;p++){
		var doveciclo=document.getElementById('risultato_'+p);
		if (elemento==doveciclo) {
			posizione_searc=p;elemento.style.background="#00003E";elemento.style.color="#FFFFFF";
		} else {	
			doveciclo.style.background="";doveciclo.style.color="";
		}
	}
}
//var enterpressed=false;
function noNumbers(e,partenza){
	//posizione_searc=partenza;
	var keynum;
	var keychar;
	var numcheck;	
	if(window.event){ // IE
		keynum = e.keyCode;
	}else if(e.which){ // Netscape/Firefox/Opera
		keynum = e.which;
	}

	if (!e) {
	      var e = window.event;
	       }
	    if (e.target) {
	      targ = e.target;
	    }
	    else if (e.srcElement) targ = e.srcElement;
	    if (targ.nodeType == 3) // defeat Safari bug
	        targ = targ.parentNode;
	
	if(keynum==9) return;
	if (keynum==13 || keynum==39){
		if(attesoris.style.display!='none'){
			resetRisultati(attesoris,true);
		}
		if(targ.id=='keyword_dest'){
			targ.blur();return false;
		}
		if(targ.id=='keyword_allog'){
			targ.blur();return false;
		}
		if(targ.id=='keyword2'){
			targ.blur();return false;
		}		
	}

	/*if (keynum==13){
		if(attesoris.style.display!='none'){
			resetRisultati(attesoris,true);
			enterpressed=true;
		}else if(enterpressed==true){
			if(e.target.id=='keyword_dest'){
				if(chk_form_searchalloggi_dest()) 
					document.getElementById('searchalloggi_dest').submit();
			}
			if(e.target.id=='keyword_allog'){
				if(TrackSearch('keyword_allog','search nome hotel') && chk_form_searchalloggi()) 
					document.getElementById('searchalloggi').submit();
			}
		}
	}*/
	
	if (keynum==40) SpostoSelezione(1);
	if (keynum==38) SpostoSelezione(-1);
}
function SpostoSelezione(direzione){
	movimento=true;
	var vadoa=posizione_searc+direzione;
	if (vadoa<1) vadoa=1;
	if (vadoa>posizione_searc_max) vadoa=posizione_searc_max;
	componente=null;
	for (p=1;p<=posizione_searc_max;p++) {
		var doveciclo=document.getElementById('risultato_'+p);
		if (p==vadoa) {
			doveciclo.style.background="#00003E";
			doveciclo.style.color="#FFFFFF";
			componente=doveciclo;
		} else {
			doveciclo.style.background="#FFFFFF";
			doveciclo.style.color="#555555";
		}
	}
	var testo='';
	if (componente!=null){
		testo=componente.innerHTML;
		id=$(componente).find('span.elid').html();
		
		//var re = new RegExp("^(.*?)<span.*?>(.*?)</span>$");
		var re = new RegExp("^(.*?)<(span|SPAN).*?>(.*?)</(SPAN|span)>$");
		var m = re.exec(testo);
		if(m!=null) testo=m[1];
		
		if(id!=""){
			var inputhidden;
			var parent=ricezione_ris.parentNode;
			if( !$(parent).find('#id_suggested').length ){
				$(parent).append('<input type="hidden" name="id_suggested" id="idsuggested" value="'+id+'" />');
			}else{
				$(parent).find('#id_suggested').attr('value',id);
			}
		}
	}
	posizione_searc=vadoa;
	ricezione_ris.value=testo;
	
}
function resetRisultati(elemento,forzo){
 	if (focusDIV && !forzo) return;
	elemento.style.border="0";
	elemento.innerHTML='';
	elemento.style.width='0px';
	elemento.style.display="none";	
	posizione_searc_max=0;
	posizione_searc=0;
	controllotasti='';
}   
function RispostaRSC(){
	if (myRequestRSC.readyState == 4 && myRequestRSC.status == 200) {
		var migliori=myRequestRSC.responseText;
		var risultati=migliori.split("<CAT_NAME>");
		var stampo='';
		posizione_searc_max=risultati.length-1;
		var larghezza_risultati=ricezione_ris.clientWidth;
		var re = new RegExp("^([0-9]+):(.*?)$");
		for (var p=1;p<risultati.length;p++) {
			var m = re.exec(risultati[p]);
			if(m!=null) {
				elemtoprint=m[2];
				elemtoid=m[1];
			}else{
				elemtoprint=risultati[p];elemtoid='';
			}
			stampo+='<div id="risultato_'+p+'"  onfocus="focusDIV=true;" onBlur="focusDIV=false;" class="risultato_suggestions" onmouseover="focusDIV=true;MouseSeleziona(this);" onmouseout="focusDIV=false;" onclick="MettiSelezione(this,\''+elemtoid+'\');">'+elemtoprint+'<span class=elid style="display:none">'+elemtoid+'</span></div>';
		}
		if (attesoris!=null){
			attesoris.innerHTML=stampo;
			if (stampo==''){
				 attesoris.style.border="none";
				 attesoris.style.display="none";
				 attesoris.style.width=0;
		      } else {
		        attesoris.style.border="solid 1px #c6c6c6";
		        attesoris.style.width=larghezza_risultati+'px';
		        attesoris.style.display="block";
		      }
		}
	}
}
</script>

