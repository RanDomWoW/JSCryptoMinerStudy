function readCookie(name) {
	var nameEQ = name + "=";
	var ca = document.cookie.split(';');
	for(var i=0;i < ca.length;i++) {
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
	}
	return null;
}

function showPopUpRedirect () {
		if (readCookie('redirect2')==null && (true)){
		if($('#popUpRedirect').css('display')=='none'){
			$('body').append('<div id=sfondo> </div>');
			var h=0;
			var w=0;
			var server_name = 'www.nozio.com';
			  if( typeof( window.innerWidth ) == 'number' ) {
			    //Non-IE
			    w = window.innerWidth;
			    h = window.innerHeight;
			  } else if( document.documentElement && ( document.documentElement.clientWidth || document.documentElement.clientHeight ) ) {
			    //IE 6+ in 'standards compliant mode'
			    w = document.documentElement.clientWidth;
			    h = document.documentElement.clientHeight;
			  } else if( document.body && ( document.body.clientWidth || document.body.clientHeight ) ) {
			    //IE 4 compatible
			    w = document.body.clientWidth;
			    h = document.body.clientHeight;
			  }

			mtop=Math.floor(h/2-(400/2));
			mleft=Math.floor(w/2-(570/2));
			if(($.browser.msie && parseInt($.browser.version)==6 )){
				$('#popUpRedirect').css({'margin-top':mtop,'margin-left':mleft});
			}else{
				$('#popUpRedirect').css({'position':'fixed','top': mtop, 'left': mleft});
			}
			$('#sfondo').click(function () {
				server_name_arr = server_name.split(".");
				SetLangCookie(server_name_arr[server_name_arr.length - 1]);
				$('#popUpRedirect').fadeOut('fast');
				$(this).remove();
			});

			$('#popUpRedirect').fadeIn('fast').css({'z-index': 500});
			$('#popUpRedirect').find('span.close').click(function (){$('#popUpRedirect').fadeOut('fast');$('#sfondo').remove();});
			$('#popUpRedirect').find('a.modalCloseImg').click(function (){$('#popUpRedirect').fadeOut('fast');$('#sfondo').remove();});
			
		}else{
			$('#popUpRedirect').fadeOut('fast');
		};
	}
}
function SetLangCookie( value ){
var today = new Date();
var mantieni = document.getElementById('cookieCambioLingua');
if (mantieni==null) return false;
today.setTime( today.getTime() );
expires = 365 * 1000 * 60 * 60 * 24;
var expires_date = new Date( today.getTime() + (expires) );
document.cookie = "redirect2=www.nozio."+escape( value ) +
( ( mantieni.checked ) ? ";expires=" + expires_date.toGMTString() : "" ) +";path=/;domain=www.nozio.com";
}
