<script type="text/javascript">

function trim(str){
	while (str.length>0 && str.substring(0,1) == ' '){str = stringa.substring(1, str.length);}
	while (str.length>0 && str.substring(str.length-1, str.length) == ' '){str = str.substring(0,str.length-1);}
	return str;
}

$(document).ready(function(){

	var sPath = window.location.pathname;
	var sDir = sPath.substring(0,sPath.lastIndexOf('/') + 1);
	var sPage = sPath.substring(sPath.lastIndexOf('/') + 1);

	$('a.intresting').click(function (){
		href=$(this).attr('href');
		var pre='';
		if(!eregi("^javascript:",href) && !eregi("^#",href)){
			if(eregi("^http:\/\/",href)){
				pre="/"+(eregi("^http://www.nozio.com",href) ?"int":"out")+"/";
			}else if(eregi("^\/",href)){
			
				if(eregi("^\/recClick",href)){
					pre='/int_nozio_banner/';
				}
				else if(eregi("^\/goto",href)){
					pre='/out/';
				} else {
					pre='/int/';
				}
			}else {
				pre='/int/';
			}
		}else{
			pre='/int/';
		}

		var post='';
		post=$(this).attr('title');
		if(post==undefined || post=="" || post=='undefined' ) {post=$(this).text();}
		if(post==undefined || post=="" || post=='undefined' ) {post=$(this).attr('title');}
		if(post==undefined || post=="" || post=='undefined' ) {post=$(this).find('img:first').attr('alt');}
		if(post==undefined || post=="" || post=='undefined' ) {post=$(this).find('img:first').attr('src');post=post.replace("/img/","");}

		post=post.replace(/\//g,"");
		arr=post.split(" ");
		app=Array();
		for(i=0;i<arr.length;i++) {
			if(trim(arr[i])=="") continue;
			app[app.length]=arr[i];
		}
		post=app.join("_");
		pageTracker._trackPageview(pre+post);
		//onclick viene cmq richiamato
		//$(this).attr('onclick');
	});
});
</script>
