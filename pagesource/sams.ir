	<script type="text/javascript">
	    $(document).ready(function () {
	        //		$("#divResellers").accordion();

	        $("#dialogOnlineSupport").dialog({ title: "پشتیبانی آنلاین", autoOpen: false });

	        $("#btnOnlineSupport").click(function () {
	            $("#dialogOnlineSupport").dialog("open");
	            //			var iframe = document.getElementById('iframeChat');
	            //			iframe.src = iframe.src;
	        });

	        // Skitter slide show
	        $('#slideShow').skitter({ dots: true, interval: 3000 });

	        // ================ animated div ================== for customer list
	        var speed = 40; // pixels per second
	        function animate(obj) {
	            // setup animation of specified block "obj"
	            var text_height = obj.find(".text").height();
	            // calculate distance of animation
	            var distance = parseInt(obj.css("top")) - (-text_height);
	            //duration will be distance / speed
	            obj.animate(
							{ top: -text_height },  //scroll upwards
							1000 * distance / speed,
							"linear",
							function () {
							    obj.css("top", 0); // scroll to start position
							    animate(obj);
							});
	        }

	        $("#rollup").each(function () {
	            var obj = $(this).find(".news");
	            //				obj.find(".text").clone().appendTo(obj);

	            var stoped = null;
	            $(this).find(".rollup_Scroller").mouseover(function () {
	                if (stoped)
	                    return;

	                stoped = true;
	                obj.stop();

	                obj.css("top", 0); // scroll to start position
	                $(this).css("overflow-y", "auto");
	            }).mouseout(function () {
	                stoped = null;
	                obj.css("top", 0); // scroll to start position
	                $(this).css("overflow-y", "hidden");
	                $(this).scrollTop(0);
	                animate(obj); // resume animation
	            });
	            animate(obj); // start animation
	        });



	    });
	</script>
