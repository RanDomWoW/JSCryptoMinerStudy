                    $(".tpContent").html(data.Text);
                    $(".tpName").html(data.Author + ", <span class='tpDateCreated'>" + data.Created + "</span>");
                    var iOS = /iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream;
                    var iw = (iOS) ? screen.width : window.innerWidth;
                    if (iw < 600) {
                        $(".tpName").html($(".tpName").html().replace(", ", "<br/>"));
                    }
                    setTimeout(function () {
                        $("#pnlTPReview").css("min-height", $("#pnlTPReviewTemplate").height());
                    }, 300)
                    setTimeout(function () {
                        $("#pnlTPReview").html($("#pnlTPReviewTemplate").html().replace("[fadeReview]", "fadeReview"));
                    }, 700);
                })
                .fail(function (jqXHR, textStatus, errorThrown) {
                    $("[id*='pnlTrustpilot']").hide();
                    OnError("showTrustPilotReview", jqXHR.status);
                });
            }
            TPReview(7, false);
