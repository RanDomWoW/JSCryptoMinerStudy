    <script>
        $(document).ready(function () {
            function TPReview(_index, _next) {
                $.getJSON('/account/ajax/trustpilotReview.aspx', { index: _index, next: _next }, function (data) {
                    $("#pnlTPReview").html($("#TPLoader").html());
                    $(".reviewIndex").html(data.Index);
