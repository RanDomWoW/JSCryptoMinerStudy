    <script type="text/javascript">
      $(document).on('ready', function() {
        $("#mainSlide>.slide div").clone().prependTo("#mainSlide .thumb");
        var isSp = false;

        var ua = navigator.userAgent;
        if( $(window).width() < 721 || ua.indexOf('iPhone') > 0 || ua.indexOf('iPod') > 0 || ua.indexOf('Android') > 0 && ua.indexOf('Mobile') > 0){
          isSp = true;
        }

        $("#mainSlide>.slide").slick({
          dots: false,
          infinite: true,
          slidesToShow: 1,
          fade: true,
          arrows: false,
          asNavFor: '#mainSlide>.thumb',
          autoplay: true,
          autoplaySpeed: 4500,
          speed: 500,
          swipe:isSp,
          responsive: [{
            breakpoint: 721,
            settings: {
              slidesToShow: 1,
              slidesToScroll: 1,
              infinite: true,
              dots: false
            }
          }]
        });
              
        $("#mainSlide>.thumb").slick({
          dots: false,
          infinite: true,
          slidesToShow: 3,
          slidesToScroll: 1,
          speed: 500,
          swipe:isSp,
          asNavFor: '#mainSlide>.slide',
          appendArrows: $('#mainSlide .arrows'),
          focusOnSelect: true,
          responsive: [{
            breakpoint: 721,
            settings: {
              slidesToShow: 2.5,
              slidesToScroll: 1,
            }
          }]
        });

        $('#mainSlide>.thumb').on('mouseenter', '.slick-slide', function (e) {
          
          var slickObj = $('#mainSlide>.slide').slick('getSlick');
          
          if( slickObj.animating || isSp ) return;

          var $currTarget = $(e.currentTarget);
          var index = $currTarget.data('slick-index');
          var _ = $('#mainSlide>.thumb').slick('getSlick');

          $('#mainSlide>.slide').slick('slickPause');

          slickObj.options.speed = 0;
          _.options.speed = 0;

          _.slideCurrentPos = 0 - ( ( $currTarget.offset().left - $('#mainSlide>.thumb>.slick-list').offset().left ) / 225 ) ;

          slickObj.slickGoTo(index);

          slickObj.options.speed = 500;
          _.options.speed = 500;
            
        })
        .on('mouseout', function(e) {
          $('#mainSlide>.slide').slick('slickPlay');
        });

      });
    </script>
