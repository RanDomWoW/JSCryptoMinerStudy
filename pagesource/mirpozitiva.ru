(function () {
  var cookie = {
    get: function (name) {
      var value = document.cookie.match( new RegExp("(?:^|; )"+ name +"=([^;]*)") );
      return (value && value[1]) ? value[1] : null;
    },

    set: function (name, value) {
      document.cookie = name +"="+ value;
    }
  };

  if ( cookie.get("slidingPopup") )
    return;

  var slidingPopup = document.getElementById("slidingPopup");

  function close() {
    slidingPopup.style.bottom = "-"+ (slidingPopup.clientHeight + 35) +"px";
  }

  close();

  document.getElementById("slidingPopup_close").onclick = function() {
    close();
  };

  setTimeout(function()
  {
    cookie.set("slidingPopup", "1");

    slidingPopup.style.left = "50%";
    slidingPopup.style.bottom = "0px";
  }, 15000);
})()

