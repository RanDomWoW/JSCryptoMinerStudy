				var $map_details_slick_nav = $map_details_slick_controls.find('.slick-nav');
			}

			$map_details_slick.slick(
			{
				speed : 300,
				// fade : true,
				arrows : true,
				infinite : true,
				cssEase : 'linear',
				adaptiveHeight : false,
				appendArrows : $map_details_slick_nav,
				nextArrow : '<span class="slick-arrow slick-next"><span class="fa fa-angle-right"></span></span>',
				prevArrow : '<span class="slick-arrow slick-prev"><span class="fa fa-angle-left"></span></span>'
			});

			//console.log('Initialized: init_map_details_slick();');
		}


		function init_interactive_map()
		{
			var $cnt_map_details_slick = $('#map_details_slick');
			var $cnt_map_markers = $('#cnt_map_markers');
			var $marker = $cnt_map_markers.find('a');
			var $cnt_marker = $cnt_map_markers.find('.map-marker');

			if($marker.length > 0)
			{
				$marker.off('click').on('click', function ()
				{
					var slide_id = $(this).attr('href');
					var index = $cnt_map_details_slick.find('.item').index($(slide_id));

					$cnt_map_details_slick.slick('slickGoTo' , index - 1);

					$cnt_marker.removeClass('active');
					$(this).closest('.map-marker').addClass('active');

					return false;
				});
			}

			if($cnt_map_details_slick.length > 0)
			{
				var $item = $cnt_map_details_slick.find('.item');

				$cnt_map_details_slick.on('afterChange', function (event, slick, currentSlide)
				{
					var $current_item = $item.eq(currentSlide);
					var slide_id = $current_item.attr('id');
					var $anchor = $("[href='" + '#' + slide_id + "']");

					$cnt_marker.removeClass('active');
					$anchor.closest('.map-marker').addClass('active');
				});
			}
		}

		setTimeout(function()
		{
			$('.interactive-map-container .map-details-container').css("overflow", "visible");
		}, 3500);
	});
