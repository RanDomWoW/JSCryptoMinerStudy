<script type="text/javascript">
function getCarAttr(t,pid,eid){
	var h='<option selected="selected" value="0">'+(t=='series'?'车系':'车型')+'</option>';
	pid=parseInt(pid);
	if(pid>0){
		$.getJSON(HM.query+'oil/ajax/car_'+t+'/'+pid+'/time'+(new Date()).getTime(),function(r){
			for(var i=0;i<r.length;i++)h+='<option value="'+r[i][0]+'">'+r[i][1]+'</option>';
			$('#'+eid).html(h)
		})
	}else{
		$('#'+eid).html(h)
	}
}
$(function(){
	$('#searchProducts').submit(function(){
		 var u='//m.sinolub.com/index.php/oil/list/';
		 var b=parseInt($('#searchBoxBrand').val());
		 var t=parseInt($('#searchBoxType').val());
		 var r=parseInt($('#searchBoxRank').val());

		 if(b>0)u+='a'+b+'-';
	     if(t>0)u+='a'+t+'-';
		 if(r>0)u+='a'+r+'-';
		 var cb=parseInt($('#oilCarBrand').val()),cs=parseInt($('#oilCarSeries').val()),cm=parseInt($('#oilCarModel').val());
		 if(cm>0){
				u+='a'+cm+'-'
			}else if(cs>0){
				u+='a'+cs+'-'
			}else if(cb>0){
				u+='a'+cb+'-'
			}
			//alert(u);
		   window.open(u+'p1.html','_blank')
		  return false

		})
})
</script>


