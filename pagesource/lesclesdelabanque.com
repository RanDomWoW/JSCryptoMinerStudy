$(document).ready(function(){
	$('ul.liens-sites li').each(function() {
		$(this).find('div.js').addClass('description cacherContenu');
		$(this).find('a').focusin(function() {
			link_over(this);
		});
		$(this).find('a').focusout(function() {
			link_out(this);
		});
		$(this).find('a').hover(function() {
			link_over(this);
		},
		function() {
			link_out(this);
		});
	});
});
function link_over(elem) {
	var url = $(elem).find('img').attr('src');
	
	//modif Devoteam
	var hoverPattern = /_hover\.png/i;
	if (hoverPattern.test(url)) return false;
	var reg = /\.png/gi;
	var result = url.replace(reg, '');
	$(elem).find('img').attr('src', result + '_hover.png');
	$(elem).parents('li').find('div.description').removeClass('cacherContenu');
	//$('.text-intro').css({opacity: 0.2});
}
function link_out(elem) {
	var url = $(elem).find('img').attr('src');
	
	//modif Devoteam
	var hoverPattern = /_hover\.png/i;
	if (!hoverPattern.test(url)) return false;
	
	var reg = /_hover\.png/gi;
	var result = url.replace(reg, '');
	$(elem).find('img').attr('src', result + '.png');
	$(elem).parents('li').find('div.description').addClass('cacherContenu');
	//$('.text-intro').css({opacity: 1});
}
