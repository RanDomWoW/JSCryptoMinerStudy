





<!doctype html>
<html>
<head>
    
    <link rel="stylesheet" href="/resources/css/font.css">
    <link rel="stylesheet" href="/resources/css/custom.css">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>Sınaq imtahanı nəticəsi</title>
    <meta name="robots" content="index, follow">
    <meta name="keywords" content="Sınaq imtahanı nəticəsi">
    <meta name="description" content="Sınaq imtahanı nəticəsi">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <link rel="icon" type="image/png" href="/resources/img/exam-favicon.png"/>
    <script src="/resources/js/jquery.js"></script>
    <style>

        html, body {
            padding: 0;
            margin: 0;
            height:100%;
        }

        body {
            background: url(/files/img/bg.jpg) no-repeat center;
            color: #231f20;
            font-family: Arial;
            margin: 0;
        }

        .container {
            display: table;
            margin: auto auto auto auto;
            outline: 0px solid #d1d3d4;
            width: 300px;
            padding-top: 30vh;
            padding-bottom: 10vh;
        }

        .formcontainer {
            background-color: white;
            display: table;
            margin: auto;
            outline: 0px solid #d1d3d4;
            width: 300px;
            box-shadow: 0 0 8px 0 #242e33;
        }

        .formcontainertitle {
            background-color: #d1d3d4;
            font-size: 20px;
            font-weight: bold;
            padding: 11px 0;
            text-align: center;
        }

        .forminputcontainer {
            display: table;
            margin: 5px 0 10px 10px;
            outline: 0 solid;
            width: 290px;
        }

        .forminputcontainer label {
            font-size: 14px;
            font-weight: bold;
            line-height: 25px;
        }

        .forminputcontainer select {
            border: medium none;
            font-size: 14px;
            outline: 1px solid #d1d3d4;
            padding: 6px 2px;
            width: 280px;
        }

        .forminputcontainer input[type="text"] {
            border: medium none;
            font-size: 14px;
            outline: 1px solid #d1d3d4;
            padding: 6px 4px;
            width: 271px;
        }

        .forminputcontainer input[type="submit"] {
            font-size: 14px;
            margin: 0 -2px 0 -2px;
            padding: 8px;
            width: 120px;
        }

        .forminfo {
            font-size: 12px;
            line-height: 17px;
            padding: 5px 10px;
            text-align: left;
            width: 280px;
        }

    </style>
</head>
<body>

<form method="get" action="result">

    <div class="container">

        <div class="formcontainer">

            <div class="formcontainertitle">SINAQ İMTAHANI NƏTİCƏSİ</div>
            <input type="hidden" name="type" value="2">

            <div class="forminputcontainer">
                <label>İMTAHAN</label><br>
                <select autocomplete="off" name="exam" id="examId">

                    
                    <option value="253">Sınaq 5 (təkmilləşdirmə)
                    </option>
                    
                    <option value="266">5-ci qrup - Sınaq 6
                    </option>
                    

                </select>
            </div>

            <div class="forminputcontainer">
                <label>SİNİF</label><br>
                <select autocomplete="off" name="class" id="classId">
                    <option value="2">2</option>
                    <option value="3">3</option>
                    <option value="4">4</option>
                    <option value="5">5</option>
                    <option value="6">6</option>
                    <option value="7">7</option>
                    <option value="8">8</option>
                    <option value="9">9</option>
                    <option value="10">10</option>
                    <option value="11">11</option>
                </select>
            </div>

            <div class="forminputcontainer">
                <label>QRUP</label><br>
                <select autocomplete="off" name="group" id="groupId">
                    <option value="-1">---</option>
                    <option value="1">I Qrup</option>
                    <option value="2">II Qrup</option>
                    <option value="3">III Qrup</option>
                    <option value="4">IV Qrup</option>
                    <option value="5">V Qrup</option>
                </select>
            </div>

            <div class="forminputcontainer">
                <label>BÖLMƏ</label><br>
                <select autocomplete="off" name="sector" id="sectorId">
                    <option value="1">Azərbaycan bölməsi</option>
                    <option value="2">Rus bölməsi</option>
                </select>
            </div>

            <div class="forminputcontainer">
                <label>İŞ NÖMRƏSİ</label><br>
                <input type="text" autocomplete="off" name="code" id="code"/>
            </div>

            <div class="forminputcontainer">
                <input type="submit" value="NƏTİCƏ"/>
            </div>

        </div>
    </div>
</form>

</body>
</html>

<!-------------->

<script type="text/javascript">

    var pExam = null;
    var pClass = null;
    var pGroup = null;
    var pSector = null;
    var pCode = null;


    if (0 != $('#examId option[value=' + pExam + ']').length) {
        $('#examId').val(pExam);
    }

    if (0 != $('#classId option[value=' + pClass + ']').length) {
        $('#classId').val(pClass);
    }

    if (0 != $('#groupId option[value=' + pGroup + ']').length) {
        $('#groupId').val(pGroup);
    }

    if (0 != $('#sectorId option[value=' + pSector + ']').length) {
        $('#sectorId').val(pSector);
    }

    if (pCode == '0') {
        $('#code').focus();
    }


    $('form').submit(function () {

        if ($('select[name=exam]').val() == '0') {
            alert('İmtahanı seçin !');
            return false;
        }

        if ($('select[name=class]').val() == '0') {
            alert('Sinif seçin !');
            return false;
        }

//             if ($('select[name=group]').val()=='0'){
//                 alert('Qrup seçin !');
//                 return false;
//             }

        if ($('select[name=sector]').val() == '0') {
            alert('Bölmə seçin !');
            return false;
        }

        if ($('input[name=code]').val().length == 0) {
            alert('İş nömrəsini daxil edin !');
            return false;
        }
        return true;
    });

    $(document).ready(function () {
        
    });

</script>