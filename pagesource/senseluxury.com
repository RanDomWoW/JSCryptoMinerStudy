    function closeBroswerBar(){
        document.getElementById('browser-tip').style.display = "none";
        document.body.scrollTop = document.body.scrollTop + 1;
    }
    window.onscroll = function(){
        var scrTop = 0;
        if(document.documentElement&&document.documentElement.scrollTop){
            scrTop=document.documentElement.scrollTop;
        }else if(document.body){
            scrTop=document.body.scrollTop;
        }
        if (scrTop > 60 && document.getElementById('browser-tip')){
            document.getElementById('browser-tip').style.position = "fixed";
        }
        else if (document.getElementById('browser-tip')){
            document.getElementById('browser-tip').style.position = "static";
        }
    }
    var userAgent = navigator.userAgent,
        rMsie = /(msie\s|trident.*rv:)([\w.]+)/,
        rFirefox = /(firefox)\/([\w.]+)/,
        rOpera = /(opera).+version\/([\w.]+)/,
        rChrome = /(chrome)\/([\w.]+)/,
        rSafari = /version\/([\w.]+).*(safari)/;
    var browser;
    var version;
    var ua = userAgent.toLowerCase();
    function uaMatch(ua) {
        var match = rMsie.exec(ua);
        if (match != null) {
            return { browser : "IE", version : match[2] || "0" };
        }
        var match = rFirefox.exec(ua);
        if (match != null) {
            return { browser : match[1] || "", version : match[2] || "0" };
        }
        var match = rOpera.exec(ua);
        if (match != null) {
            return { browser : match[1] || "", version : match[2] || "0" };
        }
        var match = rChrome.exec(ua);
        if (match != null) {
            return { browser : match[1] || "", version : match[2] || "0" };
        }
        var match = rSafari.exec(ua);
        if (match != null) {
            return { browser : match[2] || "", version : match[1] || "0" };
        }
        if (match != null) {
            return { browser : "", version : "0" };
        }
    }
    function hintAppend(downUrl){
        var appenddiv= document.createElement("div");
        appenddiv.innerHTML = '<div id = "browser-tip"><div class = "browser-tip-main"><b><img src = "/statics/web3/css/images/warning-inbar.png"/></b><span>为了支付安全和更好的体验，请</span><a id = "update-browser" target = "_blank">升级至最新版本</a><span>，推荐使用以下主流浏览器：</span><a rel="nofollow" href = "http://www.google.cn/chrome/browser/desktop/index.html" target = "_blank">Chrome</a><span>、</span><a rel="nofollow" href = "https://www.firefox.com/" target = "_blank">Firefox</a><span>、</span><a rel="nofollow" href = "http://windows.microsoft.com/ie" target = "_blank">IE11+</a><span>、</span><a rel="nofollow" href = "https://www.apple.com/safari/" target = "_blank">Safari</a></div><div id = "browser-tip-close" onclick = "closeBroswerBar()"><img src = "/statics/web3/css/images/close-inbar.png"/></div>';
        var first=document.body.firstChild;
        document.body.insertBefore(appenddiv,first);
        document.getElementById('browser-tip').style.display = "block";
        document.getElementById('update-browser').setAttribute('href',downUrl);
    }
    var browserMatch = uaMatch(userAgent.toLowerCase());
    if (browserMatch.browser) {
        var browser = browserMatch.browser;
        var version = parseInt(browserMatch.version.split('.')[0]);
        if (browser == "IE" && version < 11){
            hintAppend('http://windows.microsoft.com/ie')
        }
        if (browser == "firefox" && version < 16){
            hintAppend('https://www.firefox.com/')
        }
        if (browser == "chrome" && version < 36){
            hintAppend('http://www.google.cn/chrome/browser/desktop/index.html')
        }
        if (browser == "opera" && version < 23){
            hintAppend('http://www.google.cn/chrome/browser/desktop/index.html')
        }
        if (browser == "safari" && version < 9){
            hintAppend('https://www.apple.com/safari/')
        }
    }

