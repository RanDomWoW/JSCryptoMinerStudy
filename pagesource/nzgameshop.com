		<script>
			function pad_number(num, size)
			{
				var s = "000000000" + num;
				return s.substr(s.length-size);
			}
		
			if ($('.time').length)
			{
				var time = $('.time').attr('rel');
				$('#countdown').countdown(time, function(event) {
					var format = '%H:%M:%S';
					var days = pad_number(parseInt(event.strftime('%-d')), 2);
					var hours = pad_number(parseInt(event.strftime('%H')), 2);
					var mins = pad_number(parseInt(event.strftime('%M')), 2);
					var secs = pad_number(parseInt(event.strftime('%S')), 2);
					if (parseInt(days) < 1)
					{
						hours = pad_number((days * 24) + hours, 2);
						var timer = "<div class='col_xs_4'>" + "<span class='deal_number'>" + hours.substr(0, 1) + "</span>" + "<span class='deal_number'>" + hours.substr(1, 1) + "</span>" + "</div>";
						timer += "<div class='col_xs_4'>" + "<span class='deal_number'>" + mins.substr(0, 1) + "</span>" + "<span class='deal_number'>" + mins.substr(1, 1) + "</span>" + "</div>";
						timer += "<div class='col_xs_4'>" + "<span class='deal_number'>" + secs.substr(0, 1) + "</span>" + "<span class='deal_number'>" + secs.substr(1, 1) + "</span>" + "</div>";
						timer += "<div class='col_xs_12 flex'>";
						timer += "<div class='col_xs_4 text_xxs caps'>Hrs</div>";
						timer += "<div class='col_xs_4 text_xxs caps'>Mins</div>";
						timer += "<div class='col_xs_4 text_xxs caps'>Secs</div>";
						timer += "</div></div>";
						format = '%-d day%!d ' + format;
					}
					else
					{
						var timer = "<div class='col_xs_4'>" + "<span class='deal_number'>" + days.substr(0, 1) + "</span>" + "<span class='deal_number'>" + days.substr(1, 1) + "</span>" + "</div>";
						timer += "<div class='col_xs_4'>" + "<span class='deal_number'>" + hours.substr(0, 1) + "</span>" + "<span class='deal_number'>" + hours.substr(1, 1) + "</span>" + "</div>";
						timer += "<div class='col_xs_4'>" + "<span class='deal_number'>" + mins.substr(0, 1) + "</span>" + "<span class='deal_number'>" + mins.substr(1, 1) + "</span>" + "</div>";
						timer += "<div class='col_xs_12 flex'>";
						timer += "<div class='col_xs_4 text_xxs caps'>Days</div>";
						timer += "<div class='col_xs_4 text_xxs caps'>Hrs</div>";
						timer += "<div class='col_xs_4 text_xxs caps'>Mins</div>";
						timer += "</div></div>";
						format = '%-d day%!d ' + format;
					}
					$(this).html(event.strftime(timer));
				});
			}
			
			if ($('.pagetime').length)
			{
				var time = $('.pagetime').attr('rel');
				$('#countdown').countdown(time, function(event) {
					var format = '%H:%M:%S';
					var days = pad_number(parseInt(event.strftime('%-d')), 2);
					var hours = pad_number(parseInt(event.strftime('%H')), 2);
					var mins = pad_number(parseInt(event.strftime('%M')), 2);
					var secs = pad_number(parseInt(event.strftime('%S')), 2);
					if (days <= 1)
					{
						hours = pad_number((days * 24) + hours, 2);
						var timer = "<div class='col_xs_4 col_sm_2 col_md_4'>" + "<span class='deal_number'>" + hours.substr(0, 1) + "</span>" + "<span class='deal_number'>" + hours.substr(1, 1) + "</span>" + "</div>";
						timer += "<div class='col_xs_4 col_sm_2 col_md_4'>" + "<span class='deal_number'>" + mins.substr(0, 1) + "</span>" + "<span class='deal_number'>" + mins.substr(1, 1) + "</span>" + "</div>";
						timer += "<div class='col_xs_4 col_sm_2 col_md_4'>" + "<span class='deal_number'>" + secs.substr(0, 1) + "</span>" + "<span class='deal_number'>" + secs.substr(1, 1) + "</span>" + "</div>";
						timer += "<div class='col_xs_12 flex'>";
						timer += "<div class='col_xs_4 col_sm_2 col_md_4 text_xxs caps'>Hrs</div>";
						timer += "<div class='col_xs_4 col_sm_2 col_md_4 text_xxs caps'>Mins</div>";
						timer += "<div class='col_xs_4 col_sm_2 col_md_4 text_xxs caps'>Secs</div>";
						timer += "</div></div>";
						format = '%-d day%!d ' + format;
					}
					else
					{
						var timer = "<div class='col_xs_12'>" + "Deal ends in <span class='text_bold'>" + days + "d " + hours + "h " + mins + "m</span>";

						timer += "</div>";
						format = '%-d day%!d ' + format;
					}
					$(this).html(event.strftime(timer));
				});
			}
		</script>
