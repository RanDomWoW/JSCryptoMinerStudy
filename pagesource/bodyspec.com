}
$.fn.multiline = function(text) {
this.text(text);
this.html(this.html().replace('||','<br/>'));
return this;
}
$.fn.prevent_double_post = function() {
$(this).on('submit', function(e) {
var form = $(this);
var now_millis = new Date().getTime();
if(form.data('submitted') === true) {
if(now_millis - form.data('submitted_millis') < 800) {
e.preventDefault();
return;
}
}
form.data('submitted', true);
form.data('submitted_millis', now_millis);
});
return this;
};
function isMobileSafari() {
return navigator.userAgent.match(/(iPod|iPhone|iPad)/) && navigator.userAgent.match(/AppleWebKit/);
}
function isChrome() {
return navigator.userAgent.toLowerCase().match(/chrome/);
}
$(function() {
$('form').prevent_double_post();
if(isChrome()) {
$('body').hide().show();
}
});
if(!isMobileSafari() && $(window).width() >= 768) {
