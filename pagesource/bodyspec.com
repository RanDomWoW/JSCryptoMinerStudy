function showTransactionDetails(tid) {
var action = $("#" + tid + "-link").text().trim();
if (action === "+") {
$("." + tid + "-details").each(function() {
$(this).removeClass('hidden');
});
$("#" + tid + "-link").text("-");
return;
}
$("." + tid + "-details").each(function() {
