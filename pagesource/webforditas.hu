<script type='text/javascript'>
<!--
    var SPELL_LANGS = ':Hu:En:De:Pl:Fr:Es:Ro:Ar:Bg:Ca:Cs:Da:El:Et:Gl:He:Hr:Id:It:Lt:Lv:No:Nl:Pt:Ru:Sk:Sl:Sr:Sv:Tl:Ua:Vi:';

    var sa_input     = '';
    var sa_skip      = false;
    function SA_Init(input, type)
    {
      sa_skip = true;
      if (document.getElementById(input+"Speller") && document.getElementById(input+"Speller").parentNode.className == 'button_light') return false;

      sa_input = input;
      var words = escape(utf8_encode(document.getElementById(input).value));
      var lang  = GetSourceLang(input+'Lang');
      HideActiveTab();
      document.getElementById('spelldesc').style.visibility = 'hidden';
      document.getElementById('sa_buttons').style.display = 'none';
      document.getElementById('spellActionOutput').innerHTML = '<center><h1>ellenőrzés folyamatban...</h1></center>';
      document.getElementById('spellActionLayer').style.display='block';
      document.getElementById('spellActionFrame').src='index.php?show=spellAction&lang=hungarian&words='+words+'&slang='+lang+'&stype='+type;
      setTimeout(sa_show,1000);
      return true;
    }

    function SA_Toggle(input)
    {
      if (sa_skip) {sa_skip = false; return; }
      var obj = document.getElementById(input+'Speller');
      if (!obj) return;
      obj.className = (obj.className == 'checked') ? 'unchecked' : 'checked';

      var expiry = new Date(); expiry.setDate( (obj.className == 'unchecked') ? expiry.getDate()-1 : expiry.getDate()+expiry.getDate()+30);
      document.cookie="sa_auto_"+input+"_on;expires="+expiry.toGMTString();
    }

    var sa_auto_func = '';
    function SA_Auto(input, type, func, button)
    {
      var obj = document.getElementById(input+'Speller');
      if (!obj || (obj.className != 'checked')) return false;

      document.getElementById('sa_button_ok').parentNode.className = 'button_red';
      document.getElementById('sa_button_ok').innerHTML = button;

      sa_auto_func = func;
      return SA_Init(input, type);
    }

    function SA_Feedback()
    {
    }

    function SA_Exit(save)
    {
      HideTooltip('');
      if (save)
      {
        var input = '';
        for (l=0; l<spellActionFrame.sa_input.length-1; ++l)
        {
          if (l!=0) input += '\n';
          for (w=0; w<spellActionFrame.sa_input[l].length-1; ++w)
          {
            if (w!=0) input += ' ';
            input += spellActionFrame.sa_input[l][w];
          }
        }
        document.getElementById(sa_input).value = input;

        if (sa_auto_func.length > 0)
        { eval(sa_auto_func+'()'); return; }
      }
      sa_input = '';
      document.getElementById('spellActionLayer').style.display='none';
      ShowActiveTab();
    }

    function sa_show()
    {
      if (sa_input == '') return;
      if (!spellActionFrame.sa_loaded) { setTimeout(sa_show,300); return; }

      spellActionFrame.sa_loaded = false;

      if (spellActionFrame.sa_error != '')
      { alert(spellActionFrame.sa_error); SA_Exit(false); 
        if (sa_auto_func.length > 0) eval(sa_auto_func+'()'); }

      if ((sa_auto_func.length == 0) || (spellActionFrame.sa_misspelled > 0))
      {
        document.getElementById('spellActionOutput').innerHTML = spellActionFrame.sa_output;
        document.getElementById('sa_buttons').style.display = 'block';
        document.getElementById('spelldesc').style.visibility = 'visible';
      }

      //gemius
      gemius_hit('.ceaqK8q25aGS9Y3gbKghZZV.l1p2geZCoerWrxsPVb.e7');
      
      if ((sa_auto_func.length > 0) && (spellActionFrame.sa_misspelled == 0))
      { SA_Exit(true); }
    }

    var sa_ttobj = null;
    function sa_tooltip(obj, hyphen)
    {
      sa_ttobj   = obj;
      var tttext = "<"+"div class='spell_suggest'>";

      //Hyphenation
      if ((sa_tooltip.arguments.length > 2) && spellActionFrame.sa_changed && (hyphen != '')) hyphen = '-';
      if (hyphen != '')
          tttext += "<"+"div class='s_hyphen"+((sa_tooltip.arguments.length>2)?' s_sep':'')+"'>" + hyphen + "<"+"/div>";

      //Suggestion
      if (sa_tooltip.arguments.length > 2)
      {
        if (sa_tooltip.arguments.length < 5)
          tttext += "<"+"div>" + "Nincs ajánlás!" + "<"+"/div>";
        else for (var i=4; i<sa_tooltip.arguments.length; i++)
          tttext += "<"+"a href='#' onclick='sa_replace(this, "+sa_tooltip.arguments[2]+", "+sa_tooltip.arguments[3]+");return false;'>" + sa_tooltip.arguments[i] + "<"+"/a>";
        tttext += "<"+"div class='text'><"+"input type='text' style='width:"+(15*obj.innerHTML.length)+"px' onfocus='tt=null' onblur='document.getElementById(\"spellActionTooltip\").style.display=\"none\"' onkeypress='if(CheckEnter(event,false))sa_replace(this, "+sa_tooltip.arguments[2]+", "+sa_tooltip.arguments[3]+", true)'><"+"/div>";
      }

      tttext += "<"+"/table>";
      document.getElementById('spellActionTooltip').innerHTML = tttext;
      ShowTooltip(obj,'spellActionTooltip',true);
    }

    function sa_replace(source, lid, wid, custom)
    {
      spellActionFrame.sa_input[lid][wid] = source.value ? source.value : source.innerHTML;
      spellActionFrame.sa_changed = true;

      sa_ttobj.style.color = (typeof(custom) != 'undefined') ? "#606a74" : "green";
      sa_ttobj.innerHTML   = source.value ? source.value : source.innerHTML;

      HideTooltip('');
      document.getElementById('spellActionTooltip').style.display="none";
    }

    function sa_update(obj_name)
    {
      var o = document.getElementById(obj_name+"Speller");
      var t = document.getElementById(obj_name);
      if (!o || !t || (typeof(t.value)!='string')) return;
      var active = (SPELL_LANGS.indexOf(':'+GetSourceLang(obj_name+"Lang")+':') >= 0) && (t.value.length > 0)
      o.parentNode.className = active ? "button_small" : "button_light";
    }

//-->
</script>
