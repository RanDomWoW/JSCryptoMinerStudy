    function LGA_Init(input)
    {
        lga_skip = true;
        if (document.getElementById(input+"Guesser").parentNode.className == 'button_light') return false;

        var words = (document.getElementById(input).value);
        words     = escape(utf8_encode(words));
        lga_input = input;
        document.getElementById('lguessActionFrame').src='index.php?show=lguessAction&lang=hungarian&words='+words;
        setTimeout(lga_show,1000);
        return true;
    }

    function LGA_Toggle(input)
    {
      if (lga_skip) {lga_skip = false; return; }
      var obj = document.getElementById(input+'Guesser');
      if (!obj) return;
      obj.className = (obj.className == 'checked') ? 'unchecked' : 'checked';

      var expiry = new Date(); expiry.setDate( (obj.className == 'unchecked') ? expiry.getDate()-1 : expiry.getDate()+expiry.getDate()+30);
      document.cookie="lga_auto_"+input+"_on;expires="+expiry.toGMTString();
    }

    var lga_auto_func = '';
    function LGA_Auto(input, func)
    {
      var obj = document.getElementById(input+'Guesser');
      if (!obj || (obj.className != 'checked')) return false;

      lga_auto_func = func;
      return LGA_Init(input);
    }

    function lga_show()
    {
        if (!lguessActionFrame.lga_loaded) { setTimeout(lga_show,1000); return; }

        if (lguessActionFrame.lga_error != '')
        { alert(lguessActionFrame.lga_error); }
        else
        {
          var lang = document.getElementById(lga_input+'Lang').value.split(',');
          if (GetSourceLang(lga_input+'Lang') != lguessActionFrame.lga_lang)
          {
            if (lang.length == 3)
            {
                if (lguessActionFrame.lga_lang == 'Hu')
                  document.getElementById(lga_input+'Lang').value = 'Hu,En,f';
                else
                  document.getElementById(lga_input+'Lang').value = 'Hu,'+lguessActionFrame.lga_lang+',b';
                UpdateLanguage(lga_input);
            }
            else
              document.getElementById(lga_input+'Lang').value = lguessActionFrame.lga_lang;
          }
        }

        if (lga_auto_func.length > 0)
        { eval(lga_auto_func+'()'); }

        lguessActionFrame.lga_loaded = false;
    }

    function lga_update(obj_name)
    {
      var o = document.getElementById(obj_name+"Guesser");
