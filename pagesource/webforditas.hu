        var s = document.getElementById(obj_name+"Voice");
        if (s)  for (var n=0; n<l.length; ++n) if (l[n] == s.innerHTML) l = l[n];
        if (typeof(l) != 'string') l = l[0];
        l = ' [<span id="'+obj_name+'Voice" onclick="roll_speech(\''+obj_name+'Voice\',\''+lang+'\')">'+l+'</span>]';
      }
      else l = '';

      var t = document.getElementById(obj_name);

      var active = (l.length && ((typeof(t.value)!='string') || (t.value.length > 0)));

      o.innerHTML = 'felolvas' + l;
      o.parentNode.className = active ? "button_small" : "button_light";

      //o.onmouseover = function() {document.getElementById(obj_name+'Voice').style.color = "#FFFFFF";};
      //o.onmouseout  = function() {document.getElementById(obj_name+'Voice').style.color = "#DDDDDD";};

      document.getElementById("speechAudio").addEventListener('ended',stop_speech);
    }

    function roll_speech(obj_name, lang)
    {
      speech_skip = true;
      var o = document.getElementById(obj_name);
      var l = SPEECH_DISP;
      for (var n=0; n<l.length-1;++n) if (l[n] == o.innerHTML)
      { o.innerHTML = l[n+1]; return; }
      o.innerHTML = l[0];
    }

    function start_speech(obj_name, lang)
    {
      if (speech_obj.length) { stop_speech(); return; }
      if (speech_skip) { speech_skip = false; return; }

      // check input
      var iobj = document.getElementById(obj_name);
      var  str = (iobj.value) ? iobj.value : ( (iobj.innerText) ? iobj.innerText : iobj.textContent );
      if (str.length == 0) return;

      // check lang
      if (!SPEECH_LANGS[lang]) return;

      var vobj  = document.getElementById(obj_name+"Voice");
      var voice =  vobj ? vobj.innerHTML : '';
      var sobj  = document.getElementById("speechAudio");

      if (voice == SPEECH_DISP[1]) voice = SPEECH_LANGS[lang][1];
      else voice = SPEECH_LANGS[lang][0];

      str = escape(utf8_encode(str));

      sobj.pause();
      sobj.innerHTML = '<source src="index.php?show=speechAction&slang=' + lang + "&voice=" + escape(utf8_encode(voice)) + "&input=" + str + '">';
      sobj.load();
      sobj.play();
      speech_obj = obj_name;

      document.getElementById(speech_obj+'Speech').className = "stop";
      
      //gemius
      gemius_hit('dwCVfcc2JPVhWFHp1A7Vvpa2nDlpf8wn4.9Pee7xgG3.37');
          }

    function stop_speech()
    {
      if (!speech_obj.length) return;
      document.getElementById("speechAudio").pause();
      document.getElementById(speech_obj+'Speech').className = "play";
      speech_obj = '';
    }

    //~ function toggle_speech()
    //~ {
      //~ if (speech_isPlaying)
        //~ getFlashObject().SetVariable("method:pause", "");
      //~ else
        //~ getFlashObject().SetVariable("method:play", "");
    //~ }

    //~ function setPosition()
    //~ {
        //~ var position = document.getElementById("inputPosition").value;
        //~ getFlashObject().SetVariable("method:setPosition", position);
    //~ }

    //~ function setVolume()
    //~ {
        //~ var volume = document.getElementById("inputVolume").value;
        //~ getFlashObject().SetVariable("method:setVolume", volume);
    //~ }
//-->
</script>
