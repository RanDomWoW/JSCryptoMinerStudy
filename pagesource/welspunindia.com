                            $(".breadcrumbs").animate({"left":240, "opacity":1},1000);
                            $('#content').animate({"opacity":1},1500, function(){
                                $(".dataloader").hide();
                                AlignPageElements();

                                /*** Yogesh 20 Sept 2014 **/
                                $('a[href="#'+targetSubURL+'"], #'+targetSubURL+'').trigger('click');
                                $("a").each(function(){$(this).parent().not('.accordion-container li').removeClass("active");});
                                /*** Yogesh 20 Sept 2014 **/
                                thisnav.parent().parent().parent().addClass('active');
                                $(".breadcrumbs").animate({"left":117, "opacity":1},800);
                                $('.enqBox').css({"right": -(enqBoxWid+2)});
                            });
                        })
                    }});
                    if(pageurl!=window.location){
                        window.history.pushState({path:pageurl},'',pageurl);
                    }
                    return false;
                }

                else{
                    $.ajax({url:pageurl+'?rel=tab',success: function(data){
                        $(".scroll-down").fadeOut();
                        $('#content').animate({"opacity":0 },1000, function(){
                            $('#content').html(data);
                            $('#content').animate({"opacity":1},1000, function(){
                                $(".dataloader").hide();
                                if(pageurl==homeURL){
                                    $(".lt-news-btn").removeClass("active");
                                    $(".lt-news").stop().animate({ "bottom": -120 });
                                    $(".logo-sm-fx").slideUp(function () {
                                        $(".homescene").slideDown();
                                    });
                                    $(".page-sidebar").stop().animate({ left: -240, opacity: 0 }, 300);
                                    $(".logo-lg-fx").animate({ "top": -110 }, 300);
                                    $(".scroll-down").fadeIn();
                                    $("header .logo-lg-fx").fadeOut();
                                }
                                else{
                                    AlignPageElements();
                                    $("header .logo-lg-fx").fadeIn();
                                }
                                $("nav a").each(function(){$(this).parent().removeClass("active");});
                                thisnav.parent().addClass('active');
                                $("#content").removeClass("home");
                            });
                        })
                    }});
                    if(pageurl!=window.location){
                        window.history.pushState({path:pageurl},'',pageurl);
                    }
                    return false;
                }
            });
        }


        /* the below code is to override back button to get the ajax content without reload*/
        $(window).bind('popstate', function() {
            $.ajax({url:location.pathname+'?rel=tab',success: function(data){
                $('#content').html(data);
            }});
        });

        ActivateLink();
        function ActivateLink(){
            $(".page-url a").each(function(){if(this.href==document.URL){
                $(this).parent().addClass('active');
            }});
        }
    });

    /*** IE8***/
    function nav(href){
        location.href=href;
    };
</script>
