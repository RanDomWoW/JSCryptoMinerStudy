                    e.preventDefault();
                    var email = $("#authentication input[name='email']").val();
                    var password = $("#authentication input[name='password']").val();
                    if($("#authentication input[name='reminder']").is(':checked')){
                        var reminder = true;
                    } else {
                        var reminder = false;
                    }
                    $.post("https://www.pharma-gdd.com/fr/customer/login/ajax", {email : email, password : password, reminder : reminder}, function(data){
                        if(data == 'success'){
                            document.location.reload();
                        } else {
                            $("#authentication .errors").replaceWith(data);
                        }
                    });
                });
            });

            $(function(){
