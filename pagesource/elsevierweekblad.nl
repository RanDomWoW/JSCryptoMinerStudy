                    if( ( lastWindowWidth > 320 && windowWidth <= 320 ) || ( lastWindowWidth <= 320 && windowWidth > 320 )
                            || ( lastWindowWidth > 750 && windowWidth <= 750 ) || ( lastWindowWidth <= 750 && windowWidth > 750 )
                            || ( lastWindowWidth > 1020 && windowWidth <= 1020 ) || ( lastWindowWidth <= 1020 && windowWidth > 1020 )
                            || ( lastWindowWidth === undefined ) ) {
                        
                        googletag.cmd.push(function () {
                                                        googletag.pubads().refresh( [AdvertisementLeaderboard] );
                        });
                    }

                                        lastWindowWidth = windowWidth;
                }));
                                                        googletag.cmd.push(function () {
                        googletag.pubads().refresh([AdvertisementLeaderboard, AdvertisementOutOfPage, AdvertisementVideo, AdvertisementSidebarRectangle]);
                    });
                            }
            var listener = function() {
                if (!refreshedRect2 && isAlmostScrolledIntoView('#AdvertisementRectangle2')) {
                    googletag.cmd.push(function() {
                        googletag.pubads().refresh([AdvertisementSidebarRectangle2]);
                    });
                    // Refresh the ad only once.
                    refreshedRect2 = true;

                    // Remove the listener now.
                    if (refreshedVideo)
                        window.removeEventListener('scroll', listener);
                }
                if (!refreshedVideo && isAlmostScrolledIntoView('#AdvertisementVideo')) {
                    googletag.cmd.push(function() {
                        googletag.pubads().refresh([AdvertisementVideo]);
                    });
                    // Refresh the ad only once.
                    refreshedVideo = true;

                    // Remove the listener now.
                    if (refreshedRect2)
                        window.removeEventListener('scroll', listener);
                }
            }
            function isAlmostScrolledIntoView(elem)
            {
                var almostOffset = 50;
                var docViewTop = $(window).scrollTop();
                var docViewBottom = docViewTop + $(window).height();
                if ($(elem).length) {
                    var elemTop = $(elem).offset().top;
                    return (((elemTop - almostOffset) <= docViewBottom));
                }else {
                    refreshedVideo = true;
                    return false;
                }

            }
