<script type="text/javascript">

	$(document).ready(function () {

		var vidEl = $("#video").get(0);
		var vidWrap = $(".video-bg-wrapper");

		// No Video? No problem. Abort.
		if (typeof vidEl == 'undefined') return;

		if (!$('body').hasClass('ie')) {
			vidEl.play()
		} else if ($('body').hasClass('ie')) {
			$(vidWrap).remove();
		}
		setTimeout(function () {
			if (vidEl.paused) {
				$(vidWrap).remove();
			} else {
				$(vidEl).addClass('video--visible');
				$(vidWrap).addClass('video--visible');
			}
		}, 800);
	});
</script>



