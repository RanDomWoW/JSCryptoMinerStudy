        function newsleft() {
          var ll = $("#news").position().left;
          if (ll == 0)
          {
             $("#news").animate({left:0},1000);
             $("#fe2").fadeTo(1000,.1);
             $("#fe1").fadeTo(1000,1);
          }
          else if (ll == -820) 
          {
            $("#news").animate({left:820},1000);
             $("#fe1").fadeTo(1000,.1);
             $("#fe2").fadeTo(1000,1);
          }
        }
        function newsright() {
          var ll = $("#news").position().left;
          if (ll == 0)
          {
             $("#news").animate({left:0},1000);
             $("#fe2").fadeTo(1000,.1);
             $("#fe1").fadeTo(1000,1);
          }
          else if (ll == -820) 
          {
            $("#news").animate({left:820},1000);
             $("#fe1").fadeTo(1000,.1);
             $("#fe2").fadeTo(1000,1);
          }
        }
        function shtooltip(index) {
           $("#tooltip").stop(true,true);
           if (index == "codemelli")
           {
                $("#tooltip").animate({ top: -8 }, 0);
                $("#tooltip").animate({ right: -10 }, 0);
                $("#tooltiptxt").text("کد ملی 10 رقمی خود را بدون خط تیره وارد نمایید.");
           }
           if (index == "name")
           {
                $("#tooltip").animate({ top: 30 }, 0);
                $("#tooltip").animate({ right: 34 }, 0);
                $("#tooltiptxt").text("نام خود را به فارسی وارد نمایید.");
           }
           if (index == "captcha")
           {
                $("#tooltip").animate({ top: 220 }, 0);
                $("#tooltip").animate({ right: -20 }, 0);
                $("#tooltiptxt").text("متن تصویری که در سمت چپ قرار دارد را وارد نمایید.");
           }
           if (index == "family")
           {
                $("#tooltip").animate({ top: 30 }, 0);
                $("#tooltip").animate({ right: 194 }, 0);
                $("#tooltiptxt").text("نام خانوادگی خود را به فارسی وارد نمایید.");
           }
           if (index == "password")
           {
                $("#tooltip").animate({ top: 50 }, 0);
                $("#tooltip").animate({ right: -50 }, 0);
                $("#tooltiptxt").html("<div style='margin-bottom:8px'>رمز عبور می بایست حداقل 6 حرفی باشد.</div><div>رمز عبور می بایست از اعداد، علامتها و یا حروف لاتین تشکیل گردد.</div>");
           }
           if (index == "passrep")
