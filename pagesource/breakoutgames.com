<script>
  var $ = jQuery;

  $('body').on('click', '.btn-close', function(event) {
    event.preventDefault();

    document.getElementById("video-full-new").pause();

    $('.video-wrap').fadeOut();
  });

  $('body').on('click', '.load-video', function(event) {
    event.preventDefault();

    var supportsVideo = !!document.createElement('video').canPlayType,
        video         = document.getElementById("video-full-new"),
        videoControls = document.getElementById('video-controls'),
        playPause     = document.getElementById('play-pause'),
        fullscreen    = document.getElementById('fullscreen'),
        progress      = document.getElementById('progress'),
        progressBar   = document.getElementById('progress-bar'),
        current_width = $(window).width();

      $('.video-wrap').fadeIn();

      $('#play-pause').html('<i class="fa fa-pause"></i>');

      if (supportsVideo) {
        video.controls = false;
        videoControls.style.display = 'block';

        video.addEventListener('click', function(e) {
          if (video.paused || video.ended) {
            playPause.title = 'Play';
            playPause.innerHTML = '<i class="fa fa-pause"></i>';
            video.play();
          } else {
            playPause.title = 'Pause';
            playPause.innerHTML = '<i class="fa fa-play"></i>';
            video.pause();
          }
        });

        playPause.addEventListener('click', function(e) {
          if (video.paused || video.ended) {
            playPause.title = 'Play';
            playPause.innerHTML = '<i class="fa fa-pause"></i>';
            video.play();
          } else {
            playPause.title = 'Pause';
            playPause.innerHTML = '<i class="fa fa-play"></i>';
            video.pause();
          }
        });

        document.getElementById('video-full-new').addEventListener('ended', videoEnded, false);

        function videoEnded(e) {
          playPause.title = 'Play';
          playPause.innerHTML = '<i class="fa fa-play"></i>';
        }

        if (window.innerWidth >= 1600) {
          progress.style.width = (1600 * 0.90) + 'px';
        } else {
          progress.style.width = ($('.m-video').width() * 0.90) + 'px';
        }

        video.addEventListener('loadedmetadata', function() {
          progress.setAttribute('max', video.duration);
        });

        video.addEventListener('timeupdate', function() {
          progress.value = video.currentTime;
          progressBar.style.width = Math.floor((video.currentTime / video.duration) * 100) + '%';
        });

        video.addEventListener('timeupdate', function() {
          if (!progress.getAttribute('max')) progress.setAttribute('max', video.duration);
          progress.value = video.currentTime;
          progressBar.style.width = Math.floor((video.currentTime / video.duration) * 100) + '%';
        });

        progress.addEventListener('click', function(e) {
          var pos = (e.pageX  - this.offsetLeft) / this.offsetWidth;
          video.currentTime = pos * video.duration;
        });

      }

      document.getElementById('video-full-new').load();
      document.getElementById('video-full-new').play();

  });
</script>

