<script>
    var $ = jQuery;

    function getParameterByName(name, url) {
        if (!url) url = window.location.href;
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
        results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, " "));
    }

    function getRandomInt(min, max) {
        return Math.floor(Math.random() * (max - min + 1)) + min;
    }

    $(document).ready(function(){
        var passedVariation = getParameterByName('variation'),
            variation = passedVariation ? parseInt(passedVariation) : null;

        var heroId = variation ? variation : getRandomInt(0, $('.hero-slider-container').length - 1);

        $('.location-hero-slider').slick({
            dots: true,
            infinite: true,
            fade: true,
            cssEase: 'linear',
            arrows: false,
            initialSlide: heroId,
            autoplay: true,
            autoplaySpeed: 10000
        });

        $('a[href*="#"]')
          // Remove links that don't actually link to anything
          .not('[href="#"]')
          .not('[href="#0"]')
          .click(function(event) {
            // On-page links
            if (
              location.pathname.replace(/^\//, '') == this.pathname.replace(/^\//, '')
              &&
              location.hostname == this.hostname
            ) {
              // Figure out element to scroll to
              var target = $(this.hash);

              target = target.length ? target : $('[name=' + this.hash.slice(1) + ']');
              // Does a scroll target exist?
              if (target.length) {
                // Only prevent default if animation is actually gonna happen
                event.preventDefault();
                console.log(target.offset().top);
                $('html, body').animate({
                  scrollTop: target.offset().top - 200
                }, 1000, function() {
                  // Callback after animation
                  // Must change focus!
                  var $target = $(target);
                  $target.focus();
                  if ($target.is(":focus")) { // Checking if the target was focused
                    return false;
                  } else {
                    $target.attr('tabindex','-1'); // Adding tabindex for elements not focusable
                    $target.focus(); // Set focus again
                  };
                });
              }
            }
          });
    });

  $(function() {

    function switchHero(id) {
        var images = {
            0 : 'https://breakoutgames.com/wp-content/themes/fm-framework/images/hero-images-casino-bw.jpg',
            1 : 'https://breakoutgames.com/wp-content/themes/fm-framework/images/hero-images-casino-color.jpg',
            2 : 'https://breakoutgames.com/wp-content/themes/fm-framework/images/hero-images-kidnapping-bw.jpg',
            3 : 'https://breakoutgames.com/wp-content/themes/fm-framework/images/hero-images-kidnapping-color.jpg',
            4 : 'https://breakoutgames.com/wp-content/uploads/2017/10/location_header_background.jpg'
        };

        $('#hero-img').attr('src', images[id]);
    }

    var heroId,
        passedVariation = getParameterByName('slide'),
        chosenVariation = cxApi.chooseVariation(),
        variation = passedVariation ? parseInt(passedVariation) : null;

    if(passedVariation) {
        heroId = passedVariation;
    } else if (chosenVariation) {
        heroId = chosenVariation;
    } else {
        heroId = getRandomInt(0, 4);
    }

    switchHero(heroId);
  });
</script>
            
    
