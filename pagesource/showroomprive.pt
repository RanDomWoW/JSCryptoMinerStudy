            });
            FB.getLoginStatus(function (response) {
                FBStatut = response.status;
                FBToken = response.authResponse.accessToken;
            });
        });
    });

    function loginFB() {
        //getLoginStatus permet de vérifier si le client est connecté à Facebook
        if (FBStatut === 'connected') {
            srpLogin();
        }
        else {
            // login permet d'ouvrir l'iframe de connexion a Facebook si le client n'y est pas connecté.
            // gère également les permissions avec le paramètre scope
            FB.login(function (response) {
                if (response.status === 'connected') {
                    srpLogin();
                    FBToken = response.authResponse.accessToken;
                }
            }, { scope: 'public_profile,email,user_friends' });
        }
    }

    //fonction d'appel ajax pour vérifier si le client est inscrit sur SRP
    function srpLogin() {
        FB.api('/me?fields=email,first_name,last_name,gender', function (response) {
            if (response == undefined) {
                displayMessage("erreur", "Ocorreu um erro");
                return;
            }
            $.post('/Ajax/FbLogin.aspx', { tokenFB: FBToken }, function (data) {
                if (data == 'inscriptionFB') {
                    if ($("#Registration_tb_prenom") != null) {
                        $("#Registration_tb_prenom").val(response.first_name);
                        $("#Registration_tb_nom").val(response.last_name);
                        $("#Registration_tb_email").val(response.email);
                        var civilite = response.gender == "male" ? "3" : "1";
                        if (civilite == "1") {
                            $("#Registration_rbl_Civilite_0").prop("checked", true);
                        }
                        else {
                            $("#Registration_rbl_Civilite_1").prop("checked", true);
                        }
                        $("#Registration_tb_password").focus();
                    }
                }
                else {
                    document.location.href = data;
                }
            });
        });
    }
    </script>
    

