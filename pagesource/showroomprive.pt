<script type="text/javascript">
    var bgSeoContents = [{"bgText":"<h1>VENDAS EXCLUSIVAS<br />DAS MELHORES MARCAS COM <br/> PREÇOS ATE <span class=\"texte_rose\">-70%</span></h1>","bgPosition":"h1_position_top","bgUrl":"//static.cedex.it.showroomprive.com/v4/5/_css/img/bg/web/bg_default_587.jpg"}];

    /* Preloading images */
    var preLoadImgs = [];
    for (var i = 0; i < bgSeoContents.length; i++) {
        var img = new Image();
        img.src = bgSeoContents[i].bgUrl;
        preLoadImgs.push(bgSeoContents[i].bgUrl);
    }

    /* Initialize Background Stretch */
    var bgStretch = {};
    bgStretch.start = function (selectorBg, selectorHangs) {

        if (preLoadImgs.length > 0) {
            var t = $(selectorBg).backstretch(preLoadImgs, {
                fade: 750,
                duration: 3000
            });

            $(selectorBg).on("backstretch.before", function (e, component, nextIndex) {
                var divHangs = $(selectorHangs);
                var nextContent = bgSeoContents[nextIndex];
                divHangs.fadeOut(300)
                    .promise()
                    .done(function () {
                        divHangs.prop("class", nextContent.bgPosition);
                        divHangs.html(nextContent.bgText);
                        divHangs.fadeIn(450);
                    });
            });
        }
    };

    $(document).ready(function () {
        bgStretch.start(".incription_main_bloc", '#divHangs');
    });
</script>

                

<script type="text/javascript">

    function SubmitCaptcha() {
        WebForm_DoPostBackWithOptions(new WebForm_PostBackOptions("Registration$LienInscription", "", true, "inscription", "", false, true));
        $('#Registration_LienInscription').attr("onclick", 'javascript: WebForm_DoPostBackWithOptions(new WebForm_PostBackOptions("Registration$LienInscription", "", true, "inscription", "", false, true))');
    }

    function objErreur(obj, args) {
        args.IsValid = false;
        obj.addClass("error");
    };

    function validationInscription(sender, args) {

        $(".error").removeClass('error');

        var nbErreur = 0;
        var messageErreur = sender.errormessage = "ATENÇÃO: o(s) campo(s) assinalado(s) não são válidos. Por favor, preencha-os novamente.";

        //validation pays inscription
        obj = $('#' + sender.ddl_country);
        if (obj.val() == "-1") {
            nbErreur++;
            objErreur(obj, args);
        };

        // validation Civilite inscription
        var obj = $('#' + sender.rbl_Civilite);
        if (obj.children('input').is(':checked') === false) {
            nbErreur++;
            objErreur(obj, args);
            obj.children('label').addClass("error");
        };

        //validation Prenom inscription
        obj = $('#' + sender.tb_prenom);
        if (obj.val() === '') {
            nbErreur++;
            objErreur(obj, args);
        };

        //validation Nom inscription
        obj = $('#' + sender.tb_nom);
        if (obj.val() === '') {
            nbErreur++;
            objErreur(obj, args);
        };

        //validation Email inscription
        obj = $('#' + sender.tb_email);
        if (obj.val() === '') {
            nbErreur++;
            objErreur(obj, args);
        }
        else if (!obj.val().match("^[a-zA-Z0-9\\-._+]+@[a-zA-Z0-9]+([\\-.][a-zA-Z0-9]+)*(\\.[a-zA-Z]{2,4})$")) {
            nbErreur++;
            objErreur(obj, args);
            messageErreur = "O seu e-mail não é válido.";
        };

        //validation MDP inscription
        obj = $('#' + sender.tb_password);
        if (obj.val() === '') {
            nbErreur++;
            objErreur(obj, args);
        }
        else if (obj.val().length < 8 || obj.val().length > 16) {
            nbErreur++;
            objErreur(obj, args);
            messageErreur = "A sua palavra-passe deve ter entre 8 e 16 caracteres, incluindo pelo menos uma letra e um número.";
        }
        else if (obj.val().match("['=]")) {
            nbErreur++;
            objErreur(obj, args);
            messageErreur = "A sua palavra-passe não pode conter os caracteres especiais seguintes: ' e =";
        }
        else if (!obj.val().match(/(?=.*\d)(?=.*[a-zA-Z])/)) {
            nbErreur++;
            objErreur(obj, args);
            messageErreur = "A sua palavra-passe deve ter entre 8 e 16 caracteres, incluindo pelo menos uma letra e um número.";
        }

        // validation Parrain inscription
        obj = $('#' + sender.tb_parrain);
        var obj2 = $('#' + sender.tb_email);

        if ($('#' + sender.cb_parr).is(':checked')) {
        if (obj.val() === '') {
            nbErreur++;
            objErreur(obj, args);
            objErreur($('#' + sender.cb_parr), args);
        }
        else if (obj.val() === obj2.val()) {
            nbErreur++;
            objErreur(obj, args);
            objErreur($('#' + sender.cb_parr), args);
            messageErreur = "O seu padrinho não está inscrito.";
        }
        else if (obj.val().length != 8) {
            nbErreur++;
            objErreur(obj, args);
            objErreur($('#' + sender.cb_parr), args);
            messageErreur = "Código apadrinhamento inválido.";
            };
        };

        // validation CGV
        obj = $('#' + sender.cb_CGV);
        if (!obj.is(':checked')) {
            nbErreur++;
            objErreur(obj.parent().children('label'), args);
            messageErreur = "Por favor, aceite as condições gerais de venda.";
        };

        if (nbErreur === 1) {
            sender.errormessage = messageErreur;
        };
    };
</script>
