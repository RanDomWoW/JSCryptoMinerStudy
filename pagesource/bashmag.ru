});

jQuery('.sociallogin.module-143').on('click', '.cancel', function()
{
	jQuery(".sociallogin.module-143 .enter-login").show();
	jQuery(".sociallogin.module-143 .enter-email").hide();
	jQuery(".sociallogin.module-143 .create").html("Создать");
	
	return false;
});

jQuery('.sociallogin.module-143 .page-footer').on('click', '.cancel', function()
{
	jQuery(".sociallogin.module-143 input[name='task']").val('cancel');
	jQuery(".sociallogin.module-143 form").submit();
	
	return false;
});

jQuery(".sociallogin.module-143 input").keydown(function(e)
{
	if (e.keyCode == 13) {
		jQuery(this).blur();
		
		if (jQuery(this).val() == "") {
			jQuery(this).addClass('empty');
		} else {
			if (jQuery(this).attr('name') != "email") {
				if (jQuery(".sociallogin.module-143 .merge").length) jQuery(".sociallogin.module-143 .merge").trigger('click');
				else if (jQuery(".sociallogin.module-143 .login").length) jQuery(".sociallogin.module-143 .login").trigger('click');
			} else {
				jQuery(".sociallogin.module-143 .create").trigger('click');
			}
		}
	}
});

jQuery('.sociallogin.module-143').on('focus', "input[type='text'], input[type='password']", function()
{
	jQuery(this).removeClass('empty');
});

jQuery(".sociallogin.module-143 .social-messages .message-close").click(function()
{
	jQuery('.sociallogin.module-143 .social-messages').fadeOut('fast');
});

