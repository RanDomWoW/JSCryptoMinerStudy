<script type="text/javascript">	
			
jQuery('.sociallogin.module-145').on('click', '.create, .merge, .login, .reset', function()
{	
	if (jQuery(this).hasClass('create')) {
		task = 'create';
	
		if (jQuery(".sociallogin.module-145 .enter-email").length) {
			if (jQuery(".sociallogin.module-145 .enter-login").is(":visible")) {
				jQuery(".sociallogin.module-145 .enter-login").hide();
				jQuery(".sociallogin.module-145 .enter-email").show();
				jQuery(".sociallogin.module-145 .create").html("Принять");
				return false;
			} else {
				if (jQuery(".sociallogin.module-145 input[name='email']").val() == '') {
					jQuery(".sociallogin.module-145 input[name='email']").addClass('empty');
					return false;
				}
			}
		}
	} else if (jQuery(this).hasClass('merge')) {
		task = 'merge';
		
		if (jQuery(".sociallogin.module-145 .enter-email").length && jQuery(".sociallogin.module-145 .enter-email").is(":visible")) {
			jQuery(".sociallogin.module-145 .enter-login").show();
			jQuery(".sociallogin.module-145 .enter-email").hide();
			jQuery(".sociallogin.module-145 .create").html("Создать");
		}
	} else if (jQuery(this).hasClass('login')) {
		task = 'login';
	} else if (jQuery(this).hasClass('reset')) {
		task = 'reset';
		
		if (jQuery(".sociallogin.module-145 input[name='email']").val() == '') {
			jQuery(".sociallogin.module-145 input[name='email']").addClass('empty');
			return false;
		}		
	}
	
	if ((task != 'create') && jQuery(".sociallogin.module-145 input[name='username']").val() == '') {
		jQuery(".sociallogin.module-145 input[name='username']").addClass('empty');
	} else if ((task != 'create') && jQuery(".sociallogin.module-145 input[name='password']").val() == '') {
		jQuery(".sociallogin.module-145 input[name='password']").addClass('empty');
	} else {
		jQuery(".sociallogin.module-145 input[name='task']").val('check');
		
		jQuery.ajax({
			url: "https://www.bashmag.ru/index.php?option=com_sociallogin&task=check&type=" + task,
			type: "post",
			dataType: "json",
			data: jQuery(".sociallogin.module-145 input"),
			beforeSend: function() {
				jQuery(".sociallogin .social-messages").hide();
				jQuery(".sociallogin .social-messages").removeClass('error warning success');
				jQuery(".sociallogin input[type='text'], .sociallogin input[type='password']").addClass('loading').prop('disabled', true);
			},					
			success: function(json) {
				if (json['error']) {
					jQuery(".sociallogin.module-145 .social-messages .message-text").text(json['error']);
					jQuery(".sociallogin.module-145 .social-messages").addClass('error').detach().insertBefore(jQuery(".sociallogin.module-145 .form-fields")).fadeIn('fast');
					jQuery(".sociallogin input[type='text'], .sociallogin input[type='password']").removeClass('loading').prop('disabled', false);
				} else {
					jQuery(".sociallogin input[type='text'], .sociallogin input[type='password']").prop('disabled', false);
					jQuery(".sociallogin.module-145 input[name='task']").val(task);
					jQuery(".sociallogin.module-145 form").submit();
				}					
			},
			error: function(xhr, ajaxOptions, thrownError) {
				//alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
			}
		});
	}
	
	return false;
});

jQuery('.sociallogin.module-145').on('click', '.edit', function()
{
	window.location.href = "/component/users/profile?layout=edit&Itemid=101";
	
	return false;
});

jQuery('.sociallogin.module-145').on('click', '.logout', function()
{
	jQuery(".sociallogin.module-145 input[name='task']").val('logout');
	jQuery(".sociallogin.module-145 form").submit();
	
	return false;
});

jQuery('.sociallogin.module-145').on('click', '.cancel', function()
{
	jQuery(".sociallogin.module-145 .enter-login").show();
	jQuery(".sociallogin.module-145 .enter-email").hide();
	jQuery(".sociallogin.module-145 .create").html("Создать");
	
	return false;
});

jQuery('.sociallogin.module-145 .page-footer').on('click', '.cancel', function()
{
	jQuery(".sociallogin.module-145 input[name='task']").val('cancel');
	jQuery(".sociallogin.module-145 form").submit();
	
	return false;
});

jQuery(".sociallogin.module-145 input").keydown(function(e)
{
	if (e.keyCode == 13) {
		jQuery(this).blur();
		
		if (jQuery(this).val() == "") {
			jQuery(this).addClass('empty');
		} else {
			if (jQuery(this).attr('name') != "email") {
				if (jQuery(".sociallogin.module-145 .merge").length) jQuery(".sociallogin.module-145 .merge").trigger('click');
				else if (jQuery(".sociallogin.module-145 .login").length) jQuery(".sociallogin.module-145 .login").trigger('click');
			} else {
				jQuery(".sociallogin.module-145 .create").trigger('click');
			}
		}
	}
});

jQuery('.sociallogin.module-145').on('focus', "input[type='text'], input[type='password']", function()
{
	jQuery(this).removeClass('empty');
});

jQuery(".sociallogin.module-145 .social-messages .message-close").click(function()
{
	jQuery('.sociallogin.module-145 .social-messages').fadeOut('fast');
});

