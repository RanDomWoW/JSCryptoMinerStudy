<script type="text/javascript">
$(function() {
	var film_roll = new FilmRoll({
		container: '#slider-buttons',
		configure_load: 50,
		animation: 250,
		interval: 6000,
		pager: false,
		prev: "#slider-control-prev",
		next: "#slider-control-next",
		no_css: true
	});
	$('#slider-buttons').on('film_roll:activate', function(event) {
		$('#slider-banners div:not(:eq(' + film_roll.index + '))').fadeOut();
		$('#slider-banners div:eq(' + film_roll.index + ')').fadeIn(1000);
	});
	$('#slider-buttons .film_roll_child').click(function() {
		if(! $(this).hasClass('.active')) {
			film_roll.moveToChild($(this));
			return false;
		}
	});
});
