    <script data-cfasync="false" src="/cdn-cgi/scripts/d07b1474/cloudflare-static/email-decode.min.js"></script><script type="text/javascript">
        /*
         * set a random wallpaper (from the guildwars website)
         *  there's a semi-transparent div over it with default 0.8 opacity,
         *  the 2nd value for each entry can be an altered opacity
         */
        $(window).load(function() {
            // IE8 and older won't support RGBA so the background makes a mess ...
            if ($.browser.msie && $.browser.version <= 8) {
                return;
            }

            var images = [
                // some color wallpapers
                ["https://d3b4yo2b5lbfy.cloudfront.net/wp-content/uploads/wallpapers/HumanBonusWP02-1920x1200.jpg", null],
                ["https://d3b4yo2b5lbfy.cloudfront.net/wp-content/uploads/wallpapers/GW2_RangerWallpaper02-1920x1200.jpg", null],
                ["https://d3b4yo2b5lbfy.cloudfront.net/wp-content/uploads/wallpapers/GW2_HumanNecromancer-1920x1200.jpg", null],
                ["https://d3b4yo2b5lbfy.cloudfront.net/wp-content/uploads/wallpapers/GuildWars2-13-1920x1200.jpg", null],
                ["https://d3b4yo2b5lbfy.cloudfront.net/wp-content/uploads/wallpapers/GuildWars2-07-1920x1200.jpg", null],
                ["https://d3b4yo2b5lbfy.cloudfront.net/wp-content/uploads/wallpapers/GuildWars2-05-1920x1200.jpg", null],
                ["https://d3b4yo2b5lbfy.cloudfront.net/wp-content/uploads/wallpapers/GuildWars2-11-1920x1200.jpg", null]
            ];


            var random = Math.floor(Math.random() * images.length);

            image   = images[random][0];
            opacity = images[random][1] || 0.2;

            $(document.body).css('background-image', "url(" + image + ")");
            $('.container-fluid').each(function(k, o) { o.style.backgroundColor = "rgba(255, 255, 255, " + opacity + ")"; });
        });

        $(document).ready(function() {
            $('.ingame-share').each(function(k, ingameshare) {
                var $span  = $(ingameshare).find('span');
                var $input = $(ingameshare).find('input');

                $span.on('click', function() {
                    $span.css('visibility', 'hidden');
                    $input.css('display', 'inline-block').focus().select();
                });

                $input.on('blur', function() {
                    $span.css('visibility', 'visible');
                    $input.css('display', 'none');
                });
            });
        });
    </script>

