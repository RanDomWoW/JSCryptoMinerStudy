                        <input maxlength="5" style="width: 50px; height: 18px; border: 1px solid #ccc" type="text"
                            id="width" name="width" value="10" />
                        *
                        <input maxlength="5" style="width: 50px; height: 18px; border: 1px solid #ccc" type="text"
                            id="height" name="height" value="10" />
