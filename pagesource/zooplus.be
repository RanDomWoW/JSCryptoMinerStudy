<script id="productreco-template-variants" type="text/x-handlebars-template">
    <ul>
        {{#each items}}
        <li class="variant-box">
            <a href="{{../languagePrefix}}{{detailURL}}" name="{{../scheme}}-variant-{{asid}}"
               title="{{name}}">
                <header>
                    <img src="{{pictureURL picturePath "65"}}" alt="{{name}}">
                </header>
                <div class="body">
                    <h5>{{shortName}}</h5>
                    <p>{{variantName}}</p>
                </div>
            </a>
            <footer class="reco">
                <a href="{{../languagePrefix}}{{detailURL}}" name="{{../scheme}}-variant-{{asid}}-price"
                   title="{{name}}">
                    <span itemprop="price" class="smalltextPrices">{{standardPriceText}}</span>
                    <br/>

                    <span class="{{styleSalePrice}}">
                        <span itemprop="price">{{reducedPriceText}}</span>
                        <br/>
                        <span class="smalltext smalltextPrices">{{pricePerUnitPriceText}}</span>
                    </span>
                </a>
                <form action="/checkout/lastMinuteOffer.htm" name="add{{asid}}" method="POST" novalidate="">
                    <input type="hidden" value="{{asid}}" name="articleId">
                    <button type="submit" class="addToCart" name="{{../scheme}}-buy_button-{{asid}}">
						<span>
							<b>{{../addToCart}}</b>
							<img alt="" src="{{mediaServerDomainUrl}}/image/icons/icon_cart.gif">
						</span>
                    </button>
                </form>
            </footer>
        </li>
        {{/each}}
    </ul>
</script>


