<script>
        OAM.later(function () {
            (function (global, $, ajaxRequest) {
                "use strict";
                var url = "//" + location.host + location.pathname + "/featured-listings",
                    callback = function (error, response) {
                        if (!response || typeof response.data === 'undefined' || !response.data.length) {
                            return;
                        }
                        var $cachedDom = $('.similar-listings'),
                            $container = $('<section/>'),
                            simAds = function () {
                                var $ul = $('<ul/>', {"class": "similar-listings__ul"}),
                                        $li = [];

                                $(response.data).each(function (index) {
                                    var $divFirst = $('<div/>', {"class": "first", "style": "background-image:url(" + response.data[index].image + ")"}),
                                            $checkboxContainer = $('<div/>', {"class": "similar-listings__checkbox-container"}),
                                            $checkbox = $('<input/>', {
                                                "name": "listingId[]",
                                                "value": response.data[index].id,
                                                "type": "checkbox",
                                                "class": "similar-listings__checkbox",
                                                "checked": true
                                            }),
                                            $imgAnchor = $('<a/>', {href: response.data[index].url }),
                                            $title = $('<div/>', {text: response.data[index].title, "class": "similar-listings__title"}),
                                            $location = $('<div/>', {html: response.data[index].location, "class": "similar-listings__location"}),
                                            $price = $('<div/>', {html: response.data[index].price, "class": "similar-listings__price"}),
                                            $divSecond = $('<div/>', {"class": "second"});

                                    $li[index] = $('<li/>', {"data-href": response.data[index].url })
                                        .append($divFirst
                                                .append($checkboxContainer.append($checkbox))
                                                .append($imgAnchor), [
                                            $divSecond
                                                .append($title).append($location).append($price),
                                        ]);
                                });

                                return $ul.append($li);
                            };

                        var $carousel = simAds();
                        $cachedDom.append($container.append($carousel));
                        OAM.script(OAM.scripts.slick).wait(function () {
                            cheki.carousel($carousel, 'featuredCars');
                            //hide placeholders on success call
                            document.getElementById("featured-listings-placeholder").style.display = "none";
                        });

                        $cachedDom.find('li').on('click', function (e) {
                            var href = $(this).data('href');
                            if (!$(e.target).is('a') && !$(e.target).is(':input')) {
                                e.preventDefault();
                                global.location.href = href;
                            }
                        });
                    };

                ajaxRequest(url, null, callback, "GET");
            })(window, jQuery, window.ajaxRequest);
        });
    </script>
