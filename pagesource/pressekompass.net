	<script>
		$(function() {
			var visible_items_var = 3;
			var scrolling_items_var = 2;

			var test = 'moinsen';


			$( '#fakten-button' ).on('click', function() {
				var $this = $(this);
				if($this.hasClass("fakten-button-open")){
					$( '.Fakten-Content' ).slideUp('slow', function(){
						$this.removeClass("fakten-button-open");
					});
				}else{
					$this.addClass("fakten-button-open");
					$( '.Fakten-Content' ).slideDown('slow');
				}
  				return false;
  			});
  			if($(window).innerWidth() < 767) {
				visible_items_var = 1;
				scrolling_items_var = 1;
			} else if($(window).innerWidth() < 960) {
				visible_items_var = 2;
				scrolling_items_var = 1;
			} else {
				visible_items_var = 3;
				scrolling_items_var = 2;
			}
			$("#demo1").als({
				visible_items: visible_items_var,
				scrolling_items: scrolling_items_var,
				orientation: "horizontal",
				circular: "no",
				autoscroll: "no"
			});
			resizeCompass();
			$(window).on('resize', function() { resizeCompass(); resizeAlsEntries(); });
		});

		function resizeAlsEntries() {
			if($(window).innerWidth() < 767) {
				visible_items_var = 1;
				scrolling_items_var = 1;
			} else if($(window).innerWidth() < 960) {
				visible_items_var = 2;
				scrolling_items_var = 1;
			} else {
				visible_items_var = 3;
				scrolling_items_var = 2;
			}
			$('#demo1').als({visible_items: visible_items_var, scrolling_items: scrolling_items_var});
		}

		function resizeCompass() {
			compassUrl = 'http://compass.pressekompass.net/compasses/production/' + compassSlug;
			if($(window).innerWidth() >= 1240) {
				compassUrl +=  '?style=desktop';
			} else {
				$('.main_kompass iframe').attr('width', '100%');
			}
			$('.main_kompass iframe').attr('src', compassUrl);
		}
	</script>

