<script>
function createXMLHttpRequest(){var ua;if(window.XMLHttpRequest){try{ua=new XMLHttpRequest();}catch(e){ua = false;}} else if(window.ActiveXObject){try {ua = new ActiveXObject("Microsoft.XMLHTTP");}catch(e){ua = false;}} return ua;}

var suggestObj,timerSuggest,currentSuggest=0;

function keySuggest(obj,e){
	if (window.event && e){e = window.event;}
	if(e){
		if(e.keyCode==13||e.keyCode==9){document.getElementById('divSuggest').style.display='none';currentSuggest=0;return true;}
		if(e.keyCode==38){highlightSuggest(1);return false;}
		if(e.keyCode==40){highlightSuggest(2);return false;}
	}
	suggestObj=obj;setTimeout('loadSuggest()',1);
}

function loadSuggest(a){
	if(!suggestObj.value){document.getElementById('divSuggest').style.display='none';currentSuggest=0;return true;}
	var v= createXMLHttpRequest();
	v.open('get', '/search.ajax.php?&q='+escape(suggestObj.value));
	v.onreadystatechange = function(){if(this.readyState==4){if(this.responseText){processSuggest(this.responseText)}else{document.getElementById('divSuggest').style.display='none';currentSuggest=0;}}};
	v.send(null);
	return true;
}

function processSuggest(r){
	clearInterval(timerSuggest); currentSuggest=0;
	document.getElementById('divSuggest').innerHTML=r;
	document.getElementById('divSuggest').style.display='';
	alignSuggest();
}

function selectSuggest(v){suggestObj.value=v;document.getElementById('divSuggest').style.display='none';currentSuggest=0;}
function hideSuggest(){timerSuggest=setTimeout("document.getElementById('divSuggest').style.display='none';currentSuggest=0;",20);}

function highlightSuggest(v){
	if(document.getElementById('divSuggest').style.display=='none'){return true}
	if(currentSuggest){document.getElementById('l'+currentSuggest).style.background='';}
	if(v==1){currentSuggest--;}
	if(v==2){currentSuggest++;}
	if(!document.getElementById('l'+currentSuggest)){currentSuggest=1;}
	document.getElementById('l'+currentSuggest).style.background='#f2f2f2';
	suggestObj.value=document.getElementById('l'+currentSuggest).getAttribute('q');
}
</script>
