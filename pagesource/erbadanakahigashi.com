            onEnterMobile: function(){ $('.menu-list').addClass('is-mobile');},
            onLeaveMobile: function(){ $('.menu-list').removeClass('is-mobile');}
        }).find('a').on('click', function(){
            $('.menu-list.is-mobile').trigger('close');
        });

        $('.main-gallery').fadeIn(500);
        $('.main-gallery').flickity({
            wrapAround: true,
            autoPlay: 6000
        });

        var isMouseMove = false;
        $( '.main-gallery' ).on( 'mousedown', function( event ) {
            isMouseMove = false;
        });
        $( '.main-gallery' ).on( 'mousemove', function( event ) {
            isMouseMove = true;
        });

        $( '.main-gallery a' ).on( 'click', function( event ) {
            event.preventDefault();
            if(!isMouseMove){
                $.fancybox.open($(this).attr('href'));
            }
        });

    });
