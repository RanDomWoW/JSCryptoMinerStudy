            data: {messageId: messageId},
            context: this,
            beforeSend: function(xhr, opts) {},
            error: function () {console.log("error");},
            success: function (data) {
              location.href = "/t5/kudos/messagepage/board-id/stories/message-id/"+data.message+"/tab/all-users";
            },
            complete: function() {}
          });
        });
          $('#lia-body .lia-content').on('click', '.kudo-display-button', function(e) {
            window.location.href = 'https://www.southwestaircommunity.com/t5/user/userloginpage?dest_url=https%3A%2F%2Fwww.southwestaircommunity.com%2F';
          });
      });
      function addKudos(messageId) {
        updateKudos('add', messageId);
      }

      function removeKudos(messageId) {
        updateKudos('remove', messageId);
      }
      function updateKudos(action, messageId) {
        $.ajax({
          type: 'post',
          url: '/plugins/custom/southwest/southwest/custom-update-message-kudos?tid=4326649926317065884',
          dataType: 'html',
          data: {messageId: messageId, action: action},
          context: this,
          beforeSend: function(xhr, opts) {},
          error: function () {console.log("error");},
          success: function (data) {},
          complete: function() {}
        });
      }
    })(LITHIUM.jQuery);

(function ($) {
        $(document).ready(function () {
                if ($('.custom-tabbed-activity-wrapper .custom-tabbed-content').length) {
                        var thisTabWrapper = $('.custom-tabbed-activity-wrapper .custom-tabbed-content');
 
                        $.each(thisTabWrapper, function () {
                                var thisTabWrapper = $(this);
 
                                var delTabIndexes = [];
                                $('ul.custom-tabbed-pane .custom-tab-content', thisTabWrapper).each(function (i, o) {
                                        if (o.children.length === 0)
                                                delTabIndexes.push(i);
                                });
 
                                delTabIndexes.sort();
                                delTabIndexes.reverse();
 
                                $.each(delTabIndexes, function (i, delIdx) {
                                        $('.custom-tabbed-nav li:nth-child(' + (delIdx + 1) + ')', thisTabWrapper).remove();
                                        $('.custom-tabbed-pane li:nth-child(' + (delIdx + 1) + ')', thisTabWrapper).remove();
                                });
 
                                if ($('ul.custom-tabbed-pane .custom-tab-content').length == 0) {
                                        $("#lia-body .custom-component-latest-tabbed").remove();
                                } else {
                                        var maxContentHeight = 0;
                                        $("#lia-body .custom-tab-content .lia-panel-content").each(function () {
                                                maxContentHeight = $(this).height() > maxContentHeight ? $(this).height() : maxContentHeight;
                                        })
 
                                        if (maxContentHeight > 0) {
                                                $("#lia-body .custom-tabbed-activity-wrapper .custom-tabbed-content ul.custom-tabbed-pane li div.custom-tab-content").height((maxContentHeight + 20));
                                        }
 
                                        $('.custom-tabbed-nav li', thisTabWrapper).mouseover(function () {
                                                $(this).addClass('hover');
                                        }).mouseout(function () {
                                                $(this).removeClass('hover');
                                        });
 
                                        $('.custom-tabbed-nav li > a', thisTabWrapper).click(function (e) {
                                                e.preventDefault();
 
                                                var thisPanelSelect = $('.custom-tabbed-nav li a', thisTabWrapper).index($(this));
                                                $('.custom-tabbed-pane > li', thisTabWrapper).hide();
                                                $('.custom-tabbed-pane > li', thisTabWrapper).eq(thisPanelSelect).show();
 
                                                $('.custom-tabbed-nav li', thisTabWrapper).removeClass('selected');
                                                $(this).parent('li').addClass('selected');
                                        });
                                        $('.custom-tabbed-nav li > a', thisTabWrapper).eq(0).click();
                                }
 
                        });
                }
        });
})(LITHIUM.jQuery);

