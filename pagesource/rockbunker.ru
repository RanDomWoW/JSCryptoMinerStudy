<script type="text/javascript">
	function randomInteger(min, max) {
		var rand = min + Math.random() * (max + 1 - min);
		rand = Math.floor(rand);
		return rand;
	}

	$(document).ready(function(){
		var whatsnews = $(".whatsnew_item");
		var bestseller = $(".bestseller_item");
		var whatsnewscount = whatsnews.length;
		var bestsellercount = bestseller.length;
		whatsnews.hide();
		bestseller.hide();
		whatsnews.eq(0).show();
		bestseller.eq(0).show();
		setInterval (function(){
			whatsnews.hide();
			bestseller.hide();
			whatsnews.eq(randomInteger(0, whatsnewscount-1)).show();
			bestseller.eq(randomInteger(0, bestsellercount-1)).show();
		}, 3000);
	});
</script>

