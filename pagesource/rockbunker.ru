<script type="text/javascript">
	$(document).ready(function(){
		var addtocart = $("#add_to_cart");
		var addtocart_list = $(".button_zakaz a.flybutton");

		addtocart_list.click(function(){
			fly_to_cart( $(this).parents(".rem_tov").find("img.flyimg") );
		})

		addtocart.click(function(){
			fly_to_cart( $(".flyimg") );
		})

		function fly_to_cart(prodimag){
			var cartbox = $("#divShoppingCart");
			var prodimag_idle = true;
			if(prodimag.length == 0){return false;}
			if(!prodimag_idle){return false;}
			prodimag_idle = false;
			var p = prodimag.offset();
			$("body").append('<img src="'+prodimag.attr("src")+'" style="left:'+p.left+"px;top:"+p.top+'px;position:absolute;z-index:1000;" id="flyimgcart" />');
			p = cartbox.find(".carts").offset();
			p.left += 94;
			p.top += Math.round(cartbox.find(".carts").outerHeight()/2);
			$("body > #flyimgcart:first").animate({width:"0px",opacity:"0",left:p.left+"px",top:p.top+"px"},1000,function(){
				$(this).remove();
				prodimag_idle=true;
			})
		}

	})
</script>

