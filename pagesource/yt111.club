		<script>
		$(function(){ lineAdRolling(); });
		$(document).on({"mouseenter":function(){ $(this).prop("stop", true); }, "mouseleave":function(){ $(this).prop("stop", false); }}, "#lineAd");
		function lineAdRolling(rolling)
		{
			var animateTime = 500;
			var rollingTime = 4000;
			var wrap = $("#lineAd");
			var height = $(wrap).height();
			var items = $(wrap).find("li").show();
			var active = $(wrap).find("li.active").index(); if(active<0) active = 0;
			if(items.length<=1) return;
			if(!$(wrap).prop("stop"))
			{
				$(items).removeClass("active");
				if(!rolling)
				{
					$(items).css({"top":height+"px"});
					$(items).eq(active).css({"top":"0px"}).addClass("active");
					setTimeout(function(){ lineAdRolling(true); }, rollingTime);
					return;
				}
				var next = active+1;
				if(next < 0) next = $(wrap).find("li:last").index();
				if(next >= items.length) next = $(wrap).find("li:first").index();
				$(items).eq(active).animate({"top":-height+"px"}, animateTime, function(){ $(items).not(":eq("+next+")").css({"top":height+"px"}); });
				$(items).eq(next).animate({"top":"0px"}, animateTime).addClass("active");
				$(items).eq(next).fadeOut(100).fadeIn(200).fadeOut(100).fadeIn(200);
			}
			setTimeout(function(){ lineAdRolling(true); }, rollingTime);
		}
		</script>

