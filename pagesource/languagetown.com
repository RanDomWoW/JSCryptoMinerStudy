<script>
	var mlastnum = 3; //이벤트 배너 갯수

	function movie_event(num) {
		if (timerm1) window.clearTimeout(timerm1);

		for(i=1;i<=mlastnum;i++) {
			document.getElementById("movieBanner"+i).style.display = "none";
		}

		if(num > mlastnum) {
			document.getElementById("movieBanner1").style.display = "block";
		} else {
			document.getElementById("movieBanner"+num).style.display = "block";
		}
	}

	function movie_event2(num) {
		if (timerm1) window.clearTimeout(timerm1);

		for(i=1;i<=mlastnum;i++) {
			document.getElementById("movieBanner"+i).style.display = "none";
		}

		if(num > mlastnum) {
			document.getElementById("movieBanner1").style.display = "block";
			timerm1 = setTimeout("movie_event2(2)", 3000);
		} else {
			document.getElementById("movieBanner"+num).style.display = "block";
			timerm1 = setTimeout("movie_event2("+eval(num+1)+")", 3000);
		}
	}

	timerm1 = setTimeout("movie_event2(2)", 3000);

	function movie_out(num) {
		if (timerm1) window.clearTimeout(timerm1);
		timerm1 = setTimeout("movie_event2("+num+")", 100);
	}

	function movieBan(num) {
		movieBanner1.style.display = "none";
		movieBanner2.style.display = "none";
		movieBanner3.style.display = "none";

		document.getElementById("movieBanner"+num).style.display = "block";
	}

	function movieGo(num) {
		if(num==1){
			location.href="http://movie.languagetown.com/movielang_list2.html?pcode=P20081223194948";
		}
		if(num==2){
			location.href="http://movie.languagetown.com/movielang_list2.html?pcode=P20100616151036";
		}
		if(num==3){
			location.href="http://movie.languagetown.com/movielang_list2.html?pcode=P20130610104814";
		}
	}
</script>
