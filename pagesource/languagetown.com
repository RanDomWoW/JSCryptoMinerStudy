<!-- 로그인/배너 시작 -->
<script>
	var lastnum = 4; //이벤트 배너 갯수
	var currnum = 1;
	var layerbg1 = "#F5F5F5";
	var layerbg2 = "#fffcdb";
	var layerbg3 = "#ffface";
	var layerbg4 = "#FFFEF1";

	function main_event(num) {
		if(!num){
			num = currnum;
		}
		if (timer1) window.clearTimeout(timer1);

		for(i=1;i<=lastnum;i++) {
			document.getElementById("main_event"+i).style.display = "none";
			document.getElementById("dot"+i).src = "./images16/dot_gray.png";
		}

		if(num > lastnum) {
			document.getElementById("main_event1").style.display = "block";
			document.getElementById("dot1").src = "./images16/dot_red.png";
			currnum = 1;
		} else {
			document.getElementById("main_event"+num).style.display = "block";
			document.getElementById("dot"+num).src = "./images16/dot_red.png";;
			currnum = num;
		}

		if(num == 1 || num > lastnum){
			main_event_bg.style.backgroundColor=layerbg1;
		}
		if(num == 2){
			main_event_bg.style.backgroundColor=layerbg2;
		}
		if(num == 3){
			main_event_bg.style.backgroundColor=layerbg3;
		}
		if(num == 4){
			main_event_bg.style.backgroundColor=layerbg4;
		}
	}

	function main_event2(num) {
		if(!num){
			num = currnum;
		}

		if (timer1) window.clearTimeout(timer1);

		for(i=1;i<=lastnum;i++) {
			document.getElementById("main_event"+i).style.display = "none";
			document.getElementById("dot"+i).src = "./images16/dot_gray.png";
		}

		if(num > lastnum) {
			document.getElementById("main_event1").style.display = "block";
			document.getElementById("dot1").src = "./images16/dot_red.png";
			currnum = 1;
			timer1 = setTimeout("main_event2(2)", 3000);
		} else {
			document.getElementById("main_event"+num).style.display = "block";
			document.getElementById("dot"+num).src = "./images16/dot_red.png";
			currnum = num;
			timer1 = setTimeout("main_event2("+eval(num+1)+")", 3000);
		}

		if(num == 1 || num > lastnum){
			main_event_bg.style.backgroundColor=layerbg1;
		}
		if(num == 2){
			main_event_bg.style.backgroundColor=layerbg2;
		}
		if(num == 3){
			main_event_bg.style.backgroundColor=layerbg3;
		}
		if(num == 4){
			main_event_bg.style.backgroundColor=layerbg4;
		}
	}

	timer1 = setTimeout("main_event2(2)", 3000);

	function eventout(num) {
		if(!num){
			num = currnum;
		}
		//alert(num);
		if (timer1) window.clearTimeout(timer1);
		timer1 = setTimeout("main_event2("+num+")", 100);
	}

	function eventprev(){
		num = currnum;
		num--;
		if(num <= 0){
			main_event2(lastnum);
		}else{
			main_event2(num);
		}
	}

	function eventnext(){
		num = currnum;
		num++;
		if(num > lastnum){
			main_event2(1);
		}else{
			main_event2(num);
		}
	}
</script>
