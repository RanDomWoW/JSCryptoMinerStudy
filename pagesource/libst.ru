<script>
    function showDetailBookModal(e) {
        $.ajax({
            url: "/Main/GetBookByIDForModalDetail?bookID=" + e,
            type: "GET",
            contentType: "application/json",
            success: function (data) {

                $("#cover_modal").remove();

                $("#bookLink").html(data.Name);
                $("#bookLink").attr("href", data.BookLink);

                //$("#authorLink").html(data.AuthorName);
                //$("#authorLink").attr("href", data.AuthorLink);
                $("#author_name_modal").html(data.AuthorLink);

                $("#boot_type_modal").html(data.BookTypeAndGenres);
                $("#date_refresh_modal").html(data.DateRefresh);
                $("#size_modal").html(data.BookSize);
                $("#price_modal").html(data.BookPrice);
                $("#status_modal").html(data.Status);
                //$("#description_modal").html(data.Description);
                $("#linkCoverContainer").attr("href", data.BookLink);
                $("#rating_modal").html(data.Raiting);
                $("#download_modal").html(data.Download);
                $("#protect_modal").html(data.Protect);

                if (data.BookCover != "") {
                    $("#linkCoverContainer").prepend("<img src=" + data.BookCover + " class='img-responsive coverBook' alt='обложка книги " + data.Name + "' id='cover_modal'>");
                }

                //if (data.SubTags != "") {
                //    $("#book_tags_modal").html(data.SubTags);
                //}

                $("#first_Chapter_Modal_Content").html(data.TextForBookModal);

                $("#detailBook").modal("show");

            }
        });
    };
</script>




