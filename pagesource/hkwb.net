// 展开高度
function $D()
{ 
    var h=d.offsetHeight; 
    var maxh=495;
     
    function dmove()
    { 
        h+=30; //设置层展开的速度 
        if(h>=maxh)
        { 
            d.style.height='495px'; 
            clearInterval(iIntervalId); 
            $D0();
        }





        else
        { 
            d.style.display='block'; 
            d.style.height= h + 'px'; 
        } 
    } 
    iIntervalId=setInterval(dmove,2); 
}




// 展开宽度
function $D0()
{ 
    var w=d.offsetWidth; 
    var maxw=1000;
     
    function dmove()
    { 
        w+=10; //设置层展开的速度 
        if(w>=maxw)
        { 
            d.style.width='1000px'; 
            clearInterval(iIntervalId); 

        }
        else
        { 
            d.style.display='block'; 
            d.style.width= w + 'px'; 
        } 
    } 
    iIntervalId=setInterval(dmove,2); 
}
// 收缩高度
function $D2()
{ 
    var h=d.offsetHeight; 
	var hv=dv.offsetHeight; 
    function dmove()
    { 
        h-=20;//设置层收缩的速度 
		hv-=20;//设置层收缩的速度 
        if(h<=0)
        { 
            d.style.display='none'; 
			dv.style.display='none';
			//sgg.innerHTML='<img src="http://bbs.hinews.cn/img/20090626/qun-06-25-29.jpg" width="950" height="80">';
			sgg.innerHTML='';
            clearInterval(iIntervalId); 			 
        }
        else
        { 
            d.style.height=h+'px'; 
			dv.style.height=hv+'px'; 
        } 
    } 
    iIntervalId=setInterval(dmove,3); 
}


// 收缩宽度
function $D3()
{ 
    var w=d.offsetWidth; 
     
    function dmove()
    { 
        w -= 10; //设置层展开的速度 
        if(w<=1000)
        { 
            clearInterval(iIntervalId);
            $D2();
        }
        else
        { 
            d.style.width = w+'px'; 
        } 
    } 
    iIntervalId=setInterval(dmove,2); 
}

//鼠标移动到标题图片上，展开图片
