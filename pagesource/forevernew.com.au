<script type="text/javascript">
var rrtsearch = {    
    siteId: '39860',    
    lastSent: '',
    send: function(search_phrase) {
        if (search_phrase == rrtsearch.lastSent) {
            console.log('RRT: skipping repeated search phrase: ' + search_phrase);
            return;
        }
        rrtsearch.lastSent = search_phrase;
        console.log('RRT: sending search phrase: ' + search_phrase);
        bk_use_multiple_iframes = true;
        bk_allow_multiple_calls = true;
        bk_addPageCtx('Aid', '7208');
        bk_addPageCtx('Etype', 'SEARCH');
        bk_addPageCtx('Searchterm', search_phrase);
        bk_doJSTag(this.siteId, 1);
        BKTAG._reset();
    },
    observe: function(container, observed_element, search_field, event) {
        event = event || 'submit';
        jQuery(container).on(event, observed_element, function() {            
            var search_phrase = jQuery(search_field).val();                        
            rrtsearch.send(search_phrase);            
        });
    },
    sendClickedText: function(container, observed_element) {
        jQuery(container).on('click', observed_element, function() {
            var search_phrase = jQuery(this).text();                        
            rrtsearch.send(search_phrase);
        });
    }
};    

console.log('loaded RRT search, siteId is ' + rrtsearch.siteId);

jQuery(document).ready(function(){
    // RRT search observers
    rrtsearch.observe('body', '#search_mini_form', '#search'); // keyword-search-result
