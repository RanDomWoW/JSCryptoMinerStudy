<script type="text/javascript">
    (function(){
        function upDateFNViewport() {
            var deviceScreen = screen.width;
            if (window.orientation == 90 || window.orientation == -90) {
                deviceScreen = screen.height;
            }
            var mvp = document.getElementById('fn-viewport');
            if (deviceScreen < 980) {
                var scale = deviceScreen / 980;
                if (deviceScreen >= 768) {
                    mvp.setAttribute('content', 'width=980, initial-scale=' + scale + ', minimum-scale=' + scale + ', maximum-scale=' + scale);
                } else {
                    mvp.setAttribute('content', 'width=980, initial-scale=' + scale + ', minimum-scale=' + scale + ', maximum-scale=1');
                }
            } else if (deviceScreen == 1024) {
                mvp.setAttribute('content','width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0');
            }
        }
        window.addEventListener("orientationchange", upDateFNViewport);
        upDateFNViewport();
    })();
</script>

