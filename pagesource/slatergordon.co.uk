<script type="text/javascript">
        //Process all the fields in the forms for touch devices to show the right keyboard
        //changing the type of text fields (e.g. email, phone, ...)
        if (('ontouchstart' in window) || window.DocumentTouch && document instanceof DocumentTouch) {
            $(".umbraco-forms-field").each(function () {
                changeInputType($(this), "phone", "tel");
                changeInputType($(this), "contactnumber", "tel");
                changeInputType($(this), "email", "email");
            });
        }

        function changeInputType(element, pattern, type) {
            //get the current classes of the element
            var ufclasses = element.attr("class").split(" ");


            //Check if the pattern is currently existing in any of the classes of the element
            var isApplicable = false;
            var index = 0;
            while (!isApplicable && index < ufclasses.length) {
                if (ufclasses[index].indexOf(pattern) != -1) {
                    isApplicable = true;
                }
                index++;
            }

            //If the class is found, the element is removed and cloned with a different type. It needs to be removed to change the type
            if (isApplicable) {
                var id = $(element).find("input").attr("id");
                $('#' + id).clone().attr('type', type).insertAfter('#' + id).prev().remove();
            }
        }
</script>
    
