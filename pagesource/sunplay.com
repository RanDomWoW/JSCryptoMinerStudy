  Found this code. Modified it so that it can be applied to any toggle button/collapsible menu pair.
  Example 1:
    <span data-menu_toggler="menu-top"> Toggler </span>

    <div class="container"  data-menu_target="menu-top">
      <div> Collapsed Content </div>
    </div>

  Example 2:
    <span data-menu_toggler="abcdefg"> ABCDEFG Toggler </span>

    <div class="sidebar"  data-menu_target="abcdefg">
      <div> ABCDEFG Collapsed Content </div>
    </div>
-->
<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery('[data-menu_target] .parent')
        .addClass('has-sub')
        .prepend("<span class='submenu-expand'>+</span>");
    
    jQuery('.submenu-expand').on('click', function() {
        var submenu = jQuery(this).siblings('ul');
        if(submenu.is(":hidden")) {
            submenu.slideDown("fast");
            jQuery(this).html("-");
        } else {
            submenu.slideUp("fast");
            jQuery(this).html("+");
        }
    })

    jQuery('.toggle').click(function() {
        var $menuToggler = jQuery(this);
        $menuTarget = jQuery('[data-menu_target="'+jQuery($menuToggler).attr('data-menu_toggler')+'"]');
        $menuTarget.click(toggleMenu($menuTarget));
    });
    function toggleMenu($target) {
        if ($target.is(":hidden")) {
            $target.slideDown("fast");
        } else {
            $target.slideUp("fast");
        }
    }
});

