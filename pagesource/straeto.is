<div class="mobile-search-open">
	<span class="openSearch"></span>
</div>
<div class="map-overlay-sidebar">
	<div class="map-overlay-element where-to" data-bind="with: strings">
		<div class="zoom-buttons">
			<button class="zoom-in" data-bind="click: $root.zoomIn" title="Zoom in"><span>+</span></button>
			<button class="zoom-out" data-bind="click: $root.zoomOut" title="Zoom out"><span>-</span></button>
			<button class="help" data-bind="click: $root.toggleWindow.bind($data, 'questions')"><span>?</span></button>
		</div>
		<div class="search-form">

			<div class="relative">
				<button class="switch" data-bind="click: $root.switchSearch"></button>
				<div class="input">
					<span data-bind="text: from+':'">Frá:</span>
					<input type="text" id="from-value" data-bind="autoComplete: { selected: $root.selectedFrom, options: $root.options, change: $root.searchChange, searched: $root.searched }, attr: { placeholder: from_placeholder }, hasFocus: $root.searchFromFocus" placeholder="Sláðu inn heimilisfang">
					<ul class="ui-autocomplete ui-menu" data-bind="visible: ($root.searchFromFocus() || $root.fromFocused()) && !$root.selectedFrom(), foreach: $root.searchedFrom.slice(0).reverse()">
						<li class="ui-menu-item" data-bind="text: label, css: type, click: $root.selectedFrom"></li>
					</ul>
				</div>
				<div class="input">
					<span data-bind="text: to+':'">Til:</span>
					<input type="text" id="to-value" data-bind="autoComplete: { selected: $root.selectedTo, options: $root.options, change: $root.searchChange }, attr: { placeholder: to_placeholder }, hasFocus: $root.searchToFocus" placeholder="Hvert viltu komast?">
					<ul class="ui-autocomplete ui-menu" data-bind="visible: ($root.searchToFocus() || $root.toFocused()) && !$root.selectedTo(), foreach: $root.searchedTo.slice(0).reverse()">
						<li class="ui-menu-item" data-bind="text: label, css: type, click: $root.selectedTo"></li>
					</ul>
				</div>
			</div>

			<div class="dropdowns">
				<div class="select arrival" trigger="type">
					<span data-bind="text: $root.arriveBy() ? arrival : departure, click: $root.changeArriveBy">Brottför</span>
				</div>
				<div class="select clockpicker" trigger="time">
					<i class="fa fa-clock-o"></i>
					<input type="text" id="timepicker" data-time="12:05" maxlength="5" data-bind="value: $root.departure, event:{ change: $root.timeSearchChange }, click: function(){ $root.timeChange(true) }, timer: { time: $root.departure, change: $root.timeChange }" readonly>
				</div>
				<div class="select" trigger="date">
					<i class="fa fa-calendar"></i>
					<input type="text" class="datepicker" data-bind="value: $root.calendar, event:{ change: $root.switchChange }" readonly>
				</div>
			</div>

		</div>
		<div class="search-submit">
			<span data-bind="visible: ($root.selectedPlan()==null && $root.selectedTo) || $root.searchChange()">
				<a href="" class="submit_search" data-bind="click: $root.searchPlan, text: search_trip">Search</a>
			</span>
			<span data-bind="visible: $root.selectedPlan()!=null && !$root.searchChange()">
				<a href="" class="submit_search remove" data-bind="click: $root.removeSearch, text: remove_search">Remove</a>
			</span>
		</div>
	</div>
	<div class="map-route-plans">
		<div data-bind="visible: !planDetailOn()">
			<div class="filter-wrap" data-bind="visible: itineraries().length > 0">
				<div class="filter clearfix" data-bind="with: strings">
					<div class="sort-by">
						<h4 data-bind="text: sort_by">Raða eftir</h4>
						<select data-bind="options: $root.filterSortBy, optionsText: 'title', value: $root.selectedPlanSort, event:{ change: $root.sortPlan }"></select>
					</div>
				</div>
			</div>
			<div class="loadBtn" data-bind="with: strings, visible: itineraries().length > 0 && $root.canSearchForPrevPlans()">
				<a class="previous" href="" data-bind="click: $root.loadPreviousPlan, text: get_previous">Sækja fyrri</a>
			</div>
			<ul data-bind="foreach: itineraries">
				<li>
					<a href="" data-bind="click: $root.selectPlan.bind($data,$index()), css: $root.selectedPlan() == $index() ? 'active': ''">
						<div class="isOnSameDate" data-bind="visible: !startSameDay, text: moment(startTime).format('DD MMMM YYYY (dddd)')"></div>
						<div class="isOnSameDate" data-bind="visible: !endSameDay, text: $root.strings().next_day_notification"></div>
						<div style="clear:both"></div>
						<div class="trip">
							<strong><span data-bind="text: startTimeAlt"></span> - <span data-bind="text: endTimeAlt"></span></strong>

							<div class="stations step-icons" data-bind="foreach: legs">
								<span data-bind="click: routeAlert ? $root.openAlert.bind($data, route) : '', clickBubble: false, ifnot: (duration <= 55 && modeAlt=='walk')">
									<span class="walk" data-bind="text: route, attr: { class: modeAlt }"></span>
									<span class="alert" data-bind="visible: routeAlert"></span>
									<span data-bind="ifnot: ($index() === ($parent.legs.length - 1))">
										<span class="next">&rsaquo;</span>
									</span>
								</span>
							</div>
						</div>

						<div class="time">
							<strong data-bind="text: durationAlt"></strong>
							<span class="walk" data-bind="text: walkTimeAlt"></span>
						</div>
						<div style="clear:both"></div>
					</a>
				</li>
			</ul>
			<div class="loadBtn" data-bind="with: strings, visible: itineraries().length > 0 && $root.canSearchForNextPlans()">
				<a class="next" href="" data-bind="click: $root.loadNextPlan, text: get_more">Sækja fleiri</a>
			</div>
		</div>
		<div class="plan-detail" data-bind="with: planDetail, visible: planDetailOn">
			<div class="header">
				<a href="" data-bind="click: $root.showPlanDetail"><i class="fa fa-chevron-circle-left"></i> <span data-bind="text: $root.strings().back">Til baka</span></a>
							</div>
			<div class="trip-date" data-bind="visible: !startSameDay, text: moment(startTime).format('DD MMMM YYYY (dddd)')"></div>
			<div class="trip-date" data-bind="visible: !endSameDay, text: $root.strings().next_day_notification"></div>
			<ul data-bind="foreach: legs">
				<span data-bind="if: duration > 55">
					<li class="step-icons">
						<span class="walk" data-bind="attr: { class: mode }"></span>
						<strong>
							<span data-bind="if: mode=='WALK'">
								<span data-bind="text: $root.strings().walk_to"></span>
								<span data-bind="text: to.name"></span>
							</span>
							<span data-bind="if: mode=='BUS'">
								<span class="bus" data-bind="text: route"></span>
								<span data-bind="text: $root.strings().direction_to + ' ' + (displayTitle ? displayTitle : to.name)"></span>
							</span>
						</strong>
						<table>
							<tr>
								<th><span data-bind="text: $root.strings().departing">Brottför kl.</span> <span data-bind="text: moment(startTime).utc().format('H:mm')"></span></th>
								<td><span data-bind="text: $root.strings().from">frá</span> <span data-bind="text: from.orig == '' ? $root.selectedFrom().label : from.name"></span></td>
							</tr>
							<tr>
								<th><span data-bind="text: $root.strings().arriving">Koma kl.</span> <span data-bind="text: moment(endTime).utc().format('H:mm')"></span></th>
								<td><span data-bind="text: $root.strings().to">til</span> <span data-bind="text: to.orig == '' ? $root.selectedTo().label : to.name"></span></td>
							</tr>
							<tr data-bind="if: route">
								<th data-bind="text: $root.strings().route">Leið:</th>
																<td data-bind="text: route"></td>
							</tr>
							<tr data-bind="if: duration">
								<th data-bind="text: $root.strings().duration">Lengd:</th>
								<td data-bind="text: durationAlt"></td>
							</tr>
							<tr>
								<th data-bind="text: $root.strings().transport">Ferðamáti:</th>
								<td data-bind="text: mode=='WALK' ? $root.strings().walk:$root.strings().bus"></td>
							</tr>
						</table>
					</li>
				</span>
			</ul>
		</div>
		<div class="errors" data-bind="visible: error, with: error">
			<div class="icon" data-bind="css: type"></div>
			<div class="text" data-bind="html: text"></div>
		</div>
	</div>
</div>
<div id="twitter-feed-element" class="twitter-feed-element" data-bind="visible: $root.twitterVisible">
	<a class="twitter-timeline" data-width="100%" data-height="100%" data-link-color="#ad0000" href="https://twitter.com/straetobs">Tweets by straetobs</a>
  <div class="loading-twitter">
    Hleður...
  </div>
	<i class="times-circle" data-bind="click: $root.toggleTwitter"></i>
</div>
<div class="map-controls bottom-left" data-bind="with: strings">
	<button class="twitter tooltip" data-bind="click: $root.toggleTwitter, attr: { 'data-title': 'Twitter' }"></button>
	<button class="announces tooltip" data-bind="click: $root.toggleAnnouncements, attr: { 'data-title': announcements_and_news }"></button>
	<div class="map-overlay-element announcements" data-bind="visible: $root.announcementsVisible">
		<div class="blue-toggler">
			<div class="half active" data-bind="click: $root.toggleAnnouncementsContent, css: { 'active' : $root.announcementsContentVisible() }">
				<img src="https://www.straeto.is/img/buttons/map-announcements-icon.svg" width="18" height="18" class="toggler-icon" data-bind="{ attr: { 'alt' : announcements_title } }" />
				<h4 data-bind="text: announcements_title">Tilkynningar</h4>
			</div>
			<div class="half" data-bind="click: $root.toggleNewsContent, css: { 'active' : $root.newsContentVisible() }">
				<img src="https://www.straeto.is/img/buttons/map-news-icon.svg" width="18" height="18" class="toggler-icon" data-bind="{ attr: { 'alt' : news } }" />
				<h4 data-bind="text: news">Fréttir</h4>
			</div>
		</div>

		<div class="map-overlay-element-content">
			<div data-bind="visible: $root.newsContentVisible()">
				<div class="news-events">
					<!-- ko foreach: $root.events -->
					<a href="#" data-bind="attr: { href: 'is/upplysingar/frettir/' + slug }">
						<span class="date" data-bind="text: moment(start).format('DD.MM.YYYY')"></span>
						<div data-bind="text: title"></div>
					</a>
					<!-- /ko -->
					<a href="https://www.straeto.is/is/upplysingar/frettir" class="more" data-bind="html: '&raquo; ' +  old_news"></a>
				</div>
			</div>
			<div data-bind="visible: $root.announcementsContentVisible(), foreach: $root.alerts">
				<div data-bind="visible: $index() == $root.activeAlert()">
					<div class="affected-routes" data-bind="foreach: $root.alerts">
						<div data-bind="click: $root.showAlert.bind($data,$index()), text: bus_name, attr: { 'class': $index() == $root.activeAlert() ? 'bus-number selected':'bus-number' }"></div>
					</div>
					<h4 data-bind="text: title"></h4>
					<h5>
						<!-- ko if: moment(start).isSame(moment(end), 'day') -->
						<span data-bind="text: moment(start).format('D. MMM YYYY (H:mm')"></span> - <span data-bind="text: moment(end).format('H:mm)')"></span>
						<!-- /ko -->
						<!-- ko if: !moment(start).isSame(moment(end), 'day') -->
						<span data-bind="text: moment(start).format('D. MMM YYYY')"></span> - <span data-bind="text: moment(end).format('D. MMM YYYY')"></span>
						<!-- /ko -->
					</h5>
					<hr>
					<div class="" data-bind="html: text"></div>
				</div>
			</div>
			<div data-bind="visible: $root.announcementsContentVisible() && $root.alerts().length == 0">
				<!-- ko if: $root.lang() == 'is' -->
				<p>
					Engar tilkynningar
				</p>
				<!-- /ko -->
				<!-- ko if: $root.lang() != 'is' -->
				<p>
					No announcements found.
				</p>
				<!-- /ko -->
			</div>
		</div>
	</div>
</div>
<div class="map-controls bottom-right" data-bind="with: strings">
	<button id="chat-bubble" class="drift-open-chat chat tooltip tt-r" data-bind="attr: {'data-title': ($root.lang() == 'is' ? 'Netspjall' : 'Service desk')}"></button>
	<button class="busfilter tooltip tt-r" data-bind="click: $root.toggleBusFilters, attr: { 'data-title': bus_tooltip }"></button>
	<div class="map-overlay-element bus-filter" data-bind="visible: $root.busFiltersVisible">
		<div class="blue-toggler">
			<div class="half" data-bind="click: $root.showHideStops, css: { active: $root.showStops() }">
				<h4 data-bind="text: show_bus_stops">Sjá biðstöðvar</h4>
				<div class="toggler-wrap"><div class="toggler"></div></div>
			</div>
			<div class="half" data-bind="click: $root.showHidePlaces, css: { active: $root.showPlaces() } ">
				<h4 data-bind="text: show_places">Show kiosks</h4>
				<div class="toggler-wrap"><div class="toggler"></div></div>
			</div>
		</div>
		<div class="filters" data-bind="foreach: $root.filterGroups">
			<div class="filter-group">
				<header>
					<h4 data-bind="text: area">Höfuðborgarsvæðið</h4>
					<span data-bind="click: $root.toggleAllBuses, text: $parent.all_none">Allir / Enginn</span>
				</header>
				<div class="buses" data-bind="foreach: items">
					<div class="bus-number" data-bind="text: bus, css: { active: active }, click: $root.toggleBusActive"></div>
				</div>
			</div>
		</div>
	</div>
</div>
<div class="map-overlay-window" data-bind="visible: windowVisible" style="display:none;">
	<i class="times-circle" data-bind="click: $root.toggleWindow"></i>
	<ul class="menu" data-bind="foreach: windowData">
		<li data-bind="click: $root.showWindowContent.bind($data,$index()), attr: { 'class': $index() == $root.activeWindow() ? 'active':'' }">
			<div class="icon">
				<img alt="" data-bind="attr: { src: '/img/help-'+$index()+'.svg' }" />
			</div>
			<span data-bind="text: name.substr(3)"></span>
		</li>
	</ul>
	<div class="content" data-bind="with: activeWindowContent">
		<h2 data-bind="text: title"></h2>
		<div class="text" data-bind="html: text"></div>
	</div>

</div>
<div class="map-tooltip" data-bind="visible: showTooltip">
	<p data-bind="text: tooltipText"></p>
</div>
