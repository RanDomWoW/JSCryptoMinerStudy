    // If section is on this current page.
    if ($section.length > 0) {
      var $ad = $('.swimlane-ad', $section);
      // If there is an ad placed in this section and it matches the slotRenderEnded event for this slot.
      console.log ('Event slot id is: ' + event.slot.getSlotElementId());
      console.log ('Ad content size is: ' + $ad.html().length);
      if (($ad.length > 0) && ($ad.attr('id') == event.slot.getSlotElementId())) {
        // If ad slot was filled, hide the last non-ad column.
        if (!event.isEmpty) {
          $('.column:not(.swimlane-ad-column)', $section).last().css('display', 'none');
        }
        else {
          // Ad slot wasn't filled.  Hide it because collapseDiv event from DFP doesn't work sometimes!!!
          $('#' + event.slot.getSlotElementId()).css('display', 'none');
        }
      }
    }
  }
  
  // Enable services (request ads)
  //console.log ('Monitoring slotRenderEnded');
  //console.log ('googletag.cmd is: ', googletag.cmd);
  googletag.cmd.push(function() {
  	googletag.pubads().addEventListener('slotRenderEnded', function(event) {
	  	//console.log ('Slot render ended ' + event.creativeId, event.slot.l.m, event);
	  	// There are some swimlanes that should have a fixed number of tiles, so if we got an ad in one of those swimlanes, hide the last tile that is not an ad-tile.
	  	hideLastNonAdChild ('section#insidescoop', event);
	  	hideLastNonAdChild ('section#giveaway', event);
	  	hideLastNonAdChild ('section#ask-wendy', event);
	  });
  });
  
