<script>	
				function set_station(stations) {
					var today = new Date();
					var currentHour = today.getHours();
					var station = false;

					for (var i = 0; i < stations.length; i++) {
						if (currentHour <= stations[i].raw_time) {
							station = stations[i];
							break;
						}
					}
					if (!station) {
						station = stations[0];
					}

					if (station.affiliate != '') {
						if (station.affilate_logo!='') {
							jQuery('#station-logo').attr("src",station.affiliate_logo);
							jQuery('#station-logo').show();
						} else {
							jQuery('#station-logo').hide();
						}
				   		jQuery('#station-weekdays').html(station.weekdays);
				   		jQuery('#station-weekdays-text').html("WEEKDAYS");
				   		jQuery('#station-state-local').hide();
				   		jQuery('#station-state-found').show();

				   	} else {
				   		jQuery('#station-state-found').hide();
				   		jQuery('#station-state-local').show();
				   	}
				   	jQuery('#zip-code').html(station.zip_code);
			   		Cookies.set('home_station',stations); // for further page refreshes
				}

				jQuery(document).ready(function($) {
					
					if (!Cookies.get('home_station')) {
						var query_vars = getUrlVars();
						var cache_bust = new Date().getTime();
						var request_data = {
							action:'get_station_by_ip',
							'cb': cache_bust
						}

						if (query_vars.force_ip) {
							request_data.force_ip = query_vars.force_ip;
						}
						// ajax to get home station
						$.ajax({
							url:'/wp-admin/admin-ajax.php',
							data: request_data,
							cache: false
						}).done(function(resp){
							var data = JSON.parse(resp);
					   		set_station (data);
						});
					} else { // we already have the station so render it (no server call required)
						set_station(JSON.parse(Cookies.get('home_station')));
					}

					$('#change-zip').keypress(function(event) { //user wants to select their zip

				        if (event.keyCode == 13) {
				        
				        	jQuery("#zip-dropdown").hide();

				           // ajax to get home station
				           var request_data = {
								action:'get_station_by_zip',
								zip_code: $('#change-zip').val()
							}
							$.ajax({
								url:'/wp-admin/admin-ajax.php',
								data: request_data
							}).done(function(resp){
								var data = JSON.parse(resp);
						   		set_station (data);
							});
					    }
				    });
					
				});

			</script>
