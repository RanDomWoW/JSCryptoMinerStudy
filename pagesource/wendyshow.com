	// fix up column heights since we have no control over what might be coming back from
	// social media api calls
	function sm_fix_col_heights() {
		var sm_col_max = 0;
		var sm_cols = jQuery('#social-stream > div > .column > div > .column >.column-inner'); // social module content divs
		var i;
		for (i = 0; i < sm_cols.length; i ++) {
			if (jQuery(sm_cols[i]).height() > sm_col_max) {
				sm_col_max = jQuery(sm_cols[i]).height();
			}
		}
		for (i = 0; i < sm_cols.length; i ++) {
			jQuery(sm_cols[i]).height(sm_col_max);
		}
		jQuery('#social-stream > div > .column > div > .column').height(sm_col_max+10);
	}
	jQuery(document).ready(sm_fix_col_heights);
</script>
