<script type="text/javascript">
$(document).ready(function(){
	setInterval(function(){
		main_tab("middle", "bottom");
	}, 7000);

	$('.new_main_list').each(function(){
		var page_n = $(this).children('ul').children('li').length;

		$(this).find('.new_goods_roop').not(".main_goods_gap2").each(function(){
			$(this).attr('show_li2',Math.floor($(this).index()/4))
		})

		var active =$(this).is(':visible');
		if (active)
		{
			$(this).attr('new_goods_wrpa');
		}

		$(this).children('ul').children( ".new_goods_roop:gt(3)" ).hide();

		var goods_i = $(this).children('ul').children('.new_goods_roop').index()+1;
		for(var i = 1 ;  i<=goods_i  ; i++ ){
		//for (var i = 0; i < goods_i; i++) {
			 if(i%4==0){
				$(this).children('ul').children('.new_goods_roop').eq(i-1).after( "<li class='main_goods_gap2'></li>" );
			 }
		}
	});

	var page2 = 1;
	var gap_page2 = 0;
	var active_wrap = 0;

	$('.new_goods_btn li').click(function(){
		page2 = 1;
		$('.new_main_list').each(function(){
			$(this).children('ul').children( ".new_goods_roop:gt(3)" ).hide();
		});

		var tap_idx = $('.new_goods_btn li').index($(this))
		$('#new_goods_wrap').find('.new_main_list ').hide();
		$('#new_goods_wrap').find('.new_main_list ').eq(tap_idx).show();
	});
	$('#new_goods_ajax').on({
		click:function(){
			$('.new_main_list').each(function(){
				var active =$(this).is(':visible');
				if (active)
				{
					 active_wrap = $(this).attr('new_goods_wrpa');

				}
			})

			var page_start = $('.new_main_list').eq(active_wrap).find('.new_goods_roop').length;
			if (page_start > 4)
			{
				$(this).parents('#new_goods_wrap').find('.new_main_list').eq(active_wrap).find('.new_goods_roop').not( ".main_goods_gap2" ).each(function(){
					var page_active = $(this).attr('show_li2');

					if (page_active == page2)
					{
						$(this).parents('#new_goods_wrap').find('.new_main_list').eq(active_wrap).find( ".main_goods_gap2" ).eq(page_active-1).show();
						$(this).show();

					}
				})

				var page_end = $(this).parents('#new_goods_wrap').find('.new_main_list').eq(active_wrap).find('.new_goods_roop').last().attr('show_li2');
				var page_end2 = $(this).parents('#new_goods_wrap').find('.new_main_list').eq(active_wrap).find('.new_goods_roop').last().is(':visible');

				if (page_end < page2){
					alert('더 이상 상품이 없습니다.')
				}else{
					page2 = page2+1
				}
			}else{
				alert('상품이 없습니다.')
			}
		}
	})
});

/* main_tab 자동 이동 start*/
var cnt_m = 0;
var cnt_b = 0;
function main_tab(mValue, bValue) {
	cnt_m++;
	cnt_b++;

	if(mValue == "middle") {
		if(cnt_m > 3) { cnt_m = 0;}
		tab_change(cnt_m, "middle_banner_tab", "middle_banner_box", mValue);
	}
	if(bValue == "bottom") {
		if(cnt_b > 2) { cnt_b = 0;}
		tab_change(cnt_b, "bottom_banner_tab", "bottom_banner_box", bValue);
	}
}

function tab_change(cnt, tab_wrap, box_wrap, posValue){
	//탭index값, 탭 타이틀 wrap 클래스, 탭 내용 wrap, 위치값
	$("." + tab_wrap).find("li").removeClass("on_" + posValue);
	$("." + tab_wrap).find("li").eq(cnt).addClass("on_" + posValue);
	$("." + box_wrap).hide();
	$("." + posValue + "_box" + (cnt+1)).show();
}
/* main_tab 자동 이동 end */

/* 전세계 추천상품 & 오직 현지에서만 배너 메뉴 클릭 이벤트 */
function on_tab(param, value, pos) {
	var tab = $(param);
	var wrap = $(param).parent().find("> li");

	if(pos == "middle") {
		wrap.removeClass("on_middle");
		$(".middle_banner_box").hide();
		tab.addClass("on_middle");

		if(value != "") {
			$(".middle_box" + value).show();
		}
	} else {
		wrap.removeClass("on_bottom");
		tab.addClass("on_bottom");
		$(".bottom_banner_box").hide();

		if(value != "") {
			$(".bottom_box" + value).show();
		}
	}
}
</script>

