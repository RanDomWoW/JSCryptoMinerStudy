<script type="text/javascript">
    function totalMenu(param, e){
        e.preventDefault();
        e.stopPropagation();
        $(param).parent().removeClass("border_pos");

        if($(param).parent().hasClass("active")){
            $(param).parent().removeClass("active");
            $(param).next().slideUp();
            $(param).removeClass("box_close").addClass("menu_total");
        } else {
            $(param).parent().addClass("active");
            $(param).next().slideDown();
            $(param).parent().siblings().find("> div").slideUp().parent().removeClass("active");
            $(param).parent().siblings().find("> a").removeClass("box_close").addClass("menu_total");
            $(param).removeClass("menu_total").addClass("box_close");
            if($(param).attr("title") == "brand"){
                $(param).parent().addClass("border_pos");
            }
        }
    }

    function search_alph(param) {
        var val = $(param).parent().find("> a");
        val.removeClass("brand_active");
        $(param).addClass("brand_active");

        //$(param).text()

        ajaxBrand($(param).text(),"B");
    }

    function searchBrand()
    {
        if($("input[name=brand_search]").val()){
            ajaxBrand($("input[name=brand_search]").val(),"S");
        }
    }

    function ajaxBrand(word, type){
        //브랜드명을 ajax로 가져온다.
        $.ajax({
            type: 'POST',
            data: {
                act : 'brand',
                brand : word,
                type : type
            },
            url: '/index.php',
            dataType: 'json',

            success: function(datas){
                var mString = "";
                $.each(datas, function(k,v) {
                    // "+this.b_ix+"
                    mString += "<li><a href='/event/goods_brand.php?b_ix="+this.b_ix+"' title=''>"+this.brand_name+" <span></span></a></li>";
                });
                $(".search_brand_detail").find("ul").html(mString);

            },
            error: function(data){
                //없음
                $(".search_brand_detail").find("ul").html("해당 브랜드가 없습니다.");
            }
        });
    }

    function subcategory(param) {
        $(".comm").hide();
        $(param).children("div").show();
        var test = $(param).children("a").attr("cid");
    }

    $(document).ready(function(){
        $(".search_alpha").find("a").eq(0).trigger("click");
    });
</script>

