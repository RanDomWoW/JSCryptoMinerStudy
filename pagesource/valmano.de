          <script type="text/javascript">
    //<![CDATA[
    var activeTabCat = '';
    var topNavItems = ["damen","herren","geschenkewelt"];

    /**
     * Displays the correct sub-menu block and gets the active parent category
     * @type {string}
     */
    jQuery(document).ready(function ($) {
        var gender = null;
        if (activeTabCat !== '') {
            switch (activeTabCat) {
                case topNavItems[0] :
                    showTabCat(activeTabCat);
                    hideTabCat(topNavItems[1]);
                    hideTabCat(topNavItems[2]);
                    gender = 'f';
                    break;
                case topNavItems[1] :
                    showTabCat(activeTabCat);
                    hideTabCat(topNavItems[0]);
                    hideTabCat(topNavItems[2]);
                    gender = 'm';
                    break;
                case topNavItems[2] :
                    showTabCat(activeTabCat);
                    hideTabCat(topNavItems[0]);
                    hideTabCat(topNavItems[1]);
                    break;
            }
        } else {
            var $homepageSubMenu = $('ul#homepage-sub-menu');
            $homepageSubMenu.fadeIn();
        }

        function showTabCat(tabCat) {
            var $tab = $('.gender-nav-container [data-cat-key="' + tabCat + '"]'),
                $genderSubMenu = $('ul#' + tabCat + '-sub-menu');

            $tab.addClass('active');
            $genderSubMenu.fadeIn();
        }

        function hideTabCat(tabCat) {
            var $tab = $('.gender-nav-container [data-cat-key="' + tabCat + '"]'),
                $genderSubMenu = $('ul#' + tabCat + '-sub-menu');

            $tab.removeClass('active');
            $genderSubMenu.hide();
        }

        // add the initial gender to the dataLayer of GTM
        if (window.localStorage.initialGender === undefined) {
            if (activeTabCat != topNavItems[2] && activeTabCat != '') {
                window.localStorage.initialGender = activeTabCat;
            }
        }

        Mage.Cookies.clear = function (name) {
            if (Mage.Cookies.get(name)) {
                Mage.Cookies.set(name, '', new Date(0));
            }
        };

                Mage.Cookies.clear('vcg');
        
        dataLayer.push({'visitorInitialGender': window.localStorage.initialGender});
    });
    //]]>
</script>
