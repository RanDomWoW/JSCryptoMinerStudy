    htmlCode+='.popup-bottomfloat-big .popup-bottomfloat-x{ width:20px;height:20px;overflow: hidden;position: absolute;right:90px;top:10px;cursor: pointer; background: url("http://i1.umivi.net/v/static/2018/ympc/public/images/bg.png?v=1") no-repeat -54px -164px;cursor: pointer;}';
    htmlCode+='@media screen and (max-width:1460px){';
