    window.onresize = function () {
        clearTimeout(t), t = setTimeout(e, 300)
    };
    e();
}(window);
var clientHeight = document.body.clientHeight;
$(function(){
    setTimeout(function(){
        console.log('window.scrollTo(0,0);');
        window.scrollTo(0,0);
    },500);
    $("#btn-scroll-top").click(function(){
        $("html,body").animate({scrollTop:0},600);
    });
    var $nav = $(".nav");
    var $logoo = $(".s1 .logoo");
    var $_text1 = $(".s1 ._text1");
    var $_text2 = $(".s1 ._text2");
    var $footer = $(".footer");
    var $body = $("body");
    var $section01 = $(".section01");
    var diff = (document.body.clientWidth>1500?300:0);
    var scrollOnce = 0;
    setTimeout(function(){
        if(scrollOnce===0){
            $("html,body").animate({scrollTop:clientHeight-32-106},1000,function(){
                $(".section01").addClass("active");
            });
        }
        $footer.removeClass('show');
    },8000);
    $(".qr-a").on('mouseover',function(){
        $(".qr").fadeIn();
    });
    $(".qr-a").on('mouseout',function(){
        $(".qr").fadeOut();
    });
    $("#add-height").css({'height':clientHeight-$(".header").get(0).offsetHeight+"px"});
    $(".fix-bg").css({'min-height':clientHeight});
    setTimeout(function(){
        $(".s1").addClass('active');
    },1000);
    setTimeout(function(){
        $(".s1 ._mask").hide();
        $(".s1").css({'z-index':1});
        $footer.addClass('show');
        $body.removeClass('o-hidden');
        var s1Hide = 0;
        var lastScrollTop = 0;
        var func_scroll = function(){
            var scrollTop=document.documentElement.scrollTop||document.body.scrollTop;
            var scrollHeight=document.documentElement.scrollHeight||document.body.scrollHeight;
//                console.log(scrollTop,lastScrollTop);
            if(scrollTop>=lastScrollTop){
                if(scrollTop>clientHeight&&!$nav.hasClass('hide')){
                    //滚动超过一屏高度时
//                        console.log("滚动超过一屏高度时");
                    $nav.addClass('hide');
                }
                if(scrollTop+clientHeight>=scrollHeight-100){
                    $footer.addClass('show');
                }
            }else{
                if(scrollTop<=clientHeight){
                    $nav.removeClass('hide');
                }
                $footer.removeClass('show');
            }
            if(clientHeight+scrollTop>=scrollHeight-100){loadMore();}
//                if(scrollTop>$section01.get(0).offsetTop-356-diff){
            if(scrollTop>document.body.clientWidth*0.3083+clientHeight-650){
                $(".section01").addClass("active");
            }
            setTimeout(function(){
                lastScrollTop = scrollTop;
            },100);
            if(s1Hide){return;}
            var top = (50-scrollTop/(clientHeight/2)*100)+"%";
            var opacity = (1-3*scrollTop/(clientHeight/2)).toFixed(2);
            if(opacity<0){
//                    $('body')[0].onscroll = function(){};
                $('.s1').hide();
                $('.icon-scroll').fadeOut();
                s1Hide = 1
            }
            opacity = opacity<0?0:opacity;
            $logoo.css({top:top,opacity:opacity});
            $_text1.css({top:top,opacity:opacity});
            $_text2.css({top:top,opacity:opacity});
        };
        func_scroll();
        setTimeout(function(){
            $("#b04").css({
                'min-height':    document.body.clientWidth*0.3083+"px"
            });
        })
        var t = null;
        window.onscroll = function(){
            if(scrollOnce==0){
                $("html,body").animate({scrollTop:clientHeight-32-106},1000,function(){
                    $(".section01").addClass("active");
                });
                scrollOnce = 1;
                $('#b04').unslider({
                    dots: true,fluid:true,delay:3000,autoplay:true
                });
            }
            func_scroll();
//                clearTimeout(t),setTimeout(func_scroll,300);
        }
    },3000);
    setTimeout(function(){
        var i = 0;
        var f = function(){
            $divs.eq(i%4).addClass('in');
            var _f = function(i){
                return function(){
                    $divs.eq(i%4-1).removeClass('in');
                }
            }(i)
            setTimeout(_f,1500);
            i++;
        };
        f();
        setInterval(f,3500);
    },0);
    var $divs = $(".fix-bg div");
})
$('html,body').animate({scrollTop: '0px'}, 1);
</script>
