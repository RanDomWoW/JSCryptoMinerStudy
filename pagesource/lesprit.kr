<script type="text/javascript">
// Connection 유지
function connectRefreshReq() {
  var fMS;
  // Send an XMLHttpRequest
  var para = Math.random();	// If the httpRequest is sent with a file, IE caches the response and returns it afterwards.
  var xml = 'connect_refresh_resp.php?' + para;
  if(window.XMLHttpRequest) {
    reqBeat = new XMLHttpRequest();
    reqBeat.open("GET", xml, true);
	fMS = false;
  }
  else{
    reqBeat = new ActiveXObject("Microsoft.XMLHTTP");
    reqBeat.open("GET", xml, true);
	fMS = true;
  }
  reqBeat.onreadystatechange = function () {
    if(reqBeat.readyState == 4 && reqBeat.status == 200) {
      var respText = reqBeat.responseText;
	  // 상태 갱신
      var respArr = respText.split(',');
	  var fReady = respArr[0]*1;
	  var fOpen = respArr[1]*1;
      document.getElementById('fReady').value = fReady;
      document.getElementById('fOpen').value = fOpen;
    }
  }
  if(!fMS) reqBeat.send(null);
  else reqBeat.send();
  // Repeat the function periodically
  setTimeout('connectRefreshReq()', 1000);
}
connectRefreshReq();
</script>
