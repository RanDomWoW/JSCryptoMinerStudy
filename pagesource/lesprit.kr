<script type="text/javascript">
var gInfoOpen = '';
function info_click(info) {
  if(gInfoOpen != info) {
    if(gInfoOpen !='') {
      gInfoOpen.style.display = 'none';    
    }
    info.style.display = '';
    gInfoOpen = info;
  }
}
function info_toggle() {
  if(gInfoOpen == '') {
    info_click(count);
    setTimeout('info_toggle()', 2000);		// 2초
  }
  else if(gInfoOpen == count) {
    info_click(member);
    setTimeout('info_toggle()', 2000);		// 2초
  }
  else if(gInfoOpen == member) {
    info_click(fb);
    setTimeout('info_toggle()', 2000);		// 2초
  }
  else if(gInfoOpen == fb) {
    info_click(info_1);
    setTimeout('info_toggle()', 2000);		// 2초
  }
  else if(gInfoOpen == info_1) {
    info_click(info_2);
    setTimeout('info_toggle()', 1000);		// 1초
  }
  else if(gInfoOpen == info_2) {
    info_click(count);
    setTimeout('info_toggle()', 2000);		// 2초
  }
}
info_toggle();
