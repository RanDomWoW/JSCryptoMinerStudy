  for(var i = 0; i < numReadies; i++) readies[i].style.display = readyDisp;
  for(var i = 0; i < numLeaves; i++) leaves[i].style.display = leaveDisp;
  for(var i = 0; i < numOpens; i++) opens[i].style.display = openDisp;
  for(var i = 0; i < numCloses; i++) closes[i].style.display = closeDisp;
}
