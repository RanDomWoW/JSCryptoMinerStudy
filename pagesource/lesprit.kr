      var arr = [];
      var reg = new RegExp('(^| )'+classname+'( |$)');		// 클래스는 시작, 끝, 공백으로 구분
      var elms = node.getElementsByTagName("*");
      for(var i = 0; i < elms.length; i++)
        if(reg.test(elms[i].className)) arr.push(elms[i]);
      return arr;
    }
