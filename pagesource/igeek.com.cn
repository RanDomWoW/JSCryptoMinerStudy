<script>

var tid = 60;

// 点击加载更多
/*
$(".more a").click(function(){
	//getNewsArticles();
});*/
function getmore(){
	getNewsArticles();	
}


// 点击获取分类
$(".fenlei a").click(function(){
		$(".fenlei a").removeClass('here');
		$(this).addClass('here');
		tid = $(this).attr('tid');
		getCateArticle(tid);
});

getCateArticle(60); 
function getCateArticle(tid){
	$.ajax({
			type: 'POST',
			url: '/index.php?do=articlen_gettotalpages',
			data: {tid:tid,op:'index','tbl':'1'},
			dataType: 'json',
			success: function(data){
				//alert(data.rc)
				
					if(data.rc == 200){
						$("#currentPage").val(0);
						$("#totalPage").val(data.rv.totalPage);
						$(".list4").html('');
						getNewsArticles();																											}
																																			}
																																	});
}
function getNewsArticles(){
		
	var currentPage = parseInt($("#currentPage").val());
	var totalPage = parseInt($("#totalPage").val());
	//$('.more').hide();
	$('.more').html("<a href='javascript:;' onclick='getmore()'>更多内容</a>");
	if(currentPage < totalPage){
		
		var jsonData = {
				page : currentPage+1,
				tid : tid,
				op : 'index',
				tbl:'1'
		}
		$.ajax({
			type: 'POST',
			url: '/index.php?do=articlen_getnewsarticles',
			data: jsonData,
			dataType: 'json',
			success: function(data){
				var result = data.rv.articles;
				var str = "";
				$.each(result, function(index, val){
					str += '<li>';
					str +=  '<a href="/article-'+ val.id +'-1.html" target="_blank" title="'+val.title1+'">';
					
					if(val.isvideo == 1){
						str +=      '<i class="video"></i><em></em>';
					}else{
						str +=      '<i></i>';	
					}
					//str +=      '<i></i>';
					str +=      '<img src="'+ val.previmg +'" width="273" height="152" alt="" onerror="javascript:this.src=\'/static/images/articlen/nopic.jpg\';"/>';
					str +=      '<div class="title">';
					str +=          '<h2>'+ val.title +'</h2>';
					str +=          '<p>'+ val.summary +'</p>';                  
					str +=      '</div>';
					str +=  '</a>';
					str += '</li>';
				});
			//	alert(str)
				$(".list4").append(str);
				$("#currentPage").val(currentPage+1);
				if(currentPage+1 >= totalPage){
					//$('.more').hide();
					$('.more').html("<span>暂无更多</span>");
				}
			}

		});
	}else{
		$('.more').html("<span>暂无更多</span>");	
	}
}
 </script>