function calcOrdCntDcPrice(sum_count) {
    var dc_price = 0;
    if(sum_count > 19 && sum_count < 50){
        dc_price        = 2000;
    } else if(sum_count > 49 && sum_count < 100) {
        dc_price        = 3500;
    } else if(sum_count > 99) {
        dc_price        = 5000;
    }
