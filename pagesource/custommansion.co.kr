<script type="text/javascript" src="/js/jquery.cookie.js"></script>
<script>
var page_name = "index.php";
//var page_name = "index.php";

$(document).ready(function(){
    //----------------------------------------------------------------------------------------------------------------------------
    //  popup이 존재하는 경우 모달창에 내용을 표시한다.
    //  쿠키를 읽어와서 Ajax호출시 값을 전달한다.
    //  노출기간 : 기간설정(sdate, edate)
    //  노출위치 : 메인, 특정페이지??
    //  노출구분 : 매일1회노출, 기간내1회노출, 항상노출
    //  제목(tinytext), 내용(text)
    //  닫기옵션 : 노출구분에따라 (오늘닫기, 다시보지않음, 닫기)
    //  닫기옵션을 선택하는 경우 : 쿠키저장 : uid^닫은날짜
    //  $('#popModal').modal('show');
    //----------------------------------------------------------------------------------------------------------------------------
    //  Popup 체크
    //  Popup관련 쿠키값 읽어오기
    var cookie_val = $.cookie(page_name);
    if(cookie_val == undefined) { cookie_val = ""; }
    
    $.ajax({
        type        :   "POST",
        url         :   "/ajax/UTIL_popup.php",
        dataType    :   "JSON",
        data        :   {act_type:'popup_info', page_name:page_name, cookie_val:cookie_val},
        success     :   function (data) {
            switch(data.result) {
                case "OK":
                    if(data.cnt_popup > 0) {
                        $('.pop-up_contents').html(data.popup[0].cfg_txt_val1);
                        $('#ind_cfg_uid').val(data.popup[0].ind_cfg_uid);
                        $('#cfg_val2').val(data.popup[0].cfg_val2);
                        $('#pop-up').show();

                        if(data.popup[0].cfg_val2 == 'all' || data.pop_show_flag == false) {
                            $('#chk_popup_close').hide();
                        } else {
                            $('#chk_popup_close_str').html(' 오늘 하루 보이지 않음');
                        }

                    }
                    break;

                case "NG":
                    //alert("현재 진행중인 업무가 있습니다.\n\n진행중인 업무를 종료 또는 중지한 이후에 이 업무를 시작할 수 있습니다.");
                    return;
                    break;

                default:
                    //alert("정보를 불러오는중 에러가 발생하였습니다.\n\n잠시후에 다시 시도해주세요");
                    break;
            }
        },
        error       :   function(request,status,error){
            alert("code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error);
            alert("ERROR : 정보를 불러오는중 에러가 발생하였습니다.\n\n잠시후에 다시 시도해주세요");
        }
    });
    //get : $.cookie('login_id');
    //set : $.cookie(page_name, uid^date('Y-m-d'));
    //del : $.removeCookie("login_id");
});

function savePopupCookie() {
    var cfg_val2 = $('#cfg_val2').val();    //  노출구분
    if(cfg_val2 != 'all') { //  항상노출인 경우는 체크박스가 없다.
        var chk_popup_close = $("input:checkbox[id='chk_popup_close']").is(":checked");

        if( chk_popup_close == true ) { //  그만보기 체크박스를 체크한 경우
            var today = "2018-03-21";
            var ind_cfg_uid = $('#ind_cfg_uid').val();
            var cookie_val = ind_cfg_uid+"^"+today;

            var last_cookie_val = $.cookie(page_name);
            if(last_cookie_val){
                cookie_val = last_cookie_val+'|'+cookie_val;
            }
            $.cookie(page_name, cookie_val);
        }
    }
    $('#pop-up').hide();
}
</script>

