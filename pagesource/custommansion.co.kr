<script>
function LoginChk(){
    var modal_type  = $('#modal_type').val();
    var mem_id      = $('#login_mem_id').val();
    var mem_pw      = $('#login_mem_pw').val();

    if(!mem_id){ alert('아이디를 입력하세요.'); $('#login_mem_id').focus(); return;  }
    if(!mem_pw){ alert('비밀번호를 입력하세요.'); $('#login_mem_pw').focus(); return;  }
    

    $.ajax({
        type        :   "POST",
		url         :   "/ajax/UTIL_login.php",
        dataType    :   "JSON",
        data        :   {act_type:'login', mem_id:mem_id, mem_pw:mem_pw, login:'Y', mem_type:'client' },
		success     :   function (data) {

            switch(data.result) {
                case 'OK'      :
                    if(modal_type == 'login'){
                        location.reload();
                    } else {
                        //location.href = '/contents/mypage/index.php';
                        location.reload();
                    }
                    break;

                case 'NG'  :
                    alert("입력하신 정보가 일치하지 않습니다. 확인하고 다시 입력해주세요.");
                    break;

                default:
                    alert("[ "+data.result+" ] 정보를 불러오는중 문제가 발생하였습니다. 잠시후에 다시 시도하시기 바랍니다.");
                    break;
            }
        },
		error       :   function(request,status,error){
            alert("code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error);
            alert("ERROR : 정보를 불러오는중 에러가 발생하였습니다.\n\n잠시후에 다시 시도해주세요");
        }
	});
}

function ChkdupId(){
    var mem_uid             = $('#mem_uid').val();
    var mem_id              = $('#mem_id').val();
    if(!mem_id) { alert('아이디를 입력하세요.'); $('#mem_id').focus(); return; }

    var regEmail = /([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
    if(!regEmail.test(mem_id)) { alert("이메일 주소가 유효하지 않습니다."); return; }
    
    $.ajax({
        type        :   "POST",
        url         :   "/ajax/UTIL_mem_reg.php",
        dataType    :   "JSON",
        data        :   { act_type:'chk_dup_id', mem_id:mem_id, mem_info_act_type:'INS'},
        success     :   function (data) {
            switch(data.result) {
                case 'OK'      :
                    alert("사용할 수 있는 아이디입니다.");
                    $('#mem_id').attr("readonly",true);
                    $('#dupchkid_btn').hide();
                    $('#chk_id_flag').val('Y');
                    break;

                case 'NG_dup'      :
                    alert("중복된 아이디가 있습니다. 다시 입력하세요.");
                    $('#chk_id_flag').val('N');
                    break;

                default:
                    alert("[1] 저장중에 문제가 발생하였습니다. 잠시후에 다시 시도하시기 바랍니다.");
                    break;
            }
        },
        error       :   function(request,status,error){
            alert("code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error);
            alert("ERROR : 정보를 불러오는중 에러가 발생하였습니다.\n\n잠시후에 다시 시도해주세요");
        }
    });
}

function memRegProc() {
    var mem_id      = $('#mem_id').val();
    var mem_pw      = $('#mem_pw').val();
    var mem_pw_chk  = $('#mem_pw_chk').val();
    var mem_name    = $('#mem_name').val();

    if(!mem_id){ alert('이메일(아이디)를 입력하세요.'); $('#mem_id').focus(); return;  }
    var regEmail = /([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
    if(!regEmail.test(mem_id)) { alert("이메일 주소가 유효하지 않습니다."); return; }
    
    if(!mem_pw){ alert('비밀번호를 입력하세요.'); $('#mem_pw').focus(); return;  }
    var mem_pw_length = mem_pw.length;
    if(mem_pw_length < 10){ alert("비밀번호는 최소 10자 이상으로 입력하세요."); $('#mem_pw').focus(); return;  }
    if(!mem_pw_chk){ alert('비밀번호를 입력하세요.'); $('#mem_pw_chk').focus(); return;  }
    if(mem_pw != mem_pw_chk) { alert("비밀번호가 일치하지 않습니다."); $('#mem_pw_chk').focus(); return;  }

    if(!mem_name){ alert('이름을 입력하세요.'); $('#mem_name').focus(); return;  }
    
    if(!confirm("회원가입을 진행하시겠습니까?")) { return; }
    $.ajax({
        type        :   "POST",
		url         :   "/ajax/UTIL_mem_reg.php",
        dataType    :   "JSON",
        data        :   {act_type:'mem_reg', mem_id:mem_id, mem_pw:mem_pw, mem_name:mem_name, },
		success     :   function (data) {
            switch(data.result) {
                case 'OK'       : location.reload(); break;
                case 'NG_dup'   : alert("이미 등록된 아이디 입니다."); $('#mem_id').focus(); return; break;
                case 'NG'       : alert("입력하신 정보에 문제가 있습니다. 확인하고 다시 입력해주세요."); location.reload(); break;
                default         : alert("[ "+data.result+" ] 정보를 불러오는중 문제가 발생하였습니다. 잠시후에 다시 시도하시기 바랍니다."); break;
            }
        },
		error       :   function(request,status,error){ alert("code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error); alert("ERROR : 정보를 불러오는중 에러가 발생하였습니다.\n\n잠시후에 다시 시도해주세요"); }
	});
}
</script>

