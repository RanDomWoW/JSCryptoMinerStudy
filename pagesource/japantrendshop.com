function postToCart(productId, quantity) {
  
	$.ajax({
	    type: 'POST',
	    // make sure you respect the same origin policy with this url:
	    // http://en.wikipedia.org/wiki/Same_origin_policy
	    url: 'ajax_shopping_cart.php?action=add_product',
	    data: { 
				'is_ajax' : true,
        'quantity': (typeof quantity === 'undefined') ? 1 : quantity, // quantity defaults to 1
        'products_id': productId
	    },
	    success: function(cartHtml){
				$('#header_shopping_cart').html(cartHtml);
        $('#addedToCartModal').foundation('reveal', 'open', {animation: 'fade'});
	    }
	});
}

