<script type="text/javascript">
    var $email = $('#tbxEnewsSignUpEmail');
    var $btn = $('#btnEnewsSignUp');
    var $message = $('#divEnewsletterMessage');
    $btn.click(function (e) {
        e.preventDefault();
        openPreferences();
    });
    $email.keypress(function (e) {
        if (e.which == 13) { //enter key
            e.preventDefault();
            openPreferences();
        }
    });
    function openPreferences() {
        $message.html('').hide();
        var email = $email.val();
        if (email == '') {
            $message.html('Please enter e-mail address.').fadeIn(200);
        }
        else {
            // send tracking request to piwik
            // validate email format
            if (validateEmail(email)) {
                // get btm_id
                var btmId = readCookie('btm_id');
                if(btmId != null){
                    if (btmId.length >= 1) {
                        var _paq = _paq || [];
                        _paq.push(["setCookieDomain", "*.automotive-fleet.com"]);
                        _paq.push(['setCustomVariable', 1, 'btm_ea', email, 'page']);
                        _paq.push(['setCustomVariable', 2, 'btm_id', btmId, 'visit']);
                        _paq.push(['setCustomVariable', 3, 'btm_source', 'form-enews-signup', 'page']);
                        _paq.push(["trackPageView"]);
                        _paq.push(["enableLinkTracking"]);
                        (function () {
                            var u = (("https:" == document.location.protocol) ? "https" : "http") + "://bbmstats.com/piwik/";
                            _paq.push(["setTrackerUrl", u + "piwik.php"]);
                            _paq.push(["setSiteId", "6"]);
                            var d = document, g = d.createElement("script"), s = d.getElementsByTagName("script")[0]; g.type = "text/javascript";
                            g.defer = true; g.async = true; g.src = u + "piwik.js"; s.parentNode.insertBefore(g, s);
                        })();
                    }
                }
            }
            

            // open signup form window
            var url = 'https://bobit.sub-forms.com/dragon/init.do?site=BOB6226_AFauto&em=' + email;
            window.open(url, '_blank');
            window.focus();
        }
    }
    
    function validateEmail(email) {
        var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return re.test(email);
    }

    function readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') c = c.substring(1, c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
        }
        return null;
    }

    $(document).ready(function () {
        $email.watermark("Your e-mail address", { className: "watermarked" });
    });
</script>
       
