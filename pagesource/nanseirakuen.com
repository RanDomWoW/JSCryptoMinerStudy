<style>
/* .mainVisualSS */
.mainVisualSS {
	position: relative;
	overflow: hidden;
	height: 640px;
	min-height: calc(100vh - 220px);
}
.mainVisualSS_item {
	position: absolute;
	top: 0;
	left: 0;
	height: 640px;
	min-height: calc(100vh - 220px);
}
.mainVisualSS_item:nth-child(n+2) {
	display: none; 
}
.mainVisualSS_item img {
	position: relative;
	width: 100%;
	height: auto;
}
.mainVisualSS .fac-gallery {
	margin: 0;
	padding: 0;
}
.mainVisualSS .fac-gallery .fac-gallery-slider {
	overflow: visible;
}
.mainVisualSS .fac-gallery .fac-gallery-item {
	display: block; 
	position: absolute;
	z-index: 1;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	transition: opacity 2s ease-in-out 0s;
	opacity: 0;
}
.mainVisualSS .fac-gallery .fac-gallery-item img {
	max-width: none;
}
.mainVisualSS .fac-gallery-initialize .fac-gallery-item {
	transition-duration: 0s;
}
.mainVisualSS .fac-gallery .fac-gallery-item.fac-gallery-selected {
	z-index: 2;
}
.mainVisualSS .fac-gallery .fac-gallery-item.selected {
	animation: mainVisualSS-zoom 8s linear 0s forwards;
	opacity: 1;
}
.mainVisualSS .fac-gallery .fac-gallery-nav {
	display: none;
}

@keyframes mainVisualSS-zoom {
0% {
 transform: scale(1);
}
100% {
 transform: scale(1.15);
}
}

@media screen and (max-width: 736px) {

.wrapper {
	padding-top: 0; 
}

.mainVisualSS {
	height: 320px;
	min-height: 0;
}
.mainVisualSS_item {
	height: 320px;
	min-height: 0;
}
.mainVisualSS_item:nth-child(n+2) {}
.mainVisualSS_item img {}
.mainVisualSS .fac-gallery {}
.mainVisualSS .fac-gallery .fac-gallery-slider {}
.mainVisualSS .fac-gallery .fac-gallery-item {}
.mainVisualSS .fac-gallery .fac-gallery-item img {}
.mainVisualSS .fac-gallery-initialize .fac-gallery-item {}
.mainVisualSS .fac-gallery .fac-gallery-item.fac-gallery-selected {}
.mainVisualSS .fac-gallery .fac-gallery-item.selected {}
.mainVisualSS .fac-gallery .fac-gallery-nav {}

}
</style>
<script src="/content/100864674.js"></script>
<script>
$(function () {
	
	//.mainVisualSS
	var gallery = new fac.GalleryV2(".mainVisualSS > ul", {duration:2.0, wait:5.0, auto:true, nav:false});
	
	$(gallery)
		.on(fac.GalleryV2.ON_SELECT, function (event, select, last_select) {
			
			gallery.element_items.eq(select)
				.addClass("selected");
			
		})
		.on(fac.GalleryV2.ON_SELECTED, function (event, select, last_select) {
			
			setTimeout(function () {
				
				gallery.element_items.eq(last_select)
					.removeClass("selected");
					
			}, 2000);
			
		});
	
	var area           = $(".mainVisualSS");
	var gallery_images = gallery.element_items.find("img");
	var image_width    = parseInt(gallery_images.eq(0).attr("width"));
	var image_height   = parseInt(gallery_images.eq(0).attr("height"));
	
	function onResize () {
		
		var area_width   = area.width();
		var area_height  = area.height();
		var width_scale  = area_width  / image_width;
		var height_scale = area_height / image_height;
		var scale        = Math.max(width_scale, height_scale);
		var width        = Math.round(image_width  * scale);
		var height       = Math.round(image_height * scale);
		var left         = Math.round((area_width  - width)  / 2);
		var top          = Math.round((area_height - height) / 2);
		
		gallery_images
			.css({width:width, height:height, left:left, top:top});
		
	}
	
	$(window).on("resize orientationchange", onResize);
	onResize();
	
	gallery.start();
	
});
</script>




