<script>

    function showVideos(videos) {
        var template = $('#video-template').html();
        var max = 3;
        var chunks = [];
        var count = 0;

        while (count != 1) {
            chunks.push(videos.splice(0, max));
            count++;
        }

        for (i in chunks) {
            var chunk = chunks[i];
            var list = $('<ul />', {'class': 'products video-sidebar clearfix'});

            for (var j = 0; j < chunk.length; j++) {
                // if (j == 1){
                //   template = $('#video-link').html();
                // }
                var item = chunk[j];
                var data = {};

                data.html_class = '';
                if (j == 0) data.html_class = 'first';
                if (j == chunk.length - 1) data.html_class = 'last';
                data.title = item.snippet.title;
                data.thumbnail = item.snippet.thumbnails.high.url;
                data.url = "https://www.youtube.com/watch?v=" + item.snippet.resourceId.videoId;

                var publishedAt = new Date(item.snippet.publishedAt);

                data.publishedAt = publishedAt.getDate() + "." + (publishedAt.getMonth() + 1) + "." + publishedAt.getFullYear() + " " + publishedAt.getHours() + ":" + publishedAt.getMinutes();

                var rendered = Mustache.render(template, data);

                list.append(rendered);
            }
            $('.video-sidebar-wrapper .spinner-wrapper').remove();
            $('.video-sidebar-wrapper').prepend(list);
            $('.video-sidebar-wrapper .show-more-videos').removeClass('hidden');
        }
    }

    function onGoogleLoad2() {
        gapi.client.setApiKey('AIzaSyDb8wDs_p_wi1wTnBztvOcLKkl9HLL4xZ0');
        gapi.client.load('youtube', 'v3', function () {

            var request = gapi.client.youtube.channels.list({
                id: 'UC51n0kPREVzryruUQLUDyvQ',
                part: 'snippet,contentDetails',
            });

            request.execute(function (response) {

                playlistId = response.result.items[0].contentDetails.relatedPlaylists.uploads;
                channelId = response.result.items[0].id;

                var requestOptions = {
                    playlistId: playlistId,
                    part: 'snippet,id',
                    maxResults: 3
                };

                var request = gapi.client.youtube.playlistItems.list(requestOptions);
                request.execute(function (response) {
                    if (response.items.length > 0) {

                        var items = response.items;
                        var videos = jQuery.extend(true, [], items);
                        showVideos(videos);
                        if (Modernizr.localstorage && JSON) {
                            var to_store = {};
                            to_store.timestamp = new Date().getTime();
                            to_store.items = items;
                            localStorage.setItem("itnews_videos_sidebar_new", JSON.stringify(to_store, null, ' '))
                        }
                    }
                });
            });
        });
    }

    $(function () {
        function loadDataApi() {
            $.getScript("https://apis.google.com/js/client.js?onload=onGoogleLoad2");
        }

        if (Modernizr.localstorage) {
            var stored = localStorage.getItem('itnews_videos_sidebar_new');

            if (stored) {
                stored = $.parseJSON(stored);
                var diff = Math.round((new Date().getTime() - new Date(stored.timestamp).getTime()) / 1000); // in seconds
                var until = 60 * 60 * 12 * 1; // 0,5 day

                if (until > diff) {showVideos(stored.items);
                } else {loadDataApi();}

            } else {loadDataApi();}

        } else {loadDataApi();}
    });
</script>

