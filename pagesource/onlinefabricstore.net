    <!--Box slider controls -->
    <script type="text/javascript">
        var activeID = "pic1";
        var stoploop = false;
        var interval = null;
        var intervalLong = null;

        //$(document).ready(function () {
        //$("[id^=pic]").hide();

        $(document).ready(function () {
            var campaign = 0;
            if ($('ul.bxslider').length > 0) {
                campaign = 1;
                $("#pic1").fadeIn(40000);

                $(".circleFill").click(function () {
                    stoploop = true;
                    SelectOverRide(this);
                });
                $(".circleEmpty").click(function () {
                    stoploop = true;
                    SelectOverRide(this);
                });
                $('ul.bxslider li').removeAttr("style");
                MainSlider = $('.bxslider').bxSlider({
                    speed: 1500
                });
                MainSlider.startAuto();
            }
        });
        /*
        $(window).load(function () {
    
            console.log(('ul.bxslider').length);
            if (eval(campaign)!=1) {
                MainSlider.startAuto();
            }
        });
        */
        function SelectOverRide(e) {
            if (e.id == "circle1") {
                activeID = "pic3";
            } else if (e.id == "circle2") {
                activeID = "pic1";
            } else if (e.id == "circle3") {
                activeID = "pic2";
            }
            slideSwitch();
        }
        function slideSwitchLongWait() {
            stoploop = false;
            clearInterval(intervalLong);
            interval = setInterval("slideSwitch()", 15000);
        }
        function slideSwitch() {
            $("[id^=pic]").fadeOut(1000);
            if (activeID == "pic1") {
                activeID = "pic2";
                $("#pic2").addClass('activeH');
                $("#pic1").removeClass('activeH');
                $("#pic2").fadeIn(4000);
                ClearSelectButtons();
                $("#circle2").addClass('circleFill');
            }
            else if (activeID == "pic2") {
                activeID = "pic3";
                $("#pic3").addClass('activeH');
                $("#pic2").removeClass('activeH');
                $("#pic3").fadeIn(4000);
                ClearSelectButtons();
                $("#circle3").addClass('circleFill');
            }
            else if (activeID == "pic3") {
                activeID = "pic1";
                $("#pic1").addClass('activeH');
                $("#pic3").removeClass('activeH');
                $("#pic1").fadeIn(4000);
                ClearSelectButtons();
                $("#circle1").addClass('circleFill');
            }

            if (stoploop == true) {
                clearInterval(interval);
                if (intervalLong != null) { clearInterval(intervalLong); }
                intervalLong = setInterval("slideSwitchLongWait()", 15000);
            }

        }

        $(function () {
            interval = setInterval("slideSwitch()", 15000);
        });

        function ClearSelectButtons() {
            $("[id^=circle]").removeClass('circleFill');
            $("[id^=circle]").addClass('circleEmpty');
        }

        function runCampaign() {
            //if a campaign is running, set the dates and activate var today
            var today = getESTDay();
            return (today = "1" || today == "2" || today == "3");
        }

        function getESTDay() {
            ESTDate = convertToEST();
            return ESTDate.getDate();
        }

        function convertToEST() {
            //EST
            offset = -4.0;
            clientDate = new Date();
            utc = clientDate.getTime() + (clientDate.getTimezoneOffset() * 60000);
            serverDate = new Date(utc + (3600000 * offset));
            return (serverDate);
        }
    </script>


