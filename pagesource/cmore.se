  <head><script>
      function isCmoreUser() {
          var decodedCookie = decodeURIComponent(document.cookie);
          var ca = decodedCookie.split(';');
          for(var i = 0; i <ca.length; i++) {
              var c = ca[i];
              while (c.charAt(0) == ' ') {
                  c = c.substring(1);
              }
              if (c.indexOf('cmoreAuthToken=') == 0 || c.indexOf('rememberMe=') == 0) {
                  return true;
              }
          }
          return false;
      }

      if (isCmoreUser()) {
        window.location = '/start'+window.location.hash;
      }
