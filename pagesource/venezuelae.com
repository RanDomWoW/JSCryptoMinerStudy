<script type="text/javascript">
$(document).ready( function() {

	

	$(".lugar").on("click", ".menu_mostrar", function(e) {
		var rel = $(this).attr("data-rel");
		$("#"+rel).simplemodal({
			closeHTML : "<a href='#' title='Cerrar' class='modal-close'><span class='icono-x'></span></a>",
			opacity : 70,
			overlayClose : true,
			autoResize : true,
			minWidth : 220
		});
	});

	

	$("img.lazy").lazyload({
		failure_limit : 1000
	});

});



</script>
