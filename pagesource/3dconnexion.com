    <script id="my3dx__loader" type="text/template">
        <div class="my3dx__loader-container">
            <div class="my3dx__loader-outer">
                <div class="my3dx__loader-inner">
                    <div class="my3dx__loader">
                        <svg class="my3dx__loader-circular" viewBox="25 25 50 50">
                            <circle class="my3dx__loader-path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10"/>
                        </svg>
                    </div>
                </div>
            </div>
        </div>
    </script>

    <script id="my3dx__success" type="text/template">
        <div class="my3dx-welcome__success">
            <!--<em class="my3dx-welcome__success-title">
                {{title}}
            </em>-->
            <p class="my3dx-welcome__success-message">
                {{message}}
            </p>
        </div>
    </script>

    <script id="my3dx__errors" type="text/template">
        <div class="my3dx-welcome__errors">
            <!--<em class="my3dx-welcome__errors-title">
                {{title}}
            </em>-->
            <p class="my3dx-welcome__errors-message">
                {{message}}
            </p>
        </div>
    </script>

    <script id="my3dx__form-row" type="text/template">
        <div class="my3dx-welcome__form-row{{items}}">
            {{content}}
        </div>
    </script>

    <script id="my3dx__form-input" type="text/template">
        <input class="my3dx-welcome__form-input" id="{{name}}" name="{{name}}" value="{{value}}" {{type}} {{required}} />
    </script>

    <script id="my3dx__form-checkbox" type="text/template">
        <div class="my3dx-welcome__form-item"{{data_required}}>
            <div class="my3dx-account__form-checkbox">
                <input class="my3dx-account__form-checkbox-element" type="checkbox" name="{{name}}" id="{{name}}" {{checked}} {{required}} />
                <label class="my3dx-account__form-checkbox-label" for="{{name}}">
                    <span></span>
                    {{label}}
                </label>
            </div>
        </div>
    </script>
	
	<script id="my3dx__form-link" type="text/template"><a class="my3dx-welcome__form-link{{klass}}" href="{{href}}"{{accordion_view}}{{targetBlank}}>{{label}}</a></script>
	<script id="my3dx__form-item" type="text/template"><div class="my3dx-welcome__form-item"{{data_required}}><label class="my3dx-welcome__form-label" for="{{name}}"{{tooltip}}>{{label}}</label>{{input}}{{link}}</div></script>

    <script id="my3dx__form-notice" type="text/template">
        <p class="my3dx-welcome__form-notice">
            {{content}}
        </p>
    </script>

    <script id="my3dx__form-cancel-button" type="text/template">
        <button class="my3dx-welcome__form-button cancel-button" type="button">{{label}}</button>
    </script>

    <script id="my3dx__form-tooltip-title" type="text/template">
        data-tooltip-title="{{title}}"
    </script>

    <script id="my3dx__form-tooltip" type="text/template">
        data-tooltip="{{text}}"
    </script>
	<script id="my3dx__form-select" type="text/template">
        <div class="my3dx-welcome__form-select-wrap">
            <select class="my3dx-welcome__form-select" id="{{name}}" name="{{name}}" {{type}} {{required}}>
                {{options}}
            </select>
        </div>
    </script>
	<script id="my3dx__form-multiselect" type="text/template">
        <div class="my3dx-form__multiselect" data-options='{{data}}'>
            <div class="my3dx-form__multiselect-inner">
                <div class="my3dx-form__multiselect-wrap">
                    <select class="my3dx-form__multiselect-element" id="{{name}}[select]" name="{{name}}" type="select" {{required}}>
                        {{select_options}}
                    </select>
                </div>
                <button class="my3dx-form__multiselect-button js-my3d-multiselect-add" type="button">+</button>
            </div>
            <div class="my3dx-form__multiselect-options">
                {{options}}
            </div>
        </div>
    </script>

    <script id="my3dx__form-multiselect-option" type="text/template">
        <input type="hidden" name="{{name}}[options][]" value="{{value}}" />
        <span class="my3dx-form__multiselect-option" data-option-value="{{value}}">{{label}}</span>
    </script>
