<script type="text/javascript">
$( document ).ready(function() {
	setHeightsToMax('blog-text');
	setHeightsAndWidthsToMaxWidth('blog-img');
});

function setHeightsToMax(className) {
    var elements = $('.' + className);
    var maxHeight = 0;
    
    elements.each(function( index ) {
    	var currentHeight = parseInt($(this).height());
		if( currentHeight > maxHeight ) {
			maxHeight = currentHeight;
		} 
    });
    
    elements.each(function( index ) {
		$(this).css('height', maxHeight + 'px');
	});
}
function setHeightsAndWidthsToMaxWidth(className) {
    var elements = $('.' + className);
    var maxWidth = 0;
    
    elements.each(function( index ) {
    	var currentWidth = parseInt($(this).width());
		if( currentWidth > maxWidth ) {
			maxWidth = currentWidth;
		} 
    });
    
    elements.each(function( index ) {
		$(this).css('width', maxWidth + 'px');
		$(this).css('height', maxWidth + 'px');
	});
}
</script>

