                    var long=getCookie_weather_widge("weather_set_longitude","0.1275");
                      lat=parseFloat(lat);
                      lat=(lat+180);
                      lat=lat.toFixed(2);
                      lat=parseFloat(lat)*100;
                      long=parseFloat(long)+180;
                      long=long.toFixed(2);
                      long=parseFloat(long)*100;
                      var cache_no=lat.toString()+long.toString();
                    $.ajax
                    (
                    {
                        type: "POST",
                        url:"/weatherajax_"+cache_no ,
                        data:
                    {
                        "ajax_weather_widge": true,
                        "cache_number":cache_no,

