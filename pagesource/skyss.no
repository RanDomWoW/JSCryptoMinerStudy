<script type="text/javascript">

    var mediumBreak = 560;

    function scrollbarWidth() {
        var div =
            $('<div style="width:50px;height:50px;overflow:hidden;position:absolute;top:-200px;left:-200px;"><div style="height:100px;"></div>');
        $('body').append(div);
        var w1 = $('div', div).innerWidth();
        div.css('overflow-y', 'scroll');
        var w2 = $('div', div).innerWidth();
        $(div).remove();
        return (w1 - w2);
    }

    function getDevice() {
        var actualWidth = $(window).width() + scrollbarWidth();
        if (actualWidth >= mediumBreak) {
            return "desktop";
        } else {
            return "mobile";
        }
    }

    function replacePhoneNumbers() {
        var data = $('.phone');
        if (data.length > 0) {
            try {
                var device = getDevice();
                $(data)
                    .each(function(i, element) {
                        var phoneNumberText = element.innerHTML;
                        var phoneNumber = phoneNumberText.replace(/\D/g, '');
                        var html = phoneNumberText;
                        var renameValue = $(this).hasClass('rename');

                        if (device === "mobile") {
                            if (renameValue) {
                                phoneNumberText = "Ring";
                            }
                            html = '<a href="tel://' + phoneNumber + '">' + phoneNumberText + '</a>';
                        }

                        element.innerHTML = html;
                    });
            } catch (e) {
                console.log(e);
            }
        }
    }

    replacePhoneNumbers();
</script>


