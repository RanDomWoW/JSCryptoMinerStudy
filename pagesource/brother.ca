    //master datalayer json function
    function masterDataLayerObj(user, pageContext) {
        var navData = [];
        
        //Read cookie
        var cookieData = {};
        document.cookie.split("; ").map(function(value)
        { 
            if( value.split("=")[0].indexOf("userAge") != -1)
                cookieData[value.split("=")[0]] = value.split("=")[1];
        });

        if(window.getPreLoadJSON("navigation")) {
            window.getPreLoadJSON("navigation").map(function (v, i) {
                if (i > 1) navData.push(v.name);
            });  
        }
        
        
        
        return JSON.stringify({  
            page: {    
                "gaStep": "",
                "requestURI": window.location.pathname,
                "language": (window.maxmindCookie ? (window.maxmindCookie.siteLang === "English" ? "en" : "fr"): ""),
                "province": (window.maxmindCookie ? window.maxmindCookie.siteProvince : ""),
                "contentCategory": pageContext.pageType,
                "contentSubcategoryLevel1": (navData && navData.length > 3 ? navData[navData.length-2] : ""),
                "contentSubcategoryLevel2": (navData && navData.length > 4 ? navData[navData.length-3] : ""),
                "contentPage": (pageContext.productCode != undefined ? pageContext.productCode : (pageContext.title != undefined ? pageContext.title : pageContext.pageType ) ),
                "scrollTracking": ""
            },
            user: {
                "userType": "",    
                "userHasAccount": (!user.isAnonymous ? "yes" : "no"),
                "ageBracket": (cookieData && cookieData.userAge != "317" ? cookieData.userAge : ""),
                "userID": user.accountId
            }
        });
    }

    //create data layer here. main function where data will be pushed to dataLayer
    var pushDataLayer = function (useCaseId, nonPVData, nonPVValue, gaStep,isUser,isPage) {
        var newMasterDLObj = JSON.parse(masterDataLayerObj(userObj, pageContextObj));

        if(isUser === false) {
            delete newMasterDLObj.user;
        }
        
        if(isPage === false) {
            delete newMasterDLObj.page;
        }
        
        if (useCaseId && useCaseData[useCaseId]) {
            newMasterDLObj.useCase = useCaseData[useCaseId];
            if (gaStep) {
                newMasterDLObj.page.gaStep = gaStep;
            }
            if (nonPVValue && nonPVValue.length > 0) {
                nonPVValue.map(function (v, i) {
                    newMasterDLObj[v] = nonPVData[i];
                });
            }
            
            if(!newMasterDLObj.event) {
                newMasterDLObj.event = "PV";
            }
            window.dataLayer.push(newMasterDLObj);
        }

    };

    return pushDataLayer;

