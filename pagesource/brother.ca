<script type='text/javascript'>
var myVar;
var h = 0;
var targetRight = 5;
var CallDelay = 10;
var moveDelta = 5;
var panel;

//Will get called by the engagement engine once "rn_Dialog_0" exists (means a chat has been offered)
function Determine(){
	h = -305;
	panel = document.getElementById("rn_Dialog_0")
	if(panel != null){
		panel.parentElement.style.zIndex = "50";
		myVar = setInterval(MovePanel, CallDelay );		
	}	
}

//Gets called repeatedly to slide the chat panel in the viewport. 
function MovePanel() {
    h += moveDelta;
    panel.style.right = h + "px";
    if(h >= targetRight){
		panel.style.right = targetRight + "px";
		clearInterval(myVar);
    }
}
</script> 

<script type="text/javascript">
	var cfn = "Guest";
	var cln = "Guest";
	var cea = "proactive@brother.ca";
	
	function getEEU(){
		var container = document.getElementById("data-mz-preload-user");
		if(container == null){
			return;
		}
		if(container.innerHTML == ""){
			return;
		}
		
		var jContainer = JSON.parse(container.innerHTML);
		
		if(jContainer.hasOwnProperty('email') == false){
			return;
		}	
		
		if(jContainer.email == ""){
			console.log("Logged in as Guest");
		}
		else{
			cfn = jContainer.firstName;
			cln = jContainer.lastName;
			cea = jContainer.email;
		}
	}
</script>

