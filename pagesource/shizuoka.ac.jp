<script type="text/javascript">
$(function(){
  var slide_common_itemNum = $('#slide_common .slides').find("li").length;
  var slide_common_startAt = Math.floor(Math.random() * slide_common_itemNum);
  $('#slide_common').flexslider({
    controlNav:false,
    slideshow:true,
    animationSpeed:300,
    slideshowSpeed:5000,
    startAt: slide_common_startAt,
    start: function(){
      change_pageNum_slide_common();
    },
    after: function(){
      change_pageNum_slide_common();
    }
  });

  $('#slide_main').flexslider({
    controlNav: "thumbnails",
    directionNav:false,
    animationSpeed:300,
    slideshowSpeed:3000,
    randomize:true,
    start: function(){
      change_slide_menu_class();
      $("#slide_menu").find("li").click(function(){
        var clickGakubu = $(this).attr("data-gakubu");
        $(".flex-control-thumbs").find("img[src*=" + clickGakubu + "]").trigger("click");
      });
    },
    after: function(){
      change_slide_menu_class();
    }
  });
  function change_slide_menu_class() {
    $("#slide_menu").find("li").removeClass("flex-active");
    var activeGakubu = $("#slide_main").find(".flex-active-slide").attr("data-gakubu");
    $("#slide_menu").find("li[data-gakubu='" + activeGakubu + "']").addClass("flex-active");
  }
  function change_pageNum_slide_common() {
    var index = $('#slide_common .slides').find("li.flex-active-slide").index();
    var pageNum = index - slide_common_startAt;
    if (pageNum < 0) {
      pageNum = pageNum + slide_common_itemNum;
    }
    pageNum = pageNum + 1;
    $("#slide_common .page").text(pageNum + "/" + slide_common_itemNum);
  }
});
</script>

