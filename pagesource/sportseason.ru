</div>

<script type="text/javascript">
var base_domain = 'sportseason.ru';

$(function() {

  // Клик на Да или Закрыть
  $('#city_approve_popup .buttons .yes, #city_approve_popup .close').click(function(e) {
    e.preventDefault();
    $('#city_approve_popup').slideUp('fast');
    // пишем кукиз
    var date = new Date(new Date().getTime() + 31536000);
    var host = location.hostname.split('.');
    if (host.length == 3) host = host[1] +'.'+ host[2];
    else host = host[0] +'.'+ host[1];
    document.cookie = "sportseason_city_approved=1; domain=."+ host +"; path=/; expires=" + date.toUTCString();
  });

  // Клик на Изменить
  $('#city_approve_popup .buttons .no').click(function(e) {
    e.preventDefault();
    $('#city_approve_popup').slideUp('fast');
    $("#city_change").trigger('click');
  });

  // Клик на городе для изменения
	$("#city_change").click(function(e) {
    e.preventDefault();

    // Определяем ширину блока (для мобильных)
    if ($(window).width() <= 600 ) boxWidth = 270;
    else boxWidth = 330;
    // Показываем попап
		$.colorbox({inline:true, href:"#city_select_popup", opacity:0.5, overlayClose: true, escKey: true, closeButton: false, close: '', initialWidth: 50, initialHeight: 50, innerWidth: boxWidth,});

    // Автоподстановка города
    $("#city_select_form #city").kladr({
      type: 'city',
      verify: true,
      withParents: true,
      // форматирование списка подстановки
      labelFormat: function (obj, query) {
        var objs;

        var label = '',
          name,
          objName,
          queryName,
          start;

        if (obj.typeShort) {
          label += obj.typeShort + '. ';
        }

        name = obj.name;
        objName = name.toLowerCase();
        queryName = query.name.toLowerCase();
        start = objName.indexOf(queryName);
        start = ~start ? start : 0;

        if (queryName.length < objName.length) {
          label += name.substr(0, start);
          label += '<strong>';
          label += name.substr(start, queryName.length);
          label += '</strong>';
          label += name.substr(start + queryName.length);
        } else {
          label += '<strong>' + name + '</strong>';
        }

        if (obj.parents && 1==1) {
          $.each(obj.parents, function(k, v) {
            label += ', '+ v.name +' '+ v.typeShort +'.'
          });
        }

        return label;
      },
      // при выборе города
      change: function (obj) {
        if (!obj) return;
        // пишем индекс если есть
        postcode = '';
        if (obj.zip) postcode = obj.zip;
        $("#city_select_form #postcode").val(postcode);
        // пишем регион если есть
        $("#city_select_form #region").val('');
        if (obj.parents) {
          $.each(obj.parents, function(k, v) {
            if (v.contentType == 'region') {
              $("#city_select_form #region").val(v.name +' '+ v.type.toLowerCase());
            }
          });
        }
      },
    });

	});

  // Клик на быстром выборе города
  $("#city_select_popup .fast_choice a").click(function(e) {
    e.preventDefault();
    $("#city_select_form #region").val($(this).attr('data-region'));
    $("#city_select_form #postcode").val($(this).attr('data-postcode'));
    $("#city_select_form #city").kladr('verify', false);
    $("#city_select_form #city").val($(this).text());
    $("#city_select_form").submit();
  });

	// Сохранение данных
	$("#city_select_form").submit(function(e) {
		e.preventDefault();
		$("#city_select_form #city").trigger('blur');
		$("#city_select_btn").attr('disabled', '1').val('Сохраняем...');

		// Ждем ответа от КЛАДР
		setTimeout(function() {

      // Проверка города
      if ($("#city_select_popup #city").val() == '') {
        alert('Укажите пожалуйста город!');
        $("#city_select_btn").removeAttr('disabled').val('Сохранить');
        return false;
      }
      if ($("#city_select_popup #city").hasClass('kladr-error')) {
        alert('Такой город не найден! Проверьте пожалуйста свой ввод.');
        $("#city_select_btn").removeAttr('disabled').val('Сохранить');
        return false;
      }

      // Сохранение
      jQuery.getJSON('/city/save/', jQuery("#city_select_form").serialize(), function(data) {
        if (true === data.success) {
          // Успешно
          $("#city_change").text($("#city_select_popup #city").val());
          // Переадресация
          if (data.redirect) {
            newUrl = '//';
            newUrl += data.redirect;
            newUrl += location.pathname;
            location.assign(newUrl);
          } else {
            location.reload();
          }
        } else {
          // Ошибка
          alert(data.message);
          $("#city_select_btn").removeAttr('disabled').val('Сохранить');
          return false;
        }
      });

		}, 1000);
	});
});
</script>    </div>
