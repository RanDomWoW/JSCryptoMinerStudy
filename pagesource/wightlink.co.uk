          var dropdownMenu = dropdown.find('ul.sub-menu');


          dropdownLink.on('click', function() {

            // uncomment to close other mobile sub-navs when opening another
            //$('ul.sub-menu').removeClass('active');

            dropdown.toggleClass('active');
            dropdownMenu.toggleClass('active');

            $('.menu-item-has-children').not(dropdown).removeClass('active');
            $('ul.sub-menu').not(dropdownMenu).removeClass('active');
          })
        })

        $(window).resize(function() {
          if($(window).width() > 1024) {
            $('.mobile-menu, .mobilemenu-container__menu__icon').removeClass('active');
            $('body').removeClass('menu-active');
            $('.mobilemenu-container__menu').find('p').text('Menu');
          }
        })

        $('.menu-item-has-children').first().addClass('active');
        $('.menu-item-has-children').first().find('.sub-menu').addClass('active');
      });
    </script>

