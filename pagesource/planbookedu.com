<script type="text/javascript">
	$(function(){
		var $reviews = $(".review");
		var reviewTimeout = null;

		var reviewI = Math.floor(Math.random() * $reviews.length);
		$reviews.hide().removeClass("active").addClass("inactive");
		$($reviews[reviewI]).show().removeClass("inactive").addClass("active");
		reviewTimeout = setTimeout(function() {
			nextReview();
		}, 15000);

		$(".nextReview").click(function(e) {
			e.preventDefault();
			nextReview();
		});

		function nextReview() {
			clearTimeout(reviewTimeout);
			var height = $("#reviews").height();
			$("#reviews").height(height);
			$("#reviews .active").removeClass("active").addClass("inactive").fadeOut(function() {
				reviewI ++;
				if(reviewI >= $reviews.length) {
					reviewI = 0;
				}

				$("#reviews").height("auto");
				$($reviews[reviewI]).fadeIn().removeClass("inactive").addClass("active");

				reviewTimeout = setTimeout(function() {
					nextReview();
				}, 15000);
			});
		}
	});
</script>

