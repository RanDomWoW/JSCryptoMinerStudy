<script type="text/javascript">
var base_url = 'https://www.oxo.com/';
function load_sc_cc_popup(){
var sc_cc_popup_delay_time=5*1000;
setTimeout(function(){
sc_cc_loadPopup();
}, sc_cc_popup_delay_time); // .5 second
return false;
}
function sc_cc_close_popup(){
sc_cc_disablePopup();
}
/* event for close the popup */
jQuery("div.close").hover(
function() {
jQuery('span.ecs_tooltip').show();
},
function () {
jQuery('span.ecs_tooltip').hide();
}
);
jQuery(this).keyup(function(event) {
if (event.which == 27) { // 27 is 'Ecs' in the keyboard
sc_cc_disablePopup(); // function close pop up
}
});
jQuery("div#ccbackgroundPopup").click(function() {
sc_cc_disablePopup(); // function close pop up
});
/************** start: functions. **************/
function loading() {
jQuery("div.loader").show();
}
function sc_cc_closeloading() {
jQuery("div.loader").fadeOut('normal');
}
var popupStatus = 0; // set value
function sc_cc_loadPopup() {
if(popupStatus == 0) { // if value is 0, show popup
sc_cc_closeloading(); // fadeout loading
jQuery("#cc_toPopup").fadeIn(0500); // fadein popup div
//jQuery("#backgroundPopup").css("opacity", "0.7"); // css opacity, supports IE7, IE8
jQuery("#ccbackgroundPopup").fadeIn(0001);
popupStatus = 1; // and set value to 1
}
}
function sc_cc_disablePopup() {
if(popupStatus == 1) { // if value is 1, close popup
jQuery("#cc_toPopup").fadeOut("normal");
jQuery("#ccbackgroundPopup").fadeOut("normal");
popupStatus = 0; // and set value to 0
}
}
//custom
function required_sc_cc_popup(){
jQuery("#cc_close").hide();
jQuery(document).keyup(function(e) {
if (e.keyCode == 27) {return false }
});
}
/************** end: functions. **************/
function sc_cc_ClearPlaceHolder2 (input) {
if (input.value == input.defaultValue) {
input.value = "";
}
}
function sc_cc_SetPlaceHolder2 (input) {
if (input.value == "") {
input.value = input.defaultValue;
}
}
function sc_cc_submit(){
//alert('test');
jQuery('.cc_email_error').html('');
var fname = jQuery('#ccfname').val();
var lname = jQuery('#cclname').val();
var email = jQuery('#ccemail').val();
var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
if(!emailReg.test( email )){
jQuery('.cc_email_error').html('Invalid Email');
return false;
}
var sc_ccobj = {'fname':fname,'lname':lname,'email':email};
jQuery.ajax({
type:'GET',
url: base_url+'scconstantcontact/index?sccc=true&ccobj='+JSON.stringify(sc_ccobj),
success: function(data){
var sts = JSON.parse(data);
jQuery('#ccmsg').html(sts.msg);
if(sts.ccstatus==1){
setTimeout(function(){
sc_cc_disablePopup();
}, 4000);
}
}
});
return false;
}
</script>
