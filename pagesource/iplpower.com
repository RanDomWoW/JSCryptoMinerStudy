 <script type="text/javascript">

     $(function () {
         $("#UserName, #Password").keypress(function (e) {
             if ((e.which && e.which == 13) || (e.keyCode && e.keyCode == 13)) {
                 if ($("#UserName").val().length > 0) {
                     $("#LoginButton").click();
                 }
                 return false;
             }
         });

         $("#LoginButton").on("click", function () {
             var usernameSupplied = validateTextField($("#UserName"));
             var passwordSupplied = validateTextField($("#Password"));

             return usernameSupplied && passwordSupplied;
         });


         $("#UserName").on("change", function () {
             validateTextField($(this));
         });

         $("#Password").on("change", function () {
             validateTextField($(this));
         });


         $("#LoginButton").on("click", function () {
             var usernameSupplied = validateTextField($("#UserName"));
             var passwordSupplied = validateTextField($("#Password"));

             return usernameSupplied && passwordSupplied;
         });

     });

     function validateTextField(field) {
         if (jQuery.trim(field.val()).length == 0) {
             field.attr("placeholder", field.attr("data-error-msg"));
             field.addClass("error");
             return false;
         }
         else {
             field.attr("placeholder", field.attr("data-msg"));
             field.removeClass("error");
             return true;
         }
     }

 </script>
