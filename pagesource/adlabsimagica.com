			// 	$('#bookingWidgetDate').parent().css({'visibility':'visible'});
			// 	break;
			case 'novotel-imagica-khopoli':
				$('#bookingWidgetSelect option:eq(3)').prop('selected', true);
				$('#bookingWidgetDate').prop('disabled', true);
				// $('#bookingWidgetDate').parent().css({'display':'none'});
				break;
			default:
				$('.bookingWidgetSelect option:eq(0)').prop('selected', true);
				// $('#bookingWidgetDate').parent().css({'visibility':'visible'});
