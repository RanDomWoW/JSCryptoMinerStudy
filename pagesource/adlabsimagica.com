		<script type="text/javascript">
	$(document).ready(function() {
		$('#bookingWidgetSelect').on('change', function(e) {
			if ($('option:selected', this).val() == 'hotel') {
				$('#bookingWidgetDate').prop('disabled', true);
				$('#bookingWidgetDate').parent().css({'visibility':'hidden'});
			} else {
				$('#bookingWidgetDate').prop('disabled', false);
				$('#bookingWidgetDate').parent().css({'visibility':'visible'});
			}
		});
		preSelectWidget();
		// scrollDisplay();
		$('.widgetActionButton').on('click', function() {
			widget_date = $('[name="widget_date"]', '#bookingWidgetForm').val();
			dest = $('#bookingWidgetSelect').val();
			protocol = 'https://';
			params = '?dest=' + dest + '&date_visit=' + widget_date + '&date_departure=' + widget_date + '&total_day=1' + '&adult=1&child=0&sc=0&college=0&param=3';
			switch (dest) {
				case '34':
					bookingDomain = 'beta-aquamagica.adlabsimagica.com/';
					bookingDomain = 'book-waterpark.adlabsimagica.com/';
					bookingTargetUrl = protocol + bookingDomain + params;
					break;
				case '7754':
				case '7732':
					bookingDomain = 'beta-snomagica.adlabsimagica.com/';
					bookingDomain = 'book-snowpark.adlabsimagica.com/';
					bookingTargetUrl = protocol + bookingDomain + params;
					break;
				case 'hotel':
					bookingDomain = 'beta-hotels.adlabsimagica.com/';
					bookingDomain = 'book-hotel.adlabsimagica.com/';
					bookingTargetUrl = protocol + bookingDomain;
					break;
				case '7781':
				case '7756':
					bookingDomain = 'beta-combo.adlabsimagica.com/';
					bookingDomain = 'book-multipark.adlabsimagica.com/';
					bookingTargetUrl = protocol + bookingDomain + params;
					break;
				case 33:
				default:
					bookingDomain = 'beta-imagica.adlabsimagica.com/';
					bookingDomain = 'book.adlabsimagica.com/';
					bookingTargetUrl = protocol + bookingDomain + params;
					break;
			}
			window.location.href = bookingTargetUrl;
		})
	});
