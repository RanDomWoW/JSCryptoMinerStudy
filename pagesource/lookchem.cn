    <script type="text/javascript">
        $(function () {
            $("#slider").responsiveSlides({
                auto: true,
                pager: false,
                nav: true,
                speed: 500,
                // 对应外层div的class : slide_container
                namespace: "slide"
            });
            $(".slide_nav").hide();
            $("#slider,.slide_nav").hover(function () {
                $(".slide_nav").show();
            }, function () {
                $(".slide_nav").hide();
            });
            var dialoglogin;
            $(".publish").click(function () {
                $.ajax({
                    type: "POST",
                    url: '/ajax/submit_ajax.ashx',
                    dataType: "json",
                    async: false,
                    data: {
                        "act": "Account"
                    },
                    success: function (data) {
                        if (data.status == "n") {
                            dialoglogin = dialog({
                                title: "立即登录",
                                url: "/Ajax/dialog_Login.aspx?IsParent=1&u=http%3a%2f%2fwww.lookchem.cn%2f&dt=" + new Date(),
                                width: 360
                            }).showModal();
                            return;
                        } else {
                            var casno = $("input[name=casno]");
                            var proname = $("input[name=proname]");
                            var quantity = $("input[name=quantity]");
                            var unit = $('input[name="unit"]:checked ').val();
                            var validitydata = $('input[name="validdate"]:checked ').val();
                            var txtnote = $("textarea[name=txtnote]");
                            if (proname.val().length < 2) {
                                proname.focus();
                                util.showModel("请输入品名");
                                return;
                            }
                            if (quantity.val().length == 0 || !(/^[0-9]*$/.test(quantity.val()))) {
                                quantity.focus();
                                util.showModel("请输入采购数量或正确的数字");
                                return;
                            }
                            if (txtnote.val().length < 5) {
                                txtnote.focus();
                                util.showModel("请输入备注信息，不得少于5字！");
                                return;
                            }
                            if (txtnote.val().length >= 5) {
                                $.post('/ajax/dialog_Buy.aspx', {
                                    "act": "save",
                                    "note": txtnote.val(),
                                    "casno": casno.val(),
                                    "proname": proname.val(),
                                    "quantity": quantity.val(),
                                    "unit": unit,
                                    "validitydata": validitydata
                                }, function (result) {
                                    util.showUrlModel(result, "/");
                                }, "text");
                            } else {
                                txtnote.focus();
                                util.showModel("请在文本框内输入报价信息，不得少于5字符！");
                            }
                        }
                    }
                });
            });
        });
    </script>

    
