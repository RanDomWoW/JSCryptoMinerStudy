<script>
	function shuffleAD_s(){
    //念のため、全部非表示にする
    $('#ad-type-s .bnr-child').hide();
    // リストをシャッフルする
    var item = $('#ad-type-s .bnr-child').get().sort(function() {
      return Math.round(Math.random()) - 0.5;
    }).slice(0, 1);
    // ランダムで1つだけ抽出して表示
    $(item).show();
  }
	function shuffleAD_b(){
    //念のため、全部非表示にする
    $('#ad-type-b .bnr-child').hide();
    // リストをシャッフルする
    var item = $('#ad-type-b .bnr-child').get().sort(function() {
      return Math.round(Math.random()) - 0.5;
    }).slice(0, 1);
    // ランダムで1つだけ抽出して表示
    $(item).show();
  }
  function shuffleAD_c(){
    //念のため、全部非表示にする
    $('#ad-type-c .bnr-child').hide();
    // リストをシャッフルする
    var item = $('#ad-type-c .bnr-child').get().sort(function() {
      return Math.round(Math.random()) - 0.5;
    }).slice(0, 1);
    // ランダムで1つだけ抽出して表示
    $(item).show();
  }
  function shuffleAD_sh(){
    // 念のため、全部非表示にする
    $('#ad-type-sh .bnr-child').hide();
    // リストをシャッフルする
    var item = $('#ad-type-sh .bnr-child').get().sort(function() {
      return Math.round(Math.random()) - 0.5;
    }).slice(0, 1);
    $(item).addClass('selected');
    // シャッフル
    var arr = [];
    $('#ad-type-sh .bnr-child.selected').children().each(function() {
        arr.push($(this).html());
    });
    arr.sort(function() {
        return Math.random() - Math.random();
    });
    $('#ad-type-sh .bnr-child.selected').empty();
    for(i=0; i < arr.length; i++) {
        $('#ad-type-sh .bnr-child.selected').append('<li>' + arr[i] + '</li>');
    }
    // 表示
    $('#ad-type-sh .bnr-child.selected').show();
  }
  $(function (){
    shuffleAD_s();
    shuffleAD_b();
    shuffleAD_c();
    shuffleAD_sh();
  });
</script>

