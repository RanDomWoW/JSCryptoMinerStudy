		if (v == "")
		{
			al.innerHTML = "Trebuie sa introduceti adresa de e-mail!";
			jsShowElement("alert_div");
			document.getElementById("empassword").focus();
         	return false;
		}
		if (!jsIsEmail("empassword"))
		{
			al.innerHTML = "Adresa de e-mail este invalida!";
			jsShowElement("alert_div");
			document.getElementById("empassword").focus();
         	return false;
		}
		alert("ATENTIE:\nEste posibil ca acest mesaj sa fie mutat automat in \ndirectorul 'Spam' al casutei Dvs de e-mail.");
		document.authentication_form.cmd.value = "home";
		document.authentication_form.subcmd.value = "recovery";
		document.authentication_form.submit();
		return true;
	}
</script>
