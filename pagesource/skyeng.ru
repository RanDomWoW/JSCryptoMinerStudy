<script>
  (function ($) {

    function get(selector) {
      return document.querySelector(selector);
    }

    function on(element, type, callback) {
      if (!(element instanceof HTMLElement)) {
        element = get(element);
      }
      element.addEventListener(type, callback, false);
    }

    function stopAll(id) {
      $('.video-player').each(function (index, item) {
        try {
          var $btnPlay = $(item).find('.btn-payer-play');
          var $vid = $(item).find('video');
          if($btnPlay.attr('id') !== ('play' + id) && !$vid.get(0).paused) {
            $vid.get(0).pause();
            $btnPlay.attr("data-control", "play");
            $btnPlay.removeClass('b-ff-index-player__btn_play_pause');
          }
        } catch (ex)
        {
          //ignore - html5 async video load error
        }
      });
    }

    var instances003 = plyr.setup('#playerCDN-003', {
      controls: [''],
      loadSprite: false,
      volume: 8,
      showPosterOnEnd: true,
      hideControls: false,
      storage: false,
      captions: {
        defaultActive: 1
      }
    });

    instances003.forEach(function (item) {
      var playBtn003 = $('#play003');
      var bodyPlyr003 = $("#cdnBoxPlayer003");

      item.on('ready', function (event) {
        item.toggleMute();
        item.toggleCaptions();
      });

      item.on('play', function (event) {
        stopAll('003');
        playBtn003.attr("data-control", "pause");
        playBtn003.addClass('b-ff-index-player__btn_play_pause');
      });

      item.on('pause', function (event) {
        playBtn003.attr("data-control", "play");
        playBtn003.removeClass('b-ff-index-player__btn_play_pause');
      });

      // item.on('ended', function(event) { });

      on('#play003', 'click', function () {
        var $btn = $(this);
        var rules = $btn.attr("data-control");
        if (rules != 'play') {
          $btn.attr("data-control", "play");
          item.pause();
        }
        else {
          $btn.attr("data-control", "pause");
          item.play();
        }
      });

      on('#ext-play003', 'click', function () {
        var $btn = $('#play003');
        $btn.attr("data-control", "pause");
        item.play();
      });

      on('#ext-pause003', 'click', function () {
        var $btn = $('#play003');
        $btn.attr("data-control", "play");
        item.pause();
      });

      on('#mute003', 'click', function () {
        var $btn = $(this);
        var rule = item.isMuted();
        if (!rule) {
          $btn.addClass('b-ff-index-player__btn_mute_off');
        }
        else {
          $btn.removeClass('b-ff-index-player__btn_mute_off');
        }
        item.toggleMute();
        item.toggleCaptions();
      });

      on('#fullscreen003', 'click', function () {
        var $bodyPlayer = $("#cdnBoxPlayer003");
        var playerHeight = $bodyPlayer.outerHeight();
        $bodyPlayer.css('height', playerHeight + 'px');
        bodyPlyr003.addClass('b-ff-index-player__body_fullscreen');
        item.play();

        var rule = item.isMuted();
        if (rule) {
          item.toggleCaptions();
          item.setVolume(8);
        }

        $('#mute003').removeClass('b-ff-index-player__btn_mute_off');
        var scrollBarWidth = window.innerWidth - document.body.offsetWidth;
        $("body").css({"overflow": "hidden", "margin-right": scrollBarWidth + "px"});
      });

      on('#closeFullscreen003', 'click', function () {
        bodyPlyr003.removeClass('b-ff-index-player__body_fullscreen');
        $("body").css({"overflow": "", "margin-right": ""});
      });

    });

  }(jQuery));
</script>                </div>
