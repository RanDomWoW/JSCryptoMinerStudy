            <script>
              (function ($) {
                var videoAutoPlay = false;
                var active1 = true;
                var active2 = true;
                var active3 = true;

                checkWindowWidth();
                $(window).resize(checkWindowWidth);
                $(window).scroll(videoAutoPlayControll);

                function checkWindowWidth() {
                  var w = $(window).width();
                  if (w >= '980') {
                    if (!videoAutoPlay) {
                      videoAutoPlay = true;
                    }
                  }
                  else {
                    if (videoAutoPlay) {
                      videoAutoPlay = false;
                    }
                  }
                }

                function videoAutoPlayControll() {
                  if (videoAutoPlay) {
                    var $window = $(window);
                    var wh = $window.height() / 2;
                    var $pos = $window.scrollTop() + wh;
                    var $elem1 = $('#cdnBoxPlayer001');
                    var $elem2 = $('#cdnBoxPlayer002');
                    var $elem3 = $('#cdnBoxPlayer003');
                    var $offset1 = $elem1.offset();
                    var $offset2 = $elem2.offset();
                    var $offset3 = $elem3.offset();
                    var height1 = $elem1.height();
                    var height2 = $elem2.height() * 2;
                    var height3 = $elem3.height() * 2;
                    var startpoint1 = Math.floor($offset1.top);
                    //var startpoint2 = Math.floor($offset2.top);
                    //var startpoint3 = Math.floor($offset3.top);
                    //var endpoint1 = Math.floor($offset1.top + height1);
                    var delta1 = Math.floor($offset2.top - ($offset1.top + height1));
                    var endpoint1 = Math.floor($offset1.top + height1 + delta1 / 2);
                    //var endpoint2 = Math.floor($offset2.top + height2);
                    var endpoint3 = Math.floor($offset3.top + height3);
                    var startpoint2 = Math.floor(endpoint1 + 5);
                    var delta2 = Math.floor($offset3.top - ($offset2.top + height2));
                    var endpoint2 = Math.floor($offset2.top + height2 + delta2 / 2);
                    var startpoint3 = Math.floor(endpoint2 + 5);

                    if ($pos > startpoint1 && $pos < endpoint1) {

                      if (active1) {
                        active1 = false;
                        $('#ext-play001').trigger('click');
                        $('#ext-pause002').trigger('click');
                        $('#ext-pause003').trigger('click');
                      }
                    }
                    else if ($pos > startpoint2 && $pos < endpoint2) {
                      if (active2) {
                        active2 = false;
                        $('#ext-play002').trigger('click');
                        $('#ext-pause001').trigger('click');
                        $('#ext-pause003').trigger('click');
                      }
                    }
                    else if ($pos > startpoint3 && $pos < endpoint3) {

                      if (active3) {
                        active3 = false;
                        $('#ext-play003').trigger('click');
                        $('#ext-pause002').trigger('click');
                        $('#ext-pause001').trigger('click');
                      }
                    }
                    else {
                      if (!active1) {
                        $('#ext-pause001').trigger('click');
                        active1 = true;
                      }
                      if (!active2) {
                        $('#ext-pause002').trigger('click');
                        active2 = true;
                      }
                      if (!active3) {
                        $('#ext-pause003').trigger('click');
                        active3 = true;
                      }
                    }

                  }
                }

              }(jQuery));
            </script>
