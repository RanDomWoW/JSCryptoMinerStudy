<div id="popinConnexionPro" class="reveal-modal medium" data-reveal>
    <div class="cale-popin">
        <h2>Se connecter à son espace pro</h2>
        <form id="form_espace_pro_header_connexion">
            <input id="espace_pro_email_connexion" name="email" type="text" value="Votre adresse e-mail" onfocus="if(this.value=='Votre adresse e-mail') this.value='';" onblur="if(this.value=='') this.value='Votre adresse e-mail';">
            <input id="espace_pro_pass_connexion" name="password" type="password" value="Mot de passe" onfocus="if(this.value=='Mot de passe') this.value='';" onblur="if(this.value=='') this.value='Mot de passe';">
            <a href="" title=""
               onclick="connexionEspacePro('/espace-pro?connexion');return false;"
               class="bouton">Se connecter</a>
        </form>
        <h2>s’inscrire à l’espace pro</h2>
        <form id="form_espace_pro_header">
            <input type="text" value="Nom" name="espace_pro_nom" id="espace_pro_nom" onfocus="if(this.value=='Nom') this.value='';" onblur="if(this.value=='') this.value='Nom';">
            <input type="text" value="Prénom" name="espace_pro_prenom" id="espace_pro_prenom"  onfocus="if(this.value=='Prénom') this.value='';" onblur="if(this.value=='') this.value='Prénom';">
            <input type="text" value="E-mail" name="espace_pro_email" id="espace_pro_email"  onfocus="if(this.value=='E-mail') this.value='';" onblur="if(this.value=='') this.value='E-mail';">
            <input type="password" value="Mot de passe" name="espace_pro_pass_crypte" id="espace_pro_pass_crypte"  onfocus="if(this.value=='Mot de passe') this.value='';" onblur="if(this.value=='') this.value='Mot de passe';">
            <select  name="espace_pro_profil" id="espace_pro_profil">
                <option>[Profil]</option><option value="journaliste">journalistes</option><option value="libraire">libraires</option>            </select>
            <input type="text" value="Société" name="espace_pro_societe" id="espace_pro_societe"  onfocus="if(this.value=='Société') this.value='';" onblur="if(this.value=='') this.value='Société';">
