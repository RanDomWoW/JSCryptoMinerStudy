<script>
function setCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
    var expires = "expires=" + d.toGMTString();
    document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
}

function getCookie(cname) {
    var name = cname + "=";
    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(';');
    for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}
function surveysend() {        
        txtName = $('#survey_txtName').val();
        txtEmail = $('#survey_txtEmail').val();
        CountryCode = $('#survey_CountryCode').val();
        txtPhone = $('#survey_txtPhone').val();
        var txtAnswer;
        var error = 0;
        if (txtName == "") {
            $('#survey_txtName').attr("style", "border:1px solid #e57f2d !important;");
            error = 1;
        } else {
            var matches = txtName.match(/\d+/g);
            if (matches != null) {

            }
            $('#survey_txtName').attr("style", "border:0px solid #000 !important;");
        }
        if (txtEmail == "") {
            $('#survey_txtEmail').attr("style", "border:1px solid #e57f2d !important;");
            error = 1;
        } else {
            $('#survey_txtEmail').attr("style", "border:0px solid #000 !important;");
        }
        if (txtPhone == "") {
            $('#survey_txtPhone').attr("style", "border:1px solid #e57f2d !important;");
        error = 1;
        } else {
            $('#survey_txtPhone').attr("style", "border:0px solid #000 !important;");
        }

        if ($('#survey_errorsFName').is(':visible') || $('#survey_errorsEmail').is(':visible') || $('#survey_errorsMobileNumber').is(':visible'))
        {
            error = 1;
        }
        
        if ($("#survey_rdb1").is(':checked') || $("#survey_rdb2").is(':checked') || $("#survey_rdb3").is(':checked') || $("#survey_rdb4").is(':checked') || $("#survey_rdb5").is(':checked')) {
            $('.survery-error').hide();
        }
        else {
            $('.survery-error').html("Please select an asnwer");
            error = 1;
        }

        if (error == 1)
        {
            return false;
        }

        if ($("#survey_rdb1").is(':checked'))
        {
            txtAnswer = "1 month";
        }
        if ($("#survey_rdb2").is(':checked')) {
            txtAnswer = "2 months";
        }
        if ($("#survey_rdb3").is(':checked')) {
            txtAnswer = "3 months";
        }
        if ($("#survey_rdb4").is(':checked')) {
            txtAnswer = "1 to 2 years";
        }
        if ($("#survey_rdb5").is(':checked')) {
            txtAnswer = "Not Interested";
        }
        txtPhone = CountryCode + txtPhone;
        $('.submitdata').show();
        var obj = {};
        obj.txtName = txtName;
        obj.txtEmail = txtEmail;
        obj.txtPhone = txtPhone;
        obj.txtAnswer = txtAnswer;
        $.ajax({
            async: true,
            type: "POST",
            contentType: "application/json; charset=utf-8",
            url: "/contactfunctionalities.aspx/surveryform",
            data: JSON.stringify(obj),
            dataType: "json",
            success: function (r) {
                $('.submitdata').hide();
                status = r.d;                
                if (status == "exist")
                {
                    $('.survery-error').show();
                    $('.survery-error').html("You have already answered");
                }
                else if (status == "error") {
                    $('.survery-error').show();
                    $('.survery-error').html("There is error processing your data");
                }
                else
                {
                    setCookie("surveyform", "done", 30);
                    $('.survey-form').hide();
                    $('.survey-title').html("Thank You");
                    $('.survey-thankyou').show();
                    setTimeout(function () {
                        $('#modalSurvey').modal('hide');
                    }, 3000);
                    setTimeout(function () {
                        $('.survey-form').show();
                        $('.survey-title').html("We want to know you better!");
                        $('.survey-thankyou').hide();
                        $('#survey_txtName').val('');
                        $('#survey_txtEmail').val('');
                        $('#survey_CountryCode').val('971');
                        $('#survey_txtPhone').val('');
                    }, 5000);
                }
                
            },
            error: function (Result) {
                    
            }
        });
        return false;
    }
</script>
