// main category
function listsubcatorunit() {
    var projectid = document.getElementById('contactus_contact_form_Subject').value;
    var projectindex = document.getElementById("contactus_contact_form_Subject");
    var phaseText = projectindex.options[projectindex.selectedIndex].text;

    var obj = {};
    obj.prodid = projectid;
    obj.lang = 'English';
    $.ajax({
        async: true,
        type: "POST",
        contentType: "application/json; charset=utf-8",
        url: "/contactfunctionalities.aspx/listsubcatorunit",
        data: JSON.stringify(obj),
        dataType: "json",
        success: function (r) {
            var ddlCustomerssub = $("[id*=Subsubject]");
            var ddlCustomers = $("[id*=Unittype]");
            if (r.d['0']['producttype'] == "subcategory") {
                ddlCustomerssub.empty().append('<option selected="selected" value="0">Choose a phase in ' + phaseText + '</option>');
                $.each(r.d, function () {
                    if (this['Value'] != null) {
                        ddlCustomerssub.append($("<option></option>").val(this['Value']).html(this['Text']));
                    }
                });
                $('#contactus_contact_form_subsubjectdiv').show();
                $('#contactus_contact_form_unittypediv').hide();
            }
            else {
                var resultcount = 0;
                ddlCustomers.empty().append('<option value="0">Choose a Property type</option>');
                $.each(r.d, function () {
                    if (this['Value'] != null) {
                        ddlCustomers.append($("<option></option>").val(this['Value']).html(this['Text']));
                        resultcount++;
                    }
                });
                if (resultcount > 1) {
                    $('#contactus_contact_form_Unittype option:eq(0)').attr("selected", "selected");
                        $('#contactus_contact_form_unittypediv').show();
                        $('#contactus_contact_form_Unittype').show();
                }
                 else if(resultcount == 0 )
            {
                $('#contactus_contact_form_unittypediv').hide();
                    $('#contactus_contact_form_Unittype').val(null);
            }
                    else {
                        $('#contactus_contact_form_Unittype option:eq(1)').attr("selected", "selected");
                        showunittype(projectid);
                        $('#contactus_contact_form_unittypediv').hide();
                     $('#contactus_contact_form_subsubjectdiv').hide();
                     $('#contactus_contact_form_Subsubject').val(null);
                     
                        $('#contactus_contact_form_Unittype').show();
                    }
                }
        },
        error: function (Result) {
            alert("Error");
        }
    });

        $('#contactus_contact_form_bedroomdiv').hide(); $('#contactus_contact_form_bedrooms').hide();
}


