padding-bottom:0px;
margin-bottom:20px;
text-align: justify;
overflow: hidden;
}

/* Highlight Box */
.tipBox {
font: normal medium 500 arial;
color: #000;
font-style: normal;
margin-left: 10px;
margin-right: 10px;
margin-top: 20px;
margin-bottom: 20px;
-webkit-border-radius: 8px;
-moz-border-radius: 8px;
border: 0 solid #eee;
padding: 5px;
border-radius: 8px;
border: 1px solid #FDEAA0;
-webkit-border-top-left-radius: 0;
border-top-left-radius: 0;
-moz-box-shadow: 4px 4px 4px #bbb;
-webkit-box-shadow: 4px 4px 4px #bbb;
box-shadow: 4px 4px 4px #bbb;
background: #FEF7C5;
}

.tipBoxpadding {
padding: 10px;
}

.tipBox a {
padding: 2px;
font: normal medium 500 arial;
color: #039;
}

.tipBox a:hover {
background: #FEF7C5;
}

.tipBoxLeft {
float: left;
width: 45%;
padding: 10px;
padding-right: 15px;
border-right: 3px solid #FDEAA0;
}

.tipBoxRight {
float: left;
width: 45%;
padding: 10px;
padding-left: 25px;
}

.tipImg {
margin-left: 0;
padding-bottom: 5px;
}
/* to remove quote open icon */
blockquote:after {
display: none;
}
/* To align sidebar with top of page */
#page {
min-height: 0px;
}
/* For Simple Footnotes plugin */
sup {font-size:small; vertical-align:super;}

