                            html += '<tr><td>EPS (ttm): </td><td><strong>'+ data.query.results.quote.EarningsShare +'</strong></td></tr>';
                        if(data.query.results.quote.DividendShare)
                            html += '<tr><td style="border-bottom: 0">Div & Yield: </td><td style="border-bottom: 0"><strong>'+ data.query.results.quote.DividendShare + '(' + data.query.results.quote.DividendYield + '%)' +'</strong></td></tr>';
                        html += '</table>';
                        this.set('content.text', html);
                    }
                }
            },
            show: {
                event: event.type,
                ready: true
            },
            position: {
                my: 'top center',
                at: 'bottom center',
                target: obj
            }
        }, event);
    });
});
</script>
  
