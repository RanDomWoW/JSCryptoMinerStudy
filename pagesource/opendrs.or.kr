<script type="text/javascript">

// dialog 알림창 삽입
document.write("<div id=\"dialog-modal\" title=\"알림창\"><p id=\"pMsg\"></p></div>");

/**
*@ 로딩이미지
*/
var loader = jQuery('<div id="loader"><img src="/images/loading.gif" alt="loading..." /></div>')
.appendTo("body")
.hide();

/**
* TOP 메뉴 마우스 오버
*/
$(".mainTopMenu").mouseover(function(){
	$('#menu_dept1, #menu_dept2, #menu_dept3').css("display","none");
	$('#subMenu1').attr("src", "/img/main/menu_01_off.gif")
	$('#subMenu2').attr("src", "/img/main/menu_02_off.gif")
	$('#subMenu3').attr("src", "/img/main/menu_03_off.gif")

	if (this.id == "subMenu1") {
		$("#menu_dept1").slideDown("slow");
		$(this).attr("src", "/img/main/menu_01_on.gif")
	} else if (this.id == "subMenu2") {
		$("#menu_dept2").slideDown("slow");
		$(this).attr("src", "/img/main/menu_02_on.gif")
	} else if (this.id == "subMenu3") {
		$("#menu_dept3").slideDown("slow");
		$(this).attr("src", "/img/main/menu_03_on.gif")
	}
});

/**
* TOP 메뉴 클릭
*/
$(".mainTopMenu").click(function(){
	// tab 메뉴 처리
	$("#mainRollingImg, #mainRollingImg2, #mainRollingImg3").css("display", "none");

	$("#mainMenu1").attr("src", "/img/main/bannertab_01_gray.gif");
	$("#mainMenu2").attr("src", "/img/main/bannertab_02_gray.gif");
	$("#mainMenu3").attr("src", "/img/main/bannertab_03_gray.gif");
	$("#mainbox, #mainbox2, #mainbox3").css("display", "none");


	if (this.id == "subMenu1") {
		$("#mainRollingImg").fadeIn();
		$("#mainbox").fadeIn();
	} else if (this.id == "subMenu2") {
		$("#mainRollingImg2").fadeIn();
		$("#mainbox2").fadeIn();
	} else if (this.id == "subMenu3") {
		$("#mainRollingImg3").fadeIn();
		$("#mainbox3").fadeIn();
	} 

	
});

$(".topMenuCloseBtn").click(function(){
	$('#menu_dept1, #menu_dept2, #menu_dept3').slideUp("fast");
});

</script>
