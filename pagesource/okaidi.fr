//Visionneuse Infini
function init_visio(nbI,pas,el,timer)
{
    //Verification de l'existant
    if(jQuery(el).length>0 && jQuery(el+" .item").length>nbI)
    {
        // Clonage 1er slide
        jQuery(el+' .content .item:eq(0)').clone(true, true).insertAfter(jQuery(el+' .content .item:eq('+ (jQuery(el+" .content .item").length - 1) +')')).addClass('original');
        //On affecte une classe originale aux premiers éléments
        jQuery(el+" .content .item").toggleClass("original");

        //Copie des données//Effacement des temps
        jQuery(el+" .content").append(jQuery(el+" .suivTemp").html());
        jQuery(el+" .suivTemp").remove();

        //Setting de la taille
        jQuery(el+" .content").width((jQuery(el+" .item").length*jQuery(el+" .item").width()));

        //Positionnement à l'item 1
        jQuery(el+" .content").css("left","0px");

        //On Bind les elements
        jQuery(el+" .prev").bind('click',function(){visioPrev(nbI,el,pas,timer);});
        jQuery(el+" .next").bind('click',function(){visioNext(nbI,el,pas,timer);});
        jQuery(el+" .numero li").bind('click',function(){visioJump(this,nbI,el,pas,timer);});

        //On lance la fonction automatique
        globalTimer = setInterval(function(){visioNext(nbI,el,pas,timer);}, timer);

        //On affecte un killau mouse over et un set au mouse out
        jQuery(el).hover(
            function(){
                //On Kill
                clearInterval(globalTimer);
                jQuery(this).data('hover',true);
            },
            function(){
                //On Set
                globalTimer=setInterval(function(){visioNext(nbI,el,pas,timer);}, timer);
                jQuery(this).data('hover',false);
            }
        );
    }
    else
    {
        jQuery(el+" .prev,"+el+" .next").css("visibility","hidden");
    }
}

//Retourne une valeur sans px
function returnWPX(value)
{
    var retour = value.split("px");
    return retour[0];
}

//Deplacement de la Visionneuse Infini
function visioPrev(nbI,el,pas,timer,jump)
{
    //On enleve l'interval
    clearInterval(globalTimer);
    //Unbind des elements de navigation
    jQuery(el+" .prev, "+el+" .next").unbind('click');
    jQuery(el+" .numero li").unbind('click');

    //T'es au bout(YAH!) ?
    if(returnWPX(jQuery(el+" .content").css("left"))==0)
        jQuery(el+" .content").css("left","-"+(jQuery(el+" .item").length-nbI)*jQuery(el+" .item").width()+"px");

    //On doit JUMPER !
    var oldPas = pas;
    if(arguments.length==5)
    {
        pas = jump;
    }

    jQuery(el+" .content").animate({left:'+='+(pas*jQuery(el+" .item").width())},1800,function(){
        pas = oldPas;
        visioPos(nbI,el,pas);
        //Remise en route de l'interval et de la nav
        if(!jQuery(el).data("hover"))
        {
            //Kill preventif pas de double Set (clic et sort)
            clearInterval(globalTimer);
            globalTimer=setInterval(function(){visioNext(nbI,el,pas,timer);}, timer);
        }
        jQuery(el+" .prev").bind('click',function(){visioPrev(nbI,el,pas,timer);});
        jQuery(el+" .next").bind('click',function(){visioNext(nbI,el,pas,timer);});
        jQuery(el+" .numero li").bind('click',function(){visioJump(this,nbI,el,pas,timer);});
    });
}

//Deplacement de la Visionneuse infini
function visioNext(nbI,el,pas,timer,jump)
{
    //On enleve l'interval
    clearInterval(globalTimer);
    //Unbind des elements de navigation
    jQuery(el+" .prev, "+el+" .next").unbind('click');
    jQuery(el+" .numero li").unbind('click');

    //T'es au bout(YAH!) ?
    if(returnWPX(jQuery(el+" .content").css("left"))=="-"+(jQuery(el+" .item").length-nbI)*jQuery(el+" .item").width())
        jQuery(el+" .content").css("left","0px");

    //On doit JUMPER !
    var oldPas = pas;
    if(arguments.length==5)
    {
        pas = jump;
    }

    jQuery(el+" .content").animate({left:'-='+(pas*jQuery(el+" .item").width())},1800,function(){
        pas = oldPas;
        visioPos(nbI,el,pas);
        //Remise en route de l'interval et de la nav
        if(!jQuery(el).data("hover"))
        {
            //Kill preventif pas de double Set (clic et sort)
            clearInterval(globalTimer);
            globalTimer=setInterval(function(){visioNext(nbI,el,pas,timer);}, timer);
        }
        jQuery(el+" .prev").bind('click',function(){visioPrev(nbI,el,pas,timer);});
        jQuery(el+" .next").bind('click',function(){visioNext(nbI,el,pas,timer);});
        jQuery(el+" .numero li").bind('click',function(){visioJump(this,nbI,el,pas,timer);});
    });
}

//Retourne la position de l'element
function visioPos(nbI,el,pas)
{
    //Nb elements originaux
    var nbO = jQuery(el+" .item.original").length;
    //Calcul de la position
    var pos = ((returnWPX(jQuery(el+" .content").css("left"))*-1)/(nbI*jQuery(el+" .item").width()))+1;
    //Si on est dans les duplications
    if(pos>nbO)
        pos=pos-nbO;
    //Retourne la valeur
    jQuery(el+" .numero li").removeClass("on");
    jQuery(el+" .numero li:eq("+(pos-1)+")").addClass("on");
}

//Fonction de Jump
function visioJump(li,nbI,el,pas,timer)
{
    if(!jQuery(li).hasClass("on"))
    {
        var index = jQuery(li).index()+1;
        //Nb elements originaux
        var nbO = jQuery(el+" .item.original").length;
        //Calcul de la position
        var pos = ((returnWPX(jQuery(el+" .content").css("left"))*-1)/(nbI*jQuery(el+" .item").width()))+1;
        //Si on est dans les duplications
        if(pos>nbO)
            pos=pos-nbO;

        var ecart = index-pos;

        if(ecart<0)
        {
            ecart = ecart*(-1);
            visioPrev(nbI,el,pas,timer,ecart);
        }
        else
        {
            visioNext(nbI,el,pas,timer,ecart);
        }
    }
}
