	function mainBannerAction(idx){
		var area = $('.mainBannerArea');
		area.each(function() {
			var area = $(this);
			var cnt = area.find("a").length;
			var now_obj = area.find('a');
			now_obj.hide();
			now_obj.eq(idx).css({display:'block'});

			$(".banner_cate li").mouseover(function(){
				if (!$(this).is('.on')) {
					var overcheck = $(this).parent().find('.on'),
						 imgSrc = $(this).find('img').attr('overImg'),
						 imgSrc2 = $(this).parent().find('.on').find('img').attr('outImg');
					$(this).addClass('on').find('img').attr('src',imgSrc);
					overcheck.removeClass('on').find('img').attr('src',imgSrc2);

					$(this).parent().parent().prev().find('a').css('display', 'none');
					$(this).parent().parent().prev().find('a').eq($(this).index()).css('display','block');
				}
			});

			area.next().find('li').eq(idx).addClass("on");
			area.next().find('li').eq(idx).siblings().removeClass("on");

			area.next().find('li').eq(idx).siblings().find("img").each(function(){
				$(this).attr("src",$(this).attr("outImg"));
			})
			var now_img_obj = area.next().find('li').eq(idx).find("img");
			now_img_obj.attr("src",now_img_obj.attr("overImg"));
		})
	}

