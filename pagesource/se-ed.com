<script type="text/javascript">


    function langClick(a) {
        $("#" + a).css({
            display: "block"
        });
        var b = $("#" + a + " .lang-list").css("height");
        $("#" + a + " .lang-list").css({
            height: "2px",
            display: "block"
        });
        $("#" + a + " .lang-list").animate({
            height: b
        }, 300, function () {
            $(document).ready(function () {
                $(document).bind("click", function (b) {
                    var c = $(b.target);
                    if (!c.parents().hasClass("lang-list")) {
                        $("#" + a + " .lang-list").stop(true, true).animate({
                            height: "2px"
                        }, 200, function () {
                            $("#" + a).css({
                                display: "none"
                            });
                            $("#" + a + " .lang-list").css({
                                height: "",
                                display: "none"
                            })
                        })
                    } else { }
                    $(document).unbind(b)
                })
            })
        });
        return false
    }


</script>

