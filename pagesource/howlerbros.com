					<a rv-href="item.url" class="image">
						<img rv-src="item.image | productImageSize 'compact'">
						<div class="quantity" rv-text="item.quantity"></div>
					</a> <!--/.image-->
					
					<a rv-href="item.url" class="details">
						<div class="title" rv-text="item.product_title"></div>
						<ul rv-hide="item.variant_options | empty">
							<li rv-each-variant="item.variant_options" rv-text="variant" rv-hide="variant | eq 'Default Title'"></li>
						</ul>
						<div class="discounts" rv-unless="item.discounts | empty">
							<span rv-each-discount="item.discounts" rv-text="discount.title"></span>
						</div>
					</a> <!--/.details-->
					
					<div class="price" rv-unless="item.discounted_price | lt item.original_price">
						<span class="normal" rv-html="item.original_price | money Currency.currentCurrency"></span>
					</div>
					
					<div class="price" rv-if="item.discounted_price | lt item.original_price">
						<span class="original" rv-html="item.original_price | money Currency.currentCurrency"></span>
						<span class="discounted" rv-html="item.discounted_price | money Currency.currentCurrency"></span>
					</div>
					
					<a class="remove" href="#" rv-data-cart-remove="index | plus 1">&times;</a>
					
				</div> <!--/.item-->
				
				<div class="no-items" rv-show="cart.item_count | lt 1">
					There are currently no items in your shopping cart.<br>
					<a class="close-cart" href="/collections">Continue shopping &raquo;</a>
				</div> <!--/.no-items-->
				
			</div> <!--/.items-->
			
			<footer>
				
				<div class="totals clearfix">
					<div class="label">Subtotal</div>
					<div class="value" rv-html="cart.total_price | money Currency.currentCurrency"></div>
				</div>
				
				<div class="notes">Shipping, taxes and discounts will be calculated during checkout.</div>
				
				<form action="/cart" method="post" class="buttons">
					<button type="submit" name="checkout">Proceed to Checkout</button>
					<a href="/cart">Edit Cart</a>
				</form> <!--/.buttons-->
				
			</footer>
			
		</div> <!--/.drawer-->
		
	</aside> <!--/#cart-drawer-->

