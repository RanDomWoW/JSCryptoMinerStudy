<script>
$(function() {
	var pickup = new Pickup();

	pickup.scroll('current', $('#pickup-box'), appendArticle, {offset: 31});

	function appendArticle(container, pickup) {
		var titleClass = 'imagetitle';
		var medias = null;
		var dateSplits = pickup.publish_date.split(' ');
		if (pickup.media && pickup.media != '') {
			$article = $('.current-pickup-template-image').clone();
			$article.removeClass('current-pickup-template-image');
			medias = pickup.media.split(',');
		} else {
			$article = $('.current-pickup-template-noimage').clone();
			$article.removeClass('current-pickup-template-noimage');
			titleClass = 'noimagetitle';
		}
		$article.removeClass('hidden');
		$article.attr('id', 'a_' + pickup.id);
		$('.detail-link', $article).attr('href', '/pickup/' + dateSplits[0].replace(/-/g, '') + '/' + pickup.id)
			.attr('onlick', "ga('send', 'event', 'ピックアップ', '" + pickup.title + "【" + pickup.name + "】(" + pickup.id + ")')");
		var upDate = $('<span/>').addClass('up-date').text(pickup.publish_date.replace(/-/g, '.') + ' UP');
		var category = $('<span/>').addClass('category-' + pickup.slag).text(pickup.name);
		if (medias) {
			$('.article-img', $article).attr('src', medias[0]).attr('alt', pickup.title);
		}
		$('.' + titleClass, $article).text(pickup.title).append($('<br>')).append(upDate).append(category);
		container.append($article);
	}
});
</script>
