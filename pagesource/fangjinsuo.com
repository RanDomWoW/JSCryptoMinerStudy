<script>
    function receiveMessage(event)
    {
        if(event.data=="close"){
            $('#onlineConsult').dialog( "destroy" );
            return;
        }
        if(event.data=="large"){
            dialog_frame.width="600px";
            dialog_frame.height="520px";
            $('#onlineConsult').dialog("option", "width", 600);
            $('#onlineConsult').dialog('option', 'position', { my: "right top", at: "left top", of: $('#embed-online-consult')});
            return;
        }

        if(event.data=="normal"){
            dialog_frame.width="320px";
            dialog_frame.height="520px";
            $('#onlineConsult').dialog("option", "width", 320);
            $('#onlineConsult').dialog('option', 'position', { my: "right top", at: "left top", of: $('#embed-online-consult')});
            return;
        }

    }
    window.addEventListener("message", receiveMessage, false);
    function startChat(){
        //請填寫客戶端網站
        var homePage="http://www.fangjinsuo.com/";
        dialog_frame.src="http://ks2016001.ezphone.cn/ucsocial/webChat.jsp?s=c842e545c1d558afa34affe16543a96c&u="+encodeURI(homePage);
        dialog_frame.width="320px";
        dialog_frame.height="520px";
        $('#onlineConsult').dialog({
                    open: function() {
                        $(this).siblings('.ui-dialog-titlebar').remove();
                    },
                    closeOnEscape: false,
                    draggable: false,
                    height:520,
                    width:320,
                    dialogClass: "fixed-dialog",
                    position: {
                        of: $('#embed-online-consult'),
                        my: "right top",
                        at: "left top"
                    }
                }
        );
    }
</script>
