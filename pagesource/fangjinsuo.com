function setCookie(name, value) {
    var exp = new Date();
//    exp.setTime(exp.getTime() + 60 * 60 * 12 * 1000);
    exp.setTime(exp.getTime() + 60 * 60 * 48 * 1000 * 30);
    document.cookie = name + "=" + escape(value) + "; expires=" + exp.toGMTString()+"; path=/; domain="+window.location.host;
}
