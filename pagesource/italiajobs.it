			}
		});

		$("#l").on("keydown",function (e) {
				if($("#sugg .sugg_item").size() > 0)
				{
					if($("#sugg").find(".selected").size() > 0){
						var sugg_item = $("#sugg").find(".selected");
					}else{
						var sugg_item = $("#sugg .sugg_item:first").addClass('selected');
					}

					if (e.keyCode == 40) { //down
							sugg_item.removeClass('selected').next().addClass('selected');
							// if (sugg_item.next().length == 0) {
			        //     $("#sugg .sugg_item:first").addClass('selected');
			        // }
			    } else if (e.keyCode === 38) { //up
			        sugg_item.removeClass('selected').prev().addClass('selected');
			        // if (sugg_item.prev().length == 0) {
			        //     $("#sugg .sugg_item:first").addClass('selected');
			        // }
			    }else if(e.keyCode == 13){
							e.preventDefault();
