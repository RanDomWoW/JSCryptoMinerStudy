  <script>
    // Initialize SwiperJS
    $(document).ready(function() {
    var swiper = new Swiper('.testimonial-slider', {
      slidesPerView: 5,
      grabCursor: true,
      spaceBetween: 50,
      loop: true,
      centeredSlides: true,
      speed: 1000,
      navigation: {
        nextEl: '.s-button-next',
        prevEl: '.s-button-prev',
      },
      breakpoints: {
      // when window width is <= 320px
      1330: {
        slidesPerView: 3
      }
     }
    });

    // Testimonial Modal
    $('.testimonial-modal i').click(function(){
      $('body').css('overflow', 'visible');
      $("div#modalPlayer").css('display', 'none');
      $('iframe#ytPlayer').attr('src', ' ');
    });
    // Changing modal youtube video
    $("i#cardPlayButton").click(function(){
      let $this = this;
      if($($this).attr('data-videourl')) {
        $('body').css('overflow', 'hidden');
        let ytSettings = "autoplay=1&playsinline=1&showinfo=0&start=0&color=white";
        let ytPlayerSrc = $($this).data('videourl') + '?' + ytSettings;
        $("div#modalPlayer").css('display', 'flex');
        $('iframe#ytPlayer').attr('src', ytPlayerSrc);
      } else {
        console.log('Error: Testimonial Card has no video url.');
      }
    });
  });
  </script>
