            $('.customInput, ' + '.input' ).cornerz({radius:6, background:'#ebebdd'});
            $('.droapps-ui-customselect').not('.select-neighborhood .droapps-ui-customselect').cornerz({radius:6, background:'#ebebdd'});

            spacingSubitem("Cliente Unimed-Rio");


