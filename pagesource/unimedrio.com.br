			if ($('#lightbox-planos').length >=1 && !$.cookie("verMaisPlanos")) {
				var url = '/static/popups/modal_planos.html';
				disparaModalRequest(url);
			}
			
			// lightbox receita
			if ($('#lightbox-receita').length >=1 && !$.cookie("verMaisReceita")) {
				var url = '/static/popups/modal_receita.html';
				disparaModalRequest(url);
			}
			
			// lightbox exa
			if ($('#lightbox-exa').length >=1) {
				var url = '/static/popups/modal_exa.html';
				disparaModalRequest(url);
			}
			// lightbox olimpiadas
			if ($('#lightbox-incidente').length >=1) {
				var url = '/static/popups/modal_incidente.html';
				disparaModalRequest(url);
			}
			
			if ($('#lightbox-topsaude').length >=1) {
				var url = '/static/popups/modal_aviso-topsaude.html';
				disparaModalRequest(url);
			}
			
			// lightbox exa
			if ($('#lightbox-avisocorretor').length >=1) {
				var url = '/static/popups/modal_aviso-corretor.html';
				disparaModalRequest(url);
			}
			
			// lightbox exa
			if ($('#lightbox-avisopf').length >=1) {
				var url = '/static/popups/modal_aviso-pf.html';
				disparaModalRequest(url);
			}
			
			// lightbox exa
			if ($('#lightbox-avisopj').length >=1) {
				var url = '/static/popups/modal_aviso-pj.html';
				disparaModalRequest(url);
			}
			
			if ($('#lightbox-sos').length >=1) {
				var url = '/static/popups/modal_aviso-sos.html';
				disparaModalRequest(url);
			}
            //cornerz
            $('#areaLogin').cornerz({radius:10, background:'#5a9600'});

            $('#restrito .centraliza .noticias img[alt=Próximo]').removeClass('seta');
            $('#restrito .centraliza .noticias img[alt=Próximo]').addClass('seta2');

            $('#fancybox-close').click(function(){
                //alert();

                return false;
            });
			
			$('#senha').focus(function(){
                if ($('#senha').val() == 'senha'){
                    $('#senha').val("");
                }
            });
            $('#senha').focusout(function(){
                if ($('#senha').val() == ""){
                    $('#senha').val("senha");
                }
            });

			//Smart banner APP Unimed-Rio
			$.smartbanner({ daysHidden: 0, daysReminder: 0, title:'Unimed-Rio', price: 'Gr&aacute;tis', inAppStore: 'App Store', inGooglePlay: 'Play Store', button: 'Abrir', icon: '/static/img/ico-unimed.png' });

        });

        function spacingSubitem(title){
            $('#nav ul li a').each(function() {
                var findItemenu = $(this).attr('title');
                if (findItemenu == title) {
                    $(this).addClass('breaking-itemSubmenu');

                };
            });
        };
		function exibirConteudo(conteudo) { 
			if ($('#lightbox-receita').length >=1) {
				$('body').append("<div id='lightbox_novo' class='fix'>"
				+"  <div class='mask'></div>"
				+       "<div class='lightbox_principal_receita'>"
				+"          <a href='javascript:;' class='fechar_modal_receita' title='Fechar' onclick='fechaModal()'>Fechar [ x ]</a>"
				+"          <div id='ligbtox_content' class='clearfix'></div>"
				+"      </div>"
				+"  </div>");
			}else if ($('#lightbox-incidente').length >=1) {
				$('body').append("<div id='lightbox_novo' class='fix'>"
				+"  <div class='mask'></div>"
				+       "<div class='lightbox_principal_incidente'>"
				+"          <a href='javascript:;' class='fechar_modal' title='Fechar' onclick='fechaModal()'>Fechar [ x ]</a>"
				+"          <div id='ligbtox_content' class='clearfix'></div>"
				+"      </div>"
				+"  </div>");
			}else{
				$('body').append("<div id='lightbox_novo' class='fix'>"
				+"  <div class='mask'></div>"
				+       "<div class='lightbox_principal'>"
				+"          <a href='javascript:;' class='fechar_modal' title='Fechar' onclick='fechaModal()'>Fechar [ x ]</a>"
				+"          <div id='ligbtox_content' class='clearfix'></div>"
				+"      </div>"
				+"  </div>");
			}
			$('#ligbtox_content').html(conteudo);
	
		}
		function disparaModalRequest(url) {
			// console.log("disparaModalRequest: "+url+"|"+tamanhp);
			$.ajax({
				url: url,
				data:'html',
				beforeSend: function(){
					if ($('#lightbox-receita').length >=1) {
						$('body').append("<div class='lightbox_principal_receita preloading'><p class='loading-modal'><img src='/static/img/common/fancybox/loading.gif' alt='' class='fl' /> <span class='fl'>Carregando</span></p></div>");
					}else if ($('#lightbox-incidente').length >=1) {
						$('body').append("<div class='lightbox_principal_incidente preloading'><p class='loading-modal'><img src='/static/img/common/fancybox/loading.gif' alt='' class='fl' /> <span class='fl'>Carregando</span></p></div>");
					}else{
						$('body').append("<div class='lightbox_principal preloading'><p class='loading-modal'><img src='/static/img/common/fancybox/loading.gif' alt='' class='fl' /> <span class='fl'>Carregando</span></p></div>");
					}
					// return false;
				},
				success: function(conteudo){
					if ($('#lightbox-receita').length >=1) {
						$(".lightbox_principal_receita").remove();
					}else if ($('#lightbox-incidente').length >=1) {
						$(".lightbox_principal_incidente").remove();
					}else{
						$(".lightbox_principal").remove();
					}
					$("body, html").css("overflow","hidden");
					exibirConteudo(conteudo);
				}
			});

			return false;
		}
		// FECHA O MODAL AJAX
		function fechaModal() {
			$("body").find("#lightbox_novo").fadeTo("normal", 0, function(){
					$(this).remove().hide();
					$("body, html").removeAttr("style");
			});
		}

