        function abre(quem){
            if (quem==1) {
                $('.FechaDiv').fadeOut('slow');
                $('.agil').fadeIn('slow');
            } else if (quem==2) {
                $('.FechaDiv').fadeOut('slow');
                $('.corporativo1').fadeIn('slow');
            } else if (quem==3) {
                $('.FechaDiv').fadeOut('slow');
                $('.corporativo2').fadeIn('slow');
            }
        }
