    var latest = (function() {
        var self = this;
        var aid = 0;
        var debug = false;

        var getData = function() {
            // fetch the new data
            $.ajax({
                url: 'http://www.monitor.bg/api/getRecentStories/',
                type:'GET',
                beforeSend:function() {

                },
                success:function(data) {
                    if(parseInt(data[0].aID) > parseInt(aid)) {
                        setTimeout(function() {
                            destroy();
                        }, 500);
                        setTimeout(function() {
                            removeLast();
                        }, 500);
                        setTimeout(function() {
                            render(data[0]);
                        }, 500);
                        setTimeout(function() {
                            init();
                        }, 500);
                    }

                    else {
                        if(debug) {
                            console.log('nothing new'); // remove for production
                        }
                        return 1;
                    }
                }
            });

            setTimeout(function() {
                getData();
            },44000);
        }

        var render = function(data) {
            // fade in effect?!
            if(debug) {
                console.log(data.aID + ' added');
            }

            $('.topCarousel').prepend('<div class="latestItem" aid="'+data.aID+'">' +
                '<img src="'+data.images[0].path+'">' +
                '<a title="'+data.title+'" href="http://www.monitor.bg//a/view'+data.alias+'">"'+data.title+'"</a>' +
            '</div>');
        };

        var removeLast = function() {
            // fadeout effect?!
            if(debug) {
                console.log($('.latestItem').last().attr('aid') + ' removed');
            }

            $('.latestItem').last().remove();
        };

        var init = function() {
            aid = $('.latestItem').first().attr('aid');

            if(debug) {
                console.log('latest aid is: ' + aid); // remove for production
            }

            // create the slider
            $('.topCarousel').slick({
                nextArrow: 'none',
                prevArrow: 'none',
                slidesToShow: 3,
                slidesToScroll: 1,
                autoplay: true,
                adaptiveHeight: false,
                autoplaySpeed: 4000,
            });

            // resume event shortly after click
            $('.topCarousel a').on('click', function() {
                setTimeout(function() {
                    $('.topCarousel').slick('slickPlay');
                },1500);
            });

            return 0;
        };

        var destroy = function() {
            // destroy slider
            $('.topCarousel').slick('unslick');
            return 0;
        };

        var __construct = function(that) {
            // check if there is new data once per N seconds
            setTimeout(function() {
                getData();
            },33000); // this should be result of (number of slides * autoplaySpeed)
        }(this)

        return {
            init: function() {
                return init();
            },
            destroy: function() {
                return destroy();
            },
            render: function() {
                setTimeout(function() {
                    destroy();
                }, 500);
                setTimeout(function() {
                    removeLast();
                }, 500);
                setTimeout(function() {
                    render();
                }, 500);
                setTimeout(function() {
                    init();
                }, 500);
            }
        }
    }());

