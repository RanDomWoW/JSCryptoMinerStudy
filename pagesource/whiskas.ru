<script>
    $(document).ready(function () {
        var search = $('#search_text').val();
        var timeout;
        $('#search_text').on('keyup', function () {
            var newSearch = $(this).val();
            if (newSearch != search) {
                search = newSearch;
                if (timeout) {
                    clearTimeout(timeout);
                }
                timeout = setTimeout(function () {
                    $.ajax({
                        url: '/',
                        type: 'post',
                        data: {
                            'ajax_call': 'y',
                            'INPUT_ID': 'search_text',
                            'q': search,
                            'l': '2'
                        },
                        success: function (answer) {
                            if (answer.length > 0) {
                                $('#search_dropdown .search_result').html(answer).show();
                            }
                            else {
                                $('#search_dropdown .search_result').html(answer).hide();
                            }
                        }
                    });
                }, 500);
            }
        })
    })
</script>


