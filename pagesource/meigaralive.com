<script>
$(function(){
	window.liveaccesskey = "1";
	window.parentver = "20161206";

	if(window.localStorage){
		var codes = localStorage.getItem('codes');
		if(isset(codes)){
			window.codes = codes.split(',');
		}
		window.codes_flg = localStorage.getItem('codes_flg');

		var screen_names = localStorage.getItem('screen_names');
		if(isset(screen_names)){
			window.screen_names = screen_names.split(',');
		}
		window.screen_names_flg = localStorage.getItem('screen_names_flg');

		//volume設定
		window.slvolume = localStorage.getItem('volume');
		if(isset(window.slvolume) && window.slvolume.length > 0){
			document.getElementById("audio1").volume = window.slvolume;
		}

		//chkbx設定
		window.slchkbx_1 = localStorage.getItem('chkbx_1');
		window.slchkbx_2 = localStorage.getItem('chkbx_2');
		window.slchkbx_3 = localStorage.getItem('chkbx_3');

	}
});
