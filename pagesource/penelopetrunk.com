jQuery(function($) {

	var b = {
		banners : [],
		init: function(type, el) {
			if (el.length) {
				b.type = type;
				b.el = el;
				switch (b.type) {
					case 'header' :
						b.banners = headerBanners;
					break;

					case 'sidebar' :
						b.banners = sidebarBanners;
					break;

					case 'related' :
						b.banners = relatedBanners;
					break;

					default :
						b.banners = [];
					break;
				}

				// console.log(b.banners);
				// b.showRandom();

				if (b.banners.length) {
					b.showRandom();
				}
			}
		},
		showRandom : function() {

			var count = b.banners.length;

			var luckyId = Math.floor(Math.random() * count);
			var banner = b.banners[luckyId];

			b.el.attr('href', banner.url);
			b.el.find('img:eq(0)').attr('src', banner.img);
		}
	}

	b.init('sidebar', $('.banner-url'));
	b.init('sidebar', $('.quistic-banners-footer'));
	b.init('sidebar', $('.quistic-banner-container a'));
	b.init('sidebar', $('.quistic-banner-container-2 a'));
	b.init('related', $('.related-banner-container a'));
	// b.init('header',  $('.top-ad-container a'));

});
</script>

