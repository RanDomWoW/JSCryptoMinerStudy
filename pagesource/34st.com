  toggle(e, 'film');
});

document.getElementById('musicButton').addEventListener('click', function(e) {
  toggle(e, 'music');
});

document.getElementById('artsButton').addEventListener('click', function(e) {
  toggle(e, 'arts');
});

document.getElementById('styleButton').addEventListener('click', function(e) {
  toggle(e, 'style');
});

document.getElementById('lastpageButton').addEventListener('click', function(e) {
  toggle(e, 'lastpage');
});

var closeBtns = document.getElementsByClassName('closer');
for (var i = 0; i < closeBtns.length; i++) {
  closeBtns[i].addEventListener('click', function(e) { closeAll(e); });
};
