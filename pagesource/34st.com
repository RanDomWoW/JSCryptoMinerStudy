<script type="text/javascript">

function closeAll(e) {
  var sections = ['features', 'ego', 'wots', 'film', 'music', 'arts', 'style', 'lastpage', 'search'];
  for (var i = 0; i < sections.length; i++) {
    classes = document.getElementById(sections[i] + 'Sidebar').classList;
    if (classes !== undefined && classes.contains('open')) {
      document.getElementById(sections[i] + 'Sidebar').classList.remove('open')
    }
  }
}

function toggle(e, menu) {
  if (document.getElementById(menu + 'Sidebar').classList.contains('open')) {
    document.getElementById(menu + 'Sidebar').classList.remove('open');
  } else {
    closeAll(e);
    document.getElementById(menu + 'Sidebar').classList.add('open');
  }
}

document.getElementById('searchButton').addEventListener('click', function(e) {
  toggle(e, 'search');
});

document.getElementById('featuresButton').addEventListener('click', function(e) {
  toggle(e, 'features');
});

document.getElementById('egoButton').addEventListener('click', function(e) {
  toggle(e, 'ego');
});

document.getElementById('wotsButton').addEventListener('click', function(e) {
  toggle(e, 'wots');
});

