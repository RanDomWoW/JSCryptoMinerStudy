<script type="text/javascript">
function footHideEmailError(){
	document.getElementById("footEmailError").style.visibility = "hidden";
}
function footEmailEnter(e){
	if(e.which == 13 || e.keyCode == 13){
		footEmailSubmit(document.getElementById("footEmailSignupButton"));
		return false;
	}else{
		return true;
	}
}
function footEmailSubmit(theButton){
	var emailAddress = document.getElementById("footEmailAddress");
	var emailLoading = document.getElementById("footEmailSubmitLoading");
	if(emailAddress.value != "") {
		var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
		if(!(re.test(emailAddress.value))){
			document.getElementById("footEmailError").style.visibility = "visible";
			return;
		}
		theButton.disabled = true;
		emailAddress.readOnly = true;
		emailLoading.style.visibility = "visible";
		xmlHttp = GetXmlHttpObject();
		xmlHttp.open("POST", "/save_email.asp?p=" + encodeURIComponent(window.location.pathname.replace("/", "")), true);
		xmlHttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
		xmlHttp.onreadystatechange = function() {
			if(xmlHttp.readyState == 4) {
				if(xmlHttp.status == 200) {
					if (xmlHttp.responseText == "2") {
						$.fancybox({
							"type" : "iframe",
							"href" : "/email_popup.asp?e=" + encodeURIComponent(emailAddress.value) + "&s=1",
							"width" : 650,
							"height" : 220,
							"border" : 0,
							"scrolling" : "no"
						});
					} else {
						$.fancybox({
							"type" : "iframe",
							"href" : "/email_popup.asp",
							"width" : 640,
							"height" : 380,
							"border" : 0,
							"scrolling" : "no"
						});
					}
				} else {
					$.fancybox({
						"type" : "iframe",
						"href" : "/email_popup.asp",
						"width" : 640,
						"height" : 380,
						"border" : 0,
						"scrolling" : "no"
					});
				}
				emailLoading.style.visibility = "hidden";
				theButton.disabled = false;
				emailAddress.readOnly = false;
				emailAddress.value = "";
			}
		};
		xmlHttp.send("Email_Address=" + emailAddress.value + "&Collection_Point=6");
	}
}
function listrakEmailPopup() {
	console.log("ep");
	_ltk.Modal.modals.forEach(function(modal){ 
		if (modal.ModalName === "Header") modal.show(); 
	} );
}
</script>
