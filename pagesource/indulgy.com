	</div>
	<hr />
	<div class="center"> 
    	<label for="picDescription">Description</label>
      	<textarea placeholder="Describe it in few words ..." class="txt picDescription" name="picDescription" maxlength="140"><% if(_.isString(title)) { print(title.replace(/\"/g, "&quot;")); } %></textarea>
      	<label for="category">Collection</label>
      	<input name="category" placeholder="Type collection name..." class="txt fieldCollection" type="text" value="<% if(_.isString(category)) { print(category) } %>" maxlength="90"/>
      	<% if (typeof(action) == 'undefined' || action!='repost') { %>
        	<label for="linkToPicturePage" <% if(typeof(action) != 'undefined' && action=='save'){print('style="display:none;"');} %>>Link to picture page</label>
			<input name="linkToPicturePage" class="txt linkToPicturePage" <% if(typeof(action) != 'undefined' && action=='save'){print('style="display:none;"');} %> type="text" value="<% if(_.isString(link_to_pic)) { print(link_to_pic) } %>"/>
      	<% }  %>
      	<input name="saved_filename" class="txt" type="hidden" value="<% if(_.isString(saved_filename)) { print(saved_filename) } %>"/>
	  	<input name="postId" class="txt" type="hidden" value="<% if(_.isString(postId)) { print(postId) } %>"/>
	  	<input name="token" class="txt" type="hidden" value="<% if(_.isString(token)) { print(token) } %>"/>
    </div>  	
	<hr />
	<div class="action-btns">
		<input type="submit" value="Save" class="btn save" value="<% if(typeof(saveButtonLabel) == 'undefined'){print('Save');}else{ print(saveButtonLabel);} %>"  onClick="_gaq.push(['_trackEvent', 'Photo', 'Collect', '<% if (_.isString(saved_filename)) { print(saved_filename) } else { print('/resources/img/loadinfo.net.gif') } %>' ]);" >
		<input type="button" value="Close"  class="btn-close cancel" data-dismiss="modal">
	</div>	
</div>
  </form>


</script>
<script type="text/x-underscore-tmpl" id="submit_view" class="template">

