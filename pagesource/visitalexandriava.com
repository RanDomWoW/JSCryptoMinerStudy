<script>
	require(["jquery", "plugins_collections_custom_videoplayer", "lodash", "plugins_collections_custom_magnific", "sv_siteSlick", "domReady!"], function($, vp, lodash) {
		var root = $("[data-guid=3cf77304-9937-4809-825c-f929ad3bb0d4]");
		var slider = root.find("[data-sv-slides]");
		var slides = root.find("[data-sv-slide-track] [data-sv-slide]");

		var videoPlayers = [];
		var slideVideos = root.find('[data-slideshow-video]');
		var imgWrappers =  root.find('.imageOuter'); 
		var viewPort = $(window).height();
		var videoloaded = false;
		var disable_video_autoplay = true;
		var autorotate = true;

		
		
		//add background color to text on each slide
		$.each(slides, function(i, val) {
			var thisElement = $(this);
			var color = thisElement.find("[data-sv-colorchange]").attr("data-color");
			var rgba = "";

			if(color){
				rgba = getRgbaFromHex($.trim(color));
				thisElement.find("[data-sv-colorchange]").css("background-color", rgba);
			}
		});


		slider.on("init", function(event, slick) {
			videoloaded = true;
			root.animate({"opacity":"1"}, 600);
			root.css({ visibility : "visible" });
			if(!disable_video_autoplay) {
				setUpVideos();
			}			
		}).on("beforeChange", function(event, slick, current, next) {
			pauseAllVideos();
			turnOnImages();
		}).on('afterChange', function(event, slick, currentSlide, nextSlide){
			setImage(getActiveSlide());
		});	

		slider.slick({
			prevArrow : root.find(".prevSlide"),
			nextArrow : root.find(".nextSlide"),
			slide : ".slide",
			draggable : true,
			fade : true,
			dots : false,
			pauseOnHover : true,
			autoplay : autorotate,
			autoplaySpeed : 3000
		})

		root.find('.videolink').magnificPopup({
			type: 'iframe',
			preloader: false,
			iframe: {
				patterns: {
						youtube: {
						index : 'youtube.com/',
						id: "embed/"
					}
				}
			}
		});

		function getRgbaFromHex(color){
			var color = color;

			var redHex = parseInt(color.substring(1, 3), 16);
			var greenHex = parseInt(color.substring(3, 5), 16);
			var blueHex = parseInt(color.substring(5, 7), 16);
			color = "rgba(" + redHex + "," + greenHex + "," + blueHex + ", 0.75)" ;

			return color;
		}

		// BEGIN video functions
		function setAllImages(){
            imgWrappers.each(function(i, value){
                var img = $(value).find("[data-sv-image]");
                var width =  $(window).width();
                if( width < 1025  && width > 640){
                    img.attr( 'src', img.attr('data-medium-src') );
                }
                else if( width < 641){
                    img.attr( 'src', img.attr('data-small-src') );
                }
                else{
                    img.attr( 'src', img.attr('data-large-src') );
                }
            })
        }
        setAllImages();

		var setImage = function(activeSlide){
			if(activeSlide) {
				var imgWrapper = activeSlide.find(".imageOuter");
				var slideImage = activeSlide.find("[data-sv-image]");
				var videoPlayer = getVideoPlayer(activeSlide.attr("data-sv-video-id"));
				var width =  $(window).width();

				if( width < 1025  && width > 640){
					slideImage.attr( 'src', slideImage.attr('data-medium-src') );
					activeSlide.find("[data-td-video]").hide();
					imgWrapper.css('opacity', '1');
					if(typeof videoPlayer !== "undefined" && typeof videoPlayer.player !== "undefined"){
						if(typeof videoPlayer.player.playVideo === "function"){
							videoPlayer.player.pauseVideo();
						}
					}
					if(!autorotate) { slider.slick('slickPause'); }
				}
				else if( width < 641){
					slideImage.attr( 'src', slideImage.attr('data-small-src') );
					activeSlide.find("[data-td-video]").hide();
					imgWrapper.css('opacity', '1');
					if(typeof videoPlayer !== "undefined" && typeof videoPlayer.player !== "undefined"){
						if(typeof videoPlayer.player.playVideo === "function"){
							videoPlayer.player.pauseVideo();
						}
					}
					//slider.slick('slickPlay');
				}
				else{
					activeSlide.find("[data-td-video]").show();
	                slideImage.attr( 'src', slideImage.attr('data-large-src') );
					if(typeof videoPlayer !== "undefined" && typeof videoPlayer.player !== "undefined"){
						if(typeof videoPlayer.player.playVideo === "function"){
							videoPlayer.player.playVideo();
						}
					}
					if(!autorotate) { slider.slick('slickPause'); }
				}
			}
		}

		function manageVideoState(activeSlide) {
			if (!activeSlide) {
	            return;
	        }
	        var imgWrapper = activeSlide.find(".imageOuter");
	        var videoPlayer = getVideoPlayer(activeSlide.attr("data-sv-video-id"));
	        var width =  $(window).width();
	        if (width < 1025  && width > 640) {
	            //activeSlide.find("[data-td-video]").hide();
	            imgWrapper.css('opacity', '1');
	            if (typeof videoPlayer !== "undefined" && typeof videoPlayer.player !== "undefined") {
	                if (typeof videoPlayer.player.playVideo === "function") {
	                    videoPlayer.player.pauseVideo();
	                }
	            }	            
	            if(!autorotate) { slider.slick('slickPause'); }
	        }
	        else if (width < 641) {
	            //activeSlide.find("[data-td-video]").hide();
	            imgWrapper.css('opacity', '1');
	            if (typeof videoPlayer !== "undefined" && typeof videoPlayer.player !== "undefined") {
	                if (typeof videoPlayer.player.playVideo === "function") {
	                    videoPlayer.player.pauseVideo();
	                }
	            }
	            //slider.slick('slickPlay');
	        }
	        else {
	            //activeSlide.find("[data-td-video]").show();
	            if (typeof videoPlayer !== "undefined" && typeof videoPlayer.player !== "undefined") {
	                if (typeof videoPlayer.player.playVideo === "function") {
	                    videoPlayer.player.playVideo();
	                }
	            }	        
	            if(!autorotate) { slider.slick('slickPause'); }
	        }
		}

		function getActiveSlide() {
	        for(var i = 0; i < slides.length; i++){
				if($(slides[i]).hasClass('slick-active')){
					return $(slides[i]);
				}
			}
	    }
	    
	    function getVideoPlayer(videoId) {
	        for (var i = 0; i < videoPlayers.length; i++) {
	            if (videoPlayers[i].options.video_id === videoId) {
	                return videoPlayers[i];
	            }
	        }
	    }

	    function pauseAllVideos() {
	        for (var i = 0; i < videoPlayers.length; i++) {
	            videoPlayers[i].player.pauseVideo();
	        }
	    }

	    function turnOnImages() {
	        for (var i = 0; i < imgWrappers.length; i++) {
	            $(imgWrappers[i]).css('opacity', '1');
	        }
	    }	    

	    function setUpVideos() {
	        if (slideVideos.length) {
	            buildVideos($(slideVideos[0]));
	        }
	    }

	    function buildVideos(slideVideo) {
	        var autoplay = 1;
	        var tempactiveSlide = getActiveSlide();
	        if (!tempactiveSlide) {
	            return;
	        }

	        if (!tempactiveSlide.hasClass('video-slide') || videoPlayers.length >= 1) {
	            autoplay = 0;
	        }

	        var videoId = slideVideo.attr("data-sv-video-id");
	        var videoType =  slideVideo.attr("data-sv-video-type");
	        var videoAudio =  slideVideo.attr("data-sv-video-audio");
	        var volume = 0;

	        if (videoAudio === "true") {
	            volume = 50;
	        }

	        new vp().init({
	            options: {
	                parent: slideVideo, 
	                type: videoType,
	                video_id : videoId,
	                iframe_id: 'video-iframe-' + videoId,
	                frameborder: 0,
	                scrolling: 'no',
	                width: "100%",
	                height: "1080",
	                volume: volume,
	                settings: {
	                   autoplay: autoplay,
	                    loop: 1,
	                    fullscreen: 1,
	                    controls : 0,
	                    modestbranding : 1,
	                    background: 1,
	                    vq: '1080',
	                    rel: 0,
	                    showinfo : 0,
	                    wmode: "transparent",
	                },
	                events: {
	                    ready: onReady,
	                    play: onPlay,
	                    pause: onPause,
	                },
	            }
	        }, onPlayerInit);
	    }

	    function onPlayerInit(_videoPlayer) {
	        videoPlayers.push(_videoPlayer);
	        [].shift.call(slideVideos);
	        setUpVideos();
	    }

	    function onPlay() {
	        if ($(window).width() > 1024) {
	            setTimeout(function() {
	                var activeSlide = getActiveSlide()
	                var videoPlayer = getVideoPlayer(activeSlide.attr("data-sv-video-id"));
	                if (videoPlayer) {
	                    activeSlide.find(".imageOuter").css('opacity', '0');
	                }
	            }, 2000)
	        }
	    }

	    function onReady() {
	        //setImage(getActiveSlide());
	        if ($(window).width() > 1024) {
	            setTimeout(function() {
	                var activeSlide = getActiveSlide()
	                var videoPlayer = getVideoPlayer(activeSlide.attr("data-sv-video-id"));
	                if (videoPlayer) {
	                    activeSlide.find(".imageOuter").css('opacity', '0');
	                }
	            }, 2000)
	        }
	    }

	    function onPause() {}

	    $('.video-mute').on('click', function () {
	        var activeSlide = getActiveSlide()
	        var videoPlayer = getVideoPlayer(activeSlide.attr("data-sv-video-id"));
	        if (!videoPlayer) {
	            return;
	        }
	        if ($(this).hasClass('mute')) {
	            $(this).removeClass('mute');
	            if ($(this).hasClass('youtube')) {
	                videoPlayer.player.setVolume(50);
	            } else {
	                videoPlayer.player.setVolume(1);
	            }
	        } else {
	            $(this).addClass('mute');
	            videoPlayer.player.setVolume(0);
	        }
	    });

	    $(window).on('resize', lodash.debounce(function() {
		    if(!disable_video_autoplay) {
		    	manageVideoState(getActiveSlide());
		    }		    
	    }, 150));

	    // Begin bandaid, something messes up load order and slick init function doesn't fire sometimes
	    setTimeout(function() {
	        if (!videoloaded && !disable_video_autoplay) {
	            //root.css({ visibility : "visible" });
	            root.css({ opacity : 1 });
	            setUpVideos();
	        }
	    }, 2500)
	    // END bandaid


	});
</script>
</div>
