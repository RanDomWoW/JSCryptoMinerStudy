<script>
require(["jquery", "jqueryui","moment","domReady!"], function($, jqueryui,moment){
	$('[data-sv-booking-datepicker]').datepicker({
		minDate: moment().format('l'),
		// showOn: 'button',
		// buttonImage: '/includes/public/assets/images/calicons.png',
		// buttonImageOnly: true
	});

	var booking = $(".bookingCont");
	if(booking != undefined){
		var offers = $('[data-sv-cat]');
		var ids = offers.attr('data-sv-cat');

		if(ids != undefined){
			var idsa = ids.split(',');
			
			for(var i=0;i<idsa.length;i++){
				if(idsa[i]==4 || idsa[i]==6){
					booking.hide();
				}
			}
		}
	}

	var defaultChildAge = 10;
	var defaultstartdate = moment().format('MM/D/YYYY');
	var defaultenddate = moment().add(1,'days').format('MM/D/YYYY');

	$('#hotel_checkin, #flights_checkin, #packages_checkin, #attraction_checkin').val(defaultstartdate);
	$('#hotel_checkout, #car_checkin, #flights_checkout, #packages_checkout, #attraction_checkout').val(defaultenddate);

	$('#car_checkout').val(moment().add(2,'days').format('MM/D/YYYY'));

	$('#leg1').val('from:PHX,to:EWR,departure:' + defaultstartdate + 'TANYT');
	$('#leg2').val('from:EWR,to:PHX,departure:' + defaultenddate + 'TANYT');

	$('#NumRoom, #TotalAdults, #TotalChild').change(function() { ProcessDetails($(this)); });
	$('#flight-adults, #flight-children, #flights_depart, #flights_checkin, #flights_checkout').change(function() { ProcessDetailsFlight(); });
	$("#booking_packages").on("change", "input,select", ProcessPackages);

	$("#checkrates").on("click",function(){
		$("#booktab_hotels").click()
	});

	function ProcessPackages(){
		$(".exp-killfirst", "#booking_packages").remove();
		var numRooms = parseInt($("[name='NumRoom']", "#booking_packages").val(), 10);
		for(var i = 2; i <= numRooms; i++){
			$("#booking_packages").append('<input class="exp-killfirst" type="hidden" name="NumAdult'+i+'" value="2" />');
		}
	}


//?GOTO=HOTSEARCH&SearchType=Destination&CityName=Seattle&InDate=11/9/2015&OutDate=11/12/2015&NumRoom=2&NumAdult=2&NumAdult2=2&NumChild=2&NumChild2=3&Child1Age=2&Child2Age=4&Rm2Child1Age=6&Rm2Child2Age=8&Rm2Child3Age=10

	function ProcessDetails(changedElement) {
		var _form = changedElement.parents("form");
		var temprooms = $('#NumRoom').val();
		var tempadults = $('#TotalAdults').val();
		var tempchildren = $('#TotalChild').val();
		var finaladults = "";
		var finalchildren = "";

		if(temprooms == 0) { temprooms = 1; }
		if(tempadults == 0) { tempadults = 1; }

		$(".exp-killfirst").remove();

		var rooms = [];
		for(var i = 1; i <= temprooms; i++){
			rooms.push({adults:1, children:0});
		}
		for(var i = 1; i <= temprooms; i++){
			if(tempadults > 0 && tempadults > temprooms){
				rooms[i-1].adults = rooms[i-1].adults + 1;
				tempadults--;
			}

			if(tempchildren > 0){
				rooms[i-1].children = rooms[i-1].children + 1;
				tempchildren--;
			}
		}
		if(tempadults > 0 && tempadults > temprooms){
			rooms[rooms.length - 1].adults += tempadults;
		}
		if(tempchildren > 0){
			rooms[rooms.length - 1].children += tempchildren;
		}

		for(var i = 0; i < rooms.length; i++){
			var person_number = (i + 1);
			var room_number = "Rm" + (i + 1);

			_form.append('<input class="exp-killfirst" type="hidden" name="NumAdult'+person_number+'" value="'+rooms[i].adults+'" />');
			if(rooms[i].children){
				_form.append('<input class="exp-killfirst" type="hidden" name="NumChild'+person_number+'" value="'+rooms[i].children+'" />');
				_form.append('<input class="exp-killfirst" type="hidden" name="'+room_number+'Child'+person_number+'Age" value="7" />');
			}
		}

		/*for(var r = 1; r <= temprooms; r++){
			_form.append('<input class="exp-killfirst" type="hidden" name="NumAdult'+r+'" value="'+adults_per_room+'" />');
			_form.append('<input class="exp-killfirst" type="hidden" name="NumChild'+r+'" value="'+children_per_room+'" />');
		}*/

		/*for(var a = 2; a <= temprooms; a++){
			_form.append('<input class="exp-killfirst" type="hidden" name="NumAdult'+a+'" value="'+tempadults+'" />');
		}

		var child_distribute = tempchildren >> 1;
		for(var c = 1; c < temprooms >> 1; c++){
			for(var d = 0; d < child_distribute; d++){
				_form.append('<input class="exp-killfirst" type="hidden" name="NumChild'+c+'" value="1" />');
				_form.append('<input class="exp-killfirst" type="hidden" name="Child'+c+'Age" value="7" />');
			}
		}*/

		for(var i = 1; i <= temprooms; i++) {
			if(finaladults == "") {
				finaladults = tempadults;
			} else {
				finaladults = finaladults + ',1'; // default to 1 after first room
			}
		}

		for(var i = 1; i <= tempchildren; i++) {
			if(finalchildren == "") {
				finalchildren = '1_' + defaultChildAge; // forcing all into first room
			} else {
				finalchildren = finalchildren + ',' + 1 + '_' + defaultChildAge; // forcing all into first room
			}
		}
		$('input[name=adults]').val(finaladults);
		$('input[name=children]').val(finalchildren);
	}

	function ProcessDetailsFlight() {
		var tempadults = $('#flight-adults').val();
		var tempchildren = $('#flight-children').val();
		var tempdepartcity = $('#flights_depart').val();

		var tempcheckin = $('#flights_checkin').val();
		var tempcheckout = $('#flights_checkout').val();

		var finaladults = "";
		var finalchildren = "";
		var finalleg1 = "";
		var finalleg2 = "";

		if(tempadults > 0) { finaladults = "adults:" + tempadults; }
		if(tempadults == 0 && tempchildren == 0) { finaladults = "adults:1"; }

		if(tempdepartcity == "Departing Airport" || tempdepartcity == "PHX") { tempdepartcity = "PHX"; }

		for(var i = 1; i <= tempchildren; i++) {
			if(finalchildren == "") {
				finalchildren = 'children:' + tempchildren + '[' + defaultChildAge;
			} else {
				finalchildren = finalchildren + ';' + defaultChildAge;
			}
		}
		if(finalchildren != "") { finalchildren = finalchildren + ']'; }
		if(finalchildren != "" && finaladults != "") {
			finaladults = ',' + finaladults;
		}
		$('#passengers-flight').val(finalchildren + finaladults);

		$('#leg1').val('from:' + tempdepartcity + ',to:EWR,departure:'+ tempcheckin + 'TANYT');
		$('#leg2').val('from:EWR,to:' + tempdepartcity + ',departure:'+ tempcheckout + 'TANYT');
	}

	

	
		$("[data-sv-booking] [data-sv-booking-tabs] > div").click(function(){
			var current = $(this).attr("id");
			$(this).addClass("current");
			$("[data-sv-booking] div#" + current).addClass("current");
		});
		$("[data-sv-booking] .close").click(function(){
			$("[data-sv-booking] [data-sv-booking-form]").removeClass("current");
		})
	

});
</script></div>
