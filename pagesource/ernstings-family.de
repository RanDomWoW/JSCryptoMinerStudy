function getCookie(cname) {
  var name = cname + "=";
  var ca = document.cookie.split(';');
  for (var i = 0; i < ca.length; i++) {
      var c = ca[i];
      while (c.charAt(0) == ' ') c = c.substring(1);
      if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
  }
  return "";
};

 function setDevice() {
  var device = getCookie("deviceType");
  if(device && (device == 'D' || device == 'T', device == 'M')) {
    device = device;
  } else {
    device = 'D';
  }
  tc_vars.env_device =  device;
};

function setLanguage() {
  tc_vars.env_language =  'de';
};

function setEnvWork() {
  tc_vars.env_work =  "prod";
};

function setCountry(langId) {
  if(langId === '-1000') {
    tc_vars.env_country =  'AT';
  } else {
    tc_vars.env_country =  'DE';
  }
};

setEnv();
</script>


