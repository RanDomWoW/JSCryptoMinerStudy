<script>
    $(document).ready(function() {
        $('[data-toggle="popover"]').popover();
        var header = $("#nav");
        var originalTop = header.position().top;
        $('#nav-dummy').height(header.height());

        $(window).scroll(function() {
            var scroll = $(window).scrollTop();

            if (scroll >= originalTop) {
                $('#nav-dummy').show();
                header.addClass("navbar-fixed-top navbar-animate-border");
                header.removeClass("container");
            } else {
                $('#nav-dummy').hide();
                header.addClass("container");
                header.removeClass("navbar-fixed-top navbar-animate-border");
            }
        });

        $('#menu-additional-toggle').click(function () {
            $('#menu-additional').slideToggle();
            $('#menu-additional-toggle span').toggleClass('glyphicon-menu-hamburger').toggleClass('glyphicon-remove-sign');
            return false;
        });

        // Track advert views
        var trackedAds = [];
        $('.ad-tracked').each(function(e) {
            var id = $(this).data('ad-id');
            if(!trackedAds.includes(id)) {
                trackedAds.push(id);
                ga('send', 'event', 'YAMU_Ads', 'View Ad', id);
            }
        });

        // Track advert clicks
        $('.ad-tracked').click(function(e) {
            var id = $(this).data('ad-id');
            var url = $(this).attr('href');
            ga('send', 'event', 'YAMU_Ads', 'Click Ad', id, {'hitCallback':
                function () {
                    document.location = url;
                }
            });
            e.preventDefault();
        });
    })
</script>

