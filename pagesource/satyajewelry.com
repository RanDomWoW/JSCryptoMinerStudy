  <script id="cartTemplate" type="text/x-handlebars-template">
    
      {{#each items}}
        {{# if this.properties._bundle-child }}
          <input type="hidden" name="updates[]" value="{{ this.quantity }}" data-bundle-id="{{ this.properties._bundle-id }}"/>
        {{ else }}
          <div class="quick-cart-item">
            <div class="quick-cart-image">
              <a href="{{ this.url }}" title="{{ this.title }}">
                <img src="{{ this.image }}" alt="{{ this.title }}" />
              </a>
              </div>
                <div class="quick-cart-details">
                  <p>
                    <a href="{{ this.url }}">{{ this.title }}</a>

                    {{#if this.properties._bundle-contents }}
                      <p class="lip">{{{ this.properties._bundle-contents }}}</p>
                    {{/if}}

                    {{#each this.discounts }}
                      <br><span class="cart-discount-message">{{ this.title }}</span>
                    {{/each}}
                 </p>
                  <p>
                    {{#if this.properties._bundle-price }}
                      {{ this.properties._bundle-price }}
                    {{else }}
                      {{# if this.discounted }}
                        <span class=money>{{ this.discountedPrice }}&nbsp;<del>{{ this.price }}</del></span>
                      {{else }}
                        <span class=money>{{ this.discountedPrice }}</span>
                      {{/if }}
                    {{/if }}
                  </p>
                  <p>
                    <input type="hidden" name="updates[]" value="{{ this.quantity }}" data-id="{{ this.variant.id }}" data-bundle-id="{{ this.properties._bundle-id }}"/>
                    <a class="remove_item" href="#" data-id="{{ this.id }}" data-bundle-id="{{ this.properties._bundle-id }}">Remove</a>
                </p>
              </div>
          </div>
        {{/if}}
      {{/each}}
    
    <a class="checkout-link" href="/cart">Checkout</a>
  </script>




