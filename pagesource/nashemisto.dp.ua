function checkStickyMenu() {
    
    var page = jQuery('#wrapper');
    
    var topPosValue = jQuery('#topheader-wrap').height() + jQuery('#header').height();
    
    
    
    if (jQuery(window).scrollTop() > topPosValue) {
        if (!page.hasClass('page_has_sticky')) {
            page.addClass('page_has_sticky');
            
            var hmw = jQuery('#header-menu-wrap').height()+'px';
            jQuery('#wrapper').css('padding-top', hmw);
        }
    }
    else {
        if (page.hasClass('page_has_sticky')) {
            page.removeClass('page_has_sticky');
            jQuery('#wrapper').css('padding-top', '0px');
        }
    }
    
}

jQuery(window).scroll(function(){
    checkStickyMenu();
});



