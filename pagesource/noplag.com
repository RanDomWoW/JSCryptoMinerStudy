                    this.innerHTML = num.toFixed(1) + ' +'
                }
            });
            $('#num2').animate({ num: 10 - 1 }, {
                duration: 2000,
                step: function (num){
                    this.innerHTML = +(num + 1).toFixed(1) + ' +'
                }
            });
            $('#num3').animate({ num: 5 - 1 }, {
                duration: 2000,
                step: function (num){
                    this.innerHTML = +(num + 1).toFixed(1) + ' +'
                }
            });
        })
    </script>
    <script>
        // expand block with feautures
        $(document).on('click', '.js_more-features', function(){
            var  hideClassLength = $('.hide-feautures').length;
            $('.js_awesome-features').toggleClass('hide-feautures');
            if(hideClassLength){
                $('.js_more-features').text('Less features');
                $('.js_more-features').addClass('less-features');
            }else{
                $('.js_more-features').text('More features');
                $('.js_more-features').removeClass('less-features');
            }
        });

        //expand noplag description text
        $('.expand-button').on("click", function() {
            $('.noplag-description').toggleClass('is-expanded');
            $('.js-expanded').slideToggle('fast');
        });

    </script>
    <script>
        $(document).ready( function() {
            if ( $('.js-textarea-focus').val()) {
                $('.js-bg-text').hide();
            }
            $('.js-textarea-focus').on('focus', function() {
                $('.js-bg-text').hide();
            })
            $('.js-textarea-focus').on('focusout', function() {
                if ( !$('.js-textarea-focus').val()) {
                    $('.js-bg-text').show();
                }
            })
            $('#captcha').hide();
            if ( $('.js-textarea-focus').val() !== '' ) {
                $('#captcha').show();
            }
            $('.form-start-checking__textarea').on('input', function() {
                $('#captcha').show();
            });
        })
    </script>
    <script>
        $('.js-tab-report').on('change', function() {
            if($(this).is(':checked')) {
                $('.js-report-dark').hide();
                $('.js-report-light').show();
            } else {
                $('.js-report-dark').show();
                $('.js-report-light').hide();
            }
        });

        // scroll to plagiarizm textarea
        $('button.plagiarism-check').on('click', function(){
            $('body').scrollTo( 0, {duration: 300});
        });
    </script>

