					var member_first_char = logged_in_member.first_name.substr(0,1).toUpperCase();
					
					$(".member_trigger > span").html(member_first_char).parent().addClass("logged_in");
					$.each(logged_in_member,function(key,value){
						$(".form.profile .field[data-key="+key+"]").val(value);	
					});
					$(".form.profile .profile_header .thumb_container").html(member_first_char);
					$(".form.profile .profile_header .member_name").html(logged_in_member.first_name + " " + logged_in_member.last_name);
					$(".form.profile .profile_header .member_email").html(logged_in_member.email);
					
					ga('karnaval.set', 'dimension2', 'logged_in');
