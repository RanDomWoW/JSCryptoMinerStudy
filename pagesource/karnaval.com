			function get_form_data(selector){
				var to_return = {"result":true,"data":{}};
				$(".error",selector).removeClass("error");
				$(".field",$(selector)).each(function(index,field){
					var key 	= $(field).data("key");
					var value 	= $.trim($(field).val());
					if($(field).is(":checkbox")){
						value = $(field).is(":checked") ? 1 : 0;
					}
					
					if($(field).hasClass("forced") && value == ""){
						$(field).addClass("error");
						to_return.result = false;
					}
					
					to_return.data[key] = value;
				});
				
				return to_return;
			}
			
