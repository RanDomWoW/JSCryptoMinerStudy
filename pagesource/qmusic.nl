<script type="text/html" id="tmpl_mgp_rating">
  <div class="grid-item mgp-tile">
    <div class="aspect">
      <div class="inner">
        <h2 class="mgp-title">Kies de volgende track</h2>
        <%
        for(var i=0;i<this.songs.length;i++){
        var song = this.songs[i];
        %>
        <div class="song" data-song-id="<%= song.id %>">
          <div class="cover">
            <img src="https://static1.qmusic.medialaancdn.be/cover/w600/<%= song.image_hash %>" />
            <div class="overlay-title">
              <span class="artist"><%= song.artist %></span>
              <span class="track"><%= song.title %></span>
            </div>
          </div>
          <div class="mgp-button">
            <span class="progress" style="width:<%= song.score.percentage %>%;"></span>
            <span class="text" data-vote-text="STEM" data-voted-text="GESTEMD" data-winner-text="WINNAAR" data-loser-text="">STEM</span>
          </div>
        </div>
        <% } %>
      </div>
    </div>
  </div>
</script>

    
