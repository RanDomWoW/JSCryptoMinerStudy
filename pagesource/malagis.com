    $( '#tab-container-2' ).Tabs();

    
        // home slider
    $('#slider').nivoSlider({
      pauseTime: 9000,
      effect: 'fade' 
    });
    //ajax-load-index
    var canload = 1;
    var currentpage = 1;
    var allpages = 140;
    $('.load-more').on('click',function(){
      if(canload==1&&currentpage<allpages){
        //set flag
        canload = 0;
        //change style
        $('.load-more').addClass('loading-more');
        $('.load-more').html('加载中...<img src="https://malagis.com/usr/themes/krrrrr/images/loading.gif" width="12">');
        //load data
        currentpage ++;
        var load_post_url = "https://malagis.com/" + "page/" + currentpage + "/";
        $.get(load_post_url,function(html){
          var newposts = $(html).find(".post");
          $('.ajax-page-loader').before(newposts);
          //reset flag
          canload = 1;
          //reset style
          if(currentpage>=allpages){
            $('.load-more').remove();
          }else{
            $('.load-more').removeClass('loading-more');
            $('.load-more').html('加载更多');
          }
        })
      }
    });
        
    
    
    //left-right-follow
    $(window).on('scroll', throttle(followAction, 50));
    // $(window).on('scroll', followAction);
  });

  /**
  * Global Script
  **/

  function debounce(fn, delay) {
    var timer = null;
    return function() {
      var con = this;
      var args = arguments;
      // console.log(con);
      // console.log(args);
      clearTimeout(timer);
      timer =  setTimeout(function() {
        fn.apply(con, args);
      }, delay)
    }
  }
  function throttle(fn, threshhold) {
    var last,timer;

    threshhold || (threshhold = 250)
    return function () {
      var context = this
      var args = arguments
      var now = +new Date()
      if (last && now < last + threshhold) {
        clearTimeout(timer)
        timer = setTimeout(function () {
          last = now
          fn.apply(context, args)
        }, threshhold)
      } else {
        clearTimeout(timer)
        last = now
        fn.apply(context, args)
      }
    }
  }
  function followAction() {
    //for left
    var v= viewport();
    if(v['width']>940){
      if($("#leftboard").height()<$("#main").height()){
        if($("#leftboard").height()+$(document).scrollTop()>=$("#main").height()){
          var lefttop = $("#main").height() - $("#leftboard").height()-25;
          $("#leftboard").css({ position: "absolute", top: lefttop+"px" });
        }else{
          $("#leftboard").css({ position: "fixed", top: "auto" });
        }
      }else{
        $("#leftboard").css({ position: "static", top: "auto" });
      }
    } else {
      $("#leftboard").css({ position: "relative", top: "auto" });
    }
    //for right
    rightHeightResize();
  }

  /**
  * 右边高度自动调整
  */
  function rightHeightResize(){
    var vp = viewport();
    var sc = $(document).scrollTop();
    var tabh = $("#tab-container-2").height();
    var mh = $("#main").height();
    var h  = $(window).height();
    var sh = $("#sidebar").height();
    var tuih = $("#tab-container").height();
    if(mh-200>sh&&vp['width']>940&&h>371){
      var fw=$("#tab-container-2").width();
      tabh = tabh + 270 + 20 + 20; 
      // (tab-container-2'h&margin(20)) + (ad'h+margin(250+20)) + global-side-margin(20)
      $("#sidebar-follow,#tab-container,#gomoney").css({"width":fw+"px"});
      if(sc<tabh){
        $("#tab-container").css({position: "static", top: "auto"});
        $("#sidebar-follow").css({position: "static", top: "auto"});
        //$("#gomoney").css({position: "static", top: "auto"});
      }
      if(sc>tabh&&sc+tuih+275<mh-25){
        $("#tab-container").css({position: "fixed", top: "5px"});
        $("#sidebar-follow").css({position: "fixed", top: 25+tuih+"px"});
      }
      if(sc+tuih+275>mh-25){
        var toph = mh - 275;
        $("#tab-container").css({position: "absolute", top: toph-tuih-20+"px"});
        $("#sidebar-follow").css({position: "absolute", top: toph+"px"});
      }
    }
  }
  /**
  * 获取外部宽度
  **/
  function viewport(){
    var e = window
    , a = 'inner';
    if ( !( 'innerWidth' in window ) )
    {
    a = 'client';
    e = document.documentElement || document.body;
    }
    return { width : e[ a+'Width' ] , height : e[ a+'Height' ] }
  }

  /**
  * 生成文章目录
  **/
  function createCatalog() {
    if ($('.justcon h2').length < 2) return;
    $('.justcon h2').each(function(i) {
      $('#postCatalog ul').append('<li>'+$(this).html()+'</li>');
    });
    $('.justcon img').eq(0).css({"max-width":"405px"});
    $('#postCatalog').show();
  }
</script>
  
