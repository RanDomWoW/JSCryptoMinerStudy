<script type="text/javascript">
		//滚动加载
		var go_top = $("#go_top");
		var listLoad = $("#listLoad");
		var cateMore = $(".cateMore");
		var cataid = cateMore.attr("data-id");
		var cataNode = $("#cate"+cataid);
		var curP;//当前页数
		
		var cataArr = {"72486":{"num":"0","name":""}};
		
		go_top.click(function(){$(window).scrollTop(0);});
				$(window).scroll(function(){
			var scrollTop = $(this).scrollTop();
			var scrollHeight = $(document).height();
			var windowHeight = $(this).height();
			
			if($(window).scrollTop() > $(".sakura").offset().top){go_top.show();$(".leaf").addClass("position");
			$(".tree").css("padding","0px 20px 0px 150px");}
			
			else{go_top.hide();$(".leaf").removeClass("position");$(".tree").css("padding","0px 20px 0px 0px");}
			go_top.click(function(){$(window).scrollTop(0)});
			
			
		});
		
		$(".cateMore").click(function(){
			cateMore.hide();
			listLoad.show();  
			
			cataArr[cataid].num++;
			curP = cataArr[cataid].num;
			
			var getUrl = "http://m.cnwest.com/pc/?p="+curP+"&pagesize=22&cateid="+cataid;
			$.ajax({
				type : "GET", 
				async:false,
				url : getUrl,
				dataType : "jsonp",
				jsonp: "callback",
				success : function(data){ 
					//alert(data.name);
					if(data.status == "0"){
						var node = "";
						if(parseInt(data.p)+1 < data.pagenum)
						{cateMore.show();}
						else{cateMore.hide();}
			
						for(var i=0;i<data.list.length;i++){
							node += '<li class="warp-tw"><div class="warp-tw-pic"><a href="'+data.list[i].url+'" target="_blank"><img src="'+data.list[i].thumb+'"></a></div><div class="warp-tw-nr"><h3><a href="'+data.list[i].url+'" target="_blank">'+data.list[i].title+'</a></h3><p><span>'+geMyTime(data.list[i].pubtime,3)+'</span></p></div></li>';
						}
						cataNode.append(node);
						listLoad.hide();
					}
					if(data.status == "-3"){
					cateMore.html("已加载完全部数据").show();
					listLoad.hide();
					}
					else{console.log(data.msg)}
				},
				 error: function(){
					 alert('error');
				 }
			});
		})
		/*时间格式化*/
		function geMyTime(str,type)
		{
			str = parseInt(str)*1000;
			var content = "";
			var addtime_obj = new Date();
			addtime_obj.setTime(str);
			add_year = addtime_obj.getYear();
			add_year = add_year<1900?(1900+add_year):add_year;
			add_mon = addtime_obj.getMonth()+1;
			if(add_mon<10) add_mon='0'+add_mon;
			add_daily = addtime_obj.getDate();
			if(add_daily<10) add_daily='0'+add_daily;
			add_hour = addtime_obj.getHours();
			if(add_hour<10) add_hour='0'+add_hour;
			add_min = addtime_obj.getMinutes();
			if(add_min<10) add_min='0'+add_min;
			add_sec = addtime_obj.getSeconds();
			if(add_sec<10) add_sec='0'+add_sec;
			switch(type)
			{
				case 1://2001-12-12 12:12:01
				content = add_year+"-"+add_mon+"-"+add_daily+" "+add_hour+":"+add_min+":"+add_sec;
				break;
				case 2://12-12 12:12:01
				content = add_mon+"-"+add_daily+" "+add_hour+":"+add_min+":"+add_sec;
				break;
				case 3://2001-12-12
				content = add_year+"-"+add_mon+"-"+add_daily;
				break;
				case 4://12-12 12:12
				content = add_mon+"-"+add_daily+" "+add_hour+":"+add_min;
				break;
				case 5://2001-12-12 12:12:01
				content = add_year+"-"+add_mon+"-"+add_daily+" "+add_hour+":"+add_min;
				break; 
				case 6://12-12
				content = add_mon+"-"+add_daily;
				break; 
			}
			return content;
		}
	</script>
