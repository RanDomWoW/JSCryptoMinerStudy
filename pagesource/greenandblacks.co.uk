<script type="text/javascript">
function getUpdatedPrice(pro_quantity, proId, updatedElement) {
	url = 'https://www.greenandblacks.co.uk/updatecartquantity/index/updatequantity';
	data = {isAjax: 1, product_id: proId, product_quantity: pro_quantity};
	jQuery.ajax({
		url: url,
		data: data,
		type: 'POST',
		dataType: 'json',
		success: function(data) {
			jQuery('.cart-arrow-wrapper').removeClass('showloading');
			// updatedElement.closest('div.qty-price-wrapper').children('span.price').html(data.price);
			jQuery('div.cart-content-item').find('span.cartqty > span.item-qty').html(data.totalItems);
			jQuery('div.cart-content-item').find('span.price').html(data.subTotal);
			jQuery('#cart-sidebar').html(data.cartContent);
			jQuery('#reward-msg').html(data.reward);
			jQuery('#minicart-incr-qty.incr-qty').on('click', function() {
			jQuery('.cart-arrow-wrapper').addClass('showloading');
			incElement = jQuery(this);
			incElement.parent('.qty-box').children('#minicart-dec-qty').removeClass('disable');
			incrementedValue = parseInt(incElement.parent('.qty-box').children('#minicart-qty').val()) + 1;
			productId = incElement.parent('.qty-box').children('input#p-id').val();
			incElement.parent('.qty-box').children('#minicart-qty').val(incrementedValue);
			if(incElement.parent('.qty-box').find('.personalise-item').length > 0)
			{
				personaliseQty = incElement.parent('.qty-box').children('#personalise-item-qty').val();
				personaliseCartValue = incElement.parent('.qty-box').children('#minicart-qty').val();
				optionId = incElement.closest('.product-details').children('.personalise-delete').attr('data-val');
				setPersonaliseQty = personaliseQty * personaliseCartValue;
				incrementedValue = setPersonaliseQty;
			}
			getUpdatedPrice(incrementedValue, productId, incElement);
		});

		jQuery('#minicart-dec-qty.dec-qty').on('click', function() {
			jQuery('.cart-arrow-wrapper').addClass('showloading');
			decElement = jQuery(this);
			decrementedValue = parseInt(decElement.parent('.qty-box').children('#minicart-qty').val()) - 1;
			productId = decElement.parent('.qty-box').children('input#p-id').val();
			if (decrementedValue > 0) {
				if (decrementedValue == 1) {
					decElement.parent('.qty-box').children('#minicart-dec-qty').addClass('disable');
				}
				decElement.parent('.qty-box').children('#minicart-qty').val(decrementedValue);
				if(decElement.parent('.qty-box').find('.personalise-item').length > 0)
				{
					personaliseQty = decElement.parent('.qty-box').children('#personalise-item-qty').val();
					personaliseCartValue = decElement.parent('.qty-box').children('#minicart-qty').val();
					optionId = decElement.closest('.product-details').children('.personalise-delete').attr('data-val');
					setPersonaliseQty = personaliseQty * personaliseCartValue;
					decrementedValue = setPersonaliseQty;
				}
				getUpdatedPrice(decrementedValue, productId, decElement);
			}
		});
		},
		error: function(xhr, textStatus, thrownError) {
			jQuery('.cart-arrow-wrapper').removeClass('showloading');
			console.log(thrownError);
		}
	});
} 
</script>


