	<script>
		$(function(){
			var initalFontSize = 100;
			var currentFontSize = initalFontSize;
			var step = 10;
			var range = 40;

			function updateFontSize(size){
				if(size < (initalFontSize-range) || size > (initalFontSize+range)) return;
				currentFontSize = size;
				$(":root").css("font-size", currentFontSize + "%");
			}

			$("#topbar a.smaller").click(function(){
				updateFontSize(currentFontSize-step);
			});
			$("#topbar a.medium").click(function(){
				updateFontSize(initalFontSize);
			});
			$("#topbar a.bigger").click(function(){
				updateFontSize(currentFontSize+step);
			});
			$("#topbar a.contrast").click(function(){
				$("body").toggleClass("dark");
			});

			$("#topbar .web.actions a").each(function(){
				var navItem = $(this)
