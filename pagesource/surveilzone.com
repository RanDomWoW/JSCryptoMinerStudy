                        function changenum(rec_id, diff)
            {
            var goods_number = Number($('#mini_goods_number_' + rec_id).val()) + Number(diff);
            if (goods_number <= 0) {
            var msg = 'Wrong goods number';
            alert(msg);
            return false;
            }
            change_goods_number(rec_id, goods_number);
            }
            function change_goods_number(rec_id, goods_number)
            {
            $.ajax({
            type: "POST",
            dataType: 'JSON',
            url: '/cart/ajax_update_cart',
            data: 'rec_id=' + rec_id + '&goods_number=' + goods_number,
            success: change_goods_number_response
            });    
                 
            }
            function change_goods_number_response(result)
            {
                           
                            if (result.error == 0)
                            {
                                var rec_id = result.rec_id;
                                $('#goods_number_' + rec_id).val(result.goods_number);//更新数量
                                $('#mini_goods_number_' + rec_id).val(result.goods_number);//更新数量
            $('#goods_number_box_' + rec_id).html(result.goods_number);//更新数量                                
                                $('#goods_subtotal_' + rec_id).html(result.goods_subtotal);//更新小计
                                $('#mini_goods_subtotal_' + rec_id).html(result.goods_subtotal);//更新小计
                                if (result.goods_number <= 0)
                                { // 数量为零则隐藏所在行
                                    $('#tr_goods_' + rec_id).hide();
                                    $('#tr_goods_' + rec_id).html('');
                                }
            var all_price=result.all_price;
            $.each(all_price,function(index){
                 
                $('#mini_goods_subtotal_' + index).html(this);
            });

            $('.total_desc').html(result.shopping_money);//更新合计
                $('#total_discount').html(result.total_desc);//更新合计                 
                                if ($('#total_num'))
                                { //更新购物车数量
            $('.cart_count').html(result.real_goods_count);
                                   $('#total_num').html(result.real_goods_count);
                                }
            $('.car_nume').html(result.real_goods_count);
                            }
                            else if (result.message != '')
                            {
                                alert(result.message);
                            }                
                        }
            function go_checkout()
            {
            $.post("/checkout/index",{ajax:1}, function(result) {
            if (parseInt(result) == '1')
            {
            //location.href = '/user/login';
            openLoginDiv();
            }
            else if (parseInt(result) == '2')
            {
            location.href = '/checkout/consignee';
            }
            else
            {
            // $(".shopcar_detail_info").html(result).show();
            location.href = '/checkout/index';
            }

            });
            }
