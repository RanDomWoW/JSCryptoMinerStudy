    function trackAddToCart_GTM(productCode, quantityAdded, name, price) {
        
        var dataLayer = window.dataLayer || [];
        window.dataLayer = window.dataLayer || [];
        dataLayer.push({
            'event': 'add-to-cart',
            'productId': productCode,
            'productName': name,
