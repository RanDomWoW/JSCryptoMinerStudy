            console.log(images[i].offsetWidth === 0 && images[i].offsetHeight === 0);

             var src,availableWidths = [],srcARRAY,cwidth;
             if( images[i].getAttribute("data-media")!=undefined){

                var eMedia = images[i].getAttribute("data-media");
                eMedia =  JSON.parse(eMedia);

                for (var key in eMedia) {
                  if (eMedia.hasOwnProperty(key)) {
                    availableWidths.push(parseInt(key));
