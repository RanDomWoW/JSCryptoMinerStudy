                cwidth = imageGetClosestValue(window.innerWidth, availableWidths);

                if(images[i].getAttribute("src") == srcARRAY[cwidth]){return;}
                images[i].setAttribute("src", srcARRAY[cwidth]);

                images[i].addEventListener('load', showCategoryCollageComponent, false);

            }

        }
    }


};

