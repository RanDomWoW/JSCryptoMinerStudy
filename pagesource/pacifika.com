function showCategoryCollageComponent(){
    var categoryCollageComponents = document.getElementsByClassName("category-collage-component");

    for(var i=0; i<categoryCollageComponents.length; i++){
        var categoryCollageComponent = categoryCollageComponents[i];
        if(categoryCollageComponent) {
            var classVisibility = categoryCollageComponent.getAttribute("data-visibility");
            if(classVisibility==='' || classVisibility===' '){
                categoryCollageComponent.style.display = "block";
            } else {
                var currentClass = categoryCollageComponent.getAttribute("class");
                var newClass = currentClass.replace("display-none", classVisibility);
                categoryCollageComponent.removeAttribute("class");
                categoryCollageComponent.setAttribute("class",newClass);
            }

            var images = categoryCollageComponent.getElementsByTagName("img");
            for(var j=0; j<images.length; j++){
                var src = images[j].getAttribute("data-src");
                images[j].setAttribute("src", src);
            }
        }
    }
}

