<script>
  function footerMobileDropdown(){
  	if ($(window).width() < 1024) {
  		$("footer.footer--media li.hs-item-has-children > .hs-menu-children-wrapper").css("display", "none");	 
  		if(!$("footer.footer--media li.hs-item-has-children i").length ) {
  			$("footer.footer--media li.hs-item-has-children > .hs-menu-children-wrapper").before("<i class='fa fa-plus' aria-hidden='true'></i>");
  		}	
  	} else {
  		$("footer.footer--media li.hs-item-has-children .hs-menu-children-wrapper").css("display", ""); 
  		$("footer.footer--media li.hs-item-has-children i").remove();
  	}	
  }	
  
  function blogHeaderSearch(searchForm) {
  	$(".site--head--search, .header--main--search").each(function() {
  		if (($(searchForm).find('.header--search--box--input').val().length||"") > 0) {
  			window.location.href = "/blog?search="+$(searchForm).find('.header--search--box--input').val();
  		}
  		else {
  		}
  	});    
  }
  
  function searchOpen(){
  	$(window).resize(function() {
  		if($(".header--media").hasClass('header--search--active')) {
  			if ($(window).width() < 840) {
  				$('.header--main--logo--image').css('display','none');
  			} else {
  				$('.header--main--logo--image').css('display','inline-block');
  			}
  		}
  	});      
  	if(!$(".header--media").hasClass('header--search--active')) {
  		$('.categories--submenu').css('z-index','95');
  		$('.header--media').addClass('header--search--active');
  		$('.header--main--nav--standard').fadeOut(500, function () {
  			$('.header--main--nav--close').fadeIn();
  			$('.header--sub-overlay').fadeIn();  
  			$(".header--search--main input").focus();
  		});
  		if ($(window).width() < 840) {
  			$('.header--main--logo--image').fadeOut(500);
  		}
  		if($(".header--main").hasClass('background--none')) {
  			$('.header--media').css('background','rgba(0,0,0,0.9)');   
  		} else {
  		}   
  	}
  }    
  
  function searchClose(){
  	if($(".header--media").hasClass('header--search--active')) {
  		$('.header--media').removeClass('header--search--active');
  		$('.header--main--nav--close').fadeOut(500, function () {
  			$('.header--main--nav--standard').fadeIn();
  			$(".header--search--main input").blur(); 
  			$('.header--main--logo--image').fadeIn(500);
  		});
  		if($(".header--main").hasClass('background--none')) {
  			$('.header--media').css('background','');   
  		} else {
  		}  
  		$('.header--sub-overlay').fadeOut().promise().done(function() {
  			$('.categories--submenu').css('z-index','')
  		});
  	} else {
  	}
  }   
  
  
  function subMenuClose(){
  	$('.header--media').css('z-index','');
  
  	if ($('.header--sub').hasClass("header--sub--topics")){
  		$('.header--sub--topics-menu--subnav .fa-caret-up').fadeOut(500, function () {
  			$('.header--sub--topics-menu--subnav .fa-caret-down').fadeIn();
  		});
  	}	
  	$('.header--sub--mobile--arrow .fa-caret-up').fadeOut(500, function () {
  		$('.header--sub--mobile--arrow .fa-caret-down').fadeIn();
  	});	
  
  	$('.header--sub--mobile').css('max-height','');
  	$('.header--sub').removeClass("slideActive");
  
  	$('.header--sub-overlay').fadeOut();
  }	
  
  // Update the menu height when we resize the window since things could shift!
  function headerMenuHeight() {
  	$(window).resize(function () {	
  		if ($('.header--sub').hasClass("slideActive")){
  			if ($(window).width() > 1024) {
  				if ($('.header--sub').hasClass("header--sub--topics")){
  					$('.header--sub .header--sub--mobile').css('max-height',$('.header--sub--mobile').find('.header--sub--mobile--container').outerHeight(true)+'px');
  				}
  			} else {
  				$('.header--sub .header--sub--mobile').css('max-height',$('.header--sub--mobile').find('.header--sub--mobile--container').outerHeight(true)+'px');
  			}
  		}
  
  	});
  }		
  
  $(document).ready(function () {
  	$("a[href='#menu--scroll--top']").click(function() {
  		$("html, body").animate({ scrollTop: 0 }, "slow");
  		return false;
  	});	
  
  	$(".header--main-mobile-wrapper li.hs-item-has-children .hs-menu-children-wrapper").css("display", "none"); 
  	$(".header--main-mobile-wrapper li.hs-item-has-children").append("<i class='fa fa-caret-down' aria-hidden='true'></i>");
  	$(".header--main-mobile-wrapper li.hs-item-has-children").addClass("relative");
  
  	$('.header--main-mobile-wrapper li.hs-item-has-children i').click(function(event) {
  		event.preventDefault();
  		if($(this).parent().find("a").next().first().hasClass('slow')){return;}
  		$(this).parent().find("a").next().first().addClass('slow');
  		$(this).parent().addClass('active');
  
  		$(this).parent().find("a").next().first().stop().slideToggle(300,function(){
  			$(this).removeClass('slow');
  			$(this).parent().addClass('active');
  		});
  		$(this).stop().toggleClass('fa-caret-down fa-caret-up');
  	});
  
  	$('.header--main--nav--close img').click(function() {
  		searchClose();
  	});
  	var isAnimating = false;
  	$(".header--nav-icon").click(function(event) {
  		if (isAnimating) {
  			return;
  		}
  		isAnimating = true;
  		if($(".header--media").hasClass('header--search--active')) {
  			searchClose();
  		}
  		if($(".header--media").hasClass('hamburger--active hamburger--background')) {
  			$('.header--media').removeClass('hamburger--background');
  			$('.header--nav-icon').removeClass('open');
  			$('.header--main-mobile-wrapper').fadeOut(500, function () {
  				$('.header--media').removeClass('hamburger--active');
  				$('body').removeClass('overflow--hidden');
  				$('.header--main-mobile-wrapper').css('display','block');   
  			});
  			if($("html").css('overflow-y') == 'hidden') {
  				$("html").css('overflow-y','scroll');
  			}
  		}
  		else {
  			$('.header--nav-icon').addClass('open');
  			$('.header--media').addClass('hamburger--active hamburger--background');
  			$('body').addClass('overflow--hidden');
  			if($("html").css('overflow-y') == 'scroll') {
  				$("html").css('overflow-y','hidden');
  			}
  		}
  		setTimeout(function() {
  			isAnimating = false;
  		}, 1500);
  	});
  
  
  	// Control the Sticky SubNav Open or Close
  	// We are looking on click of both the topics arrow and the normal mobile arrow
  
  	$('.header--sub--mobile--arrow, .header--sub--topics-menu--subnav').click(function() {
  		if($(".header--media").hasClass('header--search--active')) {
  			searchClose();
  		}
  
  		if (!$('.header--sub').hasClass("slideActive")){
  
  			$('.header--sub').addClass("slideActive").focus();
  			$('.header--media').css('z-index','90');
  			$('.header--sub-overlay').fadeIn();
  
  			if ($('.header--sub').hasClass("header--sub--topics")){
  				$('.header--sub--topics-menu--subnav .fa-caret-down').fadeOut(500, function () {
  					$('.header--sub--topics-menu--subnav .fa-caret-up').fadeIn();
  				});
  			}	
  
  			$('.header--sub--mobile--arrow .fa-caret-down').fadeOut(500, function () {
  				$('.header--sub--mobile--arrow .fa-caret-up').fadeIn();
  			});	
  				
  
  			if ($(window).width() > 1024) {
  				if ($('.header--sub').hasClass("header--sub--topics")){
  					$('.header--sub .header--sub--mobile').css('max-height',$('.header--sub--mobile').find('.header--sub--mobile--container').outerHeight(true)+'px');
  				}
  			} else {
  				$('.header--sub .header--sub--mobile').css('max-height',$('.header--sub--mobile').find('.header--sub--mobile--container').outerHeight(true)+'px');
  			}	
  		}
  
  		else {
  			subMenuClose();
  		}  	
  	});
  
  	headerMenuHeight();
  
  	// Sticky Subnave Distance from Top
  	if ($( ".header--sub").length ) {
  		var stickyTop = $('.header--sub').offset().top;
  		$(window).on('scroll', function(){
  			if ($(window).scrollTop() >= stickyTop) {
  				$('.header--sub').css({"position":"fixed", "top":"0px"});
  				$('.header--sub h5').css({"left":"0"});
  				$('.header--sub h5 img').css({"opacity":"1"});
  				$('.header--sub h5 img').css({"filter":"alpha['opacity=1']"});
  			} else {
  				$('.header--sub').css({"position":"absolute", "top": ""});
  				$('.header--sub h5').css({"left":""});
  				$('.header--sub h5 img').css({"opacity":""});
  				$('.header--sub h5 img').css({"filter":""});
  			}
  		});    
  	}
  
  	$(window).resize(function() {
  		if ($(window).width() > 1024) {
  			if ($('.header--sub').hasClass('header--sub--topics') || 
  				$('.header--sub-overlay').hasClass('header--sub-overlay--catagories')) {
  				// don't close
  			} else {
  				subMenuClose();
  			}					
  		}
  	});
  
  	$('.top--header--search').click(function() {
  		searchOpen();
  		if ($('.header--sub').hasClass("slideActive")){
  			subMenuClose();
  		}
  	});
  
  	$('footer.footer--media li.hs-item-has-children').on('click',function() {
  		console.log('clickyy')
  		if ($(window).width() < 1024) {
  			if($(this).find('.hs-menu-children-wrapper').hasClass('toggling')){return;}
  			$(this).find('.hs-menu-children-wrapper').addClass('toggling')
  			$(this).find('.hs-menu-children-wrapper').stop().slideToggle(300,function(){
  				$(this).removeClass('toggling');
  			});
  			$(this).find('i').stop().toggleClass('fa-plus fa-minus');
  		}	
  	});	
  	footerMobileDropdown();
  });	
  
  $(window).resize(function () {
  	footerMobileDropdown();
  });
</script>


