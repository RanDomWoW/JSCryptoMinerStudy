      function scrollItemHP() {
          $('.scroll--wrapper--hp .scroll--item--hp').each(function () {
              $(this).css('position','absolute');
              if ($(window).scrollTop() > $(this).closest('.scroll--wrapper--hp').offset().top - 80) {
                  if ($(window).scrollTop() - $(this).closest('.scroll--wrapper--hp').offset().top + $(this).height() >= $(this).closest('.scroll--wrapper--hp').height()) {
                      // If bottom
                      $(this).css('margin-top',$(this).closest('.scroll--wrapper--hp').height()-$(this).height()+80+'px');
                  }
                  else {
                      // If not at at top or bottom
                      $(this).css('margin-top',$(window).scrollTop() - $(this).closest('.scroll--wrapper--hp').offset().top + 80 +'px');
                  }
              }
              else {
                  // If top
                  $(this).css('margin-top','');
              }               
          });
      }
      
      function fixedmenu() {
      	if (!$('.categories--submenu').hasClass("slideActive")){
      	    $('.categories--submenu--anchor').css({
      	        'display':'block',
      	        'height':($('.categories--submenu').height())+'px'
      	    });
      	}    
          $('.categories--submenu').css({
              'position':'absolute'
          });
          pageScrolled = $(window).scrollTop();
          if (pageScrolled >= $('.categories--submenu--anchor').offset().top) {
          	$('.categories--submenu').addClass('fixed--submenu');
          }
          else {
              $('.categories--submenu').removeClass('fixed--submenu');
          }   
      }    
      
      // ---- Custom Function for the Homepage Dropdown at Midpage -----
      function menuHeight() {
      	$(window).resize(function () {
      		if ($('.categories--submenu').hasClass("slideActive")){
      			if ($(window).width() > 1024) {
      				$('.categories--submenu .header--sub--topic-list').css('max-height',$('.categories--submenu').find('.header--sub--categories').outerHeight(true)+'px');
      			} else {
      				$('.categories--submenu .header--sub--topic-list').css('max-height',$('.categories--submenu').find('.header--sub--menu--mobile').outerHeight(true)+'px');
      			}	
      		}	
      	});
      }
      
      function cataMenuClose() {
      	$('.header--media').css('z-index','');
      	$('.header--sub--topics-menu .fa-caret-up').fadeOut(500, function () {
      		$('.header--sub--topics-menu .fa-caret-down').fadeIn();
      	});
      	$('.header--sub--mobile--arrow--cata .fa-caret-up').fadeOut(500, function () {
      		$('.header--sub--mobile--arrow--cata .fa-caret-down').fadeIn();
      	});		
      	$('.header--sub--topic-list').css('max-height','');
      	$('.categories--submenu').removeClass("slideActive");
      	$('.header--sub-overlay').fadeOut();
      }	
      
      function mobileSlide(triggerType){
      	if (triggerType == "close") {
      		$('.post--single--bottom-bar--lead').addClass('bottom-bar--hide');
      		$.cookie('slideStopping', '1', {expires: 1});
      	} else {
      		if($(".post--single--bottom-bar--lead").hasClass('bottom-bar--hide')) {
      			setTimeout(function(){ 
      				$('.post--single--bottom-bar--lead').removeClass('bottom-bar--hide');
      			}, 2000);
      		}
      	}	
      } 
      
      $(document).ready(function () {  
      	$(".bounce").click(function() {
      	    $('html, body').animate({
      	        scrollTop: $(".categories--submenu").offset().top 
      	    }, 1500);
      	});
      
      	// Bottom Bar Function
      
      	if (!$.cookie('slideStopping')) {
      		if (viewport('portable')){
      			mobileSlide();	
      		}
      	}	
      
      	$(window).resize(function () {
      		if (!$.cookie('slideStopping')) {
      			if (viewport('portable')){
      				mobileSlide();	
      			}
      		}	
      	});
      
      	$('.post--single--bottom-bar--lead .close-bar').click(function() {
      		mobileSlide("close");  
      	});
      
      
      	if (($('body').find('.categories--submenu').length > 0)&&($('body').find('.categories--submenu--anchor').length > 0)) {
      		$('#fixedmenu').css({
      			'position':'absolute'
      		});
      		fixedmenu();
      		$(document).scroll(function () {
      			fixedmenu();
      		});
      		$(window).resize(function () {
      			fixedmenu();
      		});
      	}
      
      	$('.header--sub--topics-menu, .header--sub--mobile--arrow--cata').click(function() {
      		if($(".header--media").hasClass('header--search--active')) {
      			searchClose();
      		}
      		if (!$('.categories--submenu').hasClass("slideActive")){
      			$('.categories--submenu').addClass("slideActive");
      			$('.header--media').css('z-index','90');
      			$('.header--sub-overlay').fadeIn();
      			$('.header--sub--topics-menu .fa-caret-down').fadeOut(500, function () {
      				$('.header--sub--topics-menu .fa-caret-up').fadeIn();
      			});
      			$('.header--sub--mobile--arrow--cata .fa-caret-down').fadeOut(500, function () {
      				$('.header--sub--mobile--arrow--cata .fa-caret-up').fadeIn();
      			});	
      			if (viewport("lap-and-up")){
      				$('.categories--submenu .header--sub--topic-list').css('max-height',$('.categories--submenu').find('.header--sub--categories').outerHeight(true)+'px');
      				
      			} else {
      				$('.categories--submenu .header--sub--topic-list').css('max-height',$('.categories--submenu').find('.header--sub--menu--mobile').outerHeight(true)+'px');
      			}	
      		}
      		else {
      			cataMenuClose();
      		}  	
      	});
      
      	$('.categories--submenu').on('focusout', function () {
      		cataMenuClose();
      	});
      
      	// To compensate for resizing
      	menuHeight();
      
      
          if ($('.scroll--wrapper--hp .scroll--item--hp').length>0) {
              scrollItemHP();
              $('.scroll--wrapper--hp .scroll--item--hp').each(function () {
                  // Set height if needed
                  if ($(this).height() > $(this).closest('.scroll--wrapper--hp').height()) {
                      $(this).closest('.scroll--wrapper--hp').css('min-height',$(this).height()+'px');
                  }
              });
              $('.scroll--wrapper--hp').ctaready(function () {
                  $('.scroll--wrapper--hp .scroll--item--hp').each(function () {
                      // Set height if needed
                      if ($(this).height() > $(this).closest('.scroll--wrapper--hp').height()) {
                          $(this).closest('.scroll--wrapper--hp').css('min-height',$(this).height()+'px');
                      }
                  });
              });
              $(window).scroll(function () {
                  scrollItemHP();
              });
              $(window).resize(function () {
                  scrollItemHP();
                  $('.scroll--wrapper--hp .scroll--item--hp').each(function () {
                      // Set height if needed
                      if ($(this).height() > $(this).closest('.scroll--wrapper--hp').height()) {
                          $(this).closest('.scroll--wrapper--hp').css('min-height',$(this).height()+'px');
                      }
                  });
              });
          }  
      
      });
      /* end doc ready */
      
      
      $.ajax({
      	url: "https://www.impactbnd.com/blog/page/1",
      	type:'GET',
      	success: function(data){
      		$(data).find(".blog--listing--cards--post-item").each(function () {
      			var postImagePosition = $(this).find('.post-item--featured-image').css("background-position");
      			var postIndex = $(this).attr('rel');
      			$('body').find('.blog--listing--cards--post-item:eq('+postIndex+') .post--single-featured-image').css('background-position', postImagePosition);
      		});
      	}
      })
      
      /* Infinite Scroll */        
      var ias = $.ias({
      	container:  '.blog--listing--cards--listing',
      	item:       '.blog--listing--cards--post-item',
      	pagination: '.blog--listing--cards--pagination',
      	next:       '.next-posts-link'
      });
      
      ias.extension(new IASSpinnerExtension());
      
      ias.extension(new IASTriggerExtension({
      	text: '<div class="text--center"><div class="btn btn--blue-to-transparent btn--pill btn--scroll--trigger palm-margin--short--top">Load More Posts</div></div>',
      	offset: 0
      }));
      
      ias.on('render', function(items) {
      	$(items).each(function(index) { 
      		var postLink = $(this).find('.btn--readmore').attr('href');
      
      		$(this).addClass('grid__item one-whole hard--top hard--sides margin--tall--bottom short padding--short background--white border--lightest-grey border--1');
      		$(this).find('.post-item--author br').remove();
      		$(this).find('.post-item--body').wrapAll("<div class='grid__item one-whole portable-one-whole hard' />");
      		$(this).find('.post-item--body').parent('.one-whole').wrapAll("<div class='post-item--body--container post-item--loaded' />");
      
      		$(this).find('a.post-item--featured-image').prependTo($(this).find('.post-item--body--container'));
      		$(this).find('a.post-item--featured-image').wrapAll('<div class="grid__item one-whole hard" />');
      
      		$(this).find('.post-item--body--container').after('<a href='+postLink+' class="no--underline post-single--reading-button"><p class="hard--bottom font--heading color--blue bold font--15">Continue Reading<img src="https://cdn2.hubspot.net/hubfs/145335/IMPACT_Framework_3/Homepage/Assets/blog--arrow.svg", onerror="this.src=\'https://cdn2.hubspot.net/hubfs/145335/IMPACT_Framework_3/Homepage/Assets/impact-homepage-arrow-01.png\'", alt="IMPACT Blog Continue Reading", width="17", height="10" /></p></a>');
      
      	});
      });
      
      ias.extension(new IASNoneLeftExtension({html: "<div class='end-text'><p class='hard text--center'><em>You have reached the end of our posts, Please consider subscribing if you haven't already!</em></p></div>"}));
      
      $(window).resize(function () {
      	if (viewport('portable')){
      		$('.post-item--loaded').each(function(index) {
      			$(this).find('.five-twelfths').prependTo($(this));
      		});
      	}
      });
    </script>
  

