        };

        if (typeof divclassmin !== 'undefined' || typeof divclassmax !== 'undefined') {
            if (window.matchMedia("(min-width: " + divclassmin[1] + "px)").matches) {
                ajax_load(divclassmin[0]);
            } else if (window.matchMedia("(max-width: " + divclassmax[1] + "px)").matches) {
                ajax_load(divclassmax[0]);
            }
        }

