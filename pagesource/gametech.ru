        var ajax_load = function (class_name) {
            $.each($("." + class_name), function(){
                var that = $(this);
                $.ajax({
                    url: "//www.gametech.ru/banners/banqs.cgi?place_id=" + that.html(),
                    success: function(data) { that.html(data); }
