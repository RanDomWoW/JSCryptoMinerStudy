        var blocks = $('.home_categories > a');
        blocks.each(function(index, block){
            if(index > (showFirst - 1))
                $(this).hide();
        });
        $('.allcat > h3').text(text[0]);
        $('.allcat').click(function(e){
            e.preventDefault();
            blocks.each(function(index, block){
                if(index > (showFirst - 1))
                    $(this).slideToggle( "fast" );
            });
            switch($('.allcat').text()){
                case text[0]:
                    $('.allcat').text(text[1]);
                    break;
                case text[1]:
                    $('.allcat').text(text[0]);
                    break;
                default:
                    $('.allcat').text(text[0]);
                    break;
            }
        });
        
        $('div.officescount').css('cursor', 'pointer');
        $('div.officescount').click(function(e){
            $(this).children('a')[0].click();
        });
        
    });
