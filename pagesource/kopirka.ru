        // functions

        function open() {
            map.animate({
                width: '320px'
            }, 800);
            scheme.animate({
                width: '920px'
            }, 800);
            scheme.attr('class', 'opened');
        }

        function close() {
            scheme.animate({
                width: '320px'
            }, 800, false, function () {
                textContent.html('<img src="/wa-apps/shop/img/metro/symbolMsk.png"><br/>показать <br/> на схеме метро');

                $('.scheme-map').empty();
                scheme.attr('class', 'closed');
                $('#overlay-map').fadeOut(800);
            });
            map.animate({
                width: '920px'
            }, 800);

            $('#overlay').fadeIn(800);
            schemeMapWrapper.hide();
            schemeMapWrapper.css('z-index', '-1');


        }


        var onLimit = true; // включить лимит краев схемы, по умолчанию: false
