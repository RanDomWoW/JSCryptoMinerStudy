        var limitLeft = 400; // лимит левого края
        var limitRight = -200; // лимит правого края

        // init draggable
        $('.scheme-map').draggable({
            stop: function (event, ul) {
                if (onLimit === true) {

                    var top = $(this).css('top'),
                            left = $(this).css('left');

                    top = Number(top.substr(0, top.length - 2));
                    left = Number(left.substr(0, left.length - 2));

                    if (top < limitBottom)
                        $(this).css('top', limitBottom + 'px');
                    else if (top > limitTop)
                        $(this).css('top', limitTop + 'px');
                    if (left > limitLeft)
                        $(this).css('left', limitLeft + 'px');
                    else if (left < limitRight)
                        $(this).css('left', limitRight + 'px');
                }
            }
        });

    });
</script>


