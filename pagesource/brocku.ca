	<script>
		(function($) {
			
            var owl = '';
            var owl_cfg = {
                singleItem: true,
                loop : true,
                autoHeight: true,
                lazyLoad : false,
                lazyFollow : false,
                lazyEffect : false, // prevent flash on load. Requires lazyload false as trade-off.

                //Basic Speeds
                slideSpeed : 400,
                paginationSpeed : 800,
                rewindSpeed : 1000,
                responsiveRefreshRate : 50,

                //Autoplay
                //autoPlay : 4000,
                autoPlay : false,
                stopOnHover : true, // better XP

                // Navigation
                navigation : false,
                navigationText : ["<span class=\"fa fa-angle-left\"></span>","<span class=\"fa fa-angle-right\"></span>"],
                rewindNav: true,
                
                //Pagination
                pagination : true,
                paginationNumbers: false,

                // we can access the active one jQuery('.owl-wrapper .owl-item.active');
                addClassActive : true,

                //Theme
                theme : 'brocku-theme',

                // Here we read a data attribute we've inserted during the markup rendering so we
                // can dynamically update the custom readme label so it's readable by readers.
                afterAction : function (e) {
                    var button_title = jQuery('.owl-wrapper .owl-item.active img.lazyOwl').data('custom_sub_title');
                    $('.custom_readmore_button_label').html(button_title);
                },

                /**
                 * After the slide is loaded this changes the tab index of the current slide's read more link.
                 * @param {type} e ?
                 * @returns void
                 */
                INACTIVE_afterAction : function (e) {
                    var parent = jQuery('.owl-wrapper .owl-item.active');

                    $(".owl-prev,.owl-next").prop("tabindex", 0);
                    $(".readmore").prop("tabindex", 0);

                    $(".owl-prev").prop("tabindex", 1);
                    $(".owl-next").prop("tabindex", 2);
                    $(".readmore", parent).prop("tabindex", 3);
                }
            };

            var prev_focused = false;
            var next_focused = false;

            $(document).keydown(function (e) {
                // if prev/next are not seleced get out pf here
                if ( !$('.owl-prev,.custom_prev_button').is( ":focus" ) && !$('.owl-next,.custom_next_button').is( ":focus" ) ) {
                    if (next_focused) {
                        $('#owl-slide-'+ owl.currentItem + ' .readmore').focus();
                    }

                    return ;
                }

                prev_focused = $('.owl-prev').is( ":focus" );
                next_focused = $('.owl-next').is( ":focus" );

                switch(e.which) {
                    case 37: // left
                        owl.trigger('owl.prev'); // http://owlgraphic.com/owlcarousel/demos/custom.html
                    break;

                    // Enter and spacebar trigger an even based on which button: prev/next is selected
                    // http://mikemurko.com/general/jquery-keycode-cheatsheet/
                    case 13: // enter
                    case 32: // space
                        if ( $('.owl-prev, .custom_prev_button').is( ":focus" ) ) {
                            owl.trigger('owl.prev'); // http://owlgraphic.com/owlcarousel/demos/custom.html
                        } else {
                            owl.trigger('owl.next'); // http://owlgraphic.com/owlcarousel/demos/custom.html
                        }
                    break;

                    case 39: // right
                        owl.trigger('owl.next');
                    break;

                    //case 38: // up
                    break;

                    //case 40: // down
                    break;

                    default:
                        return; // exit this handler for other keys
                }

                e.preventDefault(); // prevent the default action (scroll / move caret)
            }); // keydown
           
			$(window).load(function() {
								
				// Fade in for general fancyness
				var featureContainer = $('#carousel');
				featureContainer.hide().filter('#carousel').fadeIn('fast');
				var slideOverlay = $('.slide-overlay');
				slideOverlay.hide().filter('.slide-overlay').fadeIn('slow');
				
				owl = $('#carousel');

				/** Owl Carousel 1.3.2 */
				owl.owlCarousel(owl_cfg);
				
                // when the main nav is hidden we can use custom buttons
                $(".custom_prev_button").on('click', function() {
                    owl.trigger('owl.prev');
                    return false;
                });

                // Custom Navigation Events
                $(".custom_next_button").on('click', function(e) {
                    owl.trigger('owl.next');
                    return false;
                });

                $(".custom_readmore_button").on('click keypress', function(e) {
                    var current_slide = jQuery('.owl-wrapper .owl-item.active');

                    // When Click, SPACEBAR or ENTER keys are pressed then readmore link is focused
                    // we'll send the user to the desination URL.
					if ( e.type == 'click' || e.which == 13 || e.which == 32 ) { // 13 is enter, 32 is space
						window.location.href = $('.readmore', current_slide).prop('href');
                        return false;
					}

                    return true;
                });

                // Alternates the play/pause button
                $('.play_pause_button').on('click', function(e) {
                    var text = '';
                    var state = '';

                    // trigger play and invert the label
                    if ($(this).data('state') == 'play') {
                        text = 'Pause';
                        state = 'pause';
                        owl.trigger('owl.play', [ 10000 ] ); // resume play at the same speed as in cfg
                    } else {
                        text = 'Start';
                        state = 'play';

                        owl.trigger('owl.stop');
                    }

                    $(this).data('state', state);
                    $(this).prop('title', text);

                    $('.play_pause_icon').removeClass('play pause fa-play fa-pause').addClass('fa-' + state);
                    $('.play_pause_button_label').text(text);
                    $('.play_pause_button_cta').text(text + ' the rotation');

                    //e.preventDefault(); // prevent the default action (scroll / move caret)
                    return false;
                });

                // When SPACEBAR or ENTER keys are pressed when readmore link is focuses
                // we'll send the user to the desination URL.
				$(".INACTIVE_readmore").keypress(function(e) {
					if ( e.which == 13 || e.which == 32 ) { // 13 is enter, 32 is space
						window.location.href = $(this).prop('href');
						//ev.preventDefault();
                        return false;
					}

                    return true;
				});

                // in order to allow tab and other actions we'll handle
                // only the events we need.
                $('.play_pause_button').on('keypress', function(e) {
                    if ( e.which == 13 || e.which == 32 ) { // 13 is enter, 32 is space
                        $('.play_pause_button').trigger('click');
                        return false;
					}

                    return true;
                });

                // Let's have the carousel autoplay by default.
                $('.play_pause_button').trigger('click');
                
                
			});
			
			$(document).ready(function () {
				
				// Isolate the last line of each slide's title.
				/* disabled as branding no longer requires an underline.
				$( "#carousel div.slide.subtitle" ).each(function( i ) {
	                
	                var selector = $(this).find( '.slide-overlay h2' );
	                var parText = selector.html(); // Grab the text
					var parSplit = parText.split( '<br>' ); // Separate each line of text
					var lastLine = parSplit[ parSplit.length-1 ]; // Isolate the last line
					
					// Remove the last line from the original text
					var trimmedText = parText.replace( lastLine , '' );
					selector.html( trimmedText );
					
					// Append the last line but wrapped in a <span> tag so we can style it
					selector.append( '<span class="lastline">'+lastLine+'</span>' );
					
					//console.log(parSplit);
					//console.log(lastLine);
					//console.log(parText);
					
				}); */
				
			});
		
		})(jQuery);
	</script>

    				
		
