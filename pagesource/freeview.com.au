                                <img err-src="{{serie.logo}}">
                                <div class="channel-logo"><img ng-src="{{serie.logo}}" err-src="{{ channelName(serie) }}" data-logo="true"></div>
                            </div>
                        </a>
                        <div class="text-box">
                            <p class="innertext"><span>{{serie.scheduleInfo[0].title}}</span> <br> {{serie.scheduleInfo[0].startTimeShow12}} {{serie.scheduleInfo[0].day}}</p>
                        </div>
                    </div>
                </div>
            </div>
          </div>
          <!-- End COMING SOON -->
	</div>
</section>
