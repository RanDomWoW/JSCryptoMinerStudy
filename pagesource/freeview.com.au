<section class="my-tab home-tab hidden-lg">
	<ul class="nav nav-tabs" switch-tab="">
		<li class="active"><a data-toggle="tab" href="#menu0">DONT MISS</a></li>
		<li><a data-toggle="tab" href="#menu1">CATCH UP</a></li>
		<!--<li><a data-toggle="tab" href="#menu2">COMING SOON</a></li>-->
	</ul>

	<div class="tab-content">
		<!-- Start DONT MISS -->
		<div id="menu0" class="tab-pane in active clearfix">
            <div class="my-thumbnail" ng-repeat="program in channelsGroupbyNetworks track by $index | limitTo: 5">
                <div class="carousel bxslider" data-value="{{$index}}">
                    <div class="carousel-cell" ng-repeat="prog in filterProgramsDontMiss(program.programmesDoNotMiss) | orderBy:'startTimeShow'  | limitTo: 3">
                        <div class="single-show" ng-click="showPopup(prog, $event )">
                            <div class="image-content" style="background-image:url(http://freeviewblob.blob.core.windows.net/gracenote/{{prog.popupInfo.media}})"></div>
                            <div class="channel-logo {{channelName(prog)}}"></div>
                        </div>
                        <div class="text-box">
                            <p class="innertext"><span>{{prog.title}}</span> {{prog.startTimeShow12}} {{prog.popupInfo.weekDay}}</p>
                        </div>
        		    </div>
                </div>
            </div>
            <div class="preloader-tile"><img src="http://freeviewcdn.azureedge.net/images/assets/loading.gif" width="80"></div>
         </div>
        <!-- End DONT MISS -->

		<!-- Start CATCH UP -->
		 <div id="menu1" class="tab-pane clearfix">
            <div class="my-thumbnail" ng-repeat="program in channelsGroupbyNetworks | limitTo: 5">
               <div class="carousel bxslider" data-value="{{$index}}">
                   <div class="carousel-cell" ng-repeat="prog in filterProgramsCatchUp(program.programmesCatchup) | orderBy: 'startTimeShow' : true | unique: 'programid' | limitTo: 3">
                        <div class="single-show" ng-click="showPopup(prog, $event)">
                            <div class="image-content" style="background-image:url(http://freeviewblob.blob.core.windows.net/gracenote/{{prog.popupInfo.media}})"></div>
                            <div class="channel-logo {{channelName(prog)}}"></div>
                        </div>
                        <div class="text-box">
                            <p class="innertext"><span>{{prog.title}}</span> {{prog.startTimeShow12}} {{prog.popupInfo.weekDay}}</p>
                        </div>
                  </div>
               </div>
            </div>
            <div class="preloader-tile"><img src="http://freeviewcdn.azureedge.net/images/assets/loading.gif" width="80"></div>
         </div>
        <!-- End CATCH UP -->

		<!-- Start COMING SOON 
        <div id="menu2" class="tab-pane clearfix">
           <div class="my-thumbnail" ng-repeat="program in comingUp track by $index | limitTo: 5" on-finish-render="{{program.network_name}}">
               <div class="carousel bxslider" data-value="{{$index}}">
                    <div class="carousel-cell" ng-repeat="serie in program.serviceArr">
                        <div ng-if="!Img404" class="single-show" ng-click="showPopup(serie, $event)" ng-init="{Img404:false}">
                            <img ng-src="http://freeviewblob.blob.core.windows.net/gracenote/{{serie.schedule[0].programme.media.filename}}" err-src="{{serie.logo}}">
                            <div class="channel-logo"><img ng-src="{{serie.logo}}" err-src="{{ channelName(serie) }}"></div>
                        </div>
                        <a ng-if="Img404" href="/tv-guide/">
