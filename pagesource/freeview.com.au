<section class="home-popup" ng-show="popUpObj.popupInfo.show" ng-cloak>
    <div class="popup-map" ng-click="closePopup($event)">
        <div id="popup" ng-model="popUpObj" ng-show="popUpObj.popupInfo.show">

            <img id="close-popup" class="close-popup" ng-click="closePopup($event)" src="/images/assets/cross_bold.png">
            <div class="row">
                <div class="col-sm-4">
                    <img src="http://freeviewblob.blob.core.windows.net/gracenote/{{popUpObj.popupInfo.media}}">
                    <p></p>
                    <p class="hidden-xs service-time-data">
                        <strong>{{popUpObj.popupInfo.serviceName}}</strong> {{popUpObj.popupInfo.genre}}
                        <br/><span>{{popUpObj.startTimeUTC}} - {{popUpObj.endTimeShow}}</span> ({{popUpObj.popupInfo.durationInMins}} mins)
                    </p>
                    <span></span>
                </div>
                <div class="col-sm-8">

                    <h3>{{popUpObj.title}} {{popUpObj.popupInfo.classification}}</h3>
                    <h4><span>{{popUpObj.popupInfo.episode_title}}</span> <span ng-style="spanCopyRight" ng-if="CopyrightYear">{{CopyrightYear}}</span> <span ng-style="spanSeries" ng-if="popUpObj.popupInfo.isSeries">S{{popUpObj.popupInfo.series_season}} EP{{popUpObj.popupInfo.series_ep}}</span></h4>
                    <p ng-if="popUpObj.popupInfo.cast"><i>({{popUpObj.popupInfo.cast}})</i></p>
                    <p>{{popUpObj.popupInfo.synopsis}}</p>
                    <div class="visible-xs hidden-sm hiddem-md hidden-lg">
                        <hr />
                        <h4>
                            <span>{{popUpObj.popupInfo.serviceName}}</b> <span class="mins">{{popUpObj.popupInfo.genre}}</span></span>
                            <span>{{popUpObj.startTimeShow | cmdate: 'hh:mmA'}} - {{popUpObj.endTimeShow}} <span class="mins">({{popUpObj.popupInfo.durationInMins}} mins)</span></span>
                        </h4>
                    </div>
                    <div class="btn" ng-click="topAddToFavHome(popUpObj)">
                        <i class="glyphicon glyphicon-star"></i>
                        <span>
                            <span ng-if="popUpObj.popupInfo.inMyGuide">REMOVE FROM MYGUIDE</span>
                            <span ng-if="!popUpObj.popupInfo.inMyGuide">ADD TO MYGUIDE</span>
                        </span>
                    </div>

                </div>
            </div>
        </div>
    </div>
</section>


