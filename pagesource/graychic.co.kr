$(".gc_rightsl_act .gc_rightsl_btn").click(function(){
    if(vi == "off"){
        $(".gc_rightsl_act").animate({"right":"-162px"});
        $(".gc_rightsl_btn").html("<img src='/web/upload/zgcn/img/icon_aopen.png'>");

       vi = "on";
        
    }else{
       $(".gc_rightsl_act").animate({"right":"0px"});
       $(".gc_rightsl_btn").html("<img src='/web/upload/zgcn/img/icon_aclose.png'>");
       vi = "off";
       

    }
 });
 
 
function setcookie(aa,bb) {
     document.cookie = aa+"="+bb+"; path=/";
     return false;
 }
 
  
function getcookie(Key) { 
    var cookies = document.cookie.split("; ");
    for (var i = 0; i < cookies.length; i++) {
      var coValues = cookies[i].split("=");
      if (coValues[0] == Key) {
        return unescape(coValues[1]);
      }
    }
    return false;
  }
 
});
