<script>$(document).ready(function(){
	$.fn.followTo = function (pos) {
		var $this = this,
			$window = $(window);

		$window.scroll(function (e) {
			if ($window.scrollTop() > pos) {
				$("body.front .headernew div.headernew-middle").css({
					display: 'none',					
					height: '0px'
				});
				$(".headernew").css({
					position:'fixed'					
				});
				$("body.front .headernew .hntlogo").css({
					display:'block',
					padding: '.5% 1% !important'				
				});
				
			} else {
				
				$("body.front .headernew div.headernew-middle").css({
					display: 'block ',
					height: '90px'					
				});
				$(".headernew").css({
					position:'initial'					
				});
				$("body.front .headernew .hntlogo").css({
					display:'none',
					padding:'0 !important'
				});
			}
		});
	};

	$('.headernew').followTo(10);
  });</script>
