<script>
window.fbAsyncInit = function() {
  FB.init({
    appId      : '287984944588378',
    status     : false, // check login status
    cookie     : true, // enable cookies to allow the server to access the session
    xfbml      : true  // parse XFBML
  });
  FB.Event.subscribe('auth.authResponseChange', function(response) {
    if (response.status === 'connected') {
        if (!logged_user) {
           $.post('/ajax/login.php', 
            { action: 'facebookLogin' }, 
            function(result) {
                if (result.status != 'error') {
                    $('.boxlogin .warning-message').hide();
                    logged_user = result.user;
                    $('body').trigger($.Event( "login"));
                    $('body').trigger($.Event( "facebook_connect"));
                    close_login();
                } else {
                    console.log(result.profile);
                    if (result.message == 'email') $('.boxlogin .warning-fb-email').show();
                    if (result.message == 'unregistered') $('.boxlogin .warning-fb-unreg').show();
                    if (result.message == 'nouser') $('.boxlogin .error-message').html('Utente non trovato');
                }
            },'json');
        } else    
        if (logged_user.facebook != response.authResponse.userID) {
            $.post('/ajax/user_profile.php', { action: 'associaFacebook', uid: response.authResponse.userID }, function(result) {
                if (result.status == 'ok') {
                    logged_user.facebook = result.facebook;
                    $('body').trigger($.Event( "facebook_connect"));
                }
            },'json');
        }
    } else if (response.status === 'not_authorized') {
        $.post('/ajax/user_profile.php', { action: 'dissociaFacebook' }, function(result) {
            if (result.status == 'ok') {
                $('body').trigger($.Event( "facebook_unconnect"));
                logged_user.facebook = 0;
            }
        },'json');
      //FB.login();
    } else {
        $('body').trigger($.Event( "facebook_logout"));
    }
  });
};
