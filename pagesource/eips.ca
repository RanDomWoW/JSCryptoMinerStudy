    		ontime = 0, delayed = 0, cancelled = 0, all = 0;
    		$.each(all_buses, function(index, element) {
    			if(data[element.id]) {
    				element.status_name = data[element.id].status_name;
    			}
				all++;
    			if(element.status_name == "On Time"){
					ontime++;
				}else if(element.status_name == "Delayed"){
					delayed ++;
				}else if(element.status_name == "Cancelled"){
					cancelled++;
				}	
				//remove any ones that were just made inactive from the totals
				if(data[element.id] && data[element.id].active == '0') {
					all--;
	    			if(element.status_name == "On Time"){
						ontime--;
					}else if(element.status_name == "Delayed"){
						delayed --;
					}else if(element.status_name == "Cancelled"){
						cancelled--;
					}	
				}
    		});
    		if(!all_buses || all_buses.length < 1) {
    			$.each(data, function(index, element) {
					all++;
	    			if(element.status_name == "On Time"){
						ontime++;
					}else if(element.status_name == "Delayed"){
						delayed ++;
					}else if(element.status_name == "Cancelled"){
						cancelled++;
					}	
					//remove any ones that were just made inactive from the totals
					if(element.active == '0') {
						all--;
		    			if(element.status_name == "On Time"){
							ontime--;
						}else if(element.status_name == "Delayed"){
							delayed --;
						}else if(element.status_name == "Cancelled"){
							cancelled--;
						}	
					}
	    		});
    		}
    		var status_content = "", status_class, num_buses = '';

    		if(all != 0 && (ontime == all || delayed == all || cancelled == all)) num_buses = 'All';

    		$('.bus_status_container').removeClass('cancelled').removeClass('delayed').removeClass('ontime').removeClass('inactive');
    		$('.bus_status').removeClass('single').removeClass('double');
			
			if(all == 0 || (ontime == 0 && delayed == 0 && cancelled == 0)) {
				status_content = "<span>No buses running.</span>";
				$('.bus_status_container').addClass('inactive');
			} else {
				if(delayed == 0 && cancelled == 0){
					$('.bus_status_container').addClass('ontime');
					status_class = 'on time';
					num_buses = !num_buses ? ontime : num_buses;
				} else {
					if(delayed > 0) {
						$('.bus_status_container').addClass('delayed');
						status_class = 'delayed';
						num_buses = !num_buses ? delayed : num_buses;
					}
					if(cancelled > 0){
						$('.bus_status_container').addClass('cancelled');
						status_class = 'cancelled';
						num_buses = !num_buses ? cancelled : num_buses;
					}
					if(delayed > 0 && cancelled > 0) {
						$('.bus_status').addClass('double');
					} else {
						$('.bus_status').addClass('single');
					}
					
				}
				if(cancelled >= 1) {
					status_content += '<span class="cancelled-buses">' + num_buses + ' bus' + (cancelled > 1 || all == cancelled ? 'es ' : ' ') + 'cancelled</span>';
				}
				if(delayed >= 1) {
					status_content += '<span class="delayed-buses">' + num_buses + ' bus' + (delayed > 1 || all == delayed ? 'es ' : ' ') + 'delayed</span>';
				}
				if(delayed == 0 && cancelled == 0) {
					status_content += '<span class="ontime-buses">All buses on time</span>';
				}
			}

			$(".bus_status").html(status_content);
		});
    });
