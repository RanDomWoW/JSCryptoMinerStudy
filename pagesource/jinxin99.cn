    function timer(intDiff,id){
        var stop = 0;
        stop = window.setInterval(function(){
            var day=0,
                    hour=0,
                    minute=0,
                    second=0;//时间默认值
            if(intDiff >= 0){
                hour = Math.floor(intDiff / (60 * 60));
                minute = Math.floor(intDiff / 60) - (day * 24 * 60) - (hour * 60);
                second = Math.floor(intDiff) - (day * 24 * 60 * 60) - (hour * 60 * 60) - (minute * 60);
                if (minute <= 9) minute = '0' + minute;
                if (second <= 9) second = '0' + second;
                $('#hour_show_'+id).html(Math.floor(hour/10)+''+Math.floor(hour%10));
                $('#minute_show_'+id).html(Math.floor(minute/10)+''+Math.floor(minute%10));
                $('#second_show_'+id).html(Math.floor(second/10)+''+Math.floor(second%10));
            }else{
                $('#butn_'+id).html("立即投资");
                $('#seckill_'+id).hide();
                $('.schedule_'+id).show();
                window.clearInterval(stop);
            }
            intDiff--;
        }, 1000);
    }
