    infScrollApp.factory('littleMagic',
        function ($http) {
            var littleMagic = function () {
                this.items = [];
                this.busy = false;
                this.after = 2;
            };

            littleMagic.prototype.nextPage = function () {
                if (this.busy) return;
                this.busy = true;
                if (this.after === 2) {
                    var requestUrl = "/ajax/get/data/mainpage/all?pageIndex=" + this.after;
                    $http.get(requestUrl)
                        .success(function (data) {
                            for (var i = 0; i < data.length; i++) {
                                this.items.push(data[i]);
                            }
                            this.after = this.after + 1;
                            this.busy = false;
                        }.bind(this));
                }
            };

            littleMagic.prototype.LoadMore = function () {
                var requestUrl = "/ajax/get/data/mainpage/all?pageIndex=" + this.after;
                $http.get(requestUrl)
                    .success(function (data) {
                        for (var i = 0; i < data.length; i++) {
                            this.items.push(data[i]);
                        }
                        this.after = this.after + 1;
                        this.busy = false;
                    }.bind(this));
            }
            return littleMagic;
        });
