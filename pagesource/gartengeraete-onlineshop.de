AjaxPro.observe('onFailure', function(e) {
    var r = e.memo.response;
    function _redirect() {
        if (r.redirectUrl == window.location.href) {
            return;
        }
        if (r.redirectUrl) {
            window.location.href = r.redirectUrl;
            return;
        }
        window.location.reload();
    }

    if(r.messages) {

        if (r.messages.error) {

            var messageUpdate = new Array();
            for (var i = 0; i < r.messages.error.length; i++) {

                if (r.messages.error[i] != Translator.translate('Some of the products are currently out of stock')) {
                    messageUpdate[i] = r.messages.error[i];

                    if (r.messages.error[i] == Translator.translate('This product is currently out of stock.')) {
                        messageUpdate[i] = Translator.translate('This product is currently not in stock and unfortunately not available. Please select another product. We apologize for any inconvenience. Thank you!');
                    }
                }
            }

            r.messages.error = messageUpdate;
        }

        if ($('ajaxpro-notice-form')) {
            AjaxPro.effect.message
                .setElementId('ajaxpro-notice-form')
                .setMessages(r.messages)
                .show()
            ;
            $('ajaxpro-notice-form').select('.ajaxpro-button').each(function(element){
                element.observe('click', _redirect);
            });
            return;
        }
        var _messages = r.messages, message = '';
        for(var code in _messages) {
            var temp = _messages[code], length = temp.length;
            for(var i = length; i--;) {
                message += '\n' + code + ' : ' + temp[i];
            }
        }
        alert(message);
    }
    _redirect();
});
