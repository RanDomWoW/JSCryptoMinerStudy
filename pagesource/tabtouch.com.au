        <p data-bind="visible: editable">Select some formula options below, or continue to the next step without making a selection for a standard All Up bet</p>
        <ol class="link-list clearfix" data-bind="foreach: formulas">
            <li data-bind="allUpParlay: $data"></li>
        </ol>
    </div>
    <!-- /ko -->

    <!-- ko if: multiOn() && !isBetSlipEmpty() -->
    <h4 class="dark grey text-upper">Multi</h4>
    <div class="no-focus clearfix multi-investment" tabindex="-1">
        <label for="allup-investment">Bet Amount</label>
        <!-- ko if: editable -->
        <input id="allup-investment" type="number" min="0" max="99999.50" class="win" step="0.5" data-bind="multiInvestValue: multiInvestment">
        <a class="button" data-bind="click: toggleCalculator.bind($data, 'investment')"><span class="tticon tticon-pencil"><span class="accessible">Quick Amount</span></span></a>
        <!-- ko if: calcVisible -->
        <betslip-calculator params="disableApplyToAll: true"></betslip-calculator>
        <!-- /ko -->
        <!-- /ko -->
        <!-- ko ifnot: editable -->
        <span class="value text-highlight-04" data-bind="text: multiInvestment().toFixed(2)"></span>
        <!-- /ko -->
        <div class="cashbet-label" data-bind="visible: cashMode">
            <label>Name this bet</label>
            <input type="text" maxlength="50" class="basicInput required" data-bind="textInput: allupMultiNotes">
        </div>
        <div class="clearfix">
            <label class="big-12-10 text-highlight-04">Multi Return</label>
            <span id="bet-slip-multi-return" class="big-12-10 text-highlight-04" data-bind="text: '$' + multiReturn().toFixed(2)"></span>
        </div>
    </div>
    <!-- ko if: editable-->
    <div class="multi-formula">
        <h4 class="dark grey text-upper">
            Multi Formula
            <a id="toggle-multi-formula-section" href="#" class="btn-expandcollapse right r-5" data-bind="click: toggleMultiFormulaSection"><span class="tticon tticon-arrowdown" data-bind="css: { 'tticon-arrowdown': !multiFormulasExpanded(), 'tticon-arrowup': multiFormulasExpanded() }"><span class="accessible">Expand/Collapse</span></span></a>
        </h4>
        <div style="display:none" class="pb10" data-bind="expandSlideVisible: multiFormulasExpanded">
            <!-- ko with: multiFormulaViewModel -->
            <!-- ko if: isAvailable -->
            <div class="row clearfix t10 b5 l0 r0 big-12-10 text-highlight-04">
                <div class="col-xs-4 pr10 faux-strong"></div>
                <div class="col-xs-4 pl0 pr0 faux-strong">Bet Amount</div>
                <div class="col-xs-4 pl0 faux-strong">Cost</div>
            </div>
            <!-- ko foreach: formulas -->
            <div data-bind="visible: isVisible" class="row clearfix l0 r0 big-12-10 text-highlight-04">
                <div class="col-xs-4 pr10"><!--ko text: name--><!--/ko--> &#215; <!--ko text: optionNumberCalc--><!--/ko--></div>
                <div data-ui="formula-item" class="col-xs-4 pl0 pr0">
                    <input type="number" min="0" max="99999.99" value="0.00" data-bind="multiInvestValue: investment, attr: { 'step': name === 'Singles' ? 1 : 0.5, 'max': name === 'Singles' ? '99999.99' : '99999.50'}">
                    <a class="button btn-quickamount" data-bind="click: $parent.toggleCalculator.bind($data, name)"><span class="tticon tticon-pencil"><span class="accessible">Quick Amount</span></span></a>
                    <!-- ko if: $parent.calcVisible() && $parent.calcAction() === name -->
                    <betslip-calculator params="disableFiftyCents: (name === 'Singles'), disableApplyToAll: true"></betslip-calculator>
                    <!-- /ko -->
                </div>
                <div class="col-xs-4 pl0" data-bind="text: totalCostText"></div>
            </div>
            <!-- /ko -->
            <!-- /ko -->
            <!-- ko ifnot: isAvailable -->
            <div class="notice small warning">
                <span class="tticon tticon-exclaiminvert"></span>Multi Formula is only available for bets that contain between 2 and 6 legs.
            </div>
            <!-- /ko -->
            <!-- /ko -->
        </div>
    </div>
    <!-- /ko -->
    <!-- ko if: !editable() && multiFormulaViewModel.isAvailable() && multiFormulaViewModel.hasMultiFormulas() -->
    <div class="multi-formula">
        <h4 class="dark grey text-upper">Multi Formula</h4>
        <div class="pb10">
            <div class="row clearfix t10 b5 l0 r0 big-12-10 text-highlight-04">
                <div class="col-xs-4 pr10 faux-strong"></div>
                <div class="col-xs-4 pl0 pr0 faux-strong">Bet Amount</div>
                <div class="col-xs-4 pl0 faux-strong">Cost</div>
            </div>
            <!-- ko foreach: multiFormulaViewModel.formulasWithInvestment() -->
            <div data-bind="visible: isVisible" class="row clearfix l0 r0 big-12-10 text-highlight-04">
                <div class="col-xs-4 pr10"><!--ko text: name--><!--/ko--> &#215; <!--ko text: optionNumberCalc()--><!--/ko--></div>
                <div class="col-xs-4 pl0 pr0" data-bind="text: betAmountText"></div>
                <div class="col-xs-4 pl0" data-bind="text: totalCostText"></div>
            </div>
            <!-- /ko -->
        </div>
    </div>
    <!-- /ko -->
    <!-- /ko -->
</div>

<div data-bind="if: !isBetSlipEmpty()">
    <h3 class="dark">BET SUMMARY</h3>
    <ul id="bet-slip-summary" class="panel-list">
        <!-- ko if: allUpOn() || multiOn()-->
        <li>
            <span class="title">No. Legs</span>
            <span id="bet-slip-total-legs" class="data" data-bind="text: totalBetCount"></span>
        </li>
        <li>
            <span class="title">No. Combs.</span>
            <span id="bet-slip-combinations" class="data" data-bind="text: combinations"></span>
        </li>
        <!-- /ko -->
        <!-- ko if: multiOn -->
        <li>
            <span class="title">Projected Pay</span>
            <span id="bet-slip-multi-proj-pay" class="data">$<!-- ko text: multiProjPay().toFixed(2) --><!-- /ko --></span>
        </li>
        <!-- /ko -->
        <!-- ko if: mode() === 'Standard' -->
        <li>
            <span class="title">Total Number of Bets</span>
            <span id="bet-slip-total-bets" class="data" data-bind="text: totalBetCount"></span>
        </li>
        <!-- /ko -->
        <li>
            <span class="title">Total Cost</span>
            <span id="bet-slip-total-cost" class="data" data-bind="text: '$' + totalCost().toFixed(2)"></span>
        </li>
        <!-- ko if: (bonusBetAmount() > 0) -->
        <li>
            <span class="title">Bonus Bet</span>
            <!-- ko if: bonusBetAmount() > 0 -->
            <span id="bet-slip-bonus-cost" class="data" data-bind="text: '- $' + bonusBetAmount().toFixed(2)"></span>
            <!-- /ko -->
            <!-- ko ifnot: bonusBetAmount() > 0 -->
            <span id="bet-slip-bonus-cost" class="data" data-bind="text: '$' + bonusBetAmount().toFixed(2)"></span>
            <!-- /ko -->
            <!-- ko if: showCampaignTooltip -->
            <span class="tticon tticon-infoinvertround right r10" data-bind="toolTipster: { contentAsHTML: true, maxWidth: 500, content: campaignInfo }"><span class="accessible">More info</span></span>
            <!-- /ko -->
        </li>
        <li>
            <!-- ko ifnot: committed -->
            <span class="title">Amount Payable</span>
            <!-- /ko -->
            <!-- ko if: committed -->
            <span class="title">Amount Paid</span>
            <!-- /ko -->
            <span id="bet-slip-amount-payable" class="data" data-bind="text: '$' + totalPayable().toFixed(2)"></span>
        </li>
        <!-- /ko -->
    </ul>
    <!-- ko if: editable -->
    <div class="bet-buttons">
        <a id="betslip-clear-button" data-bind="click: clear" class="red button text-upper separator-right">CLEAR BETS</a>
        <a id="betslip-continue-button" data-bind="click: precommit, css: { 'disabled': !hasSelectedBets() }, text: nextButtonText" class="green button confirm">CONTINUE</a>
    </div>
    <!-- /ko -->
    <!-- ko if: validated -->
    <div class="bet-buttons">
        <a id="betslip-back-button" data-bind="click: cancel" class="red button text-upper separator-right">BACK</a>
        <a id="betslip-confirm-bets-button" data-bind="click: commit" class="green button confirm">CONFIRM BETS</a>
    </div>
    <!-- /ko -->
    <!-- ko if: committed -->
    <div class="bet-buttons">
        <a id="betslip-close-clear-button" data-bind="click: closeAndClear, css: {red: failedBetCount() > 0, green: failedBetCount() == 0, 'width-100pc': failedBetCount() == 0}, text: (failedBetCount() == 0 ? 'DONE' : 'CLEAR')" class="button">DONE</a>
        <a id="betslip-close-retry-button" data-bind="click: closeAndRetry, visible: failedBetCount() > 0" class="green button confirm">RETRY</a>
    </div>
    <!-- /ko -->
</div>

<busy-overlay-panel params="{ show: isBusy }"></busy-overlay-panel>

<script id="tote-template" type="text/html">
    <div class="bet-item" data-bind="css: {success: isPlaced, error: (responseMessage || responseCode != ozbetApi.responseCodes.Success)}, attr: {id: itemId}">

        <div data-bind="template: { name: 'campaigns-template' }"></div>

        <h4>
            <!-- ko if: $parent.editable -->
            <a class="remove tticon tticon-cross" data-bind="click: $parent.remove"><span class="accessible">Remove</span></a>
            <!-- /ko -->
            
            <a class="meeting" tabindex="-1" data-bind="attr: { href: racingItem.meetingUrl }, text: racingItem.courseName"></a>
            <span class="icon-placed" data-bind="visible: isPlaced">Placed <span class="tticon tticon-tickinvert l5"><span class="accessible">bet</span></span></span>
            <span class="tticon tticon-mysteryinvert r5" data-bind="visible: toteRacingItem.isMystery"><span class="accessible">Mystery Bet</span></span>
            <!-- ko if: hasSpecialOffer -->
            <span class="tticon tticon-special special-bright r5"><span class="accessible">Has a Special</span></span>
            <!-- /ko -->
        </h4>
        <div data-ui="bet-item-response-message" data-bind="visible: responseMessage, css: {error: (responseCode != ozbetApi.responseCodes.Success), warning: (responseCode == ozbetApi.responseCodes.Success)}" class="notice small">
            <span class="tticon" data-bind="css: {'tticon-crossinvert': (responseCode != ozbetApi.responseCodes.Success), 'tticon-exclaiminvert': (responseCode == ozbetApi.responseCodes.Success)}"></span>
            <!-- ko text: responseMessage --><!-- /ko -->
        </div>
        <div class="race-details clearfix">
            
            <a class="race" tabindex="-1" data-bind="attr: { href: racingItem.raceUrl }, text: racingItem.raceDisplay"></a>
            <span class="time" data-bind="text: startTimeDisplay, startTimeAlert: $parent.serverTime"></span>
        </div>

        <!-- ko if: isWinPlace -->
        <div class="bet-details clearfix" data-bind="foreach: toteRacingItem.lines[0].starters">
            <div class="starter-number" data-bind="text: num"></div>
            <span class="starter-name" data-bind="text: name"></span>
        </div>
        <!-- /ko -->
        <!-- ko ifnot: isWinPlace -->
        <div class="bet-details clearfix">
            <div class="market">
                <strong data-bind="text: betName"></strong><br />
                <small data-bind="text: toteRacingItem.betSubtype"></small>
            </div>
            <span class="selections-short" data-bind="html: toteRacingItem.selections"></span>
            <a class="button tticon" data-bind="click: toggleExpand, css: { 'tticon-arrowdownsolid' : !expanded(), 'tticon-arrowupsolid' : expanded()}"><span class="accessible">Expand/Collapse</span></a>
        </div>
        <!-- ko ifnot: isMysteryCombo -->
        <div class="selections-detail" data-bind="foreach: { data: toteRacingItem.lines, as: 'line' }, expandSlideVisible: expanded()" style="display: none;">
            <!-- ko foreach: starters -->
            <div class="clearfix">
                <!-- ko if: line.prefix -->
                <span class="leg-number" data-bind="text: $index() == 0 ? line.prefix : ' '"></span>
                <!-- /ko -->
                <div class="starter-number" data-bind="text: num"></div>
                <span class="proposition-name" data-bind="text: name"></span>
            </div>
            <!-- /ko -->
        </div>
        <!-- /ko -->
        <!-- ko if: isMysteryCombo -->
        <div class="selections-detail" data-bind="foreach: { data: toteRacingItem.combos, as: 'combo' }, expandSlideVisible: expanded()" style="display: none;">
            <div class="clearfix">
                <strong data-bind="text: combo.betType"></strong>
                <!-- ko text: combo.subType--><!-- /ko -->
                <span class="right" data-bind="text: 'Div x ' + combo.div">Div x 3</span>
            </div>
            <div class="clearfix" data-bind="css: { 'border-bottom': ($index() < $parent.toteRacingItem.combos.length - 1) }">
                <span class="selection multiple">
                    <!-- ko foreach: starters -->
                    <div class="starter-number" data-bind="text: $data"></div>
                    <!-- /ko -->
                </span>
                <span class="proposition-name" data-bind="text: combo.name"></span>
            </div>
        </div>
        <!-- /ko -->
        <!-- /ko -->

        <div class="bet-amount clearfix no-focus" tabindex="-1">
            <!-- ko switch -->
            <ol class="values" data-bind="case: isNonFlexiMystery">
                <li class="no-focus" tabindex="-1">
                    <label>Investment</label>
                    <!-- ko if: $parent.editable -->
                    <input type="number" name="investment1" class="win" min="0" max="99999.99" data-bind="investValue: investment"/>
                    <!-- /ko -->
                    <!-- ko ifnot: $parent.editable -->
                    <span class="value" data-bind="text: investment().toFixed(2)"></span>
                    <!-- /ko -->
                </li>
                <li>
                    <label>Bet Cost</label>
                    <!-- ko if: $parent.editable -->
                    <span class="value" data-bind="text: cost().toFixed(2)"></span>
                    <!-- /ko -->
                    <!-- ko ifnot: $parent.editable -->
                    <span class="value" data-bind="text: betCost.toFixed(2)"></span>
                    <!-- /ko -->
                </li>
                <li class="cashbet-label" style="display: list-item" data-bind="visible: $parent.cashMode">
                    <label>Name this bet</label>
                    <input type="text" maxlength="50" class="basicInput required" data-bind="textInput: notes">
                </li>
                <li class="cashbet-label" style="display: list-item" data-bind="visible: $parent.cashMode() && toteRacingItem.isMystery">
                    <label>Ignore selections?</label>
                    <div class="checker"><span data-bind="css: { checked: ignoreMysterySelections }"><input type="checkbox" class="uniformed" data-bind="checked: ignoreMysterySelections" /></span></div>
                </li>
            </ol>
            <ol class="values" data-bind="casenot: isFlexi">
                <li class="no-focus" tabindex="-1" data-bind="visible: $parent.showInvestment(investment())">
                    <label>Tote Win</label>
                    <!-- ko if: $parent.editable -->
                    <input type="number" name="investment1" class="win" min="0" max="99999.99" placeholder="Win" data-bind="investValue: investment"/>
                    <a class="button" data-bind="click: toggleCalculator.bind($data, 'win')"><span class="tticon tticon-pencil"><span class="accessible">Quick Amount</span></span></a>
                    <!-- /ko -->
                    <!-- ko ifnot: $parent.editable -->
                    <span class="value" data-bind="text: investment().toFixed(2)"></span>
                    <!-- /ko -->
                </li>
                <li data-bind="visible: calcVisible() && calcAction() === 'win'">
                    <betslip-calculator params="applyToAllButtonText: 'Apply to all win bets'"></betslip-calculator>
                </li>
                <li class="no-focus" tabindex="-1" data-bind="visible: isPlaceAvailable && $parent.showInvestment(investmentPlace())">
                    <label>Tote Place</label>
                    <!-- ko if: $parent.editable -->
                    <input type="number" name="investment2" class="place" min="0" max="99999.99" placeholder="Place" data-bind="investValue: investmentPlace"/>
                    <a class="button" data-bind="click: toggleCalculator.bind($data, 'place')"><span class="tticon tticon-pencil"><span class="accessible">Quick Amount</span></span></a>
                    <!-- /ko -->
                    <!-- ko ifnot: $parent.editable -->
                    <span class="value" data-bind="text: investmentPlace().toFixed(2)"></span>
                    <!-- /ko -->
                </li>
                <li data-bind="visible: calcVisible() && calcAction() === 'place'">
                    <betslip-calculator params="applyToAllButtonText: 'Apply to all place bets'"></betslip-calculator>
                </li>
                <!-- ko if: toteRacingItem.lines[0].starters[0].num === 'FD' -->
                <li>
                    <label>Bet Cost</label>
                    <span class="total" data-bind="text:'$' + cost().toFixed(2)"></span>
                </li>
                <!-- /ko -->
                <li class="cashbet-label" style="display: list-item" data-bind="visible: $parent.cashMode">
                    <label>Name this bet</label>
                    <input type="text" maxlength="50" class="basicInput required" data-bind="textInput: notes">
                </li>
            </ol>
            <ol class="values" data-bind="case: isFlexi">
                <li class="no-focus" tabindex="-1">
                    <label>Spend</label>
                    <!-- ko if: $parent.editable -->
                    <input type="number" name="investment1" min="0" max="99999.99" placeholder="Spend" data-bind="spendValue: investment"/>
                    <a class="button" data-bind="click: toggleCalculator.bind($data, 'win')"><span class="tticon tticon-pencil"><span class="accessible">Quick Amount</span></span></a>
                    <!-- /ko -->
                    <!-- ko ifnot: $parent.editable -->
                    <span class="value" data-bind="text: investment().toFixed(2)"></span>
                    <!-- /ko -->
                </li>
                <li data-bind="visible: calcVisible() && calcAction() === 'win'">
                    <betslip-calculator params="applyToAllButtonText: 'Apply to all ' + betType.toLowerCase() + ' bets'"></betslip-calculator>
                </li>
                <li class="no-focus" tabindex="-1">
                    <label>Flexi %</label>
                    <!-- ko if: $parent.editable -->
                    <input type="text" name="flexi" pattern="/^\d+(\.\d{2})?%$/" placeholder="Flexi" data-bind="flexiValue: flexiPercent" data-max="999999900"/>
                    <!-- /ko -->
                    <!-- ko ifnot: $parent.editable -->
                    <span class="value" data-bind="text: flexiPercent() + '%'"></span>
                    <!-- /ko -->
                </li>
                <li class="cashbet-label" style="display: list-item" data-bind="visible: $parent.cashMode">
                    <label>Name this bet</label>
                    <input type="text" maxlength="50" class="basicInput required" data-bind="textInput: notes">
                </li>
            </ol>
            <!-- /ko -->
        </div>

        <span class="ticket-number" data-bind="visible: isPlaced, text: 'Ticket Number ' + ticketSeq"></span>
    </div>
</script>
<script id="fixed-template" type="text/html">
    <div class="bet-item fixed-bet" data-bind="css: { success: isPlaced, error: (responseCode != ozbetApi.responseCodes.Success) }, attr: { id: itemId }">

        <div data-bind="template: { name: 'campaigns-template' }"></div>

        <h4>
            <!-- ko if: $parent.editable -->
            <a class="remove tticon tticon-cross" data-bind="click: $parent.remove"><span class="accessible">Remove</span></a>
            <!-- /ko -->
            <div class="checker right" data-bind="visible: !$parent.committed()"><span data-bind="betItemSelectionToggle: selected"><input type="checkbox" name="multi" value="1" class="right" style="opacity: 0;" /></span></div>
            
            <a class="meeting" tabindex="-1" data-bind="attr: { href: racingItem.meetingUrl }, text: racingItem.courseName"></a>
            <span class="icon-placed" data-bind="visible: isPlaced && !$parent.multiOn()">Placed <span class="tticon tticon-tickinvert l5"><span class="accessible">bet</span></span></span>
            <span class="tticon tticon-finvert r5"><span class="accessible">Fixed Odds</span></span>
            <!-- ko if: showSpecialOfferAwardedAtBetPlacement() -->
            <span class="tticon tticon-special special-bright r5"><span class="accessible">Has a Special</span></span>
            <!-- /ko -->
        </h4>
        <div data-ui="bet-item-response-message" data-bind="visible: responseMessage, css: { error: (responseCode != ozbetApi.responseCodes.Success), warning: (responseCode == ozbetApi.responseCodes.Success) }" class="notice small">
            <span class="tticon" data-bind="css: { 'tticon-crossinvert': (responseCode != ozbetApi.responseCodes.Success), 'tticon-exclaiminvert': (responseCode == ozbetApi.responseCodes.Success) }"></span>
            <!-- ko text: responseMessage --><!-- /ko -->
        </div>
        <div class="win-place-each" data-bind="visible: $parent.multiOn() && $parent.editable() && showPlace">
            <ul class="link-list clearfix">
                <li data-bind="multiWinPlaceClick: winPlaceOption, Option: winPlaceOptions.Win"><button value="win">Win</button></li>
                <li data-bind="multiWinPlaceClick: winPlaceOption, Option: winPlaceOptions.Place"><button value="place">Place</button></li>
            </ul>
        </div>
        <div class="race-details clearfix">
            
            <a class="race" tabindex="-1" data-bind="attr: { href: racingItem.raceUrl }, text: racingItem.raceDisplay"></a>
            <span class="time" data-bind="text: startTimeDisplay, startTimeAlert: $parent.serverTime"></span>
        </div>

        <div class="bet-details clearfix">
            <!-- ko if: fixedItem.fixedType == fixedBetTypes.Starter -->
            <div class="starter-number" data-bind="text: fixedItem.propShortName"></div>
            <span class="starter-name" data-bind="text: fixedItem.propositionName"></span>
            <!-- /ko -->
            <!-- ko if: fixedItem.fixedType == fixedBetTypes.RaceMarket -->
            <strong data-bind="text: betName"></strong> -
            <span data-bind="text: fixedItem.propositionName"></span>
            <!-- /ko -->
            <a class="button tticon" data-bind="click: toggleExpand, css: { 'tticon-arrowdownsolid' : !expanded(), 'tticon-arrowupsolid' : expanded() }"><span class="accessible">Expand/Collapse</span></a>
        </div>

        <div class="selections-detail clearfix" data-bind="expandSlideVisible: expanded()" style="display: none;">
            <ul>
                <li><span class="title">Selection No.</span><span class="data" data-bind="text: fixedItem.propositionNumber"></span></li>
                <li><span class="title">Rules</span><span class="data" data-bind="text: fixedItem.rules"></span></li>
            </ul>
        </div>

        <div class="bet-amount clearfix" data-bind="visible: !$parent.multiOn()">
            <ol class="values">
                <li class="no-focus" tabindex="-1" data-bind="visible: $parent.showInvestment(investment())">
                    <label>
                        <span class="position-name">Fixed Win</span>
                        <span class="dividend" data-bind="text: fixedItem.displayWinReturn().toMoney(), css: { boosted: shouldHighlightBoostedWinPrice }"></span>
                        <betslip-item-price-change params="{ priceChange: fixedItem.fixedWinPriceChange }" />
                    </label>
                    <!-- ko if: $parent.editable -->
                    <input type="number" name="investment1" class="win" min="0" max="99999.99" step="1" placeholder="Win" data-bind="investValue: investment"/>
                    <a class="button" data-bind="click: toggleCalculator.bind($data, 'win')"><span class="tticon tticon-pencil"><span class="accessible">Quick Amount</span></span></a>
                    <!-- /ko -->
                    <!-- ko ifnot: $parent.editable -->
                    <span class="value" data-bind="text: investment().toFixed(2)"></span>
                    <!-- /ko -->
                </li>
                <li data-bind="visible: calcVisible() && calcAction() === 'win'">
                    <betslip-calculator params="disableFiftyCents: true, applyToAllButtonText: 'Apply to all fixed win bets'"></betslip-calculator>
                </li>
                <!-- ko if: showPlace -->
                <li class="no-focus" tabindex="-1" data-bind="visible: $parent.showInvestment(investmentPlace())">
                    <label>
                        <span class="position-name">Fixed Place</span>
                        <span class="dividend" data-bind="text: fixedItem.displayPlaceReturn().toMoney(), css: { boosted: shouldHighlightBoostedPlacePrice}"></span>
                        <betslip-item-price-change params="{ priceChange: fixedItem.fixedPlacePriceChange }" />
                    </label>
                    <!-- ko if: $parent.editable -->
                    <input type="number" name="investment2" class="place" min="0" max="99999.99" step="1" placeholder="Place" data-bind="investValue: investmentPlace"/>
                    <a class="button" data-bind="click: toggleCalculator.bind($data, 'place')"><span class="tticon tticon-pencil"><span class="accessible">Quick Amount</span></span></a>
                    <!-- /ko -->
                    <!-- ko ifnot: $parent.editable -->
                    <span class="value" data-bind="text: investmentPlace().toFixed(2)"></span>
                    <!-- /ko -->
                </li>
                <li data-bind="visible: calcVisible() && calcAction() === 'place'">
                    <betslip-calculator params="disableFiftyCents: true, applyToAllButtonText: 'Apply to all fixed place bets'"></betslip-calculator>
                </li>
