        <h3 class="dark">NEXT JACKPOTS<span class="image-matrix next-5-jackpots">&#160;</span></h3>
        <ul class="panel-list links">
            <!-- ko foreach: nextFiveJackpots -->
            <li>
                <a data-bind="attr: { href: url }">
                    <span class="race-time" data-bind="nextRacesAstFormatter: ast"></span>
                    <span class="race-name">
                        <span class="image-matrix" data-bind="SetClassFromObservable: raceCode, text: raceCodeDescription"></span>
                        <!-- ko text: meetingId --><!-- /ko -->&nbsp;<!-- ko text: jackpotDescription --><!-- /ko -->
                    </span>
                    <span class="jackpot-amount" data-bind="text: jackpotAmountOrGenericText"></span>
                </a>
            </li>
            <!-- /ko -->
            <li class="right-column-all">
                <a href='/jackpots'>
                    <span class="accessible">Go to </span>all<span class="accessible"> jackpots</span><span class="image-matrix right-arrow"></span>
                </a>
            </li>
        </ul>
    </div>
    <!-- /ko -->
</script>
<script id="create-predetermined-bet-dialog-template" type="text/html">
    <div style="display: none">
        <div id="create-predetermined-bet-dialog" class="myblackbook-add-contact front" data-bind="colorboxDialog: { display: isShown }">
            <h2 class="center">CREATE BET SHORTCUT</h2>
            <form class="clearfix standard-form uniform">
                <div id="createBetShortCutDisplay" data-bind="visible: showEntryScreen">
                    <div class=" notice error small" data-bind="visible: showServerError">
                        <span class="image-matrix"></span>
                        <span data-bind="text: serverErrorText"></span>
                    </div>
                    <p>Give this bet shortcut a name</p><br />
                    <ol>
                        <li>
                            <label class="wide-100">Name</label>
                            <input id="create-predetermined-bet-dialog-name" type="text" class="required wide-200" data-bind="value: data" maxlength="50">
                            <label class="error error-200" data-bind="text: errorText, visible: showError"></label>
                        </li>
                    </ol>
                    <div><br /></div>
                    <div class="button-group clearfix">
                        <button id="create-predetermined-bet-dialog-cancel" class="button red close text-upper" data-bind="click: closeDialog">CANCEL</button>
                        <!-- ko ifnot: isBusy -->
                        <button id="create-predetermined-bet-dialog-createlink" class="button green" data-bind="click: create,colorboxResize: showServerError">CREATE SHORTCUT</button>
                        <!-- /ko -->
                        <!-- ko if: isBusy -->
                        <button class="button green disabled">Creating... <img src="/content/ozbet/images/ajax-loader-small.gif" alt="Creating" /></button>
                        <!-- /ko -->
                    </div>
                </div>
                <div id="createBetShortCutResponseDisplay" data-bind="visible: !showEntryScreen()">
                    <div class="bet-amount clearfix">
                        <ol class="values">
                            <li>
                                <p>Copy and paste this code to use your bet shortcut</p>
                                <textarea rows="4" cols="45" readonly data-bind="value: predeterminedText,textselect:true"></textarea>
                            </li>
                        </ol>
                        <a id="create-predetermined-bet-dialog-createlink" class="button green text-upper faux-strong button-fullwidth" data-bind="click:closeDialog">DONE</a>
                    </div>
                </div>
            </form>
        </div>
    </div>
</script>
<script id="feedback-form-dialog-template" type="text/html">
    <div style="display: none">
        <div id="feedback-colorbox" data-bind="colorboxDialog: { display: dialogVisible, completeHandler: afterDisplay }">
            <div id="feedback-form" class="footer-email" data-bind="visible: showEntry">
                <h2 class="center">Provide your feedback or ask a question using the form below</h2>
                <form class="clearfix standard-form uniform validate footer-email" action="" method="post" id="footer-email" data-bind="submit: doNothing, colorboxShowLoader: isBusy">
                    <ol>
                        <li>
                            <label for="control-subject">Subject <span class="text-highlight-02">*</span></label>
                            <select id="control-subject" class="required" name="subject" data-bind="uniformOptions: subjects, optionsCaption: 'Select one...', value: selectedSubject"></select>
                        </li>
                        <li>
                            <label for="footer-email-first-name">First Name</label>
                            <input id="footer-email-first-name" name="first-name" type="text" maxlength="25" data-bind="nextOnEnter: $data, value:firstName" />
                        </li>
                        <li>
                            <label for="footer-email-family-name">Last Name</label>
                            <input id="footer-email-family-name" name="last-name" type="text" maxlength="30" data-bind="nextOnEnter: $data, value:lastName" />
                        </li>
                        <li>
                            <label for="control-email">Email <span class="text-highlight-02">*</span></label>
                            <input type="email" id="control-email" name="email" class="required email" maxlength="256" data-bind="nextOnEnter: $data, value:email" />
                        </li>
                        <li>
                            <label for="control-phone">Phone</label>
                            <input type="tel" id="control-phone" name="phone" maxlength="20" data-bind="nextOnEnter: $data, value:phone">
                        </li>
                        <li>
                            <label for="footer-email-acc">TAB Account Number</label>
                            <input type="number" id="footer-email-acc" name="account" data-bind="nextOnEnter: $data, value:account" />
                        </li>
                        <li>
                            <label for="footer-email-message">Message <span class="text-highlight-02">*</span></label>
                            <textarea id="footer-email-message" name="message" cols="3" rows="5" class="required" maxlength="3000" data-bind="value:message"></textarea>
                        </li>
                    </ol>
                    <div class="notice small text-center">
                        <span class="image-matrix floatless b10"></span>
                        <br>
                        <span class="text b10">To talk to someone in person or if you have an issue that requires immediate assistance please call our Customer Management Team on 1300 36 36 88.</span>
                    </div>
                    <input type="hidden" name="formSourceName" value="StandardForm" />
                    <div class="button-group clearfix">
                        <button class="button red close" type="button" data-bind="clickResetValidation: cancel">CANCEL</button>
                        <button class="button green" type="button" data-bind="clickValidation: submitForm">SEND</button>
                    </div>
                </form>
            </div>
            <div class="footer-email-success" data-bind="visible:showSuccess">
                <span class="image-matrix big-success"></span>
                <h2>Thanks for getting in touch!</h2>
                <p class="big-21-10">Your message has been sent to our Customer Management Team. Should you require immediate assistance please call 1300 36 36 88.</p>
                <a class="button green button-fullwidth large text-upper" data-bind="click: cancel">CONTINUE</a>
            </div>
            <div class="footer-email-failure" data-bind="visible:showError">
                <span class="image-matrix big-failure"></span>
                <h2>Something went wrong</h2>
                <p class="big-21-10" data-bind="html: error"></p>
                <div class="button-group clearfix">
                    <button class="button red close" data-bind="click: cancel">CANCEL</button>
                    <button class="button green" data-bind="click: retry">TRY AGAIN</button>
                </div>
            </div>
        </div>
    </div>
</script>
<script id="logon-dialog-template" type="text/html">
    <div style="display: none">
        <div id="loginDialog" data-bind="colorboxDialog: { display: dialogVisible, completeHandler: afterDisplay, closedHandler: onClosed }">
            <div id="logonPane" class="login front" data-bind="visible: logonPaneVisible">
                <h2 class="center">Log in to your Member Account</h2>
                <div class="copy errors" data-bind="visible: logonAttemptFailed, text: invalidLogonMessage, colorboxResize: logonAttemptFailed">
                </div>

                <form class="clearfix uniform pb10">
                    <ol>
                        <li>
                            <label>Account Number</label>
                            <input id="logonAccountNumber" tabindex="1" type="text" maxlength="8" data-bind="value: userName, valueUpdate: 'input', hasfocus: focusUserName, executeOnEnter: logon"/>
                            <p class="text-right t5 b10 clearfix">
                                <a id="logonRecoverAccountNumber" tabindex="-1" href="/recover-account-number">Forgot account number </a>
                            </p>
                        </li>
                        <li>
                            <label>Password</label>
                            <input id="logonPassword" type="password" tabindex="2" data-bind="value: password, valueUpdate: 'input', hasfocus: focusPassword, executeOnEnter: logon"/>
                            <p class="text-right t5 b0 clearfix">
                                <a id="logonRecoverPassword" tabindex="-1" href="/recover-password">Forgot password</a>
                            </p>
                        </li>
                        <li>
                            <label for="rememberMe">Remember Me</label>
                            <div class="checker"><span data-bind="css: { checked: rememberMe }"><input type="checkbox" tabindex="-1" id="rememberMe" class="uniformed" data-bind="checked: rememberMe, executeOnEnter: logon, uniformCheckbox: $data" /></span></div>
                        </li>
                    </ol>
                </form>

                <div class="button-group clearfix">
                    <button id="logonCancelButton" tabindex="-1" class="button red styled close" data-bind="click: cancel">CANCEL</button>
                    <div data-bind="if: isBusy">
                        <button id="logonRequestButton" tabindex="3" class="button green disabled">LOGGING IN... <img src="/content/ozbet/images/ajax-loader-small.gif" data-bind="visible: isBusy && !sliderVisible" alt="Logging on" /></button>
                    </div>
                    <div data-bind="ifnot: isBusy">
                        <button id="logonRequestButton" tabindex="3" class="button green" data-bind="visible: !sliderVisible(), enable: canLogon, click: logon, css: { disabled: canLogon() === false }">LOG IN</button>
                    </div>
                    <div>
                        <div id="slider-verify" data-bind="visible: sliderVisible(), slider: slideValue, sliderOptions: {min: 0, max: 100, range: 'min', step: 10},event: { OnSliderMaxreached: logon }" class="ui-slider ui-slider-horizontal ui-widget ui-widget-content ui-corner-all QapTcha">
                            <div class="track"><span>Slide to log in</span></div>
                            <a class="ui-slider-handle ui-state-default ui-corner-all" href="#" style="left: 0%;"></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>


