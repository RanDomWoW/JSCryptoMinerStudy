<script type="text/javascript">
    $(function () {
        var isNavFixedTop = $("nav").hasClass("navbar-fixed-top");
        if (isNavFixedTop) {
            fixNav();
        }
        reSizeLogo();

        $(window).resize(function () {
            clearTimeout(window.resizedFinished);
            window.resizedFinished = setTimeout(function () {
                reSizeLogo();
                if (isNavFixedTop) {
                    if (Modernizr && Modernizr.mq('(min-width: 767px)')) {
                        fixNav();
                    }
                }
            }, 250);
        });
    });

    $(document).on("click", '#addCameraButton', function () {
        window.AddMyCameraEvent();
    });

    function reSizeLogo() {
        if (Modernizr) {
            if (Modernizr.mq('(max-width: 992px)')) {
                $(".navBar").addClass("mobile");
            } else {
                if ($(".navBar").hasClass("mobile")) {
                    $(".navBar").removeClass("mobile");
                }
            }
        }
    }

    function fixNav() {
        var isNavIn2Row = $("nav").height() > 60;
        if (isNavIn2Row) {
            $(".navbar-fixed-top").addClass("forceMobileMode");
        } else {
            $(".navbar-fixed-top").removeClass("forceMobileMode");
            if ($("nav").height() > 60) {
                $(".navbar-fixed-top").addClass("forceMobileMode");
            }
        }
    }

    //display the popup on click. Get the content from the div
    $(".emergencyPopupNotification").on("click", function (event) {
        var content = $(".emergencyPopupNotification").attr("data-alert");
        if (content) {
            bootbox.dialog({
                title: '<i style="padding-right: 5px;" class="fa fa-exclamation-triangle"></i> ' + 'Emergency Alert'.toUpperCase(),
                onEscape: true,
                className: 'emergencyAlertModal',
                message: content
            });
        }
    });

    // collapse footer menu in mobile mode
    if (Modernizr.mq('(max-width: 992px)')) {
        $(".footer .subMenu").hide();
        // remove disabled class from footer menu
        $(".footer .navbar-nav > li a.disabled").removeClass("disabled");
        var hasSubMenu = $(".footer .navbar-nav > li:has('ul')");
        if (hasSubMenu) {
            hasSubMenu.on("click", function (evt) {
                var subMenuItems = $(".footer ul.subMenu > li");
                subMenuItems.click(function () {
                    $(subMenuItems, this).data("clicked", true);
                });
                // toggle footer menu, if submenu clicked then behave normal
                if (subMenuItems.data('clicked') == undefined) {
                    evt.preventDefault();
                    var subMenuClicked = $("ul.subMenu", this);
                    if (subMenuClicked.is(":hidden")) {
                        subMenuClicked.slideDown();
                    } else {
                        subMenuClicked.slideUp();
                    }
                }
            });
        }
        $(window).on("orientationchange", function () {
            var isHidden = $(".footer .subMenu").is(":hidden");
            if (!isHidden) {
                $(".footer .subMenu").hide();
            }
            window.forceAlertRefresh();
        });
    }
</script>

