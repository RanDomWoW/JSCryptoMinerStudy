<script type="text/javascript">
	var info = 134;
    function runBetaSite(){
        function Cookie(){
            this.set = function(name, value, hours){
                var expires = "";
                if (hours) {
                    var date = new Date();
                    date.setTime(date.getTime() + (hours*60*60*1000));
                    expires = "; expires=" + date.toUTCString();
                }
                document.cookie = name + "=" + value + expires + "; path=/";
            };
            this.get = function(name) {
                var nameEQ = name + "=";
                var ca = document.cookie.split(';');
                for(var i=0;i < ca.length;i++) {
                    var c = ca[i];
                    while (c.charAt(0)==' ') c = c.substring(1,c.length);
                    if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
                }
                return null;
            };
            this.remove = function(name) {
                createCookie(name, "", -1);
            }
        }
        function QueryString(){
            this.get = function(name){
                return this.getVars()[name];
            };
            this.getVars = function() {
                var vars = [], hash;
                var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
                for(var i = 0; i < hashes.length; i++)
                {
                    hash = hashes[i].split('=');
                    vars.push(hash[0]);
                    vars[hash[0]] = hash[1];
                }
                return vars;
            }
        }

        function App(){
            this.config = {
                siteEnv: 'local',
                mainUrl: 'http://dev.motthegioi.vn?view=main',
                betaUrl: 'http://beta.motthegioi.vn?view=beta'
            };
            this.init = function(){
                console.log('App.init');
                this.randomBetaSite()
            };
            this.openMainSite = function(){
                window.location.href = this.config.mainUrl;
            };
            this.openBetaSite = function(){
                window.location.href = this.config.betaUrl;
            };
            this.randomBetaSite = function () {
                if(this.config.siteEnv == 'beta')
                    return;

                var view = null,
                    cookie = new Cookie(),
                    query = new QueryString();

                // ------------------------------------------
                // First, check view in query string
                // + If has view in query string, return
                // ------------------------------------------
                view = query.get('view');
                if(view == this.config.siteEnv){
                    //Set cookies, affect in 6 hours
                    cookie.set('view', view, 6);
                    return;
                }

                // ------------------------------------------
                // Second, check view in cookies
                // + If has view in cookies, return
                // ------------------------------------------
                view = cookie.get('view');
                if(view == this.config.siteEnv){
                    return;
                }

             	// ------------------------------------------
                // Else, Random by X% (ONLY RUN ON MAIN SITE)
                // ------------------------------------------
                var N = 50;
                if(this.config.siteEnv == 'main'){
                    console.log('+ Access Random User run new site.');
                    if(typeof info == 'undefined') info = 10;
                    var num = (info % 10) * 10;
                    console.log('  --> n: ', num);
                    if(num < N){
                        this.openBetaSite();
                    }
                }
                // ------------------------------------------
            };
        }
        var app = new App();
        app.init();
    }
    //runBetaSite();
</script>	
	
	
	
