	<p>Bạn đã gửi ý kiến cho bài viết này.</p>
</div>
<script type="text/javascript">
	var appReload;
	function AppReload(){
		this.reload = function(){
			var cName = $('#comment-name').val(),
				cEmail = $('#comment-email').val(),
				cSubject = $('#comment-subject').val(),
				cMessage = $('#comment-message').val();
			if(
				(typeof cName == 'undefined' || cName == '') &&
				(typeof cEmail == 'undefined' || cEmail == '') &&
				(typeof cSubject == 'undefined' || cSubject == '') &&
				(typeof cMessage == 'undefined' || cMessage == '') ) {
				// Exec reload page
				window.location.reload();
			}
		}
		
		this.init = function(){
			setInterval(function(){
				if(appReload)
					appReload.reload();
			}, 5 * 60 * 1000);
		}
		
	}
	$(function(){
		appReload = new AppReload();
		appReload.init();
	});
</script>
</body>
</html>