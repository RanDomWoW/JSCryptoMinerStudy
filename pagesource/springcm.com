<!-- Copy Landing Page Campaign to hidden form field for Attribution -->
<script>
    // Run on window load since HubSpot forms load after everything else
// $(window).load(function() {
//     setTimeout(function() { $('input[name="landing_page_campaign"]').val('').change(); }, 1000);
// });
var timeSwitch = false;
var myInterval = null;
function myIntervalFunc(){
    if(timeSwitch === false){
        var campaign = '';
        if (campaign === "") {
        	campaign = 'No campaign set';
    	}
        $('input[name="landing_page_campaign"]').val(campaign).change();
    }else{
        clearInterval(myInterval);
    }
}   
myInterval = setInterval(myIntervalFunc, 50);
setTimeout(function(){
    timeSwitch = true;
},5000);
</script>
<!-- End Copy Landing Page Campaign to hidden form field for Attribution -->


