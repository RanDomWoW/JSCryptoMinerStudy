   <script><!--
   
   
function tracking(){ 
    if($('#track').hasClass('for_open')){
	   
        var html = '';   
        var dati = "n_ldv=" + $('#tracking_ldv').val();
        
        //LGE aggiunto if per cobtrollo campo tracking vuoto
        if($('#tracking_ldv').val().trim()== '' )
        {
            html +='<tr><td>Inserire il numero LDV della Spedizione</td></tr>';

            $('#track').removeClass("for_open");
	    $('#track').addClass("for_close");
            $('#tracking').empty();
			//$('#track').val('x');
            $('#tracking').append(html);
            $('#track_content').css({
                            "display":"block", 
                            "background": "#fff"
            });
            $('#arrow_track').css({
                            "display":"block" 

            });

        } else {

            $.ajax({
                    type: 'POST',
                    url: 'index.php?route=common/header/tracking',
                    data: dati,
                    dataType: 'json',

                    beforeSend: function() {

                            $('#block1').after('<span class="wait"><img src="catalog/view/theme/lagon/image/loading.gif" alt="" /></span>');		
                    },

                    complete: function() {

                            $('.wait').remove();					
                            $('#track').removeClass("for_open");
                            $('#track').addClass("for_close");
                            //LGE  $	
                    },

                    success: function(track) {

                            if(track[0]['error']){
                                html +='<tr><td>' + track[0]['error'] +'</td></tr>';
                            } else {

                                if(track[0]['result']['meta']['code'] == '404'){
                                     html +='<tr><td>La Spedizione con Lettera di Vettuta ' + track[0]['tracking_number'] +' non &eacute; presente negli archivi Paccofacile.it</td></tr>';

                                } else {

                                    if((track[0]['result']['data']['tracking']['checkpoints'].length) > 0){
                                        var stato_last = (track[0]['result']['data']['tracking']['checkpoints'][(track[0]['result']['data']['tracking']['checkpoints'].length -1 )]['tag']).toLowerCase(); 
                                        html += '<tr><td colspan="3" style="text-align: right; border-bottom:1px solid; border-color:gray;" ><div class="' + stato_last + 'T" style="font-weight:bold; display:inline-block; font-size:13px;text-align: center;line-height: 35px;margin-right: 10px;">' + track[0]['result']['data']['tracking']['checkpoints'][track[0]['result']['data']['tracking']['checkpoints'].length -1]['message'] + '</div><div style="float:right; display:inline-block;" class="track-big-container ' + stato_last +'"><i class="icon-aftership-tags ' + stato_last + 'B"></i></div> </td> </tr>';
                                        for (var i = (track[0]['result']['data']['tracking']['checkpoints'].length -1 ); i >= 0; i--) {
                                            var checktime = track[0]['result']['data']['tracking']['checkpoints'][i]['checkpoint_time'];
                                            var stato = (track[0]['result']['data']['tracking']['checkpoints'][i]['tag']).toLowerCase(); 
                                            var data =  checktime.substring(8,10) + "/" + checktime.substring(5,7) + "/" + checktime.substring(0,4);
                                            var ora =  checktime.substring(11,16);

                                            html += '<tr><td style="text-align:right" class="middle">' + data + '<br /><small>' + ora +'</small></td><td class="icon_track middle"><div class="track-detail-status-icon ' + stato + '"></div></td><td style="text-align:left" class="middle">' + track[0]['result']['data']['tracking']['checkpoints'][i]['message'] + '<br /><small>' + track[0]['result']['data']['tracking']['checkpoints'][i]['country_name'] ;

                                            if(track[0]['result']['data']['tracking']['checkpoints'][i]['city']){
                                                html += ' - ' + track[0]['result']['data']['tracking']['checkpoints'][i]['city'];

                                            }
                                            html +='</small></td></tr>';

                                        }
                                    } else {
                                        html +='<tr><td>La Spedizione con Lettera di Vettuta ' + track[0]['tracking_number'] +' non risulta spedita.</td></tr>';	
                                    }
                                }
                            }
                            $('#tracking').empty();
                            $('#tracking').append(html);
                            $('#track_content').css({
                                            "display":"block", 
                                            "background": "#fff"
                            });
                            $('#arrow_track').css({
                                            "display":"block" 

                            });

                    }

            });
        }
    } else {
	$('#track').removeClass("for_close");
	$('#track').addClass("for_open");
	//$('#track').val('>');
	$('#tracking_ldv').val('');		
	$('#track_content').css({
			"display":"none"
	}); 
	$('#arrow_track').css({
			"display":"none" 
	}); 
	   
    }
};
	
		
