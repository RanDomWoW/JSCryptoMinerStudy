$(document).ready(function(){
	$(".checkAvailability").on("click", function(){
       // var rootElement = $(this).parents(".home-select");
		$(".error").remove();
        sessionStorage.removeItem('checkInDate');
        sessionStorage.removeItem('checkOutDate');
        sessionStorage.removeItem('hotel');
        var rootElement = $(".home-select");
        if(rootElement.find(".propertiesList").val()==""||rootElement.find(".checkInDate").val()==""||rootElement.find(".checkOutDate").val()==""){
			$(".booking-widget .row").append("<div class='error'> Please Enter all the details</div>");
        }
        else{
        var url = "/mysun/booking"; 
        var bookingDetails = rootElement.find(".propertiesList").val();


        var aryBookingDetails = bookingDetails.split("~");
        if(aryBookingDetails.length == 2 && rootElement.find(".propertiesList option:selected").text()!='')
        {
            console.log('Hotel Dtls available');
            sessionStorage.setItem('hotelCode',aryBookingDetails[1]);
       		sessionStorage.setItem('hotel',rootElement.find(".propertiesList option:selected").text());
        }

        if(rootElement.find(".checkInDate").val()!='')
        {
			sessionStorage.setItem('checkInDate',moment(rootElement.find(".checkInDate").val()).format('D-MMM-YYYY'));
        }

        if(rootElement.find(".checkOutDate").val()!='')
        {
			sessionStorage.setItem('checkOutDate',moment(rootElement.find(".checkOutDate").val()).format('D-MMM-YYYY'));
        }

        console.log("Booking URL=" + url);
        window.open(url, '_blank');
        }

     });

	if($("#allHomeRatedProperties").html().length > 0){
		   var allHomeRatedProperties = $.parseJSON($("#allHomeRatedProperties").html());
		   var count = 0;
		   var iteration = 4;
		   if(allHomeRatedProperties.length < 4){
			   iteration = allHomeRatedProperties.length;
		   }
		   var aryNewJSON = new Array();
		   var achieved = false; 
		   
		   if(allHomeRatedProperties.length > 0){
			   while(!achieved){
				   var randomItem = allHomeRatedProperties[Math.floor(Math.random()*allHomeRatedProperties.length)]
				   if(randomItem){
					   var exists = false;
					   for(var k = 0; k < aryNewJSON.length; k++){
						   if(aryNewJSON[k].name == randomItem.name){
							   exists = true;
							   break;
						   }
					   }
					   if(exists == false){
						   var l = aryNewJSON.length;
			               aryNewJSON[l] = randomItem;
						   var source   = $("#propertyBlockTemplate").html();
		                   var template = Handlebars.compile(source);
		                   $("#rndPropertiesBlock").append(template(randomItem));
		                   //console.log(JSON.stringify(aryNewJSON));
		                   count++;
		                   if(count == 4 || count == allHomeRatedProperties.length){
		                	   achieved = true;
		                	   $(".view-button").click(function(){
		                		   var viewUrl = $(this).data("href");
		                		   location.href = viewUrl;
		                	   });
		                   }
					   }
				   }
			   }
		   }
	}

});
