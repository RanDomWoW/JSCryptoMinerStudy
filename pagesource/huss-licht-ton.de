// Javascript für Express Countdown
function countdown(timerEnde, id){
 
  // Sekunden bis zum Countdown Ende setzen
  tempTimerEnde = timerEnde;
  
  // Stunden berechnen
  h = Math.floor(tempTimerEnde/(60*60));
 
  // Minuten berechnen
  m = Math.floor(tempTimerEnde/60) %60;
 
  // Sekunden berechnen
  s = tempTimerEnde %60;
 
  // Zahlen zweistellig formatieren formatieren
  h = (h < 10) ? "0" +h : h;
  m = (m < 10) ? "0" +m : m;
  s = (s < 10) ? "0" +s : s;
 
  // Ausgabestring generieren
  //strZeit = h + " Std " + m + " Min " + s + " Sek";
  strZeit = h + " Std " + m + " Min " + s;

  if(timerEnde > 0) {
    window.setTimeout('countdown('+ --timerEnde+',\''+id+'\')', 1000);
  }
 
  document.getElementById(id).innerHTML = strZeit;
  //document.getElementById(id).innerHTML = "sepp fff eee";
}

