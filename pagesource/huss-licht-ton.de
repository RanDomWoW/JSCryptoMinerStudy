function addCart(form) {
    var post = '';
    var ieVersion = getInternetExplorerVersion();
    for (i = 0; i < form.elements.length; i++) {
        if (form.elements[i].type != 'radio' || form.elements[i].checked == true) {
            if (i > 0) { post += '&'; }
            post += form.elements[i].name + '=' + encodeURIComponent(form.elements[i].value);
        }
    }
    post += '&XTCsid=';
    //alert(post);
    showAddCart(post);
    if (ieVersion != 6){
      hussOpenLightbox();
      window.setTimeout(hussCloseLightbox, 2000);
    }
    return false;
}

function addCartQuick(form) {
    var addquickartnr = document.getElementById("huss_add_quick_art_nr").value;
    var ieVersion = getInternetExplorerVersion();
    document.quick_add.reset();
    var post = 'artnr=' + addquickartnr + '&XTCsid=';
    showAddCartQuick(post);
    return false;
}

function addCartList(data) {
    var post = data;
    var ieVersion = getInternetExplorerVersion();
    post += '&XTCsid=';
    showAddCart(post);
    if (ieVersion != 6){
      hussOpenLightbox();
      window.setTimeout(hussCloseLightbox, 2000);
    }
    return false;
}

function showAddCart(post) {
	//alert(post);
    if (http != null) {
        //http.abort();
        http.open("POST", 'ajaxAddCart.php', true);
        http.setRequestHeader("Content-Type","application/x-www-form-urlencoded");
        //http.overrideMimeType("text/xml; charset=ISO-8859-15");
        http.onreadystatechange = function() {
            if (!http) {
                return;
            }
            if (http.readyState == 4) {
                var response = http.responseText;
                var amp = new RegExp("&amp;", "g");
                document.getElementById("box_cart").innerHTML = response.replace(amp, '&');;
                setColor(255, 255, 255, 'new_in_cart')
                setTimeout("fade(255, 255, 255, 255, 255, 255, 50, 'new_in_cart');", 2000);
            }
        }
        http.send(post);
    }
}

function showAddCartQuick(post) {
	var post1 = post + '&checkdata=1';
    if (http != null) {
        http.open("POST", 'ajaxAddCart.php', true);
        http.setRequestHeader("Content-Type","application/x-www-form-urlencoded");
        http.onreadystatechange = function() {
            if (!http) {
                return;
            }
            if (http.readyState == 4) {
                var response = http.responseText;
                var amp = new RegExp("&amp;", "g");
                var responsedata = response.replace(amp, '&');;
                var data = responsedata.split("|");
                data[0] = Number(data[0]);
                data[1] = Number(data[1]);
                if (isNaN(data[0]) || data[0] == 0 || isNaN(data[1]) || data[1] < 1){
                	if (ieVersion != 6){
                        hussOpenLightboxError();
                        window.setTimeout(hussCloseLightboxError, 2000);
                    }
                } else {
                	var ieVersion = getInternetExplorerVersion();
                	if (ieVersion != 6){
                        hussOpenLightbox();
                        window.setTimeout(hussCloseLightbox, 2000);
                    }
                	post = "products_qty=" + data[1] + "&products_id=" + data[0] + "&XTCsid=";
                	showAddCart(post);
                }
            }
        }
        http.send(post1);
    }
}

function setColor(r, g, b, element_name) {
    var hr = hex(r); var hg = hex(g); var hb = hex(b);
    element = document.getElementById(element_name);
    element.style.backgroundColor = "#"+hr+hg+hb;
    //console.log("#"+hr+hg+hb);
}

function hex(i) {
    if (i < 0)
    return "00";
    else if (i > 255)
    return "ff";
    else
    return "" + hexa[Math.floor(i/16)] + hexa[i%16];
}

function fade(sr, sg, sb, er, eg, eb, step, element) {
    for(var i = 0; i <= step; i++) {
        setTimeout("setColor(Math.floor(" + sr + " + ((( " + er + " - " + sr + " )/ " + step + ") * " + i + ")), Math.floor(" + sg + " + ((( " + eg + " - " + sg + " )/ " + step + ") * " + i + ")), Math.floor(" + sb + " + ((( " + eb + " - " + sb + " )/ " + step + ") * " + i + ")), '" + element + "');", i * step);

    }
}

function hussOpenLightbox (value) {
    document.getElementById('fade').style.display='block';
    hussLightboxFadeInBackground(0);
    document.getElementById('light').style.display='block';
    hussLightboxFadeInLight(0);
}

function hussOpenLightboxError (value) {
    document.getElementById('fade').style.display='block';
    hussLightboxFadeInBackground(0);
    document.getElementById('lighterror').style.display='block';
    hussLightboxFadeInLightError(0);
}

function hussCloseLightbox (value){
	hussLightboxFadeOutLight(100);
	hussLightboxFadeOutBackground(20);
}

function hussCloseLightboxError (value){
	hussLightboxFadeOutLightError(100);
	hussLightboxFadeOutBackground(20);
}

function hussLightboxFadeInBackground(step) {
   step = step || 0;

   document.getElementById('fade').style.opacity = step/100;
   document.getElementById('fade').style.filter = "alpha(opacity=" + step + ")"; // IE

   step = step + 2;

    if (step <= 20) {
        window.setTimeout(function () { hussLightboxFadeInBackground(step); }, 1);
    }
}

function hussLightboxFadeInLight(step) {
   step = step || 0;

   document.getElementById('light').style.opacity = step/100;
   document.getElementById('light').style.filter = "alpha(opacity=" + step + ")"; // IE

   step = step + 10;

    if (step <= 100) {
        window.setTimeout(function () { hussLightboxFadeInLight(step); }, 1);
    }
}

function hussLightboxFadeInLightError(step) {
   step = step || 0;

   document.getElementById('lighterror').style.opacity = step/100;
   document.getElementById('lighterror').style.filter = "alpha(opacity=" + step + ")"; // IE

   step = step + 10;

    if (step <= 100) {
        window.setTimeout(function () { hussLightboxFadeInLightError(step); }, 1);
    }
}

function hussLightboxFadeOutLight(step) {
   step = step || 0;

   document.getElementById('fade').style.opacity = step/100;
   document.getElementById('fade').style.filter = "alpha(opacity=" + step + ")"; // IE

   step = step - 5;

    if (step > 0) {
        window.setTimeout(function () { hussLightboxFadeOutLight(step); }, 1);
    } else {
    	document.getElementById('light').style.display='none';
    }
}

function hussLightboxFadeOutLightError(step) {
   step = step || 0;

   document.getElementById('fade').style.opacity = step/100;
   document.getElementById('fade').style.filter = "alpha(opacity=" + step + ")"; // IE

   step = step - 5;

    if (step > 0) {
        window.setTimeout(function () { hussLightboxFadeOutLightError(step); }, 1);
    } else {
    	document.getElementById('lighterror').style.display='none';
    }
}

function hussLightboxFadeOutBackground(step) {
   step = step || 0;

   document.getElementById('fade').style.opacity = step/100;
   document.getElementById('fade').style.filter = "alpha(opacity=" + step + ")"; // IE

   step = step - 1;

    if (step > 0) {
        window.setTimeout(function () { hussLightboxFadeOutBackground(step); }, 1);
    } else {
    	document.getElementById('fade').style.display='none';
    }
}

function getInternetExplorerVersion(){
  //IE 6 Check
  var rv = -1;
  if (navigator.appName == 'Microsoft Internet Explorer')
  {
    var ua = navigator.userAgent;
    var re  = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");
    if (re.exec(ua) != null)
      rv = parseFloat( RegExp.$1 );
  }
  return rv;
}

