        <script type="text/javascript" src="//resources.personal.com.ar/components/tpinjector/v1/tpinjector.js"></script>
        <script type="text/javascript" src="//resources.personal.com.ar/components/jquery/owl/owl.carousel.min.js"></script>
        <!--script type="text/javascript" src="//resources.personal.com.ar/components/jquery/deserialize/jquery.deserialize.min.js" async></script-->
        <!--script type="text/javascript" src="//resources.personal.com.ar/components/jquery/taffy/taffy-min.js"></script-->
        <script type="text/javascript">
                function uiResize() {
                    var maxHeight = -1;
                    $(".product").each(function(index) {
                        $(this).height('auto'); //reseteo la altura de cada elemento
                        if ($(this).height() > maxHeight) {
                            maxHeight = $(this).height(); //busco la altura maxima
                        }
                    });

                    $(".product").each(function(index) {
                        $(this).height(maxHeight); //aplico la altura maxima a todos los elementos
                    });
                }

            function mayRevealTarget(that) {
                if ($(that).data("revealtarget") != null) {
                    var target = false;
                    if ($(that).data("revealtarget") == "" && $(that.hash)) {
                        var target = $($(that.hash));
                    } else {
                        var target = $(that).data("revealtarget");
                    }

                    $('html, body').animate({
                        scrollTop: Math.max(0, $(target).offset().top - 0)
                    }, 500); //focus element
                }
            }

            function renderItems(dataTemplate, container, dataObject) {
                var itemTpl = $('script[data-template="' + dataTemplate + '"]').text().split(/\$\{(.+?)\}/g);
                $(container).append(dataObject.map(function(item) {
                    return itemTpl.map(renderItem(item)).join('');
                }));
            }

            function renderItem(props) {
                return function(tok, i) {
                    return (i % 2) ? props[tok] : tok;
                };
            }

            $(document).ready(function() {
                var tam = -1;
                jQuery.getJSON(("//resources.personal.com.ar" + "/data/home.json?callback="), function(jsondata) {
						planes=""
						jQuery.getJSON(("/tienda/planes/data/planes.json"), function(data) {
							planes=data;
							var planes=[];
							jQuery.each( data, function( i, val ) {
								if(val.muestraEn.search("home") >= 0){
									planes.push(val)
								}
							});
							if ($("#planes-list").length == 1) {
								renderItems("producto-template", '#planes-list', planes);
							}
							
						})                        
					tam = planes.length + jsondata.planes.length + jsondata.planes.length;


                    if ($("#celulares-list").length == 1) {
                        renderItems("producto-template", '#celulares-list', jsondata.celulares);
                    }
                    if ($("#celuplanes-list").length == 1) {
                        renderItems("producto-template", '#celuplanes-list', jsondata.celuplan);
                    }

                    $('.well-badge p').each(function() {
                        if ($(this).text() === " ") {
                            $(this).closest('.icono').hide();
                        }
                    });

                   

                    var checkproducts = setInterval(function() {
                        if (tam = $('.item').length) {
                            var owl2 = $('.producto-carousel');
                            owl2.owlCarousel({
                                margin: 10,
                                nav: true,
                                loop: false,
                                rewind: true,
                                responsive: {
                                    0: {
                                        navText: ['<i class="tpicon tpicon-flechaizquierda" aria-hidden="true"></i>', '<i class="tpicon tpicon-flechaderecha" aria-hidden="true"></i>'],
                                        items: 1,
                                        stagePadding: 40,
                                        margin: 30,
                                        nav: true,
                                        dots: true
                                    },
                                    768: {
                                        navText: ['<i class="tpicon tpicon-flechaizquierda" aria-hidden="true"></i>', '<i class="tpicon tpicon-flechaderecha" aria-hidden="true"></i>'],
                                        items: 2,
                                        margin: 30,
                                        nav: true,
                                        dots: true
                                    },
                                    992: {
                                        navText: ['<i class="tpicon tpicon-flechaizquierda" aria-hidden="true"></i>', '<i class="tpicon tpicon-flechaderecha" aria-hidden="true"></i>'],
                                        items: 3,
                                        margin: 30,
                                        nav: true,
                                        dots: true
                                    }
                                }
                            });
                            clearInterval(checkproducts);
                            uiResize();
                        }
                    }, 100);
                });

                var owl = $('.oportunidades-carousel');
                owl.owlCarousel({
                    margin: 10,
                    nav: true,
                    loop: false,
                    responsive: {
                        0: {
                            navText: ['<i class="tpicon tpicon-flechaizquierda" aria-hidden="true"></i>', '<i class="tpicon tpicon-flechaderecha" aria-hidden="true"></i>'],
                            items: 1,
                            stagePadding: 40,
                            margin: 30,
                            nav: true,
                            dots: true
                        },
                        768: {
                            navText: ['<i class="tpicon tpicon-flechaizquierda" aria-hidden="true"></i>', '<i class="tpicon tpicon-flechaderecha" aria-hidden="true"></i>'],
                            items: 3,
                            margin: 30
                        }
                    }
                });

                $(window).resize(function() {
                    uiResize();
                });
                uiResize();

                $("#ajaxModal").on("show.bs.modal", function(e) {
                    var link = $(e.relatedTarget);
                    $(this).find(".modalhtml").load(link.attr("href"));
                });

                $("BODY").on("click", "DIV.side-menu UL>LI.haschildren", function(event) {
                    event.preventDefault();
                    var subelement = $(event.target).closest("li").hasClass("side-menu-sub-item") ? true : false;
                    if (subelement) {
                        var theLink = $(event.target).closest("a");
                        if ($(theLink).length == 1) {
                            var href = $(theLink).attr('href');
                            window.location = href;
                        }
                    }

                    var thisOpened = $(this).hasClass("closed") ? false : true;

                    var sidemenuDIV = $(this).closest(".side-menu");
                    sidemenuDIV.find(".haschildren.opened UL").slideUp("fast", function() {
                        $(this).parent().removeClass("opened").addClass("closed");
                    });

                    if (!thisOpened) {
                        $(this).find("ul").slideDown("fast", function() {
                            $(this).parent().removeClass("closed").addClass("opened");
                        });
                    }
                });

                $(".side-menu-mobile-table TD").click(function() {
                    var theLink = $(this).find("A")
                    if ($(theLink).length == 1) {
                        var href = $(theLink).attr('href');
                        document.location = href;
                    }
                });

                jQuery("BODY").on("click", ".btn-hatch-on", function(e) {
                    event.preventDefault();
                    var hatch = $(this).closest(".hatch");
                    $(hatch).removeClass("hatch-off").addClass("hatch-on");
                    mayRevealTarget(this);
                    uiResize();
                });

                jQuery("BODY").on("click", ".btn-hatch-off", function(e) {
                    event.preventDefault();
                    var hatch = $(this).closest(".hatch");
                    $(hatch).removeClass("hatch-on").addClass("hatch-off");
                    mayRevealTarget(this);
                    uiResize();
                });

                function verTodos() {
                    setTimeout(function() {
                        $("ul.nav-products.nav-tabs li:first-child").on("click", function() {
                            $(".ver-todo div a").text("VER TODOS LOS PLANES");
                            $(".ver-todo div a").attr("href", "/tienda/planes/?icn=ver-todos-planes&amp;ici=personal_home_h3");
                        })
                        $("ul.nav-products.nav-tabs li:nth-child(2)").on("click", function() {
                            $(".ver-todo div a").text("VER TODOS LOS CELULARES");
                            $(".ver-todo div a").attr("href", "http://tienda.personal.com.ar/?icn=ver-todos-celulares&amp;ici=personal_home_h3");
                        })
                    }, 2000);
                }
                verTodos();

                function modalPromo() {

                        //start day
                        var startDate = $(".promo .desde span.fecha").text();
                        startDate = new Date(startDate);
                        var startHour = $(".promo .desde span.hora").text();
                        startDate.setHours(startHour);


                        //end day
                        var endDate = $(".promo .hasta span.fecha").text();
                        endDate = new Date(endDate);
                        var endHour = $(".promo .hasta span.hora").text();
                        endDate.setHours(endHour);



                        if (((startDate < new Date()) && (endDate > new Date()))) {
                            $("button#aparece-promo").click();
                        }

                    }

                     setTimeout(function(){ 

                     $("#planes-list .owl-item:first-child .plan-product .precio-plan-container").attr("id","1");
                      $("#planes-list .owl-item:nth-child(2) .plan-product .precio-plan-container").attr("id","2");
                      $("#planes-list .owl-item:nth-child(3) .plan-product .precio-plan-container").attr("id","6");

                     }, 4000);
                    modalPromo();
            });
        </script>
    </body>

    </html>