<div id="pop" class="popup login-pop">
	<div class="inner">
		<a href="#" class="popup-close"><i class="icon22 icon-close"></i></a>
		<div class="content">
			<h2 class="popup-title">Pro pokračování se musíte registrovat nebo přihlásit</h2>
      <p class="popup-text"></p>
			<a href="/profil/login?ReturnUrl=https%3a%2f%2fwww.bety.cz%2f" class="btn btn-orange pop-action">Přihlásit se</a>
			<a href="/profil/login?ReturnUrl=%2f" class="btn btn-orange pop-action" data-type="register">Registrovat</a>
		</div>
	</div>
</div>

    
<div class="bety-widget popup message-pop new-message-pop" data-bety-widget-module="messagecomposer">
	<div class="inner">
		<a href="#" class="popup-close"><i class="icon22 icon-close"></i></a>
		<div class="content">
			<div data-bind="visible: !sent()">
  			<h2 class="popup-title">Odeslat zprávu</h2>
				<div class="message-form-line">
					<label>Komu:</label>
					<div class="message-input">
						<div class="recipient">
							<img alt="" data-bind="attr: { 'src': recipient().AvatarUrl }" width="30" /><span data-bind="	text: recipient().Login"></span>
						</div>
					</div>
				</div>

				<div class="message-form-line">
					<label>Předmět:</label>
					<div class="message-input">
						<div>
							<input type="text" data-bind="value: subject" placeholder="Předmět:" />
						</div>
					</div>
				</div>

				<div class="message-form-line">
					<label>Předmět:</label>
					<div class="message-area">
						<textarea rows="5" data-bind="value: text" placeholder="Předmět:"></textarea>
					</div>
				</div>

				<div class="popup-footer">
					<button class="btn btn-orange" type="submit" data-bind="click: send">Odeslat</button>
				</div>
			</div>
			<h2 class="popup-title" data-bind="visible: sent()">Zpráva byla odeslána</h2>
		</div>
	</div>
</div>
    
<div class="message-detail popup bety-widget message-pop" data-bety-widget-module="messagedetail">
	<div class="inner" data-bind="with: currentMessage">
		<a href="#" class="popup-close"><i class="icon22 icon-close"></i></a>
		<div class="content">
			<h2 class="popup-title" data-bind="text: Subject">.</h2>
			<div class="popup-header">
				<img alt="" data-bind="attr: { 'src': Sender.AvatarUrl }" width="30" />
				<a href="#" class="name" data-bind="text: Sender.Name, attr: {'href': '/uzivatelky/' + Sender.Name() }"></a> | <span data-bind="text: Created"></span>
			</div>
			<p data-bind="text: Text"></p>

			<div class="popup-footer">
				<button class="btn btn-orange" data-bind="click: $parent.reply">Odpovědět</button>
			</div>
		</div>
	</div>
</div>

    
