          testimonialSliderResponse(testimonialTarget);
          testimonialResetTiming();
      }
  });

  $('.testimonial_next').click(function() {
      testimonialTarget = $('ul.testimonial_trigger li.active').index();
      testimonialTarget === testimonialLastElem ? testimonialTarget = 0 : testimonialTarget = testimonialTarget+1;
      testimonialSliderResponse(testimonialTarget);
      testimonialResetTiming();
  });

  $('.next').click(function() {
      target = $('ul.triggers li.active').index();
      target === lastElem ? target = 0 : target = target+1;
      sliderResponse(target);
      resetTiming();
  });

  $('.prev').click(function() {
      target = $('ul.triggers li.active').index();
      lastElem = triggers.length-1;
      target === 0 ? target = lastElem : target = target-1;
      sliderResponse(target);
      resetTiming();
  });

  $('.testimonial_prev').click(function() {
      testimonialTarget = $('ul.testimonial_trigger li.active').index();
      testimonialLastElem = testimonialTrigger.length-1;
      testimonialTarget === 0 ? testimonialTarget = testimonialLastElem : testimonialTarget = testimonialTarget-1;
      testimonialSliderResponse(testimonialTarget);
      testimonialResetTiming();
  });

  function sliderResponse(target) {
      images.fadeOut(300).eq(target).fadeIn(300);
      triggers.removeClass('active').eq(target).addClass('active');
  }

  function testimonialSliderResponse(target) {
      testimonialImages.fadeOut(300).eq(target).fadeIn(300);
      testimonialTrigger.removeClass('active').eq(target).addClass('active');
  }

  function sliderTiming() {
      target = $('ul.triggers li.active').index();
      target === lastElem ? target = 0 : target = target+1;
      sliderResponse(target);
  }

  function testimonialSliderTiming() {
      testimonialTarget = $('ul.testimonial_trigger li.active').index();
      testimonialTarget === testimonialLastElem ? testimonialTarget = 0 : testimonialTarget = testimonialTarget+1;
      testimonialSliderResponse(testimonialTarget);
  }

  function resetTiming() {
      clearInterval(timingRun);
      timingRun = setInterval(function() { sliderTiming(); },5000);
  }

  function testimonialResetTiming() {
      clearInterval(testimonialTimingRun);
      testimonialTimingRun = setInterval(function() { testimonialSliderTiming(); },5000);
  }
});
</script>
