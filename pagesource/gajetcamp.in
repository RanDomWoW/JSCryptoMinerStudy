<script type="text/javascript">
jQuery(document).ready(function($) {


if ($("[id^='ktss-offer-slider']").length) {

    // $("[id^='ktss-offer-slider']").each(function(){
    //     var counter_id=$(this).attr('id');
    //     var idString='ktss-offer-slider';
