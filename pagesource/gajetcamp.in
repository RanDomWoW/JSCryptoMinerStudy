                        //Change All English Numbers To Persian
            function traverse(el){
                if(el.nodeType==3){
                    var list=el.data.match(/[0-9]/g);
                    if(list!=null && list.length!=0){
                        for(var i=0;i<list.length;i++)
                            el.data=el.data.replace(list[i],persian[list[i]]);
                    }
                }
                for(var i=0;i<el.childNodes.length;i++){
                    traverse(el.childNodes[i]);
                }
            }

            function convertToPersian( selector, exclude) {
                if (selector.get(0).nodeType == 3) {
                    var content = selector.get(0).textContent;
                    var list=content.match(/[0-9]/g);
                    if(list!=null && list.length!=0){
                        for(var i=0;i<list.length;i++)
                            content=content.replace(list[i],persian[list[i]]);
                       selector.get(0).textContent = content;
                    }
                }
                var childrens = selector.contents();
                for (counter=0;counter < exclude.length;counter++) {
                    childrens = childrens.not(exclude[counter]);
                }
                if ( childrens.length > 0 ) {
                    for ( var counter = 0;counter < childrens.length;counter++ ) {
                        convertToPersian( childrens.eq( counter ), exclude );
                    }
                }
            }
            
            //Change MiniCart Numbers To Persian
            $( document ).ajaxComplete( function( event, xhr, settings  ) {
                if ( settings.url.includes( 'wc-ajax' ) ||
