			function onMailQuery() {
				var code = $("#querycode").val();
				if(code == "") {
					alert("请输入查询码.");
					return;
				}
				window.open("http://220.168.150.35:8017/xzrx/front/email.do?method=query&viewcard=" + code);
			}

			function pushInfoJsonpCallBack(data) {
				var count = 0;
				var datatitle = "";
				var dataStr = "";
				$.each(data, function(i, json) {
					count++;
					if(count <= 9) {
						var title = json.title;
						if(title.length > 30) {
							title = title.substring(0, 25) + "...";
						}

						if(json.pubDate.indexOf("李克强分别 批示") != -1) {
							dataStr += ("<li><span>" + json.pubDate.replace("李克强分别 批示", "") + "</span><a href='" + json.link + "' target='_blank'>" + title + "</a></li>");
						} else {
							dataStr += ("<li><a href='" + json.link + "' target='_blank'>" + title + "</a><span>" + json.pubDate + "</span></li>");
						}
					}
					if(count == 11) {
						dataStr = dataStr.replace("<br>", "").replace("<br />", "").replace("<br/>", "").replace("\n", "").replace("\r", "");
						$("#ywtj,#ywList").html(dataStr);
					}
				});
			}

			function getYW() {
				$.ajax({
					url: 'http://www.gov.cn/pushinfo/v150203/pushinfo.jsonp',
					dataType: "jsonp",
					scriptCharset: "UTF-8",
					jsonp: "pushInfoJsonpCallBack",
					jsonpCallback: "pushInfoJsonpCallBack",
					timeout: 2000,
					success: function(data) {
						var count = 0;
						var datatitle = "";
						var dataStr = "";
						$.each(data, function(i, json) {
							count++;
							if(count <= 9) {
								var title = json.title;
								if(title.length > 30) {
									title = title.substring(0, 25) + "...";
								}

								if(json.pubDate.indexOf("李克强分别 批示") != -1) {
									dataStr += ("<li><span>" + json.pubDate.replace("李克强分别 批示", "") + "</span><a href='" + json.link + "' target='_blank'>" + title + "</a></li>");
								} else {
									dataStr += ("<li><a href='" + json.link + "' target='_blank'>" + title + "</a><span>" + json.pubDate + "</span></li>");
								}
							}
							if(count == 11) {
								dataStr = dataStr.replace("<br>", "").replace("<br />", "").replace("<br/>", "").replace("\n", "").replace("\r", "");
								$("#ywtj,#ywList").html(dataStr);
							}
						})
					},
					error: function(XMLHttpRequest, textStatus, errorThrown) {
						$("#ywtj,#ywList").html("<iframe src='http://www.gov.cn/pushinfo/v150203/base_14px_pubdate.htm' width='100%' height='270' scrolling='no' frameborder='none'>");
					}
				});
			};

