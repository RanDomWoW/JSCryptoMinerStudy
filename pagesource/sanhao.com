<script>
    if(window.attachEvent) {
        window.attachEvent('onload', dialogRegHandler);
    } else {
        window.addEventListener('load', dialogRegHandler, false);
    }

    function dialogRegHandler() {
        var $cxbj = $("#cxbj"),
            $cxbjRegW = $cxbj.find('#cxbj-reg-w'),
            $formLogBanner = $cxbj.find('#form-log-banner');

        //清除coid
        $cxbj.on('click', '.cxg', function () {
            Util.setCookie('coid', '', -1);
        });

        //注册、登录切换
        $cxbj.on('click', '#cxbj-reg-w .p-ext-fn a', function () {
            $cxbjRegW.hide();
            $formLogBanner.show();
        }).on('click', '#form-log-banner .p-ext-fn a', function () {
            $cxbjRegW.show();
            $formLogBanner.hide();
        });

        /*0元抢1对1体验课*/
        $("a[data-cmd='reg']").click(function(){
            if(Util.user.status == 0) {
                Util.openWindow('/user.php?nav=' + encodeURI('体验课'), 'fv6');
            } else {
                $cxbj.show();
            }
        });

        /*4大系统课程*/
        $("a[data-cmd='ksb']").click(function(){
            if(Util.user.status == 0) {
                var serid = $(this).data('ser');
                Util.openWindow('/user.php?nav=' + encodeURI('课时包') + '&cat_id=24&teacher_level=4&service=' + serid, 'fv6');
            } else {
                $cxbj.show();
            }
        });

        function callback() {
            $cxbj.hide();
            //如果是从菜单点进去的
            var coid = Util.getCookie('coid');
            if(coid) {
                Util.setCookie('coid', '', -1);
                var arr = coid.split('-'),
                    serId = arr[0],
                    njId = arr[1];

                location.href = 'http://www.sanhao.com:80/user.php?nav=' + encodeURI('课时包') + '&cat_id=' + njId + '&teacher_level=3&service=' + serId;
            } else {
                location.href = 'http://www.sanhao.com:80/user.php?nav=' + encodeURI('课时包');
            }
        }

        c_reg('cxtc-dialog', true, callback);
        c_log('form-log-banner', callback);
    }
</script>
