<script>
    //jQuery加载前使用
    if(window.attachEvent) {
        window.attachEvent('onload', serviceHandler);
    } else {
        window.addEventListener('load', serviceHandler, false);
    }

    function serviceHandler() {
        //回到顶部
        var $goTop = $('#gotop');
        $goTop.on('click', function () {
            gotoscroll(0);
        });

        function gotoscroll(n) {
            var _window = document.documentElement;
            if (window.navigator.userAgent.toLowerCase().indexOf('firefox') != -1) {
                $(_window).animate({ 'scrollTop': n }, 400);
            }
            else if (window.navigator.userAgent.toLowerCase().indexOf('chrome') != -1) {
                $(_window).animate({ 'scrollTop': n }, 400);
                $('body').animate({ 'scrollTop': n }, 400);
            }
            else {
                $(window).scrollTop(n);
            }
        }

        //当滚动到一定位置显示回到顶部
        $(window).on('scroll', function () {
            var $t = $(this);
            if ($t.scrollTop() > 800) {
                $goTop.fadeIn(200);
            } else {
                $goTop.fadeOut();
            }
        }).trigger('scroll');
    }
