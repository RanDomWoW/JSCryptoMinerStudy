<script>
    jQuery(function () {
        //class="bg-lazy" 图片懒加载
        $('.bg-lazy, img').lazyload({ effect: 'fadeIn', skip_invisible: true, threshold: 200 });

        //region 头部登录模块
        var $loginShow = $('#login-show'), topData = {};
        if (Util.user.status == 0) {
            topData.islogin = 1;
            topData.nickname = Util.user['data']['nickname'];
            topData.type = Util.user['data']['type'];
            topData.user_type_alias = Util.user['data']['type'] == 1 ? '老师' : '同学';
        }
        else {
            //未登录
            topData.islogin = 0;
        }
        var html = template('login-template', topData);
        $('#login-show').html(html);
        //endregion

        //region 头部退出模块
        $loginShow.on('click', '[data-cmd="loginout"]', function (e) {
            if(document.domain == 'ke.' + HT + '.com' || document.domain == 'toutiao.' + HT + '.com') {
                $.ajax({
                    url: 'http://ke.' + HT + '.com' + '/do.php?act=user&method=out',
                    type: 'POST',
                    async: false,
                    xhrFields: { withCredentials: true },
                    success: function () {}
                });
                $.ajax({
                    url: 'http://toutiao.' + HT + '.com' + '/do.php?act=user&method=out',
                    type: 'POST',
                    async: false,
                    xhrFields: { withCredentials: true },
                    success: function () {}
                });
            }

        });
        //endregion

        //region 点击显示证书
        var imgSrc = ['gxjsqy','certificate','icp','icp_2'];
        $('.f-part-down p a').click(function () {
            var index = $(this).index();
            $('.certificate-bj').html('');
            var img = new Image();
            img.src = 'http://img.sanhao.com/static/images/foot/' + imgSrc[index] + '.jpg';
            $(img).appendTo($('.certificate-bj'));
            img.onload = function () {
                $('.certificate-bj').show();
            }
        });
        $('.certificate-bj').click(function () {
            $('.certificate-bj').hide();
        });
        //endregion

        //region 官方微信
        $("#Qr_Code_Weixin_Open").click(function(){
            var lazy_src = $("#Qr_Code_Window > img").attr("lazy_src");
            $("#Qr_Code_Window > img").attr("src", lazy_src);

            $("#Qr_Code_Weixin").show();
        });
        $(".bj-weixin").click(function(){
            var lazy_src = $("#Qr_Code_Window > img").attr("lazy_src");
            $("#Qr_Code_Window > img").attr("src", lazy_src);

            $("#Qr_Code_Weixin").show();
        });
        $("#Qr_Code_Window_Close").click(function(){$("#Qr_Code_Weixin").hide();});
        //endregion

        //region 意见反馈
        $(".feedback").click(function(){
            //console.log(50);
            var feedback = new SH.confirmBox({"boxName":"#feedback"});
            feedback.ok(function(){
                var type = $(".parent").find("li[class=active]").text();
                //var feedback_title = $(".children").find("li[class=active]").text();
                var feedback_title="";
                var feedback_Content = $("#fkyj").val();
                if (feedback_Content.length == 0) {
                    MessageShow('意见别为空哦亲');
                    return;
                }

                var feedback_mobile = $("#mobile").val();
                if (feedback_mobile == 0) {
                    MessageShow('联系方式别为空哦亲');
                    return;
                }

                $.ajax({
                    url: base_url + 'teacher.php?act=feedback',
                    type: 'post',
                    data: {'type': type,'feedback_title': feedback_title, 'feedback_Content': feedback_Content,'feedback_mobile': feedback_mobile},
                    success: function (data) {
                        if (data.status == 0) {
                            feedback.hide();
                            MessageShow('提交成功');
                        } else {
                            MessageShow('提交失败：' + data.error);
                        }
                    }
                });
            });
        });
        $('#feedback_Type ul li').click(function(){
            if($(this).hasClass('active')){
            }else{
                $(this).parent().children('li').removeClass('active');
                $(this).addClass('active');
            }
        });
        $("#fkyj").keyup(function(){
            showCount();
        });
        showCount();
        function showCount() {
            var count = $("#fkyj").val().length;
            $("#word_Count").html(200-count);
        }
        //endregion
    });
</script>

