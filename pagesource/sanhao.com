<script>
    if(window.attachEvent) {
        window.attachEvent('onload', regBarHandler);
    } else {
        window.addEventListener('load', regBarHandler, false);
    }

    document.getElementById('calc-count').innerHTML = (now = new Date, endTimespan = (+ now) - (+ new Date(now.getFullYear(), now.getMonth(), now.getDate())), parseInt(300 * (1 - endTimespan / (24 * 60 * 60 * 1000)))) + '名';

    function regBarHandler() {
        //fixed-注册
        var $comReg = $('#reg-bar'),
            $btnCode = $comReg.find('.btn-code'),
            $btnSubmit = $comReg.find('.btn-submit'),
            footerTop = $('.footer-w').offset().top;
        function toogleFloorTips() {
            var iSTop = $(this).scrollTop(),
                wH = $(this).height();

            if ((iSTop + wH) >= footerTop) {
                $comReg.css({ position: 'absolute', top: footerTop - 60, bottom: 'auto' });
            } else {
                $comReg.css({ position: 'fixed', top: 'auto', bottom: 0 });
            }
        }
        toogleFloorTips();
        $(window).on('scroll', toogleFloorTips).on('resize', toogleFloorTips);

        //注册文本框tips
        var valid = true, $valids = $comReg.find('#phone-num, #phone-code, #s-nj, #s-km');
        $valids.on('blur', function () {
            var $this = $(this),
                $tip = $this.next('.ui-form-explain');
            if (!$this.val() || $this.val() == -1) {
                valid = false;
                $tip.show();
            } else {
                $tip.hide();
            }
        }).on('focus', function () {
            var $this = $(this),
                $tip = $this.next('.ui-form-explain');
            $tip.hide();
        });

        $btnCode.on('click', function () {
            $btnCode.unbind('click.send');
            valid = true;
            $comReg.find('#phone-num').trigger('blur');
            if(valid) {
                //调用公用的注册
                c_reg('reg-bar', true);
                $btnCode.trigger('click.send');
            } else {
                return false;
            }
        });

        $btnSubmit.on('click', function () {
            $btnSubmit.unbind('click.core');
            valid = true;
            $valids.trigger('blur');
            if(valid) {
                //调用公用的注册
                c_reg('reg-bar', true);
                $btnSubmit.trigger('click.core');
            } else {
                return false;
            }
        });
    }
</script>
