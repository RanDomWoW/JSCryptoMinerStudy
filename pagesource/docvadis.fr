<script type="text/javascript">

    function readCookie(name) {
      var nameEQ = name + '=';
      var ca = document.cookie.split(';');
      for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) === ' ') c = c.substring(1, c.length);
        if (c.indexOf(nameEQ) === 0) return c.substring(nameEQ.length, c.length);
      }
      return null;
    }

    function setCookie(name, value, days) {
      var expires = "";
      if (days) {
        var date = new Date();
        date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
        expires = "; expires="+date.toUTCString();
      }
      document.cookie = name + "=" + value + expires + "; path=/";
    }

		function janrainSpecificCaptureWidgetOnLoad() {
		  janrain.events.onCaptureSessionFound.addHandler(function(result) {
				janrain.capture.ui.modal.close();
			});

      janrain.events.onCaptureLoginSuccess.addHandler(function(result) {
				var fromDeepLink = (readCookie('dvd-iam-deeplink') !== null);
        setCookie("dvd-iam-deeplink", "", -1);
        if(fromDeepLink || true) {
					janrain.capture.ui.modal.close();
					xhr = new XMLHttpRequest();
					var url = '/api/connect/connect/user/';
					xhr.open("POST", url, true);
					xhr.timeout = 30000;
					xhr.setRequestHeader("Content-type", "application/json");
					xhr.onreadystatechange = function () {
						if (xhr.readyState === 4 && xhr.status === 200) {
							document.location.href = '/';
						}
					};
					xhr.send(JSON.stringify(result));
				}
			});

      janrain.events.onCaptureSessionEnded.addHandler(function(result) {
      });

      janrain.events.onCaptureProfileCookieSet.addHandler(function(result) {
        /*console.log('onCaptureProfileCookieSet');*/
        /*console.log(result);*/
        /* Do nothing */
      });

      janrain.events.onCaptureRegistrationSuccess.addHandler(function(result) {
        janrain.capture.ui.modal.close();
        xhr = new XMLHttpRequest();
        var url = '/api/connect/create/user/';
        xhr.open("POST", url, true);
        xhr.timeout = 30000;
        xhr.setRequestHeader("Content-type", "application/json");
        xhr.onreadystatechange = function () {
          if (xhr.readyState === 4 && xhr.status === 200) {
            document.location.href = '/';
          }
        };
				xhr.send(JSON.stringify(result));
      });

      janrain.events.onCaptureValidationFailed.addHandler(function(result) {
      });

      janrain.events.onCaptureAccessDenied.addHandler(function(){
				var iamCookie = readCookie('dvd-iam');
				if(iamCookie !== null){
				  return;
        }
      });

      janrain.events.onCaptureScreenShow.addHandler(function(result) {
      });

      janrain.capture.ui.start();
    }
</script>
