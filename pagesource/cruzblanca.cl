    function changeRegion(val) {
        //Val es la región seleccionada
        if(val === "*") {
            //Se deja el combobox de ciudades vacío
            document.getElementById("ciudad").innerHTML = '';
            //Se obtiene obj para cargar ciudades
            var CMB_ciudad = document.getElementById("ciudad");
            //Se setea primer option por defecto
            CMB_ciudad.options[0] = new Option("- Ciudad -", "*");
        } else {
            //Se deja el combobox de ciudades vacío
            document.getElementById("ciudad").innerHTML = '';
            //Se obtiene obj para cargar ciudades
            var CMB_ciudad = document.getElementById("ciudad");
            //Se setea primer option por defecto
            CMB_ciudad.options[0] = new Option("- Ciudad -", "*");
            //Se obtienen ciudades correspondientes a la región seleccioanda
            var ciudades = ciudad[val];
            //Se recorren las ciudades y se agregan al combobox
            for(var i=0;i<ciudades.length;i++){
                CMB_ciudad.options[i + 1] = new Option(ciudades[i], ciudades[i]);
            }
        }
    };

    //Función encargada de cargar el combobox con las regiones disponibles para filtrar
    function cargaRegiones () {
        //Se obtiene el objeto para cargar regiones
        var CMB_region = document.getElementById("region");
        //Se setea option por defecto
        CMB_region.options[0] = new Option("- Región -", "*");
        //Se recorren las regiones almacenadas y se cargan
        for(var i=0;i<region.length;i++){
            CMB_region.options[i + 1] = new Option(region[i], region[i]);
        }
    };

    $(document).ready(function () {
        cargaRegiones();
    });
</script>

