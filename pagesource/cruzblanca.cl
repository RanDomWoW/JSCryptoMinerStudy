                        }
                        else if (car != ".") {
                            rutOut += car;
                        }
                        else if (car == ".") {
                            coin++;
                        }
                    }
                }

                if (rut.indexOf("-") > -1) {
                    //El formato es 1234567-8
                    rutOut = rut.substring(0, rut.indexOf("-"));
                }
                else {
