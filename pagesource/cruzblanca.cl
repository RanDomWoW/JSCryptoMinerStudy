        function validaRut(txtRut)
        {
            try
            {
var rut = txtRut.value;
                var rutOut = "";
                var dvOut = "";
                var dvCorrecto = false;
                if (rut.indexOf(".") > -1) {
                    //El formato es 12.123.234-4
                    var coin = 0;
                    for (i = 0; i < rut.length; i++) {
                        var car = rut.charAt(i);
                        if (car == "-")
