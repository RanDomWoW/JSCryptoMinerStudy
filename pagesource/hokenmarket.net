			$(function(){
			var $elem = $(".btn-area"),
			$content = $(".following-btn"),
			$win = $(window);
			var contentTop = 0;
			$win
			.load(function(){
				updatePosition();
				update();
			})
			.resize(function(){
				updatePosition();
				update();
			})
			.scroll(function(){
				update();
			});
			function updatePosition(){
				if($content.length) contentTop = $content.get(0).offsetTop + $elem.outerHeight();
			}
			function update(){
				if( $win.scrollTop() + $win.height() > contentTop ){
				$elem.addClass("static");
				}else if( $elem.hasClass("static") ){
				$elem.removeClass("static");
