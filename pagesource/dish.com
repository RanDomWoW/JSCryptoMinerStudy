<script type="text/javascript">
    triggerOverlay();

    function ProcessNeedHelpOverlay(isChatOpened) {
        if (isChatOpened) {
            $(".needhelp-chatbtn").show();
            //$(".needhelp-txtOr").show();
        }
        else {
            $(".needhelp-chatbtn").hide();
            //$(".needhelp-txtOr").hide();
        }
    }
    function overlay() {
        var windowWidth = $(window).width();

        $('#needhelp-popup-girl, #needhelp-popup-guy').parent('.dishOverlayContent').css({ 'width': '549px', 'height': 'auto', 'padding': '0', 'z-index': '51' });
        $('#needhelp-popup-girl, #needhelp-popup-guy').parents("#dishOverlayWrapper").css({ 'z-index': '51' });
        $('#needhelp-popup-girl, #needhelp-popup-guy').parents("#dishOverlayWrapper").prev("#dishOverlay").css({ "background": "#000", "opacity": "0.6", "filter": "alpha(opacity: 0.6)", "z-index": "50" });
        if (windowWidth < 768) {
            $('#needhelp-popup-girl, #needhelp-popup-guy').parents("#dishOverlayWrapper").prev("#dishOverlay").css({ "background": "none", "opacity": "1", "filter": "alpha(opacity: 1)", "z-index": "50" });
            $('#needhelp-popup-girl, #needhelp-popup-guy').parent('.dishOverlayContent').css({ 'position': 'fixed', 'left': '50%', 'margin-left': '-156px' });
        }

        $('#needhelp-popup-girl, #needhelp-popup-guy').parent('.needhelp-popup-girl, .needhelp-popup-guy').find('.dishCloseOverlay').bind("click", function () {
            createCookieInDays("Chatpopviewed", "Y", "");
            if (document.location.pathname == "/build-a-package/") {
                eventOnlyTracking("event75");
            } else {
                eventOnlyTracking("event72");
            }
        });
    }

    function openUrl() {
        var chatStatus = window.tfsObj.getChatStatus();
        if (!chatStatus) {
            //open chat popup - girl
            objOverlay.appendHTML("#needhelp-popup-girl", false);
            //open chat popup - guy
            //objOverlay.appendHTML("#needhelp-popup-girl", false);
            if (document.location.pathname == "/build-a-package/") {
                eventOnlyTracking("event74");
            } else {
                eventOnlyTracking("event71");
            }
            setTimeout(function () {
                overlay();
            }, 100);
        }
    }

    function triggerOverlay() {
        var curr_page = window.location.pathname;
        var checkInActivityPageList = ["/equipment-configurator/"];

        if ($.inArray(curr_page, checkInActivityPageList) > -1) {
            var InterstitialVisited = getCookie("InterstitialVisited");
            if(InterstitialVisited == null)
                DetectUserInActivity();
        }
        else
            ShowPopup(35);
    }
    function DetectUserInActivity() {
        (function (seconds) {
            var refresh,
                intvrefresh = function () {
                    clearInterval(refresh);
                    console.log('Detecting User InActivity - seconds : ' + seconds);
                    refresh = setTimeout(function () {
                        //location.href = location.href;
                        ShowPopup(1);
                    }, seconds * 1000);
                };

            $(document).on('mousemove', function () { intvrefresh() }); //$(document).on("keypress click"
            intvrefresh();

        }(30)); // define here seconds
    }
    function ShowPopup(seconds) {
        $(".needhelp-chatbtn").hide();
        $(".needhelp-txtOr").hide();

        //check page visit on the cookie and trigger every other page.
        var cookieval = 1;
        var Chatpopviewed = getCookie("Chatpopviewed");
        var chatpopvisits = getCookie("chatpopvisits");
        var PARTNER_NAME = getCookie("PARTNER_NAME");

        var excludePopup = ["/build-a-package/", "/satellite-tv-for-rv/", "/displayflex2/", "/displayflex110/", "/displayflexpack/", "/searchflex2/", "/searchflex110/", "/searchflexpack/", "/find-retailer/", "/special-offers/", "/hbo-included/", "/free-hd-for-life/", "/free-premiums/", "/free-installation/", "/price-guarantee/", "/displayrtq2evergreen/", "/searchq1evergreen/", "/", "/programming/packages/", "/programming/channels/", "/compare/", "/dish-vs-directv/", "/dish-vs-comcast/", "/dish-vs-charter/", "/searchq1satellite/", "/searchq1wireline/", "/bundles/", "/internet/", "/wireline/", "/dishnet/", "/swbundles/", "/nainternet/", "/swbundles/", "/cedia/"];
        var curr_page = window.location.pathname;
        //alert($.inArray(curr_page, excludePopup))
        if (Chatpopviewed == null && chatpopvisits==null && PARTNER_NAME == null && $.inArray(curr_page, excludePopup) == -1)
            setTimeout(openUrl, seconds * 1000); // Wait 35 seconds

        console.log("cookieval::" + cookieval);
    }

    function donotshowpopup() {
        createCookieInDays("chatpopvisits", "0", 1);
    }


</script>

