<script>
    $(document).ready(function () {
        $("#emailvalidationerror").text("");
        $("#txtEmail").keyup(function () {
            if ($("#txtEmail").text != "") {
                $(".recaptcha").css("display", "block");
            }
        });
    });
    function fireEmail() {
        var emailcookie = readCookie("emailCollected");
        var rsrecaptcha = grecaptcha.getResponse();

        if (rsrecaptcha != undefined && rsrecaptcha != null && rsrecaptcha != "") {
            $("#emailvalidationerror").text("");
            if (emailcookie == undefined || emailcookie == null || emailcookie == "") {
                PostToResponsys();
            }
            else {
                $("#txtEmail").val("Thank you. You are already subscribed.");
            }
        }
        else {
            $("#emailvalidationerror").text("Captcha is not validated.");
            $(".recaptcha").css("display", "block");
        }
    }

    $("#txtEmail").keyup(function (e) {
        if (e.keyCode == 13) {
            fireEmail();
        }
    });
    function PostToResponsys() {
        var strEmail = $("#txtEmail").val();
        //var currDateTimeEscaped = $("#hidCurrDateTimeEscaped").val();
        //var currDateTime = $("#hidCurrDateTime").val();
        //var ranno = $("#hidRandomNo").val();

        if (strEmail != "") {
            if (/^([0-9a-zA-Z]([-.\w]*[0-9a-zA-Z])*@([0-9a-zA-Z][-\w]*[0-9a-zA-Z]\.)+[a-zA-Z]{2,9})$/.test(strEmail)) {
                //Change the URL as per requirement - Dont use the same
                var postURL = "https://email.dishemail.com/pub/rf?_ri_=X0Gzc2X%3DWQpglLjHJlTQGkpynHM55MzdiRe5YzceMkJi8RPbReIRVwjpnpgHlpgneHmgJoXX0Gzc2X%3DWQpglLjHJlTQGiYKAJhLj0EPvMupNDdzeUvzggFAX4nf&EMAIL_PERMISSION_STATUS_=I&SOURCE_ACQ=WELCOME&SOURCE_ACQ_DT=";
                var param = "&EMAIL_ADDRESS_=" + strEmail;
                $.ajax({
                    type: "GET",
                    url: '/api/responsysemail/responsysemailservice.asmx/callapi?posturl=' + encodeURIComponent(postURL) + param,
                    success: function (objResponse) {
                        if (objResponse != undefined && objResponse != null && objResponse == "SUCCESS") {
                            console.log("Responsys Log:  CALL SUCCESS");
                            EmailResult("SUCCESS");
                        }
                        else if (objResponse != undefined && objResponse != null && objResponse == "DUPLICATE") {
                            //Duplicate code here
                            console.log("Responsys Log:  DUPLICATE");
                            EmailResult("DUPLICATE");
                        }
                        else {
                            console.log("Responsys Error" + objResponse);
                            EmailResult("ERROR");
                        }
                    },
                    error: function (objError) {
                        console.log("Responsys Error" + objError);
                        EmailResult("ERROR");
                    }
                });
            }
        }
    }
    function EmailResult(postSuccessFull) {
        if (postSuccessFull == "SUCCESS") {
            $("#txtEmail").val("Thank you.");
            createCookieInDays("emailCollected", "true", 3);
            dataLayer.push({
                'event': 'emailSubmit'
            });
            emailTracking('/' + $('#txtEmail').closest('section[id]').attr('id') + '/' + $('#txtEmail').closest('div[id]').attr('id') + '/thanks/');
        }
        else if (postSuccessFull == "DUPLICATE") {
            $("#txtEmail").val("Thank you. You are already subscribed.");
        }
        else {
            removeCookie("emailCollected");
            $("#txtEmail").val("OOPS Something goofed up ....!");
        }
    }
</script>

