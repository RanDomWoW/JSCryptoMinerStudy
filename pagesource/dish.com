<script type="text/javascript">
	$(document).ready(function () {
		var maxCount = 0, changeSlideTimeout, hovering = false;
		// CAROUSEL 

		$("#feat-car-wrap").on('initialized.owl.carousel changed.owl.carousel refreshed.owl.carousel', function (event) {
			if (!event.namespace) return;
			var carousel = event.relatedTarget,
			  element = event.target,
			  current = carousel.current(),
			  index = event.item.index,
			  items = event.item.count;

			//Scroll index changes //
			if (event.type == "changed") {
				$("#carousel-nav > div").removeClass("active2");
				$("#carousel-nav").children().eq(index).addClass("active2");
				
				clearTimeout(changeSlideTimeout);

				if (index === 2) {
					console.log(hovering);
					if (!hovering) {
						changeSlideTimeout = setTimeout(function () {
							changeSlide(0);
						}, 7000);
					}
				}
				changeImage(index);
			}
			owlAutoChange(event, current); // Analytic Tracking
		});
		$(".owl-prev, .owl-next").click(function () {
			var activeDevice = $("#carousel-nav .active2").attr("id");

			if (activeDevice == "nav3" || activeDevice == "nav1") {
				maxCount = maxCount + 1;
			}
			else {
				maxCount = 0;
			}

			if (activeDevice == "nav3" && maxCount > 1) {
				$("#feat-car-wrap").trigger("to.owl.carousel", [0, 1, true]);
				maxCount = 0;
			}
			else if (activeDevice == "nav1" && maxCount > 1) {
				$("#feat-car-wrap").trigger("to.owl.carousel", [2, 1, true]);
				maxCount = 0;
			}

			stopAutoPlay();
		});
		$("#whole--home").hover(function () {
			hovering = true;
			clearTimeout(changeSlideTimeout);
		}, function () {
			hovering = false;
		});

		//CUSTOM CAROUSEL NAVIGATION
		$("#carousel-nav").on("click", ".carouselNav a", function () {
			var $this = $(this), index = $this.parents(".carouselNav").index();

			$("#carousel-nav div").removeClass('active2');
			$this.closest("div").addClass('active2');

			changeSlide(index);
			changeImage(index);
			stopAutoPlay();
			return false;
		});
		function changeSlide(index) {
			$("#feat-car-wrap").trigger("to.owl.carousel", [index, 1, true]);
		}
		function stopAutoPlay() {
			$("#feat-car-wrap").trigger("stop.owl.autoplay");
		}

		var currClass = "hero1";
		var prevClass = "";
		function changeImage(index) {
			$("#whole--home").removeClass(prevClass);

			if (index == 0) {
				currClass = "hero1";
				//	$("#whole--home").css("transition", ".5s ease-in-out");
				$("#jdpower-three-col").css("background", "linear-gradient(#fff , #edf7f8)");

			}
