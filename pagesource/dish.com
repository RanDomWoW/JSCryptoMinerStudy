<script>
    $(function () {
        var eventAttached = false;
        dish_footer();
        $(window).on("resize", function () {
            dish_footer();
        });

        function dish_footer() {
            var winWidth = $(window).width();
            if (winWidth < 768) {
                if (!eventAttached) {
                    $(".toggle").find(".toggle-img").attr("src", "/images/plus-1.png");
                    $(".toggle").first().find(".toggle-img").attr("src", "/images/minus-1.png");
                    $(".toggle-content").hide();
                    $(".toggle-content").first().show();
                }
            }
            else {
                $(".toggle-content").show();
                $("#main-footer-dish").find(".toggle").off("click");
            }
            if (winWidth < 768) {
                if (!eventAttached) {
                    $("#main-footer-dish").find(".toggle").on("click", function () {
                        var $this = $(this),
                            $toggleImage = $this.children(".toggle-img"),
                            $toggleContent = $this.next(".toggle-content");

                        if ($toggleContent.css("display") === "none") {
                            //reset
                            $("#main-footer-dish").find(".toggle-img").attr("src", "/images/plus-1.png");
                            $("#main-footer-dish").find(".toggle-content").hide();

                            //to current item
                            $toggleContent.slideDown();
                            $toggleImage.attr("src", "/images/minus-1.png");
                        }
                        else {
                            $toggleContent.slideUp();
                            $toggleImage.attr("src", "/images/plus-1.png");
                        }
                    });
                    eventAttached = true;
                }

            }
            else {
                $("#main-footer-dish").find(".toggle").off("click");
                eventAttached = false;
            }
        }
    });
</script>
