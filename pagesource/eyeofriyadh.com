						return "Monday"
						}
					if(wdn==1){
						return "Tuesday"
						}
					if(wdn==2){
						return "Wednesday"
						}
					if(wdn==3){
						return "Thursday"
						}
					if(wdn==4){
						return "Friday"
						}
					if(wdn==5){
						return "Saturday"
						}
					if(wdn==6){
						return "Sunday"
						}
	return ""

}

function isnumeric(num)
  {
    var strlen = num.length
    var i
    for ( i = 0; i < strlen ; ++i )
    {
      //if (!((num.charAt(i) >= '0') && (num.charAt(i)<='9') || (num.charAt(i)=='.')))
      if (!((num.charAt(i) >= '0') && (num.charAt(i)<='9') || (num.charAt(i)=='.') || (num.charAt(i)=='-')))
      {
           return false;
      }
    }
  }

