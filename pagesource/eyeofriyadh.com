		        style = style || 'notice';           //<== default style if it's not set
		
		        //create message and show it
					 if(fadingtime){ 
		        $('<div>')
		          .attr('class', style)
		          .html(text)
		          .insertBefore($(msgid))  //<== where ever you want it to show
		         	.fadeIn('slow')
		          	.animate({opacity: 1.0}, fadingtime)     //<== wait 3 sec before fading out
		          	.fadeOut('slow', function()
		          	{
		                $(this).remove();
		          	});
		        }else{
		        	$('<div>')
		          .attr('class', style)
		          .html(text)
		          .insertBefore($(msgid))  //<== where ever you want it to show
		        }
		};

