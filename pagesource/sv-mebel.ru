<script type="text/javascript">
	$(function() {
		function fade(slick, centerIdx)
		{
			var leftIdx = centerIdx - 1,
				rightIdx = centerIdx + 1;

			if (leftIdx < 0)
			{
				leftIdx += slick.$slides.length;
			}

			if (rightIdx > slick.$slides.length - 1)
			{
				rightIdx -= slick.$slides.length;
			}

			/*
			slick.$slides.eq(centerIdx).removeClass('faded');
			slick.$slides.eq(leftIdx).addClass('faded');
			slick.$slides.eq(rightIdx).addClass('faded');
			*/

			//$('.slick-slide').addClass('faded').filter('.slick-center').removeClass('faded');
			/*
			$('.slick-slide[data-slick-index="-1"]').addClass('faded');
			$('.slick-slide[data-slick-index="0"]').removeClass('faded');
			$('.slick-slide[data-slick-index="1"]').addClass('faded');
			*/
		}

		$('.slider').on('init', function(event, slick) {
			fade(slick, 0);

			var $links = slick.$slides.find('a').filter('[href$=".jpg"],[href$=".jpeg"],[href$=".png"]');
			$links.each(function() {
				var $l = $(this);
				var url = $l.attr('style').match(/\((.*?)\)/)[1].replace(/('|")/g,'');
				$l.attr('href', url);
			});

			setTimeout(function () {
				init_fancy($links);
			}, 100);
		}).on('beforeChange', function(event, slick, currentSlide, nextSlide) {
			fade(slick, nextSlide);
		}).slick({
			infinite: true,
			centerMode: true,
			centerPadding: '231px',
			slidesToShow: 1
		});
	});
</script>


