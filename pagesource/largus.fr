	<script type="text/javascript">
		//détection de la visite d'une page "édito" via le path
		var isEditorial = /^\/(actualite-automobile|essais|fiabilite-occasion|francfort|geneve|guide-achat|kiosque|mondial|qualite-fiabilite|retromobile|tokyo)\//.test(location.pathname);
		var isNewsletter = /^\/actualite-automobile\/lettre\//.test(location.pathname);
		if (!isNewsletter && isEditorial) {
			$(function() {
				//Incrémentation du cookie de compte des visites édito
				var nbHitsEdito = docCookies.getItem("HITS_EDITO");
				if (nbHitsEdito == null || nbHitsEdito == "") {
					nbHitsEdito = 1;
				}
				else {
					nbHitsEdito++;
				}
				docCookies.setItem("HITS_EDITO", nbHitsEdito, 60*60*24*365, "/");
				
				//quand on atteind trois, on lance la popin
				if (docCookies.getItem("INSCRIT_NEWSLETTER") != "1" && nbHitsEdito == 2) {
					var $form = $("#newsletterModal form");
					var isFormLocked = false;
					$form.submit(function(event){
						// Stop form from submitting normally
						event.preventDefault();
						if (isFormLocked) {
							return;
						}
						$.ajax({
							type: "POST",
							data: $form.serialize(),
							url:  "//www.largus.fr/V5/Argus/Part/remote/blocs/Editorial.cfc?method=subscribeToNewsletter",
							beforeSend: function( jqXHR, settings ) {
								isFormLocked = true;
							},
							success: function(data, textStatus, jqXHR) {
								isFormLocked = false;
								if (data.success) {
									$form.replaceWith('<div class="confirmation">Merci pour votre inscription!</div>');
									 setTimeout(function() {
									    $('#newsletterModal').modal('hide');
									  }, 1500);
								}
								else {
									for (var n in data.elements) {
										$form.find("[name=" + n + "]").addClass("has-error");
									}
								}
							},
							error: function( jqXHR, textStatus, errorThrown ) {
								isFormLocked = false;
							}
						});
						return false;
					});
					window.setTimeout(function() {$('#newsletterModal').modal();}, 15000);
				}
			});
		}
	</script>
