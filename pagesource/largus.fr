<script type="text/javascript">
	function triggerGaTrackPageview(url,bHash) {
		var urlToSend;
		if ( typeof(url) == 'undefined') {
			urlToSend = location.pathname + traiteHash();
		}
		else {
			if ( typeof(bHash) != 'undefined' && bHash == true ) {
				urlToSend = url + traiteHash();
			}
			else {
				urlToSend = url;
			}
		}
		_gaq.push(['_trackPageview', urlToSend]);
	}

	
	
	function traiteHash( hash ) {
		var v;
		if ( typeof(hash) == 'undefined' ) {
			v = document.location.hash.split("/")[0];
		}
		else {
			v = hash;
		}
		if ( v.match(/#[f|p]{1}[0-9]+/) || (v === "#") ) {
			v = "";
		}
		return v;
	}
	
	
	function trackTcOngletHeader(element, eventLabel, eventAction) {
		try {
			if (eventLabel === undefined || eventLabel === null || eventLabel == "") {
				eventLabel = element.text.replace(/^\s+|\s+$/gm,'');
			}
			if (eventAction === undefined || eventAction === null || eventAction == "") {
				eventAction = "Niveau_1";
			}
			tc_events_3(element,'NAVIGATION',{'EVENT_CATEGORY':'Navigation','EVENT_ACTION':eventAction,'EVENT_LABEL':eventLabel});
		} catch(e){}
		return true;
	}

	
	
	

	jQuery(document).ready(function($) {
		var $html = $("html");
		var $mobileNav = $('#mobile-nav');
		var $mobileNavContainer = $(".list-mobile-nav-container");
		var $btnMobileNav = $('#btn-mobile-nav');
		var $overlayHideNav = $('#hide-mobile-nav');

		// zone de clic (ou touch)  permettant de cacher la nav mobile ou le champ de recherche quand
		// l'utilisateur clique ailleurs que sur le menu, la recherche ou le header (c'est-à-dire sur la page)
		var $btnMobileSearch = $('#btn-mobile-search');
		var $mobileSearch = $('#mobile-search');

		// au clic du bouton recherche situé dans le header à droite, on fait apparaître le champ de recherche
		$btnMobileSearch.click(function() {
			// toggle affichage de la recherche
			$mobileSearch.toggle();
		});

		// montre/cache le menu mobile au clic sur le bouton situé dans le header à gauche
		$btnMobileNav.click(function(event) {
			if ("history" in window) {
				var currentState = window.history.state;
				if (currentState != null && typeof currentState === "object" && "dummyState" in currentState && currentState.dummyState == "drawer") {
					$html.toggleClass("mobile-nav-open", false);
					window.history.back();
					return;
				}
				else {
					var newState = {
						"dummyState": "drawer"
					};
					$html.toggleClass("mobile-nav-open", true);
					window.history.pushState(newState, null, null);
					return;
				}
			}
			$html.toggleClass("mobile-nav-open");
		});
		
		// cache le menu mobile au clic
		$overlayHideNav.click(function(event) {
			event.preventDefault();
			if ("history" in window) {
				var currentState = window.history.state;
				if (currentState != null && typeof currentState === "object" && "dummyState" in currentState && currentState.dummyState == "drawer") {
					window.history.back();
				}
			}
			$html.toggleClass("mobile-nav-open", false);
		});

		// Quand l'utilisateur clique sur précédent/suivant
		$(window).bind('popstate', function(event) {
			var currentState = window.history.state;
			if (currentState != null && typeof currentState === "object" && "dummyState" in currentState && currentState.dummyState == "drawer") {
				// Retour à un une entrée où le menu était ouvert: on le réouvre
				$html.toggleClass("mobile-nav-open", true);
			}
			else {
				if ($html.hasClass("mobile-nav-open")) {
					$html.toggleClass("mobile-nav-open", false);
				}
			}
		});
		
		// Rechargement d'une page
		if ("history" in window) {
			var currentState = window.history.state;
			if (currentState != null && typeof currentState === "object" && "dummyState" in currentState && currentState.dummyState == "drawer") {
				// Le menu était ouvert: on le réouvre
				$html.toggleClass("mobile-nav-open", true);
			}
		}

	});
</script>

