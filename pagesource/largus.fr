		<script type="text/javascript">
			function acceptCookies() {
				//On détermine le domaine de base -> "www.largus.fr" donnera ".largus.fr"
				var hostname = location.hostname;
				var reHost = /^([a-z][a-z0-9\-]*\.)*([a-z][a-z0-9\-]*\.[a-z][a-z0-9\-]*)$/;
				var res = reHost.exec(hostname);
				var hostCookie;
				if (res == null) {
					hostCookie = hostname;
				}
				else {
					hostCookie = "." + res[2];
				}
				var maxAge = 345600000; //400 jours. Selon la CNIL : "La durée de validité de ce consentement est de 13 mois maximum." -> 
				document.cookie = encodeURIComponent("COOKIES-ACCEPTED") + "=1; max-age=" + maxAge + "; domain=" + hostCookie + "; path=/";
				
				var oCookieBar = document.getElementById("cookie-bar");
				oCookieBar.style.display = "none";
			}
			(function() {
				var cookieValue = decodeURIComponent(document.cookie.replace(new RegExp("(?:(?:^|.*;)\\s*" + encodeURIComponent("COOKIES-ACCEPTED").replace(/[\-\.\+\*]/g, "\\$&") + "\\s*\\=\\s*([^;]*).*$)|^.*$"), "$1")); 
				if (cookieValue == 1) {
					var oCookieBar = document.getElementById("cookie-bar");
					oCookieBar.style.display = "none";
				}
				else {
					var oButton = document.getElementById("cookie-bar-button-ok");
					oButton.style.display = "inline-block";
				}
			})();
		</script>
	
		
	
