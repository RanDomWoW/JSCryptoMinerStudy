			function refreshFamillesHome() {
				var $form = $('form[name=rechercheDossiersQF]');
				var newIdMarque = $form.find("[name=marque]").val();
				var autorisation = $.trim($form.attr('data-autorisationmodele'));
				var $familleSel = $form.find("[name=famille]");
				if (newIdMarque.length == 0 || newIdMarque == 0) {
					populateFamilleWith($familleSel,null);
				}
				else {
					$.getJSON(
						'/v4/proxy/particulier/guideAchat/FicheOccasion.cfc',
						{
							method : "getFamilles",
							marqueID : newIdMarque,
							autorisation : autorisation
						},
						function(recordset) {
							populateFamilleHomeWith($familleSel,recordset);
						}
					);
				}
			}
			
			function populateFamilleHomeWith($select,data) {
				var $noFamille = $select.find('*[value=]');
				$select.empty();
				$select.append($noFamille);
				if (data != null) {
					for(var i = 0; i < data.recordcount; i++) {
						if (String(data.data.famillelib[i]).length > 0) {
							var sSelected = "";
							if (data.recordcount == 1 ) {
								sSelected = "selected='selected'";
							}
							var newOption = "<option value='" + data.data.famillecode[i] + "' " + sSelected + ">" + data.data.famillelib[i] + "</option>";
							$select.append(newOption);
						}
					}
				}
			}
		</script>
