<script type="text/javascript">
function compareNumbers(a, b) { return a - b; }
var manualUploader = new qq.FineUploader({
element: document.getElementById('qq-manual-trigger'),
template: 'qq-template-manual-trigger',
request: { endpoint: '//storage.vfl.ru/qqep.php' },
thumbnails: {
placeholders: {
waitingPath: '/jfyuruig3/images/waiting-generic.png',
notAvailablePath: '/jfyuruig3/images/not_available-generic.png'
}
},
autoUpload: false,
chunking: {
enabled: true,
concurrent: {
enabled: true
},
success: {
endpoint: "//storage.vfl.ru/qqep.php?done"
}
},
cors: {
expected: true,
sendCredentials: true
},
callbacks: {
onStatusChange: function() {
if (manualUploader.getUploads({status: qq.status.SUBMITTED}).length>0) {
document.getElementById("trigger-upload").disabled=0;
document.getElementById("trigger-upload").className='ctrigger-upload';
}
else {
document.getElementById("trigger-upload").disabled=1;
document.getElementById("trigger-upload").className='dtrigger-upload';
}
},
onAllComplete: function(successful, failed) {
successful.sort(compareNumbers);
for (i=0; i<successful.length; i++) {
addinput('MultiPowUploadFileName_'+i,manualUploader.getName(successful[i]));
addinput('MultiPowUploadFileId_'+i,successful[i]);
addinput('qqUuid_'+i,manualUploader.getUuid(successful[i]));
if (manualUploader.getFile(successful[i]).lastModified) addinput('MultiPowUploadFileDate_'+i,manualUploader.getFile(successful[i]).lastModified);
else if(manualUploader.getFile(successful[i]).lastModifiedData) addinput('MultiPowUploadFileDate_'+i,new Date(manualUploader.getFile(successful[i]).lastModifiedDate).getTime());
}
document.getElementById('vflform').submit();
}
}
});
qq(document.getElementById("trigger-upload")).attach("click", function() {
manualUploader.uploadStoredFiles();
});
document.getElementById("trigger-upload").disabled=1;
</script>
