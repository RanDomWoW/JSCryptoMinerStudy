$(function(){
    slidePicCount = $("#banner_list div.divList").length;
    var circleDotDom = "";
    for(var i=0; i < slidePicCount; i++){
        if(i==0){
            circleDotDom += "<li class=\"on\">" + (i+1) + "</li>";
        }else{
            circleDotDom += "<li>" + (i+1) + "</li>";
        }
    }
    $(".circleDot").html(circleDotDom);
    $("#banner_list div.divList:not(:first-child)").hide();
    $("#newTJContSlide02 li").click(function() {
        var i = $(this).index();//获取li在同辈元素中的索引位置
        slidePicIndex = i;
        if (i >= slidePicCount) return;
        $("#banner_list div.divList").filter(":visible").fadeOut(0);//隐藏图片
        $("#banner_list div.divList").eq(i).fadeIn(0);//显示图片
        $(this).addClass("on");
        $(this).siblings().removeClass("on");
    });
});



</script>




