<script>




  var popImg = '';

  var popImg_w, popImg_h;
  function showAIs() {
    if(popImg!="") {
      var str, i;

      str = '<div id="div_ai_show" style="position:absolute; z-index:1000;">'
          + '<div style="position:absolute; z-index:10;"><div id="div_ai_cc" style="left:270px; top:6px; position:relative;"><img src="http://pic.gooooal.com/userCenter/images/ico_cross.gif" style="cursor:pointer;" onclick="hideAIs();" title="关闭"></div></div>';
      for(i=1; i<6; i++)
        str += '<div style="position:absolute; z-index:'+(i-8)+';"><div id="div_ai_bb_'+i+'" style="position:relative; background-color:#666666; '+(document.all?'filter:alpha(opacity=20);':'opacity:0.2;')+' left:'+i+'px; top:'+i+'px;"></div></div>';
      str += '<div id="div_ai_img">'+popImg+'</div></div>';

      document.write(str);
      window.setTimeout(setAIsWH, 2000);
    }
  }
  function setAIsWH() {
    var div = document.getElementById("div_ai_img");
    if(div) {
      popImg_w = div.scrollWidth;
      popImg_h = div.scrollHeight;
      if(popImg_h==0) popImg_h = 160;
      window.onscroll = setAIsp;
      window.onresize = setAIsp;
    }
    div = document.getElementById("div_ai_cc");
    if(div) div.style.left = (popImg_w-24)+"px";
    for(var i=1; i<6; i++) {
      div = document.getElementById("div_ai_bb_"+i);
      if(div) {
        div.style.width = (popImg_w)+"px";
        div.style.height = (popImg_h)+"px";
      }
    }
    div = document.getElementById("div_ai_show");
    if(div) {
      div.style.left = ((typeof(window.pageXOffset)!='undefined'?window.pageXOffset:document.documentElement.scrollLeft) + document.documentElement.clientWidth - popImg_w - 30) + "px";
      div.style.top = ((typeof(window.pageYOffset)!='undefined'?window.pageYOffset:document.documentElement.scrollTop) + document.documentElement.clientHeight) + "px";
    }
    setAIsp1();
  }
  function setAIsp1() {
    var div = document.getElementById("div_ai_show");
    if(div) {
      if(div.offsetTop > (typeof(window.pageYOffset)!='undefined'?window.pageYOffset:document.documentElement.scrollTop)+document.documentElement.clientHeight-popImg_h-15) {
        div.style.top = (div.offsetTop - 5) + "px";
        window.setTimeout(setAIsp1, 10);
      }
    }
  }
  function setAIsp() {
    var div = document.getElementById("div_ai_show");
    if(div) {
      div.style.left = ((typeof(window.pageXOffset)!='undefined'?window.pageXOffset:document.documentElement.scrollLeft) + document.documentElement.clientWidth - popImg_w - 30) + "px";
      div.style.top = ((typeof(window.pageYOffset)!='undefined'?window.pageYOffset:document.documentElement.scrollTop) + document.documentElement.clientHeight - popImg_h - 15) + "px";
    }
  }
  function hideAIs() {
    var div = document.getElementById("div_ai_show");
    if(div) {
      if(div.offsetTop < (typeof(window.pageYOffset)!='undefined'?window.pageYOffset:document.documentElement.scrollTop)+document.documentElement.clientHeight) {
        div.style.top = (div.offsetTop + 10) + "px";
        window.setTimeout(hideAIs, 10);
      } else {
          div.style.display = "none";
      }
    }
  }
  showAIs();
