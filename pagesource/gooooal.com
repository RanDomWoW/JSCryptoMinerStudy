    <script>
    var indexPicBan = 0, indexPicBcont;
    indexPicBcont = $("#indexBanner_list a").length;
	var indexcircleDotDom = "";
	for(var i=0; i < indexPicBcont; i++){
		if(i==0){
            indexcircleDotDom += "<li class=\"on\">" + (i+1) + "</li>";
		}else{
            indexcircleDotDom += "<li>" + (i+1) + "</li>";
		}
	}
	$(".indexcircleDot").html(indexcircleDotDom);
	$("#indexBanner_list a:not(:first-child)").hide();
	$("#indexBanner li").click(function() {
		var i = $(this).index();
        indexPicBan = i;
		if (i >= indexPicBcont) return;
		$("#indexBanner_list a").filter(":visible").fadeOut(500);
		$("#indexBanner_list a").eq(i).fadeIn(600);
		$(this).addClass("on");
		$(this).siblings().removeClass("on");
	});
	var t = setInterval(function () {
        indexPicBan = indexPicBan >=(indexPicBcont - 1) ? 0 : ++indexPicBan;
        $(".indexcircleDot li").eq(indexPicBan).trigger('click');
    }, 4000);
	$("#banner").hover(
		function(){
			clearInterval(t);
		}, 
		function(){
			t = setInterval(function () {
                indexPicBan = indexPicBan >=(indexPicBcont - 1) ? 0 : ++indexPicBan;
                $(".indexcircleDot li").eq(indexPicBan).trigger('click');
            }, 4000);
		}
	);
    </script>
