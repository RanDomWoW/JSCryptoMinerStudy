            $("#container").css({"height":con_h+"px"});
            $("#container").find(".web").css({"height":"100%"});
        }else{
        
        }
        
        
      
        $(window).resize(function(){
            var win_w = $(window).width();
            var win_h = $(window).height();
            var foot_h = $("#footer").height();
            var choice_box_w = $(".choice_box").width()-50;
            var img_btn_w = $(".img_btn").width();
            var con_h = win_h - foot_h +2;
			
            if(win_h < 850){
                $("#footer").css({"position":"relative"});
                $("#container").css({"height":"auto"});
                var cont_h = $("#container").css({"height":"auto"});
                $("#container").find(".web").css({"height":cont_h+"px"});
            }
			
			if(1023 < win_w){
             $("#footer").css({'display':'fixed'});
				$("#container").css({"height":con_h+"px"});
            $("#container").find(".web").css({"height":"100%"});
        }else{
        
        }
            
            if(1023 < win_w && win_h < 1024){
				$(".foot").hide();
                $("#footer").css({"height":"225px;"});
                $(".choice_box").css({"height":"172px"});
                $("#container").find(".web").css({"height":con_h+"px"});
            }else{
            	$("#container").find(".web").css({"height":con_h+"px"});
            }
            
            if(win_h < 768 && win_w < 768 ){
                $("#container").css({"height":"auto"});
                var cont_h = $("#container").css({"height":"auto"});
                $("#container").find(".web").css({"height":cont_h+"px"});
            }
			 if( win_w < 1023 ){
                $(".foot").show();
            }
			

        });
        
		// footer 이벤트    
 		var prevPosition = 0;
		$(window).on('scroll', function(event){
			var initPosition = $(this).scrollTop();
			if(1023 < win_w && win_h < 1024){
				$(".foot").show();
			prevPosition = initPosition
			}
		});
 		// footer 이벤트 e 

    });

    </script>
