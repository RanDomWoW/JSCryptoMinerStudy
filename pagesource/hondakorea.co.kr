    <script>
    $(document).ready(function(){
    	
        if($("#outer").val()=='mail'){
        	fnConfirmBoxOpenPop("뉴스레터 수신을 원하지 않으신다면 해당내용을 입력해주세요.", updateNewsletter);
        }
else if($("#outer").val()=='character'){
        	fnMsgBoxOpenPop("혼다코리아 사이트는 URL에 특수문자 입력을 금지합니다.");
        }        $("#email2").prop("readonly", true);
        $("#email3").val($("#email2").val());
        if($("#email3").val() == "" && $("#email2").val() != "") {
            $("#email3").val('direct');     
        }
        if($("#email3").val() == 'direct') {
            $("#email2").prop("readonly", false);
        }
        
        $(document).on('change', '#email3', function() {
            if($("#email3").val() == 'direct') {
                $("#email2").val("");
                $("#email2").prop("readonly", false);
            } else {
                $("#email2").val($("#email3").val());
                $("#email2").prop("readonly", true);
            }
        });
        
    });

    function updateNewsletter(){
    	var userName = $("#user_name").val();
    	var email = $("#email1").val();
        var emailAddr = $("#email2").val();
        if(userName==''){
        	alert("사용자 명을 입력해주세요.");
        	location.reload();
        	return false;
        }
        if(email=='' || emailAddr==''){
        	alert("이메일 주소를 입력해주세요.");
        	location.reload();
        	return false;
        }
        $.ajax({
            url: "/comm/updateNewsletter.json",
            type: "POST",
            data: JSON.stringify({w_email_1: email ,w_email_2: emailAddr}),
            dataType: "json",
            contentType: "application/json; charset=UTF-8",
            headers: {"Accept": "application/json"},
            success: function(data, textStatus, jqXHR) {
                    alert("뉴스레터 수신이 취소되었습니다.");
            },
            error: function(jqXHR, textStatus, errorThrown) {
                alert("뉴스레터 수신이 취소가 실패하였습니다.");
            }
        });
    }
		
    $(function(){
