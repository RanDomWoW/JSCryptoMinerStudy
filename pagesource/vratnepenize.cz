    <script>
        var vterina = 1000;
        var minuta = vterina * 60;
        var hodina = minuta * 60;
        var den = hodina * 24;
        var rok = den * 365.24219;

        var slova = {
            roku: ["rok", "roky", "let"],
            dnu: ["den", "dny", "dnů"],
            hodin: ["hodina", "hodiny", "hodin"],
            minut: ["minuta", "minuty", "minut"],
            vterin: ["vteřina", "vteřiny", "vteřin"]
        };

        function sklonovani(pocet, co) {
            if (pocet == 1) return slova[co][0];
            if (pocet < 5 && pocet > 0) return slova[co][1];
            return slova[co][2];
        }

        function odpocet(el) {
            if (el != null)
            var konec = el.getAttribute("data-konec");
        
     // alert(konec);
            var arr = konec.split(' ');

            konec = new Date(arr[0] + "T" + arr[1] + "Z");
            var konec = konec.getTime();
//            alert(konec);


            var ted = new Date();
            ted = ted.getTime();
//            var intZ = new Date("Z").getMilliseconds();

            var rozdil = konec - ted - 3600000;

//            alert (rozdil);

            if (rozdil < vterina) {
                if ( document.getElementById('popup_obal') !== null )
                    document.getElementById('popup_obal').style.display = document.getElementById('popup_stin').style.display = 'none';
                if ( document.getElementById('reminder') !== null )
                    document.getElementById('reminder').style.display = 'none';
                if ( document.getElementById('odpocet') !== null )
                    document.getElementById('odpocet').style.display = 'none';
                if ( document.getElementById('hide-zavorka') !== null )
                    document.getElementById('hide-zavorka').style.display = 'none';
//                if ( document.getElementById('show-sentence-rem') !== null && !window.location.href.includes("rozsireni"))
//                    document.getElementById('show-sentence-rem').style.display = 'inline';
                if ( document.getElementById('header-bg') !== null )
                    document.getElementById('header-bg').style.height = '90px';
                if ( document.getElementById('header-outer') !== null )
                    document.getElementById('header-outer').style.height = '90px';
//                if (window.location.href.includes("registrace") && document.getElementById('odpocet2') !== null ) {
//                    document.getElementById('odpocet2').style.display = 'none';
//                } else {
                    document.getElementById('odpocet2').innerHTML = document.getElementById('odpocet2').getAttribute("odpocet2defaultText");
//                }
                return;
            }
            var zbyva = {
                roku: Math.floor(rozdil / rok),
                dnu: Math.floor(rozdil % rok / den),
                hodin: Math.floor(rozdil % den / hodina),
                minut: Math.floor((rozdil % hodina) / minuta),
                vterin: Math.floor((rozdil % minuta) / vterina)
            }


            var vypis = el.getAttribute("data-zbyva");
//            var vypis = document.getElementById('vypis').value;
            var exist = document.getElementById('hide-zavorka');

            for (co in zbyva) {
                var pocet = zbyva[co];
                if (pocet > 0) {


//                        document.getElementById('hide-zavorka').style.display='inline';


                    vypis += " " + pocet + " " + sklonovani(pocet, co);
                }

            }
            if (el.getAttribute("id") === "odpocet2" && !(window.location.href.indexOf("registrace") > -1)) {
                vypis += ")";
            }
            el.innerHTML = vypis;
            setTimeout(function () {
                odpocet(el);
            }, vterina);
        }
    </script>



