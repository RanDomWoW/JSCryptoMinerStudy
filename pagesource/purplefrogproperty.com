<script type="text/javascript">
	jQuery(function() {
		$('.slider').noUiSlider({
			start: [50,195],
			step: 1,
			connect: true,
			range: {
				'min': 50,
				'max': 195			}
		}).on({
			slide: function(){
				vals = $(this).val();
				$('#search .from').text(Math.round(vals[0]));
				$('#search .to').text(Math.round(vals[1]));

				$('#price-from').val(Math.round(vals[0]));
				$('#price-to').val(Math.round(vals[1]));
			},
			change: function(){
				changing_el = 'slider';
				params = {
					area : $("#search select[name='area']").val(),
					beds : $("#search select[name='beds']").val(),
					price: {
						from : $('#price-from').val(),
						to : $('#price-to').val(),
					},
					type : $('#search select[name="type"]').val(),
					year : $('#search select[name="year"]').val()
				}

				request_stats(params,changing_el);
			}
		});

   		$(".change-search").change(function() {
			changing_el = $(this).attr('name');

			if($(this).hasClass('area-search')){
				$(this).closest('#property_search').attr('action', '/student-accommodation/'+$(this).val()+'/');
			}

			beds = $("#search select[name='beds']").val();

			params = {
				area : $("#search select.area-search").val(),
				beds : beds,
				price: {
					from : $('#price-from').val(),
					to : $('#price-to').val(),
				},
				type : $('#search select[name="type"]').val(),
				year : $('#search select[name="year"]').val()
			}

			if(changing_el=="type" || changing_el=="year"){
				params = {
					area : $("#search select.area-search").val(),
					beds : beds,
					type : $('#search select[name="type"]').val(),
					year : $('#search select[name="year"]').val()
				}
			}

			request_stats(params,changing_el);
   		});

		function request_stats(params, changing_el){
			last_from_price = $('#search input[name="from"]').val();
			last_to_price = $('#search input[name="to"]').val();

			$.getJSON( "/student-accommodation/search/get_data", params, function( data ) {
				options = {
					start: [data['price']['min'] || 0, data['price']['max'] || 0 ],
					range: {
						'min': data['price']['min'] || 0 ,
						'max': data['price']['max'] || 0 
					}
				}

				if(data['price']['count']<=1 || (last_from_price==0 && last_to_price==0) ){
					options['start'] = [data['price']['min'], data['price']['max']]
				}

				if( (changing_el!="slider" && (data['price']['min']!==data['price']['max']))){
					$('.slider').noUiSlider(options, true);
					//$('#search #price-from').val(Math.floor(data['price']['min']));
					//$('#search #price-to').val(Math.ceil(data['price']['max']));
				}

				if(changing_el!="slider"){
					$('#search .from').text(Math.floor(data['price']['min']));
					$('#search .to').text(Math.ceil(data['price']['max']));
				}

				if(changing_el=="type" || changing_el=="year"){
					options['start'] = [data['price']['min'], data['price']['max']];
					options['range'] = {
						'min': Math.floor(data['price']['min']),
						'max': Math.ceil(data['price']['max'])
					}
					$('.slider').noUiSlider(options, true);
					$('#search #price-from').val(Math.floor(data['price']['min']));
					$('#search #price-to').val(Math.ceil(data['price']['max']));
					$('#search .from').text(Math.floor(data['price']['min']));
					$('#search .to').text(Math.ceil(data['price']['max']));
				}

				count_text = data['price']['count'] == 1 ? "property" : "properties"

				$('#search .count').text(data['price']['count'] + " " + count_text + " found");
			});
		}
	});
</script>			</div>
