<script>
$(document).on("keyup", ".inputTxt", function(){
    var keyVal = $(this).val();
    if(keyVal.charAt(0) == 'A' || keyVal.charAt(0) == 'a')
    {
        $(this).val('A');
    }
});

$(document).on("keypress", ".inputTxt",function(key){
	if(key.which == 13)
	{
		key.preventDefault();
	}
});
</script><script>
$( document ).ready(function() {
    $( document ).on('submit', '#login_form', function(event) {
        
        $.ajax({
            url: "https://webscte.co.in/user/loginProcess",
            type: "POST",                
            data:  $('#login_form').serialize(),
            dataType: "json",
            beforeSend: function() {
                $('.alert-error').remove();
            },
            success: function(data) {
                var msg = "<p>" + data.error_msg + "</p>";
                if(data.error)
                {
                    $('#login_form').before('<div class="alert-error">'+msg+'</div>'); 
                    // $( "#login_form" ).prev().delay(2000).fadeOut(500);                  
                }
                else
                {
                    $('.alert-error').remove();
                    $('#login_form').before('<div class="alert-success">'+msg+'</div>');
                    // $( "#login_form" ).prev().delay(2000).fadeOut(500);
                    if(data.redirect)
                    {
                        window.setTimeout(function(){
                            window.location.href = "https://webscte.co.in/teacher/home";
                        }, 1000);
                    }
                    else
                    {
                        window.setTimeout(function(){
                            window.location.href = "https://webscte.co.in/collage/home";
                        }, 1000);
                    }                    
                }                
            },
            complete: function() {},
            error: function() {}
        });

        event.preventDefault();

    });
});

