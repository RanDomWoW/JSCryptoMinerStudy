<script>
function goalDay(date){
	var digit = 2;digit=digit*(-1)
	var goal = new Date("9/25/2015");
	if((goal-date) <= 0) {
		var daysStr="00";
	}else{
		var daysStr = ("00"+(Math.floor((goal-date)/1000/60/60/24)+1)).slice(digit);
	}
	var str="";
	for(var i=0; i<daysStr.length; i++){
		str+='<img src="/img/num/count_num'+ daysStr.charAt(i) +'.png" alt="0" width="70" height="80">';
	}
	$("div#countDown").html(str);
	return;
}

