.slider2{
  width: 100%; /*Same as width of the large image*/
  position: relative;
  /*Instead of height we will use padding*/
  margin: 0 auto;
}
/*Last thing remaining is to add transitions*/
.slider2>a>img{
  width:100%;
  position: absolute;
  left: 0; top: 0;
  transition: all 0.5s;
  z-index:0;
}

.slider2 input[name='slide_switch'] {
  display: none;
}
.slider2 label {
  /*Lets add some spacing for the thumbnails*/
  margin: 5px 0 0 0;
  border: 3px solid #999;
  float: left;
  cursor: pointer;
  transition: all 0.5s;
  /*Default style = low opacity*/
  opacity: 0.6;
  width: 16.5%;
}
.slider2 label img{
  display: block;
}
/*Time to add the click effects*/
.slider2 input[name='slide_switch']:checked+label {
  border-color: #666;
  opacity: 1;
}
/*Clicking any thumbnail now should change its opacity(style)*/
/*Time to work on the main images*/
.slider2 input[name='slide_switch'] ~ a>img {
  opacity: 0;
  transform: scale(1.1);
  z-index:-1;
}
/*That hides all main images at a 110% size
On click the images will be displayed at normal size to complete the effect
*/
.slider2 input[name='slide_switch']:checked+label+a>img {
  opacity: 1;
  transform: scale(1);
  z-index:1;
}
/*Clicking on any thumbnail now should activate the image related to it*/

/*Mismo codigo pero para iframes*/
.slider2>iframe{
  width:100%;
  position: absolute;
  left: 0; top: 0;
  transition: all 0.5s;
  z-index:0;
  height:100%;
}

.slider2 input[name='slide_switch'] ~ iframe {
  opacity: 0;
  transform: scale(1.1);
  z-index:-1;
}
.slider2 input[name='slide_switch']:checked+label+iframe {
  opacity: 1;
  transform: scale(1);
  z-index:1;
}



