if (document.URL === 'https://avanceytec.com.mx/') {

    checkCookie();

function checkCookie() {
    var user=getCookie("username");

    if (user == "") {
        //Si la cookie no existe entonces mostrar pop up
        function popBoletin() {$.colorbox({iframe:true, href:"//app.getresponse.com/site/avance/webform.html?wid=13310005&amp;u=3f0v&amp;mg_param3=4", open:true, width:"825px", height:"95%"});}setTimeout(popBoletin, 5000);                                                                                                                      
        //Y crear cookie
        var date = new Date();
        date.setTime(date.getTime()+(12*60*60*1000));
        var expires = " expires="+date.toGMTString();

        document.cookie = "username=no;"+expires;
    }
}

function getCookie(cname) {
    var name = cname + "=";
    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(';');
    for(var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}

