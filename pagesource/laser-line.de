					{
						if ($active == null)
						{
							$elem.hide();
						} else {
							$('#megamenu-html-source .megamenu').not($elem).hide();
							$elem.show();
						}
					};
                      //                  $elem.show();
                    					$link.on('mouseover', function () {
						$active = $elem;
						check_menu();
					}).on('mouseout', function () {
						$active = null;
						window.setTimeout(check_menu, delay);
					});
					$elem.on('mouseover', function () {
						$active = $elem;
						check_menu();
					}).on('mouseout', function () {
						$active = null;
						window.setTimeout(check_menu, delay);
					});
