function applyModuleTimer() {
    // Timing logic for controling headmodule visiblity - Start
    $('.boe_module:not(.boe_header_user_memo)').mouseleave(function() {
        var time = 100;
        if($(this).is("[data-time]")){
            time = $(this).attr('data-time');
        }
        moduleTimer = setTimeout(function() {
            hideHeadModule();
        }, time);
    });

    $('.boe_module').mouseenter(function() {
        clearTimeout(moduleTimer);
    });
    // Timing logic for controling headmodule visiblity - End
}

