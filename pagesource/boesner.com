function loadModuleContent(module,target,formId) {
    showLoaderOverlay();

    var data = jQuery('#' + formId).serialize();
    $.ajax({
        type: 'POST',
        url: '/boesner/responder/moduleContent.dojo?module=' + module,
        data: data,
        success: function(content) {
            $('#' + target).empty();
            $('#' + target).append(content);
            hideLoaderOverlay();
            applyModuleTimer();
        }
    });
}

