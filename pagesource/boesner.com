    var currency = "€";
    var maxPrice = 15977.25;

    function formatPrice(price) {
        if (price == undefined || price == null) {
            return "egal";
        }
        if (price == 15977.25) {
            return "egal";
        }
        if((""+price).replace(/,/, "").replace(/\./, "") == ("15977.25").replace(/,/, "").replace(/\./, "")) {
            return "egal";
        }
        strPrice = parseFloat(price).toFixed(2);
        strPrice = strPrice.replace(/,/, "x");
        strPrice = strPrice.replace(/\./, ",");
        strPrice = strPrice.replace(/x/, ".");
        strPrice = strPrice + " " + currency;
        return strPrice;
    }
    function getPriceValue (price) {
        if(price!=undefined && price!=null && price!='') {
        if(price == "egal") {
            return 15977.25;
        }
        strPrice = price.replace(/ /, "");
        strPrice = strPrice.replace(new RegExp(currency), "");
        var ppunkt = strPrice.indexOf(".");
        var pkomma =strPrice.indexOf(",");
        if (ppunkt > 0 && pkomma > 0){
            if (ppunkt > pkomma){
                //Punkt hinten
                strPrice = price.replace(/,/, "");
            } else {
                //Komma hinten
                strPrice = price.replace(/\./, "");
            }
        }
        strPrice = strPrice.replace(/,/, ".");
        } else {
            strPrice = "0";
        }
        if (isNaN(strPrice)) {
            strPrice = "0";
        }
        dPrice = parseFloat(strPrice);
        if(dPrice < 0) {
            dPrice *= -1;
        }
        return dPrice;
    }
