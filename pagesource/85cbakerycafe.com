<script>
jQuery(document).ready(
function(){
	jQuery('#mob-search #searchsubmit').click(function(){
		//if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
			$("#mob-search #s").toggleClass("active");return false;
		//}
	});
	/***************************************************/
	if(jQuery(".milestones-main").length > 0){
		jQuery(".milestones-main .swiper-slide:first-child").trigger("click");
	}
	/***************************************************/
	jQuery(".menu-pop-link").click(function(){
		jQuery("body").addClass("overflow-body");
		jQuery(".popup-warp").hide();
		jQuery("#menu-popup").fadeIn();
		
		$('html, body').css('overflowY', 'hidden'); 
		return false;	
	});
	jQuery(".cloes-btn").click(function(){
		jQuery(".popup-warp ").fadeOut();
		jQuery("body").removeClass("overflow-body");
		//$('html, body').css('overflowY', 'auto'); 
	});
	/***************************************************/
	if(jQuery(".milestones-main").length > 0){
		var swiper = new Swiper('.milestones-main .swiper-container', {
			speed: 1000,
			pagination: '.milestones-main .swiper-pagination',
			slidesPerView: 'auto',
			centeredSlides: true,
			paginationClickable: true,
			spaceBetween: 30,
			keyboardControl:true,
			resistance:false
		});
		jQuery(".milestones-main .swiper-slide").click(function(){
			var index = $(this).index();
			jQuery(".milestones-main .swiper-pagination-bullet").eq(index).trigger("click");
		});
	}
	/***************************************************/
	
	
	$(".phone-info").click(function(){
		$(".download-app-pop").fadeToggle();
		
	});
	if ($('#wpsl-search-input').val()) {
			
			// set the default value of a text input field
			setTimeout(function(){
					$('#wpsl-search-btn').click();
			},2000);
	}
	}
);
</script>
