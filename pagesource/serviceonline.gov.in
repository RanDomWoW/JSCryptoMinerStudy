					     }]
					    
						});
					return true;	
			}else if(count == 2){
				
				pieHighChartPaidStateWise(stateCode,jsonArray);
			}else if(count == 3){
				pieHighChartPaidFreeService(stateCode,jsonArray);
			}

}




