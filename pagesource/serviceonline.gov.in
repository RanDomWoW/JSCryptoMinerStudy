    function addCommas(nStr)
    {
        nStr += '';
        x = nStr.split('.');
        x1 = x[0];
        x2 = x.length > 1 ? '.' + x[1] : '';
        var rgx = /(\d+)(\d{3})/;
        while (rgx.test(x1)) {
            x1 = x1.replace(rgx, '$1' + ',' + '$2');
        }
        return x1 + x2;
    }
    
    function applicationCounterAll(count, number){
    	$('.application-wrappern'+count).empty();
    	
    	
    	
    	var newn=addCommas(number);
    	$(".application-wrappern"+count).append('<span style="cursor: pointer" class="app-digit-con"><span  class="inlineBlock">'+newn+'</span></span>');	
    	
    	$(".inlineBlock:nth-last-child(3n+4)").after("<span class='comma'>,</a>");
    }
    
    function serviceCounter(number){
    	$('.service-counter-wrapper').empty();
    	if(number != null)
        	var numArray = number.toString().split("");
        	else
        	var numArray = "";
    	for(var i=0; i<numArray.length; i++) { 
    		numArray[i] = parseInt(numArray[i], 10);
    		$(".service-counter-wrapper").append('<span style="cursor: pointer" class="serv-digit-con"><span class="serv-digit'+i+'">0<br>1<br>2<br>3<br>4<br>5<br>6<br>7<br>8<br>9<br></span></span>');	
    	}
    	var increment = $('.serv-digit-con').outerHeight();
    	var speed = 100;
    	for(var i=0; i<numArray.length; i++) {
    		$('.serv-digit'+i).animate({top: -(increment * numArray[i])}, speed);
    	}
    	$(".serv-digit-con:nth-last-child(3n+4)").after("<span class='comma'>,</a>");
    }
    
