    function changeFontSize(element,step){
    	step = parseInt(step,10);
    	var el = $('p,a:not("#eNag")');
    	$.each(el,function(){
	    	var curFont = parseInt($(this).css('font-size'));
	    	$(this).css('font-size',(curFont+step)+'px');
    	});
    }
			
