<script type='text/javascript'>
function pieHighChart(count,stateCode,jsonArray) {
	
	
		
		
		if(count==0)
			{	
		
	var json = JSON.parse(jsonArray);
							var processed_json = new Array();
							$.map(json, function(obj, i) {
								processed_json.push([obj.name, parseInt(obj.count)]);
							});
			
		$('#container'+stateCode).highcharts({
					chart: {
						marginRight: 30,
			          marginTop:0,
			          reflow: true,
			          type: 'pie',
			          options3d: {
			              enabled: true,
			              alpha: 50,
			              beta: 0
			          }
			      },
			      
			subtitle: {
			    text: 'Application Status Percentage Wise'
			},
			      credits: {
			          enabled: false
			      },
			      exporting: {
			          enabled: false
			 			},
			      tooltip: {
			          pointFormat: '{series.name}:<b>{point.percentage:.2f}%</b>:<br/>Total Applications<b>{point.y}</b>'
			      },
			      plotOptions: {
			          pie: {
			              allowPointSelect: true,
			              cursor: 'pointer',
			              depth: 35,
			              dataLabels: {
			                  enabled: true,
			                  format: '{point.name}'
			              }
			          }
			      },
			      series: [{
			          type: 'pie',
			          name: 'Percentage',
			          data: processed_json              
			         
			      }]
			     
				});
			return true;	
			}
			else if(count == 1)
			{
				$(".customMap").hide();  // priyanka-----
				$(".inner_container #container0").css("display","block"); // priyanka-----
				
				 
				var stateJson = JSON.parse(jsonArray);
						var processed_json = new Array();
						$.map(stateJson, function(obj, i) {
							processed_json.push([obj.stateName, parseInt(obj.totalApp)]);
						});
