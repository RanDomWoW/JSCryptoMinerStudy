function recenter() {

	var x;
	var y;
	
	var app = navigator.appName;

	//alert(app);

	if (app == "Microsoft Internet Explorer"){	

		x = window.document.body.offsetWidth;
		y = window.document.body.offsetHeight;
	
	} else if (app == "Netscape") {
		
		x = window.innerWidth;
		y = window.innerHeight;

	} else {
		x = window.innerWidth;
		y = window.innerHeight;
	}
	
	var divTag = window.document.getElementById("toni");
	
	var layerWidth = divTag.offsetWidth;
	var layerHeight = divTag.offsetHeight;
	
	var newPosX = (x - layerWidth)/2;
	var newPosY = (y - layerHeight)/2;
	
	divTag.style.left = newPosX;
	divTag.style.top = newPosY;
	
	
}

