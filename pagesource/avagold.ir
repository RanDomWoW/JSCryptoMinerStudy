<body lock>


<script>
    window.nabzTitle = 'خبر فوری';
</script>

<header class="tophat-menu new-style " ng-controller="HeaderController as headerCtrl">
    <h6 class="login loginPanelBtn" ng-class="{arrowUp: headerCtrl.showloginPanel}" ng-click="headerCtrl.toggleLoginPanel()">
        <i class="userIco"></i>
        {{loginUser ? loginUser.Title : 'ورود به سامانه آوای آگاه'}}
    </h6>
    <h6 class="login notify-title">
        <i class="Font fa-envelope" ng-click="headerCtrl.toggleNotificationPanel()"></i>
        <span ng-click="headerCtrl.toggleNotificationPanel()">{{notifications.length | number}}</span>

<div class="notification" ng-class="{showNotification: showNotificationPanel}"
     ng-controller="NotificationPanelController as notifyCtrl">
    <header>
        <span>آخرین رویدادهای سایت ( {{notifications.length}} مورد )</span>
        <i class="Font fa-trash-o deleteAll" title="حذف همه" ng-click="notifyCtrl.removeAll()" ng-if="notifications.length"></i>
    </header>
    <div class="format">
        <div class="flash part" ng-repeat="item in notifications track by $index" ng-class="{comment: item.isComment}">
            
            <div class="content">
                <a class="textLink" ng-click="notifyCtrl.show(item)">{{item.title}}</a>
                <footer><pre>{{item.body}}</pre></footer>
            </div>
            <i class="Font fa-trash-o delete" ng-click="notifyCtrl.remove(item)"></i>
        </div>
    </div>

</div>
    </h6>
    <a class="productName" ng-href="/store">
        فروشگاه
        <i class="Font fa-shopping-cart"></i>
    </a>
    
    <a class="productName" ng-repeat="room in headerCtrl.conferenceRooms" ng-href="/Conference/{{room.Url}}">
        {{room.Title}}
        <i class="Font fa-users"></i>
    </a>

<script>
    window.loginUser = null;
</script>

<div class="loginTop" ng-class="{ show: headerCtrl.showloginPanel }">
    <div class="loginContainer">
        <h2>{{loginUser ? loginUser.Title : 'ورود به سیستم'}} <span class="permissionPeriod" ng-if="loginUser.PermissionPeriod">{{loginUser.PermissionPeriod}}</span></h2>
        <div class="row two" ng-if="!loginUser">
            <div class="column">
                <form ng-submit="headerCtrl.login()">
                    <label class="loginLabel">
                        <span>نام کاربری :</span>
                        <input type="text" id="username" name="username" placeholder="نام کاربری..." ng-model="headerCtrl.username" />
                    </label>
                    <label class="loginLabel">
                        <span>کلمه عبور :</span>
                        <input type="password" id="password" name="password" placeholder="کلمه عبور..." ng-model="headerCtrl.password" />
                    </label>
                    <label class="loginLabel">
                        <input type="checkbox" name="remember" ng-model="headerCtrl.remember">&nbsp; مرا به خاطر بسپار
                    </label>
                    <div class="btnHeadPos">
                        <button type="submit" class="btnLog" click-effect>ورود</button>
                        <a ng-href="/signup" class="textLink cover btnSign" click-effect>ثبت نام</a>
                    </div>
                    <div class="forgetPass">
                        <a ng-click="headerCtrl.forgotPassword()">کلمه عبور خود را فراموش کرده ام</a>
                    </div>
                </form>
            </div>
            <div class="column" ng-controller="AdvertiseController as advCtrl">
                <h3 class="advertiseH3" ng-show="advCtrl.slides && advCtrl.slides.length">{{advCtrl.activeSlide.Title}}</h3>
                <div class="advertise">
                    <div class="imgAdertise">
                        <ul>
                            <li ng-repeat="slide in advCtrl.slides" class="slideLi"
                                ng-class="{active: slide.Active}">
                                <a ng-href="{{slide.Link}}" target="{{setTarget(slide.Link)}}" class="link">
                                    <img ng-src="{{getSlideImageUrl(slide.Id, false)}}" alt="{{slide.Title}}" />
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="row" ng-if="loginUser" ng-class="{ userProfileLogin:loginUser }">
            <div class="column threeWide">
                <figure class="user">
                    <img ng-src="{{getPersonImageUrl(loginUser.PersonId, false)}}" alt="{{loginUser.Title}}" />
                </figure>
                <h3>{{loginUser.Username}}</h3>
                <div class="buttonPart">
                    <button click-effect class="btnChange" ng-click="headerCtrl.changePass()">تغییر کلمه عبور</button>
                    <button click-effect class="btnExit" ng-click="headerCtrl.logout()">خروج از سیستم</button>
                </div>
            </div>
            <div class="column threeWide">
                <div class="myProfile">
                    <a class="edit" ng-href="/profile/{{loginUser.Username}}/3">ویرایش اطلاعات شخصی</a>
                    <a class="list" ng-href="/profile/{{loginUser.Username}}/2">لیست مقالات من</a>
                    <a class="comment" ng-href="/profile/{{loginUser.Username}}/1">نظرات من</a>
                    <a class="twit" ng-href="/profile/{{loginUser.Username}}/4">مشاهده گردش حساب</a>
                    <a class="shopping" ng-href="/profile/{{loginUser.Username}}/5">خرید های من</a>
                    <a class="money" ng-href="/profile/{{loginUser.Username}}/6">موجودی</a>
                    <a class="submit" ng-href="/profile/{{loginUser.Username}}/7">ثبت مقاله جدید</a>
                </div>
            </div>
            <div class="fourWide column" ng-controller="AdvertiseController as advCtrl">
                <h3 class="advertiseH3" ng-show="advCtrl.slides && advCtrl.slides.length">{{advCtrl.activeSlide.Title}}</h3>
                <div class="advertise">
                    <div class="imgAdertise">
                        <ul>
                            <li ng-repeat="slide in advCtrl.slides" class="slideLi"
                                ng-class="{active: slide.Active}">
                                <a ng-href="{{slide.Link}}" target="{{setTarget(slide.Link)}}" class="link">
                                    <img ng-src="{{getSlideImageUrl(slide.Id, false)}}" alt="{{slide.Title}}" />
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</header>

<nav class="menuHead" ng-controller="MenuController as menuCtrl">
    <div class="nameProgram" ng-click="menuCtrl.goToMain()" click-effect style="background-image:url('/Content/images/logo/Avagold.png');">
        <figure></figure>
    </div>
    <ul class="menuPart" ng-class="{ showing: hideMenuTop }">
        <li class="liMenuPart" ng-repeat="menu in menuCtrl.menuParents" ng-class="{ show: menu.showItems && menu.Items.length }">
            <a ng-href="{{menu.Link}}" target="{{setTarget(menu.Link)}}">
                {{menu.Title}}
                <i ng-if="menu.Items.length" class="arrowDown"></i>
            </a>
            <ul class="menuUl" ng-if="menu.Items.length" ng-class="{ show: menu.showItems }">
                <li class="menuLi" ng-repeat="subMenu in menu.Items">
                    <a ng-href="{{subMenu.Link}}" target="{{setTarget(subMenu.Link)}}">{{subMenu.Title}}</a>
                </li>
            </ul>
        </li>
    </ul>
    <div class="IconMenu" ng-click="menuCtrl.showMenu()" click-effect>
        <i class="Font fa-bars"></i>
    </div>
    <div class="searchHead">
        <i class="Font fa-search" ng-click="menuCtrl.toggleSearch()" click-effect></i>
        <input type="search" placeholder="جستجو" ng-class="{show: menuCtrl.showSearch}"
                ng-model="menuCtrl.searchBox" ng-enter="menuCtrl.doSearch()" />
    </div>
    <div class="social">
        <a target="_blank" href="https://instagram.com/avagold.ir"><i class="Font fa-instagram" click-effect></i></a>
        <a target="_blank" href="https://telegram.me/AvaGoldAdviser"><i class="Font fa-send-o" click-effect></i></a>
        
    </div>
</nav>
        <section ng-class="{ hide:hideLatestNews, hideSec:hideMenuTop, maxHeight: setMaxHeight }">
                

<div class="home-container">
    <div class="container-index">
    <div class="price" ng-controller="PricesController as pricesCtrl">
        <div class="pricePart">
            <span class="priceChoise">
                <ul>
                    <li ng-repeat="item in pricesCtrl.priceGroups" ng-class="{ active: item.Active }"
                        ng-click="pricesCtrl.setPrices(item)">
                        {{item.Title}}
                    </li>
                </ul>
            </span>
            <div class="prices">
                <div class="row {{convertToString(pricesCtrl.prices.length)}}">
                    <div class="column" ng-repeat="price in pricesCtrl.prices">
                        <label class="unitName" flash-cell="price.PriceTypeTitle">{{price.PriceTypeTitle}}</label>
                        <label class="unitDate" flash-cell="price.EntryDate" title="{{price.EntryDate | persianDateTime}}">{{price.EntryDate | persianShortTime}}</label>
                        <label class="unitValue" flash-cell="price.Value">{{price.Value | number}}</label>
                        <label class="unitChange" flash-cell="price.Change" ng-class="{raiseUp: price.Change>0, raiseDown:price.Change<0 }">{{price.Change | number}}</label>
                    </div>
                </div>
            </div>
        </div>
        <span class="cog">
            <i class="Font fa-cog" ng-click="pricesCtrl.changeSettings()"></i>
        </span>
        <span class="cog">
            <i class="Font fa-refresh" ng-click="pricesCtrl.refreshData()"></i>
        </span>
    </div>

        <div class="slide-show-index">

<div class="" ng-controller="SlideShowController as slideShowCtrl">
    <div class="slide-show">
        <div class="slide-part">
            <article class="slides" ng-repeat="slide in slideShowCtrl.topSlides" ng-class="{active: slide.Active}">
                <figure class="slides-figure">
                    <a ng-href="{{slide.Link}}" target="{{setTarget(slide.Link)}}">
                        <div class="slides-img " ng-style="{'background-image': 'url(' + getSlideImageUrl(slide.Id, false) +')'}"></div>
                    </a>
                </figure>
                <header class="slides-title">
                    <span class="slides-time">
                        {{slide.DateTime | persianDateTime}}
                        <b class="Font {{slide.PermissionIcon}}"><span class="not-font">{{slide.PermissionTitle}}</span></b>
                        <i new-tab="{{slide.Link}}"></i>
                    </span>
                    <span class="slides-subject">
                        <a ng-href="{{slide.Link}}" target="{{setTarget(slide.Link)}}" class="textLink">{{slide.Title}}</a>
                    </span>
                    <span class="slides-detail"><a ng-href="{{slide.Link}}" target="{{setTarget(slide.Link)}}" class="textLink">{{slide.Description}}</a></span>
                </header>
            </article>
            <div class="slide-chooser">
                <div class="slide-chooser-part" ng-repeat="slide in slideShowCtrl.topSlides" ng-class="{active: slide.Active}" ng-click="slideShowCtrl.activeThisSlide(slide)">
                    {{$index+1}}
                </div>
            </div>
        </div>
        <div class="slide-part-2">
            <article class="slides" ng-repeat="slide in slideShowCtrl.slides">
                <figure class="slides-figure">
                    <a ng-href="{{slide.Link}}" target="{{setTarget(slide.Link)}}">
                        <div class="slides-img" ng-style="{'background-image': 'url(' + getSlideImageUrl(slide.Id, true) +')'}"></div>
                    </a>
                </figure>
                <header class="slides-title">
                    <span class="slides-time">
                        {{slide.DateTime | persianDateTime}}
                        <b class="Font {{slide.PermissionIcon}}"><span class="not-font">{{slide.PermissionTitle}}</span></b>
                        <i new-tab="{{slide.Link}}"></i>
                    </span>
                    <span class="slides-subject">
                        <a ng-href="{{slide.Link}}" target="{{setTarget(slide.Link)}}" class="textLink">{{slide.Title}}</a>
                    </span>
                    <span class="slides-detail"><a ng-href="{{slide.Link}}" target="{{setTarget(slide.Link)}}" class="textLink">{{slide.Description}}</a></span>
                </header>
            </article>
        </div>

        <button ng-click="slideShowCtrl.getOlderSlides()" class="moreLink">مطالب قدیمی تر</button>
    </div>
</div>

        </div>

        <tabset class="price-news-left tab-set">
            <tab heading="اخبار فوری">

<script>
    window.breakingNews = null;
</script>



<div class="breaking-news" ng-controller="BreakingNewsController as brkNewsCtrl">
    <div class="breaking-news-part flash" ng-repeat="item in brkNewsCtrl.news track by $index | orderBy: item.Time">
        <span class="breaking-news-time">{{item.Time | persianTime}}</span>
        <a class="textLink" ng-href="{{item.Link}}" target="{{setTarget(item.Link)}}" ng-bind-html="item.Note">
            
        </a>
    </div>
</div>
<button class="moreLink"><a class="textLink" href="/BreakingNews">نمایش همه</a></button>
            </tab>
        </tabset>


<div ng-controller="ArticleGroupSectionController as articleGroupSectionCtrl" class="footer-section">
    <div class="tab-set">
        <ul class="nav nav-tabs">
            <li ng-class="{active: articleGroupSectionCtrl.selectedTab.Id == section.Id}" ng-repeat="section in articleGroupSectionCtrl.articleGroupSections">
                <a class="textLink cover" ng-click="articleGroupSectionCtrl.selectedTab = section">
                    {{section.Title}}
                </a>
            </li>
        </ul>
        <div class="tab-content">
            <div class="footer-section-part" ng-repeat="item in articleGroupSectionCtrl.selectedTab.News">
                <figure>
                    <a ng-href="/article/{{item.Id}}/{{item.Link}}">
                        <img ng-src="{{getArticleImageUrl(item.Id, true)}}" alt="{{item.Title}}" />
                    </a>
                </figure>
                <div class="content">
                    <span class="time">
                        {{item.Date | persianDateTime}}
                        <b class="Font {{item.LevelIcon}}">
                            <span class="not-font">{{item.LevelTitle}}</span>
                        </b>
                        <i new-tab="/article/{{item.Id}}/{{item.Link}}"></i>
                    </span>
                    <a class="textLink" ng-href="/article/{{item.Id}}/{{item.Link}}">
                        <span class="title">{{item.Title}}</span>
                    </a>
                    <p class="detail">{{item.Body}}</p>
                </div>
            </div>
            <button class="moreLink"><a class="textLink" ng-href="/section/{{articleGroupSectionCtrl.selectedTab.Key}}">نمایش همه</a></button>
        </div>
    </div>
</div>
<div ng-controller="ConferenceSectionController as confSectionCtrl">
    <tabset class="conference-ask tab-set latest-news">
        <tab heading="{{room.Title}}" ng-repeat="room in confSectionCtrl.rooms">
            <div class="latest-news-part conference-section">
                <div class="latest-news-article flash" ng-repeat="comment in room.Comments">
                    <figure class="user">
                        <a ng-href="/profile/{{comment.Username}}">
                            <img ng-src="{{getPersonImageUrl(comment.PersonId, false)}}" alt="{{comment.PersonTitle}}" border="0"/>
                        </a>
                    </figure>
                    <div class="content">
                        <div class="inline">
                            <a ng-href="/profile/{{comment.Username}}" class="textLink">
                                <h5>{{comment.PersonTitle}}</h5>
                            </a>
                            <span class="postDate" title="{{comment.PostDate | persianDateTime}}">{{comment.PostDate | timeDiff}}</span>
                        </div>
                        <footer>
                            <a ng-href="{{comment.Link}}" target="{{setTarget(comment.Link)}}" class="textLink">
                                <div class="initial" ng-bind-html="comment.Body | link:linkText" td-max-length="50"></div>
                            </a>
                        </footer>
                    </div>
                </div>
                <button class="moreLink">
                    <a class="textLink" ng-href="/Conference/{{room.Url}}">نمایش همه</a>
                </button>
            </div>
        </tab>
    </tabset>
</div>

<div class="last-news" ng-controller="LatestNewsController as latestNewsCtrl">
    <div class="latest-news tab-set">
        <div select="latestNewsCtrl.selectTab(3)" class="last-news-section">
            <header>
                <span>آخرین مطالب</span>
                <span ng-if="latestNewsCtrl.getArticleNotificationsCount()" class="counts">{{latestNewsCtrl.getArticleNotificationsCount() | number}}</span>
            </header>
            <div class="tabButton">
                <div class="tab-part">
                    <span ng-repeat="tab in latestNewsCtrl.latestArticlesTabs" ng-click="latestNewsCtrl.selectedLatestArticlesTabChanged(tab)"
                          ng-class="{active: tab.id == latestNewsCtrl.selectedLatestArticlesTab.id}" class="tab">{{tab.title}}</span>
                    <i class="commentTab{{latestNewsCtrl.selectedLatestArticlesTab.id}}"></i>
                </div>
            </div>
            <div class="latest-news-part">
                <div class="latest-news-article flash " ng-repeat="item in latestNewsCtrl.selectedLatestArticlesTab.data" ng-class="{new: latestNewsCtrl.hasNotification(item.Id)}">
                    
                    <div class="content">
                        <a class="textLink" ng-href="/article/{{item.Id}}/{{item.Link}}"><h5>{{item.Title}}</h5></a>
                        <p class="detail">{{item.Body}}</p>
                    </div>
                </div>
                <button ng-click="latestNewsCtrl.loadMoreNews()" class="moreLink">مطالب قدیمی تر</button>
            </div>
        </div>
        <div select="latestNewsCtrl.selectTab(4)" class="last-news-section">
            <header>
                <span>آخرین نظرات</span>
                <span ng-if="latestNewsCtrl.getCommentNotificationsCount()" class="counts">{{latestNewsCtrl.getCommentNotificationsCount() | number}}</span>
            </header>
            <div class="tabButton">
                <div class="tab-part">
                    <span ng-repeat="tab in latestNewsCtrl.commentTabs" ng-click="latestNewsCtrl.selectedCommentTabChangeed(tab)"
                          ng-class="{active: tab.id == latestNewsCtrl.selectedCommentTab.id}" class="tab">{{tab.title}}</span>
                    <i class="commentTab{{latestNewsCtrl.selectedCommentTab.id}}"></i>
                </div>
            </div>
            <div class="latest-news-part">
                <div class="latest-news-article flash" ng-repeat="comment in latestNewsCtrl.selectedCommentTab.data" ng-class="{new: latestNewsCtrl.hasNotification(comment.Id)}">
                    
                    <div class="content">
                        <div class="inline">
                            <a ng-href="/profile/{{comment.Username}}" class="textLink">
                                <h5>{{comment.PersonTitle}}</h5>
                            </a>
                            <i class="date">
                                <span>{{comment.Title}}</span>
                            </i>
                            <span class="postDate" title="{{comment.PostDate | persianDateTime}}">{{comment.PostDate | timeDiff}}</span>
                        </div>
                        <footer>
                            <a ng-href="{{comment.Link}}" target="{{setTarget(comment.Link)}}" class="textLink">
                                <div class="initial" ng-bind-html="comment.Body | link:linkText" td-max-length="50"></div>
                            </a>
                        </footer>
                    </div>
                </div>
                <button ng-click="latestNewsCtrl.loadMoreComments()" class="moreLink">نظرات قدیمی تر</button>
            </div>
        </div>
        <div select="latestNewsCtrl.selectTab(5)" class="last-news-section">
            <header>
                <span>پربازدیدترین ها</span>
            </header>
            <div class="tabButton">
                <div class="tab-part">
                    <span class="tab" ng-repeat="tab in latestNewsCtrl.topArticlesTabs" ng-click="latestNewsCtrl.selectedTopArticlesTabChanged(tab)"
                          ng-class="{active: tab.id == latestNewsCtrl.selectedTopArticlesTab.id}">{{tab.title}}</span>
                    <i class="commentTab{{latestNewsCtrl.selectedTopArticlesTab.id}}"></i>
                </div>
            </div>
            <div class="latest-news-part">
                <div class="latest-news-article flash" ng-repeat="item in latestNewsCtrl.selectedTopArticlesTab.data">
                    <h4 ng-if="$index == 0 && item.ViewDate">{{item.ViewDate}}</h4>
                    <h4 ng-if="$index > 0 && item.ViewDate != latestNewsCtrl.selectedTopArticlesTab.data[$index-1].ViewDate">{{item.ViewDate}}</h4>
                    
                    <div class="content">
                        <a class="textLink" ng-href="/article/{{item.ArticleId}}/{{item.Url}}"><h5>{{item.Title}}</h5></a>
                        <footer>
                            <span flash-cell="item.ViewCount">تعداد نمایش : {{item.ViewCount | number}} <i class="Font fa-eye"></i></span>
                        </footer>
                    </div>
                </div>
                <button ng-click="latestNewsCtrl.loadMoreArticles()" class="moreLink">{{latestNewsCtrl.selectedTopArticlesTab.id == 3 ? 'ادامه' : latestNewsCtrl.selectedTopArticlesTab.title + ' های پیش'}}</button>
            </div>
        </div>
    </div>
    
</div>

        <div class="advertising Conference">
                <div conference-room></div>
        </div>

    </div>
</div>


                
            </section>
    <footer class="footer">
        <div class="menu-format">

<div class="detailCompany" ng-controller="SiteMapController as siteMapCtrl">
    <div class="row {{convertToString(siteMapCtrl.siteMapColumns.length)}}">
        <div class="column" ng-repeat="c in siteMapCtrl.siteMapColumns">
            <div ng-repeat="p in c">
                <h5>{{p.Title}}</h5>
                <ul>
                    <li ng-repeat="i in p.items | orderBy:['Priority','Id']">
                        <a ng-href="{{i.Link}}" target="{{setTarget(i.Link)}}">
                            {{i.Title}}
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>

        </div>
        <div class="description-footer">
            <span class="right">کلیه حقوق متعلق به شرکت مشاور سرمایه گذاری آوای آگاه است.</span>
            &nbsp;|&nbsp;
            <span class="eng"> Copyright © 2018</span>
            <a class="textLink eng" href="http://asax.ir" target="_blank">. Powered By Asa</a>
        </div>
    </footer>

</body>
</html>