		<script type="text/javascript">
		
		function getVariationAttributeByValueID(variationAttribute, combinedValueString, markup_array)
		{
			for(var m in markup_array)
			{
				var arrayValueString = combinedValueString.split(":");
				var arrayMarkupValue = markup_array[m]["values"].split(":");
				var intersection = intersect(arrayValueString, arrayMarkupValue);
				
				if( intersection.length === arrayValueString.length || (combinedValueString === undefined && markup_array[m]["values"] == ""))
				{
					return markup_array[m][variationAttribute];
				}
			}
			return null;
		}
		
		function intersect(a, b) {
		    var d = {};
		    var results = [];
		    for (var i = 0; i < b.length; i++) {
		        d[b[i]] = true;
		    }
		    for (var j = 0; j < a.length; j++) {
		        if (d[a[j]])
		            results.push(a[j]);
		    }
		    return results;
		}

		function priceChangeOnMarkup(value_id, markup_id, number_format, priceDivID, markup_array, current_markup_array, oldPrice, combinedValueString, rrpContainerId)
		{
			var resultPrice = getVariationAttributeByValueID("price", combinedValueString, markup_array);
			var rrp = getVariationAttributeByValueID("rrp", combinedValueString, markup_array);
			
			if(rrp === null){
			    try{
			        //try to load static rrp
			       rrp = jQuery("#" + priceDivID + "_mrp").attr("value");
			    }catch(e){
			    
			    }
			}
			
			
			
			if(resultPrice === null){ return null; }
			var newPrice = parseFloat(resultPrice);
			var price_div = jQuery("#"+priceDivID);

			// save total price into the exchange container
			var priceTotalContainer = document.getElementById(priceDivID + "_total");
		
			if (priceTotalContainer)
			{
				priceTotalContainer.value = newPrice.toString();
			}

			if(price_div != null)
			{
				price_div.html( (number_format == 0 ? round(newPrice,2).replace(".", ",") : round(newPrice,2)) );	
						
				var price_saveDynamicValue = jQuery("#" + priceDivID + "_savingDynamic");
				var price_saveDynamicPercentValue = jQuery("#" + priceDivID + "_savingDynamicPercent");
				var price_savingDynamicDecimalSeparatorDot = jQuery("#" + priceDivID +"_savingDynamicDecimalSeparatorDot");
				
				if(price_saveDynamicValue != null || price_saveDynamicValue != undefined){
				    price_saveDynamicValue.html(numberFormat(0,0));
				}
				
				if(price_saveDynamicPercentValue != null || price_saveDynamicPercentValue != undefined){
				    price_saveDynamicPercentValue.html("0");
				}
				
				if(price_savingDynamicDecimalSeparatorDot != null || price_savingDynamicDecimalSeparatorDot != undefined){
				    price_savingDynamicDecimalSeparatorDot.html("0.00");
				}
				
				var cfgFormat = jQuery("#" + priceDivID + "_cfgFormat").attr("value");
	            
	            var rrpContainer = jQuery("#" + rrpContainerId );
	            rrpContainer.html(numberFormat(rrp,cfgFormat));
	            
	            
	            var priceDiv = priceDivID.split("_");
	            var rowId = priceDiv[2];
	            var itemId = priceDiv[3];
	            
	            
	            try{
	            
	                if(markup_array[(value_id-1)]["liveShoppingPriceOriginal"] !== null || markup_array[(value_id-1)]["liveShoppingPriceOriginal"] !== undefined){
	            
	                    try{
	                        var liveShoppingPriceOriginalContainer = jQuery("#liveShoppingPriceOriginal_"+rowId+"_"+itemId);	            
	                        liveShoppingPriceOriginalContainer.html(numberFormat(markup_array[(value_id-1)]["liveShoppingPriceOriginal"],cfgFormat));
	                    }catch(e){
	                
	                    }
	           
	                }
	            
	            }catch(e){
	            
	            }
	            
	            
	            
	            
						
				if(rrp > 0 && rrp > newPrice){
			        var saving = rrp - newPrice;
			        var saving_percent = (saving*100)/rrp;
				
				    price_saveDynamicValue.html(numberFormat(saving, parseInt(number_format)));
				    price_savingDynamicDecimalSeparatorDot.html(numberFormat(saving, 1))
				    price_saveDynamicPercentValue.html(numberFormat(saving_percent, parseInt(number_format)));
				}		
				
			}
		}
		
	
		function numberFormat(value, cfgFormat){
		    value = parseFloat(value).toFixed(2).replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1.");
		    
		    if(cfgFormat===1){
		        return value;
		    }else{
		        return value.replace(/.([^.]*)$/, ",$1");
		    }
		}
		

		function updateVariationNumber(variationArray, combinedValueString)
		{
			var resultCustomNumber = getVariationAttributeByValueID("customNumber", combinedValueString, variationArray);
			if(resultCustomNumber === null){ return null; }
			var customNumberDiv = jQuery('[data-plenty=customNumber]');

			if(customNumberDiv != null)
			{
				customNumberDiv.text(resultCustomNumber);
			}
		}

		function getValueString(element)
		{
			if(element === document)
			{
				return jQuery(element).find('div[data-item-attributes]:first [name^=ArticleAttribute]').serializeArray().map(function(el, i){ return el['value'];}).join(':');
			}
			return jQuery(element).parents('div[data-item-attributes]').find('[name^=ArticleAttribute]').serializeArray().map(function(el, i){ return el['value'];}).join(':');
		}

		function round(x,s)
		{
			//round value x to s internal decimal places

			if(x.toFixed)
			{
				return x.toFixed(s);
			}
			else
			{
				return parseInt(x*Math.pow(10,s)+0.5)/Math.pow(10,s);
			}
		}
		
		function updateBarcode(combinedValueString, variationArray, aAttribute_Barcode_Values)
		{
		
		    if(typeof jQuery(document).find('[data-plenty=variationBarcode]')[0] != 'undefined')
		    {
		    
		         var elements = jQuery(document).find('[data-plenty=variationBarcode]'); 
		         
		         var barcodeNames = [];
		         
		         for(var i = 0; i < elements.length; i++)
		         {
		            barcodeNames[i] = jQuery(elements[i]).attr("data-barcode");
		         }
		           
		    
                 var variationId = getVariationAttributeByValueID("variationId", combinedValueString, variationArray);
             
             
                 var max = aAttribute_Barcode_Values.length;
                               
                
                 for(var k = 0; k < barcodeNames.length; k ++)
                 {
                    var i = -1
                    
                    if(barcodeNames[k] !== "" && typeof barcodeNames[k] != 'undefined')
                    {
                 
                         for(var x = 0;x < max;x++)
                         {  
                                if(aAttribute_Barcode_Values[x][2] == parseInt(variationId) &&
                                    aAttribute_Barcode_Values[x][1] === barcodeNames[k])
                                {
                                    i = x;
                                    break;
                                }
                            
                         }
                         
                         
                         if(i !== -1){ 
                                jQuery(elements[k]).text(aAttribute_Barcode_Values[i][0]);		 
                         }else{
                                jQuery(elements[k]).text('');
                         }
                 
                    }
                 
                 
                 
                 
                 }
                    
                 
                 
		    }
		            
		}
		
		
