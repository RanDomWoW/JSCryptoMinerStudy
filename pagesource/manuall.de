<script>
function sparePartChild(id,grandparent,data) { 
    var a = document.querySelector("#"+id);
    var state = a.style.display;
    var category = a.getAttribute("data-cat");
    var size = window.innerWidth;
    var modal = document.getElementById('myModal');
    if(size < 768){
        console.log("The screen is " + size + "px > small screen");
        grandparent = encodeURIComponent(grandparent);
        var info = "id="+id+"&grandparent="+grandparent;
        var xhttp = new XMLHttpRequest();
        xhttp.onreadystatechange = function() {
            if (this.readyState == 4 && this.status == 200) {
                document.getElementById("demo").innerHTML = this.responseText;
            }
        };
        xhttp.open("POST", "/spare_parts.php",true);
        xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
        xhttp.send(info);
        modal.style.display = "block";

        var span = document.getElementsByClassName("close")[0];
        span.onclick = function() {
            modal.style.display = "none";
        }
        window.onclick = function(event) {
            if (event.target == modal) {
                modal.style.display = "none";
            }
        }
    }
    else if(state == 'none' || (state == 'block' && category != data)){
        a.setAttribute("data-cat",data);
        grandparent = encodeURIComponent(grandparent);
        var info = "id="+id+"&grandparent="+grandparent;
        var xhttp = new XMLHttpRequest();
        xhttp.onreadystatechange = function() {
            if (this.readyState == 4 && this.status == 200) {
                a.innerHTML = this.responseText;
            }
        };
        xhttp.open("POST", "/spare_parts.php",true);
        xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
        xhttp.send(info);

        a.style.display = 'block';
    }
    else {
        a.style.display = 'none';
    }
}
</script>

<script>
function faqRating(id) {
    if(typeof(id) !== "number"){
        //console.log("not a number");
        return;
    }
    var faq = document.querySelector("#faq"+id);
    faq.style.color = 'green';
    faq.style.cursor = 'default';
    faq.style.fontWeight = 'bold';
    faq.removeAttribute('onclick');
    var ratingLocation = document.querySelector("#count"+id);
    var rating = ratingLocation.innerHTML;
    rating = parseInt(rating) + 1;
    ratingLocation.innerHTML = rating;
    console.log(id);
    var info = 'id='+id;
    var xhttp = new XMLHttpRequest();
    xhttp.onreadystatechange = function() {
        if (this.readyState == 4 && this.status == 200) {
            //document.getElementById("demo").innerHTML = this.responseText;
            console.log(this.responseText);
        }
    };
    xhttp.open("POST", "/faqrating.php",true);
    xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
    xhttp.send(info);

    var faq = getCookie("faq");
    if(faq) {
        var value = faq+','+id;
        setCookie('faq',value,1);
    }
    else {
        setCookie('faq',id,1);
    }
}

function setCookie(cname,cvalue,exyears) {
    var d = new Date();
    d.setTime(d.getTime() + (exyears*365*24*60*60*1000));
    var expires = "expires=" + d.toGMTString();
    console.log(d);
    //path = window.location.pathname;
    console.log(cname + "=" + cvalue + ";" + expires);
    document.cookie = cname + "=" + cvalue + ";" + expires;
}

function getCookie(cname) {
    var name = cname + "=";
    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(';');
    for(var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}

window.addEventListener("load",checkCookie);
function checkCookie() {
    var data = document.querySelectorAll('[data-faq]');
    var z = 0;
    while(z < data.length) {
        id = data[z].getAttribute("data-faq");
        var faq = getCookie("faq");
        if(faq) {
            faqs = faq.split(',');
            var y = 0;
            for(var i = 0; i < faqs.length && y != 1; i++) {
                if(faqs[i] == id){
                    y = 1;
                }
            }
            if(y == 1){
                question = document.querySelector("#faq"+id);
                question.style.color = 'green';
                question.style.cursor = 'default';
                question.style.fontWeight = 'bold';
                question.removeAttribute('onclick');
            }
        }
        z++;
    }
}
</script>

