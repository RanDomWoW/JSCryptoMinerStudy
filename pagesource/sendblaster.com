<script type="text/javascript">
	//<![CDATA[
	var language = 'en';
	
	var skype_caption1 = 'Skype us!';
	var skype_caption2 = 'Call us!';
	
	var skype_button = 
		'<a class="skype_button ctaButton ctaSky"'
		+ ' title="Call us for free via Skype! Skype name: sendblaster.support2"'
		+ ' href="skype:sendblaster.support2?call">'
		+ '<span class="skype_button_caption">'
		+ skype_caption1
		+ '</span>'
		+ '</a>';
	
	var skype_number =
		'<span class="skype_number"'
		+ ' title="Call our Commercial Support!">'
		+ '+1 347 80 93 100</span>';
	
	var S_OFFLINE = 1;
	var S_ONLINE = 2;
	var S_AWAY = 3;
	var S_BUSY = 5;
	
	var skype_status = 2;
	
	var skype_alternate_caption = false;
	
	var change_caption_rate = 3; // seconds
	
	function changeSkypeCaption() {
		var cur_caption =
				skype_alternate_caption?
				skype_caption1:
				skype_caption2;
		var captions = getElementsByClassName("skype_button_caption");
		if(!captions.length) return;
		for(var i = 0; i < captions.length; ++i) {
			captions[i].innerHTML = cur_caption;
		}
		skype_alternate_caption = !skype_alternate_caption;
		if(change_caption_rate > 0) {
			window.setTimeout(
				changeSkypeCaption,
				change_caption_rate * 1000
			);
		}	
	}
	
	function displaySkypeStatus() {
		var containers = getElementsByClassName("skype_container");
		if(skype_status == S_ONLINE) {
			for(var i = 0; i < containers.length; ++i) {
				containers[i].innerHTML = skype_button;
				containers[i].removeAttribute("style");
			}
			changeSkypeCaption();
		} else {
			for(var i = 0; i < containers.length; ++i) {
				containers[i].innerHTML = "";
				containers[i].setAttribute("style", "display: none;");
			}		
		}
	}
	
	function phoneContainer () {
		if(language == 'en') {
			containers = getElementsByClassName("phone_container");
			if(skype_status == S_ONLINE
				|| skype_status == S_AWAY) {
				for(var i = 0; i < containers.length; ++i) {
					containers[i].innerHTML = skype_number;
					containers[i].removeAttribute("style");
				}
			} else {
				for(var i = 0; i < containers.length; ++i) {
					containers[i].innerHTML = "";
					containers[i].setAttribute("style", "display: none;");
				}		
			}
		}
	}
	
	displaySkypeStatus();
	phoneContainer();
//]]>

</script>  </div>

  
