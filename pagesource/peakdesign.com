<script>
    
    //function for setting a newsletter
    function closeNewsletter() {
        var days = 7;
        myDate = new Date();
        myDate.setTime(myDate.getTime()+(days*24*60*60*1000));
        document.cookie = 'closeNewsletter=true;path=/;expires=' + myDate.toGMTString();
    };
    
    //function for getting the cookie name
    function getCookie(cname) {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for(var i = 0; i <ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
    return "";
}
    //function to get newsletter modal to popup
    function popUp() {
        javascript:Modal.show('#modalnewsletter');
    };
    
    //function to submit the email address
    function newsModalSubmit() {
        var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/
         $j('#submit-button').on("click", function(){

            var email = $j('#email-field').val(); 
            if (!email.match(re)){
                alert('Please enter a valid email');
                return false;
            }
            javascript:Modal.close();
            $j("input[name='EMAIL']").val(email); //input the email in footer for newsletter
            $j('#newsletter-button').trigger('click'); //trigger the submit
        });        

    
    }
  </script>
