<script>
AmCharts.ready(function () {
	
	current_category_id = 0;
	
	chartData = [];
	
	chart = new AmCharts.AmSerialChart();
	chart.dataProvider = chartData;
	chart.categoryField = "date";
	chart.columnWidth = 0.99;
	
	var categoryAxis = chart.categoryAxis;
	categoryAxis.parseDates = true;
	categoryAxis.minPeriod = "DD";
	categoryAxis.gridAlpha = 0.2;
	categoryAxis.axisAlpha = 0;
	categoryAxis.gridPosition = "start";
	categoryAxis.dateFormats = [
		{period:'fff',format:'JJ:NN:SS'},
		{period:'ss',format:'SS'},
		{period:'mm',format:'NN'},
		{period:'hh',format:'JJ'},
		{period:'DD',format:'M/D'},
		{period:'MM',format:'M/D'},
		{period:'YYYY',format:'M/D'}
	];
	categoryAxis.equalSpacing = true;
	
	
	
	
	var valueAxis = new AmCharts.ValueAxis();
	valueAxis.axisAlpha = 0;
	valueAxis.gridAlpha = 0.2;
	valueAxis.minimum = 0;
	valueAxis.maximum = 25;
	chart.addValueAxis(valueAxis);
	
	var graph = new AmCharts.AmGraph();
	graph.valueField = "index";
	graph.colorField = "color";
	graph.balloonText = "[[category]]: <b>[[value]]</b>位";
	graph.type = "column";
	graph.lineAlpha = 0;
	graph.fillAlphas = 1;
	chart.addGraph(graph);
	
	var chartCursor = new AmCharts.ChartCursor();
	chartCursor.cursorAlpha = 0;
	chartCursor.zoomable = false;
	chartCursor.categoryBalloonEnabled = false;
	chartCursor.categoryBalloonDateFormat = "YYYY/M/D";
	chart.addChartCursor(chartCursor);
	
	chart.addListener('clickGraphItem', function(event) {
		//console.log(event.item.category);
		
		var click_date = event.item.category;
		
		var Y = click_date.getFullYear();
		var M = click_date.getMonth() + 1;
		if(M.length === 1){
			M = '0' + M;
		}
		var D = click_date.getDate();
		if(D.length === 1){
			D = '0' + D;
		}
		
		window.open('/seo_news.php?d=' + Y + '-' + M + '-' + D + '#' + Y + '-' + M + '-' + D);
		
	});
	
	
	chart.write("graph_wrap");
	
	$.ajax({
		type: "POST",
		url: "./",
		cache: false,
		dataType: 'json',
		data: {
			rank_index_graph: 1,
			category_id: 0,
			start: $('.change_period').val()
		}
	})
	.done(function (data, textStatus, jqXHR) {
		var data2 = [];
		var i = 0;
		for (var i = 0; i < data.length; i++){
			var item = data[i];
			var item2 = [];
			
			item2['date'] = Date.parse(item['date']);
			item2['index'] = item['value'];
			if(item['value'] >= 10) {
				item2['color'] = '#ff0000';
			} else if(item['value'] >= 6.7) {
				item2['color'] = '#ff952b';
			} else {
				item2['color'] = '#ffcc33';
			}
			
			chartData.push({
				date: item2['date'],
				index: item2['index'],
				color: item2['color']
			});
		}
		chart.validateData();
	});
});
$('.change_period').change(function() {
	
	var period = $(this).val();
	
	$.ajax({
		type: "POST",
		url: "./",
		cache: false,
		dataType: 'json',
		data: {
			rank_index_graph: 1,
			category_id: current_category_id,
			start: period
		}
	})
	.done(function (data, textStatus, jqXHR) {
		
		chartData = [];
		
		var data2 = [];
		var i = 0;
		for (var i = 0; i < data.length; i++){
			var item = data[i];
			var item2 = [];
			
			item2['date'] = Date.parse(item['date']);
