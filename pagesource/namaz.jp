			var item2 = [];
			
			item2['date'] = Date.parse(item['date']);
			item2['index'] = item['value'];
			if(item['value'] >= 10) {
				item2['color'] = '#ff0000';
			} else if(item['value'] >= 6.7) {
				item2['color'] = '#ff952b';
			} else {
				item2['color'] = '#ffcc33';
			}
			
			chartData.push({
				date: item2['date'],
				index: item2['index'],
				color: item2['color']
			});
		}
		
		chart.dataProvider = chartData;
		
		chart.validateData();
	});
});
