	$.ajax({
		type: "POST",
		url: "./",
		cache: false,
		dataType: 'json',
		data: {
			rank_index: 1,
			category_id: current_category_id,
		}
	})
	.done(function (data, textStatus, jqXHR) {
		var class_string;
		
		if(data > 10) {
			class_string = 'change-img2-b';
		} else if(data > 6.7) {
			class_string = 'change-img3-b';
		} else {
			class_string = 'change-img-b';
		}
		
		$('#rank_index_class').attr('class', class_string);
		$('#rank_index_value').text(data);
		
		
	});
	
	var period = $('.change_period').val();
	
	$.ajax({
		type: "POST",
		url: "./",
		cache: false,
		dataType: 'json',
		data: {
			rank_index_graph: 1,
			category_id: current_category_id,
			start: period
		}
	})
	.done(function (data, textStatus, jqXHR) {
		
		chartData = [];
		
		var data2 = [];
		var i = 0;
		for (var i = 0; i < data.length; i++){
