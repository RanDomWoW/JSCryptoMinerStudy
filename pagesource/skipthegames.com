$(document).ready(function(){


// remember and set Top blocks based on last click
	/*
	PART ONE
	In which we READ the cookie
	and activate the corresponding category
	*/
	// Cookie reader function
	function getCookie(cname) {
		var name = cname + "=";
		var ca = document.cookie.split(';');
		for(var i=0; i<ca.length; i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1);
			if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
		}
		return "";
	}
	// If our cookie has value
	if ( getCookie('STGtop') !== ""){
		// show the right tab
		console.log('STGtop cookie value = ' + getCookie('STGtop'));
		$('ul.tabs li.tab-title').each(function(){
			$(this).removeClass('active');
			if($(this).attr('rel') == getCookie('STGtop')){
				$(this).addClass('active');
				$('div.tabs-content div.content').removeClass('active').each(function(){
					if($(this).attr('rel') == getCookie('STGtop')){
						$(this).addClass('active');
					}
				});
			}
		});
	}

	/*
	PART TWO
	In which we set the cookie
	with the rel of the last clicked tab.
	*/
	STGhostname = window.location.hostname;
	STGcookieName = "STGtop";
	STGd = new Date();
	STGmilliseconds = 365*24*60*60*1000;
	STGd.setTime(STGd.getTime()+STGmilliseconds); // 1 year ahead
	STGexpires ="expires="+STGd.toGMTString(); // expire the cookie in 1 year

	function STGsetCookie(cookieName){
		document.cookie = STGcookieName+"="+cookieName+";"+STGexpires+";path=/;domain="+STGhostname;
	}

	$("li.tab-title").on("click", function() {
	  STGsetCookie($(this).attr('rel'));
	});	
  
	  
	  
