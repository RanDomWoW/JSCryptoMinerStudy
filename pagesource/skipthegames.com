    $.adCity.nameLen = $.adCity.name.length;
    
    $(document).ready(function() {
        checksearch();
        setInstantCitySearch();
    });
    
    (function($) { // absolute-center the note
        $.fn.extend({
            center: function () {
                return this.each(function() {
                    var top = ($(window).height() - $(this).outerHeight()) / 2;
                    var left = ($(window).width() - $(this).outerWidth()) / 2;
                    $(this).css({position:'fixed', margin:0, top: (top > 0 ? top : 0)+'px', left: (left > 0 ? left : 0)+'px'});
                });
            }
        }); 
    })(jQuery);    
    
    
    function checksearch() {
        cookieValue = $.cookie("search");
        if (cookieValue) {
            $('#input_search_location').val(cookieValue);
            $.removeCookie('search');
            $('.city-notification').center().fadeIn('fast');
            setTimeout(function() { $('.city-notification').fadeOut('slow'); }, 1000);
            $('#input_search_location').focus(function(){this.select();});
            $('#input_search_location').focus();
        }
    }
    
    function setInstantCitySearch() {
        $('input#input_search_location').autocomplete({
            source: function(req, res) {
                var city = new RegExp("\\b" + req.term, "gi");
                var found = [];
                for (var i=0; i<$.adCity.nameLen; i++) {
                    var cityMatch = $.adCity.name[i].match(city);
                    if (cityMatch)
                        found.push($.adCity.name[i]);
                }
                if (found.length<=1 && $.inArray("Unlisted City, World", found)==-1) {
                    found.push("Unlisted City, World");
                    var world = true;
                }
                else if (world) {
                    found.pop(0);
                    var world = false;
                }
                res(found);
            }, // end of source
            minLength: 2,
        });
    }
    
    function sanitizeKeywords() {
        if (!$('#input_search_location').val() || $('#input_search_location').val() === 'Type a city name') {
            $('#input_search_location').addClass('LV_invalid_field').focus();
            return false;
        }
        else {
            $('input#input_search_optional_keywords')
                .val($.trim($('input#input_search_optional_keywords')
                        .val().replace(/[^A-Za-z0-9\s]/gi, ' ').replace(/[_\s]/g, ' ')));
            _gaq.push(['_trackEvent', 'Forms', 'Submit', 'Main search form']);
            ga('send','event','Forms','Submit','Main search form');
            document.forms['topSearchForm'].submit();
            return true
        }
    }
