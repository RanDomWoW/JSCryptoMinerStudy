    <script type="text/javascript">
        var animation = true;
        $(document).ready(function () {
            $('#expandContentDiv').hide();

            //var $elem = $('#sitemapContent');
            var $elem = $('body');
            $('#sitemapContent').hide();
            $('#sitemapTitle').click(function () {
                // When clicked, toggle the "view" div.
                if ($('#sitemapContent').is(":visible")) {
                    $('#sitemapContent').slideToggle(400, function () {
                        return;
                        //$('#sitemapTitle').attr('class', 'sMapDownArrow');
                    });
                    return false;
                } else {
                    $('#sitemapContent').toggle();
                    //var scrollY = $('body').scrollTop()?$('body').scrollTop():$('html').scrollTop();
                    $('#sitemapTitle').attr('class', 'sMapUpArrow');
                    var scrollY = $elem.height();
                    $('html, body').animate({
                        scrollTop: scrollY + $('#sitemapContent').outerHeight()
                    }, 'medium');
                }
            });

            /* * * * * * * * * * * * * * *
             * Navigation Menu Height
             * * * * * * * * * * * * * * */
            function resizeNavigationMenu() {
                var $home   = $('.homeContainer');
                var $header = $('#header .navbar-header');
                var $menu   = $('#header .navbar-nav');
                var $news   = $('.expandHeaderDiv');
                if (window._shouldRespond && Modernizr.mq('(max-width: 1024px)')) {
                    $menu.outerHeight($home.outerHeight(true) - $header.outerHeight(true) - $news.outerHeight(true));
                } else {
                    $menu.outerHeight('auto');
                }
            };
            resizeNavigationMenu();
            $(window).resize(function() {
                var $home = $('.homeContainer');
                var homePrevWidth  = $home.data('width');
                var homePrevHeight = $home.data('height');
                var homeCurrWidth  = $home.outerWidth(true);
                var homeCurrHeight = $home.outerHeight(true);
                if (homePrevWidth !== homeCurrWidth || homePrevHeight !== homeCurrHeight) {
                    $home.data('width', homeCurrWidth);
                    $home.data('height', homeCurrHeight);
                    resizeNavigationMenu();
                }
            });

            setTimeout(function () {
                $('.lightbox').hide();
            }, 10000);
        });

        $(document).click(function () {
            $('.lightbox').hide();
        });
	</script>
    
