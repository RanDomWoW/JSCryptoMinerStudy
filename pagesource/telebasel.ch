    <script>
        ADTECH.config.placements['telebasel-skyscraper'] = { responsive : { useresponsive: true, bounds: [{id: '5826503', min: 1280, max: 9999 }]}, params: { target: '_blank' }};
        ADTECH.enqueueAd('telebasel-skyscraper');
        ADTECH.executeQueue();

        $(function () {
            var node = $('#telebasel-skyscraper');
            var prev = node.prev();
            var win = $(window);
            var isParent = false;

            if (prev.length === 0) {
                isParent = true;
                prev = node.parent();
            }

            function updateSkyscraper() {
                //check visibility
                if (node.css('display') == 'none') {
                    return;
                }

                //move
                var yPos = prev.offset().top;

                if (!isParent) {
                    yPos += prev.height();
                }

                var top = Math.max(0, win.scrollTop() - yPos);

                node.css('top', top + 'px');
            }

            win.scroll(function () {
                updateSkyscraper();
            });

            win.resize(function () {
                updateSkyscraper();
            });

            updateSkyscraper();
        });
    </script>
