        $(function () {
            //lead carosel
            var leadCarousel = $('#lead-carousel');

            if (!leadCarousel) {
                return;
            }

            leadCarousel.carousel();
            leadCarousel.on('slid.bs.carousel', function () {
                var idx = leadCarousel.find('div.active').index();

                selectMiniItem(idx, true);
            });

            //mini carousel
            var miniCarousel = $('#mini-carousel');
            var miniSelPos = 0;
            var miniItems = miniCarousel.find('ul li');

            miniCarousel.show();
            miniCarousel.flexslider({
                animation: 'slide',
                controlNav: false,
                animationLoop: false,
                slideshow: false,
                touch: true,

                //item size
                itemWidth: 140,
                itemMargin: 5
            });

            miniItems.on('mouseover click', function (evt) {
                var idx = miniItems.index(this);

                if (idx === -1) {
                    return;
                }

                if (evt.type === 'click') {
                    openLeadItem(idx);
                    return;
                }

                selectMiniItem(idx, false);
                selectLeadItem(idx);

                evt.preventDefault();
            });

            function selectMiniItem(idx, fromMain) {
                if (fromMain && nextPos !== -1) {
                    return;
                }

                if (idx !== miniSelPos) {
                    $(miniItems[miniSelPos]).removeClass('flex-active-slide');
                    miniSelPos = idx;
                    $(miniItems[idx]).addClass('flex-active-slide');
                }
            }

            var nextPos = -1;

            function selectLeadItem(idx) {
                var carousel = leadCarousel.data('bs.carousel');

                //console.log('selectLeadItem: ' + idx);

                if (carousel.sliding) {
                    if (nextPos !== -1) {
                        nextPos = idx;
                        return;
                    }

                    nextPos = idx;

                    carousel.$element.one('slid.bs.carousel', function () {
                        //ready for next animation
                        carousel.to(nextPos);
                        nextPos = -1;
                    });
                } else if (nextPos === -1) {
                    carousel.to(idx); //Note: queues updates
                }
            }

            function openLeadItem(idx) {
                var item = $(leadCarousel.find('.carousel-inner .item')[idx]);

                item.find('a')[0].click();
            }
        });
        </script>
