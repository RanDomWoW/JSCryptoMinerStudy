function isSpecial(str){
	nlength = str.length;
	for(i=0;i<nlength;i++){
		if ((str.charCodeAt(i)<128) && (str.charCodeAt(i)>123)){
			return false
		}
		if ((str.charCodeAt(i)<96) && (str.charCodeAt(i)>90)){
			return false
		}
		if ((str.charCodeAt(i)<65) && (str.charCodeAt(i)>57)){
			return false
		}
		if ((str.charCodeAt(i)==37)){
			return false
		}
		if ((str.charCodeAt(i)==44)){
			return false
		}
	}
	return true
}
