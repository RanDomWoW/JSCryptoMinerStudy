var flag = 'closed'
function SubItems(ID)
	{
		var sectionsCount =13
		var myLang = "arb"

		for (var i=1;i<=sectionsCount;i++){
			if (ID == 'all')
			{
				if (flag == 'opened')
				{
					document.all['Item'+i].style.display='none'
					document.all['opened'].style.display='none'
					document.all['closed'].style.display=''
				}
				else if (flag == 'closed')
				{
					opened.innerHTML="<img src='/shared/images/lang/close_menu_link_"+myLang+".jpg' border=0>"
					document.all['Item'+i].style.display= ''
					document.all['opened'].style.display=''
					document.all['closed'].style.display='none'
				}
			}
			else if (i!=ID)
			{
				document.all['Item'+i].style.display= 'none'
				document.all['opened'].style.display='none'
				document.all['closed'].style.display=''
			}
		}
		if (flag == 'closed')
		{
			flag = 'opened'
		}
		else
		{
			flag = 'closed'
		}
		if (ID != 'all')
		{
			if (document.all['Item'+ID].style.display== '')
			{
				document.all['Item'+ID].style.display='none'
			}
			else if (document.all['Item'+ID].style.display== 'none')
			{

				document.all['Item'+ID].style.display= ''
			}
		}
	 }
