function charConv(strcharConv){
	var aLen = aArray.length;
	var nlength = strcharConv.length;
	var newstr='';
	var flg;
	for(var i=0;i<nlength;i++){
		flg=0;
		for(var j=0;j<aLen;j++){
			if(aArray[j]==strcharConv.charCodeAt(i)){
				newstr = newstr + '&#' + strcharConv.charCodeAt(i) + ';';
				flg = 1;
				break;
			}
		}
		if (flg==0){
			newstr = newstr + strcharConv.charAt(i);
		}
	}
	return newstr;
}

function getEncode(strcharConv){
	var nlength = strcharConv.length;
	var newstr='';
	for(var i=0;i<nlength;i++){
		newstr = newstr + "&#" + strcharConv.charCodeAt(i) + ";"
	}
	return newstr;
}

