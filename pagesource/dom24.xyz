		var extenders = $(this).data('extenders');
		var lastDate = $(this).data('lastDate');
		$.post('/load.php', {
			'offset': offset,
			'parents': parents,
			'extenders': extenders,
			'last_date': lastDate,
			'action': 'load'
		}, function(data) {
			if (data['content'] != '') {
				$('<div class="list-group" style="display:none;">' + data['content'] + '</div>').appendTo($('.ajax-content')).slideDown("fast");
				$('.load_a_ajax').data('lastDate', data['lastDate']);
			}else{
				$(".load_a_ajax").hide();
			}

		}, 'json');
		$('.load_a_ajax').data('offset', offset + 10);

		return false;
	});

