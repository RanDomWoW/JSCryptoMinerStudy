<script type="text/javascript">
    var magazineboxTimerID;
    var magazineboxPanel = $('ul', '#magazineboxCont');
    magazineboxPanel.mouseover(function () {
        stopMagazineSlider();
    });
    magazineboxPanel.mouseout(function () {
        startMagazineSlider();
    });


    function startMagazineSlider() {
        magazineboxTimerID = setInterval(function () {
            var magazineboxItems = $('li', magazineboxPanel);
            var itemFirst = magazineboxItems.first();
            var itemWidth = itemFirst.width();

            magazineboxPanel.animate({ left: '-=' + itemWidth }, 500, function (e) {
                itemFirst.appendTo(magazineboxPanel);
                magazineboxPanel.css("left", "0px");
            });
        }, 3000);
    }

    function stopMagazineSlider() {
        clearInterval(magazineboxTimerID);
    }

    startMagazineSlider();

</script>

