<script>
    function fetchSlider(id, direction, catid, custid) {
        var inprogress = $("#animationinprogress");
        if (inprogress.val() == "1") {
            return false;
        }
        //set animation in progress
        inprogress.val("1");
        //console.log("id: " + id + "\ndirection: " + direction + "\ncatid: " + catid + "\ncustid: " + custid);
        if ($("#slidercurrent").val() != '') {
            //currently performing another slider operation so do nothing
            return false;
        }

        $("#slidercurrent").val(id); //save the slider id
        $("#slidercurrentdirection").val(direction); //save the direction

        var count = ($('#slidercnt_' + id).val()) * 1;
        var start;
        var end;
        if (direction == "right") {
            start = count + 1;
            end = start + 4;
        }
        else {
            start = count - 9;
            end = start + 4;
        }

        if (start < 0) {
            //trying to move past the beginning
            $("#slidercurrent").val('');
            inprogress.val('');
            return false;
        }

        var obj =
            {
                SliderID: id,
                SliderDirection: direction,
                StartRow: start,
                EndRow: end,
                CategoryID: catid,
                CustomerID: custid
            }

        $.ajax({
            type: "POST",
            cache: false,
            url: "/api/AJax/FetchProducts",
            data: JSON.stringify(obj),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                if (data != "0") {
                    //valid data was returned
                    //console.log(data)
                    var prods = JSON.parse(data);
                    if (prods.length != 0) {
                        slide(prods);
                    }
                    else {
                        //there was no data returned so no animation was done
                        $("#animationinprogress").val("");

                        //if no data was returned we must be at the end
                        var id = $("#slidercurrent").val();
                        var rightArrow = $("#prod_right_arrow_" + id);
                        rightArrow.removeClass("prod_right_arrow");
                        rightArrow.addClass("prod_right_arrow_page0");

                    }
                    $("#slidercurrent").val("");
                    $("#slidercurrentdirection").val("");

                } else {
                    //an error occured get new data... do nothing
                    alert('error');
                }
            }
        });
    }

    function clearAnimationInProgress() {
        //clear animation in progress
        $("#animationinprogress").val("0");;
    }

    function slide(prods) {
        var id = $("#slidercurrent").val();
        var direction = $("#slidercurrentdirection").val();

        var containerA = $('#slider_' + id + "_A");
        var containerB = $('#slider_' + id + "_B")
        var content = "#slider_" + id + "_";
        var counter = $('#slidercnt_' + id)
        var count = counter.val();

        var isvisibleA = false;
        var isvisibleB = false;
        //see which container is visible
        //alert('containerA: ' + containerA.css('display') + "\ncontainerB: " + containerB.css('display'));
        if (containerA.css('display') !== 'none') {
            isvisibleA = true;
        }
        else {
            isvisibleA = false;
        }
        isvisibleB = !isvisibleA;

        var show_direction;
        var hide_direction;
        if (direction == "right") {
            show_direction = "right";
            hide_direction = "left";
        }
        else {
            show_direction = "left";
            hide_direction = "right";
        }
        var prod_format = unescapeHtml($("#prod_format").val());

        if (isvisibleA) {
            //console.log('loading B');
            //A is visible so we want to load the new data into B

            if (direction == "left") {
                count = count - 10;
            }
            //console.log("count: " + count);
            for (var mycnt = 1; mycnt <= 5; mycnt++) {
                count = count * 1 + 1;  //the *1 forces JS to treat count as a number
                var prod = prods[mycnt - 1];

                //blank the current contents
                $(content + 'B' + mycnt).html('');

                if (prod != null) {
                    var prod_html = prod_format.replace(/\{0\}/g, prod.LinkTarget)
                        .replace(/\{1\}/g, prod.ImageLocation)
                        .replace(/\{2\}/g, prod.ItemDescriptionShort)
                        .replace(/\{3:C\}/g, formatMoney(prod.Price));

                    $(content + 'B' + mycnt).html(prod_html);

                    var rightArrow = $("#prod_right_arrow_" + id);
                    rightArrow.removeClass("prod_right_arrow_page0");
                    rightArrow.addClass("prod_right_arrow");
                }
                else {
                    //if we're here then the scroller has reached the end of i'ts data.
                    var rightArrow = $("#prod_right_arrow_" + id);
                    rightArrow.removeClass("prod_right_arrow");
                    rightArrow.addClass("prod_right_arrow_page0");
                }
            }

            containerA.toggle("slide", { direction: hide_direction }, 1500, function () { clearAnimationInProgress(); });
            containerB.toggle("slide", { direction: show_direction }, 1500, function () { clearAnimationInProgress(); });
        }
        else {
            //console.log('loading A');

            //B is visible so we want to load the new data into A
            if (direction == "left") {
                count = count - 10;
            }

            //console.log("count: " + count);

            for (var mycnt = 1; mycnt <= 5; mycnt++) {
                count = count * 1 + 1; //the *1 forces JS to treat count as a number
                var prod = prods[mycnt - 1];

                //blank the current contents
                $(content + 'A' + mycnt).html('');

                if (prod != null) {
                    var prod_html = prod_format.replace(/\{0\}/g, prod.LinkTarget)
                        .replace(/\{1\}/g, prod.ImageLocation)
                        .replace(/\{2\}/g, prod.ItemDescriptionShort)
                        .replace(/\{3:C\}/g, formatMoney(prod.Price));
                    $(content + 'A' + mycnt).html(prod_html);

                    var rightArrow = $("#prod_right_arrow_" + id);
                    rightArrow.removeClass("prod_right_arrow_page0");
                    rightArrow.addClass("prod_right_arrow");
                }
                else {
                    //if we're here then the scroller has reached the end of i'ts data.
                    var rightArrow = $("#prod_right_arrow_" + id);
                    rightArrow.removeClass("prod_right_arrow");
                    rightArrow.addClass("prod_right_arrow_page0");
                }
            }

            containerB.toggle("slide", { direction: hide_direction }, 1500, function () { clearAnimationInProgress(); });
            containerA.toggle("slide", { direction: show_direction }, 1500, function () { clearAnimationInProgress(); });
        }
        counter.val(count);
        if (count <= 5) {
            $("#prod_left_arrow_" + id).removeClass('prod_left_arrow');
            $("#prod_left_arrow_" + id).addClass('prod_left_arrow_page0');
        }
        else {
            $("#prod_left_arrow_" + id).addClass('prod_left_arrow');
            $("#prod_left_arrow_" + id).removeClass('prod_left_arrow_page0');
        }
    }

    function unescapeHtml(data) {
        return data
             .replace(/&amp;/g, "&")
             .replace(/&lt;/g, "<")
             .replace(/&gt;/g, ">")
             .replace(/&quot;/g, "\"")
             .replace(/&#039;/g, "'");
    }

    function formatMoney(nStr) {
        nStr += '';
        var x = nStr.split('.');
        var x1 = x[0];

        var x2 = x.length > 1 ? '.' + x[1] : '.00';

        if (x.length > 1) {
            //the number has a decimal component
            x2 = x[1] + '00';  //make sure it's at least 2 digits long
            x2 = '.' + x2.substr(0, 2);
        } else {
            //the number does not have a decimal component
            x2 = '.00';
        }

        var rgx = /(\d+)(\d{3})/;
        while (rgx.test(x1)) {
            x1 = x1.replace(rgx, '$1' + ',' + '$2');
        }
        if (x1 == "") {
            x1 = "0";
        }
        return "$" + x1 + x2;
    }

</script>
