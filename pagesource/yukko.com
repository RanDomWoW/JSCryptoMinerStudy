    <script> 
        window.addEventListener('load', function () {
            var home = window.location.pathname == '/shop/' || window.location.pathname == '/';
            var product = jQuery('.productpage-breadcrumb').length > 0;
            var cart = window.location.href.indexOf('/sepet') != -1;
            var category = jQuery('.listpage-breadcrumb.breadcrumb').length > 0;
            var purchase = window.location.href.indexOf('checkout/success') != -1;
            var id = '';
            var price = '';
            var pagetype = '';
            if (home) {
                var pagetype = "home"
            }
            else if (purchase) {
                var pagetype = "purchace"
            }
            else if (category) {
                var pagetype = "category"
            }
            else if (product) {
                var id = google_tag_params.prodid;
                var pagetype = "product"
                var price = google_tag_params.price;
            }
            else if (cart) {
                var id = google_tag_params.prodid;

                var pagetype = "cart"
                var price = google_tag_params.price;
            }
            else {
                var pagetype = "other"
            }

            google_custom_params = {
                'ecomm_prodid': id,
                'ecomm_pagetype': pagetype,
                'ecomm_totalvalue': price
            }

            try {
                ga('set', 'dimension1', google_custom_params.ecomm_prodid);
            } catch (e) { }
            try {
                ga('set', 'dimension2', google_custom_params.ecomm_pagetype);
            } catch (e) { }
            try {
                ga('set', 'dimension3', google_custom_params.ecomm_totalvalue);
            } catch (e) { }
            ga('send', 'event', 'page', 'visit', google_custom_params.ecomm_pagetype, {
                'nonInteraction': 1
            });
        })
</script>

