    function FvTopSellersSuggest(suggestData) {
        if (typeof suggestData[1] != 'undefined')
            suggestData[1].first = true;

        InitializeTemplateData('/htmlTemplates/VisilabsCategoryBestSellers.html?v=' + globalVersion,
        {
            categories: { upperBest: suggestData[0], rest: [suggestData[1], suggestData[2], suggestData[3]] },
            toFixed: function () {
                return function (num, render) {
                    return parseFloat(render(num)).toFixed(2);
                }
            }
            , index: function () {
                return ++window['INDEX'] || (window['INDEX'] = 1);
            }
            , get_index: function () {
                return window['INDEX'];
            }
            , resetIndex: function () {
                window['INDEX'] = null;
                return;
            },
            floor: function () {
                return function (num, render) {

                    return Math.floor(render(num));
                }
            }
        },
        $('#ph_en_cok_satan_kategoriler'),
        function (data) {

            if (data) {

                $('ul.tabs li').click(function () {
                    var tab_id = $(this).attr('data-tab');

                    $('ul.tabs li').removeClass('current');
                    $('.tab-content').removeClass('current');

                    $(this).addClass('current');
                    $("#" + tab_id).addClass('current');
                });

                // 0 : upper category top sellers div
                // 1,2,3 : category top seller tabs
                for (var i = 0; i <= 3; i++) {

                    var postfix = (i > 0 ? "_" + i : "");

                    // Products count lower than 3, hide this category tab
                    if (i > 0 && $("#en_cok_satan_slider" + postfix).children().length < 3) {
                        $("#tab-" + i).remove();
                        $("li[data-tab='tab-" + i + "']").remove();
                        continue;
                    }

                    // Products count lower than 3, hide upper category best seller div
                    if (i == 0 && $("div.upperBestSellers ul.site_urunler").children().length < 3) {
                        $("div.upperBestSellers").remove();
                        continue;
                    }

                    $("#en_cok_satan_slider" + postfix).flexisel({
                        visibleItems: 5,
                        animationSpeed: 300,
                        autoPlay: false,
                        autoPlaySpeed: 3000,
                        pauseOnHover: true,
                        enableResponsiveBreakpoints: false,
                    });

                }

                if ($("div.tabbedBestSellers div.tab-content").length == 0) {
                    $("div.tabbedBestSellers").remove();
                }

            }


            $("ul.tabs li.tab-link").first().addClass('current');
            $("div.tab-content").first().addClass('current');

            var hideContainer = $('div.en_cok_satan_kategoriler ul.site_urunler').children().length == 0 ||
                $('div.en_cok_satan_kategoriler div.tab-content ul.site_urunler').children().length == 0;

            if (hideContainer)
                $('#ph_en_cok_satan_kategoriler').hide();
        });
    }
</script>





