function center_info_set(value, center_id, map_id) {
    
    if(map_id === undefined){
        map_id = false;
    }
    
    var region = $.grep(data, function (e) {
        return e.value == value;
    });
    var centers = region[0].centers;
    var center = centers[center_id];
    jQuery('#center_info_name').html(center.name);
    jQuery('#center_info_phone').html(center.phone);
    jQuery('#center_info_helpline').html(center.helpline);
    jQuery('#center_info_fax').html(center.fax);
    jQuery('#center_info_email').html(center.email);
    jQuery('#center_info_address').html(center.address);
    jQuery('#center_info_img').attr('src', 'http://admin.standart.uz/files/img/map/' + center.img_thumb);
    if(center.url){
        jQuery('#center_info_link').attr('href', center.url).attr('title', center.url);
    }else{
        jQuery('#center_info_link').removeAttr('href').attr('title', "Bu tashkilotning web sahifasi yo'q");
    }
    
    if(map_id){
        console.log('ttt');
        console.log(map_id);
//        jQuery('#map-select2 option[value="'+center_id+'"]').attr("selected", "selected").change();
        $('#map-select2').val(map_id).trigger('change');
    }
}


