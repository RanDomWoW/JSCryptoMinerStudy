                                        function date_time(id)
                                        {
                                            w = "(GMT+5)";
                                            sp = "&nbsp;&nbsp;&nbsp;";

                                            date = new Date;

                                            year = date.getFullYear();
                                            month = date.getMonth();
                                            months = new Array('01', '02', '03', '04', '05', '06', '07', '08', '09', '10', '11', '12');
                                            d = date.getDate();
                                            day = date.getDay();
                                            days = new Array('Ya', 'Du', 'Se', 'Ch', 'Pa', 'Ju', 'Sh');                                        h = date.getHours();
                                            if (h < 10)
                                            {
                                                h = "0" + h;
                                            }
                                            if (d < 10)
                                            {
                                                d = "0" + d;
                                            }
                                            m = date.getMinutes();
                                            if (m < 10)
                                            {
                                                m = "0" + m;
                                            }
                                            s = date.getSeconds();
                                            if (s < 10)
                                            {
                                                s = "0" + s;
                                            }
                                            result = ' ' + d + '.' + months[month] + '.' + year + '. ' + days[day];
                                            document.getElementById(id).innerHTML = result;
                                            setTimeout('date_time("' + id + '");', '1000');
                                            return true;
                                        }
                                    </script>

