<script>
  
  
  // Cik leils intervāls
  var interval1 = 0,
	  interval2 = 0,
	  timeout = 0,
	  DesktopTmplBgPosition = 0,
	  MobileTmplBgPosition = 0;
  
  //dekstop
  function dekstoPTmeplate() {
	interval1 = setInterval(function(){
	  // pārakstās pēc 4 sek uz jauno
	  DesktopTmplBgPosition = DesktopTmplBgPosition - 649;
	  //pievienojas jaunā vērtība
	  var newDesktopTmplBgPosition =  DesktopTmplBgPosition + "px 0px";
	  // pievieno BG vērtību
	  $(".TemplatePreviewBlock--templates").css("backgroundPosition", newDesktopTmplBgPosition);
	}, 4000);
  };
  //mobile
  function mobileTmeplate() {
	timeout = setTimeout(function(){
	  interval2 = setInterval(function(){
		// pārakstās pēc 4 sek uz jauno
		MobileTmplBgPosition = MobileTmplBgPosition - 159;
		//pievienojas jaunā vērtība
		var newDesktopTmplBgPosition =  MobileTmplBgPosition + "px 0px";
		// pievieno BG vērtību
		$(".TemplatePreviewBlock--mobileTemplates").css("backgroundPosition", newDesktopTmplBgPosition);
	  }, 4000);
	}, 2000);
  }
  dekstoPTmeplate();
  //mobile
  mobileTmeplate()
	
	// If not on focus
	$(window).blur(function() {
	  clearInterval(interval1);
	  clearInterval(interval2);
	  clearTimeout(timeout);
	});
  $(window).focus(function() {
	clearInterval(interval1);
	clearInterval(interval2);
	clearTimeout(timeout);
	dekstoPTmeplate();
	mobileTmeplate();
  });
</script>

