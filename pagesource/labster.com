    var titles = [
        {
            key : 'learner_profile',
            title: 'My Profile'
        },
        {
            key : 'account_settings',
            title: 'Account Settings'
        },
        {
            key : 'logout',
            title: 'Logout'
        }
    ];
    /**
     * Parses document.cookie and returns value for the given cookie.
     */
    var getCookie = function(cookieName) {
        return document.cookie.split(';').map(function(x) {
            return x.trim().split(/(=)/);
        }).reduce(function(a, b) {
            a[b[0]] = a[b[0]] ? a[b[0]] + ', ' + b.slice(2).join('') : b.slice(2).join('');
            return a;
        }, {})[cookieName];
    };
    /**
     * Parses edx's user info cookie and returns it as an Object.
     */
    var getEdXUserInfo = function() {
        var cookie = getCookie('prod-labster-user-info');
        if (cookie) {
            return JSON.parse(
                cookie.slice(1, -1).replace(/\\054/g, ',').replace(/\\/g, '')
            );
        }
    };
    /**
     * Formats the given string with the given context.
     */
    var sprintf = function(str, context) {
        return str.replace(/%\((.+?)\)s/g, function(expr, param) {
            if (Object.hasOwnProperty.call(context, param)) {
                return context[param];
            }
            return expr;
        });
    };
    /**
     * Returns data for the edx menu items.
     */
    var getMenuItemsData = function(items) {
        return titles.reduce(function(a, b) {
            a[b.title] = items[b.key];
	    return a;
        }, {});
    };

    var buildSubmenu = function(items) {
        var element = document.createElement('ul');
        element.classList.add('edx-account-submenu');
        var content = Object.keys(items).map(function(key) {
            var context = {
                title: key,
                link: items[key]
            }
            return sprintf('<li><a href="%(link)s">%(title)s</a></li>', context);
        }).join('');
        element.innerHTML = content;
        return element;
    };

    Array.prototype.forEach.call(document.getElementById('top-menu').children, function(child) {
        var pattern = /^\[=(.+)\|(.+)=\]$/;
        var link = child.querySelector('a');
        var match = pattern.exec(link.textContent.trim());

        if (match) {
            var userInfo = getEdXUserInfo();
            if (userInfo) {
                var items = getMenuItemsData(userInfo.header_urls);
                link.textContent = match[2];
                child.appendChild(buildSubmenu(items));
            } else {
                link.textContent = match[1];
            }
            link.style.cssText = [
                'background: white',
                'padding: 8px 12px 10px',
                'border-radius: 3px',
                'color: black',
            ].join(';');
