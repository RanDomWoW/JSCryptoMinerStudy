        function mobileDropdown() {
            var isMobile = window.mobileAndTabletcheck();

            if (isMobile) {
                $('.container').css('margin', '0');

                var main_toggle = $('.tb-megamenu-item.dropdown');
                var main_toggle_link = main_toggle.find('a').first();

                main_toggle.on('touchstart', function (e) {
                    e.preventDefault();

                    if ($(this).hasClass('open')) {
                        closeDropDown($(this), e);
                    } else {
                        openDropDown($(this), e);
                    }
                });

                $(document).on('click', function () {
                    if ($('.tb-megamenu-item.dropdown').hasClass('open')) {
                        $('.tb-megamenu-item.dropdown').removeClass('open');
                    }
                });
            }

            var openDropDown = function (elem, e) {
                $('.tb-megamenu-item.dropdown').removeClass('open');
                elem.addClass('open');
            };

            var closeDropDown = function (elem, e) {
                if ($(e.target).is('a, a *')) {
                    window.location.href = e.target;
                    return;
                }

                if ($(e.target).not('li.tb-megamenu-item')) {
                    return;
                }
                elem.removeClass('open');
            };
        }

        
        $(function(){
            mobileDropdown();
        });

