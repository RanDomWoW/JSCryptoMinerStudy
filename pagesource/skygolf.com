tDictionary() {\r\n        var args = [0];\r\n        skyxdm_Post(\"courses.getNameDictionary\", args, web_build_course_success, web_build_list_error);\r\n    }\r\n\r\n    function getSuggest() {\r\n        if (typeof(words)=='undefined') return;\r\n        var arr = $(\"#sfc_container #course_name\").val().split(' ');\r\n        var word = arr[arr.length-1].toLowerCase();\r\n        var c = word.substring(0,1);\r\n        var i = 0;\r\n        var tmp = [];\r\n\r\n        $('#word-list').html('');\r\n        autoText = '';\r\n        if (typeof(words[c])=='undefined') return;\r\n        $.each(words[c], function(k,v) {\r\n            if ((k.indexOf(word)==0)&&(k.length>=word.length)) {\r\n                tmp.push(k);\r\n                i++;\r\n                if (i>=5) return false;\r\n            }\r\n        });\r\n\r\n        tmp.sort();\r\n        $.each(tmp, function(k,v) {\r\n            if (k==0) {\r\n                autoText = v;\r\n                suggestText = v;\r\n                $('#word-list').append('<div class=\"lineItem currentLineItem\"  onclick=\"\">'+v+'<\/div>');\r\n            } else {\r\n                if ((k==1)&&(suggestText.length<=2)) {\r\n                    suggestText = v;\r\n                }\r\n                $('#word-list').append('<div class=\"lineItem\"  onclick=\"\">'+v+'<\/div>');\r\n            }\r\n        });\r\n\r\n        if (i>0) {\r\n            var left = $(\"#sfc_container #course_name\").position().left+$(\"#sfc_container #course_name\").val().length*5;\r\n            if ($(\"#sfc_container #course_name\").val().length*5 > ($(\"#sfc_container #course_name\").width()-20)) {\r\n                left = $(\"#sfc_container #course_name\").width()-20;\r\n            }\r\n            $('#word-list').css({\"top\":$(\"#sfc_container #course_name\").position().top+$(\"#sfc_container #course_name\").height()+12,\"left\":left});\r\n            $('#word-list').show();\r\n            $('#resetButton').attr(\"for\", \"course_name\");\r\n            $('#resetButton').addClass('course');\r\n            $('#resetButton').show();\r\n        }\r\n    }\r\n\r\n\r\n    \/\/ Address\r\n    function web_build_address_success(r) {\r\n        $('#address-list').html('');\r\n        arrAddress = r;\r\n        for (var i=0; i < r.length; i++) {\r\n            $('#address-list').append('<div class=\"lineItem\" onClick=\"\">' + r[i] + '<\/div>');\r\n        }\r\n        if (r.length > 0) {\r\n            $('#address-list').show();\r\n        }\r\n        \/\/$('input[name=location]').removeClass('loadingAddress');\r\n        $('#resetButton').attr(\"for\", \"location\");\r\n        $('#resetButton').addClass('location');\r\n        $('#resetButton').show();\r\n    }\r\n\r\n    function doBuildAddressList() {\r\n        var args = new Array($(\"#sfc_container #location\").val());\r\n        skyxdm_Post(\"courses.getAddressList\", args, web_build_address_success, web_build_list_error);\r\n    }\r\n\r\n    function buildAddressList() {\r\n        $('#resetButton').hide();\r\n\r\n        if ((arrAddress.length==0) || ((arrAddress.length > 0) && (arrAddress[0].toLowerCase().indexOf($('input[name=location]').val().substring(0,3).toLowerCase()) < 0))) {\r\n            doBuildAddressList();\r\n        } else {\r\n            var check = false;\r\n            $('#address-list').html('');\r\n            for (var i=0; i < arrAddress.length; i++) {\r\n                if (arrAddress[i].toLowerCase().replace(',','').trim().indexOf($('input[name=location]').val().trim().toLowerCase())>=0) {\r\n                    $('#address-list').append('<div class=\"lineItem\" onclick=\"\">'+arrAddress[i]+'<\/div>');\r\n                    check = true;\r\n                }\r\n            }\r\n            if (check) {\r\n                $('#address-list').show();\r\n            } else {\r\n                doBuildAddressList();\r\n            }\r\n        }\r\n    }\r\n\r\n    var user_id = null;\r\n\tfunction showUserInfo() {\r\n\t\t\/\