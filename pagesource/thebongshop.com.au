      </select> 
      <select name="bday" id="bday" value="1">
        
      </select>
      <select name="byear" id="byear" value="1950">
        
      </select>
      <div style="clear:both; margin-bottom:15px"></div>
    </div>
    <button id="submit_birthdate" class="button-enter btn styled-submit" onclick="ageCheck()" style="display:inline-block">Enter - Yes I am 18+</button> 
