	mainImageChange(5000,10000);
});
var clsShow = "mv02";
function mainImageChange(speed, interval){
	if(clsShow == "mv02"){
		$(".main_visual .mv02").fadeOut(speed);
		$(".main_visual .mv01").fadeIn(speed);
		clsShow = "mv01";
	}else{
		$(".main_visual .mv01").fadeOut(speed);
		$(".main_visual .mv02").fadeIn(speed);
		clsShow = "mv02";
	}
	setTimeout(function(){mainImageChange(speed,interval)},interval);
}
</script>
