<script type="text/javascript">
$(document).ready(function(){
	$(".dropdown a").click(function(){
		var menuType = $(this).attr("menuType");
		var groupId = $(this).attr("groupId");
		var pageId = $(this).attr("pageId");
		var menuId = $(this).attr("menuId");
		var link = $(this).attr("link");
		if(menuType == "MENU_TYPE-0-01"){
			goMenuPage("/board/boardList.do",groupId, pageId, menuId);
		}
		else if(menuType == "MENU_TYPE-0-02"){
			goMenuPage("/contents/contentsPage.do",groupId, pageId, menuId);
		}
		else if(menuType == "MENU_TYPE-0-04"){
			goMenuPage(link,groupId, pageId, menuId);
		}
		else{
			window.open(link);
		}
	});
	
	$(".DB_main>a").click(function(){
		if($(this).parent().hasClass("m05")){
			return;
		}
		var firstChild = $(this).siblings(".dropdown").find("li:first>a");
		var menuType = firstChild.attr("menuType");
		var groupId = firstChild.attr("groupId");
		var pageId = firstChild.attr("pageId");
		var menuId = firstChild.attr("menuId");
		var link = firstChild.attr("link");
		if(menuType == "MENU_TYPE-0-01"){
			goMenuPage("/board/boardList.do",groupId, pageId, menuId);
		}
		else if(menuType == "MENU_TYPE-0-02"){
			goMenuPage("/contents/contentsPage.do",groupId, pageId, menuId);
		}
		else if(menuType == "MENU_TYPE-0-04"){
			goMenuPage(link,groupId, pageId, menuId);
		}
		else{
			window.open(link);
		}
	});
	
	$("#btnSearch").click(function(){
		var groupId = $(this).attr("groupId");
		var pageId = $(this).attr("pageId");
		var menuId = $(this).attr("menuId");
		goSearchPage($("#searchKeyword").val(), groupId, pageId, menuId);
	});
	
});
</script>






