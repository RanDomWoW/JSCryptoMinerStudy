<script>
	require(['jquery', 'domReady!'], function($) {
		//local vars
		var clicked = "";
		var root = $(".regionMap");
		var button = root.find($(".regionArea"));
		var width = $(window).innerWidth();

		// Check if the media query is activated
		if (width > 640){
		  $( ".regionPopup" ).first().addClass( "active" );
		}	

		button.click(function() {
			//get pos and clip regionButton Class
			clicked = ($(this).data("position"));
			clicked = ".regionPopup." + clicked;

			//add active class and remove active from previously active class
			//this will remove active from ALL other buttons and popups
			$(this).siblings().removeClass( "active");
			$(clicked).siblings().removeClass( "active");
			$(this).addClass( "active" );
			$(clicked).addClass( "active" );
		});
		//if closed button clicked remove active class from button and popup
		$(".closeButton").click(function() {
			$(clicked).removeClass( "active" );
		});
	});
</script></div>
