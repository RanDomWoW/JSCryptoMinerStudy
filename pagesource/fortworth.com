		function navTranslate() {
			var wrapper = $('.translateWrapper .navTranslate');
			var dropdown = $('.translateWrapper .navTranslate .translate-dropdown');
			var arrow = $('.translateWrapper .navTranslate .translate-top');

			arrow.click(function() {
				wrapper.toggleClass('open');
				dropdown.slideToggle();
			});

			$(document).mouseup(function(e) {
				if (!wrapper.is(e.target) && wrapper.has(e.target).length === 0) {
					dropdown.slideUp();
					$(wrapper).removeClass('open');
				}
			});
		}

		function navTranslateMobile() {
			var wrapper = $('.translateWrapper-mobile .navTranslate');
