function encode_slno(estamp_slno)
{

	var orgval=estamp_slno;
	var encoded = "";
	for (i=0; i<orgval.length;i++) {
	var a = orgval.charCodeAt(i);
	var b = a ^ 123;    // bitwise XOR with any number, e.g. 123
	encoded = encoded+String.fromCharCode(b);
	}
	return encoded;
}
function verifyEstamp()
{
    var estamp_slno = $('#estamp_slno').val();
	estamp_slno = encode_slno(estamp_slno);
	window.location.href="http://keralaregistration.gov.in/pearlpublic/qrcode_verify.php?estamp_slno="+encodeURIComponent(estamp_slno);
	//window.location.href="http://10.5.9.147/regusr/public_html/pearlpublic/qrcode_verify.php?estamp_slno="+encodeURIComponent(estamp_slno);
