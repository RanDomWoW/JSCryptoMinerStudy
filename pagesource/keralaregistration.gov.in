								//document.getElementById('divMessage').style.display="block";	
								document.getElementById('divStatus').style.display="block";											 
								document.getElementById('mesgContentNotice').innerHTML = caption+ " Required"
								
                                 obj.focus();
                                 return false;
                    }
                if(_isTrim(obj.value)=="" && (Mandatory === undefined || Mandatory==null) ){

                           return true;
                    }
                    else
                        {
   for (i = 0; i < sText.length && IsNumber == true; i++)
   {
   	Char = sText.charAt(i);
    if (ValidChars.indexOf(Char) == -1)
    {
		//alert("Enter Integer Values for  " + caption);
