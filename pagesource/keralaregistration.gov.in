function _isTrim(_str){
	  var  str=String(_str);
		if(!str || typeof str != 'string')        return str;return str.replace(/^[\s]+/,'').replace(/[\s]+$/,'').replace(/[\s]{2,}/,' ');}


//========Number of occurence of a string=========string.count('/') will return 2=========working fine used in _isDate=================================
	String.prototype.count=function(s1) {
return (this.length - this.replace(new RegExp(s1,"g"), '').length) / s1.length;
}


	function _isEmpty(obj,caption) {
	  var  sText = _isTrim(obj.value);
          //alert(obj.value);
	  var str=_isTrim(sText);
	
		var re = /.+/;		
		if(!str.match(re)) {
			//alert(caption + " Required");
			document.getElementById('divStatus').style.display="block";
			 document.getElementById('divSuccess').style.display="none";
		 document.getElementById('divMessage').style.display="none";											 
			document.getElementById('mesgContentNotice').innerHTML = caption
			obj.focus();
			return false;
		} 		
		else
			return true;
		
	}
	
function _isNumber(obj,caption,Mandatory)
{
    sText=_isTrim(obj.value);
   var ValidChars = "0123456789";
   var IsNumber=true;
   var Char;
   sText=(sText);
        if(_isTrim(obj.value)=="" && (Mandatory !== undefined && Mandatory!=null))
