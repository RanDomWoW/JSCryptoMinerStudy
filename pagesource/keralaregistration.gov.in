function loadContents(Name){ //alert("1Name: "+Name);
        var menus = Name.substring(0,3); //alert("menuz ::"+menus);
        if(menus=='not'){
            var menu = 'notification'; //alert("menu ::"+menu);
            loadXMLDocGO(Name);
            loadMenupath(menu);
        }
        else if(menus=='faq'){
            var menu = 'faq'; //alert("menu ::"+menu);
            loadXMLDocFAQ(Name);
            loadMenupath(menu);
        }
        else{
            loadXMLDoc(Name);        
            loadMenupath(Name);
        }
            
}

