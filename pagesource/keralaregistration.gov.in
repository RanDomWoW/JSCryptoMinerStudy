<script>
    function searchGO(val){
        var ser = document.getElementById('columns').value; //alert(ser);
        
        if(ser=='Select Your Option'){
            alert("Choose Your Search Option");
            return false;
        }
        var ajaxRequests = getHTTPObject();
        ajaxRequests.onreadystatechange = function()
        {
            if(ajaxRequests.readyState == 4)
            {
                     results=ajaxRequests.responseText;
                     //alert(results);
                     if(results!="" || results!=null)
                      {
                            document.getElementById('gos').innerHTML='';
                            document.getElementById('gos').innerHTML=results;
                      }
            }

        }
        if(val==''){
            alert("Invalid Entry");
            return false;
        }
       
        if(ser==''){
            alert("Invalid Entry");
            return false;
        } 
        var queryString = "?opt=3&val="+val+"&ser="+ser;
        ajaxRequests.open("GET", "xml/xmlCommonFunctions.php" + queryString, true);
        ajaxRequests.send(null);
    }
</script>
