                                                            <script type="text/javascript">
                        var advise;
                        var loadedAdviser = false;

                        advise = function (sAction) {
                            window.loadedAdviser = true;
                            var sUrl = '';
                            switch (sAction) {
                                case 'close':
                                    sUrl = 'https://www.novaturas.lt/paieska//index/adviser/close';
                                    $("#adviceInfo2").hide();
                                    break;
                                default:
                                    sUrl = 'https://www.novaturas.lt/paieska//index/adviser/start?ConId=6&scode=NNOV';
                            }

                            var timeoutId;

                            $('#adviser').load(encodeURI(sUrl),
                                    {
                                        'callUrl': sUrl
                                    },
                                    function () {

                                        var topSearchContainer = $('#top-search-container'),
                                            adviceInfo = $('.advice-info'),
                                            initAdviserTop = parseInt(adviceInfo.css('top')),
                                            topContainerHeight = topSearchContainer.offset().top + topSearchContainer.outerHeight();

                                        adviceInfo.css('top', topContainerHeight);
                                        $(window).scroll(function() {
                                            if (topContainerHeight - initAdviserTop > $(window).scrollTop()) {
                                                adviceInfo.css('top', initAdviserTop + ((topContainerHeight - initAdviserTop) -$(window).scrollTop()));
                                            } else {
                                                adviceInfo.css('top',  initAdviserTop);
                                            }
                                        });


                                        $("#adviceInfo2").mouseleave(function () {
                                            adviceTime = setTimeout(function () {
                                                clearTimeout(adviceTime);
                                                $('#adviceInfo2').hide();
                                            }, 500);
                                        });
                                        $("#adviceInfo2").mouseenter(function () {
                                            try {
                                                clearTimeout(adviceTime);
                                            } catch (err) {
                                            }
                                        });

                                        $("#adviceInfo").mouseenter(function () {
                                            try {
                                                if (!timeoutId) {
                                                    timeoutId = window.setTimeout(function () {
                                                        timeoutId = null;
                                                        $("#adviceInfo2").show('normal');
                                                    }, 1000);
                                                }
                                            } catch (err) {
                                            }
                                        });

                                        $("#adviceInfo").mouseleave(function () {
                                            if (timeoutId) {
                                                window.clearTimeout(timeoutId);
                                                timeoutId = null;
                                            }
                                        });

                                        $("#adviceInfo").click(function () {

                                            if ($("#adviceInfo2").is(':visible')) {
                                                $("#adviceInfo2").hide('normal');
                                            } else {
                                                $("#adviceInfo2").show('normal');
                                            }
                                            if (timeoutId) {
                                                window.clearTimeout(timeoutId);
                                                timeoutId = null;
                                            }
                                        });
                                    });
                        }
                    </script>
