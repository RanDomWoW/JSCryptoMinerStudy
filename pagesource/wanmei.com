	$.fn.fadeSlide=function(){
		var totalslice=$(this).find("li").length;
		var nowslice=0;
		var timeout=4000;
		var speed=1000;
		
		var dothtml='<ol class="dots clearfix">'
		for(var i=0;i<totalslice;i++){
			if(i==0){dothtml+='<li class="dot active"></li>';}
			else{dothtml+='<li class="dot"></li>';}
			}
		dothtml+='</ol>';
		$(this).after(dothtml);
		var interval=setInterval(nextSlice,timeout);
		
		function nextSlice(index){
			if(totalslice<2)return;
			var newSlice;
			if(index){
				newSlice=index-1;
				}
			else{
				newSlice=nowslice+1;
				}
				
			if(newSlice>=totalslice)newSlice=0;
			$(".bgpicscroll ol li:eq("+newSlice+")").addClass("active").siblings(".active").removeClass("active");
			$(".bgpicscroll ul li:eq("+nowslice+")").fadeOut(speed);
			$(".bgpicscroll ul li:eq("+newSlice+")").fadeIn(speed);
			
			nowslice=newSlice;
			}
		$(".bgpicscroll ol li").click(function(){
			if($(this).hasClass("active"))return;
			nextSlice($(this).index()+1);
			clearInterval(interval);
			interval=setInterval(nextSlice,timeout);
			})
		
		}
	Array.prototype.shuffle = function() {
		var input = this;
		for (var i = input.length-1; i >=0; i--) {
			var randomIndex = Math.floor(Math.random()*(i+1));
			var itemAtIndex = input[randomIndex];
			input[randomIndex] = input[i];
			input[i] = itemAtIndex;
		}
		return input;
	}
