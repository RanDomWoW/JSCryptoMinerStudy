<script>
$(document).ready(function()
{
var $top1= $('#floatingbar').offset().top + 1;   
var $mid1 =  Math.floor($(window).height() / 1);
$('#dirbutton').data('mode','scrollup');
$('#dirbutton').click(function()
{
   $mode = $(this).data('mode');
   switch($mode)
   {
     case 'scrolldown':
	     $('html, body').animate({scrollTop:0}, 'slow');
		 return false;
		 break;
	 case 'scrollup':
	    $scrollpos = $('body').outerHeight();
	    $('html, body').animate({scrollTop:$scrollpos}, 'slow');
		return false;
		break;
   }
});
$(window).scroll(function()
{   
		if ($(window).scrollTop()>$top1)   
		{
		 $('#floatingbar').addClass('floater');
		}
		else
		{
		 $('#floatingbar').removeClass('floater');
		 }
		 if($(window).scrollTop() > $mid1)
		  {
		     $('#dirbutton').data('mode','scrolldown');
		     $('#dirbutton').show();
		  }else
		  {	    
			 $('#dirbutton').data('mode','scrollup');
		  }
});
});
</script>
