    </script>



    <script>
        var shortClubListCookieName = 'Goodlife.ClubShortlist';
        /*
        Select Club is used to select a club from a button click or some other similar event. Issues with iOS setting cookies and server redirects has made this more complicated than it needed
        to be. It does the following:
        1. Get the current cookie short club list.
        2. Talk to the server (via MoreClubsOverlay/SelectClub). It passes the selected club id plus the current cookie list. The server will return the updated cookie list (it doesn't set it!)
        3. Set the cookie for the new short club list.
        4. Tak appropriate action which could be redirect back to the club page or simply refresh the current page.
        */
        function selectClub(id, redirectToClubPage, hashLoc) {

            // get the current cookie
            var cookieValue = $.cookie(shortClubListCookieName) && $.cookie(shortClubListCookieName).toLowerCase();
            if (!cookieValue)
                cookieValue = "";

            // prepare the url we want to call to let the server we'd like to change the club.
            var href = "/Goodlife/MoreClubsOverlay/SelectClub?clubId=" + id + "&currentClubs=" + cookieValue;
            // make the ajax call to the server
            $.ajax({
                type: "GET",
                url: href,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data) {
                    // set the cookie to the latest club shortlist
                    $.cookie.raw = true;
                    $.cookie(shortClubListCookieName, data, { path: '/', domain: 'goodlifehealthclubs.com.au', expires: 1000 });
                    console.log("data=" + data);
                    console.log("redirectToClubPage=" + redirectToClubPage);
                    var ecom = '';

                    if (ecom !== "true") {
                        // determine what we want to do now
                        switch (redirectToClubPage) {
                            // we want to move to the club page
                            case 0:
                                // determine the url we would like to use to redirect to a club. We call to the server because we only have a club id and we need to translate to a club name type url.
                                href = "/Goodlife/MoreClubsOverlay/MoveToClub?clubId=" + id;
                                window.location.href = href;
                                break;
                                // we want to simply close the overlay (this option has not been tested yet!)
                            case 1:
                                closeOverlay();
                                break;
                                // we want to simply reload this page
                            case 2:
                                //window.location.reload();
                                var currentpage = document.location.pathname;

                                window.location = currentpage + '?club=' + id;

                                break;
                                // we want to reload the page with a hash
                            case 3:
                                window.location.hash = hashLoc;
                                window.location.reload();
                                break;

                            case 4:
                                href = "/Goodlife/MoreClubsOverlay/MoveToEcom?clubid=" + id;
                                window.location.href = href;
                                break;
                        }
                    } else {
                        href = "/Goodlife/MoreClubsOverlay/MoveToEcom?clubid=" + id;
                        window.location.href = href;
                    }
                },
                error: function (result) {
                    location.href = "/ServerError.html";
                },
                timeoutNumber: 120000 // 120 secs
            });

        }

        /* This will check the current url. If it looks like its a clubs or clubs subpage, e.g. facilities, we need to determine the club, set the cookie appropriately and refresh the page
        */
        function checkClubPage() {

            var currentHref = window.location.href;

            // work out the club name from the url. If it doesn't contain the word clubs in it, then we don't need to check the url further.
            var clubName = '';
            if (typeof currentHref != 'undefined') {
                var parts = currentHref.split('/');
                var index = parts.indexOf("clubs");
                if (index == -1)
                    return;
                clubName = parts[index + 1];
            }

            clubName = clubName.split("#")[0];

            // if we couldn't get the club name then nothing more to be done.
            if (clubName == "")
                return;

            if (!navigator.cookieEnabled) {
                //alert('You need to enable cookies to get the most from this website.');
                $(".cookies-modal").removeClass("hidden");
                return;
            }

            // get the current cookie. If we don't have one we just set to "" as a starting value
            var cookieValue = $.cookie(shortClubListCookieName) && $.cookie(shortClubListCookieName).toLowerCase();
            if (typeof cookieValue == 'undefined')
                cookieValue = "";

            // We will need to call the server to get cookie information. We tell it the name of the club and it will adjust the cookie.
            // In this case we may have been able to set cookie in the controller but want to keep consistent with how the selectClub function (especially for use with iOS).
            var href = "/Goodlife/MoreClubsOverlay/SelectClubByName?clubName=" + clubName + "&currentClubs=" + cookieValue;
            // call the server.
            $.ajax({
                type: "GET",
                url: href,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data) {
                    // if the cookie data is different then we need to reload the page.
                    if ((typeof cookieValue == 'undefined') || (data != cookieValue)) {
                        // update the cookie.
                        $.cookie.raw = true;
                        $.cookie(shortClubListCookieName, data, { path: '/', domain: 'goodlifehealthclubs.com.au', expires: 1000 });
                        // refresh the page to see information for this club
                        window.location.reload();
                    }
                },
                error: function (result) {
                    location.href = "/ServerError.html";
                },
                timeoutNumber: 120000 // 120 secs
            });
        };
    </script>

</body>
