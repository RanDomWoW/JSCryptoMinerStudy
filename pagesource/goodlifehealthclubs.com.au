<script>

    // Set Main Club function
    function SetMainClub(clubId, clubName) {
        var shortClubListCookieName = 'Goodlife.ClubShortlist';
        var cookieValue = $.cookie(shortClubListCookieName);
        if (!cookieValue) {
            cookieValue = "";
        }

        var clubs = cookieValue.split(',');
        var clubIndexOf = clubs.indexOf(clubId);
        
        if (clubIndexOf != -1) {
            clubs.splice(clubIndexOf, 1);
        }
        clubs.unshift(clubId);
        
        $.cookie(shortClubListCookieName, clubs, { path: '/', domain: 'goodlifehealthclubs.com.au', expires: 1000 });

        // now we need to move to the selected page. would have been nice to set cookie and redirect in the controller but Safari iOS didn't work properly.
        var href = "/Goodlife/ClubMenu/MoveToClub?clubId=" + clubs[0] + "&contextItemId=" + '{A92E458C-4C01-491B-9FE6-124D76F7883C}';
        window.location.href = href;
    }

    // Remove Club function
    function RemoveClubFromMenu(clubId) {
        var shortClubListCookieName = 'Goodlife.ClubShortlist';
        var cookieValue = $.cookie(shortClubListCookieName);
        if (!cookieValue) {
            cookieValue = "";
        }

        var clubs = cookieValue.split(',');
        var clubIndexOf = clubs.indexOf(clubId);
        clubs.splice(clubIndexOf, 1);

        if (clubs.length > 0) {
            $.cookie(shortClubListCookieName, clubs, { path: '/', domain: 'goodlifehealthclubs.com.au', expires: 1000 });

            // now we need to move to the selected page. would have been nice to set cookie and redirect in the controller but Safari iOS didn't work properly.
            if (clubIndexOf === 0) {
                var href = "/Goodlife/ClubMenu/MoveToClub?clubId=" + clubs[0] + "&contextItemId=" + '{A92E458C-4C01-491B-9FE6-124D76F7883C}';
                window.location.href = href;
            }
        } else {
            $.removeCookie(shortClubListCookieName, { path: '/', domain: 'goodlifehealthclubs.com.au' });
            //document.location.href = "/";
        }
    }

</script>
                
