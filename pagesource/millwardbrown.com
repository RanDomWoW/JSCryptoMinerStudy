                //tabs[currentTab] = window.location.pathname;
                tabs[currentTabUrl] = currentTab;
                localStorage.setItem("tabs", JSON.stringify(tabs));
            } else {
                //$(".tab-main a")[0].href = window.location.pathname;
                var first = $($(".tab-main a")[0]);
                first.attr('href', '/');
                first.parent().addClass("current");
            }
            var tabsString = JSON.stringify(tabs);
            if (tabsString.split(",").length > 6) {
                //why substring appends closing bracket
                localStorage.setItem("tabs", "{" + tabsString.substring(tabsString.indexOf(",") + 1));
            }
            tabs = JSON.parse(localStorage.getItem("tabs"));
            for (var tab in tabs) {
                var newLI = document.createElement("li");
                if (tab == currentTabUrl) newLI.className = "current";
                ul.appendChild(newLI);
                newLI.innerHTML = "<a href=\"" + tab + "\" title=\"" + tabs[tab] + "\" ><strong>" + tabs[tab] + "</strong></a>" + "<span class=\"icon-close\" id=\"" + tab + "\"></span>";
            }

        }
        if ($('#tabs li').length != 0 && !$(".tab-main").hasClass("current")) {
            $(".tabs-close-btn").attr("style", "display:inline;");
        }
        $('.tabs-close-btn').click(function () {
            var newTabs = {};
            var localTabs = localStorage.getItem("tabs").replace('{', "").replace('}', "").split(",");
            localTabs.splice($('#tabs li.current').index(), 1);
            localTabs.forEach(function (entry) {
                var entrySplit = entry.split(":");
                newTabs[entrySplit[0].split('"').join("")] = entrySplit[1].split('"').join("");
            });
            localStorage.setItem("tabs", JSON.stringify(newTabs));
            window.location = $(".tab-main a")[0].href;
        });
        $('#tabs .icon-close').click(function () {
            var id = $(this).attr("id");
            id = $('<div/>').text(id).html();
            var newTabs = {};
            var localTabs = localStorage.getItem("tabs").replace('{', "").replace('}', "").split(",");
            localTabs.forEach(function (entry) {
                if (!entry.startsWith("\"" + id)) {
                    var entrySplit = entry.split(":");
                    newTabs[entrySplit[0].split('"').join("")] = entrySplit[1].split('"').join("");
                }
            });
            localStorage.setItem("tabs", JSON.stringify(newTabs));
            if ($(this).parent().hasClass("current")) {
                window.location = $(".tab-main a")[0].href;
            } else {
                $(this).parent().remove();
                resizeHandler();
            }
        });
    });
