$(function(){
	$('.form-busca').submit(function(){
		if(document.getElementById("s").value =="Digite sua busca aqui..."){
			document.getElementById("s").focus();
			return false
		}
	});
	$('input[placeholder]').each(function(){
		var ph=$(this).attr('placeholder');
		if($(this).val()=='' || $(this).val()==undefined){
			$(this).val(ph);
		}
		$(this).focus(function(){
			if($(this).val()==ph){
				$(this).val('');
			}
		}).blur(function(){
			if($(this).val()=='' || $(this).val()==undefined){
				$(this).val(ph);
			}
		});
	});
});
</script>

