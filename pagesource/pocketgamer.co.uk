<script type="text/javascript">
		
	function dc_wide_slide(what, change, check)
	{
		if ($(what).hasClass("off")) return;
		$(what).siblings().removeClass("off");
		
		dc_page = dc_page + change;
		$dc_container.animate({marginLeft: -((dc_page - 1) * dc_width) + "px"});
		var index = $dc_widget_wide.find("nav span img.on").removeClass("on").index();
		$dc_widget_wide.find("nav span img").eq(index + change).addClass("on");
		if (dc_page == check)
			$(what).addClass("off");
	}
		
	var $dc_widget_wide = $(".dc_widget.dc_wide");
	if ($dc_widget_wide.length > 0)
	{
		var dc_page = 1;
		var dc_pages = Math.ceil($dc_widget_wide.find(".dc_container .dc_article").length / 3);
		var dc_width = $dc_widget_wide.width() + 1;
		var $dc_container = $dc_widget_wide.find(".dc_container");
		
		if (dc_pages > 1) 
		{
			$dc_widget_wide.find("nav .dc_right").removeClass("off");
			$dc_widget_wide.find("nav").prepend( $("<span>") );
			for (i = 0; i < dc_pages; i++)
				$dc_widget_wide.find("nav span").append( $("<img>").attr("src", "images/devcorner_dot_5x5.png") );
			$dc_widget_wide.find("nav span img").first().addClass("on");
		}
			
		$dc_widget_wide.find("nav .dc_left").click(function(e) {
			dc_wide_slide(this, -1, 1);
		});
		
		$dc_widget_wide.find("nav .dc_right").click(function(e) {
			dc_wide_slide(this, 1, dc_pages);
		});
	}
	
	
	function dc_slide(index)
	{
		var article_offset = $dc_contents.find(".dc_article").eq(index).offset().top;
		var offset = parseInt($dc_contents.css("margin-top").replace("px", ""));
		new_offset = offset - (article_offset - dc_offset);
		dc_animating = true;
		$dc_contents.animate({marginTop: new_offset + "px"}, function(){		
			if (new_offset != 0)
				$dc_widget.find("nav .dc_left").removeClass("off");
			else
				$dc_widget.find("nav .dc_left").addClass("off");
			
			var $last = $dc_contents.find(".dc_article:last");
			if ($last.offset().top + $last.height() < dc_bottom)
				$dc_widget.find("nav .dc_right").addClass("off");
			else
				$dc_widget.find("nav .dc_right").removeClass("off");
			dc_animating = false;
		});
	}
	
	var $dc_widget = $(".dc_widget").not(".dc_wide");
	if ($dc_widget.length > 0)
	{
		var $dc_container = $dc_widget.find(".dc_container");
		var $dc_contents = $dc_container.find(".dc_contents");
		var dc_height = $dc_container.height();
		var dc_offset = $dc_container.offset().top;
		var dc_bottom = dc_height + dc_offset;
		var dc_index = 0;
		var dc_animating = false;
		
		$dc_container.find(".dc_article").each(function(index, element) {
			if ($(this).offset().top > dc_bottom)
			{
				$dc_widget.find("nav .dc_right").removeClass("off");
				return false;
			}
		});
		
		// slide up
		$dc_widget.find("nav .dc_left").click(function(e) {
			if ($(this).hasClass("off") || dc_animating) return;
			var $current = $dc_contents.find(".dc_article").eq(dc_index);
			$dc_container.find(".dc_article").each(function(index, element) {
                if ($current.offset().top - $(this).offset().top < dc_height)
				{
					dc_index = index;
					return false;
				}
            });
			dc_slide(dc_index);
		});
		
		// slide down
		$dc_widget.find("nav .dc_right").click(function(e) {
			if ($(this).hasClass("off") || dc_animating) return;
			$dc_container.find(".dc_article:gt("+dc_index+")").each(function(index, element) {
				dc_index++;
				if ($(this).offset().top + $(this).height() > dc_bottom)
					return false;
			});
			dc_slide(dc_index);
		});
	}
</script>
