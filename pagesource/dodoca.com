<script>
    var actName = 'index';
    var ctrlName = 'index';
//打开聊天窗口
function goTalk()
{
    $('#docaht_wsl_open')[0].click();
}

//获取验证码
function imcref(){
    $('#img_imgcode').attr('src','/icv/imgcode?'+Math.random());
}

$(function(){ 
    
    imcref();//验证码初始化
    
  //拉取子公司、友情链接数据

    if (actName != 'index' || ctrlName != 'index') {
        $.ajax({
            url: '/index/ajaxlinks',
            type: 'POST',
            dataType: 'json',
            async: false,
            data: {'id':1},
            success: function (data) {
                var quanzi1 = '';
                var quanzi2 = '';
                var cangu1 = '';
                var cangu2 = '';
                var friendly_link = '';
                for(var i in data)
                {
                    var value=data[i];
                    for(var j in value)
                    {
                        if(i == '2')
                        {
                            
                            if(j <= 2) {
                                quanzi1 += '<span>'+value[j].title+'</span>';
                            }
                            if(j > 2) {
                                quanzi1 += j == 3 ? '<i class="i-arrow"></i>' : '';
                            }
                            
                            if(j == 0)
                            {
                                quanzi2 += '<div class="showPanel">';
                            }
                            quanzi2 += '<p>'+value[j].title+'</p>';
                            if(j == value.length-1)
                            {
                                quanzi2 += '</div>';
                            }
                        }
                        else if(i == '3')
                        {
                            
                            if(j <= 2) {
                                cangu1 += '<span>'+value[j].title+'</span>';
                            }
                            if(j > 2) {
                                cangu1 += j == 3 ? '<i class="i-arrow"></i>' : '';
                            }
                            if(j == 0)
                            {
                                cangu2 += '<div class="showPanel">';
                            }
                            cangu2 += '<p>'+value[j].title+'</p>';
                            if(j == value.length-1)
                            {
                                cangu2 += '</div>';
                            }
                        }
                    }
                }
                
                if (quanzi == '') {
                    $("#quanzi").prev().html('');
                }
                if (cangu == '') {
                    $("#cangu").prev().html('');
                }
                $("#quanzi").html(quanzi1+quanzi2);
                $("#cangu").html(cangu1+cangu2);
                $("#friendly_link").parent().find('a').eq(0).hide();
            }
        });
    }
});
</script>
