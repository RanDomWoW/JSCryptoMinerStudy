    <div ng-controller="SystemMessageCtrl" >
    <div ng-if="Messages[0] != null" ng-cloak>
        <!-- Modal -->
        <div id="Modal" class="modal fade" role="dialog">
            <div class="modal-dialog unca-modal">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <div ng-repeat="message in Messages">
                    <!-- Modal content-->
                    <div class="title">
                        <div class="modal-icon">
                            <a href="{{message.MessageLink}}"><i ng-class="message.MessageIconClass" aria-hidden="true"><span class="sr-only sr-only-focusable">{{message.MessageIconName}}</span></i></a>
                        </div>
                        <h2>{{message.ModalTitle}}</h2>
                    </div>
                    <div class="content">
                        <p ng-bind-html="Validate(message.ModalText)"></p>
                        <div class="modal-footer">
                            <a class="red-btn" href="{{message.MessageLink}}">{{message.MessageLinkText}}</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
