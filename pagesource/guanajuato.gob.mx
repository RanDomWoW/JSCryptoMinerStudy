function downloadAll() {
  var link = document.createElement('a');
  var link2 = document.createElement('a');
  var link3 = document.createElement('a');
  link.setAttribute('download', 'CONVOCATORIA-COMPLETA.pdf');
  link.style.display = 'none';
  link2.setAttribute('download', '1.-Formato-de-Registro-GML-MUNICIPIOS.pdf');
  link2.style.display = 'none';
  link3.setAttribute('download', '2.-Formato-de-Registro-GML-SOCIEDADCIVIL.pdf');
  link3.style.display = 'none';

  document.body.appendChild(link);
  document.body.appendChild(link2);
  document.body.appendChild(link3);

  link.setAttribute('href', 'pdf/CONVOCATORIA-COMPLETA.pdf');
  link.click();
  link2.setAttribute('href', 'pdf/1.-Formato-de-Registro-GML-MUNICIPIOS.pdf');
  link2.click();
  link3.setAttribute('href', 'pdf/2.-Formato-de-Registro-GML-SOCIEDADCIVIL.pdf');
  link3.click();


  document.body.removeChild(link);
  document.body.removeChild(link2);
  document.body.removeChild(link3);
}
