            $(document).ready(function ($) {

                var $window = $(window);
                var carousel = $("#carousel");
                var pageContent = $(".page-content");
                var customCarouselWrapper = $(".carousel-wrapper");
                var carouselViewMode = "";

                $window.resize(function() {
                    if ($window.width() >= 1350) {
                        if (carouselViewMode !== "large") {
                            initLarge();
                        }
                    } else {
                        if (carouselViewMode !== "small") {
                            initSmall();
                        }
                    }
                }).resize();

                function startCarousel() {
                    var cw = $(".carousel-wrapper");
                    cw.removeClass('init');
                }

                function selectActualItem() {
                    var actualCarouselItem;
                    if (carouselViewMode === "small") {
                        actualCarouselItem = $(".carousel-item").first();
                    } else {
                        actualCarouselItem = $(".carousel-item:nth-child(2)");
                    }
                    return actualCarouselItem;
                }

                function openStage() {
                    carousel.find("a.stage-controller").addClass("clicked").text(closeLinkText);

                    $("#prev, #next").hide();

                    var actualCarouselItem = selectActualItem();
                    actualCarouselItem.find(".bottom").show();
                    var height = actualCarouselItem.height() + actualCarouselItem.find(".bottom").outerHeight();
                    $(".caroufredsel_wrapper").animate({height:height},250);
                }
                function closeStage() {
                    carousel.find("a.stage-controller").removeClass("clicked").text(openLinkText);

                    $("#prev, #next").show();

                    var actualCarouselItem = selectActualItem();
                    var height = actualCarouselItem.height() - actualCarouselItem.find(".bottom").outerHeight();
                    $(".caroufredsel_wrapper").animate({height:height},250);
                    actualCarouselItem.find(".bottom").hide();
                }

                function initSmall() {

                    var currentItemNumber = 0;

                    carouselViewMode = "small";
                    customCarouselWrapper.removeClass("large-carousel  ");

                    $('#carousel').carouFredSel({
                        responsive: true,
                        width: '100%',
                        items: {
                            height: 'auto',
                            visible: 1,
                            start: 'random'
                        },
                        scroll: {
                            items: 1,
                            duration: 1000,
                            timeoutDuration: 10000,
                            onBefore: function( data ) {
                                //	hide current block
                                data.items.visible.eq( currentItemNumber ).find( '.slideFadeInOut' ).show();

                                // load all items displayed after scroll
                                data.items.visible.each(function( index ) {
                                    loadImagesInContainer( data.items.visible.eq(index) );
                                });

                            },
                            onAfter: function( data ) {
                            }
                        },
                        onCreate: function(data){
                            $(this).find('.slideFadeInOut').hide();
                            data.items.eq( currentItemNumber ).find('.slideFadeInOut').show();

                            data.items.each(function( index ) {
                                loadImagesInContainer( data.items.eq(index) );
                            });

                            startCarousel();
                        },
                        auto: {
                            //delay: -10000
                            play: true,
                            button: 'a.stage-controller'
                        },
                        prev: '#prev',
                        next: '#next'
                    });

                }

                function initLarge() {
                    var currentItemNumber = 1;

                    carouselViewMode = "large";
                    customCarouselWrapper.addClass("large-carousel  ");

                    $('#carousel').carouFredSel({
                        responsive: false,
                        width: '100%',
                        height: 'auto',
                        items: {
                            visible: 3,
                            start: 'random'
                        },
                        scroll: {
                            items: 1,
                            duration: 1000,
                            timeoutDuration: 10000,
                            onBefore: function( data ) {
                                //	hide current block
                                data.items.old.add( data.items.visible ).find( '.slideFadeInOut' ).stop().fadeOut();

                                // load all items displayed after scroll
                                data.items.visible.each(function( index ) {
                                    loadImagesInContainer( data.items.visible.eq(index) );
                                });
                            },
                            onAfter: function( data ) {
                                data.items.visible.eq( currentItemNumber ).find( '.slideFadeInOut' ).stop().fadeIn();
                            }
                        },
                        onCreate: function(data){
                            $(this).find('.slideFadeInOut').hide();
                            data.items.eq( currentItemNumber ).find('.slideFadeInOut').show();

                            data.items.each(function( index ) {
                                loadImagesInContainer( data.items.eq(index) );
                            });

                            startCarousel();
                        },
                        auto: {
                            //delay: -10000
                            play: true,
                            button: 'a.stage-controller'
                        },
                        prev: '#prev',
                        next: '#next'
                    });

                }

                //Menu animation
                //carousel.find(".bottom").css({display: "none"}); //Fix Opera
                carousel.find("a.stage-controller").click(function () {
                    $(this).data("clicked", !$(this).data("clicked"));

                    if ($(this).data("clicked")) {
                        openStage();
                    } else {
                        closeStage();
                    }

                    return false;
                });
                carousel.find(".close").click(function () {

                    carousel.trigger("resume");
                    carousel.find("a.stage-controller").data("clicked", false);

                    closeStage();

                    return false;
                });

                function loadImagesInContainer( container ) {
                    images = container.find( 'img.lazy-carousel' );

                    images.each(function() {
                        image = $(this);
                        if ( image.is( 'img' ) ) {
                            if ( image.attr( 'src' ).indexOf( '_blank' ) != -1 ) {
                                image.attr( 'src', image.data( 'original' ) );
                            }
                        }
                    });
                }
            });


            function triggerResize() {
                window.dispatchEvent(new Event('resize'));
                $(".caroufredsel_wrapper").height($(".top").first().height());
            }

            $(window).load(function () {
                triggerResize();
            });

        </script></body> </html>