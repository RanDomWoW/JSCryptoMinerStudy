var to_reciprocal_close_day_a_id;
var to_reciprocal_close_day_b_id;
var to_reciprocal_close_day_b_status = 1 ;

function to_reciprocal_close_day_a(sel_s){

	var show_info = "";
	var show_s = parseFloat(sel_s)-1;
	var show_day = parseInt(show_s/86400);
	var show_hour = parseInt((show_s-(show_day*86400))/3600);
	var show_min = parseInt((show_s-(show_day*86400)-(show_hour*3600))/60);
	var show_sec = parseInt(show_s-(show_day*86400)-(show_hour*3600)-(show_min*60));


	if( show_s >= 0 ){

		//show_info += "<span class='discount_caption'>促銷價</span> ";

		if(show_day >= 0){show_info += show_day+" <span style='font-size:10px;'>天</span> ";}
		if(show_hour>= 0){show_info += show_hour+" <span style='font-size:10px;'>時</span> ";}
		if(show_min >= 0){show_info += show_min+" <span style='font-size:10px;'>分</span>&nbsp;&nbsp;";}
		if(show_sec >= 0){
			show_info += show_sec+" <span style='font-size:10px;'>秒</span> ";
		}

		document.getElementById("id_close_day_reciprocal_a").innerHTML = show_info;
		if(show_s==0){
			location.reload();
		}else{
			to_reciprocal_close_day_a_id = setTimeout("to_reciprocal_close_day_a('"+show_s+"')",1000);
		}

	}else{

		clearTimeout( to_reciprocal_close_day_b_id );

		to_reciprocal_close_day_b_status = 0;

		document.getElementById("id_close_day_reciprocal_b").innerHTML = '00';
		show_info += "00 <span style='font-size:10px;'>天</span> ";
		show_info += "00 <span style='font-size:10px;'>時</span> ";
		show_info += "00 <span style='font-size:10px;'>分</span>&nbsp;&nbsp;"

		document.getElementById("id_close_day_reciprocal_a").innerHTML = show_info;
	}
}

function to_reciprocal_close_day_b(sel_s){

	if( to_reciprocal_close_day_b_status == 1 ){
		var show_s = parseFloat(sel_s)-1;
		document.getElementById("id_close_day_reciprocal_b").innerHTML = show_s;
		if(show_s==10){
			to_reciprocal_close_day_b_id = setTimeout("to_reciprocal_close_day_b('99')",1);
		}else{
			to_reciprocal_close_day_b_id = setTimeout("to_reciprocal_close_day_b('"+show_s+"')",10);
		}
	}
}
