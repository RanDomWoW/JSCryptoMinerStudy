jQuery(function($){
	
	var parent = $('.slider');
	var image = $('.thumb img', parent);
	var items = $('.item', parent);
	var item;
	var timer;
	
	items.mouseenter(function(){
		var el = $(this);
		var thumb = el.data('url');
		if (thumb && image.attr('src') != thumb) {
			el.addClass('active').siblings().removeClass('active');
			image.stop().fadeOut(300, function(){
				image.attr('src', thumb).fadeIn(300);
			});
		}
	});
	
	function start_animation(){
		timer = setInterval(function(){
			item = items.filter('.active').next();
			if (item.length == 0) item = items.first();
			item.trigger('mouseenter');
		}, 5000);
		return false;
	}
	
	function stop_animation(){
		clearInterval(timer);
		return false;
	}
	
	start_animation();
	
});

</script>

