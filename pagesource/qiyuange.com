	$(function() {
        $('a[rel*=leanModal]').leanModal({top: 150, closeButton: ".modal_close"});
		$('form[name=signinform]').submit(function(e){
			var username = $('#signin_username').val();
			var password = $('#signin_password').val();
			$.post('/user/login/',{username:username, password:password},function (data) {
				if (data.code==1) {
					window.location.reload();
				} else {
					$('.tip').text(data.msg);
				}
			});
			return false;
		});
		$('form[name=signupform]').submit(function (e) {
			var username = $('#signup_username').val();
			var email = $('#signup_email').val();
			var password = $('#signup_password').val();
			$.post('/user/register/',{username:username, email:email, password:password},function (data) {
				if (data.code==1) {
					window.location.reload();
				} else {
					$('.tip').text(data.msg);
				}
			});
			return false;
		});
	});
</script>

