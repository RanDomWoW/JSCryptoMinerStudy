    <!-- HomePage Rotator -->
    

    <script type="text/javascript">

        var tplHomeRotatorImages;
        var tplHomeRotatorPager;

        var isLongBanner = _.where(Site.PageData.PageSkin, {}).length == 0 || _.where(Site.PageData.PageSkin, { IsShort: false }).length > 0;

        jQuery(document).ready(function ($) {

            $(document).on("click", ".banner-link", function () {
                var href = $(this).attr("href");
                var pos = $(this).attr("data-position");
                var name = $(this).attr("data-name");

                dataLayer.push({
                    'event': 'enchancedClick',
                    'ecommerce': {
                        'promoClick': {
                            'promotions': [{
                                'creative': 'HomeRotator',
                                'position': 'HomeRotator ' + pos,
                                'name': name.replace("'"," ")
                            }]
                        }
                    },
                    'eventCallback' : function () {document.location = href}
                });


                if (/^.*((youtu.be\/)|(v\/)|(\/u\/\w\/)|(embed\/)|(watch\?))\??v?=?([^#\&\?]*).*/.test(href))
                {
                    var embed = href.replace("watch?v=", "embed/");
                    var mWidth = 800;
                    var mHeight = 450;
                    var vAlign = 65;

                    $("#modal-video").children(".modal-container").css("width", mWidth);
                    $("#modal-video").dialog({
                        modal: true,
                        resizable: false,
                        width: mWidth,
                        position: ['center', vAlign],
                        dialogClass: 'transparent',
                        open: function (type, data) {
                            $(this).parent().appendTo($("form"));
                            var content = $('#modal-video .modal-content');
                            content.html('<iframe width="' + mWidth + '" height="' + 450 + '" src="' + embed + '?autoplay=1&VQ=HD720&rel=0" frameborder="0" allowfullscreen></iframe>');
                        },
                        close: function () {
                            var content = $('#modal-video .modal-content');
                            content.html("");
                        }
                    });

                    return false;
                }

                return true;
            });



            function syncPosition(el) {
                var name = $(".synced").text();
                
                var current = this.currentItem;
                var dataLayerCurrent = this.prevItem + 1;
                $("#homeBannerPager")
                    .find(".owl-item")
                    .removeClass("synced")
                    .eq(current)
                    .addClass("synced");

                if ($("#homeBannerPager").data("owlCarousel") !== undefined) {
                    center(current);
                    var bannerItemAmount = $("#homeBannerPager").data("owlCarousel").itemsAmount;
                    var receiveData = "HomeRotator" + dataLayerCurrent;
                    if (lookingBanner.length < bannerItemAmount) {
                        lookingBanner.push(receiveData);
                    
                        dataLayer.push({
                            'ecommerce': {
                                'promoView': {
                                    'promotions': [
                                        {
                                            'name': name,
                                            'position': 'HomeRotator ' + dataLayerCurrent
                                        }
                                    ]
                                }
                            },
                             'event': 'eePromoImpression'
                        });
                    }
                }
            }

        
          
        

            var tplHomeRotatorImages = $("#templateHomeRotatorImages").html();
            var tplHomeRotatorPager = $("#templateHomeRotatorPager").html();
           
            if(Site.PageData.Banner != null && Site.PageData.Banner.length != 0) {
                var result = Site.PageData.Banner;

                var itemSize = _.size(result);
       
                if (itemSize <= 4) {
                    var bannerView = itemSize;
                } else {
                    var bannerView = 4;
                }

                $("#homeBannerImages").html(_.template(tplHomeRotatorImages, {
                    items: result
                }));

                $("#homeBannerPager").html(_.template(tplHomeRotatorPager, {
                    items: result
                }));


                var homeBannerImages = $("#homeBannerImages");
                var homeBannerPager = $("#homeBannerPager");

                homeBannerImages.owlCarousel({
                    singleItem: true,
                    slideSpeed: 1000,
                    navigation: true,
                    pagination: false,
                    stopOnHover: true,
                    autoPlay: 10000,
                    afterAction: syncPosition,
                    responsiveRefreshRate: 200,
                });

                homeBannerPager.owlCarousel({
                    items: bannerView,
                    pagination: false,
                    responsiveRefreshRate: 100,
                    afterInit: function (el) {
                        el.find(".owl-item").eq(0).addClass("synced");                     
                    }
                });
               
                $("#homeBannerPager").on("click", ".owl-item", function (e) {
                    e.preventDefault();
                    var number = $(this).data("owlItem");
                    homeBannerImages.trigger("owl.goTo", number);
                });
                

                function center(number) {
                    var homeBannerPagerVisible = homeBannerPager.data("owlCarousel").owl.visibleItems;
                    var num = number;
                    var found = false;
                    
                    if (found === false) {
                        if (num > homeBannerPagerVisible[homeBannerPagerVisible.length - 1]) {
                            homeBannerPager.trigger("owl.goTo", num - homeBannerPagerVisible.length + 2);
                        } else {
                            if (num - 1 === -1) {
                                num = 0;
                            }
                            homeBannerPager.trigger("owl.goTo", num);
                        }
                    } else if (num === homeBannerPagerVisible[homeBannerPagerVisible.length - 1]) {
                        homeBannerPager.trigger("owl.goTo", homeBannerPagerVisible[1]);
                    } else if (num === homeBannerPagerVisible[0]) {
                        homeBannerPager.trigger("owl.goTo", num - 1);
                    }
                }
            }
        });

        
    </script>


    <div class="banner-area">
        <div id="homeBannerImages"></div>
        <div id="homeBannerPager"></div>
    </div>




