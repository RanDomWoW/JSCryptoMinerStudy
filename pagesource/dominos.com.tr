<script>

    var isEasyOrder = false;

    String.prototype.padRight = function (l, c) { return this + Array(l - this.length + 1).join(c || " ") }
    String.prototype.padLeft = function (l, c) { return Array(l - this.length + 1).join(c || " ") + this }

    $(document).ready(function () {
     

        $("#toggleBannerBtn").on("click", function () {
            if ($("#toggleBannerBtn").hasClass("down-arrw")) {
                setTimeout(function () { 
                    $("#tektiksiparisImg").show();
                }, 325);
            }
            else {
                setTimeout(function () { 
                    $("#tektiksiparisImg").hide();
                }, 1);
            }
        });

        var easyOrderItem = {};
        $(".delete-easy-order").click(function () {
            easyOrderItem._this = $(this);
            easyOrderItem.id = easyOrderItem._this.data("id");
            $("#confirm").modal("show");
        });

        $("#okeyBtn").on("click", function () {
            Dominos.RestAPI.EasyOrder.Delete(easyOrderItem.id).done(function (result) {
                if (result == true) {
                    noty({ text: "Kolay siparişiniz silindi.", type: 'success' });
                    easyOrderItem._this.parents(".order-item").remove();
                    var $el = $(".icheck-radio input").eq(0);
                    if ($el != undefined) {
                        $el.icheck("checked");
                    }
                    $("#confirm").modal("hide");
                }
                else
                    noty({ text: "Kolay siparişiniz silinirken hata oluştu!", type: 'warning' });
            });
        });

        $(".set-default-easy-order").click(function () {
            var id = $(this).data("id");

            Dominos.RestAPI.EasyOrder.SetDefault(id).done(function (result) {
                if (result == true) {
                    noty({ text: "Varsayılan olarak işaretlendi.", type: 'success' });
                }
                else {
                    noty({ text: "Varsayılan olarak işaretlenirken hata oluştu!", type: 'warning' });
                }
            });
            dataLayer.push({
                'pageView': '/vp/TekTıkSiparişSeç/',
                'event': 'virtualPageview'
            });

        });

        $(document).on("EasyOrderTimeStart", function (eventArgs) {
            $.event.trigger("EasyOrderSetStatus", { title: "Tek tıkla siparişin hazırlanıyor!", cancel: true });
        });

        $(document).on("EasyOrderTimeDone", function (event, args) {

            var easyOrderId = args.easyOrderId; //parseInt($('.easy-order-item.selected').attr('data-id'));
            var paymentMethodCode = args.paymentMethodCode;
            var cardName = args.cardName;

            if (_.isUndefined(easyOrderId)) {
                noty({ text: eventArgs.errorMessage, type: 'Bir hata oluştu' });
                return;
            }

            if ($.event.trigger("EasyOrderSetStatus", { title: "Siparişin hesaplanıyor...", cancel: true })) {

                Dominos.RestAPI.EasyOrder.Calculate(easyOrderId).done(function (result) {

                    if (result != null) {

                        if (result.StatusCode == 'InvalidTimeRangeNewOrder') {
                            noty({ text: "3 dakika geçmeden yeni sipariş veremezsiniz.", type: 'error' });
                            $('#timerLayer').hide();
                            return;
                        }
                        if (result.StatusCode == 'IncompletedOrderForEasyOrder') {
                            noty({ text: "Bir önceki kolay siparişin tamamlanmadığı için tekrar sipariş veremezsin", type: 'error' });
                            $('#timerLayer').hide();
                            return;
                        }

                        var price = result.PaymentAmount;
                        var trackerCode = result.TrackerCode;
                        var locationCode = result.LocationCode;

                        if (paymentMethodCode == 17 && cardName != '') {

                            if ($.event.trigger("EasyOrderSetStatus", { title: "Online ödeme yapılıyor...", cancel: true })) {

                                var paymentData = null;

                                /* MasterPass */

                                Dominos.Cart.MasterPass.Init
                                (
                                    true,
                                    function (accountStatus) {
                                        Dominos.Cart.CurrentCart.PaymentTotal = price;
                                        Dominos.Cart.CurrentCart.TrackerCode = trackerCode;
                                        Dominos.Cart.CurrentCart.LocationCode = locationCode;
                                        Dominos.Cart.MasterPass.SetCvv(args.cvc, function () {})
                                        Dominos.Cart.MasterPass.Purchase(cardName, function (paymentData) {
                                            if ($("#masterpass-otp-modal").hasClass("ui-dialog-content"))
                                                closeAllDialog();
                                            process(easyOrderId, paymentData, trackerCode);
                                        });
                                    },
                                    function (cards) {
                                        if (cards == null) {
                                            $.event.trigger("EasyOrderHide");
                                            noty({ text: "Kayıtlı kartınız bulunamadığı için ödeme yapılamadı!", type: 'warning' });
                                        }
                                    },
                                    function (desc, callBack, resend, maxSecond) {
                                        $("#masterpass-otp-modal").dialog({
                                            modal: true,
                                            resizable: false,
                                            width: '560',
                                            height: '413',
                                            position: ['center', 122],
                                            dialogClass: 'transparent',
                                            open: function (type, data) {

                                                var _this = $(this);

                                                $('#masterpass-otp-modal', _this).css('width', '100%');
                                                $("#masterpass-validation-code", _this).val("");
                                                $("#masterpass-time-text", _this).html("").show();

                                                var modalTitle = $("#masterpass-otp-modal .modal-title", _this);
                                                var icon = $("#masterpass-otp-modal .icon", _this);

                                                if (desc.indexOf("Bankanız") > -1) {
                                                    modalTitle.text("Banka Doğrulama");
                                                    icon.attr("src", "/_assets/css/img/svg/cardMpass.png");
                                                } else {
                                                    modalTitle.text("Telefon Doğrulama");
                                                    icon.attr("src", "/_assets/css/img/svg/phonempass.png");
                                                }

                                                $("#masterpass-validation-title", _this).html(desc);
                                                $("#masterpass-send-otp", _this).show();
                                                $(".layer,.tooltip-wrap", _this).hide();

                                                if (maxSecond == null)
                                                    maxSecond = 60 * 3;

                                                var second = maxSecond;

                                                var updateTimeInterval = setInterval(function () {
                                                    var timeText = Math.floor(second / 60).toString().padLeft(2, '0') + ':' + (second % 60).toString().padLeft(2, '0');
                                                    $("#masterpass-time-text", _this).html(timeText);
                                                    if (second <= 0) {
                                                        $(".layer,.tooltip-wrap", _this).show();
                                                        clearInterval(updateTimeInterval);
                                                    }
                                                    second--;
                                                }, 1000);

                                                $("#masterpass-re-send-otp", _this).unbind("click").click(function () {
                                                    clearInterval(updateTimeInterval);
                                                    closeAllDialog();
                                                    $("#masterpass-time-text", _this).hide();
                                                    resend();
                                                });

                                                $("#masterpass-send-otp", _this).unbind("click").click(function () {
                                                    clearInterval(updateTimeInterval);
                                                    var validationCode = $("#masterpass-validation-code", _this).val();
                                                    closeAllDialog();
                                                    $("#masterpass-time-text", _this).hide();
                                                    callBack(validationCode, second);
                                                });

                                                $("#masterpass-cancel-otp,#none-sendcode", _this).unbind("click").click(function () {
                                                    clearInterval(updateTimeInterval);
                                                    closeAllDialog();
                                                    $.event.trigger("EasyOrderHide");
                                                    $("#masterpass-time-text", _this).hide();
                                                });

                                            },
                                            close: function () {
                                                var _this = $(this);
                                                closeAllDialog();
                                            }
                                        });
                                    },
                                function (callBack) {

                                    $("#masterpass-cvv-modal").dialog({
                                        modal: true,
                                        resizable: false,
                                        width: '560',
                                        height: '413',
                                        position: ['center', 122],
                                        dialogClass: 'transparent',
                                        open: function (type, data) {

                                            var _this = $(this);

                                            $('#masterpass-cvv-modal', _this).css('width', '100%');
                                            $("#masterpass-cvv-code", _this).val("");

                                            $("#masterpass-cvv-send", _this).unbind("click").click(function () {
                                                var cvv = $("#masterpass-cvv-code", _this).val();
                                                closeAllDialog();
                                                Dominos.Cart.MasterPass.SetCvv(cvv, callBack);
                                            });

                                            $("#masterpass-cvv-cancel", _this).unbind("click").click(function () {
                                                closeAllDialog();
                                            });

                                        },
                                        close: function () {
                                            var _this = $(this);
                                            closeAllDialog();
                                        }
                                    });



                                }
                                );

                                /* // MasterPass */

                            }

                        }
                        else
                            process(easyOrderId, null, trackerCode);

                    }

                });

            }


        });

        function process(easyOrderId, paymentData, trackerCode) {

            if ($.event.trigger("EasyOrderSetStatus", { title: "Siparişin gönderiliyor...", cancel: false })) {

                Dominos.RestAPI.EasyOrder.Process(easyOrderId, paymentData, trackerCode).done(function (result) {

                    $.event.trigger("EasyOrderHide");

                    if (result != null) {

                        switch (result.StatusCode) {
                            case 'Success':
                                window.location.href = '/sepet/siparis-ozeti.aspx';
                                break;
                            case 'Success3D':
                                    
                                $("#modal-3d").dialog({
                                    modal: true,
                                    resizable: false,
                                    width: '550',
                                    height: '550',
                                    position: ['center', 122],
                                    dialogClass: 'transparent',
                                    closeOnEscape: false,
                                    open: function (type, data)
                                    {

                                        var source = Dominos.Cart.MasterPass.Url3D + '&returnUrl=' + Site.httpBase + '/routes/MasterPass3DReturn.aspx?cartType=regular%26trackerCode=' + Dominos.Cart.CurrentCart.TrackerCode;

                                        $('#modal-3d').css('width', '100%');
                                        $('#modal-3d #modal-3d-iframe').attr("src", source);

                                    },
                                    close: function()
                                    {
                                        return;
                                    }
                                });


                                break;
                            case 'InvalidCampaignCode':
                                noty({ text: Dominos.Resources.CommonResource.InvalidCampaignCode, type: 'error' });
                                break;
                            case 'IncompleteOrder':
                                noty({ text: Dominos.Resources.CommonResource.IncompleteOrder, type: 'error' });
                                break;
                            case 'IncompleteAddress':
                                noty({ text: Dominos.Resources.CommonResource.IncompleteAddress, type: 'error' });
                                break;
                            case 'InvalidServiceMethod':
                                noty({ text: Dominos.Resources.CommonResource.InvalidServiceMethod, type: 'error' });
                                break;
                            case 'InvalidPaymentType':
                                noty({ text: Dominos.Resources.CommonResource.InvalidPaymentType, type: 'error' });
                                break;
                            case 'InvalidOrderItem':
                                noty({ text: Dominos.Resources.CommonResource.InvalidOrderItem, type: 'error' });
                                break;
                            case 'InvalidCouponItem':
                                noty({ text: Dominos.Resources.CommonResource.InvalidCouponItem, type: 'error' });
                                break;
                            case 'IncorrectStore':
                                noty({ text: Dominos.Resources.CommonResource.IncorrectStore, type: 'error' });
                                break;
                            case 'UnavailableDiscount':
                                noty({ text: Dominos.Resources.CommonResource.UnavailableDiscount, type: 'error' });
                                break;
                            case 'UnexpectedError':
                                noty({ text: Dominos.Resources.CommonResource.SendOrderError, type: 'error' });
                                break;
                            case 'InvalidAdvanceOrderDate':
                                noty({ text: Dominos.Resources.CommonResource.InvalidAdvanceOrderDate, type: 'error' });
                                break;
                            case 'InvalidTrackerCode':
                                noty({ text: Dominos.Resources.CommonResource.InvalidTrackerCode, type: 'error' });
                                break;
                            case 'StoreClosed':
                                noty({ text: Dominos.Resources.CommonResource.CloseStore, type: 'error' });
                                closeAllDialog();
                                break;
                            case 'StoreNotReachable':
                                noty({ text: Dominos.Resources.CartResource.StoreNotReachable, type: 'error' });
                                break;
                            case 'MinPrice':
                                noty({ text: String.format(Dominos.Resources.CartResource.MinPrice, Dominos.Cart.Settings.MINPRICE), type: 'warning' });
                                break;
                            case 'CarryOutMinPrice':
                                noty({ text: String.format(Dominos.Resources.CartResource.CarryOutMinPrice, Dominos.Cart.Settings.CarryOutMinPrice), type: 'warning' });
                                break;
                            case 'TechnicalProblem':
                                noty({ text: Dominos.Resources.CommonResource.TechnicalProblem, type: 'error' });
                                break;
                            case 'InvalidPriceOfCampaign':
                                noty({ text: String.format(Dominos.Resources.CommonResource.MinLrCampaignPrice, Dominos.Cart.Settings.MINCARTAMOUNTFORCAMPAIGN), type: 'warning' });
                                break;
                            case 'OutSidePaymentError':
                                noty({ text: Dominos.Resources.CommonResource.SendOrderError, type: 'error' });
                                break;
                            case 'InvalidTimeRangeNewOrder':
                                noty({ text: Dominos.Resources.CommonResource.InvalidTimeRangeNewOrder, type: 'error' });
                                break;
                            case 'IncompletedOrderForEasyOrder':
                                noty({ text: 'Bir önceki kolay siparişin tamamlanmadığı için tekrar sipariş veremezsin',type: 'error' });
                                break;
                            default:
                                noty({ text: Dominos.Resources.CommonResource.SendOrderError, type: 'error' });
                                break;
                        }

                        if (result != 'Success')
                        {
                            $('#timerLayer').hide();
                        }
                    }
                }).fail(function () {
                    $.event.trigger("EasyOrderHide");
                    noty({ text: 'Bir hata oluştu', type: 'error' });
                    return;
                });
            }
        }
    });
</script>





