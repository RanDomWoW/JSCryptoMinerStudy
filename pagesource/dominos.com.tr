                    current.removeClass('hidden'); 
                    return false;
                }
            });

            $('.products-list .action-add-basket').each(function () {
                var current = $(this);
                var currentItem = $(this).parents('.products-item');
                var currentItemCategoryCode = current.attr('data-categoryCode');

                switch (currentItemCategoryCode) {
                    case 'Pizza':

                        current.click(function () {

                            var locationURL = current.attr('data-url');


