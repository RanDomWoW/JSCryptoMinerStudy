        function loadQuestionnaire(questionnaire) {
            var templateQuestionnaire = $("#templateQuestionnaire").html();
            $('#modal-poll .modal-content').html(_.template(templateQuestionnaire, { questionnaire: questionnaire }));
        }

        function loadMessage(message) {
            var templateMessage = $("#templateMessage").html();
            $('#modal-poll .modal-content').html(_.template(templateMessage, { message: message }));
        }

    </script>



