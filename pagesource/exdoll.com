            container : {
                header : '系统提示',
                content : "<div style='padding-top:20px;' >"+message+"</div>"
                /*,yesFn : closeEasyDialog*/
            },
            overlay : true
        });
        if(icon != '' && icon != null){
            $(".easyDialog_content").css("background","url("+icon+") no-repeat scroll 8px center #EDF8F9");
        }
    };

    var alertCallBack = function(message , callBack_fn , icon){
        $(".easyDialog_content").css("background","url(deng.gif) no-repeat scroll 8px center #EDF8F9");
