$(document).ready(function() {
    var cutOffTime = '2030';
    var myDate = new Date(), myDay = myDate.getDay(), drawDay;
    var currTime = addZ(myDate.getHours()).toString() + addZ(myDate.getMinutes()).toString();
    if ((myDay===2 && (currTime < cutOffTime)) || (myDay===5 && (currTime < cutOffTime))){
      drawDay = ', heute um 20:30 Uhr'; 
    } else if (myDay<2 || myDay>=5) {
      drawDay = ' am Dienstag';
    } else if (myDay>2 || myDay<5) {
      drawDay = ' am Freitag';  
    }
    $('#stagedrawDayEM').html($('#stagedrawDayEM').html()+drawDay);
});    
function addZ(n){return n<10? '0'+n:n;}   
</script>
