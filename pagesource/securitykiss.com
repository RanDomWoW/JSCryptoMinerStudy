<script>

    // Yeah, if you are looking here you see it's not the real data from the backend
    // But be assured that this is well intentioned - it's just a reasonable estimate so the user know the size of the network

    function seconds_this_month() {
        var now = new Date().getTime(),
            monthStart = new Date();
        monthStart.setDate(1);
        monthStart.setHours(0);
        monthStart.setMinutes(0);
        monthStart.setSeconds(0);
        monthStart.setMilliseconds(0);
        return Math.floor((now - monthStart.getTime()) / 1000);
    }
    
    function animate_counter(el, ratePerSec, sigmaSec) {
        var old = el.textContent;
        var prev = parseInt(old.replace(/,/g, ""));
        if (!isFinite(prev)) {
            prev = 0;
        }
        var next = Math.floor(seconds_this_month() * ratePerSec + sigmaSec * Math.random());
        if (next > prev) {
            el.textContent = next.toLocaleString("en-GB");
        }    
    };

    setInterval(function() {animate_counter(document.getElementById("datactr"), 0.5, 4)}, 100);
    setInterval(function() {animate_counter(document.getElementById("connctr"), 4, 5)}, 100);

    document.getElementById("servctr").textContent = 104 + Math.floor(new Date().getTime()/1000/60/60/18) % 4;

</script>


