	<script>
	function validacaoEmail(field) { 
		usuario = field.value.substring(0, field.value.indexOf("@")); 
		dominio = field.value.substring(field.value.indexOf("@")+ 1, field.value.length); 
		
		if ((usuario.length >=1) && 
			(dominio.length >=3) && 
			(usuario.search("@")==-1) && 
			(dominio.search("@")==-1) && 
			(usuario.search(" ")==-1) && 
			(dominio.search(" ")==-1) && 
			(dominio.search(".")!=-1) && 
			(dominio.indexOf(".") >=1)&& 
			(dominio.lastIndexOf(".") < dominio.length - 1)) { 
			document.getElementById("msgemail").innerHTML="<span class='suss'>E-mail válido</span>";
		} else { 
			document.getElementById("msgemail").innerHTML="<span class='inva'>E-mail inválido</span>"; alert("E-mail invalido");
		}
	} 
	</script>
	<!-- End Valida campos -->

	
