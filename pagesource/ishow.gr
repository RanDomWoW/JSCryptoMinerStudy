<script language="javascript">
function addNoMemberMail()
{
 var a = $('#emailNoMember').val();
 
 if(a.length<7 || a.indexOf('.')<1 || a.indexOf('@')<1 || a.indexOf('"')>0 || a.indexOf("'")>0)
 {
	$('#NoMemberErrorDiv').html('Το mail σας δεν είναι σωστό');
	$('#NoMemberErrorDiv').show();
	return false;	
 }
 
 $.post("doAddNoMemberMail.asp", { aEmail: a },
   function(data){
	   if(data.indexOf('error1')>0)
	   {
			$('#NoMemberErrorDiv').html('Το mail σας δεν είναι σωστό');
			$('#NoMemberErrorDiv').show();			
		}
	   if(data.indexOf('error2')>0)
	   {
			$('#NoMemberErrorDiv').html('Το mail σας υπάρχει ήδη');
			$('#NoMemberErrorDiv').show();			
		}
	   if(data.indexOf('ok!')>0)
	   {
			$('.newsLetterInputCont').html('<center><span style="color:white"><b>Το mail καταχωρήθηκε με επιτυχία!</b></span><br>Σας έχει σταλεί mail επιβεβαίωσης στην διεύθυνση που δηλώσατε. Πρέπει να κάνετε κλίκ στο link που θα βρείτε ώστε να ενεργοποιηθεί το mail σας.<br>Ευχαριστούμε</center>').css("color","white");
			$('#NoMemberErrorDiv').hide();			
		}
   });
}

</script>


