}
// Adds a click event to the "Learn More" majors link for mobile so it shows/hides the text bubble
if (matchMedia('only screen and (max-width: 500px)').matches) {
	var link = $('.majorsFancy .bubbleMe a').attr('href');			// saves the URL
	$('.majorsFancy .bubbleMe a').removeAttr('href');			// removes the href from the bubble link
	$('.majorsFancy .bubbleMe a').click(function() {			// add a click event to toggle text off/on
		//console.log(link);
		$('.majorsFancy .majors-content').toggle();			// toggles the text off/on (on in this case)
		$('.majorsFancy .bubbleMe').addClass('clickedBubble');		// adds a CSS class to the bubble
		setTimeout(function() {						// delays the next section by... (see bottom)
			$('.majorsFancy .bubbleMe a').attr('href', link);	// adds the URL + href back to the bubble link
			$('.majorsFancy .bubbleMe a').html('Visit Site »');	// changes the bubble text
			$('.majorsFancy .majors-content').append('<span class="tapToHide"> (tap to hide)</span>'); // adds a span to text
		}, 100);					// 100ms
	});
	$('.majorsFancy .majors-content').click(function() {			// add a click event to toggle text off/on
		$('.majorsFancy .majors-content').toggle();			// toggles the text off/on (off in this case)
		$('.tapToHide').remove();					// removes the span from the text
		$('.majorsFancy .bubbleMe a').removeAttr('href');		// removes the href from the bubble link
		$('.majorsFancy .bubbleMe').removeClass('clickedBubble');	// removes the CSS class from the bubble
		$('.majorsFancy .bubbleMe a').html('Learn More');		// changes the bubble text back
	});
}

