// removes it when you scroll down... mostly.
$(window).scroll(function() {    
	var scroll = $(window).scrollTop();
	if (matchMedia('only screen and (max-width: 500px)').matches) {
		if (scroll >= 50) { $('.floatingButtons div').removeClass('open'); }
		else { $('.floatingButtons div').addClass('open'); }
	} else {
		if (scroll >= 500) { $('.floatingButtons div').removeClass('open'); } 
		else { $('.floatingButtons div').addClass('open'); }
	}
});

