<script>
$(function() {
	$(".education").autocomplete({
		source: 'https://www.hirelateral.com/widget/educationlist',
		select: function (event, ui) {
			$('.education').val(ui.item.value);
			$('.hidden_edu').val(ui.item.item);
		    return false;
		  },
	});
	$(".location").autocomplete({
		source: 'https://www.hirelateral.com/widget/locationList',
		select: function (event, ui) {
			$('.location').val(ui.item.value);
			$('.hidden_loc').val(ui.item.item);
		    return false;
		  },
	});
		$(".designation").autocomplete({
		source: 'https://www.hirelateral.com/widget/designationList',
		select: function (event, ui) {
			$('.designation').val(ui.item.value);
			$('.hidden_desig').val(ui.item.item);
		    return false;
		  },
	});
	$('.searchButton').on('click',function(){
		var education=$('.hidden_edu').val();
		var location=$('.hidden_loc').val();
		var designation=$('.hidden_desig').val();
		if(education!=''){
			document.location.href="https://www.hirelateral.com/search-jobs/"+education+"-jobs-in-india";
		}else if(location!=''){
			document.location.href="https://www.hirelateral.com/search-jobs/jobs-in-"+location;
		}
		else if(designation!=''){
			document.location.href="https://www.hirelateral.com/search-jobs/job-alert/"+designation;
		}else {
			document.location.href="https://www.hirelateral.com/search-jobs";
		}
	});
});
</script>
<script async src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js" onload="sticky_relocate()"></script>
<script>
function sticky_relocate() {
  var window_top = $(window).scrollTop();
  var div_top =100;
  if (window_top > div_top) {
    $('.items').addClass('stick');
  } else {
    $('.items').removeClass('stick'); }
}
function style() {
$(window).scroll(sticky_relocate);
sticky_relocate();
}
</script>
<script>
$(document).ready(function(){   
        var $scrltop=jQuery.noConflict();
$scrltop(window).scroll(function(){
        var scroller_anchor = $scrltop(".scroller_anchor").offset().top;  
    
    if ($scrltop(this).scrollTop() >= scroller_anchor && $scrltop('.scroller').css('position') != 'fixed') {    
        $scrltop('.scroller').css({
            'width':'160px',
            'position': 'fixed',
            'top': '2em',
            'right' : '0'
        });
       
    } else if ($scrltop(this).scrollTop() < scroller_anchor && $scrltop('.scroller').css('position') != 'relative') {    
        $scrltop('.scroller_anchor').css('height', '0px');
        $scrltop('.scroller').css({
            'position': 'relative'
        });
    }
});
});
</script>
