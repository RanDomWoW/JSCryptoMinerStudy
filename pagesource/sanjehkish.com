                <a href="{{url}}" class="result algolia-clearfix">
                    <div class="result-content">
                        <div class="result-thumbnail">
                            {{#image_url}}<img src="{{{ image_url }}}"/>{{/image_url}}
                            {{^image_url}}<span class="no-image"></span>{{/image_url}}
                        </div>
                        <div class="result-sub-content">
                            <h3 class="result-title text-ellipsis">
                                {{{ _highlightResult.name.value }}}
                            </h3>
                            <div class="ratings">
                                <div class="ratings-wrapper">
                                    <div class="ratings-sub-content">
                                        <div class="rating-box">
                                            <div class="rating" style="width:{{rating_summary}}%" width="148" height="148"></div>
                                        </div>
                                    </div>
                                    <div class="price">
                                        <div class="price-wrapper">
                                            <div>
                                                <span
                                                    class="after_special {{#price.IRT.default_original_formated}}promotion{{/price.IRT.default_original_formated}}">
                                                    {{price.IRT.default_formated}}
                                                </span>

                                                {{#price.IRT.default_original_formated}}
                                                    <span class="before_special">
                                                        {{price.IRT.default_original_formated}}
                                                    </span>
                                                {{/price.IRT.default_original_formated}}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="result-description text-ellipsis">{{{ _highlightResult.description.value }}}</div>

                            {{#isAddToCartEnabled}}
                                {{#in_stock}}
                                    <form action="https://sanjehkish.com//checkout/cart/add/product/{{objectID}}"
                                          method="post">
                                        <input type="hidden" name="form_key"
                                               value="UFiTmMH1MWBQMwLc"/>
                                        <input type="hidden" name="qty" value="1">
                                        <button type="submit">افزودن به سبد</button>
                                    </form>
                                {{/in_stock}}
                            {{/isAddToCartEnabled}}
