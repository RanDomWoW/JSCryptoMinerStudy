    <script type="text/javascript">
        function setRating() {
            var $ = jQuery.noConflict();
            var ratingIds = $('*[id^="Rating_"]');
            for (var i = 0; i < ratingIds.length; i++) {
                var id = $(ratingIds[i]).attr('id');
                if ($('#' + id).data('israted') == 'true' || $('#' + id).data('israted') == true) {
                    continue;
                }
                else {
                    $('#' + id).attr('data-israted', true);
                    $('#' + id).rateit();
                    $('#' + id).rateit('readonly', true);
                    $('#' + id).rateit('step', 0.1);
                    $('#' + id).rateit('value', (jQuery('#' + id).data('attr')));
                }
            }
        }
    </script>

    

