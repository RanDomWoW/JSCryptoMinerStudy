    <script type="text/javascript">

        $(document).ready(function () {
            jQuery.noConflict();
            $('selectLanguage').modal('hide');
            $('selectLanguage').css('display', 'none');
            $('selectLanguage').hide();
        });
        function SelectLanguageDropdown() {
            try {

