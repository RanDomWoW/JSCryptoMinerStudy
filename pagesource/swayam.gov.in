        function SelectMenu(res) {
            if ($(res).text() != "") {
                $('#dropdownSelected').text($(res).text());
                $('#dropdownSelected').val($(res).data('val'));
            }
        }
        function SetLanguage() {

            var currentUrl = window.location.href;
            $.ajax({
                type: 'GET',
                url: "/languagechange/setlanguage",
                data: { language: $('#dropdownSelected').val().trim(), currentUrl: currentUrl },
                cache: false,
                success: function (response) {
                    $(document).ready(function () {
                        $('#selectLanguage').modal("hide");
                    });
                    $('#dropdownSelected').val(response.cultureName);
                    window.location.reload(true);
                },
                error: function (err) {
                }
            });
        }

        function SetLanguageServer(res) {

            var currentUrl = window.location.href;
            $.ajax({
                type: 'GET',
                url: "/languagechange/setlanguage",
                data: { language: res, currentUrl: currentUrl },
                cache: false,
                success: function (response) {
                    window.location.reload(true);
                },
                error: function (err) {
                }
            });
        }
