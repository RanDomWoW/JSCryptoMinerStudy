    <script>
        var presetSortedOrder = ['All', 'School', 'Certificate', 'Diploma', 'Undergraduate', 'IIT PAL', 'Post graduate'];
        function sortLevelArray(arr) {
            var result = [],
                i, j;
            for (i = 0; i < presetSortedOrder.length; i++)
                while (-1 != (j = $.inArray(presetSortedOrder[i], arr)))
                    result.push(arr.splice(j, 1)[0]);
            return result.concat(arr);
        }

        var isPopularCourseSectionLoaded=false;
        $(window).scroll(function() {
            if(isPopularCourseSectionLoaded==true) return;
            var hT = $('#partialDivMostPopularCurrentCourse').offset().top,
                hH = $('#partialDivMostPopularCurrentCourse').outerHeight(),
                wH = $(window).height(),
                wS = $(this).scrollTop();
            //if (wS > (hT+hH-wH) && (hT > wS) && (wS+wH > hT+hH)){
                if (wS > (hT+hH-wH)){
                isPopularCourseSectionLoaded=true;
                getMostPopularCourse("all", "All");
                getNewsList();
            }
        });

        //$(document).ready(function () {
            //getMostPopularCourse("all", "All");
            // get news
            //getNewsList();
        //});

        // get news list
        function getNewsList() {
            $.ajax({
                type: "GET",
                data: {},
                url: 'https://api.swayam.gov.in/MMADServices/api/Students/GetNewsForHome',
                success: function (result) {
                    var res  =  result.Data;
                    console.log(res);
                    if (res != null && res !== "null" && res.length>0) {
                        document.getElementById("newsLoader").style.display = "none";
                        renderNewsList(res);
                    }
                    else if(res.length == 0)
                    {
                        document.getElementById("noNewsdiv").style.display = "block";
                        document.getElementById("newsLoader").style.display = "none";
                    }
                    else {
                        alert("There is some error please try again");
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    console.log('Error: ' + jqXHR.statusText);
                }

            });
        }

        // method to get news details - to open  popup
        function GetNewsDetail(newsId) {
            $.ajax({
                type: "GET",
                data: { 'newsId': newsId },
                url: '/news',
                success: function (result) {
                    if (result != null) {
                        document.getElementById("newsDetails").innerHTML = "";
                        document.getElementById("newsDetails").innerHTML = result;
                    }
                    else {
                        alert("There is some error please try again");
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    console.log('Error: ' + jqXHR.statusText);
                }

            });
        }

        // to close the news detail popup
        function closeNewsDetail() {
            document.getElementById("newsDetails").innerHTML = "";
        }
        function getMostPopularCourse(type, learningPath) {

            var data = { "type": type, "learningPath": learningPath };
            $.ajax({
                url: 'https://api.swayam.gov.in/MMADServices/api/Students/GetMostPopularCourses?type=' + type + '&learningPath=' + learningPath,
                type: 'GET',
                //data: JSON.stringify(data),
                //dataType: 'json',
                //contentType: 'application/json; charset=utf-8',
                error: function (xhr) {
                    console.log('Error: ' + xhr.statusText);
                },
                success: function (result) {
                    var parsedResult = result.Data;
                    if (parsedResult.hasOwnProperty("upcoming") && parsedResult["upcoming"] != null) {
                        var upcomingparsedResult = parsedResult["upcoming"];
                        if (upcomingparsedResult.Facets.levels != undefined) {
                            levelAjaxCallBack(upcomingparsedResult.Facets.levels, "upcoming");
                        }
                        if (upcomingparsedResult != undefined) {
                            courseCountAjaxCall(upcomingparsedResult.Count, "upcoming");
                        }
                        if (upcomingparsedResult != undefined) {
                            renderMostPopularUpcomingCourses(upcomingparsedResult);
                        }
                    }
                    if (parsedResult.hasOwnProperty("current") && parsedResult["current"] != null) {
                        var currentparsedResult = parsedResult["current"];
                        if (currentparsedResult.Facets.levels != undefined) {
                            levelAjaxCallBack(currentparsedResult.Facets.levels, "current");
                        }
                        if (currentparsedResult != undefined) {
                            courseCountAjaxCall(currentparsedResult.Count, "current");
                        }
                        if (currentparsedResult != undefined) {
                            renderMostPopularCurrentCourses(currentparsedResult);
                        }
                    }

                }
            });
        }
        function renderNewsList(list) {
            var result = {
                News: list
            };

            $('#newsListUl').setTemplateElement("newsList_Templates").processTemplate(result);
            document.getElementById("newsListUl").style.position = null;
        }

        function renderMostPopularUpcomingCourses(search) {
            var data = {
                search: search.Result,
                showRating: false
            };
            $('#demo6').setTemplateElement("txtBoxMostPopularCurrent_Template").processTemplate(data);

            jQuery('#demo6').scrollbox({
                direction: 'h',
                autoPlay: false
            });
        }
        function renderMostPopularCurrentCourses(search) {
            var data = {
                search: search.Result,
                showRating: true
            };
            $('#demo5').setTemplateElement("txtBoxMostPopularCurrent_Template").processTemplate(data);
            setRating();

            jQuery('#demo5').scrollbox({
                direction: 'h',
                autoPlay: false
            });
        }

        function levelAjaxCallBack(data, type) {
            if (type === "upcoming") {
                var upcomingCourseLevels = $('#hiddenUpcomingCourseLevels').val();
                if (upcomingCourseLevels == "") {
                    var Upcominglevels = [];
                    Upcominglevels.push("All");
                    $.each(data, function (index, item) {
                        Upcominglevels.push(item.value);
                        Upcominglevels = sortLevelArray(Upcominglevels);
                    });
                    $('#hiddenUpcomingCourseLevels').val(JSON.stringify(Upcominglevels));
                    upcomingCourseLevels = $('#hiddenUpcomingCourseLevels').val();
                    if (upcomingCourseLevels != "") {
                        $("#facetLevelUpcomingCourseList").find('option').remove();
                        upcomingCourseLevels = $.parseJSON(upcomingCourseLevels);
                        $.each(upcomingCourseLevels, function (index, item) {
                            $("#facetLevelUpcomingCourseList").append($("<option></option>").val(item).html(item));
                        });
                    }
                    var hiddenUpcomingSelected = document.getElementById('hiddenUpcomingSelected').value;
                    if (hiddenUpcomingSelected != "") {
                        $("#facetLevelUpcomingCourseList").val(hiddenUpcomingSelected);
                    }
                }
            } else {
                var currentCoursesLevels = $('#hiddenCurrentCoursesLevels').val();
                if (currentCoursesLevels == "") {
                    var Currentlevels = [];
                    Currentlevels.push("All");
                    $.each(data, function (index, item) {
                        Currentlevels.push(item.value);
                        Currentlevels = sortLevelArray(Currentlevels);
                    });
                    $('#hiddenCurrentCoursesLevels').val(JSON.stringify(Currentlevels));
                    currentCoursesLevels = $('#hiddenCurrentCoursesLevels').val();
                    if (currentCoursesLevels != "") {
                        $("#facetLevelCurrentCourseList").find('option').remove();
                        currentCoursesLevels = $.parseJSON(currentCoursesLevels);
                        $.each(currentCoursesLevels, function (index, item) {
                            $("#facetLevelCurrentCourseList").append($("<option></option>").val(item).html(item));
                        });
                    }
                    var hiddenCurrentSelected = document.getElementById('hiddenCurrentSelected').value;
                    if (hiddenCurrentSelected != "") {
                        $("#facetLevelCurrentCourseList").val(hiddenCurrentSelected);
                    }
                }
            }
        }

        function courseCountAjaxCall(count, type) {
            if (type === "upcoming") {
                document.getElementById('coursesCountUpcoming').innerHTML = count;
            } else {
                document.getElementById('coursesCountCurrent').innerHTML = count;
            }
        }

        function getSelectUpcomingLeaningPath(selectObject) {
            var select = $("#facetLevelUpcomingCourseList option:selected").text();
            document.getElementById('hiddenUpcomingSelected').value = select;
            getMostPopularCourse("upcoming", select);
        }

        function getSelectCurrentLeaningPath(selectObject) {
            var select = $("#facetLevelCurrentCourseList option:selected").text();
            document.getElementById('hiddenCurrentSelected').value = select;
            getMostPopularCourse("current", select);
        }

        function courseTitleLimit(string, limit) {
            var dots = "...";
            if (string.length > limit) {
                string = string.substring(0, limit) + dots;
            }
            return string;
        }
    </script>

