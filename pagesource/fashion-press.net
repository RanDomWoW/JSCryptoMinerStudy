$(function(){
  function opt_size(cls){
    c = ".tri_slide."+cls+" img"
    $(c).each(function(i, elem) {
      $(elem).load(function(){
        style = {
          "object-fit": "cover",
          "height": $(elem).width()
        }
        if(cls=='collection_slide'){
          style['object-position'] = 'top center'
        }
        $(elem).css(style);
      });
    });
  }
  opt_size("snap_slide")
  opt_size("collection_slide")
  $(window).resize(function() {
    $(".tri_slide.snap_slide img").each(function(i, elem) {
      $(elem).css({"object-fit": "cover", "height": $(elem).width()});
    });
    $(".tri_slide.collection_slide img").each(function(i, elem) {
      $(elem).css({"object-position":"top center", "object-fit": "cover", "height": $(elem).width()});
    });
  });
});
</script>

