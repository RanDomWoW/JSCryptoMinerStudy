window.goAdverticum3.onReady(function( goa3 ) {
    
    var slug_lista = [];
    var codes_from = [];
    var codes_to = [];
    var current_slug = window.location.toString();
    var van_csere = false;

    if ( 'undefined' !== typeof(slug_recodes_for_goa_blocks) ) {    
        slug_recodes_for_goa_blocks.forEach( function(sor, sor_index) {
            temp = sor.split('|'); slug = temp[0]; codes = temp[1];
            if ( slug.trim() !== '' ) {
                if ( current_slug.indexOf( slug ) !== -1 ) {
                    console.log( "Adverticum zóna cserék "+slug+" slug alapján.");
                    van_csere = true;

                    codes.split(',').forEach( function(kod_par, par_index) {
                        temp = kod_par.split('-');
                        codes_from.push( temp[0] ); // eredeti kódok
                        codes_to.push( temp[1] );  // új kódok
                    } );
                }
            }
        });
    } else {
        console.log('Hiányzik a slug csere konfiguráció');
    }
    
    var $ = goa3.getjQuery();
    var goa_blocks = [];
    var block_type = 'zone_pc_';

    if ( $("body").outerWidth(true) < 641 ) {	block_type = 'zone_mobile_'; }
    
    $(".goAdverticum").each(function(){
        
        if( $(this).attr('id').indexOf( block_type ) !== -1 ) {	
            
            zone_code = $(this).attr('id').substr( block_type.length );
            
            if ( van_csere ) {
                zone_csere_index = codes_from.indexOf( zone_code );
                if ( zone_csere_index !== -1 ) {
                    console.log( block_type + zone_code + ' --> ' + block_type + codes_to[ zone_csere_index ] );
                    zone_code = codes_to[ zone_csere_index ];
                } else {
                    console.log($(this).attr('id'));
                    /*console.log($(this).attr('id') + ' ' + $(this).parent().attr('id'));*/
                }
            } else {
                console.log($(this).attr('id'));
                /*console.log($(this).attr('id') + ' ' + $(this).parent().attr('id'));*/
            }
            
            
            goa_blocks.push({
                node: $(this), id: zone_code,
                p: typeof( $(this).data('priority') ) !== "undefined" ? $(this).data('priority') : 100
            });
            
        }
        
    });
    if (goa_blocks.length > 0) { goa3.invocation( goa_blocks );	}
});
</script>


