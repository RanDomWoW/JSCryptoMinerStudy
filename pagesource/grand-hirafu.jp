		init();

		$(window).resize(function() {
			$('#contents_wrp').css('top', $('#main_visual').height());
			$('#main_visual .page_links li a').css('height', $('#main_visual').height());
		});

		$(window).on('beforeunload', function(){
			var slider = $('.slider').bxSlider();
			slider.destroySlider();
		});

	});

function init() {
		$("#loading").fadeOut(2000);
		var obj = $('.slider').bxSlider({
			mode: 'fade',
			auto: true,
			infiniteLoop: true,
			responsive: true,
			speed: 1000,
			displaySlideQty: 1,
			pager: false,
			controls: false,
			pause: 6000,
			onSliderLoad: function() {
				$('#contents_wrp').css('top', $('#main_visual').height());
			}
		});

	$('.page_links li.summer_link a').hover(
		function() {
			$('#bg_obi').addClass('bg_obi_summer');
		},
		function() {
			$('#bg_obi').removeClass('bg_obi_summer');
		}
	);
    
    $('.page_links li.summer_en_link a').hover(
		function() {
			$('#bg_obi').addClass('bg_obi_summer_en');
		},
		function() {
			$('#bg_obi').removeClass('bg_obi_summer_en');
		}
	);

	$('.page_links li.winter_link a').hover(
		function() {
			$('#bg_obi').addClass('bg_obi_winter');
		},
		function() {
			$('#bg_obi').removeClass('bg_obi_winter');
		}
	);

	$('.page_links li.winter_en_link a').hover(
		function() {
			$('#bg_obi').addClass('bg_obi_winter_en');
		},
		function() {
			$('#bg_obi').removeClass('bg_obi_winter_en');
		}
	);

}

