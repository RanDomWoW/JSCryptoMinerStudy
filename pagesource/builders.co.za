function trackAddToCart_google(productCode, quantityAdded) {
	//_gaq.push(['_trackEvent', 'Cart', 'AddToCart', productCode, quantityAdded]);
	ga('send', 'event', 'Cart', 'AddToCart', productCode, quantityAdded);
}

function trackSocialShare_google(network, socialAction, target, source) {
	//_gaq.push(['_trackSocial', network, socialAction, target, source]);
	ga('send', 'social', network, socialAction, target, source);
	//_gaq.push(['_trackEvent', 'Social Interactions', 'Click', network]);
	ga('send', 'event', 'Social Interactions', 'Click', network);
}

function trackFormSubmit_google(eventCategory, eventAction, eventLablel) {
	//_gaq.push(['_trackEvent', eventCategory, eventAction, eventLablel]);
	ga('send', 'event', eventCategory, eventAction, eventLablel);
}

function trackAttachmentDownload_google(eventName, eventAction, attachmentFilename) {
	//_gaq.push(['_trackEvent', eventName, eventAction, attachmentFilename]);
	ga('send', 'event', eventName, eventAction, attachmentFilename);
}

function trackRemoveFromCart(productCode, initialQuantity) {
	//_gaq.push(['_trackEvent', 'Cart', 'RemoveFromCart', productCode, initialQuantity]);
	ga('send', 'event', 'Cart', 'RemoveFromCart', productCode, initialQuantity);
}

window.mediator.subscribe('trackAddToCart', function(data) {
	if (data.productCode && data.quantity)
	{
		trackAddToCart_google(data.productCode, data.quantity);
	}
});

window.mediator.subscribe('trackSocialShare', function(data) {
	if (data.network && data.socialAction && data.target && data.source)
	{
		trackSocialShare_google(data.network, data.socialAction, data.target, data.source);
	}
});

window.mediator.subscribe('trackFormSubmit', function(data) {
	if (data.eventCategory && data.eventAction && data.eventLablel)
	{
		trackFormSubmit_google(data.eventCategory, data.eventAction, data.eventLablel);
	}
});

window.mediator.subscribe('trackAttachmentDownload', function(data) {
	if (data.eventCategory && data.eventAction && data.attachmentFilename)
	{
		trackAttachmentDownload_google(data.eventCategory, data.eventAction, data.attachmentFilename);
	}
});

window.mediator.subscribe('trackRemoveFromCart', function(data) {
	if (data.productCode && data.initialCartQuantity)
	{
		trackRemoveFromCart(data.productCode, data.initialCartQuantity);
	}
});
