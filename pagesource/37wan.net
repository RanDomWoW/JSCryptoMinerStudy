<script id="sq-pref" data-sitetype="5">var fixNum = function(number) {
        var left = 0;
        var right = 0;
        var arr =[];
        if (typeof number!=="undefined") {
            number = number.toString();
            var str = number.split(".");
            right = str[1];
            number = str[0];
        }
        var totalLength = number.length;
        var len = totalLength % 3 === 0 ? totalLength / 3 : (~~totalLength / 3 + 1);
        for (var i = 1; i <= len; i++) {

            arr.push(number.substring(totalLength - i * 3, totalLength - (i - 1) * 3));
        }
        var result = arr.reverse().join(",");
        if (right && right.length) {
            result += "." + right;
        }
        return result;
    }
    //股票部分
    if (typeof v_sz002555 !== "undefined") {
        var elements = v_sz002555.split("~");
        var timeArr = [];
        var hms = [];
        timeArr.push( elements[30].slice(0,4));
        timeArr.push( elements[30].slice(4,6));
        timeArr.push( elements[30].slice(6,8));
        hms.push (elements[30].slice(8,10));
        hms.push (elements[30].slice(10,12));
        hms.push (elements[30].slice(12,14));
        if ( elements[31] > 0) {
            elements[31] = '+' + elements[31]
            elements[32] = '+' + elements[32]
        }

        var dom1 =  '三七互娱 (002555) ' +
                '<span>' + '截至' +  timeArr.join("/") + "&nbsp;&nbsp;" + hms.join(":") + '&nbsp;&nbsp;&nbsp;&nbsp;(报价有十五分钟或以上延迟，资料来源： 新浪财经)' + '</span> ';
        var dom2 = '<span class="price count-number">' + elements[3] + '</span> ' +
                '<span class="price-0 count-number">' + elements[31] + '&nbsp(' + elements[32] + '%)' + '</span> ';
        var dom3 = '<span>' + elements[5] + '</span> ' +
                '<span>' + elements[4] + '</span> ' +
                '<span>' + elements[41] + '</span> ' +
                '<span>' + elements[42] + '</span>' +
                '<span>' + (fixNum(elements[36]/100)) + '万手</span>' +
                '<span>' + elements[45] + '亿</span>'
        if( + elements[31] > 0) {
            $(".shares").addClass("red").removeClass("green");
        } else {
            $(".shares").addClass("green").remove("red");
        }
        $(".shares > p").html( dom1 );
        $(".share-price").html( dom2 );
        $(".share-data > p + p").html( dom3 );
    }</script>
