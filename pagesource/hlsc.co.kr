<script type="text/javascript">
$(document).ready(function(){

// totop
$(".totop").hide(); // 탑 버튼 숨김
$(function () {
                 
	$(window).scroll(function () {
		if ($(this).scrollTop() > 100) { // 스크롤 내릴 표시
			$('.totop').fadeIn();
		} else {
			$('.totop').fadeOut();
		}
	});
			
	$('.totop').click(function () {
		$('body,html').animate({
			scrollTop: 0
		}, 500);  // 탑 이동 스크롤 속도
		return false;
	});
});


// 스크롤 부드럽게
$(".page_in_scroll").click(function(event){            
		event.preventDefault();
		$('html,body').animate({scrollTop:$(this.hash).offset().top}, 500);
});

// 전체메뉴보기
$('.btn_menu_open').click(function() {
	$('.sitemap_wrap').stop(true,true).slideDown('');
});
$('.btn_menu_close').click(function(){
	$('.sitemap_wrap').stop(true,true).slideUp('');
});


// gnb
$('.gnb ul li').hover(function() {
	$(this).find('.gnb_sub').stop(true,true).fadeIn('fast');		
}, function() {
	$(this).find('.gnb_sub').stop(true,true).fadeOut('fast');
});
$('.gnb ul li').click(function(){
	$('.gnb ul li').not($(this)).removeClass('on');
	$(this).addClass('on');
});



// 메인 브랜드 on/off
$('.cut_icon').toggle(function() {
	$(this).html('<a><img src="/images/cut_icon_down.png" alt="브랜드 펼치기" /></a>');
	$('.cut').slideUp();
}, function() {
	$(this).html('<a><img src="/images/cut_icon_up.png" alt="브랜드 숨기기" /></a>');
	$('.cut').slideDown();
});

// 메인 상품 big
$('#main .product .pro_in a').hover(function() {
	$(this).find('.pro_over_big').stop(true,true).fadeIn('');		
}, function() {
	$(this).find('.pro_over_big').stop(true,true).fadeOut('');
});

// 메인 상품 small
$('#main .product ul li a').hover(function() {
	$(this).find('.pro_over').stop(true,true).fadeIn('');		
}, function() {
	$(this).find('.pro_over').stop(true,true).fadeOut('');
});

// 메인 커뮤니티
$('#main .community dl a').hover(function() {
	$(this).find('.community_over').stop(true,true).fadeIn('');		
}, function() {
	$(this).find('.community_over').stop(true,true).fadeOut('');
});

// 포토갤러리
$('.post_photo a').hover(function() {
	$(this).find('.post_over').stop(true,true).fadeIn('');		
}, function() {
	$(this).find('.post_over').stop(true,true).fadeOut('');
});

// 리스트 보기 형식
$('.list_option2').click(function() {
	$('.product_list').addClass('product_list2');
	$(this).addClass('on');
	$('.list_option1').removeClass('on');
});
$('.list_option1').click(function() {
	$('.product_list').removeClass('product_list2');
	$(this).addClass('on');
	$('.list_option2').removeClass('on');
});

// 브랜드 카테고리 보기
$('.brand_category ul li').hover(function(){	
	$(this).find('.brand_category_sub').show();
}, function() {
	$(this).find('.brand_category_sub').hide();
});

// 브랜드 카테고리 on/off
$('.brand_category ul li').click(function(){
	$('.brand_category ul li').not($(this)).removeClass('on');
	$(this).addClass('on');
});

// tab on/off
$('.tab_common li a').click(function(){
	$('.common_tabview').not($('.cont_'+$(this).attr("id"))).hide();
	$('.cont_'+$(this).attr("id")).show();

	$('.tab_common li a').not($(this)).removeClass('on');
	$(this).addClass('on');	

	return false;
});

// faq on/off
$('.faq_list dl').click(function(){
	$('.faq_list dl').not($(this)).removeClass('on');
	$(this).addClass('on');
});

// footer 메뉴 on/off
$('.tab_footer li').click(function(){
	$('.common_tabview').not($('.cont_'+$(this).attr("id"))).hide();
	$('.cont_'+$(this).attr("id")).show();

	$('.tab_footer li').not($(this)).removeClass('on');
	$(this).addClass('on');
});



