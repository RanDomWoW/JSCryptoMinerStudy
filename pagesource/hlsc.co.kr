		var _POPUP_START_DAY = "2018-03-02";			// 팝업1 시작일
		var _POPUP_END_DAY = "2018-03-16";				// 팝업1 종료일
		
		// 팝업2
		var _POPUP_START_DAY2 = "2018-02-02";			// 팝업2 시작일
		var _POPUP_END_DAY2 = "2018-02-12";				// 팝업2 종료일
		
		$(function () {
					
			var arrSDay = _POPUP_START_DAY.split("-");
			var arrEDay = _POPUP_END_DAY.split("-");
			
			var startTime = new Date(arrSDay[0], arrSDay[1] - 1, arrSDay[2]).getTime();
			var endTime = new Date(arrEDay[0], arrEDay[1] -1, arrEDay[2], 23, 59, 59).getTime();
			var todayTime = new Date().getTime();
			
			var popup1Open = false;
			
			$('#hlsc-popup-modal-str').magnificPopup({
				type: 'inline',
				preloader: false,
				modal: true,
				callbacks: {
							  afterClose: function() {
			//				    console.log('close of popup1');
								setTimeout(function(){
														popup2Open(); 
												}, 500);
								
								
								
							  }
				}
			});
			
			$('#hlsc-popup-modal-str2').magnificPopup({
				type: 'inline',
				preloader: false,
				modal: true
			
			});
			
			$(document).on('click', '.popup-modal-dismiss', function (e) {
				e.preventDefault();
				$.magnificPopup.close();
			});
			
			$(".today_hidden_checkbox").click(clickTodayHidden);
			
			
			
			if (startTime <= todayTime && endTime >= todayTime) {
				if($.cookie('today_popup') != 'hidden') {
					// 팝업 
					$('#hlsc-popup-modal-str').magnificPopup('open'); 	
					popup1Open = true;	
		//			console.log("popuped1");	
				}
			}
			
			if (!popup1Open) {
				popup2Open();
			}
			
			function popup2Open () {
				// 팝업2
				var arrSDay = _POPUP_START_DAY2.split("-");
				var arrEDay = _POPUP_END_DAY2.split("-");
				
				var startTime = new Date(arrSDay[0], arrSDay[1] - 1, arrSDay[2]).getTime();
				var endTime = new Date(arrEDay[0], arrEDay[1] -1, arrEDay[2], 23, 59, 59).getTime();
				var todayTime = new Date().getTime();
			
				
		
				if (startTime <= todayTime && endTime >= todayTime) {
					if($.cookie('today_popup2') != 'hidden') {
						// 팝업 
						$('#hlsc-popup-modal-str2').magnificPopup('open'); 	
			//			console.log("popuped2");
					}
					
				}
				
			}
			
			
		});
		
		
		function clickTodayHidden(e) {
			var $el = $(e.target);
			if ( $el.prop("checked")) {
				var cookieStr = $el.attr("attr_popup_no") == "2" ? "today_popup2" : "today_popup";
				
				$.cookie(cookieStr, 'hidden', {expires : 1});
				$.magnificPopup.close();
	
			}
		}
		
	</script>
	



	
	
    


</body>
</html>