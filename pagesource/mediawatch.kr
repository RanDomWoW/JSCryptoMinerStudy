			if($(data.html).find("#skin_content_" + lay_no).children().length >= $("#skin_content_" + lay_no).attr("data-count"))
			{
				$("#btn_more_" + lay_no).show();
			}
			$("#btn_more_loading_" + lay_no).hide();
		},
	});
}
