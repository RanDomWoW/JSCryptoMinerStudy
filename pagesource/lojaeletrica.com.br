        $(document).ready(function(){

            //$('.popUpTemp').css("display","block");

            //$('#btFecharPopup').click(function(){
            //    $('.popUpTemp').css("display","none");
            //});
            
            $('#slides').slidesjs({
                navigation: {
                    effect: "fade"
