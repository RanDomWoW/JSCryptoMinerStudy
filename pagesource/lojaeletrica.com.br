                    effect: "fade"
                },
                play: {
                    auto: true,
                    interval: 6000,
                    effect: "fade"
                },
                effect: {
                    fade: {
                        speed: 600
                    }
                }
            });
           
            
            $('#combo').change(function () {
                $('#text_combo_header').html($('#combo option:selected').text());
            });

              
            $('#divMenuDepartamentos').mouseover(function () { $('.divDropMenu').css('display', 'block') });
            $('#divMenuDepartamentos').mouseout(function () { $('.divDropMenu').css('display', 'none') });
            

            $('#divProductCompare').dialog({
                autoOpen: false,
                width: 800,
                draggable: false,
                resizable: false,
                modal: true,
                closeOnEscape: true,
                closeText: '',
                zIndex: 9999,
                title: "Vertico: Comparação de produtos",
                open: function(event, ui) {
                    
                    $.ajax({
                        type: "POST",
                        url: "http://www.lojaeletrica.com.br/services/VerticoServices.asmx/GetProductCompareTable",
                        data: "{idProducts: '" + $("#hdnProductSelecteds").val() + "'}",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function(msg) {
                            var obj;
                            eval("obj = " + msg.d);
                            $("#divProductCompare").html(obj.message);
                        },
                        error: function(httpRequest, status, data) {
                            $('#divProductCompare').dialog( "close" );
                        }
                    });

                    $("#divProductCompare").show();
                }
            });


            $(".bt_newsletter").mouseenter(function(){
                $(this).children("div").show();
            });

            $(".bt_newsletter").mouseleave(function(){
                $(this).children("div").hide();
            });


            $("#hpkNewsLetterSend").click(function() {
                var name = $("#iptNewsLetterName").val();
                var email = $("#iptNewsLetterEmail").val();
                $.ajax({
                    type: "POST",
                    url: "http://www.lojaeletrica.com.br/services/VerticoServices.asmx/CallNewsLetterInsert",
                    data: "{dsName: '" + name + "', dsEmail: '" + email + "'}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function(msg) {
                        setDialogMessage(msg.d);
                        $("#iptNewsLetterName").val("Digite seu nome");
                        $("#iptNewsLetterEmail").val("Digite seu e-mail");
                        $("#dialog").dialog("open");
                    },
                    error: function(httpRequest, status, data) {
                        setDialogMessage(data);
                        $("#dialog").dialog("open");
                    }
                });
            });

            //AUTO COMPLETE
            var AutoCompleteActive = 1;
            if(AutoCompleteActive == 1)
            {
                var txtDsKeyWordId = 'ctl00_txtDsKeyWord';
                var AutoCompleteType = 4;
                $('#' + txtDsKeyWordId).keyup(function() {
                    if($(this).val().length == 0)
                        $("#divAutoComplete").fadeOut("fast");
                    else
                        $.ajax({
                            type: "POST",
                            url: "http://www.lojaeletrica.com.br/services/VerticoServices.asmx/QuickSearch",
                            data: "{dsKeyWord:'" + $(this).val() + "',type:'" + AutoCompleteType + "'}",
                            contentType: "application/json; charset=utf-8",
                            dataType: "json",
                            success: function(msg) {
                                var obj;
                                eval("obj = " + msg.d);
                                if(obj.recordcount > 0)
                                {
                                    $("#divAutoComplete").html(obj.message);
                                    $("#divAutoComplete").fadeIn("fast");
                                }
                                else
                                    $("#divAutoComplete").fadeOut("fast");
                            },
                            complete: function() {
                                //clearQuickSearchRequest();
                            },
                            error: function(httpRequest, status, data) {
                                //alert(data);
                            }
                        });
                });
                $('#' + txtDsKeyWordId).blur(function() {
                    $("#divAutoComplete").fadeOut("fast");
                });
            }
            //AUTO COMPLETE
           
            //EFEITO LISTAGEM
            var Effect = 'fadeIn(x,"easeInOutQuad");';
            var dsClass = '.box_prods_home'
            if(Effect != null && Effect != "") 
            {
                var x = 300;
                $(dsClass).children("li").each(function(){
                    $(this).fadeIn(x,"easeInOutQuad");
                    x = x + 300;
                });
            }
            else
            {
                $(dsClass).children("ul").children("li").show();
            }
            //EFEITO LISTAGEM fadeIn(x);

        });

        function productRememberLoadSku()
        {
         
            $.ajax({
                type: "POST",
                url: "http://www.lojaeletrica.com.br/services/VerticoServices.asmx/GetProductSku",
                data: "{idProduct:'" + $('#dvProductRemember').attr("idProduct") + "'}",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function(msg) {
                    var obj;
                    eval("obj = " + msg.d);
                    if (obj.isError == 0) {
                        var sku = obj.message.split(",");
                        var ddl = "<option value='0'>Selecione</option>";
                        for(i = 0; i < sku.length; i++)
                        {
                            ddl += "<option value='" + sku[i] + "'>" + sku[i].split("-")[1] + "</option> \n";
                        }
                        $("#ddlRememberSku").html(ddl);
                        $("#trProductRememberSku").show();
                    }
                },
                error: function(httpRequest, status, data) {
                        
                }
            });   
        }

        function productRememberSend() {
                 
            var idProduct = $('#dvProductRemember').attr("idProduct");
            
            if(!Page_ClientValidate("ProductRemember"))
            {
                $("#ctl00_lblRemeberError").html("");
                return;
            }
            
            if($("#trProductRememberSku").is(":visible"))
            {
                if($("#ddlRememberSku").val() == "0")
                {
                    $("#ctl00_lblRemeberError").html("Por favor, selecione um tamanho");
                    return;
                }
                idProduct = $("#ddlRememberSku").val();
            }

            $("#dvProductRememberLoading").height($('#ctl00_dvProductRememberForm').height());
            $('#ctl00_dvProductRememberForm').fadeOut("fast", function() {
                $("#dvProductRememberLoading").fadeIn("fast", function() {
                    $.ajax({
                        type: "POST",
                        url: "http://www.lojaeletrica.com.br/services/VerticoServices.asmx/CallProductRememberInsert",
                        data: "{idProduct:'" + idProduct + "', dsName: '" + $("#ctl00_iptProductRememberName").val() + "', dsEmail: '" + $("#ctl00_iptProductRememberEmail").val() + "'}",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function(msg) {
                            var obj;
                            eval("obj = " + msg.d);
                            if (obj.isError == 0) {
                                $("#ctl00_iptProductRememberName").val("");
                                $("#ctl00_iptProductRememberEmail").val("");
                            }
                            $("#ctl00_lblRemeberError").html(obj.message);
                            
                            $("#dvProductRememberLoading").hide();
                            $('#ctl00_dvProductRememberForm').show();
                        },
                        error: function(httpRequest, status, data) {
                            $("#ctl00_lblRemeberError").html(data);
                            
                            $("#dvProductRememberLoading").hide();
                            $('#ctl00_dvProductRememberForm').show();
                        }
                    });
                });
            });
        }

    </script>

