        $(".FiltroInativo").hide();

        $(".conteudo_menu").each(function(index) {
            if ($(this).children("ul").children("li.FiltroInativo").size() > 0) {
                $(this).children("ul").children("li.MostrarTodos").show();
            }
        });

        $(".MostrarTodos").each(function(index) {
            if ($(this).parent().children("li.FiltroAtivo").size() < 1) {
                $(this).parent().children("li.FiltroInativo").show();
                $(this).children("a").children("strong").html("ocultar (-)");
