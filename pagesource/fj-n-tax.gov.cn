			var isIE = userAgent.indexOf("compatible") > -1 && userAgent.indexOf("MSIE") > -1 && !isOpera; //判断是否为IE浏览器
			if(isIE){
				var IE5 = IE55 = IE6 =IE7 = false;
				var reIE = new RegExp("MSIE (\\d+\\.\\d+);");
				reIE.test(userAgent);
				var FIEVersion = parseFloat(RegExp["$1"]);
				IE55 = FIEVersion ==5.5;
				IE6 = FIEVersion ==6.0;
				IE7 = FIEVersion ==7.0;
				if(IE55){
					return "IE55";
				}
				if(IE6){
					return "IE6";
				}
				if(IE7){
					return "IE7";
				}
			}//isIE  end
