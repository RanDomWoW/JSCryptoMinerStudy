            }

            var observer = (observers.length === 0 || observerExists(threshold).length === 0)
                    ? createObserver(threshold)
                    : observerExists(threshold)[0];
            
            if (elements.indexOf(element) === -1) {
                elements.push(element);
                observer.observe(element);
            }
        });

