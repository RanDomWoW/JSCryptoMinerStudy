                $('span.loading_large').remove();
            }
        }

        function number_format(number, decimals, thousands_sep, dec_point)
        {
            number = (number + '')
                    .replace(/[^0-9+\-Ee.]/g, '');
            var n = !isFinite(+number) ? 0 : +number,
                    prec = !isFinite(+decimals) ? 0 : Math.abs(decimals),
                    sep = (typeof thousands_sep === 'undefined') ? ',' : thousands_sep,
                    dec = (typeof dec_point === 'undefined') ? '.' : dec_point,
                    s = '',
                    toFixedFix = function (n, prec) {
                        var k = Math.pow(10, prec);
                        return '' + (Math.round(n * k) / k)
                                .toFixed(prec);
                    };
            // Fix for IE parseFloat(0.55).toFixed(0) = 0;
            s = (prec ? toFixedFix(n, prec) : '' + Math.round(n))
                    .split('.');
            if (s[0].length > 3) {
                s[0] = s[0].replace(/\B(?=(?:\d{3})+(?!\d))/g, sep);
            }
            if ((s[1] || '')
                    .length < prec) {
                s[1] = s[1] || '';
                s[1] += new Array(prec - s[1].length + 1)
                        .join('0');
            }
            return s.join(dec);
        }

        function calculate_add_on_status()
        {
            while (loading_sub_build > 0) {
                setTimeout(function () {
                    calculate_add_on_status();
                }, 100);
                return;
            }
            var data = 'custom_product_id=0';
            $('input[type="radio"]:checked').each(function () {
                data += '&' + $(this).attr('name') + '=' + $(this).val();
            });
            if (data !== '') {
                $.ajax({
                    type: 'POST',
                    url: 'product_information_loader.php',
                    data: data,
                    dataType: 'json'
                }).done(function (response, textStatus, jqXHR) {
                    if (response !== null) {
//console.log(response.data);
                        var addon_display = response.data.additional_charge;
//console.log(response.data);
                        var total_addon = Number(addon_display.replace(/[^0-9\.\-]+/g, ""));
                        if (addon_display.indexOf(';') > 0) {
                            total_addon = Number(addon_display.substring(addon_display.indexOf(';')).replace(/[^0-9\.\-]+/g, ""));
                        }
//console.log(total_addon);
                        if ($('td.parent_product_price > span.productSpecialPrice').length > 0) {
                            var base_total = Number($('td.parent_product_price > span.productSpecialPrice').text().replace(/[^0-9\.]+/g, ""));
                        } else {
                            var base_total = Number($('td.parent_product_price > span').text().replace(/[^0-9\.]+/g, ""));
                        }
//console.log(base_total);
                        var total_sum = Math.round((base_total + total_addon) * 100) / 100;
                        var shopping_cart_value = $('div.header_cart').find('span.price').eq(0).text();
                        var shopping_cart_currency = shopping_cart_value.split(/[0-9\.\,]+/g);
                        var total_sum_string = shopping_cart_currency[0]
                        if ('USD' !== 'JPY') {
                            total_sum_string += " " + number_format(total_sum, 2) + " ";
                        } else {
                            total_sum_string += " " + number_format(total_sum, 0) + " ";
                        }
                            total_sum_string += shopping_cart_currency[1];
                        if (response.data.is_out_of_stock === true) {
                            $("#build_btn").addClass('out_of_stock');
                            $("#build_btn > span.build_btn_text").text('Out of Stock');
                            $("#build_btn > span.add_on_value").hide();
                            $("#build_btn > span.order_in").hide();
                            $("div.build_summary_buy_btn").hide();
                            $("#build_summary_buy td.build_status").html('<span class="outofstock">Back In Stock Soon</span>')
                            $("#build_summary_buy td.build_price").html(total_sum_string);
                            $("#build_summary_buy_copy td.build_status").html('<span class="outofstock">Back In Stock Soon</span>')
                            $("#build_summary_buy_copy td.build_price").html(total_sum_string);
                        } else {
                            $("#build_btn").removeClass('out_of_stock');
                            $("#build_btn > span.build_btn_text").text('BUY');

                            $("#build_btn > span.add_on_value").html(total_sum_string);
                            if (response.data.is_order_in === true) {
                                $("#build_summary_buy td.build_status").html('<span class="orderin">Order In</span>')
                                $("#build_summary_buy_copy td.build_status").html('<span class="orderin">Order In</span>')
                            } else {
                                $("#build_summary_buy td.build_status").html('<span class="instock">In Stock</span>')
                                $("#build_summary_buy_copy td.build_status").html('<span class="instock">In Stock</span>')
                            }
                            $("#build_summary_buy td.build_price").html(total_sum_string);
                            $("#build_summary_buy_copy td.build_price").html(total_sum_string);
                            $("div.build_summary_buy_btn").show();
                            if (response.data.has_additional_charge === true) {
                                $("#build_btn > span.add_on_value").show();
                            } else {
                                $("#build_btn > span.add_on_value").hide();
                            }
                            if (response.data.is_order_in === true) {
                                $("#build_btn > span.order_in").text('Order In').show();
                            } else {
                                $("#build_btn > span.order_in").hide();
                            }
                        }
                    }

                    //New patch for Custom Build Summary Table 2015-07-08
                    //initialize the table by remove all the data row first
                    $("table#build_summary").find('tr.build_components').each(function () {
                        $(this).remove();
                    });
                    // then try to reinsert each data row again.
                    var counter = 1;
                    $('input[type="radio"]:checked').each(function () {
                        var row_html = $('<tr>');
                        $(row_html).addClass('build_components');
                        if (counter % 2 !== 0) {
                            $(row_html).addClass('productListing-odd');
                        }
                        counter++;
                        var curr_tr = $(this).closest('tr');
                        while ($(curr_tr).prev('tr').has('td.build_catory_label').length < 1) {
                            curr_tr = $(curr_tr).prev('tr');
                        }
                        var category_label = $(curr_tr).prev('tr').children('td.build_catory_label').text();
                        var selected_attributeset_name = $(this).attr('name');
                        var selected_attributeset_val = $(this).attr('value');
                        var selected_product_name = $(this).attr('data');
                        var selected_product_status = '';
                        if (typeof ($(this).attr('status')) !== "undefined") {
                            selected_product_status = $(this).attr('status');
                        }
                        var selected_product_price_diff = $(this).attr('pricediff');
                        var regExp = /\[([^\]]+)\]/;
                        var matches = regExp.exec(selected_attributeset_name);
                        if (matches !== null) {
                            var warning_message = '';
                            if (category_label.toLowerCase().indexOf('(optional)') < 0) {
                                warning_message = '<span class="summary_warning_message">Please Select ' + category_label + '</span>';
                            } else {
                                warning_message = '<span>No ' + category_label + '</span>';
                            }

                            var product_name = $('<span>');
                            var price_diff = '';
                            var message = '';
                            if (selected_product_price_diff !== '') {
                                price_diff = selected_product_price_diff;
                            }
                            if (parseInt(selected_attributeset_val) !== 0) {
                                message = selected_product_name;
                                if ((selected_product_status !== '') && (selected_product_status.toLowerCase().indexOf('in stock') < 0)) {
                                    if ((selected_product_status !== '') && (selected_product_status.toLowerCase().indexOf('in stock') < 0)) {
                                        if (selected_product_status.toLowerCase().indexOf('order in') < 0) {
                                            $(product_name).addClass('outofstock');
                                            selected_product_status = 'Back In Stock Soon';
                                        } else {
                                            $(product_name).addClass('orderin');
                                            selected_product_status = 'Order In';
                                        }
                                    }

                                    message = $(product_name).html(selected_product_name + ' (' + selected_product_status + ')');
                                }
                            }
                            var td = $('<td>');
                            $(td).addClass('component');
                            var category_label_span = $('<span>');
                            $(category_label_span).addClass('component_label');
                            $(category_label_span).html(category_label);
                            $(td).html(category_label_span);
                            $(row_html).append($(td));
                            td = $('<td>');
                            $(td).addClass('attributeset_' + matches[1]).addClass('build_pick');
                            if (message.length > 0) {
                                $(td).html(message);
                            } else {
                                $(td).html(warning_message);
                            }
                            $(row_html).append($(td));
                            td = $('<td>');
                            $(td).addClass('additional_cost');
                            $(td).html(price_diff);
                            $(row_html).append($(td));
                            $("table#build_summary").append($(row_html));
                            $(row_html).show('slow');
                        }
                    });
                }).fail(function (jqXHR, textStatus, errorThrown) {
                    console.log(errorThrown);
                });
            }
        }

        $(document).on('click', 'td.build_attributeset_selector:not(".selected_option")', function (event) {
            event.stopPropagation();
            var selector_name = $(this).children('input:radio').eq(0).attr('name');
            if ($(this).closest('table.subbuild_product_table').length === 0) {
                // only retrieve the sub build list for the 1st level
                var selected_td_build = $(this);
                var data = {products_id: $(this).children('input[name="' + selector_name + '"]').eq(0).val(), attributeset_id_name: selector_name};
                show_loading_msg();
                $.ajax({
                    type: 'POST',
                    url: 'nested_product_build.php',
                    data: data,
                    dataType: 'json'
                }).done(function (data, textStatus, jqXHR) {
                    var selected_class_name = $(selected_td_build).attr('class').split(" ");
                    for (i = 0; i < selected_class_name.length; i++) {
                        if (selected_class_name[i].indexOf('cbid_') >= 0) {
                            $('td.' + selected_class_name[i]).removeClass('selected_option');
                            $('td.' + selected_class_name[i]).find('div.nested_build').remove();
                        }
                    }
                    //$('div.nested_build').remove();
                    $(selected_td_build).addClass('selected_option');
                    if (data.response === 'success') {
                        $(selected_td_build).append(data.data);
                    }
                })
                        .fail(function (jqXHR, textStatus, errorThrown) {
                            console.log(errorThrown);
                        })
                        .always(function () {
                            loading_sub_build -= 1;
                            hide_loading_msg();
                        });
            }
            $('input[name="' + selector_name + '"]').prop('checked', false);
            $(this).children('input[name="' + selector_name + '"]').eq(0).prop('checked', true).change();
        });

        //update the variant options choice for dropdown-radio selection attribute product
        $(document).on('change', 'select.build_attributeset_selector', function (event) {
            event.stopPropagation();
            show_loading_msg();
            var selection = $(this);
            var data = {products_id: $(this).val(), build_attributeset_id: $(this).attr('id'), custom_product_id: 0};
            $.ajax({
                type: 'POST',
                url: 'product_information_loader.php',
                data: data,
                dataType: 'json'
            }).done(function (data, textStatus, jqXHR) {
                var img_path = "images/" + data.data;
                $(selection).prev('div.image_holder').children('img').eq(0).attr('src', img_path);
                var variant_option = $(selection).nextAll('div.variant_option').eq(0);
//console.log(data.variant);
                $(variant_option).html(data.variant);
                if (data.subbuild !== '') {
                    $(variant_option).siblings('div.nested_build').each(function () {
                        $(this).remove();
                    });
                    $(variant_option).after(data.subbuild);
                }
                var first_variant_option = $(variant_option).find('input[type="radio"]').first();
                if ($(first_variant_option).attr('checked')) {
                    $(variant_option).find('input[type="radio"]').first().prop('checked', true).change();
                }
                calculate_add_on_status();
            })
                    .always(function () {
                        hide_loading_msg();
                    });
        });

        $(document).on('click', 'div.variant_option > span', function (event) {
            event.stopPropagation();
            $(this).siblings('input[name^="selected_attributeset"]').prop('checked', false);
            $(this).prev('input[name^="selected_attributeset"]').prop('checked', true).change();
        });

        var product_information_loading_timeout = null;
        if ($('form[name="custom_build"]').length > 0) {
            $(document).on('change', $('form[name="custom_build"]').children('input[type="radio"]:not(:checked)'), function () {
                clearTimeout(product_information_loading_timeout);
                product_information_loading_timeout = setTimeout(function () {
                    calculate_add_on_status()
                }, 100);
            });
        }

        if ($('form[name="custom_build"]').length > 0) {
            $('td.build_attributeset_selector > input[name^="selected_attributeset"]:checked').each(function () {
                loading_sub_build += 1;
                $(this).closest('td.build_attributeset_selector').trigger("click");
            });

            $('form[name="custom_build"]').find('select').each(function () {
                if ($(this).val() !== '0') {
                    $(this).trigger("change");
                }
            });
            clearTimeout(product_information_loading_timeout);
            product_information_loading_timeout = setTimeout(function () {
                calculate_add_on_status()
            }, 100);

            $(document).on('mouseover', 'span.summary_warning_message', function () {
                $(this).css('cursor', 'pointer');
            });

            $(document).on('click touchstart', 'span.summary_warning_message', function () {
                var selection_text = $(this).closest('tr').find('span.component_label').text().trim();
                var found = false;
                $('td.build_catory_label').each(function(){
                    if(($(this).text().trim() === selection_text) && (found === false)){
                        found = true;
                        $('html, body').animate({ scrollTop: $(this).offset().top}, 500);
                    }
                });
                if(found === false){
                    $('html, body').animate({ scrollTop: $('div.custom_build_header').offset().top}, 500);
                }
            });

        }

        $(document).on('click', 'div#build_btn, div.build_summary_buy_btn', function (e) {
            var build_button = $('div#build_btn');
            var data = 'custom_product_id=0&';
            $('input[type="radio"]:checked').each(function () {
                data += $(this).attr('name') + '=' + $(this).val() + '&';
            });
            $('select.build_attributeset_selector').each(function () {
                data += 'attributeset_parent[' + $(this).attr('id') + ']=' + $(this).val() + '&';
            });
            $.ajax({
                type: 'POST',
                url: 'product_information_loader.php',
                data: data,
                dataType: 'json'
            }).done(function (data, textStatus, jqXHR) {
                if (data.data.is_out_of_stock !== true) {
                    var order_in_message = 'This build has some Order In Parts, there will be some delay, do you still want to procceed?';
                    $('select.build_attributeset_selector.warning').removeClass('warning');
                    $('span.attribute_product.warning').removeClass('warning');
                    if (data.data.has_unfilled_option === true) {
                        e.preventDefault();
                        var warning_message = 'Please select your components from ' + data.data.unfilled_options + '.';
                        alert(warning_message);
                        $('select.build_attributeset_selector').each(function () {
                            if (parseInt($(this).val()) === 0) {
                                $(this).addClass('warning');
                            }
                        });
                        $('input.first_build_option:checked').each(function () {
//                            console.log($(this));
                            if (parseInt($(this).val()) === 0) {
                                $(this).next('span.attribute_product').addClass('warning');
                            }
                        });
                    } else {
                        if (data.data.is_order_in === true) {
                            //var answer = confirm(order_in_message);
                            //if(answer){
                            var input_status = '<input type="hidden" name="order_status" value="order in" />';
                            $(build_button).closest('form').append(input_status);
                            $(build_button).closest('form').submit();
                            //}
                        } else {
                            $(build_button).closest('form').submit();
                        }
                    }
                } else {
                    e.preventDefault();
                    alert('Some item on this build is Out of Stock, please pick another option');
                }
            });
        });

        if (screen.width < 1060) {
            $("#adops-gutter").hide();
        } else {
            $("#adops-gutter").show();
        }

        if ($("#adops-gutterleft").length > 0) {
            var new_window_width = $(window).width();
