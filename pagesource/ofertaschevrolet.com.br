<script type="text/javascript">
    /*----------------------------------------------------------------
        Concessionarias
    ----------------------------------------------------------------*/
    // Abre todas concessionarias
    $(document).on('click', '.bt_todascc', function(e){
        e.preventDefault();

        $('.cp_concessionaria').slideUp(function(){
            $('.concessionaria ul.cp_concessionaria').html('');

            // Monta lista dos os dados carregados
            $.getJSON('/Lead/LocalizeConcessionariaPorRegiao', function(dados) {
                $.each(dados, function(i, dado) {
                    $(".concessionaria ul.listconc").append("<li>" + "<input id='ConcessionariaId" + dado.Id + "' name='ConcessionariaId' value='" + dado.Id + "' type='radio'>" + "<label for='ConcessionariaId" + dado.Id + "'><span class='cc_nome'>" + dado.Nome + "</span><br><span class='cc_endereco'>" + dado.Endereco + " - " + dado.Bairro + " , " + dado.Cidade_Uf + " " + dado.Cep + "</span></label>" + "</li>");
                });

                // Aplica custom scroll
                $("#listAll").mCustomScrollbar({
                    theme: '3d-thick'
                });

                // Retorna a altura padrão
                if ($('#listAll').height() != 305)
                    $('#listAll').animate({ height: 305 }, 300);

                // Autocomplete CC
                $(function(){
                    var arrayCC = [];
                    var lista = $('#listAll li');

                    for (var i = 0; i < lista.length; i++) {
                        var obj = {
                            "id": $(lista[i]).children()[0].id,
                            "title": $($(lista[i]).children('label')).children('.cc_nome')[0].innerText
                        }
                        arrayCC.push(obj);
                    }

                    var globalTimeout = null;

                    // Inicia a busca por digitacao
                    $('#buscaCC').on('keyup', function(){
                        // Inicia busca a partir do 3o caracter
                        if ($('#buscaCC').val().length >= 3){

                            if (globalTimeout != null)
                                clearTimeout(globalTimeout);
                            globalTimeout = setTimeout(SearchFunc, 300);

                            function SearchFunc(){
                                globalTimeout = null;
                                findMatches();
                            }

                            function findMatches(){
                                var strInput = $('#buscaCC').val();
                                var j = 0;

                                for (var i = 0; i < arrayCC.length; i++) {
                                    if (RegExp(strInput, "gim").test(arrayCC[i].title)) {
                                        $('#listAll h5').remove();

                                        // Caso de match exibe a atual
                                        $('#' + arrayCC[i].id).closest('li').show();
                                        j++;
                                    }else{
                                        // Caso não de match esconde a atual
                                        $('#' + arrayCC[i].id).closest('li').hide();
                                    }
                                }

                                if (j <= 4) {
                                    // Caso digite uma concessionária inexistente
                                    if (j == 0) {
                                        $('#listAll h5').remove();
                                        $('#listAll li').hide();
                                        $('#listAll').prepend('<h5>Não encontramos essa concessionária</h5>');
                                    }

                                    // Verifica altura e diminui container
                                    setTimeout(function(){
                                        $('#listAll').animate({ height: $('.mCSB_container').height() }, 300);
                                    }, 600);

                                    return false;
                                }else{
                                    slidingDown();
                                }

                                function slidingUp() {
                                    $('#listAll').slideUp(function(){
                                        $('#listAll').removeAttr('style');
                                        $('#listAll li').hide();
                                    });
                                }

                                function slidingDown() {
                                    $('#listAll').slideDown(function(){
                                        // Retorna a altura padrão
                                        setTimeout(function(){
                                            if ($('#listAll').height() != 305)
                                                $('#listAll').animate({ height: 305 }, 300);
                                        }, 600);
                                    });
                                }
                            }
                        }else{
                            // Retorna para o estado default, exibe todas as concessionárias
                            $('#listAll h5').remove();
                            $('#listAll li').show();

                            // Retorna a altura padrão
                            setTimeout(function(){
                                if ($('#listAll').height() != 305)
                                    $('#listAll').animate({ height: 305 }, 300);
                            }, 600);
                        }
                    });

                    $('#buscaCC').on('blur', function(){
                        if ($(this).val() == "" || $(this).val().length < 3) {
                            $('#listAll h5').remove();
                            $('#listAll li').show();
                            // Retorna a altura padrão
                            setTimeout(function(){
                                if ($('#listAll').height() != 305)
                                    $('#listAll').animate({ height: 305 }, 300);
                            }, 600);
                        }
                    });
                });
            }, 250);

            $('.concessionaria li input').removeAttr('checked');
            $('.bt_todascc').fadeOut();
            $('.buscaCC').slideDown(function(){
                $('.concessionaria .todascc li:first-child input').attr('checked', true);
                $('.todascc').slideDown(function(){
                    $('.bt_maiscc').fadeIn();
                    $('.cp_busca').removeAttr('disabled');
                });
            });
        });
    });

    // Abre concessionarias próximas
    $(document).on('click', '.bt_maiscc', function(e){
        e.preventDefault();

        $('.buscaCC').slideUp(function(){
            $('#buscaCC').val("");
            $('.concessionaria li input').removeAttr('checked');
            $('.bt_maiscc').fadeOut();
            $('.todascc').slideUp(function(){
                $("#listAll").mCustomScrollbar("destroy");
                $('.concessionaria ul.listconc').html('');

                $.getJSON('/Lead/LocalizeConcessionaria', function(dados) {
                    $.each(dados, function(i, dado) {
                        $(".concessionaria ul.cp_concessionaria").append("<li>" + "<input id='ConcessionariaId" + dado.Id + "' name='ConcessionariaId' value='" + dado.Id + "' type='radio'>" + "<label for='ConcessionariaId" + dado.Id + "'><span class='cc_nome'>" + dado.Nome + "</span> <span class='cc_distancia'>(à " + dado.Distanciakm + "km a partir da localização selecionada)</span><br><span class='cc_endereco'>" + dado.Endereco + " - " + dado.Bairro + " " + dado.Cidade_Uf + " " + dado.Cep + "</span></label>" + "</li>");
                    });

                    $('.concessionaria .cp_concessionaria li:first-child input').attr('checked', true);
                    $('.cp_concessionaria').slideDown(function(){
                        $('.bt_todascc').fadeIn();
                        $('.cp_busca').attr('disabled');
                    });
                }, 250);
            });
        });
    });

    var urlConcessionarias = "/Lead/LocalizeConcessionaria";
    var urlLead = "https://www.ofertaschevrolet.com.br/Lead/Lead";

    $('.lead button').html('Enviar').prop('disabled', false);

    // Preenche select de veiculo
    function veiculoNome(){
        $.ajax({
            type: "POST",
            url: "/Lead/ListarVeiculoNome",
            dataType: 'json',
            data: {},
            success: function(dados) {
                $("#VeiculoNomeId").empty().append('<option value="" style="display:none;"></option>');

                $.each(dados, function(i, dado) {
                    $("#VeiculoNomeId").append('<option value="' + dado.Value + '">' + dado.Text + '</option>');
                });
            }, error: function(ex) {
                //console('Failed to retrieve data. ' + ex);
            }
        });
    }
veiculoNome();
    // Valida se ha preenchimento
    $('.lead input[type="text"], .lead input[type="email"]').blur(function(){
        if ($(this).val() && $(this).is(':invalid'))
            $(this).val('');
    });

    /*----------------------------------------------------------------
        Submit do form
    ----------------------------------------------------------------*/
    // Revalida todos os campos no submit
    $(document).on('submit', '.lead', function(e){
        e.preventDefault();
        var hasError = false;

        // Leitura campo a campo
        $('.dados input').each(function(){
            // if (!validarCampo(this) && !$('.cp_busca'))
            if (!validarCampo(this))
                hasError = true;
        });

        // Valida endereco/concessionaria
        if (hasError === false) {
            hasError = !validarCampo('.cp_concessionaria');
        }

        // Valida autorização
        if (hasError === false && $('#cp_autorizo').is(':checked')) {
            hasError = !validarCampo('#cp_autorizo');
        } else {
            hasError = true;
            $('.txt_autorizo').addClass('erro').removeClass('correto');
        }

        // Entra se formulario esta validado
        if (hasError === false) {
            leadEnvia();

            $.ajax({
                url: urlLead,
                type: 'POST',
                data: $('.lead').serialize(),
                complete: function(dado) {
                    if (dado.responseJSON.XmlStatus == "Accepted")
                    {
                        leadSucesso(dado.responseJSON.Id);
                        dataLayer.push({
                            'transactionId': dado.responseJSON.Id,   //String - ID do Lead
                            'transactionTotal': dado.responseJSON.Preco,   //Integer - Valor da Oferta
                            'transactionAffiliation': dado.responseJSON.Concessionaria,   //String – Concessionaria escolhida
                            'transactionCpfHash': Hash64($("#CPFOriginal").val()),   //String – Concessionaria escolhida
                            'transactionProducts': [{
                                'sku': dado.responseJSON.OfertaVeiculoId, //String - ID da Oferta
                                'name': dado.responseJSON.Modelo, //String - Nome da Oferta
                                'category': dado.responseJSON.Categoria,  //String – Tipo e Modelo do Carro
                                'price': dado.responseJSON.Preco, //Integer - Valor da Oferta
                                'quantity': 1  //Integer – Sempre 1
                            }],
                            'event': 'leadEnv' //String – Valor Fixo
                        });
                    } else {
                        // Alerta o botao em vermelho se houver erros
                        $('.lead button').addClass('erro');
                        setTimeout(function(){
                            $('.lead button').removeClass('erro');
                        }, 1000);

                        alertInfo('Tivemos um problema em nosso sistema. Por favor, envie seus dados novamente.');
                        $('.lead button').attr('disabled', false).html('Enviar novamente');
                    }
                }
            });
        } else {
            // Alerta o botao em vermelho se houver erros
            $('.lead button').addClass('erro');
            setTimeout(function(){
                $('.lead button').removeClass('erro');
            }, 1000);
        }

        // Pausa/cancela submit
        return false;
    });

    function Hash64(cpf) {
        try {
            var Base64 = { _keyStr: "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=", encode: function(e){ var t = ""; var n, r, i, s, o, u, a; var f = 0; e = Base64._utf8_encode(e); while (f < e.length) { n = e.charCodeAt(f++); r = e.charCodeAt(f++); i = e.charCodeAt(f++); s = n >> 2; o = (n & 3) << 4 | r >> 4; u = (r & 15) << 2 | i >> 6; a = i & 63; if (isNaN(r)) { u = a = 64 } else if (isNaN(i)) { a = 64 } t = t + this._keyStr.charAt(s) + this._keyStr.charAt(o) + this._keyStr.charAt(u) + this._keyStr.charAt(a) } return t }, decode: function(e){ var t = ""; var n, r, i; var s, o, u, a; var f = 0; e = e.replace(/[^A-Za-z0-9+/=]/g, ""); while (f < e.length) { s = this._keyStr.indexOf(e.charAt(f++)); o = this._keyStr.indexOf(e.charAt(f++)); u = this._keyStr.indexOf(e.charAt(f++)); a = this._keyStr.indexOf(e.charAt(f++)); n = s << 2 | o >> 4; r = (o & 15) << 4 | u >> 2; i = (u & 3) << 6 | a; t = t + String.fromCharCode(n); if (u != 64) { t = t + String.fromCharCode(r) } if (a != 64) { t = t + String.fromCharCode(i) } } t = Base64._utf8_decode(t); return t }, _utf8_encode: function(e){ e = e.replace(/rn/g, "n"); var t = ""; for (var n = 0; n < e.length; n++) { var r = e.charCodeAt(n); if (r < 128) { t += String.fromCharCode(r) } else if (r > 127 && r < 2048) { t += String.fromCharCode(r >> 6 | 192); t += String.fromCharCode(r & 63 | 128) } else { t += String.fromCharCode(r >> 12 | 224); t += String.fromCharCode(r >> 6 & 63 | 128); t += String.fromCharCode(r & 63 | 128) } } return t }, _utf8_decode: function(e){ var t = ""; var n = 0; var r = c1 = c2 = 0; while (n < e.length) { r = e.charCodeAt(n); if (r < 128) { t += String.fromCharCode(r); n++ } else if (r > 191 && r < 224) { c2 = e.charCodeAt(n + 1); t += String.fromCharCode((r & 31) << 6 | c2 & 63); n += 2 } else { c2 = e.charCodeAt(n + 1); c3 = e.charCodeAt(n + 2); t += String.fromCharCode((r & 15) << 12 | (c2 & 63) << 6 | c3 & 63); n += 3 } } return t } }
            var cpf = Base64.encode(cpf);
            return cpf;
        } catch(e){
            return e.message;
        }
    }
</script>

