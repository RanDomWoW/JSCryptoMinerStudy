    <h1 class="assinatura">Bem-vindo ao site oficial de <span>Ofertas Chevrolet</span></h1>

    <section id="ofertas" class="ofertas ct">


<script type="text/javascript">
    $(document).ready(function () {
        $('#pagina').val("2");

        $("#btnMaisOfertas").click(function (e) {
            e.preventDefault();
            MaisOfertas();
        });

    });

    var pag, heightCard, heightWindow, lastCard, consVal;
    function scrollToCard(elem) {
        consVal = heightWindow <= heightCard ? $(elem).offset().top : $(elem).offset().top - (heightWindow - heightCard) / 2.25;
        setTimeout(function () {
            $('html, body').stop().animate({ scrollTop: consVal }, '350');
        }, 300);

    }

    function MaisOfertas() {
        flbkp = "";
        pag = parseInt($('#pagina').val());
        heightCard = $('#ofertas article').height();
        heightWindow = $(window).height();
        lastCard = $('#ofertas article').last().eq(0);
        $.ajax({
            type: "POST",
            url: "/Home/ListarOfertas",
            dataType: 'json',
            data: { pagina: pag, veiculoNome: $("#btnVeiculoNome").val(), versao: $("#btnVersao").val(), flbkp: $('#flbkp').val() },
            error: function (exception) {
                $("#btnMaisOfertas").hide();
            },
            success: function (dados) {
                if (dados == "" || dados.length != 8) { $("#btnMaisOfertas").hide(); }
                if (dados != "") {
                    $.each(dados, function (i, dado) {
                        $("#ofertas").append("<article class='" + dado.SeloCor + "' data-tipo='" + dado.OfertaDestaqueId + "'><a href='" + dado.UrlOferta + "' rel='nofollow' title='" + dado.TituloCard + "'>"
                                                 + "<img class='selo' src='" + dado.SeloCard + "' alt='" + dado.SeloCardDescricao + "'>"
                                                 + "<img class='veiculo' src='" + dado.ImagemVeiculo + "'>"
                                                 + "<h2>" + dado.Titulo + "</h2>"
                                                 + "<h3 class='ano'>" + dado.Ano + "</h3>"
                                                 + "<h3 class='descricao'>" + dado.Subtitulo + "</h3>"
                                                 + (dado.SeloPromocional === "" ? "" : "<img class='selo_campanha' src='" + dado.SeloPromocional + "' alt='" + dado.SeloPromocionalDescricao + "'/>")
                                                 + dado.Condicao + "<strong class='bt'>Oportunidade única</strong></a></article>");

                        if (dado.TipoCampanha == "4") {
                            if (dado.pagina >= 3) {
                                $("#btnMaisOfertas").hide();
                            }
                        }
                    });
                    pag++;
                    lastCard = $(lastCard).nextAll('article').eq(0);
                    scrollToCard(lastCard);
                    $('#pagina').val(pag);
                }
            }
        });
    };
</script>
