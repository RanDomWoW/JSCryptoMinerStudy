(function(){
	var classesToRemove = [
		{
			selector: '.txt-blue', 
			class: 'txt-blue'
		},
		{
			selector: '.section-title.uppercase', 
			class: 'uppercase'
		},
		{
			selector: '.line-half-left', 
			class: 'line-half-left'
		},
		{ selector: '.line-half-right',
		  class: 'line-half-right' 
		}
	];


	classesToRemove.forEach(function(element) {
		removeClass(element);
	});

	function removeClass(element) {
		var elm = document.querySelectorAll(element.selector),
			i = elm.length;
		
		while(i--) {
			elm[i].className = elm[i].className.replace(element.class,"");
		}
	}
})();

