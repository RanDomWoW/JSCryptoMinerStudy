		$("#btn_header_search").bind("mouseout", "searchBtnOver", removeCssClass);
		$("#btn_header_lang").bind("mouseover", "langBtnOver", addCssClass);
		$("#btn_header_lang").bind("mouseout", "langBtnOver", removeCssClass);
		$("#AKCKwd").keydown(function (evt) {
			if (evt.keyCode == 13)
				ht_search_valid();
		});

		var hotnum = 0;
		jQuery("div.hotlink a").each(function () {
			if (hotnum > 0) {
				jQuery("<span class='bar'>|</span>").insertBefore(jQuery(this));
			}
			hotnum++;
		});
		$("div.searchMenu a").bind("focus", function () { this.blur(); });
	});
</script>
