  <script type="text/javascript">
    $(function() {
        
        var showOverlay = function (sel) {
            if (sel.length) {
                sel.show();
            }
        };
        
        var hideOverlay = function (sel) {
            if (sel.length) {
                sel.fadeOut();
            }  
        };
        
        
        var queries = {};
        $.each(document.location.search.substr(1).split('&'), function(c,q){
            var i = q.split('=');
            if (i[1]) {
                queries[i[0].toString()] = i[1].toString();   
            }
        });
        
        if (queries.ref === 'producthunt') {
            showOverlay($('#product-hunt-overlay'));
        }
        
        $('.hide-overlay-button').click(function(e) {
            e.preventDefault();
            hideOverlay($('#product-hunt-overlay'));
        });
        
        $(document).keyup(function(e) {
             if (e.keyCode == 27) { // escape key maps to keycode `27`
                hideOverlay($('#product-hunt-overlay'));
            }
        });
        
        $('#product-hunt-overlay').click(function() {
            hideOverlay($('#product-hunt-overlay'));
        });
        
        $('#product-hunt-overlay .content-wrapper').click(function(event){
            event.stopPropagation();
        });
    });

      
  </script>

