    function modal(href,wide) {
        $("#modal").modal({
            remote: href,
            show: true
        }).on('shown.bs.modal', function() {
            $(this).find('.selectpicker').selectpicker({
                mobile: true
            });
            if (wide) {
                $(this).find('.modal-dialog').prop("style", "width:700px");
            }
        }).on('hidden.bs.modal', function() {
            $(this).removeData();            
        });
        return false;
    }

    function retinafyCompanyLogos(selector, maxdimension, bywidth) {
        debug("height/width: " + maxdimension +", double: " +(maxdimension*2));
        
        if (window.devicePixelRatio > 1) {
            $(selector+' img.companyLogo').each(function(i) {
                var lowres = $(this).attr('src');
                var highres = "";
                if (bywidth) {
                    highres = lowres.replace("?width="+maxdimension, "?width="+(maxdimension*2));
                } else {
                    highres = lowres.replace("?height="+maxdimension, "?height="+(maxdimension*2));
                }                    
                $(this).attr('src', highres);
            });
        }
    }


    

        
            
