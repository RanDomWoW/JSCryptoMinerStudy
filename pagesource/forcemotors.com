	function getcity(val,$form_type = "footer") {

		$.ajax({

			type: "POST",

			url: "http://forcemotors.com/enquiry/getcity",

			data:"state="+val,

			headers: {

				'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')

			},

			success: function(data){

					if($form_type == "footer"){

						if(data == ""){

							$("#city-selector").attr("readonly","readonly");

							$("#city-selector").html('<option value = "">Select City</option>');

						}else{

							$("#city-selector").removeAttr('readonly');

							$("#city-selector").html('<option value = "">Select City</option>'+data);

						}

					}else if($form_type == "dealer"){

						if(data == ""){

							$("#dealer_city").attr("readonly","readonly");

							$("#dealer_city").html('<option value = "">Select City</option>');

						}else{

							$("#dealer_city").removeAttr('readonly');

							$("#dealer_city").html('<option value = "">Select City</option>'+data);

						}

					}else if($form_type == "customer"){

						if(data == ""){

							$("#cust_city").attr("readonly","readonly");

							$("#cust_city").html('<option value = "">Select City</option>');

						}else{

							$("#cust_city").removeAttr('readonly');

							$("#cust_city").html('<option value = "">Select City</option>'+data);

						}

					}


			}

		});

	}



	function getothercity(val)

    {

        if(val == 'other')

        {

            $("#other_city").show();

            $("#other_city").removeAttr("readonly");

        }

        else {

            $("#other_city").hide();
            $("#other_city").val('');

            $("#other_city").attr( "readonly" );

        }

    }



	function getreachusdetails(val)

    {

        if(val == 'Product' || val == 'Price' || val == 'Dealer' || val == 'Service')

        {

			$("#news_other").hide();
			$("#news_other").val('');

			$("#news_other").attr( "readonly" );

            $("#product_type").show();

            $("#product_type").removeAttr("readonly");

        }

        else if(val == 'News/Other')

		{

			$("#product_type").hide();
			$("#product_type").val('');

			$("#product_type").attr( "readonly" );

            $("#news_other").show();

            $("#news_other").removeAttr("readonly");

        }

		else{

			$("#product_type").hide();
			$("#product_type").val('');

			$("#product_type").attr( "readonly" );

			$("#news_other").hide();
			$("#news_other").val('');

			$("#news_other").attr( "readonly" );

		}

    }

	

	$(function() {

		// Setup form validation on the #register-form element

		$("#footer-form").validate({

			// Specify the validation rules

			rules: {

				name: {

					required:true,

					lettersonly: true

				},

				email: {

					required: true,

					email: true

				},

				phone:{

					required:true,

					digits:true,

					minlength:10,

					maxlength:12

				},

				state: "required",

				city: "required",

				reach_us_for: "required",

				product_type: "required",

				news_other_reach_us: "required"

			},

			// Specify the validation error messages

			messages: {

				name: {

					required: "Please enter your name",

					lettersonly: "Please enter your name in proper format"

				},

				email: {

					required: "Please enter your email",

					email: "Please enter your email in valid format"

				},

				phone:{

					required:"Please enter your phone no",

					digits:"Please enter phone no in valid format",

					minlength:"Contact no should be of minimum 10 digits",

					maxlength:"Contact no should be of maximun 12 digits"

				},

				state : "Please select state",

				city : "Please select city",

				reach_us_for: "Please select any value",

				product_type: "Please select product type",

				news_other_reach_us: "Please enter your message"

			},

			submitHandler: function(form) {
				event.preventDefault();
					var formData = {
						'name'     : $('input[name=name]').val(),
						'email'    : $('input[name=email]').val(),
						'phone'  : $('input[name=phone]').val(),
						'state'  : $('select[name=state]').val(),
						'city'  : $('select[name=city]').val(),
						'other_city'  : $('input[name=other_city]').val(),
						'reach_us_for'  : $('select[name=reach_us_for]').val(),
						'product_type'  : $('select[name=product_type]').val(),
						'message'  : $('textarea[name=message]').val()
					};
					var loader_image = "http://forcemotors.com/themes/frontend/images/loader/ajax-spinner-preloader.gif";
				$.ajax({
					type: "POST",
					url: "http://forcemotors.com/enquiry",
					data: formData,
					headers: {
						'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
					},
					
					beforeSend: function() {
						$('#loader_footer').show();
						$('#loader_footer').html("<img src='"+loader_image+"' />");
					},
					
					success: function(data){
						console.log(data);
						$('#loader_footer').hide();
						$("#success_data_footer").html(data).show().delay(5000).fadeOut();
						$('#footer-form')[0].reset();
					}
				});

			}

		});

	});

	// Added manual function to validator

	jQuery.validator.addMethod("lettersonly", function(value, element) {

		return this.optional(element) || /^[a-zA-Z ]*$/.test(value);

	});
	

</script>



