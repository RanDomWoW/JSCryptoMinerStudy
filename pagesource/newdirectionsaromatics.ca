<script>
  // To store the instance of iScroll
  var objiScroll = "";

  $(document).ready(function()
  {
    $('[data-toggle="popover"]').on("mouseover mouseout", function(e)
    {
      if(e.type==="mouseover")
        $(this).popover('show');
      else
        $(this).popover('hide');
    });

    FastClick.attach(document.body);
      
    $('[data-toggle="tooltip"]').tooltip();
      
    $("#show-site-info").click(function(e){
       $("#site-info").toggle();
        $('html, body').animate({
            scrollTop: $(document).height()
        }, 'slow');
       e.preventDefault();
    });
    
    $("#bell-message").click(function(e)
    {
      var parameters = {
                    "modal_title":'<strong class="h2">Important Info</strong>',
                    "modal_size": "lg",
                    "modal_close_on_click": "yes",
                    "modal_data_from_div": "#message-for-alert"

      };
      showModal(parameters);
      e.preventDefault();
    });

    $('body').on('touchstart.dropdown', '.dropdown-menu', function (e) { e.stopPropagation();});

    $(".hide-important-message").click(function()
    {
      document.cookie="hide_important_message=1; path=/";
    });

    

    $("#show-mobile-search-box").click(function(e)
    {
      var modal_header = '<form method="get" class="form-horizontal" id="searchform" action="/products/advanced_search_result.php"><div class="input-group input-group-lg"><input type="text" name="keywords" class="form-control" id="product-search-xs" placeholder="Product Search"><span class="input-group-btn"><button class="btn btn-nda" type="submit"><i class="fa fa-search"></i></button></span></div><!-- /input-group --></form>';

      var parameters = {
                    "modal_title":modal_header,
                    "modal_size": "lg",
                    "modal_data_from_var": '<div id="products-search-results-box" style="margin-top:-10px"><br><br><strong>Type the product name in the search box given above.</strong><br><br><br></div>'};

      showModal(parameters);

      $("#product-search-xs").autocomplete({
        serviceUrl: '/products/quick_product_search.php?xs=1',
        maxHeight: 300,
        dataType: "json",
        minChars: 2,
        customContainer: true,

        formatResult: function (suggestion, currentValue, serverError)
        {
          if(serverError===true && typeof suggestion.url==="undefined") {
            return suggestion.message;
          } else {
            if(typeof suggestion.btn_html !=="undefined")
              return "";
          }
        }
      });

      e.preventDefault();
    });

    // Setting focus to the text field inside search box after opening the modal
    // Commented out because it doesn't work properly on default andriod and ios
    // $('#modal-box').on('shown.bs.modal', function (e)
    // {
    //   $("#product-search-xs").focus();
    // });

    //shows and hides the products list when using website on small screens on click
    $('.show-products-list').click(function(e)
    {
      offcanvas_products_list("click");
      e.preventDefault();
    });


    $(".compare-btn .close-btn").click(function()
    {
      show_hide_comparison_btn("hide");
    });

    $(".compare-btn img").click(function()
    {
      show_hide_comparison_btn("show");
    });

    //To show the product comparsion inside modal box.
    $(".compare-btn .message, .compare-btn img, .compare-btn .total-products").click(function()
    {
      var parameters = {
            "modal_title": "<strong class='h2'>Product Price Comparison</strong>",
            "modal_url": "/products/compare.php?action=compare_products",
            "modal_size": "max",
            "data_type": "html"};

      showModal(parameters);
    });


    //To add products in the comparison list
    $(".add-to-compare").click(function()
    {
      var products_id = $(this).attr("data-products-id");
      add_for_comparison(products_id);
    });

    //To show and hide sub categories on small screens inside left navigation menu.
    $(".sub-categories").on('click',function(e)
    {
      var obj = $(this);
      trace_event(e.type);

      $(obj).next("ul").toggle(function()
      {
        if($(this).css('display') === 'none') {
          $(obj).children("i").removeClass("fa-minus").addClass("fa-plus");
          $(obj).removeClass("expanded-nav-bar");
        } else {
          $(obj).children("i").removeClass("fa-plus").addClass("fa-minus");
          $(obj).addClass("expanded-nav-bar");
        }

        if(objiScroll !== "" && c_iscroll_supported)
          objiScroll.refresh();
      });
      e.preventDefault();
    });


    //Show hide the offcanvas main navigation menu on small screens.
    $(".show-offcanvas").on('click',function(e)
    {
      var which_canvas = $(this).attr("data-menu");

      if(which_canvas==="left") {
        if($("body").hasClass("show-offcanvas-left")) {
          $("body").removeClass("show-offcanvas-left");
          $("#fixed-content-overlay").removeClass("content-overlay-only");
        } else {
          $("body").addClass("show-offcanvas-left");
          $("#fixed-content-overlay").addClass("content-overlay-only");
        }
      }

      // iscroll has issues with android browser if attached at time of document load.
      if(objiScroll==="" && c_iscroll_supported)
      {
        $("#i-scroll-wrapper").css("overflow","hidden");

        objiScroll = new IScroll('#i-scroll-wrapper', {
          scrollbars: true,
          mouseWheel: true,
          interactiveScrollbars: true,
          shrinkScrollbars: 'scale',
          fadeScrollbars: true,
          click: c_iscroll_click,
          tap: c_iscroll_tap
        });
      }
      e.preventDefault();
    });

    //When user click on newsletter subscribe button available at bottom all pages.
    $("#subscribe-newsletter-form, #subscribe-market-report-form").submit(function(e)
    {
      var parameters = $(this).serializeArray();
      var email_address = parameters[0].value;

      if(email_address==="") {

        showBottomMessageBox("Enter email address","message-error",true,10);

      } else {

        $(".subscribe-email").attr("disabled","disabled");
        $.get("/company/newsletter.php",parameters,function(data) {
          $(".subscribe-email").removeAttr("disabled");
          showBottomMessageBox(data.message,"message-"+data.messageType,true,10);
        });
      }

      e.preventDefault();
    });

    //Removing no-display class from owl-items once document is loaded.
    $(".owl-item").removeClass("no-display");

    //For content editing purpose only. Work only with office ip.
    $(".page-tools").on("click",function()
    {
      var page_tool_box =  ".page-tools-box";
      var rightpx       = "";

      if($(page_tool_box).css("right")==="-20px") {
        rightpx = "-270px";
        $(this).html('<i class="fa fa-arrow-circle-left fa-lg"></i>');
      } else {
        rightpx = "-20px";
        $(this).html('<i class="fa fa-arrow-circle-right fa-lg"></i>');
      }

      $(page_tool_box).animate({ "right": rightpx}, "fast");
    });

    $(".yes-no-label-btn").click(function()
    {
      var obj         = $(this);
      var parameters  = [];
      var url         = "";
      var method      = "post";
      var btn_group   = false;

      $(this).each(function() {
        $.each(this.attributes, function() {
          // this.attributes is not a plain object, but an array
          // of attribute nodes, which contain both the name and value
          if(this.specified) {
            var field_name  = this.name;
            var field_value = this.value;
            var data_field  = false;

            if(field_name==="url") {
              url = field_value;
            } else if(field_name==="method") {
              method = field_value;
            } else {
              if(field_name.indexOf("data-")!==-1) {
                if(field_name!=="data-btn-group") {
                  field_name = field_name.replace('data-','');
                  field_name = field_name.replace(/-/g,'_');
                  data_field = true;
                } else {
                  btn_group = true;
                  btn_group_name = field_value;
                }
              }
            }

            if(data_field)
              parameters.push({name: field_name, value: field_value});
          }
        });
      });

      $(obj).children().show();

      if(btn_group===false)
        $('.yes-no-label-btn').attr('disabled',true);
      else
        $(obj).attr('disabled',true);
      
      
      $.ajax({
        type:    method,
        url:     url,
        data:    parameters,
        dataType: 'json',
        success: function(data)
        {
          if(btn_group===false) {
            $('.yes-no-label-btn').removeAttr('disabled');
          } else {
            $(obj).removeAttr('disabled');
            $('[data-btn-group="'+btn_group_name+'"]').addClass("label-custom").html("NO <span class='spinner'><i class='fa fa-spinner fa-spin'></i></span>");
          }

          if(parseInt(data.label_status) === 1) {
            $(obj).removeClass("label-danger label-custom");
            $(obj).addClass("label-success").html("YES <span class='spinner'><i class='fa fa-spinner fa-spin'></i></span>");
          } else if(parseInt(data.label_status) === 0) {
            $(obj).removeClass("label-success");
            $(obj).addClass("label-danger").html("NO <span class='spinner'><i class='fa fa-spinner fa-spin'></i></span>");
          }
          $(obj).children().hide();
        }
      });
    });
  });

  


  function add_for_comparison(products_id)
  {
    var parameters = {
                      "action": "add_to_compare",
                      "products_id":products_id
                      };

    $.get("/products/compare.php",parameters,function(data)
    {
      update_compare_box(data);
      var delay_time = 0;

      if(data.products_in_compare_list===1)
      {
        show_hide_comparison_btn("show");
        delay_time = 550;
      }

      if(data.messageType==="error")
        var css_class = "text-danger";
      else
        css_class = "text-success";

      show_hide_comparison_btn("show");

      setTimeout(function()
      {
        var compare_btn = ".compare-btn";
        var message = "<div class='"+css_class+" nomargin padding5'><strong>" + data.message + "</strong></div></strong>";

        $(compare_btn).popover('destroy');
        $(compare_btn).popover({"content":message,"html":true,"placement":"top"});
        $(compare_btn).popover('show');

        setTimeout(function()
        {
          $(".compare-btn").popover('destroy');
        },3000);

      },delay_time);
    });
  }

  function jslog(str,msg_type)
  {
    if(typeof console === "undefined") {
      return true;
    }

    if(0 && 0) {
      if($.isArray(str)) {
        $.each(str, function(key,val) {
          if(typeof val==="object") {
            $.each(val, function(key2,val2)
            {
              str = str + key2 + ": " + val2 + "/n";
            });
          } else {
            str = str +  key + ": " + val + "/n";
          }
        });

        console.log(str);
      } else {
        if(msg_type==="error")
          console.log("%c" + str, "color:red;font-weight:bold");
        else
          console.log(str);
      }
    } else {
      return true;
    }
  }

  function trace_event(event_type)
  {
    $("#events-called").append("<strong>" + event_type + "</strong>" + " ");
  }

  //To show and hide the product list hidden on mobile devices off the canvas
  function offcanvas_products_list(event)
  {
    var row_offcanvas = ".row-offcanvas";
    if(event==="click") {
      if($(row_offcanvas).hasClass("active"))
        show_hide_offcanvas_product_list(false);
      else
        show_hide_offcanvas_product_list(true);
    } else {
      if(event==="swipeLeft") {
        if($(row_offcanvas).hasClass("active")===true)
          show_hide_offcanvas_product_list(false);
      }
      else if(event==="swipeRight") {
        if($(row_offcanvas).hasClass("active")!==true)
          show_hide_offcanvas_product_list(true);
      }
    }
  }

  function show_hide_offcanvas_product_list(show)
  {
    if(show===true) {
      $("#category-carousel-mobile").hide();
      $(window).scrollTop(0);
      $(".row-offcanvas").addClass("active");
      $("#fixed-content-overlay2").addClass("content-overlay-only");
      var height = $("#product-categories").height();
      $("#main-product-area").css({"height":height});
    } else {
      $("#category-carousel-mobile").show();
      $(window).scrollTop(0);
      $(".row-offcanvas").removeClass("active");
      $("#fixed-content-overlay2").removeClass("content-overlay-only");
      $("#main-product-area").removeAttr("style");
    }
  }

  //Update the content in compare box after getting response from server.
  function update_compare_box(data)
  {
    $(".compare-btn .total-products").html("Products: " + data.products_in_compare_list);

    var message = "Click here to compare the product pricing.";

    if(data.products_in_compare_list===1)
      message = "Add 2 or more products to compare pricing.";

    $(".compare-btn .message").html(message);
  }

  //Show hide product comparison box.
  function show_hide_comparison_btn(status)
  {
    if(status==="show")
      $(".compare-btn").animate({left:"0px"}, 500);
    else if(status==="hide")
      $(".compare-btn").animate({left:"-325px"}, 500);
    else if(status==="hide_full")
      $(".compare-btn").animate({left:"-380px"}, 500);
  }

  //To show the loading .gif when content is being loaded on the page.
  $(".show-loading-overlay").click(function()
  {
    $(".loading-overlay").show();
  });

</script>

  <script src="/js/owl.carousel.min.js"></script>

  <script>
    $(document).ready(function()
    {
              var owl = $('#category-carousel');
        
        //Carousel for devices other than mobile phone.
        $(owl).owlCarousel({
          loop:true,
          margin:0,
          items:1,
          autoplay: true,
          dots: true,
          nav:false,
          animateOut: 'fadeOut'
        });

        //On change of slide  
        owl.on('changed.owl.carousel', function(event) 
        {
          var item = event.page.index;     // Position of the current item
          $(".show-slide").parent("li").removeClass("active");
          $("[data-slide-no="+item+"]").parent("li").addClass("active");
          $("#play-pause-carousel").attr("data-autoplay","1").html("<i class='fa fa-pause'></i>");
        });

        //When user click on carousel tabs
        $(".show-slide").click(function(e)
        {
          var slide_no = $(this).attr("data-slide-no");
          owl.trigger("to.owl.carousel",[slide_no]);
          e.preventDefault();
        });
        
      var owl_mobile = $('#category-carousel-mobile');
      //Carousel for mobile phone.
      $(owl_mobile).owlCarousel({
        loop:true,
        margin:0,
        items:1,
        autoplay: true,
        nav: false,
        dots:false,
        animateOut: 'fadeOut'
      });


      //Play pause next previous slide carousel
      $(".play-pause-carousel").click(function(e)
      {
        var objOwl = owl;
    
        if($(this).is(".play-pause-carousel")) {

          if($(this).attr("data-autoplay")=="1") {
            $(this).attr("data-autoplay","0");
            objOwl.trigger("stop.owl.autoplay");
            $(".play-pause-carousel").html("<i class='fa fa-play'></i>");
          } else {
            objOwl.trigger("play.owl.autoplay");
            $(this).attr("data-autoplay","1");
            $(".play-pause-carousel").html("<i class='fa fa-pause'></i>");
          }
        }
        
        e.preventDefault();
      });


      $("#video-box").on("click",function(e) {
        $(this).html('<iframe width="420" height="315" src="https://www.youtube.com/embed/1sf6YNluz14?autoplay=1" style="border:none" allowfullscreen></iframe>');
        e.preventDefault();
      });
    })
  </script>
