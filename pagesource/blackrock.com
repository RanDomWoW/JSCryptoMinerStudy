if (reverse == null) {
reverse = false;
}
people.videoClear();
return people.videoActive($video, reverse);
};
people.videoClear = function() {
var $video;
$video = $('#hp_module6_people .video_item');
return $video.each(function() {
if ($(this).hasClass('active')) {
$(this).css('z-index', '1');
$(this).removeClass('active');
return $(this).removeClass('reverse');
} else {
return $(this).css('z-index', '0');
}
});
};
people.videoActive = function($video, reverse) {
$video.attr('style', '');
$video.addClass('active');
if (reverse) {
return $video.addClass('reverse');
}
};
people.carouselButton = {
'default': 'https://assets.blackrock.com/blk-one-assets/images/media-bin/web/one/us/hp-people/origin-hp-pagination-default.png',
'hover': 'https://assets.blackrock.com/blk-one-assets/images/media-bin/web/one/us/hp-people/origin-hp-pagination-hover.png',
'selected': 'https://assets.blackrock.com/blk-one-assets/images/media-bin/web/one/us/hp-people/origin-hp-pagination-selected.png'
};
people.clearCarousel = function() {
$('#hp_module6_people .profile').hide();
$('#hp_module6_people .carousel_buttons img').removeClass('selected');
return $('#hp_module6_people .carousel_buttons img').attr('src', people.carouselButton['default']);
};
people.selectItem = function($profile, $button) {
$profile.show();
$button.addClass('selected');
return $button.attr('src', people.carouselButton['selected']);
};
people.getNext = function(selector, cls, reverse) {
var $nextItem, $target, wrapped;
if (reverse == null) {
reverse = false;
}
$nextItem = !reverse ? selector.next(cls) : selector.prev(cls);
wrapped = !reverse ? ':first' : ':last';
return $target = $nextItem.length ? $nextItem : $(cls + wrapped);
};
people.transition = function($profile, $button, $video, reverse) {
if (reverse == null) {
reverse = false;
}
people.changeVideo($video, reverse);
return $("#hp_module6_people .profile_container .profiles").fadeOut(500, function() {
people.clearCarousel();
people.selectItem($profile, $button);
return $("#hp_module6_people .profile_container .profiles").fadeIn(500).css("display", "");
});
};
people.advanceCarousel = function(reverse) {
var $nextArticle, $nextButton, $nextVideo;
if (reverse == null) {
reverse = false;
}
$nextArticle = people.getNext($('#hp_module6_people .profile:visible'), '#hp_module6_people .profile', reverse);
$nextButton = people.getNext($('#hp_module6_people .carousel_buttons img.selected'), '#hp_module6_people .carousel_buttons img', reverse);
$nextVideo = people.getNext($('#hp_module6_people .video_item.active'), '#hp_module6_people .video_item', reverse);
return people.transition($nextArticle, $nextButton, $nextVideo, reverse);
};
people.moveCarousel = function(itemNumber) {
var $current, currentNum, nextNum, reverse;
$current = $('#hp_module6_people .active');
currentNum = 1;
if ($current.hasClass('video_item2')) {
currentNum = 2;
}
if ($current.hasClass('video_item3')) {
currentNum = 3;
}
nextNum = itemNumber.substring(itemNumber.length - 1);
if (currentNum !== nextNum) {
reverse = currentNum < nextNum ? true : false;
return people.transition($('#hp_module6_people .profile_' + itemNumber), $('#hp_module6_people img.' + itemNumber), $('#hp_module6_people .video_' + itemNumber), reverse);
}
};
people.move = function() {
var item_number;
item_number = this.className.substring(this.className.indexOf('item'), 5);
people.moveCarousel(item_number);
return window.clearInterval(people.intervalId);
};
people.active = function() {
return $(this).attr('src', people.carouselButton['hover']);
};
people.inactive = function() {
if ($(this).hasClass('selected')) {
return $(this).attr('src', people.carouselButton['selected']);
} else {
return $(this).attr('src', people.carouselButton['default']);
}
};
people.swipeStart = function(event) {
var touch;
if (event.originalEvent.touches.length === 1) {
touch = event.originalEvent.touches[0];
people.swipeStats.startX = touch.pageX;
people.swipeStats.startY = touch.pageY;
return people.swipeStats.startTime = Date.now();
}
};
people.swipeEnd = function(event) {
var duration, horizontal, reverse, touch, vertical;
if (event.originalEvent.changedTouches.length === 1) {
touch = event.originalEvent.changedTouches[0];
vertical = touch.pageY - people.swipeStats.startY;
duration = Date.now() - people.swipeStats.startTime;
if (vertical < 30 && vertical > -30 && duration < 1000) {
horizontal = touch.pageX - people.swipeStats.startX;
people.swipeStats = {};
if (horizontal > 30) {
window.clearInterval(people.intervalId);
return people.advanceCarousel(reverse = true);
} else if (horizontal < -30) {
window.clearInterval(people.intervalId);
return people.advanceCarousel();
}
}
}
};
people.swipeCancel = function(event) {
return people.swipeStats = {};
};
people.playOnClick = function() {
return window.clearInterval(people.intervalId);
};
