            $(curr_id+"_tab").up("table").select("tr a").each(function(e){
                e.up("tr").removeClassName("activo");
            });
            $(curr_id+"_tab").addClassName("activo");
            //Switch footer
            $("article_footers").select("li").each(function(e){e.removeClassName("active")});
            $(curr_id+"_footer").addClassName("active");
        }
    });
    
    
    var fireEvent = function(element,event){
      if (document.createEventObject && isIEVersionLesserThan9()){
           //dispatch for IE
          var evt = document.createEventObject();
          return element.fireEvent('on'+event,evt);
      }
      else{
          // dispatch for firefox + others
          var evt = document.createEvent("HTMLEvents");
          evt.initEvent(event, true, true ); // event type,bubbling,cancelable
          return !element.dispatchEvent(evt);
      }
    }

    function isIEVersionLesserThan9(userAgent) {
      var userAgent = navigator.userAgent;

      if(userAgent.indexOf("MSIE 8.0") != -1) return true;
      if(userAgent.indexOf("MSIE 7.0") != -1) return true;
      if(userAgent.indexOf("MSIE 6.1") != -1) return true;
      if(userAgent.indexOf("MSIE 6.0") != -1) return true;
      return false;
    }
    
    function show_player(box, html) {
      carousel_portada.stop();
      var static = box.down('.noticia_portada_static');
      static.hide();
      var player = box.down('.noticia_portada_player');
      player.update(html);    
      player.show();
      box.addClassName("player_active");
    }

    function hide_player() {
      $("noticias-main").select(".item.player_active").each(function(box) {
        var player = box.down('.noticia_portada_player');
        player.update("");
        player.hide();    
        var static = box.down('.noticia_portada_static');
        static.show();
        box.removeClassName("player_active");
      });
    }

    $$("a.carousel-jumper").each(function(tag) {
      tag.observe("click", function(ev) {
        hide_player();
      });
    });
        
    //Do process clicks on tds
    $$('#noticias-selector tr a').each(function(atag){
      atag.up("td").observe("click",function(event){fireEvent(atag,"click");});
    });
  
