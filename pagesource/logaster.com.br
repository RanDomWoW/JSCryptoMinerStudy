    function loadImages(elements)
    {
        $(elements).each(function(idx, el)
        {
            if (idx < 6)
            {
                $(el).attr('src', $(el).data('src')).load(function()
                {
                    $(el).addClass('loaded');
                    $(el).css('background', 'none')
                });
            }
        });
    }

    loadImages('#gallery-carousel .gallery-logotype img:not(.loaded)');

