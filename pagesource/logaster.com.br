                $(document).ready(function($)
                {
                    var csrftoken = $.cookie('csrftoken');
                    function csrfSafeMethod(method)
                    {
                        // these HTTP methods do not require CSRF protection
                        return (/^(GET|HEAD|OPTIONS|TRACE)$/.test(method));
                    }

                    $.ajaxSetup({
                        crossDomain: false, // obviates need for sameOrigin test
                        beforeSend: function(xhr, settings)
                        {
                            if (!csrfSafeMethod(settings.type))
                            {
                                xhr.setRequestHeader("X-CSRFToken", csrftoken);
                            }
                        }
                    });

                    $(".language-dropdown .dropdown-menu a, .suggestion-site-popover a").click(function () {
                        var url = $(this).attr("href");
                        var haveGetParams = (url.indexOf('?') != -1);
                        var concatSymbol = haveGetParams ? "&" : "?";

                        var crossDomainParams = {
                            ssp: 1
                        };

                        var pv = $.cookie("pv");

                        if (pv !== null) {
                            crossDomainParams.pv = pv;
                        }

                        var MDS_UPID = $.cookie("MDS_UPID");
                        var MDS_UPVK = $.cookie("MDS_UPVK");

                        if (MDS_UPID !== null && MDS_UPVK !== null)
                        {
                            crossDomainParams.MDS_UPID = MDS_UPID;
                            crossDomainParams.MDS_UPVK = MDS_UPVK;
                        }

                        var redirectUrl = url + concatSymbol;
                        if (logasterNS.urlParams.length > 0)
                        {
                            redirectUrl += logasterNS.urlParams + "&";
                        }
                        redirectUrl += $.param(crossDomainParams);
                        window.location.href = redirectUrl;

                        return false;
                    });

                    
    $("a.create-logo").click(function()
    {
