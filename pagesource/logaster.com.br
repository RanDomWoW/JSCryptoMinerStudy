    var ratingCookiePrefix = 'rated_';
    $(".gallery-logotype-info .score").each(function(idx, el)
    {
        $(el).raty({
            path: 'https://s1.logaster.com/static/images/stars/', //refactor this to avoid hardcoded urls
            half: true,
            readOnly:  $.cookie(ratingCookiePrefix + $(el).data('id')) == 'true',
            hintList: ["ruim","fraco","normal","bom","maravilhoso"],
            start: function()
            {
                return $(this).data('score');
            },
            click: function(score, event)
            {
                var ratyContainer = $(this);
                var galleryLogotypeId = ratyContainer.data('id');
                if ($.cookie('rated_' + galleryLogotypeId) != 'true')
                {
                    $.post(
                        'https://www.logaster.com.br/gallery/set_gallery_score/',
                        {'galleryLogotypeId': galleryLogotypeId, 'score': score},
                        function(data)
                        {
                            ratyContainer.raty('start', data.score);
                            ratyContainer.raty('readOnly', true);
                            $.cookie('rated_' + galleryLogotypeId, 'true', { expires: 1 * 365 * 10, path: '/' }); //10 years cookie
                        }
                    );
                }
            }
        });
    });

    $(".comments .score").each(function (idx, el)
    {
        $(el).raty({
            path: 'https://s1.logaster.com/static/images/stars/', //refactor this to avoid hardcoded urls
            half: true,
            readOnly: true,
            hintList: ["ruim", "fraco", "normal", "bom", "maravilhoso"],
            start: function()
            {
                return $(this).data('score');
            }
        });
    });

    var logoCounter = {
        run: function(realLogoCount)
        {
            logoCounter.logoCount = realLogoCount - 40;
            logoCounter.realLogoCount = realLogoCount;
            logoCounter.animate();
        },
        animate: function()
        {
            logoCounter.setLogoCount(logoCounter.logoCount++);
            var timeout = 100;

            if (logoCounter.logoCount >= logoCounter.realLogoCount) return;

            if (logoCounter.logoCount < logoCounter.realLogoCount - 20)
            {
                timeout = (logoCounter.realLogoCount - logoCounter.logoCount - 20);
                timeout = 40 * (20-timeout);
            }
            else
            {
                timeout = (Math.floor(Math.random() * (7)) + 2)*1000;
            }

            //var timeout = logoCounter.logoCount < logoCounter.realLogoCount - 20?100:Math.floor(Math.random() * (7)) + 2*1000;
            window.setTimeout(logoCounter.animate, timeout);
        },

        setLogoCount: function(value)
        {
            $("#logo-count").text(logoCounter.addCommas(value));
        },
        addCommas: function(nStr)
        {
            nStr += '';
            x = nStr.split('.');
            x1 = x[0];
            x2 = x.length > 1 ? '.' + x[1] : '';
            var rgx = /(\d+)(\d{3})/;
            while (rgx.test(x1))
            {
                x1 = x1.replace(rgx, '$1' + ',' + '$2');
            }
            return x1 + x2;
        }
    };

    logoCounter.run(7544379);

