<script>
	(function(){
		var $src = $("#dataHomeGeneratedCtn");
		var $target = $("#damierCtn");
		var $ctn = $("#dataHomeCtn");
		
		var $hNews = $("#cartoucheNewsCartoucheCtn h5", $src);
		var $news = $("#cartoucheNewsCartoucheCtn .cartoucheElement", $src).addClass("newshome");
		var $agenda = $("#cartoucheAgendaCartoucheCtn .agendaElement", $src).addClass("agendahome");
		var $links = $("#weblinksListCtn .listElement", $src).clone().addClass("linkshome");/*ici je clone pour laisser la gestion de l'ordre en admin*/
		var $moreAgenda = $("#cartoucheAgendaCartoucheCtn .more", $src);
		var $moreNews = $("#cartoucheNewsCartoucheCtn .more", $src);
		
		var max = Math.max(Math.max($links.length, $agenda.length),$news.length) ;
		var curZindex = -1;
		
		var f = function(){
			if(curZindex==$target.css("z-index"))
				return;
			
			curZindex=$target.css("z-index");
			$target.empty();
			
			if($news.length>0)
				$ctn.prepend($hNews);
			if($news.length>0)
				$ctn.append($moreNews);
			if($agenda.length>0)
				$ctn.append($moreAgenda);
			
			var cptA=0; var cptL=0; var cptN=0;
			for(var i=0;i<max;i++)
			{
				if(i==0 && $news.length>0 && $target.css("z-index")>1)
				{
					$target.append( $news[cptN++]);
					if(cptL<$links.length)
						$target.append( $links[cptL++] );
					else
						$target.append( $agenda[cptA++] );
				}
				else
				{
					$target.append( $news[cptN++]);
					if((i%2)==0 || $news.length==0)
					{
						if(cptA<$agenda.length)
							$target.append( $agenda[cptA++] );
						else
							$target.append( $links[cptL++] );
						
						if(cptL<$links.length)
							$target.append( $links[cptL++] );
						else
							$target.append( $agenda[cptA++] );
					}
					else
					{
						if(cptL<$links.length)
							$target.append( $links[cptL++] );
						else
							$target.append( $agenda[cptA++] );
						
						if(cptA<$agenda.length)
							$target.append( $agenda[cptA++] );
						else
							$target.append( $links[cptL++] );
					}
				}
			}
		};
		f();
		$(window).resize(f);
	})();
</script>
