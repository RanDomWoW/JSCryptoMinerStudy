<script>
	//Active les parents du menu selectionné
	$("#menuClickMain ul.menu0 a.menuselected").parents("ul").prev("a").addClass("menuselected");

	//Creation du simulateur de menu pour position dans le design
	$("#menuClickMain ul.menu0 > li.menu0").each(function(){
		var $menulnk = $("> a.menu0", $(this));
		var $a = $menulnk.clone().attr("rel", $(this).attr("id"));
		$("img", $a).attr("id", $("img", $a).attr("id")+"menusimulator"); 
		var countsub = $(this).find("ul").length;
		if(countsub>0)
		{
			$a.click(function(evt){ 
				evt.preventDefault();
				$("#menuClickMain ul.menu0 > li.menu0").removeClass("display").filter("#"+$(this).attr("rel")).addClass("display").closest("ul.menu0").addClass("display")
				$("#menusimulator").addClass("behind");
			});
			
			$("<a>",{ "class":"menuclose", "href":"#" }).appendTo($(this));
		}
		
		$a.appendTo("#menusimulator");
	});
	//Neutralise les liens #
	$("#menusimulator a[href=#]").click(function(e){ e.preventDefault(); });
	
	function addMenuEventForDesktop()
	{
		$("#menuClickMain a[href=#]").click(function(e){ e.preventDefault(); });
		$("#menuClickMain a.menuclose").click(function(evt){
			evt.preventDefault();
			$("#menuClickMain ul.menu0").removeClass("display");
			$("#menusimulator").removeClass("behind");
		}).css("cursor", "pointer");
		
		//Gestion des menu avec anchor pour refermer le menu
		$("#menuClickMain a[href^='" + window.location.pathname + "#']").click(function(e){ $(this).closest("li.menu0").find(".menuclose").trigger("click"); });
		
	
		//Agrandi le UL.menu1 si le menu2 est plus haut que le menu1
		$("#menuClickMain li.menu1").each(function(){
			
			var $subul = $("> ul", $(this));
			if($subul.length)
			{
				var parentul = $(this).closest("ul");
				var interval = null;
				
				$(this).hover(function(){
					clearInterval(interval);
					var cursize = -1
					interval = setInterval(function(){
						parentul.css("transition","all .05s");
						parentul.css("min-height",$subul.height());
						if($subul.height()==cursize)
							clearInterval(interval);
						else
							cursize = $subul.height();
					}, 50);
				}, function(){  
					clearInterval(interval);//par securité
					if($subul.hasClass("standopen")) 
						return;
					
					parentul.css("transition","all 1.1s");
					parentul.css("min-height",0);
				});	
			}
		});
	}
	
	//Surdefini un peu le milonic pour garder ouvert les UL lorsqu'on hover le menu milonic
	if(window.$J) //test pour verifier que milonic est chargé
	{
		var originalFctMouseOut = $I;
		var originalFctMouseOver = $J;
		var overmilonicmenu = false;
		$J = function(id)
		{
			overmilonicmenu = true;
			originalFctMouseOver(id);
		}
		
		$I = function()
		{
			overmilonicmenu = false;
			setTimeout(function(){
				if(overmilonicmenu == false)
					$("#menuClickMain ul.standopen").removeClass("standopen");		
			},500);

			originalFctMouseOut();
		}
		
		var originalFctPopup = popup;
		var originalFctPopdown = popdown;
		popup = function(param, id)
		{
			overmilonicmenu = true;
			$("#menuClickMain #"+id).parents("ul").addClass("standopen");
			originalFctPopup(param, id);
		}
		popdown = function()
		{
			overmilonicmenu = false;
			setTimeout(function(){
				if(overmilonicmenu == false)
					$("#menuClickMain ul.standopen").removeClass("standopen");		
			},500);
			originalFctPopdown();
		}
	}
	
/************* RESPONSIVE PART ****************************************************************************/
	/* Fonction anonyme auto appelée pour déclencher le fonctionnement du menu */
	(function() {
		function workoutCopy()
		{
			$("#leftPanel ul > li > a.copy").each(function(){
				var $me = $(this);
				$me.closest("ul").prev("a").css("cursor", "pointer").click(function(evt){ evt.preventDefault(); $me[0].click(); });
			});
		}
		
		function moveMenu()
		{
			var $leftPanel = $("#leftPanel");
	    	var $menuHolderInLeftPanel = $(".menuHolder", $leftPanel);
	    	var $menuHolderInMainPanel = $("#mainPanel .menuHolder").not($menuHolderInLeftPanel);
	    	
	    	if($leftPanel.is(":visible"))
	    	{
	    		if($menuHolderInLeftPanel.is(":empty"))
	    		{
	    			$menuHolderInLeftPanel.append($menuHolderInMainPanel.children().remove()).find("ul").removeAttr("style");
	    			neutralizeLeftPanelLink();
	    			workoutCopy();
	    			responsiveMenuOpen();
	    		}
	    	}
	    	else if($menuHolderInMainPanel.is(":empty"))
	    	{	
	    		$menuHolderInMainPanel.append($menuHolderInLeftPanel.children().remove()).find("ul").removeAttr("style");
	    		addMenuEventForDesktop();
	    	}
	    };
	    
	    function neutralizeLeftPanelLink()
	    {
	    	$("#leftPanel .menuHolder li a[href=#]").click(function(e){ e.preventDefault(); }).css("cursor", "default"); //Empeche les liens avec un "#" de se déclencher	
	    	//Gestion des menu avec anchor pour refermer le menu
			$("#leftPanel .menuHolder li a[href^='" + window.location.pathname + "#']").click(function(e){ $("#menuIcone").trigger("click"); });
			
	    }
	    
	    function iconeMenu()
	    {
	    	var $leftPanel = $("#leftPanel");
	    	var $mainPanel = $("#mainPanel");
	    	var $menuIcone = $("#menuIcone");
	    	$menuIcone.click(function(evt){
	    		evt.preventDefault();
	    		if($mainPanel.hasClass("open"))
	    		{
	    			$("html, body").removeClass("nooverflow");
	    			$mainPanel.css("left",0).removeClass("open").find(">.neutralizer").remove();
	    			$leftPanel.removeClass("scrolling");
	    			unswipe("body");
	    		}
	    		else
	    		{
	    			$("html, body").addClass("nooverflow");
	    			$("<div class=\"neutralizer\"></div>").click(function(){ $menuIcone.trigger("click"); }).appendTo($mainPanel);
	    			$mainPanel.addClass("open").css("left",$leftPanel.width());
	    			$leftPanel.addClass("scrolling");
	    			swipe("body", function(){ $menuIcone.trigger("click"); });
	    		}
	    	});
	    	
	    	$(window).resize(function(){ 
	    		if($mainPanel.hasClass("open")) 
	    			$menuIcone.trigger("click");
	    		if($("#menuClickMain ul.menu0").hasClass("display"))
	    			$("#menuClickMain a.menuclose").first().trigger("click");		
	    	});
	    };
	    
	    $(window).resize( moveMenu );
	    $(document).ready( function(){ neutralizeLeftPanelLink(); workoutCopy(); responsiveMenuOpen(); moveMenu(); iconeMenu();  });
	})();
	
	
	/*Fonction qui permet de gérer l'ouverture du menu en version responsive*/
	function responsiveMenuOpen() {
			
		/*Met un toogle d'ouverture sur tous les a*/
		$('#leftPanel ul.menu0 a').click(function(){
			
			var lastChar = this.href;
			lastChar = lastChar.slice(- 1);
	
			//Rem. Si le UL est déjà ouvert et qu'il y a une URL sur le menu on ne relance pas le toogle
			if(!($(this).next('ul').is(":visible") && lastChar !="#"))
				$(this).next('ul').slideToggle();
		});
	
		//Réouverture des menus sélectionnés
		$('#leftPanel a.menuselected').each(function(){
			
			$(this).next('ul').css("display","block");
			
		});
	}
</script>
