    var bannerSwitch = {
      length: 4,
      idx: 0,
      timer: null,
      wait: 3500,
      init: function () {
        var self = this
        self.start();
        $('.banner-pagination').on('click', '.page-list', function () {
          self.move(self.idx, $(this).index())
        })
        $('.banner-prev').click(function () {
          var index = (self.idx-1) >= 0 ? (self.idx-1) : (self.length-1)
          self.move(self.idx, index)
        })
        $('.banner-next').click(function () {
          self.next()
        })
      },
      move: function (m, n) {
        clearInterval(this.timer)
        this.timer = null
        $('.banner-slide:eq('+ m +')').css({'opacity': 0, 'z-index': 100})
        $('.page-list:eq('+ m +')').removeClass('current')
        $('.banner-slide:eq('+ n +')').css({'opacity': 1, 'z-index': 101})
        $('.page-list:eq('+ n +')').addClass('current')
        this.idx = n
        this.start()
      },
      next: function () {
        var index = (this.idx+1) < this.length ? (this.idx+1) : 0
        this.move(this.idx, index)
      },
      start: function () {
        this.timer = setInterval(this.next.bind(this), this.wait);
      }
    }
    bannerSwitch.init()
  })
