<script type="text/javascript">
        function onCategoryChange(e){
            var category = e.value;
            var subcategories = [];
            subcategories[3] = [    "Select Subcategory", "Where is my order", "Change or cancel my order", 
                                    "Payment issues", "Share copy of my invoice", "Received some items of my order",
                                    "Order cancelled without information", "Order rescheduled without information", "Discount issues"
                               ];
            subcategories[4] = [    "Select Subcategory", "Incorrect medicines delivered", "Incorrect price charged",
                                    "Incorrect quantity delivered", "Yet to receive refund"
                               ];
            switch(category){
                case '0':
                case '1':
                    $('#subcategory').closest('div').hide();
                    $('#subcategory').prop("required", false);
                    $('#contactorderId').closest('div').hide();
                    break;
                case '2':
                case '5':
                    $('#subcategory').closest('div').hide();
                    $('#subcategory').prop("required", false);
                    $('#contactorderId').closest('div').show();
                    break;
                case '3':
                case '4':
                    $('#subcategory').closest('div').show();
                    $('#subcategory').prop("required", true);
                    $('#contactorderId').closest('div').show();
                    $('#subcategory').empty();
                    for (var i = 0; i < subcategories[category].length; i++){
                    var opt = document.createElement('option');
                    opt.text = subcategories[category][i];
                    opt.value = subcategories[category][i];
                    document.getElementById('subcategory').appendChild(opt);   
                    }
                    break;
            }
            showWarningForMobileNumber();
        }
        function onMobileNumberChange(e){
            if(e.value.length == 10){
                $.ajax.cors = true;
                $.ajax({
                    type: 'get',
                    url: '/ml-rest-services/v1/medlife/customer/checkMobileNumber?mobileNumber='+e.value+'&source=website',
				}).done(function( data ) {
                    if(data != null){
                        if(data[0] == "REGISTERED") {
                            $('#mobileregistered').val("REGISTERED");
                        } else if(data[0] == "NOT_REGISTERED"){
                            $('#mobileregistered').val("NOT_REGISTERED");
                        }
                        showWarningForMobileNumber();
                    }
                }).error(function() {
                    console.log("Error occurred");
                });
            }
        }
        function showWarningForMobileNumber(){
            if($('#mobileregistered').val() == "REGISTERED" || $('#category').val() == 0 || 
              $('#category').val() == 1 || $('#category').val() == 2 || $('#category').val() == 5){
                
                    $('#contactmobile').removeClass('warning');
                    $('#mobile-message').html("");
            
            }else if($('#mobileregistered').val() == "NOT_REGISTERED"){
                $('#contactmobile').addClass('warning');
                if($('#mobile-message').html() == ""){
                    $('#mobile-message').html("Please enter a registered mobile number for better tracking of feedback/complaint");
                }
            }
        }
</script>

