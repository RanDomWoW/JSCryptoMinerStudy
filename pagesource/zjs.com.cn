        inputValueShowElement('orderNos','home_vcode_Box');

        function searchOrder() {
            if (checkSubmitFlg == false) {
                return;
            }
            var orderNos = $.trim($("#orderNos").val());
            var vCodeNum = $.trim($("#captcha").val());
            if (orderNos == '') {
                alert('请输入运单号！');
                return false;
            }else if ( '' == vCodeNum ){
                alert('请输入验证码！');
                $('.home_vcode_Box').show();
                return false;
            }
            $('#searchOrderForm').submit();
            checkSubmitFlg = false;
            /* alert(vCodeNum);
             var options = {
             url: '/captcha_check.jspx',
             type: 'post',
             dataType: 'json',
             data: "captcha=" + vCodeNum,
             success:  function (data) {
             alert(data)
             if(data.status==0) {
             $('#searchOrderForm').submit();
             } else if(data.status==1) {
             alert("验证码错误");
             // $("#captcha_img").click();
             return false;
             }
             }
             };
             $.ajax(options);*/
        }
        /**
         *
         * @param {String} inputElId  输入框ID
         * @param {String} showElClass  要显示的元素的Class
         */
        function inputValueShowElement(inputElId,showElClass){
            $('#'+inputElId).focus(function() {
                $('.'+showElClass).show();
            });
            $('#'+inputElId).keyup(function() {
                if($(this).val()!=''){
                    $('.'+showElClass).show();
                }else{
                    $('.'+showElClass).hide();
                }
            });
            $('#'+inputElId).blur(function() {
                if($(this).val()!=''){
                    $('.'+showElClass).show();
                }else{
                    $('.'+showElClass).hide();
                }
            });
            $('#'+inputElId).change(function() {
                if($(this).val()!=''){
                    $('.'+showElClass).show();
                }else{
                    $('.'+showElClass).hide();
                }
            });
        }

    </script>

