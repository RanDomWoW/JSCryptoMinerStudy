<script type="text/javascript">

    function submit_phoneback() {
        // validate form data
        var phone=$.trim($("#phone").val());

        if(phone == "") {
            alert("手机号码不能为空！");
            return false;
        }
        if (!checkTel(phone)) {
            alert("电话号码格式不对！");
            return false;
        }

        var options = {
            url: '/phoneback.jspx',
            type: 'post',
            dataType: 'json',
            data: $("#phonebackForm").serialize(),
            success:  function (data) {
                if(data.status==0) {
                    alert("留言成功，我们会尽快联系您!");
                    location.href=location.href;
                }
            }
        };
        $.ajax(options);
        return false;
    }

</script>
