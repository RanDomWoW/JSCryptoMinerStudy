	function saveMyRecipe(a, receipe_id) {
		if (!jQuery(a).is('[is-saving]')) {
			var unique = (new Date()).getTime();
			jQuery(a).attr('is-saving', unique).html('<i class="fa fa-spin fa-spinner"></i> saving...');
			jQuery.ajax({
				url: ajaxurl,
				data: {
		            'action':'save_my_recipe',
		            'receipe_id' : receipe_id,
		            'unique' : unique
		        },
		        dataType: 'json',
		        success:function(data) {
		        	if (data.error == 0) {

		        		jQuery('a[is-saving='+data.unique+']').html(data.html);
		        		jQuery('a[is-saving='+data.unique+']').removeAttr('is-saving');
		        	} else {
		        		jQuery('a[is-saving='+data.unique+']').html(data.errorMsg)
		        	}
		        },
		        error: function(errorThrown){
		            jQuery(a).attr('is-saving', unique).html('Error! Try after some time.');
		        }
			})
		}
	}
