<script>

    localStorage.clear();

    
    
	$(function(){

	    //Responsive gallery has different captions
        var responsiveCaptions = [
            "Discuss local news, recommendations and issues with your neighbours",
            "Discover local events from markets to art classes and garage sales",
            "Get to know your neighbours with similar interests and make new friends"
        ];
        if (window.innerWidth < 992) {
            $('.gallery-images').each(function( index ) {
                $( this ).attr('data-caption', responsiveCaptions[index]);
            });
        }

        $('.home-gallery').tGallery({
            imageWrapper: '.gallery-images',
            lockSize: false,
            imageSelector: '.slide',
            caption : '.gallery-caption',
            prevImage: '.arrow.prev',
            nextImage: '.arrow.next',
            autoPlay: true,
            stopPlayingGallery: true,
            duration : 9000,
            transition: 'slide',
            lockSize: false,
        });

        if (!GLOBAL.ie89) {
            $('.anchor').click(function(){
                $('html, body').animate({
                    scrollTop: $( $.attr(this, 'href') ).offset().top
                }, 1000);
            });
        };

        var invitationCode = '' || null;
        var groupSlug = '' || null;
        var eventId = '' || null;
        var utmContent = '' || null;

        if (utmContent == 'pets') {
            $('.landing-background').addClass('utm-pets-page');
        }
        else if (utmContent == 'nho') {
            $('.landing-background').addClass('utm-nho-page');
        }
        else if (utmContent == 'youngfamilies') {
            $('.landing-background').addClass('utm-youngfamilies-page');
        }
        else if (utmContent == 'cust') {
            $('.landing-background').addClass('utm-cust-page');         
        }
        else if (utmContent == 'fit') {
            $('.landing-background').addClass('utm-fit-page');
        }
        else if (utmContent == 'events') {
            $('.landing-background').addClass('utm-events-page');
        }
        else if (utmContent == 'diy') {
            $('.landing-background').addClass('utm-diy-page');
        }
        else if (utmContent == 'foodies') {
            $('.landing-background').addClass('utm-foodies-page');
        }
        else if (utmContent == 'neighbour') {
            $('.landing-background').addClass('utm-neighbour-page');
        }
        else if (utmContent == 'safety') {
            $('.landing-background').addClass('utm-safety-page');
        }
        else if (utmContent == 'wpnabo') {
            $('.landing-background').addClass('utm-westpacnabo1');
            $('.westpac-banner .desktop').attr('src', '/img/westpac/westpacnabo1.png');
            $('.westpac-banner .mobile').attr('src', '/img/westpac/westpacnabo1MOB.png');
        }
        else if (utmContent == 'stgeorgenabo') {
            $('.landing-background').addClass('utm-westpacnabo2');
            $('.westpac-banner .desktop').attr('src', '/img/westpac/westpacnabo2.png');
            $('.westpac-banner .mobile').attr('src', '/img/westpac/westpacnabo2MOB.png');
        }
        else if (utmContent == 'banksanabo') {
            $('.landing-background').addClass('utm-westpacnabo3');
            $('.westpac-banner .desktop').attr('src', '/img/westpac/westpacnabo3.png');
            $('.westpac-banner .mobile').attr('src', '/img/westpac/westpacnabo3MOB.png');
        }
        else if (utmContent == 'bomnabo') {
            $('.landing-background').addClass('utm-westpacnabo4');
            $('.westpac-banner .desktop').attr('src', '/img/westpac/westpacnabo4.png');
            $('.westpac-banner .mobile').attr('src', '/img/westpac/westpacnabo4MOB.png');
        }
        else {
            $('.landing-background').addClass('utm-neighbour-page');
        };

        var suburbs,
            indexActive = -1,
            q = [],
            apiWorking = false;

        var delay = (function(){
            var timer = 0;
            return function(callback, ms){
                clearTimeout (timer);
                timer = setTimeout(callback, ms);
            };
        })();

        function hideErrors() {
            $('.has-error').removeClass('has-error');
            $('.errors').hide();
        }

        $('.start-buttton').click(function(e) {
            hideErrors();
            if ( !$('.address-lookup').val() ) {
                $('.address-lookup').addClass('has-error');
                $('.lookup-error').show();
            };
        });

        $('.sign-with-code-btn').click(function(e) {
            e.preventDefault();
            hideErrors();
            if ( !$('.invitation-code-input').val() ) {
                $('.invitation-code-input').addClass('has-error');
                $('.invitation-error').empty().html('Please enter the invitation code').show();
            }
            else {
                var data = {
                    invitationCode: $('.invitation-code-input').val()
                }

                if (groupSlug) {
                    data.groupSlug = groupSlug;
                };

                if (eventId) {
                    data.eventId = eventId;
                };

                API().call('/get-postcard-by-code', data, {type: "post", dontHideLoader: true}).then(function(res){

                    if (res && res.status) {
                        if (res.data.codeType) {
                            $(".invitation-code-type-input").val(res.data.codeType);
                        }
                        document.getElementById("submitWithCodeForm").submit();
                    };
                }, function(err){
                    $('.invitation-code-input').addClass('has-error');
                    $('.invitation-error').empty().html('Sorry. We don’t recognise that invitation code. Please try again, or enter your suburb.').show();
                });
            }
        });

        $( ".lookup-form" ).submit(function( event ) {
            var $form = $(this);
            if( $('#suburbId').val() && $form.find('.address-lookup').val() ) {
                // submit()
            }
            else {
                event.preventDefault();
            }
        });

        function query() {
            hideErrors();
            var data = {};
            var apiHasSomeWorkToDo = false;

            if(apiWorking) {
                setTimeout(query, 50);
            }
            else {
                apiHasSomeWorkToDo = false;
                for(var i=0; i<q.length; i++) {
                    if(!q[i].done) {
                        data.query = q[i].val;
                        q[i].done = true;
                        apiHasSomeWorkToDo = true;
                    }
                }
            }

            if(!apiHasSomeWorkToDo) {
                return;
            }


            apiWorking = true;
            $('#suburb').parent().find('.suburbId').val('');
            $('.mini-loader').show();

            API().call('/suburb/lookup', data, {type:'get', autoLoader: false}).then(function(res){
                if(res && res.data && res.data.items && res.data.items.length) {
                    suburbs = res.data.items;
                }
                else {
                    suburbs = [];
                }
                indexActive = -1;
                drawOptions(suburbs);
                $('.mini-loader').hide();
                apiWorking = false;
            }, function(err) {
                $('.mini-loader').hide();
                apiWorking = false;
            });
        }

        function drawOptions( data , index) {
            var ind = index ? index : -1;
            var $results = $('.results');
            var newWidth = $("#suburb").css("width");
            $results.css("width", newWidth);
            var ul, li, a;

            $results.empty().show();

            if(data && data.length) {
                ul = $('<ul></ul>').appendTo($results);
                for(var i=0; i<data.length; i++) {
                    li = $('<li></li>');
                    if (i == ind) {
                        li.addClass('active');
                    }
                    li.html( data[i].name );
                    li.data( 'suburb', data[i] );
                    li.appendTo(ul);

                    li.click(function(){
                        var $this = $(this);
                        var suburb = $this.data('suburb');
                        $('#suburb').val( suburb.name );
                        $('#suburbId').val( suburb.id );
                        $('#suburbShortName').val( suburb.shortName );
                        $('#lastSavedData').val( suburb.name );
                        q = [];
                        $results.hide();
                    })
                }

                if(data.length == 1) {
                    indexActive = 0;
                    $results.find('ul li').addClass('active');
                    $('#suburbId').val(data[0].id);
                    $('#suburbShortName').val( data[0].shortName );
                }
            }
            else{
                $results.hide();
            }
        }

        $('#suburb').on('paste', function(e) {
            var $this = $(this);
            setTimeout(function(){
                if( $this.val().length > 2 ) {
                    q.push({val: $this.val(), done: false})
                    query();
                }
            }, 100)
        });

        $('#suburb').keyup(function(e){
            e.preventDefault();
            e.stopPropagation();

            if( e.keyCode == 13 ) {

                if( $('.results ul li.active').length ) {
                    var suburb = $('.results ul li.active').data('suburb');
                    $('#lastSavedData').val( suburb.name );
                    $('#suburb').parent().find('button').removeClass('disabled');
                    $('#suburbId').val(suburb.id);
                    $('#suburbShortName').val( suburb.shortName );
                    $('.results').hide();
                    $(".lookup-form").submit();
                }
                return false;
            }

            if( e.keyCode == 37 ) {
                return false;
            }

            if( e.keyCode == 39 ) {
                return false;
            }

            if( e.keyCode == 38 ) {
                if(indexActive > 0) {
                    indexActive--;
                }
                $('.results ul li').removeClass('active');
                $('.results ul li').each(function(i, li){
                    if(indexActive == i) {
                        var $li = $(li);
                        var suburb = $li.data('suburb');
                        $li.addClass('active');
                        $('#suburb').val( suburb.name );
                        $('#suburbShortName').val( suburb.shortName );
                        $('#suburbId').val( suburb.id );
                    }
                })
                return false;
            }

            if( e.keyCode == 40 ) {
                if(indexActive < $('.results ul li').length-1) {
                    indexActive++;
                }
                $('.results ul li').removeClass('active');
                $('.results ul li').each(function(i, li){
                    if(indexActive == i) {
                        var $li = $(li);
                        var suburb = $li.data('suburb');
                        $li.addClass('active');
                        $('#suburb').val( suburb.name );
                        $('#suburbId').val( suburb.id )
                    }
                })
                return false;
            }

            var $this = $(this);

            q.push({val:$this.val(), done:false});

            if( $this.val().length > 2 ) {
                delay(function(){
                    query();
                }, 250 );
            }
        });
        
        $(".landing-pages.home-page").click(function (event) {
            var $results = $('.results');
            $results.hide();
        })

        $(window).load(function(){
            if( window.location.hash == '#whatIsNabo' ){
                $('html, body').animate({ scrollTop: $('#whatIsNabo').offset().top}, 1000);
            }
            else if ( window.location.hash == '#takeTour' ) {
                $('html, body').animate({ scrollTop: $('#takeTour').offset().top}, 1000);
            }
        });
	});

</script>

