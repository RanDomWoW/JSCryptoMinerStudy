    $('#RegCorreo').blur(function(){

        $('#existe').html('<img src="images/preloader.gif" alt="" />').fadeOut(1000);

        var RegCorreo = $(this).val();        
        var dataString = 'RegCorreo='+RegCorreo;

        $.ajax({
            type: "POST",
            url: "existe_email.php",
            data: dataString,
            success: function(data) {
                $('#existe').fadeIn(1000).html(data);
            }
        });
    });              
});   


  	
  	
  
  	
    	
