<script type="text/javascript">	
var $c = $('.kensaku');

$c.bind('load change', function(){
    var add = '';
    $c.each(function(index, value) {
        if (this.checked){
            if($(this).val() != ''){
            add += ($(this).val() + '　');
            };
        };
    });
    if( $('#select-grade').on('change')) {
            if($('#select-grade option:selected').val() != ''){
            add += ($('#select-grade option:selected').val() + '　');
            };
    };
    if( $('#kensaku').on('click blur keydown keyup keypress change')) {
            add += $('#kensaku').val();
    };
    $('#show').val(add);
});
</script>
