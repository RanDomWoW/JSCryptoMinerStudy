			},
			beforeSend: function(){
				el.addClass('liking');
			},
			success: function(r){				
				if(r.error != '') {
					alert(r.error);
					return false;
				}
				
				if(actionType == 'like')
					el.stop().attr('data-lid', r.id).removeClass('like').addClass('liked');
