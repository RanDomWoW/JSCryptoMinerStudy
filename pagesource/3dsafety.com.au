			<script type="text/javascript">
			var randomStrings = [
				"Manage projects with confidence","Streamline plant management","Governance made easy","Lead by example","Replace paper with technology"			];
			var randomBanner = [
				"RNSHospital.jpg","NorthenBeachesHospital.jpg","EraMirvac.jpg","200GeorgeSt-test.jpg","HunterExpressway.jpg"			];
			var randomCaption = [
				"Royal North Shore Hospital, Thiess","Northern Beaches Hospital, CPB Contractors","ERA Apartments, Mirvac","200 George Street, Mirvac","Hunter Expressway, Thiess"			];

			var timeText = 5 // seconds
			var randomDiv = document.getElementById( "toptext" );
			var caption = document.getElementById( "topcaption" );

			var logos = [
				'cpb',
				'mirvac',
				'riotinto',
				'thiess',
				'xstrata',
				'yancoal'
			];

			jQuery( document ).ready( function()
			{
/*
				Code with random text changing every 5 secs and a fixed random pic loading at every reaload */
				randomIndex = Math.ceil( ( Math.random() * randomStrings.length - 1 ) );
				newText = randomStrings[ randomIndex ];
				randomDiv.innerHTML = newText;

				randomPic = Math.ceil( ( Math.random() * randomBanner.length - 1 ) );
				newPic = randomBanner[ randomPic ];

				newCaption = randomCaption[ randomPic ];

				jQuery('#topimage').css('background-image', 'url(/templates/3dsafety/images/overlay-pattern2.png), url(/images/banners/' + newPic + ' )');
				caption.innerHTML = newCaption;

				window.setInterval( function()
				{
					randomIndex = Math.ceil( ( Math.random() * randomStrings.length - 1 ) );
					newText = randomStrings[ randomIndex ];
					randomDiv.innerHTML = newText;
				}, timeText * 1000 );


				var logo01 = logos[ Math.floor( Math.random() * logos.length ) ];
				var index = logos.indexOf( logo01 );
				logos.splice( index, 1 );
				var logo02 = logos[ Math.floor( Math.random() * logos.length ) ];

				document.getElementById( 'logoRondom01' ).src = '/images/logos/' + logo01 + '.png';
				document.getElementById( 'logoRondom02' ).src = '/images/logos/' + logo02 + '.png';
			});
		</script>
	
