			var a1 = a1 + String(a.data[0]).toLowerCase();				
			var b1 = b.value.toLowerCase().substring(0, fl) == f ? '0' : '1';				
			var b1 = b1 + String(b.data[0]).toLowerCase();				
			if (a1 > b1) {					
				return 1;				
			}				
			if (a1 < b1) {					
				return -1;				
			}				
			return 0;			
		},			
		showResult: function(value, data) {
			
			var txt=$('#company1').val();
