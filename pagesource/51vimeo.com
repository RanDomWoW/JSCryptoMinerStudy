<script src="/theme/default/js/jquery.validate.min.js"></script>
<script>
    $('#search').click(function(e){
        var href=$(this).attr('href');
        e.preventDefault();
        var keyword=$("#keywords").val();
        if(keyword.length>50){
            layer.alert('搜索内容过长');
            return;
        }else{
            href=href+'?keywords='+keyword;
            layer.load(2);
            setTimeout(function(){
                layer.closeAll();
                location.href=href;
            },'500')
        }
    })
    $('.slide_search_words').find('a').click(function(e){
        var url=$(this).attr('href');
        e.preventDefault();
        layer.load(2);
        setTimeout(function(){
            layer.closeAll();
            location.href=url;
        },'500')
    })
    $('#toggle_video').click(function(){
        var postUrl="/index/refer_video.html";
        $.ajax({
            url: postUrl,
            type: 'get',
            dataType: 'json',
            beforeSend: function(){
                layer.load(1);
            },
        })
            .done(function(data) {
                layer.closeAll();
                if(data.status==1){
                    $('#change_one_video,#change_all_video').empty();
                    $('#change_one_video').append(data.data);
                    $('#change_all_video').append(data.msg)
                }else{
                    layer.msg('暂无数据');
                }
            })
            .fail(function() {
                layer.closeAll();
                console.log("error");
                layer.msg('发送错误,请重试');
            })
    })
    $(document).on('mouseenter mouseleave','#change_one_video',function(e){
        if(e.type=='mouseenter'){
            $(this).find("i").hide();
            $(this).find("em").hide();
            $(this).find("small").stop().animate({left:"0"},500);
            $(this).find("span").stop().animate({left:"0"},500);
        }else{
            $(this).find("i").show();
            $(this).find("em").show();
            $(this).find("small").stop().animate({left:"100%"},500);
            $(this).find("span").stop().animate({left:"100%"},500);
        }
    })
    $(document).on('mouseenter mouseleave','.video_xs',function(e){
        if(e.type=='mouseenter'){
            $(this).find("i").hide();
            $(this).find("em").hide();
            $(this).find("small").stop().animate({left:"0"},500);
            $(this).find("span").stop().animate({left:"0"},500);
        }else{
            $(this).find("i").show();
            $(this).find("em").show();
            $(this).find("small").stop().animate({left:"100%"},500);
            $(this).find("span").stop().animate({left:"100%"},500);
        }
    })
    //推荐关注人click效果
    $(".creator_li_info").find("b").click(function () {
        var _this=$(this);
        var data_id=$(this).attr('data-id');
        var postUrl="/creator/add_follow.html";
        var data={id:data_id};
        $.ajax({
            url: postUrl,
            type: 'post',
            data:data,
            dataType: 'json',
            beforeSend: function(){
                layer.load(1);
            },
        })
            .done(function(data) {
                layer.closeAll();
                if(data.url){
                    layer.msg('登录之后才能关注,是否跳转至登录界面？', {
                        time: 0 //不自动关闭
                        ,btn: ['好的', '不了']
                        ,yes: function(index){
                            layer.close(index);
                            location.href=data.url;
                        }
                    });
                    return;
                }
                if(data.status==1){
                   _this.addClass("on");
                    _this.text("已关注");
                    layer.msg(data.msg);
                }else if(data.status==2){
                    _this.removeClass("on");
                    _this.text("加关注");
                    layer.msg(data.msg);
                }else{
                    layer.msg(data.msg);
                }
            })
            .fail(function() {
                layer.closeAll();
                console.log("error");
                layer.msg('发送错误,请重试');
            })
    });
    /********** 搜索框响应回车键 ************/
    enter_submit('#keywords',"#search");
</script>
<script src="/theme/default/js/zpbvideo.js"></script>
</body>
