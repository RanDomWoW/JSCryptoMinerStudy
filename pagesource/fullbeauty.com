        <script type="text/javascript">
            /*global $*/
            /*jslint browser:true,regexp:true*/

            // JavaScript should be moved into the solution at the next post-launch build.

            var email_signup_2014 = {
                confirmation: function (address) {
                    'use strict';

                    var $popup;

                    // Add the popup to the page and position it.
                    $('body').append('<div id="confirmation-fader"></div><div id="confirmation-popup"></div>');
                    $popup = $('#confirmation-popup');
                    $popup.css('left', ($(window).width() / 2 - $popup.width() / 2) + 'px');

                    // Add the email address. Add close behaviors. Remove loading indicator.
                    $popup.load('/Catalog/email_confirm_popup.aspx', function () {
                        $popup.find('span.display-email').html(address);

                        $('#confirmation-popup a.close, #confirmation-fader').on('click', function () {
                            $('#confirmation-fader, #confirmation-popup').remove();
                        });

                        $popup.addClass('loaded');
TrackEmailSignupOmniture("Footer");
$('<img height="1" width="1" style="border-style:none;" alt="" src="//insight.adsrvr.org/track/conv/?adv=6fuwo87&ct=0:57jkp3pt&fmt=3"/>').appendTo($(".trackingEmailSignupFooter"));
 $(".trackingEmailSignupFooter").show();
                    });
                },

                submit: function (address, $error, $close) {
                    'use strict';

                    // Submit email signup request and display response.
                    $.ajax({ url: '/Checkout/ChameleonEmail.aspx?source=footer&eml=' + address,
                        type: 'GET', dataType: 'text',
                        success: function (server_data) {
                            if (server_data.split('^')[0] !== 'succ') {
                                if(server_data.split('^')[1] == 'MMode')
										    window.location.href = "/Default.aspx?MMode=y";
                                else
                                            $error.html('We\'re sorry. There\'s been a server error. <!-- No success msg. -->');
                            } else {
                                $close.click();
                                email_signup_2014.confirmation(address);
                            }
                        },
                        error: function () {
                            $error.html('We\'re sorry. There\'s been a server error. <!-- $.ajax error. -->');
                        }
                    });
                },

                setup: function () {
                    'use strict';

                    // CONFIGURATION

                    // These elements should correspond to the signup form markup.
                    var $form = $('#m-emailSignupForm'),
                        $address = $('#e-emailAddress'),
                        $confirm = $('#e-emailAddress-confirm'),
                        $submit = $('#e-submitEmail'),
                        $error = $('#e-signupError'),
                        $close = $('#e-closeSignup'),

                    // Height in pixels for the signup form. Should match CSS.
                        condensed = 50,
                        expanded = 185,

                    // Default text for the form inputs.
                        address_default = 'enter email address',
                        confirm_default = 'confirm email address',

                    // stackoverflow.com/questions/46155 + escaped brackets for JSLint.
                        email_address_regex = /^(([^<>()\[\]\\.,;:\s@\"]+(\.[^<>()\[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/,

                        error_message;

                    // BEHAVIORS

                    // Expand signup form (if necessary) when the text field is clicked.
                    $address.on('click', function () {
                        if ($form.height() === condensed) {
                            $form.height(expanded);
                            $form.addClass('p-boxShadow');
                            $close.show();
                            $confirm.show();
                            $submit.insertAfter($confirm);
                            $form.find('input').removeClass('e-signupError');
                        }
                    });

                    // Close button resets the form completely.
                    $close.on('click', function () {
                        if ($form.height() === expanded) {
                            $form.height(condensed);
                            $form.removeClass('p-boxShadow');
                            $close.hide();
                            $confirm.hide();
                            $submit.insertAfter($address);
                            $error.empty();
                            $form.find('input').removeClass('e-signupError');
                            $address.val(address_default);
                            $confirm.val(confirm_default);
                        }
                    });

                    // Set default text and focus/blur/enter key behaviors.
                    $address.val(address_default)
                        .on('focus', function () { if ($address.val() === address_default) { $address.val(''); } })
                        .on('blur', function () { if ($address.val() === '') { $address.val(address_default); } })
                        .on('keypress', function (e) { if (e.which === 13) { $submit.click(); } });

                    $confirm.val(confirm_default)
                        .on('focus', function () { if ($confirm.val() === confirm_default) { $confirm.val(''); } })
                        .on('blur', function () { if ($confirm.val() === '') { $confirm.val(confirm_default); } })
                        .on('keypress', function (e) { if (e.which === 13) { $submit.click(); } });

                    // Submit email signup request and display server response.
                    $submit.on('click', function () {
                        // Submit button clicked before address entered?
                        if ($form.height() === condensed) {
                            $address.addClass('e-signupError');
                            return false;
                        }

                        // Clear any previous errors.
                        $error.empty();
                        $form.find('input').removeClass('e-signupError');

                        // Error check user input.
                        if ($address.val() !== $confirm.val()) {
                            $address.addClass('e-signupError');
                            $confirm.addClass('e-signupError');
                            error_message = 'The email addresses don\'t match.';
                        }

                        if (!email_address_regex.test($address.val())) {
                            $address.addClass('e-signupError');
                            error_message = 'Email address doesn\'t appear to be valid.';
                        }

                        if (!email_address_regex.test($confirm.val())) {
                            $confirm.addClass('e-signupError');
                            error_message = 'Email address doesn\'t appear to be valid.';
                        }

                        if ($address.val() === '' || $address.val() === address_default) {
                            $address.addClass('e-signupError');
                            error_message = 'Please enter both email addresses.';
                        }

                        if ($confirm.val() === '' || $confirm.val() === confirm_default) {
                            $confirm.addClass('e-signupError');
                            error_message = 'Please enter both email addresses.';
                        }

                        if ($form.find('input').hasClass('e-signupError')) {
                            $error.html(error_message);
                        } else {
                            email_signup_2014.submit($address.val(), $error, $close);
                        }
                    });
                }
            };

            $(function () {
                'use strict';

                email_signup_2014.setup();
            });
        </script>
		
