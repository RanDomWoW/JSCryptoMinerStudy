            else
            { var isTouchDevice = false; }

            /* fix for Ipad  so you dont have to double click an a tag*/
            if (isTouchDevice) {
              var dragging = false;
              $("body").on("touchmove", function () {
                dragging = true;
              });

              $("body").on("touchstart", function () {
                dragging = false;
              });

              $('body').on('click touchend', function (event) {
                //We are scrolling dont do anything
                if (dragging) { return; }

                var target = $(event.target);
                if (target.parents('.OSPHeader2013_menu').length > 0) {
                  if (!target.hasClass("expander") && target.is("a") && !target.hasClass("viewAll") && !target.parent.hasClass("OSPHeader2013_tab")) {
                    var link = target.attr('href');
                    window.location = link;
                  }
                }
                else {
                  if (!target.hasClass('OSPHeader2013_staticText')) {

                    $('.OSPHeader2013_menu').hide();
                  }
                  if (target.hasClass('mui_gn_filter_value') && target.attr('href') !== 'undefined') {
                    var link = target.attr('href');
                    window.location = link;
                  }
                  if (target.parents('#left-nav').length > 0) {
                    if (target.is("a")) {
                      var link = target.attr('href');
                      window.location = link;
                    }
                  }
                }
              });


              $('a, img').on('click', function (e) {
                if (dragging) { return; }
                var target = $(event.target);
                if (target.parents('.OSPHeader2013_tab').length <= 0) {
                  if (target.hasClass('OSPHeader2013_staticText')) {
                    var currentIndex = target.index();
                    $('.OSPHeader2013_menu').eq(currentIndex).show();
                  }
                  else {
                    if (!target.hasClass("viewAll") && !target.parents('.product-wrapper') && !target.hasClass("OSPHeader2013_tab")) {
                      var el = $(this);
                      var link = el.attr('href');
                      window.location = link;
                    }
                  }
                }
              });
            }
            //} // newDeptIdGet() is not being used...
          });

          // JS dynamic menus code - Do not remove it 
          var dynamicDeptId = "";
          // call to get dynamic-deptids configure in the markup 
          FetchDeptId();
          function FetchDeptId() {
            $('p[dynamic-deptid],span.OSPHeader2013_column[dynamic-deptid]').each(function (index) {
              if (dynamicDeptId.indexOf($(this).attr('dynamic-deptid')) == -1) {
                if (index == 0) {
                  dynamicDeptId = $(this).attr('dynamic-deptid');
                }
                else {
                  dynamicDeptId += $(this).attr('dynamic-deptid');
                }
                dynamicDeptId += '|';
              }
            });
          }

          //Calling Ajax to get cataegory response
          $.ajax({
            type: 'GET',
            url: '../MainMenu.ashx',
            data: {
              deptid: dynamicDeptId
            },
            dataType: "json",
            success: function (data) {
              AjaxGetCategorySuccess(data);
            },
            error: AjaxGetCategoryError
          });

          // Have to add error handling
          function AjaxGetCategoryError(XMLHttpRequest, textStatus, errorThrown) {

          }

          function AjaxGetCategorySuccess(data) {
            $('p[dynamic-deptid],span.OSPHeader2013_column[dynamic-deptid]').each(function (index) {
              var dynamicDeptIdPTag = $(this);
              var dynamicDeptIdInsertElement;
              if ($(dynamicDeptIdPTag).children('span.sub-categories').length != 0) {
                dynamicDeptIdInsertElement = $(dynamicDeptIdPTag).children('span.sub-categories');
              }
              else {
                dynamicDeptIdInsertElement = $(dynamicDeptIdPTag);
              }
              $.each(data, function (i, item) {
                if (item.id == dynamicDeptIdPTag.attr('dynamic-deptid')) {
                  var url = item.catUrl;
                  $.each(item.childCategories, function (i, subItem) {
                    $("<a />").html(subItem.displayName).attr("href", subItem.catUrl).appendTo(dynamicDeptIdInsertElement);
                  });
                }
              });
            });
          }
