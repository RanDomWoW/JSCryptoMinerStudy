	function centerCaption(){
		if( $(window).width() >= 1024){
			var middlePos = (($('#hp-slideshow').height()/2) - (parseInt($('#hp-slideshow #rs-gallery-desc-outer').height())/2 ) + 15); /* 30 is the offset to bump it down off center */
			
			$('#hp-slideshow #rs-gallery-desc-outer').stop().animate({'top': middlePos}).css('bottom', 'auto');
		}
	}
	
	function modEvents(){
	    if (!$(".upcomingevents .joel-month").length) {
	        $(".upcomingevents").modEvents({
