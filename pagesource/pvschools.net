<script type="text/javascript">
	var map;
	var geocoder;
	
	function adjustMapWidth( expand, expandWidth ){
		var winWidth = $(window).width();
		var newWidthNoOffset = ($(window).width() - 1280) / 2;
		var maxWidth = 0;
		
		if( newWidthNoOffset < 0 ) newWidthNoOffset = 0;
		
		if( winWidth >= 1024 ){
			maxWidth = newWidthNoOffset + 379;
		}else{
			
			if( winWidth > 640 ){
				maxWidth = newWidthNoOffset + 200;
			}else if(winWidth > 480){
				maxWidth = newWidthNoOffset + 60;
			}
		}
		
		if( expand == true ){
			maxWidth = $('#map-canvas').width() + expandWidth;
		}
		
		$('#map-canvas').css('width', maxWidth);
	}
	
	function initialize() {
	
		adjustMapWidth();
		
		geocoder = new google.maps.Geocoder();
		var mapOptions = {
			zoom: 17,
			center: new google.maps.LatLng(40.828353,-77.847342),
			disableDefaultUI: true,
			scrollwheel: false,
			navigationControl: false,
			mapTypeControl: true,
			scaleControl: false,
			draggable: true
		};
		
		map = new google.maps.Map(document.getElementById('map-canvas'), mapOptions);
		
		geocoder.geocode( { 'address': '15002 N. 32nd St. Phoenix, AZ 85032'}, function(results, status) {
			if (status == google.maps.GeocoderStatus.OK) {
				map.setCenter(results[0].geometry.location);
				var marker = new google.maps.Marker({
					map: map,
					position: results[0].geometry.location
				});
                
                setTimeout(function(){
                	$('#map-canvas a, #map-canvas button').attr('tabindex', -1);
				}, 300);
			} else {
				console.log('Geocode was not successful for the following reason: ' + status);
			}	
		});
        
		
		/*$('#map-canvas').click(function(e){
			if( !$(this).hasClass('showing-map') ){
				$('#footer-holder').addClass('get-out-of-here');
				$(this).addClass('showing-map');
				adjustMapWidth(true, 500);
			}
		});
		
		$('footer').hover(function(e){
		}, function(e){
			if( $('#map-canvas').hasClass('showing-map') ){
				$('#footer-holder').removeClass('get-out-of-here');
				$('#map-canvas').removeClass('showing-map');
				adjustMapWidth(true, -500);
			}
		});*/
	}
	
	$(function(){
		google.maps.event.addDomListener(window, 'load', initialize);
		google.maps.event.addDomListener(window, "resize", function() {
			var center = map.getCenter();
			
			adjustMapWidth();
			
			google.maps.event.trigger(map, "resize");
			map.setCenter(center);
		});
		$('#map-canvas img').attr('alt', 'map'); /*added by Lyndsay - Case 00388463*/
    });
	
</script>

