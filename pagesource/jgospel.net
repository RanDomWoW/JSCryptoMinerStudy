    {
		name = document.forms[0].name.value;
        if ( name == "" ) {
            return;
        }

        url = "/list.aspx?search=" + escape(name);
        if ( thisTypeID > 0 ) {
            url += "&subid=" + thisTypeID;
        }
		
		location.href = url;
		
		return false;
    }
