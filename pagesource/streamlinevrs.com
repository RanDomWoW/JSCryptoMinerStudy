<script>
jQuery(document).ready(function(){
    
        var sHei = jQuery('#slider').height();
        jQuery('#slider').height(sHei).addClass('fixHei');
        jQuery('#slider .item').height(sHei);
        
        /*fix header scroll*/
        var heiHead = jQuery('.fixing').height();
        jQuery('header').height(heiHead);
        
        /*TABS ON HOMEPAGE*/
        
        /*scroll up user*/
        jQuery('.software-suite .hiddenPart button').click(function(){
            var hsCL = jQuery(this).hasClass('showAll');
            if(hsCL == true){
                var tgSC = jQuery('.software-suite').offset().top - 100;
                jQuery('body,html').animate({scrollTop: tgSC}, 200);                
            }  
        })        
            
        /*tab anime*/
        var heiHaz = jQuery('.software-suite .tab-content').height();
        jQuery('.hiddenPart button').click(function(){
        	var checkCL = jQuery(this).hasClass('hideBut');
            if(checkCL == true){            
                var heiNeed = jQuery('.software-suite .tab-pane.active').height() + jQuery(this).height();
                jQuery('.software-suite .tab-content').animate({'height': heiNeed}, 500);
                jQuery(this).removeClass('hideBut').addClass('showAll');
            }else{
                jQuery('.software-suite .tab-content').animate({'height': heiHaz}, 300);
                jQuery(this).removeClass('showAll').addClass('hideBut');   
            }
        });
        
        /*Cheking tabs click*/
        jQuery('.software-suite .nav-tabs li').click(function(){
            var checkCL = jQuery(this).hasClass('active');        
            if(checkCL == false){
                var checkOneMore = jQuery('.hiddenPart button').hasClass('showAll');            
                setTimeout(function(){
                    if(checkOneMore == true){
                        var heiNeed = jQuery('.software-suite .tab-pane.active').height() + jQuery('.hiddenPart button').height();
                        jQuery('.software-suite .tab-content').animate({'height': heiNeed}, 200);  
                    }
                }, 50);               
            }              
        })
         
        /*for click in tabs a*/
        jQuery('.software-suite .tab-content div a').click(function(){
            var checkHref = jQuery(this).attr('href');
            var checkHref2 = checkHref.split('');        
            var checkHref3 = checkHref2[0];
            if(checkHref3 == '#'){
                jQuery('.software-suite #software-suite-tab li').removeClass('active');
                var lookFor = '.software-suite #software-suite-tab li a[href="' + checkHref +'"]';
                jQuery(lookFor).parent().addClass('active');
                var checkOneMore = jQuery('.hiddenPart button').hasClass('showAll');            
                setTimeout(function(){
                    if(checkOneMore == true){
                        var heiNeed = jQuery('.software-suite .tab-pane.active').height() + jQuery('.hiddenPart button').height();
                        jQuery('.software-suite .tab-content').animate({'height': heiNeed}, 200);  
                    }
                }, 50);                                               
            }   
        })
        /*END TABS ON HOMEPAGE*/ 
        
        /*screen animation*/
        jQuery('.deviceIcon li').click(function(){ 
        	var hzCL = jQuery(this).hasClass('active');
            if(hzCL == false){                
                jQuery('.deviceIcon li').removeClass('active');
                jQuery('.conversionScreen .conversionScreenItem').removeClass('active');
                jQuery(this).addClass('active');
                var hzID = jQuery(this).attr('id');                
                var hzID = hzID.split('-');
                var hzID = hzID.pop();
                var addCL = '#scImg-' + hzID;
                jQuery(addCL).addClass('active');       
            } 
        });     
        /*End screen animation*/
        
        /*Checking form approval*/
        function formCheck(){
            chFA = 1;
            if(chFA > 0){
                var chHei = jQuery('.frameBlock .gform_confirmation_wrapper ').height();
                if(chHei > 10){
                    document.location.href = '/brochure';    
                    sessionStorage.clickcount = 2;                
                }else{
                    setTimeout(function(){
                        formCheck()
                    }, 200);                    
                }  
            }           
        }
        setTimeout(function(){
            formCheck()
        }, 200);
        /*End Checking form approval*/
        
        /*Remove Home Banner in second Home Page load*/
        if(sessionStorage.seeHome > 1){
            jQuery('body').addClass('wasHere')    
        }
        sessionStorage.seeHome = 2; 
          
});/*End ready function*/

jQuery(window).load(function(){                  
        
        /*top image scripts*/
        function massImgCorection(micSection, micUnit){
            var cityImg = jQuery(micSection).height();
            jQuery(micUnit).each( function(){
                var heiBL = jQuery(this).find('img:first').height();
                if (cityImg > heiBL) {                    
                    jQuery(this).find('img').css({'height': '100%', 'width': 'auto'})
                    var cityImgWid = jQuery(this).width();
                    var centrImg = -(jQuery(this).find('img').width() - cityImgWid)/2;
                    jQuery(this).find('img').css({'margin-left': centrImg});
                    //alert(1)
                } else {
                    var centrImg = -(jQuery(this).find('img').height() - cityImg)/2;
                    jQuery(this).find('img').css({'margin-top': centrImg});
                    //alert(2)
                }
            })
        }
        
        massImgCorection('#slider', '#slider .item');
        
        
        jQuery(window).resize(function(){ 
            jQuery('.homeSliderGo .item img').css({'width': '100%', 'height': 'auto', 'margin': 0});
            massImgCorection('#slider', '#slider .item');
                                
        });/*End resize function*/                
        
        function massFixPos(mfpSection, mfpUnit){
            jQuery(mfpSection).each( function(){
                var heiPos = (jQuery(mfpUnit).height() - jQuery(this).height())/2;
                jQuery(this).css({'top': heiPos});
            })
        }
        massFixPos( '#slider .positioned', '.home #slider');

        /*animated scroll from ather page menu*/
        
            /*for Demo button / Home button*/
            setTimeout( function(){
                if(sessionStorage.clickcount == 1){
                    var top = jQuery('.frontContact').offset().top - 400;           
                    jQuery('body,html').animate({scrollTop: top}, 1000);
                    sessionStorage.clickcount = 0;                
                }else if(sessionStorage.clickcount == 99){
                    var top = jQuery('header').offset().top;           
                    jQuery('body,html').animate({scrollTop: top}, 300);
                    sessionStorage.clickcount = 0;                
                }           
            }, 10)           
        
        /*End animated scroll from ather page menu*/ 
                    
    jQuery(window).scroll(function(){
        
        /*Homepage animation*/        
            var tpPos = 0 + jQuery('#wpadminbar').height() 
            var needTake = jQuery('header').offset().top;       
            if(window.innerWidth > '767'){
                if(jQuery(window).scrollTop() >  needTake){
                    jQuery('header .fixing').addClass('fixed').css('top', tpPos);
                }else if(jQuery(window).scrollTop() <  needTake){
                    jQuery('header .fixing').removeClass('fixed').css('top', 0);
                }            
            }else{
                if(jQuery(window).scrollTop() >  needTake){
                    jQuery('header .fixing').addClass('fixed').css('top', tpPos);
                }else if(jQuery(window).scrollTop() <  needTake){
                    jQuery('header .fixing').removeClass('fixed').css('top', 0);
                }            
            }
                       
        /*End Homepage animation*/
    })//end scroll function        
})// END LOAD FUNCTION


</script>
