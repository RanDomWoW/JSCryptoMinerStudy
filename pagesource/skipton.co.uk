<script type="text/javascript">
    $j(document).ready(function () {

        $j('.searchbox').searchbox();

    });
    function branchClick() {
        elem = document.getElementById('header-collapse-search');
        var attr = elem.getAttribute('Class');
        //if the class already has a 'Collapse' attribute then we're ok, if it's set to 'Collapse in' then we need to remove it
        if (attr.toString().indexOf("in") > 0) {
            $j('#header-collapse-search').collapse('hide');
        }
        //We need to add the collapsed styling to the search button
        elem = document.getElementById('searchLink');
        elem.className += " collapsed";
    }
    function searchClick() {
        elem = document.getElementById('header-collapse-branch');
        var attr = elem.getAttribute('Class');
        if (attr.toString().indexOf("in") > 0) {
            $j('#header-collapse-branch').collapse('hide');
        }
        //We need to add the collapsed styling to the branch button
        elem = document.getElementById('branchLink');
        elem.className += " collapsed";
    }
</script>
