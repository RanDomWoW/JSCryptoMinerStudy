<script type="text/javascript">
function goSubmit() {
    if (document.formInputMailaddr.desEmailAddress.value == '') {
        showAlertBox('メールアドレスが入力されていません。');
        return false;
    }
    var regexp = new RegExp('^([a-zA-Z0-9])+([a-zA-Z0-9\.\+_-])*@([a-zA-Z0-9_-])+([a-zA-Z0-9\._-]+)+$');
    var mail = document.formInputMailaddr.desEmailAddress.value.trim();
    if (mail.match(regexp)) {
        document.formInputMailaddr.D_EMAIL.value = document.formInputMailaddr.desEmailAddress.value;
        document.formInputMailaddr.submit();
    }
    else {
        showAlertBox('メールアドレスの形式が不正です。');
        return false;
    }
}

if (!String.prototype.trim) {
    String.prototype.trim = function () {
        return this.replace(/^\s+|\s+$/g, '');
    }
}

function showFormSample(target) {
    var screenWidth = $(window).width();
    if (screenWidth <= 768) {
        var src = '/samples/form-sample-' + target + '-ss.html';
        window.open(src, 'new');
    }
    else {
        var src = '/samples/form-sample-' + target + '.html';
        $('#iframe-form-sample').attr('src', src);
        $('#dialog-form-sample').modal('show');
    }
}

$(function() {
    var screenWidth = $(window).width();
    if (screenWidth > 768) {
        $('.form-sample').append('<div class="form-sample-mask"></div>')
        .hover(
            function() {
                $(this).children('.form-sample-mask').hide();
            },
            function() {
                $(this).children('.form-sample-mask').show();
            }
        );
    }
    setTimeout(function() {
        var paddingTop = $('#eye-catch-message').css('paddingTop');
        var paddingTopBefore = parseInt(paddingTop) - 12;
        if (paddingTopBefore < 0) paddingTopBefore = 0;
        $('#eye-catch-message').css('paddingTop',  paddingTopBefore + 'px');
        $('#eye-catch-message').animate({
            paddingTop: paddingTop,
            opacity: '1'
        }, 1000, 'swing')
    }, 700);

    $('.ripple').ripple({});
});
</script>

