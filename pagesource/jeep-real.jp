function initmenu(){
	$('ul.globalnav li.dropdown_item').stop().hover(function(){ 
		$(this).find('ul').fadeIn(200);
		$('.opaopa').fadeTo(300,0.2);
	},
	function(){
		$(this).find('ul').fadeOut(200);
		$('.opaopa').fadeTo(300,1);
	});
	$('#header').addClass('ui-widget-content ui-corner-all');
	$('li.dropdown_item > ul').addClass('ui-widget-content ui-corner-all');
	$('li.dropdown_item > ul').hide();
};
function gototop(){
	$('.top a').click(function () {
		$('html,body').animate({
			scrollTop: 0
		}, 800, 'easeInOutExpo');
		return false;
	});
};
