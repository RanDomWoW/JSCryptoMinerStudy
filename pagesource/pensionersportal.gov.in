<!-- blink script -->
<script type="text/javascript">
    var col = new String();
    var x = 1; var y;

    var col1 = new String();
    var x1 = 1; var y1;

    function blink() {
        if (x % 2) {
            col = "rgb(255,0,0)";
        } else {
            col = "rgb(115,44,123)";
        }

        aF.style.color = col; x++; if (x > 2) { x = 1 }; setTimeout("blink()", 500);
   
    }
    function blink1() {
        if (x1 % 2) {
            col1 = "rgb(255,0,0)";
        } else {
            col1 = "rgb(115,44,123)";
        }

        aE.style.color = col1; x1++; if (x1 > 2) { x1 = 1 }; setTimeout("blink1()", 500);
    }

</script>


