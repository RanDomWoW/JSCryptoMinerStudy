	/**
	 * Page print request detection
	 */
	dojo.connect(window, "onbeforeprint", function(){
		dojo.publish("page_print_request", []);
	});

	if (window["matchMedia"]) {
		var mediaQueryList = window.matchMedia('print');
		mediaQueryList.addListener(function(mql) {
			if (mql.matches) {
				dojo.publish("page_print_request", []);
			}
		});
	}

	dojo.connect(window, "print", function(){
		dojo.publish("page_print_request", []);
	});


	dojo.ready(function(){
		if(dojo.style("print_detection_div", "width") == 2){
			dojo.publish("page_print_request", []);
		}
	});

	var print_requested = false;
	var print_subscription = dojo.subscribe("page_print_request", function(){
		if(print_requested){
			return;
		}
		dojo.publish("omniture/sendSC", ["print"]);
		print_requested = true;
	});
