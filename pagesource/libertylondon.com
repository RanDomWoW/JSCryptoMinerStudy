				function appendGetParam(url, paramName, paramValue) {
					var paramPair = (url.indexOf('?') == -1 ? '?' : '&') + encodeURIComponent(paramName) + '=' + encodeURIComponent(paramValue);
					var anchorPart = '';
					if( url.indexOf( "#" ) !== -1) {
						anchorPart = url.slice(url.indexOf("#"), url.length);
						url = url.slice(0, url.indexOf("#"));
					}
					url = url + paramPair + anchorPart;
					return url;
				}
				
				
