	<script>
	function gtm_meganav_promo_push() {
		function push_promo_details(promo_data) {
			dataLayer.push({
			  'event': 'topNavPromo', // This line must be used.
			  'ecommerce': {
			    'promoView': {
			      'promotions': promo_data
			    }
			  }
			});
		}
		var pushTimer = null;
		$( ".nav .has-sub" ).mouseenter(function() {
			
			var promo_text = $(this).find('.banner .content-asset a .center-vertical'), promo_data = [];
			if(promo_text.length == 1) {
				promo_data.push({
					'id': $(promo_text).find('.pre-heading').text(), // ID or Name is required.
			        'name': $(promo_text).find('.heading').text(),
			        'creative': Math.round($(this).find('.banner .content-asset a')[0].getBoundingClientRect().width) + 'w x '+ Math.round($(this).find('.banner .content-asset a')[0].getBoundingClientRect().height) + 'h',
			        'position': '1'
				});
				pushTimer = setTimeout(function(){push_promo_details(promo_data)}, 1000);
			}
		})
		.mouseleave( function() {
			
			clearTimeout(pushTimer);
		});
	}
	
	window.addEventListener ? window.addEventListener("load",gtm_meganav_promo_push,false) : window.attachEvent && window.attachEvent("onload",gtm_meganav_promo_push);
	
	</script>



	
	<script>	
		window.addEventListener('load', function () {
			
			function handle_promo_click(promo_clicked, promo_type, promo_position) {
				var promotion_data = [];
				
				switch(promo_type) { 
				
				case 'promo_tile':
					var tile_text = $(promo_clicked).find('.panel-text');
					
					if(tile_text.length == 1) {
						promotion_data.push({
							'id': $(tile_text).find('.pre-heading').text(),
							'name': $(tile_text).find('h4').text(),
							'creative': Math.round(promo_clicked.getBoundingClientRect().width)+'w x '+Math.round(promo_clicked.getBoundingClientRect().height)+ 'h',
							'position': $('.homepage-grid .wrapper [class*="-tile"] > a.full-link > .panel-text').index(tile_text) + 2
						});
					}
					break;
					
				case 'promo_meganav': 
					promotion_data.push({
						'id' : $(promo_clicked).find('.pre-heading').length === 1 ? $(promo_clicked).find('.pre-heading').text() : $(promo_clicked).find('.heading').text(),
						'name' : $(promo_clicked).find('.pre-heading').length === 1 ? $(promo_clicked).find('.heading').text() : $(promo_clicked).find('.sub-heading').text(),
						'creative' : Math.round(promo_clicked.getBoundingClientRect().width)+'w x '+Math.round(promo_clicked.getBoundingClientRect().height)+ 'h',
						'position' : promo_position
					});
					break;
					
				case 'promo_dept_hero_content':
					var promo_bg = $(promo_clicked).parent()[0];
					promotion_data.push({
						'id' : $(promo_clicked).find('h1').text(),
						'name' : $(promo_clicked).find('h1').text(),
						'creative' : Math.round(promo_bg.getBoundingClientRect().width)+'w x '+Math.round(promo_bg.getBoundingClientRect().height)+ 'h',
						'position' : promo_position
					});
					break;
					
				case 'promo_brands_tile':
					var promo_bg = $(promo_clicked).find('img')[0];
					promotion_data.push({
						'id' : $(promo_clicked).find('p.brand').text(),
						'name' : $(promo_clicked).find('p.brand').text(),
						'creative' : Math.round(promo_bg.getBoundingClientRect().width)+'w x '+Math.round(promo_bg.getBoundingClientRect().height)+ 'h',
						'position' : promo_position
					});
					break;
					
				}
				
				dataLayer.push({
				    'event': 'promotionClick',
				    'ecommerce': {
				      'promoClick': {
				        'promotions': promotion_data
				      }
				    }
				});
			}
			
			
			$('.homepage-grid .wrapper [class*="-tile"]').on('click', '.full-link', function(event){
				handle_promo_click(this, 'promo_tile' /*, tile position calculated inside function */);
			});
			
			
			$( ".nav .has-sub .banner .content-asset a" ).click(function(clickEvent){
				handle_promo_click(this, 'promo_meganav', '1');
			});
			
			
			$('.header-slider').each(function(){
				var initialPromoIndex = 1;
				$(this).find('a').each(function(){
					$(this).click(function(){
						var promo_tile = this;
						var promotion_data = [];
						promotion_data.push({
							'id': $(promo_tile).find('.pre-heading').text(),
							'name': $(promo_tile).find('.heading').text(),
							'creative' : Math.round(promo_tile.getBoundingClientRect().width)+'w x '+Math.round(promo_tile.getBoundingClientRect().height)+ 'h',
							'position' : initialPromoIndex
				        });
						dataLayer.push({
						    'event': 'promotionClick',
						    'ecommerce': {
						      'promoClick': {
						        'promotions': promotion_data
						      }
						    }
						});
					});
				});
			});
			
			
			$('div.intro-area').on('click', 'a', function() {
				handle_promo_click($(this).closest('div.intro-area'), 'promo_dept_hero_content', 1);
			});
		});
	</script>




	<script>
	window.addEventListener('load', function(){
		var promo_data = [];
		var initialPromoIndex = 0;
		
		
		$('.header-slider').each(function(){
			initialPromoIndex = 1;
			var promo_tile = $(this).find('a');
			promo_tile = promo_tile[0];
			promo_data.push({
				'id': $(promo_tile).find('.pre-heading').text(),
				'name': $(promo_tile).find('.heading').text(),
				'creative' : Math.round(promo_tile.getBoundingClientRect().width)+'w x '+Math.round(promo_tile.getBoundingClientRect().height)+ 'h',
				'position' : initialPromoIndex
			});
		});
		
		
		$('.listings-header .intro-area').each(function(index){
			initialPromoIndex = index + 1;
			var intro_area = $(this)[index];
			promo_data.push({
				'id': $(this).find('h1').text(),
				'name': $(this).find('h1').text(),
				'creative' : Math.round(intro_area.getBoundingClientRect().width)+'w x '+Math.round(intro_area.getBoundingClientRect().height)+ 'h',
				'position' : initialPromoIndex
			});
		});
		
		
		var home_tile_index = 1;
		$('.homepage-grid .wrapper .row').not('.more-content').find('.columns').children('[class*="-tile"]').each(function(){
			var promo_tile = $(this).find('.full-link');
			if(promo_tile.length === 1) {
				promo_tile = promo_tile[0];
				promo_data.push({
					'id': $(this).find('.pre-heading').text(),
					'name': $(this).find('h4').text(),
					'creative' : Math.round(promo_tile.getBoundingClientRect().width)+'w x '+Math.round(promo_tile.getBoundingClientRect().height)+ 'h',
					'position' : (home_tile_index ++) + initialPromoIndex
				});
			}
		});
		
		$('.amplience-abstract.short-tile').each(function(index){
			if($(this).find('.center-vertical')[0]) {
				
				var text_content = $(this).find('.center-vertical')[0];
				promo_data.push({
					'id': $(text_content).find('.pre-header').text(),
					'name': $(text_content).find('p').text(),
					'creative': Math.round(this.getBoundingClientRect().width)+'w x '+Math.round(this.getBoundingClientRect().height)+ 'h',
					'position': index + 1,
				});
			}
			else if($(this).find('.panel-text')[0]) {
				
				var text_content = $(this).find('.panel-text')[0];
				promo_data.push({
					'id': $(text_content).find('h4').text(),
					'name': $(text_content).find('p').text(),
					'creative': Math.round(this.getBoundingClientRect().width)+'w x '+Math.round(this.getBoundingClientRect().height)+ 'h',
					'position': index + 1,
				});
			}
		});
		dataLayer.push({
		  'ecommerce': {
		    'promoView': {
		      'promotions': promo_data
		    }
		  }
		});
	});
		
	
	</script>








