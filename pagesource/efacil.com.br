<script>
	function criarCookie(SNameCookie, SValueCookie, expireTime, cookiePath){
		if(cookiePath == undefined){
			cookiePath = "/";
		}
		if(expireTime == undefined){
			expireTime = 365;
		}
		document.cookie= SNameCookie + "=" + SValueCookie + "; expires=" + expireTime + "; path=" + cookiePath;
		//dojo.cookie(SNameCookie,SValueCookie,{expires: expireTime, path: cookiePath});
	}
	function excluirCookie(SNameCookie){
		var allCookiesStr = document.cookie;
		var allCookies = allCookiesStr.split(";");
		var d = new Date(-365);

		for(i=0; i<allCookies.length; i++){
			var cookieName = allCookies[i].split("=")[0].trim();
			if(SNameCookie == cookieName){
				//dojo.cookie(SNameCookie,{expires: d});
				document.cookie= SNameCookie + "=; expires=" + d.getTime() + ";";
			}
		}
	}

	excluirCookie("userID");
	excluirCookie("userName");
	excluirCookie("userMail");
	excluirCookie("storeName");
	
	if("desconhecido" == "logado"){
		criarCookie("userID",  "-1002", 0, "/");
		criarCookie("userName",  "", 0, "/");
		criarCookie("userMail",  "", 0, "/");
		criarCookie("storeName",  "", 0, "/");
	}else{
		criarCookie("userID",  "", 0, "/");
		criarCookie("userName",  "", 0, "/");
		criarCookie("userMail", "", 0, "/");
		criarCookie("storeName", "", 0, "/");
	}	
	
</script>
