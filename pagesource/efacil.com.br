<script type="text/javascript">
	Personalization = {};
	if (!String.prototype.trim) {
		  (function() {
		    // Make sure we trim BOM and NBSP
		    var rtrim = /^[\s\uFEFF\xA0]+|[\s\uFEFF\xA0]+$/g;
		    String.prototype.trim = function() {
		      return this.replace(rtrim, '');
		    };
		  })();
	};
	function displayBtnConfirm(){
		var display = "none";
		var elements = dojo.query('input[id^="linha"]');
		elements.forEach(function(e){
		if(e.value!= null && e.value != undefined && e.value.trim() != ""){display="block"}
		})
		dojo.query('.modal-personalization-confirm')[0].style.display=display;
	};
	function personalizationProcessConfirmation(){
		document.getElementById('btnConfirmPersonalization').click(function() { return false; }); 
		var linha="";
		var elements = dojo.query('input[id^="linha"]');
		elements.forEach(function(e){linha+= e.value.trim()==''?'| ':'|'+e.value.trim();});//primer linea lleva un '|' de mas.
		linha = linha.replace('|','@')+'@';//borro la ocurrencia extra y agrego @.
		Personalization.type=2;
		Personalization.linha=encodePortugueseAsHTML(linha);
		document.getElementById('buyBtn').click(); //presiono el btn comprar.
		hideBox('modal-personalization');
		return false;
	};
	function cleanPersonalizationModalImputs(){
		var elements = dojo.query('input[id^="linha"]');
		elements.forEach(function(e){e.value=""});
		dojo.query('.modal-personalization-confirm')[0].style.display="none";
		Personalization = {};
	}
	function replaceNAChar(e){
		var evtobj = window.event? event : e;
		 var code=evtobj.charCode? evtobj.charCode : evtobj.keyCode;
		 //Pregunto si presionó el |
		 if(code==124 || code==95){
			 return false;
		 }		
		 return true;
	}
	function validateALTKey(e){
		var evtobj = window.event? event : e;
		 var code=evtobj.charCode? evtobj.charCode : evtobj.keyCode;
		 //Pregunto si mantiene presionado el ALT
		 if(code==18){
			 return false;
		 }		
		 return true;
	}

</script>
		
