// Muestra la fecha con formato de hace ....
var timeAgo = function(dateString, dateDefault) {

	var rightNow = new Date();
	var then = new Date(dateString);
	var diff = rightNow - then;
	var second = 1000,
		minute = second * 60,
		hour = minute * 60;

	if (isNaN(diff) || diff < 0) {
		return "";
	}

	if (diff < minute) {
		return "Ahora"; // "Hace 1 min";
	}

	if (diff < hour) {
		return "Ahora"; // "Hace " + Math.floor(diff / minute) + " min";
	}

	if (diff < hour * 4) {

		if (Math.floor(diff / hour) == 1) {
			return  "Hace 1 hora";
		}

		return  "Hace " + Math.floor(diff / hour) + " horas";
	}

	if (diff < hour * 24) {
		return  "Hoy";
	}

	return dateDefault;
};

$(document).ready(function(){
	$("[data-fecha]").each(function() {
		m = $(this);
		if ($(m).attr("data-fecha") != undefined) {
			fechaText = timeAgo($(m).attr("data-fecha"), $(m).html());
			$(m).html(fechaText);

			// Modificación de la clase de manejo de etiquetas
			if (fechaText.length > 7) {
				$(m).parent().find(".etiqueta").addClass("fecha-larga");
			}
		}
	});
});

