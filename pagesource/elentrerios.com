            done = true;
        }
    }
    function stopVideo() {
        player.stopVideo();
    }

    $(document).ready(function(){
    });

    $(".vivo-content .volumen").click(function() {
        if (player.isMuted()) {
            player.unMute();
            $(".icon-volumen").removeClass("off")
        } else {
            player.mute();
            $(".icon-volumen").addClass("off")
        }
    });

    $(".vivo-content .icon-cerrar").click(function() {
        $(".vivo-content").slideUp();
        videoIsClose = true;
        stopVideo();
