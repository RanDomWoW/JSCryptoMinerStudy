                    $(this).parents(".compartir").find(".icon-facebook").toggleClass('bounceIn');
                    $(this).parents(".compartir").find(".icon-twitter").toggleClass('bounceIn');
                    $(this).parents(".compartir").find(".icon-facebook").toggleClass('zoomOut');
                    $(this).parents(".compartir").find(".icon-twitter").toggleClass('zoomOut');
                });
            });


            var encOneClick = [];

            var eCookieName = 'enc-4';
            if ($.cookie(eCookieName)) {

                encOneClick[eCookieName] = 1;
                $(".encuesta").addClass("ver-resultados");
                processEnc(4, 0);
            }

            // Vota en una encuesta activa
            $(".encuesta .items-votar .item").click(function() {

                enc = $(this).attr("data-encuesta-id");
                itemClick = 'enc-'+enc;

                if (!encOneClick[itemClick]) {

                    encOneClick[itemClick] = 1;

                    $(this).addClass("active");
                    val = $(this).attr("data-enc-item-id");

                } else {

                    $(this).parents(".encuesta").find(".error").html("Ud. ya votó").css("display","inline-block");
                    val = 0;
                }

                $(this).parents(".encuesta").addClass("ver-resultados");
                processEnc(enc, val);
            });

            function processEnc(enc, val) {

                $.ajax({
                    type: 'POST',
                    async: false,
                    url: '/extras/encuestas/votar.php',
                    dataType: 'json',
                    data: {enc:enc, id:val},
                    success: function(data){

                        total = 0;
                        if (data.error) {

                            $(".encuesta[data-encuesta-id='"+enc+"'] .error").html("Ud. ya votó").css("display","inline-block");

                        } else {

                            $.cookie(eCookieName, '1', { expires: 1, path: '/'});
                        }

                        $.each(data.info.items, function(i, data) {

                            total+=parseInt(data.cantidad);

                            $("[data-enc-item-id='"+i+"'] .cant").html(data.porcentaje);
                            $("[data-enc-item-id='"+i+"'] .borde").animate({width:data.porcentaje},1000, function(){
                            });
                        });

                        if (total > 100) {
                            $(".encuesta[data-encuesta-id='"+enc+"'] .cont-votos").html(total + ' votos');
                        }

                    },
                    error: function(jqXHR, textStatus, errorThrown) {
                        // Error
                        $(".encuesta[data-encuesta-id='"+enc+"'] .error").html("Error en la encuesta").css("display","inline-block");
                    }

                });
            }
