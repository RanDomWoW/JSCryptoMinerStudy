    });
    function checkVideoIsOnLine() {

        
        if (!videoIsClose) {

            $.ajax({
                url: '/extras/notas/vivo.php',
                type: 'GET',
                cache: false,
                dataType: 'json',
                success: function(data, textStatus, jqXHR) {



                    if (data.code != '') {

                        videoIsClose = true;

                        player = new YT.Player('yt-vivo', {
                            height: '208',
                            width: '117',
                            videoId: data.code,
                            playerVars: {
                                'controls': 0,
                                'showinfo': 0
                            },
                            events: {
                                'onReady': onPlayerReady,
                                'onStateChange': onPlayerStateChange
                            }
                        });

                        $(".vivo-content h3").html(data.titulo);
                        $(".vivo-content").slideDown();
                    }

                },

                error: function(jqXHR, textStatus, errorThrown) {
                }

            });

            setTimeout('checkVideoIsOnLine()',5000);
        }

            }
