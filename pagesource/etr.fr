<script type="text/javascript">
$(document).ready(function() {
	$('#searchString').keyup(function(e) {
		clearTimeout($.data(this, 'timer_search'));
		if (e.keyCode == 13)
		  search_word(true);
		else
		  $(this).data('timer_search', setTimeout(search_word, 500));
	});
});
function search_word(force) {
    var existingString = $("#searchString").val();
    if (!force && existingString.length < 3) return; //wasn't enter, not > 2 char 
        /*$('div#results').html(data);*/
		$("#search_result_content").load("http://www.etr.fr/process/search.php",{"post_string":existingString}, function(){ //recherche du mot
		});
        $('#search_result_div').show();
}
</script>
