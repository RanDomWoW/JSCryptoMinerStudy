<script type="text/javascript">
$(document).ready(function() {
    $("#button_login").on( "click", function (e){
		login_user = $('#post_login').val();
		password_user = $('#post_password').val();
		
        if($('#post_remember').is(":checked")) {
            // checkbox is checked -> do something
			remember_user = 1;
        } else {
            // checkbox is not checked -> do something different
			remember_user = 0;
        }
		
        $("#div_login").load("http://www.etr.fr/process/login.php",{"post_login":login_user,"post_password":password_user,"post_remember":remember_user}, function(){ //connexion
		});
		setTimeout(function(){
			// Refresh après l'exécution
			location.reload();
		}, 2000);
	});
});
</script>
