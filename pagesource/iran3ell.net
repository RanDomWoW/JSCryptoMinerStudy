    <script type="text/javascript">
        var animateDirectCharge = false;
        
        $(document).ready(function () {
                
            $('#operators li').text(' ');

            var currentSelectedOperator = $('#selected-operator').val();
            $('#' + currentSelectedOperator).css('display', 'block');

            for (var i = 0; i < 5; i++) {
                $('#quantity option:nth(' + i + ')').text($('#quantity option:nth(' + i + ')').text() + ' عدد ');
            }
            for (i = 0; i < 24; i++) {
                $('#bank-number-' + i).css('background-position', (-i * 24) + 'px 0px');
            }

            $('#' + currentSelectedOperator + '-cards').css('display', 'block');
            $('#cards #quantity').after('<label for="quantity" class="quantityLabel">تعداد:</label>');


            function getleft() { $('#wrapper').css('right', '500px'); }
            $('.mid .closer').click(function () {
                $('.menu .irancell').click();
            });
            var IE6 = false/*@cc_on || @_jscript_version <= 5.7@*/;
            if (IE6) {
                //alert('IE6 detected');
                $('#banks #banks-list li').hover(function () {
                    $(this).css({ 'border': 'solid 2px #ffb300', 'margin': '3px' });
                }, function () {
                    if (!$(this).is('.selected')) {
                        $(this).css({ 'border': 'none', 'margin': '5px' });
                    }
                });
                $('#banks #banks-list li').click(function () {
                    $('#banks #banks-list li').css({ 'border': 'none', 'margin': '5px' });
                    $(this).css({ 'border': 'solid 2px #ffb300', 'margin': '3px' });
                });
            }
            
            //topup
            var topupIsEnable = true ;
            if (!topupIsEnable) {
                $('#irancellTopup').remove();
            }
            
            if (!animateDirectCharge) {
                $('.menu ul li.' + currentSelectedOperator).click();
            } else {
                $(window).load(function() {
                    $('.menu ul li.' + currentSelectedOperator).click();
                });
            }
        });
        
        function menuClick(e) {
            var $this = $(e);
            if ($this.hasClass('selected-operator')) {
                return;
            } else {
                var selectedOperator = $this.attr('class');
                $('.menu ul li').removeClass('selected-operator');
                $('#operators li').css('display', 'none');
                $('.operator').css('display', 'none');
                $('#' + selectedOperator).css('display', 'block');
                $('#' + selectedOperator + '-cards').css('display', 'block');
                $this.addClass('selected-operator');
                $('div#cards input:radio').attr('checked', '');
                    
                if (selectedOperator == 'irancell') {
                    if (animateDirectCharge) {
                        $('#irancellTopup').show();
                        $('#irancellTopup a').delay(200).animate({ "height": "45px" }, 0);
                    } else {
                        $('#irancellTopup a').delay(200).css("height", "45px");
                    }
                } else {
                    $('#irancellTopup a').delay(200).animate({ "height": "0px" }, 0, function() {
                        $('#irancellTopup').hide();
                    });
                }
                animateDirectCharge = true;
                return;
            }
        }
    </script>
