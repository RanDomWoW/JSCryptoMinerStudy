<script>
    if (!defer) {
        function defer(method) {
            if (window.jQuery) {
                method();
            } else {
                setTimeout(function () {
                    defer(method);
                }, 50);
            }
        }
    }

    function menu() {

        var more = $('#more');
        var newlinks = $('.header-new-links-container');
        var links = newlinks.find("[id^=link]").length;
        var maxW =
                parseInt(newlinks.css("max-width").replace(/[^0-9\.]/g, ''), 10)
                * (newlinks.css("max-width").indexOf("%", newlinks.css("max-width").length - 1) !== -1 ? newlinks.parent().width() / 100 : 1)
                - more.outerWidth(true);

        newlinks.css("opacity", "0");
        more.css("opacity", "0");

        newlinks.find("[id^=link]").hide();
        more.find("[id^=more]").show();
        for (var i = 1; i <= links; i++) {
            newlinks.find("#link" + i).show();
            more.find("#more" + i).hide();
            if (newlinks.outerWidth() > maxW) {
                newlinks.find("#link" + i).hide();
                more.find("#more" + i).show();
                break;
            }
        }

        newlinks.css("opacity", "1");
        more.css("opacity", "1");

        var more2 = $('#more-old');
        if (!more2.find("ul").length) {
            more.find("ul").clone().appendTo(more2);
            more2.find("[id^=more]").show();
        }

        /* EMERGENCY DROP DOWN */
        $('#emergency').find('.floating').width($('#emergency').outerWidth(true) + 'px');
        $('#emergency').find('.header-quick-popup--one-zero-five').width($('#emergency').outerWidth(true) + 'px');
        $('#emergency').find('.header-quick-popup--one-zero-five').css('min-height', $('#emergency').outerHeight(true) + 'px');
        $('#emergency').find('.header-quick-popup--one-zero-five').css('top', $('#emergency').find('.floating').css('top'));
        /* ------------------- */

        /* SEARCH BOX RESIZE */
        try {
            var w = $('.top-bar__login').width();
            //console.log(w);
            $('#searchlegend-button').css('right', (w + 28) + 'px');
            $('.searchcontainer').css('padding-right', (w + 95) + 'px');
        } catch (e) {
            console.log("Some of the elements not found?");
        }
        /* ---------------- */
    }
    ;

    defer(function () {
        $(document).ready(function () {
            menu();
        });
        $(window).on('resize load', function () {
            menu();
        });
    });
</script>

   
