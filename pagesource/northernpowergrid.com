<script type="text/javascript">
    function defer(method) {
        if (window.jQuery) {
            method();
        } else {
            setTimeout(function() { defer(method) }, 50);
        }
    }
    
    defer(function(){
                var submitIcon = $('.searchbox-icon');
                var inputBox = $('.searchbox-input');
                var searchBox = $('.searchbox');
                var searchContainer = $('.searchcontainer');
                var isOpen = false;
                var searchToggle=function(){
                    if(isOpen == false){
                        searchContainer.addClass('searchcontainer-open');
                        searchBox.addClass('searchbox-open');
                        inputBox.focus();
                        isOpen = true;
                    } else {
                        searchContainer.removeClass('searchcontainer-open');
                        searchBox.removeClass('searchbox-open');
                        inputBox.focusout();
                        isOpen = false;
                    }
                };  
                 submitIcon.mouseup(function(){
                        return false;
                    });
                searchBox.mouseup(function(){
                        return false;
                    });
                $(document).mouseup(function(){
                        if(isOpen == true){
                            $('.searchbox-icon').css('display','block');
                            submitIcon.click();
                        }
                    });
               $('#searchlegend-button').click(searchToggle);
                $('#searchlegend-button').mouseup(function(){ return false; });
                submitIcon.click(searchToggle);
            });
            
            
            
                function buttonUp(){
                    var inputVal = $('.searchbox-input').val();
                    inputVal = $.trim(inputVal).length;
                    if( inputVal !== 0){
                        $('.searchbox-icon').css('display','none');
                    } else {
                        $('.searchbox-input').val('');
                        $('.searchbox-icon').css('display','block');
                    }
                }

</script>
    

