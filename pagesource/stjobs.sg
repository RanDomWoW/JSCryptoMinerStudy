<div id="divEmarsysRecommendation" class='WEmarsysRecommend' data-logic='HOME' data-limit='9' data-onsuccess='WEmarsysRecommendSuccess' data-template='emarsys-search-recommend-template'></div>
<script>
    function WEmarsysRecommendSuccess(SC, render){
    var jobids = [];
    jQuery(SC.page.products).each(function(){
    jobids.push(this.id);
    });
    jQuery.get('/candidate/jobsearch/getJobInfoByIDs', {'jobs':jobids}, function(data) {
    SC.page.products = data;
    render(SC);
    }, "json");
    }
</script>
<script type="text/html" id="emarsys-search-recommend-template">
    <![CDATA[
    {{ if (SC.page.products.length) { }}
    <div class="container clearfix" >
        <div class="company-profile-job-listings margin-top-10">
            <div class="col-sm-12 clearfix"><h3><span class="glyphicon glyphicon-thumbs-up" aria-hidden="true"></span> Recommended Jobs </h3></div>
            {{ for (var i=0; i < SC.page.products.length; i++) { }}
            {{ var p = SC.page.products[i]; }}
            <div data-scarabitem="{{= p.id }}" class="clearfix col-sm-6 col-md-6 col-lg-4 company-profile-listings">
                <div class="col-xs-4 col-sm-4 col-md-4 stj-hm-rec-logo-col">
                    {{ if (!p.logo || p.hideCompany){  }}
                    <div class="stj-hm-rec-logo"><img src="/img/stj-now_hiring_logo-120x120.jpg" alt=""/></div>
                    {{ }else { }} 
                    <div class="stj-hm-rec-logo"><img src="{{= p.logo }}?type=thumbnail&size=100" alt=""/></div>
                    {{ } }}
                </div>
                <div class="col-xs-8 col-sm-8 col-md-8 stj-hm-rec-info-col">
                    <div>
                        <h4 class="text-ellipsis margin-bottom-0">
                            <a title="{{= p.title }}" onclick="_gaq.push(['_trackEvent', 'Emarsys_Landing', '{{= p.companyName.replace(/[\']/g, "\\'") }}', '{{= p.title.replace(/[\']/g, "\\'")+" ("+p.id+")" }}']);" target="_blank" href="{{= p.url+"?ems" }}">{{= p.title }}</a>
                        </h4>
                    </div>                
                    <div class="text-ellipsis"><a title="{{= p.companyName }}" class="company-link"  onclick="_gaq.push(['_trackEvent', 'Emarsys_Landing', 'CoyTextLink','{{= p.companyName.replace(/[\']/g, "\\'") }}', {{= p.companyId }}]);" target="_blank" href="{{= p.companyURL }}">{{= p.companyName }}</a></div>                
                    <div class="recommend-text-highlight text-ellipsis">
                        {{ if(p.isShowSalary){ }}
                        <i class="fa fa-usd pull-left text-primary"></i>{{= p.salary }}
                        {{ }else if(Math.round(Math.random()) && p.city){ }}
                        <i class="fa fa-map-marker pull-left text-primary"></i>{{= p.city }}
                        {{ }else if(p.minExp){ }}
                        <i class="fa fa-briefcase pull-left text-primary"></i>{{= p.minExp }}
                        {{ } }}
                    </div>
                </div>
            </div>
            {{ } }}
        </div>
        <hr/>
    </div>
    {{ } }}
    ]]>
</script>
