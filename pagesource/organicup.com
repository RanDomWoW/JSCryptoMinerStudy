	function countCartItems()
    {
		var total = 0;
		$('#cart .mini_cart_item').each(function()
        {
			var n = Number($(this).find('.quantity_number').text());
			if(!isNaN(n)) total += n;
		});
		$('.cart-icon .cart-items').text(total);
		if(total == 0) $('.cart-icon').addClass('empty');
		else $('.cart-icon').removeClass('empty');
		
		if($('body').hasClass('woocommerce-checkout')) $('body').trigger( 'update_checkout' );
	}
    $('.product-in-cart').find('form').on('submit', function(e)
