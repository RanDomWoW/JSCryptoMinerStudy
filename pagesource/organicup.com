<script>
var cdtimers_interval;
jQuery(document).ready(function($)
{
    cdtimers_interval = setInterval(function()
    {
        $('cdtimers').each(function()
        {
            var remain = parseInt($(this).attr('data-remain'));
            var countdown = $(this).attr('data-countdown');
            if (countdown == '1')
            {
                if (remain >= 0)
                {
                    var global_remain = remain;
                    var d = Math.floor(remain / 86400);
                    var dd = (d < 10) ? '0'+d : d;
                    remain -= (d * 86400);
                    var h = Math.floor(remain / 3600);
                    var hh = (h < 10) ? '0'+h : h;
                    remain -= (h * 3600);
                    var m = Math.floor(remain / 60);
                    var mm = (m < 10) ? '0'+m : m;
                    remain -= (m * 60);
                    var s = remain;
                    var ss = (s < 10) ? '0'+s : s;
                    $(this).find('.cdtimers-d').html(d);
                    $(this).find('.cdtimers-dd').html(dd);
                    $(this).find('.cdtimers-h').html(h);
                    $(this).find('.cdtimers-hh').html(hh);
                    $(this).find('.cdtimers-m').html(m);
                    $(this).find('.cdtimers-mm').html(mm);
                    $(this).find('.cdtimers-s').html(s);
                    $(this).find('.cdtimers-ss').html(ss);
                    global_remain--;
                    $(this).attr('data-remain', global_remain);
                }
            }
        });
    }, 1000);
});
</script>
