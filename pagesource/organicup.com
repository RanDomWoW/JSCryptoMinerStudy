	$('.single_add_to_cart_button_in_cart').click(function(e){
		e.preventDefault();
		var prnt = $(this).parents('.product');
		var product = prnt.find('input[name="add-to-cart"]').val();
		if(prnt.find('.variation_id').length){
			var variation = prnt.find('.variation_id').val();
