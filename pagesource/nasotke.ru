				$(this).addClass('active').find('span').text('Свернуть');
			}
			return false;
		});	

		$('.cats .pod li:nth-child(n+4)').hide();
		$('.cats .show_cat').click(function(){
			var col = $(this).parent().parent().parent();
			if($(col).hasClass('active')){
				$(this).parent().children("li:nth-child(n+4)").slideUp(500);
				$(this).text('Смотреть все');
				$(col).removeClass('active');
			}else{
				$(this).parent().children("li").slideDown(500);
				$(this).text('Свернуть');
				$(col).addClass('active');
			}
				return false;
		});

		$('.slider-photo').slick();
		$('.slider-video').slick();

	});
</script>






