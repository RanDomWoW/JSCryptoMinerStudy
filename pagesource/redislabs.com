jQuery('.counter').each(function() {
  var x = $(this).html();
  x = x.replace(/[^0-9]/g, '');
  x = parseInt(x, 10);

  if ( x > 10 ) {
    $(this).counterUp({
      delay: 10,
      time: 1000
    });
  }
});
</script>

