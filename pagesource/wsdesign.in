$("body").on("click","#saved",function() {    

    if($('#subscribe_form').valid())
    {
          var email = $("#email").val();
                
          var SubscribeMaster  = {"email":email};
            $.ajax({
                type: 'POST',
                url: 'http://wsdesign.in/fwebservices/fsubscribeemail',
                async: false,
                data: {SubscribeMaster:SubscribeMaster},
                dataType: "json",
                success: function(data) {

                 
                  if(data.SubscribeMaster.status == 1)
                  {
                     // $("#sub_msg").css("display","block");
                     // $("#sub_msg").html("subscription done");

                      $("#news_stat").trigger("click");
                  }
                  if(data.SubscribeMaster.status == 2)
                  {
                    
                      $("#sub_msg").css("display","block");
                      $("#sub_msg").html("you are already subscribed");
                  }
                  if(data.SubscribeMaster.status == 3)
                  {
                      $("#sub_msg").css("display","block");
                      $("#sub_msg").html("Request Invalid.");

                  } 
                  
                  $("#email").val("");
                  setTimeout(function(){
                    $("#sub_msg").css("display","none");
                  },3000);               

                }
            });

            return false;
    }
    return false;

});

 

