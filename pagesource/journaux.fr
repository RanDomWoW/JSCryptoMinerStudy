					return false;
				}

				return true;
			}

			function verifierFrmLogin()
			{
				var verifMail = /^([a-zA-Z0-9_-])+([.]?[a-zA-Z0-9_-]{1,})*@([a-zA-Z0-9-_]{2,}[.])+[a-zA-Z]{2,}$/; // mail
				var verifDate = /^\d{1,2}\/\d{1,2}\/\d{4}$/;
				var verifFlotant = /^\d+(\.|,)?\d*$/ ;
				var verifEntier = /^\d+$/ ;

				if (!verifMail.test($('txtLoginEmail').value))
				{
					Sexy.alert('<p>Veuillez renseigner une adresse e-mail valide.</p>', {
					  onComplete:
					    function(returnvalue) {
					      $('txtLoginEmail').focus();
					    }
						});

					return false;
				}

				if ($('txtLoginPasse').value.length <= 0)
				{
					Sexy.alert('<p>Veuillez saisir votre mot de passe.</p>', {
					  onComplete:
					    function(returnvalue) {
					      $('txtLoginPasse').focus();
					    }
						});

					return false;
				}

				return true;
			}
			function check(param){
				if($(param).value != ''){
					$(param).removeClass('invalide');
					$(param).addClass('valide');
				}
				else{
					$(param).removeClass('valide');
					$(param).addClass('invalide');
				}
			}

