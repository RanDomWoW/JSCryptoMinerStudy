        <script type="text/javascript">
            $(function() {
                function setupMobileDemo() {
                    var $image = $('#mobile-screen');
                    var $links = $('#accordion2 a');
                    $links.click(function() {
                        var text = $(this).text();
                        var image = 'images/mobileapp_new.png';

                        switch(text.trim()) {
                            case 'Appointments Home Page':
                                image = 'images/mobile/mobileapp_new.png';
                                break;
                            case 'Status Page':
                                image = 'images/mobile/mobile_status.png';
                                break;
                            case 'Picture Upload':
                                image = 'images/mobile/mobile_file.png';
                                break;
                            case 'Mapping':
                                image = 'images/mobile/mobile_mapping.png';
                                break;
                        }

                        $image.attr('src', image);

                        return false;
                    });
                }

                setupMobileDemo();
            });
        </script>
