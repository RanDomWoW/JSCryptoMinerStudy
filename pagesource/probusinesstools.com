    <script type="text/javascript">
        function AJAXRequest(url, params, callbackfunction, errorfunction, beforesend, datatype, async) {
            if (!url || url.length == 0) { return false; }
            if (!params || params.length == 0) { params = "{}"; }
            if (!callbackfunction) callbackfunction = function () { return; }
            if (!errorfunction) errorfunction = function () { return; }
            if (!beforesend) beforesend = function () { return; };
            if (!datatype) datatype = "json";
            if (!async) async = true;

            $.ajax({
                type: "POST",
                url: url,
                data: params,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: callbackfunction
            });

        }


        $("#btnSendMessage").click(function () {
            AJAXRequest("Default.aspx/ContactUs", JSON.stringify({
                name: $("#customerName").val(),
                email: $("#customerEmail").val(), 
                phone: $("#customerPhone").val(),
                subject: $("#customerSubject option:selected").val(),
                message: $("#customerRequest").val()
            }), function (r) {
                alert(r.d);
            }, null, null, "json", true);
        });
    </script>
