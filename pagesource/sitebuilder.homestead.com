else
{
flashvars.configFile = "/~site/Media/tour/config.xml";
swfobject.embedSWF("/~site/Media/tour/player.swf", "flashcontent", width, height, "7.0.0", "", flashvars, params);
}
}
else
{

var flashcontent = document.getElementById("flashcontent");
var html = '<a href="http://www.adobe.com/go/gntray_dl_getflashplayer" class="noflash_image" rel="nofollow" target="_blank">';
