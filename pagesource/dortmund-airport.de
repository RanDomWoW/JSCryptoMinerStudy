  var marker_image_size_default = {x: 16, y: 16};
  var marker_image_size_hover = {x: 20, y: 20};

  var marker_image_dtm_default = '/assets/marker_dtm_default-7fece5facaa46c745433f439a6fec5ab.png';
  var marker_image_dtm_hover = '/assets/marker_dtm_default-7fece5facaa46c745433f439a6fec5ab.png';
  var marker_image_dtm_selected = '/assets/marker_dtm_selected-75930653ddcdf0b41bdeb12fc789a769.png';
  var marker_image_dtm_selected_hover = '/assets/marker_dtm_selected-75930653ddcdf0b41bdeb12fc789a769.png';
  var marker_image_dtm_size_default = {x: 24, y: 24};
  var marker_image_dtm_size_hover = {x: 24, y: 24};



  function initialize() {
    var center_map = new google.maps.LatLng(map_center_lat, map_center_lon);
    var MY_MAPTYPE_ID = 'custom_style';
    var featureOpts = [
        {
          "stylers": [
            { "color": "#f2f2f2" }
          ]
        },{
          "featureType": "landscape",
          "stylers": [
            { "color": "#cacaca" }
          ]
        },{
          "featureType": "administrative",
          "stylers": [
            { "visibility": "off" }
          ]
        },{
          "featureType": "poi",
          "stylers": [
            { "visibility": "off" }
          ]
        },{
          "featureType": "road",
          "elementType": "labels",
          "stylers": [
            { "visibility": "off" }
          ]
        },{
          "featureType": "road",
          "stylers": [
            { "visibility": "off" }
          ]
        },{
          "featureType": "transit",
          "stylers": [
            { "visibility": "off" }
          ]
        }, {
          "featureType": "administrative.country",
          "elementType": "geometry.stroke",
          "stylers": [
            { "visibility": "on" },
            { "color": "#c1c1c1" },
            { "weight": 2 }
          ]
        }
      ];

    var mapOptions = {
      zoom: zoom,
      center: center_map,
      disableDefaultUI: true,
      scrollwheel: false,
      mapTypeControlOptions: {
        mapTypeIds: [google.maps.MapTypeId.ROADMAP, MY_MAPTYPE_ID]
      },
      mapTypeId: MY_MAPTYPE_ID
    };

    map = new google.maps.Map(document.getElementById('map-canvas'),
        mapOptions);

    var styledMapOptions = {
      name: 'Custom Style'
    };

    var customMapType = new google.maps.StyledMapType(featureOpts, styledMapOptions);
    map.mapTypes.set(MY_MAPTYPE_ID, customMapType);
    processLocation(locations);
  }

  function createMarker(location) {
    var m_image = (location.location_id === home_location)? marker_image_dtm_default : marker_image_default;
    var m_image_hover = (location.location_id === home_location)? marker_image_dtm_hover : marker_image_hover;
    var m_image_selected = (location.location_id === home_location)? marker_image_dtm_selected : marker_image_selected;
    var m_image_selected_hover = (location.location_id === home_location)? marker_image_dtm_selected_hover : marker_image_selected_hover;
    var m_image_size_default  = (location.location_id === home_location)? marker_image_dtm_size_default : marker_image_size_default;
    var m_image_size_hover  = (location.location_id === home_location)? marker_image_dtm_size_hover : marker_image_size_hover;
    var loc = location.location_id;
    var posLatLng = new google.maps.LatLng(location.lat, location.lon);
    //var posPoint = map.getProjection().fromLatLngToPoint(posLatLng);
    //var iconPosLatLng = map.getProjection().fromPointToLatLng(new google.maps.Point(posPoint.x, posPoint.y -8));

    //7.874093045758649

    var icon = {
      url: m_image,
      anchor: new google.maps.Point(m_image_size_default.x / 2, m_image_size_default.y / 2),
      size: new google.maps.Size(m_image_size_default.x, m_image_size_default.y)
    };


    var newmarker = new google.maps.Marker({
      position: posLatLng,
      map: map,
      icon: icon,
      image_path: m_image,
      image_path_hover: m_image_hover,
      image_path_selected: m_image_selected,
      image_path_selected_hover: m_image_selected_hover,
      image_size_default: m_image_size_default,
      image_size_hover: m_image_size_hover,
      title: location.label,
      location_id: loc,
      port_code: location.port_code,
      country: location.country,
      selectable: location.selectable,
      content: infowindowTemplate(location).content
    });

    google.maps.InfoWindow.prototype.isOpen = function(){
      var map = this.getMap();
      return (map !== null && typeof map !== "undefined");
    };

    newmarker['infowindow'] = new google.maps.InfoWindow(infowindowTemplate(location));

    google.maps.event.addListener(newmarker, 'mouseover', function() {
   //   this['infowindow'].open(map, this);
      hoverMarker(this, "in");
    });

    google.maps.event.addListener(newmarker, 'mouseout', function() {
   //   this['infowindow'].close(map, this);
      hoverMarker(this, "out");
    });

    google.maps.event.addListener(newmarker, 'click', function() {
      selectMarker(this);
    });

    markers[loc] = newmarker;
    bounds.extend(newmarker.position);

  }

  function processLocation(locations) {
    for (var i = 0; i < locations.length; i++) {
      if(locations[i][0] !== 0) {
        createMarker(locations[i]);
      }
    }
  }

  function infowindowTemplate(location) {
    var html = '<div class="infowindow">\
      <p class="title">' + location.label + '</p>\
      <p><span class="info-text">' + location.country + '</span></p>\
      <div class="info-text">' + location.description + '</div>';
    return { content: html, maxWidth: 650 };
  }

  function hoverMarker(marker, mode) {
    if(mode === "in") {
      var image_path = (marker.getIcon().url === marker.image_path_selected) ? marker.image_path_selected_hover : marker.image_path_hover;
      var icon = {
        url: image_path,
        anchor: new google.maps.Point(marker.image_size_hover.x / 2, marker.image_size_hover.y / 2),
        scaledSize: new google.maps.Size(marker.image_size_hover.x, marker.image_size_hover.y)
      };
      marker.anchorPoint = new google.maps.Point(0, -14);
    } else {
      var image_path = (marker.getIcon().url === marker.image_path_selected || marker.getIcon().url === marker.image_path_selected_hover) ? marker.image_path_selected : marker.image_path;
      var icon = {
        url: image_path,
        anchor: new google.maps.Point(marker.image_size_default.x / 2, marker.image_size_default.y / 2),
        scaledSize: new google.maps.Size(marker.image_size_default.x, marker.image_size_default.y)
      };
    }
    marker.setIcon(icon);
  }

  function selectMarker(marker) {
    var icon = {
      url: marker.image_path_selected,
      anchor: new google.maps.Point(marker.image_size_default.x / 2, marker.image_size_default.y / 2),
      scaledSize: new google.maps.Size(marker.image_size_default.x, marker.image_size_default.y)
    };
    resetMarkers(marker);
    marker['infowindow'].open(map, marker);

    if(marker.selectable) {
      marker.setIcon(icon);
      var autocompleteTitle = marker.title + ', ' + marker.country + ' (' + marker.location_id + ')';
      $('input#flightmap_search_city').val(autocompleteTitle);
      $('input#flightmap_search_destination_id').val(marker.port_code);
    }
  }

  function resetMarkers(source) {
    for (var key in markers) {
      var marker = markers[key];
      var icon = {
        url: marker.image_path,
        anchor: new google.maps.Point(marker.image_size_default.x / 2, marker.image_size_default.y / 2),
        scaledSize: new google.maps.Size(marker.image_size_default.x, marker.image_size_default.y)
      };
      if(source.selectable) {
        marker.anchorPoint = new google.maps.Point(0, -18);
        marker.setIcon(icon);
      }
      marker['infowindow'].close(map, marker);
    }
  }

  function hasTouch() {
    return document.getElementsByClassName("touch").length > 0;
  }

  function fromLatLngToPoint(latLng, map) {
    var topRight = map.getProjection().fromLatLngToPoint(map.getBounds().getNorthEast());
    var bottomLeft = map.getProjection().fromLatLngToPoint(map.getBounds().getSouthWest());
    var scale = Math.pow(2, map.getZoom());
    var worldPoint = map.getProjection().fromLatLngToPoint(latLng);
    return new google.maps.Point((worldPoint.x - bottomLeft.x) * scale, (worldPoint.y - topRight.y) * scale);
  }

  google.maps.event.addDomListener(window, 'load', initialize);
</script>


