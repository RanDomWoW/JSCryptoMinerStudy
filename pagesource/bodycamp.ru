        function showVideo(videoId) {
            if (typeof jsVideo['VIDEO_' + videoId] == 'undefined') {
                return true;
            }

            var video = jsVideo['VIDEO_' + videoId];

            $('#video_container_title').text(video.name);
            //$('#video_container').empty().append(video.html);
            $('#video_container iframe')[0].src= video.path;
            $('#video_container iframe')[0].width= video.width;
            $('#video_container iframe')[0].height= video.height;

            $('.videoPart .video_items.active').removeClass('active').show();
            $('.videoPart #video_item_' + videoId).addClass('active').hide();

            return false;
        }
        $(function () {
            showVideo('1429');
