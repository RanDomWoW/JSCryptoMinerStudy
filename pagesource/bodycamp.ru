        $(function () {
            /*events tabs*/
            var eventsTabs = $('.events .tabs li');
            var eventsTabsContent = $('.events .tabsContent');

            var activeTab = '#' + eventsTabs.filter('.active').data('tab');
            eventsTabsContent.find(activeTab).siblings().hide().css('opacity', 0);
            eventsTabs.on('mouseenter', function () {
                if ($(this).hasClass('active')) {
                    return;
                }
                activeTab = '#' + $(this).data('tab');
                $(this).addClass('active').siblings().removeClass('active');
                eventsTabsContent.find(activeTab).siblings().animate({
                    'opacity': 0
                }, 150);
                setTimeout(function () {
                    eventsTabsContent.find(activeTab).siblings().hide();
                    eventsTabsContent.find(activeTab).show().animate({
                        'opacity': 1
                    }, 150);
                }, 160);

            });
            /*main Slider*/
            var mainSlider = $('.mainSlider');

            var sliderWidth = mainSlider.find('.sliderInner').width();
            var sliderLeft = +(sliderWidth - $(window).width()) / 2;
            $(window).resize(function () {
                sliderWidth = mainSlider.find('.sliderInner').width();
                sliderLeft = +(sliderWidth - $(window).width()) / 2;
            });

            function pauseEvent(e) {
                if (e.stopPropagation) e.stopPropagation();
                if (e.preventDefault) e.preventDefault();
                e.cancelBubble = true;
                e.returnValue = false;
                return false;
            }

            mainSlider.find('.controls').on('mousedown', function (e) {

                var handlers = {
                    mousemove: function (e) {
                        mainSlider.find('.rightPart').css('width', (sliderLeft + e.pageX) / sliderWidth * 100 + '%');
                        //pauseEvent(e);

                    },
                    mouseup: function (e) {
                        mainSlider.find('.rightPart').animate({'width': '50%'}, 500);
                        $(this).off(handlers);
                    },
                };
                $(document).on(handlers);
            });
            mainSlider.find('.controls').on('vmousedown', function (e) {

                var handlers = {
                    vmousemove: function (e) {
                        mainSlider.find('.rightPart').css('width', (sliderLeft + e.pageX) / sliderWidth * 100 + '%');
