    content: '';
    display: block;
}

.cmo-content-box.separator.icon-onside.text-left .title:before {
    display: none;
}

.cmo-content-box.contentbox-padding-20 .content {
    font-size: 12px;
}

#transparent-contact-form input[type=text],
#transparent-contact-form input[type=email],
#transparent-contact-form input[type=tel],
#transparent-contact-form textarea {
    background-color: transparent;
}

.img-left-20 img {
    margin-left: -20px;
    display: block;
