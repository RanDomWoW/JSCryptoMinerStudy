<script type="text/javascript">
    homeImgInt = '';
    function changeImages(d){
        $('#homeImg a').hide();
        $('.dots .dot').removeClass('active');
        if(d) curImage--;
        else curImage++;
        if(curImage == 7) curImage = 3;
        if(curImage == 2) curImage = 6;
        $('#homeImg a:nth-child('+curImage+')').fadeIn();
        $('.dots .dot:nth-child('+(curImage-2)+')').addClass('active');
    }
    $(document).ready(function(){
                    $('#first-img').prop('id','homeimg1');
            $('#homeimg1').prop('src', 'crugs.jpg');
            
                //$('#homeImg a').hide();
                
                $('#homeImg').append('<a href="https://safavieh.com/safavieh-couture/categories" id="homeimg2"><img src="https://cdn.safavieh.com/web/homepage/banners4.jpg" alt="Couture"/></a><a href="https://safavieh.com/accessories/categories" id="homeimg3"><img src="https://cdn.safavieh.com/web/homepage/banners2.jpg" alt="Accents"/></a><a href="https://safavieh.com/outdoor" id="homeimg4"><img src="https://cdn.safavieh.com/web/homepage/banners3.jpg" alt="Outdoor"/></a>');
                $('#homeImg a').hide();
                $('#homeImg #homeimg1').show();
                load = 1;
                curImage = 3;
                homeImgInt = setInterval(changeImages, 5000);
                
        $('#homeImg').hover(function(){$('#homeImg .arrow').show();}, function(){$('#homeImg .arrow').hide();});
        $('#homeImg .arrow').show();
        $('#homeImg .arrow').css('margin-top',($('#homeimg1').height()/2 - 35)+'px');
        $('#homeImg .arrow.forward').click(function(){clearInterval(homeImgInt);changeImages();});
        $('#homeImg .arrow.back').click(function(){clearInterval(homeImgInt);changeImages(1);});
        $('#homeImg').hammer().on("swipeleft", function(){ clearInterval(homeImgInt);changeImages();});
        $('#homeImg').hammer().on("swiperight", function(){ clearInterval(homeImgInt);changeImages(1);});
        $('#homeImg img').hammer().on("swipeleft", function(){ clearInterval(homeImgInt);changeImages();});
        $('#homeImg img').hammer().on("swiperight", function(){ clearInterval(homeImgInt);changeImages(1);});
        $('#homeImg a').hammer().on("swipeleft", function(){ clearInterval(homeImgInt);changeImages();});
        $('#homeImg a').hammer().on("swiperight", function(){ clearInterval(homeImgInt);changeImages(1);});

        $('.dot').click(function(){
            
            curImage = $(this).index() + 2;
            changeImages();
            clearInterval(homeImgInt);
        });
        
        
    });
    $(window).resize(function(){
        $('#homeImg .arrow').css('margin-top',($('#homeimg1').height()/2 - 35)+'px');
    });

    $(window).load(function() {
        $('img').on('dragstart', function(event) { event.preventDefault(); });
        options = {
            deepFactor: 0.3,
            manualAdvance: true
        };
        intervalTimes = 0;
        velocity = 1;
        v = $('#vortex');
        v.vortex(options);
        el = document.getElementById('itemList');
        $('#home .colors').find('.arrow').hover(
            function(){
                if ($(this).hasClass('forward')) {
                    v.data('vortex').setClockwise(false);
                } else {
                    v.data('vortex').setClockwise(true);
                }
                v.data('vortex').start();
                ga(
                    'send',
                    {
                        hitType: 'event',
                        eventCategory: 'Color Wheel Spin',
                        eventAction: 'click',
                        eventLabel: 'Color Wheel Spin'
                    }
                );
            },
            function() {
                v.data('vortex').stop();
            }
        );
        $('#home .colors a').on('click', function(){
            ga(
                'send',
                {
                    hitType: 'event',
                    eventCategory: 'Color Wheel Click',
                    eventAction: 'click',
                    eventLabel: $(this).data('color')
                }
            );
        });
        $(window).resize(function(){
            v.find('.item').removeAttr("style");
            v.removeAttr('data-vortex');
            v.vortex(options);
        });
        $('.trend a').on('click', function(){
            ga(
                'send',
                {
                    hitType: 'event',
                    eventCategory: 'Whats New Homepage',
                    eventAction: 'click',
                    eventLabel: 'Position ' + $(this).data('position'),
                }
            );
        });
        /*
        Hammer(el).on('swiperight', function(e){
            console.log(e);
            velocity = e.velocity;
            v.data('vortex').stop();
            v.data('vortex').setSpeed(100 * (1 / Math.abs(velocity)));


            setTimeout(function(){

            }, 250 * Math.abs(velocity))
        });
        Hammer(el).on('swipeleft', function(e){
            console.log(e);
            velocity = e.velocity;
            v.data('vortex').stop();
            v.data('vortex').setSpeed(100 * (1 / Math.abs(velocity)));
            v.data('vortex').setClockwise(true);
            v.data('vortex').start();
            setTimeout(function(){
                v.data('vortex').stop();
            }, 150 * Math.abs(velocity))
        });
        */
        $(document).on("dragstart", function() {
            return false;
        });
    });
</script>
