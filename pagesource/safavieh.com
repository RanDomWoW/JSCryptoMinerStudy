<script type="text/javascript">
ww = viewport().width;
$(document).ready(function() {


    $(".toggleMenu").unbind('click').bind('click',function(e) {
        e.preventDefault();
        $(".nav").toggle();
    });
    $(".nav li a").each(function() {
        if ($(this).next().length > 0) {
            $(this).addClass("parent");
        };
    })
    $(".toggleMenu").css("display", "none");

    
    $('#itemList .item .img a').each(function(){
        if($(this).children('img').height() > $(this).height()) {
            $(this).children('img').height($(this).height());
        }
    });

    $(".nav").hide();
    adjustMenu();
    adjustArrows();
    setTimeout(adjustArrows, 600);
    $('header div.searchBtn').on('click', function(){$('.searchHead').slideToggle();});
    $('img').prop('draggable',false);
    if (ww <= 1024 && ww > 660) {
        $('.nav li').each(function(){
           if ($(this).find('ul').length) {
               if ($(this).hasClass('more')) {
                   el = $(this);
                   $(this).children('a').on('click', function() {
                       $(this).parent().children('ul').css('display', 'none');
                       return false;
                   });
               } else {
                   $(this).children('a').on('click', function () {
                       return false
                   });
               }
           } 
        });

    }
    $('header').find('input[name=s]').on('focus', function(){
        ww = viewport().width;
        if (ww > 1200) {
            $(this).animate({'width': 300}, 300);
        }
    });
    $('input[name=s]').on('blur', function(){
        ww = viewport().width;
        if (ww > 660) {
            $(this).animate({'width': 140}, 300);
        }
    });
    $('form#newsletter-form').on('submit', function(){
        newsletterSubmit($(this));
        return false;
    });
    function newsletterSubmit(el) {
        args = {
            'email': el.find('input[name=email]').val(),
            'action': 'newsletter'
        };
        $.ajax({
            url: "/wp-admin/admin-ajax.php",
            type: "POST",
            data: args,
            cache: false,
            success: function (results) {
                eval(results);
            }
        }).fail(function (e) {
            if (debug) {
                alert("Sorry can't connect to server.");
            }
        });
    }

    cookies = {};

    function createCookie(name,value,days) {
        var expires = "";
        if (days) {
            var date = new Date();
            date.setTime(date.getTime() + (days*24*60*60*1000));
            expires = "; expires=" + date.toUTCString();
        }
        document.cookie = name + "=" + value + expires + "; path=/";
    }

    function readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for(var i=0;i < ca.length;i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1,c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
        }
        return null;
    }

    newsletterCookie = readCookie('newsletter');

    if (!newsletterCookie && ww < 660) {
        setTimeout(newsletterPopup, 5000);
    }

    function newsletterPopup() {
        createCookie('newsletter', 1, 20);

    }

});
function adjustArrows(){
    if($('#mainimage').length){
        h = $('#mainimage').height();
        ah = $('.arrow').height();
        margin = (h-ah)/2;
        if(margin > 0) $('.arrow').css('margin-top',margin+'px');
        else $('.arrow').css('margin-top',0);
    }
}
$(window).resize(adjustMenu);
function adjustMenu() {
    firstPage = 0;
    ww = viewport().width;
    if (ww <= 660) {
        $(".nav li").unbind('mouseenter mouseleave');
        $('.nav li').unbind('hover');
        $(".toggleMenu").css("display", "inline-block");
        //$('.nav').hide();

        $(".nav li a.parent").unbind('click').bind('click',function(e) {
            e.preventDefault();
            $(this).parent("li").toggleClass('hover');
        });
        $('#header-more').hide();
    } else {
        $(".toggleMenu").css("display", "none");
        $(".nav").show();
        $(".nav li").hover(function() {
            if ($(this).hasClass('more') && ww <= 1024 && ww > 660) {
                $('.nav').find('.more').find('.navdrop').hide();
            } else {
                $('.nav').find('.more').find('.navdrop').show();
            }
            $(this).addClass('hover');
        }, function() {
            $(this).removeClass('hover');
        });
        $(".nav li a.parent").unbind('click');
        $('.nav .more a.parent').unbind('hover').hover(function(){},function(){});
        $(".more a").unbind('click').click(function(){
            $('#header-more').slideToggle();
        });
        $('#related').hammer().on('swipeleft', function(){moverelated(-1)});
        $('#related').hammer().on('swipeleft', function(){moverelated(1)});
        $('#relatedPages').hammer().on('swipeleft', function(){moverelatedPages(-1)});
        $('#relatedPages').hammer().on('swipeleft', function(){moverelatedPages(1)});
    }
}
function moverelated(d){
    console.log(d);
    if(d === 1){
        $("#related .cont").append($("#related .cont .item:first").html());
        $("#related .cont .item:first").remove();
        $("#related .cont > a").wrap('<div class="item"></div>')
    } else if(d === -1) {
        $("#related .cont").prepend($("#related .cont .item:last").html());
        $("#related .cont .item:last").remove();
        $("#related .cont > a").wrap('<div class="item"></div>')
    }
}
function moverelatedPages(d){
    console.log(d);
    if(d == 1){
        $("#relatedPages ul").append($("#relatedPages ul li:first-child").html());
        $("#relatedPages ul li:first-child").remove();
        $('#relatedPages ul > a').wrap('<li></li>');
    } else if(d== -1) {
        $("#relatedPages ul").prepend($("#relatedPages ul li:last-child").html());
        $("#relatedPages ul li:last-child").remove();
        $('#relatedPages ul > a').wrap('<li></li>');
    }
}
</script>
