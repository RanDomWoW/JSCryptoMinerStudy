	<script>
        $(document).ready(function () {
            var horoscopevalue = Cookies.get("horoscopeselection");
            if (horoscopevalue != 'undefined' && !isNaN(horoscopevalue)) {
                $("#horoscope").val(horoscopevalue);
                getHoroscope(horoscopevalue);
            }
            else {
                document.getElementById("horoscope").options.selectedIndex = 0;
                getHoroscope(0);
            }
            resizeIframeMessageEvent('http://bylgjan.visir.is', 'rs-konnun');
        });

        $("#horoscope").on('change', function () {
            getHoroscope(this.value);
            Cookies.set("horoscopeselection", this.value);
        })

        function getHoroscope(horoscopevalue) {
            var dateObj = new Date();
            var year = dateObj.getUTCFullYear();
            var month = dateObj.getUTCMonth() + 1; //months from 1-12
            var day = dateObj.getUTCDate();
            var num = ((year * 9) + (month * 7) + (day * 5) + (horoscopevalue * 11)) % 2766;

            $.ajax({
                url: "/static/misc/horoscope/" + num + ".json",
                type: "Get",
                dataType: 'json',
                success: function (data) {
                    $("#horoscopetexti").html(data.description);
                },
                error: function (data) {
                    //alert("error");
                }
            }).done(function () {
                // Done
            });
        }
	</script>


