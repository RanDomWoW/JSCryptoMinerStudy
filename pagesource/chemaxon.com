            <script>
    (function() {
    'use strict';
    var player;
    var videoContainer = getByClass('video-container')[0];
    var playButton = getByClass('video-container__play')[0];
    var stopButton = getByClass('video-container__close')[0];
    var playerNode = document.getElementById('player');

    if (!playerNode) {
        return;
    }

    var video = getVideos();
    var list = getPlaylist();
    if (window.postMessage != undefined) {
        var tag = document.createElement('script');
        tag.src = 'https://www.youtube.com/iframe_api';
        var firstScriptTag = document.getElementsByTagName('script')[0];
        firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);


        window.onYouTubeIframeAPIReady = function () {
            player = new YT.Player('player', {
                height: '100%',
                width: '100%',
                playerVars: {
                    'theme': 'light'
                },
                events: {
                    'onReady': onPlayerReady,
                    'onStateChange': onStateChanges
                }
            });
        }
    }

    function onPlayerReady(event){
        if (list) {
            player.cuePlaylist({ list: list });
            return;
        }
        if (video) {
            (video.length === 1)
                ? player.cueVideoById(video[0])
                : player.cuePlaylist(video);
            return;
        }
    }

    function onStateChanges(event) {
        // its not working...
        // if (event.data === YT.PlayerState.CUED) {
        //     console.log('videoCued');
        // }
    }

    function getByClass(name) {
        return document.getElementsByClassName(name);
    }

    function getVideos() {
        var id = playerNode.getAttribute('data-video');
        if (!id) return false;
        return id.split(',');
    }

    function getPlaylist() {
        var id = playerNode.getAttribute('data-list');
        return  id ? id : false;
    }

    function handlePlayEvent() {
        videoContainer.classList.add('active');
        player.playVideo();
    };

    function handleStopEvent() {
        videoContainer.classList.remove('active');
        player.stopVideo();
    };

    playButton.addEventListener('click', handlePlayEvent);
    stopButton.addEventListener('click', handleStopEvent);

    }());
</script>
