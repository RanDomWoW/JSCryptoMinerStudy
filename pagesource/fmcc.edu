					.attr( 'data-oldWidth', el.attr( 'width' ) );
			} );

			$(window)
				.resize( function() {
					all_videos.each( function() {
						var el = $(this),
							newWidth = el.parents().width(),
							oldWidth = el.attr( 'data-oldWidth' );

						if ( oldWidth > newWidth ) {
							el
								.removeAttr( 'height' )
								.removeAttr( 'width' )
								.width( newWidth )
								.height( newWidth * el.attr( 'data-aspectRatio' ) );
						}
					} );
				} )
				.resize();
		})(jQuery);
	});
