<script async="async" defer="defer" src="https://www.youvisit.com/tour/Embed/js2"></script>
<script type="text/javascript">
	document.addEventListener('DOMContentLoaded', function(e) {
		var images_done = 0
		var images = 0;
		var gcse_done = 0;
		var gcse_count = 0;
		var upload_done = 0;
		var upload_count = 0;

		setTimeout(function() {
			[].forEach.call(document.querySelectorAll('img'), function(el) {
				if(el.getAttribute('alt') === null) {
					images = images + 1;
				}
			});

			[].forEach.call(document.querySelectorAll('input.gsc-search-button'), function(el) {
				if(el.getAttribute('alt') === null) {
					gcse_count = gcse_count + 1;
				}
			});

			[].forEach.call(document.querySelectorAll('input[type=file]'), function(el) {
				if(el.previousSibling === null || el.previousSibling.tagName.toLowerCase() !== 'label') {
					upload_count = upload_count + 1;
				}
			});
		}, 500);

		var image_check = setInterval(function() {
			if(images_done !== images) {
				[].forEach.call(document.querySelectorAll('img'), function(el) {
					/*if(el.getAttribute('src') === 'https://trck.youvisit.com/embed/61399.png?embed_type=custom') {
						el.setAttribute('alt', 'You Visit Pixel');
						el.setAttribute('aria-hidden', 'true');
						images_done = images_done + 1;
					} else*/ if(el.classList.contains('gsc-branding-img')) {
						el.setAttribute('alt', 'Google Branding Image');
						el.setAttribute('aria-hidden', 'true');
						images_done = images_done + 1;
					} else if(el.getAttribute('title') === 'Watch our video.') {
						el.setAttribute('alt', 'Watch our video');
						images_done = images_done + 1;
					} else if(el.getAttribute('src') === '//files.hellonetcdn.com/st/skins/e14frames/play_over.png') {
						el.setAttribute('alt', 'Play');
						el.setAttribute('aria-hidden', 'true');
						images_done = images_done + 1;
					} else if(el.getAttribute('src') === '//files.hellonetcdn.com/st/skins/e14frames/watch_corner75.png') {
						el.setAttribute('alt', 'Watch');
						el.setAttribute('aria-hidden', 'true');
						images_done = images_done + 1;
					} else {
						if(el.getAttribute('alt') === null) {
							el.setAttribute('alt', document.title);
							el.setAttribute('aria-hidden', 'true');
							images_done = images_done + 1;
						}
					}
				});

				//document.querySelector('.virtualtour_embed').innerHTML = '<span class="screen-reader-text">Virtual Tour</span>';
			} else {
				clearInterval(image_check);
				return true;
			}
		}, 1000);

		var gsce_check = setInterval(function() {
			if(gcse_done !== gcse_count) {
				[].forEach.call(document.querySelectorAll('input.gsc-search-button'), function(el) {
					if(el.getAttribute('alt') === null) {
						el.setAttribute('alt', 'Search the site');
						var gsce_label = document.createElement('label');
						el.parentNode.insertBefore(gsce_label, el);
						gsce_label.appendChild(el);

						var gsce_span = gsce_label.appendChild(document.createElement('span'));
						gsce_span.classList.add('screen-reader-text');
						gsce_span.innerText = 'Submit Search';

						gcse_done = gcse_done + 1;
					}
				});
			} else {
				clearInterval(gsce_check);
				return true;
			}
		}, 1000);

		var upload_check = setInterval(function() {
			if(upload_done !== upload_count) {
				[].forEach.call(document.querySelectorAll('input[type=file]'), function(el) {
					if(el.previousSibling === null || el.previousSibling.tagName.toLowerCase() !== 'label') {
						var file_label = el.parentNode.appendChild(document.createElement('label'));
						file_label.setAttribute('for', el.getAttribute('id'));
						file_label.innerHTML = '<span class="screen-reader-text">File Upload</span>';

						var parent_el = el.parentNode;
						parent_el.insertBefore(file_label, el);

						upload_done = upload_done + 1;
					}
				});
			} else {
				clearInterval(upload_check);
				return true;
			}
		}, 1000);
	});
</script>
