    function drildownPosition() {
        var headerBox = $j('#header-container'),
            headerBoxHeight = headerBox.height(),
            menuButton = $j('#nav > li');

        if( window.innerWidth < 768  && headerBox.height() > 90) {
            menuButton.on('click', function() {
                var menuPos = $j(this).offset().top;
                $j(this).find('.drilldown').css('top', menuPos + 13 +'px');
            }); 
        }
    }

    drildownPosition();
    
    $j(window).resize(function() {
        drildownPosition();
    });
