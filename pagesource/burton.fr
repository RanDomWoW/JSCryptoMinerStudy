<script type="text/javascript">
//<![CDATA[
    var newsletterSubscriberFormDetail = new VarienForm('newsletter-validate-detail');

    var subscribeSubmitted = false;
    var detailsFormModalBox = false;

    $('newsletter-validate-detail').down('button').observe(
        'click',
        function(event) {
            var formValidator  = new Validation($('newsletter-validate-detail'));
            if(formValidator && formValidator.validate()){
                if (subscribeSubmitted) {
                    Event.stop(event);
                    return;
                }
                subscribeSubmitted = true;
                var url = $('newsletter-validate-detail').readAttribute('action');
                $('newsletter-loading').show();
                new Ajax.Request(
                    url,
                    {
                        method: 'post',
                        parameters: Form.serialize($('newsletter-validate-detail')),
                        onSuccess: function(transport){
                            $('newsletter-loading').hide();
                            subscribeSubmitted = false;
                            var response = transport.responseText || "";
                            if (response != "") {
                                response = response.evalJSON();
                                if(response.success) {
                                    if(detailsFormModalBox) {
                                        //detailsFormModalBox.destroy();
                                        detailsFormModalBox.close();
                                        detailsFormModalBox = false;
                                    }
                                    if (response.showDetailsForm) {
                                        detailsFormModalBox = Baobaz.Modal.open('http://www.burton.fr/newsletter/subscriberDetails/showDetailsForm/', {parameters: {'message': response.success, 'email': response.email}});
                                    } else {
                                        detailsFormModalBox = Baobaz.Modal.open('http://www.burton.fr/newsletter/subscriberDetails/showSuccessMessage/', {parameters: {'message': response.success, 'email': response.email}});
                                    }
                                }
                                else if(response.error) {
                                    errorMessageDiv = $('error-validate-email-newsletter');
                                    errorMessageDiv.innerHTML = response.error;
                                    errorMessageDiv.show();
                                }
                            }
                        },
                        onFailure: function(){
                            $('newsletter-loading').hide();
                            subscribeSubmitted = false;
                        }
                    }
                );
                Event.stop(event);
            }
            else {
                subscribeSubmitted = false;
                Event.stop(event);
            }
        }
    );
//]]>
</script>

