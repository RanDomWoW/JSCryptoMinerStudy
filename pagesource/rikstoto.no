            function spawnDocumentWindow(url) {
                try {
                    if (self !== top) {
                        if (typeof parent.spawnWrappedPage !== 'undefined') {
                            parent.spawnWrappedPage(url);
                            return;
                        }
                    }
                } catch (ex) {
                    // do nothing
                }

                var features = {
                    width: 722,
                    height: 700,
                    scrollbars: 1
