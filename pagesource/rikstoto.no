<script type="text/javascript">    //<![CDATA[

    function registerWebtrendsLightboxShow(name) {
        
        try {
            if (name === undefined || name === null || name === '') {
                return; //do not log web trends visit if we don't have the name
            }
            else {
                name = 'lb_' + name;
            }
            var pageUrl = getUriStem(encodeURI(window.location.href));
            dcsMultiTrack('DCS.dcsqry', '', 'DCS.dcsuri', pageUrl, 'WT.ti', name);
        } catch (exception) {
            //ignore, it's only web trends after all
        }
        
    }

    function registerWebtrendsEvent(name, removeDefaults, urlOverride) {
        
        try {
            if (name === undefined || name === null || name === '') {
                return;
            }
            else {
                name = 'event_' + name;
            }
            var pageUrl = '';
            if (urlOverride === undefined || urlOverride === null || urlOverride === '') {
                pageUrl = getUriStem(encodeURI(window.location.href));
            } else {
                if (urlOverride.contains('?') || urlOverride.contains('http:') || urlOverride.contains('https:')) {
                    urlOverride = getUriStem(urlOverride);
                }
                pageUrl = urlOverride;
            }

            if (removeDefaults) {
                dcsMultiTrack('DCS.dcsqry', '', 'DCS.dcsuri', pageUrl, 'WT.ti', name);
            } else {
                dcsMultiTrack('DCS.dcsuri', pageUrl, 'WT.ti', name);
            }

        } catch (exception) {
            //ignore, it's only web trends after all
        }
        
    }

    function registerWebtrendsRaw(tagName, tagValue, removeDefaults, urlOverride) {
        
        try {
            var pageUrl = '';
            if (urlOverride === undefined || urlOverride === null || urlOverride === '') {
                pageUrl = getUriStem(encodeURI(window.location.href));
            } else {
                if (urlOverride.contains('?') || urlOverride.contains('http:') || urlOverride.contains('https:')) {
                    urlOverride = getUriStem(urlOverride);
                }
                pageUrl = urlOverride;
            }
            if (removeDefaults) {
                dcsMultiTrack('DCS.dcsqry', '', 'DCS.dcsuri', pageUrl, tagName, tagValue);
            } else {
                dcsMultiTrack('DCS.dcsuri', pageUrl, tagName, tagValue);
            }
        } catch (exception) {
            //ignore, it's only web trends after all
        }
        
    }
    function getUriStem(uri) {
        uri = uri.replace('//', '');
        uri = uri.substring(uri.indexOf('/'));
        if (uri.contains('?')) {
            uri = uri.substring(0, uri.indexOf('?'));
        }
        return uri;
    }

    //]]>
</script>

