                docWindow.focus();
            }
            
            function spawnPopupWindow(url, betdata, features, name){
                var path = url;
                if(betdata != null){
                    path = path +'?betdata='+betdata;
                }
                if(features == null || features==undefined){
                    features = {
                        width: 900,
                        height:  800,
                        scrollbars: 1
                    };
                }
                if(isIE() && features.scrollbars){
                    features.width = features.width+20;
                }
                gamewindow = spawn(path, "eToto3_spillvindu", features); 
            }
        
            function spawnGameWindow(betdata) {
                var path = '/App/Game/Game.aspx';
                spawnPopupWindow(path, betdata);
