            // onTranslated: onTranslatedCarousel,
            // onChange: onChangeCarousel,
            items: number_of_slide,
            autoWidth: false,
            nav: true,
            dots: true,
            navText: ['<div class="prev_button_slide"></div>','<div class="next_button_slide"></div>']
        }

        $owl_testimonials = jQuery(".list_testimonials").owlCarousel(
            owlCarouselConfig
        );

        //$owl.trigger('refresh.owl.carousel');
    }

    jQuery(() => {
        var number_of_slide = 3;

        var width = jQuery( window ).width();

        if(width >= 1400){
            number_of_slide = 3;
        }
        if(width >= 960){
            number_of_slide = 3;
        }
        else if(width >= 640){
            number_of_slide = 2;
        }
        else{
            number_of_slide = 1;
        }

        jQuery(window).resize(() => {
            width = jQuery( window ).width();
            //console.log(width);
            if(width >= 1400){
                number_of_slide = 3;
            }
            if(width >= 960){
                number_of_slide = 3;
            }
            else if(width >= 640){
                number_of_slide = 2;
            }
            else{
                number_of_slide = 1;
            }
            slide_list_testimonials(number_of_slide);
        });

        slide_list_testimonials(number_of_slide);
    })
</script>
