                // override the thumbs_rating_vote in plugins/thumbs-rating/js/general.js
                function thumbs_rating_vote(ID, type) {
                    // For the LocalStorage 
                    var itemName = "thumbsrating" + ID;
                    var container = "#thumbs-rating-" + ID;
                    // Check if the LocalStorage value exist. If do nothing.
                    if (!localStorage.getItem(itemName)) {
                        // Set HTML5 LocalStorage so the user can not vote again unless he clears it.
                        localStorage.setItem(itemName, true);

                        // Set the localStorage type as well
                        var typeItemName = "thumbsrating" + ID + "-" + type;
                        localStorage.setItem(typeItemName, true);

                        // Data for the Ajax Request
                        var data = {
                            action: "thumbs_rating_add_vote",
                            postid: ID,
                            type: type,
                            nonce: thumbs_rating_ajax.nonce
                        };

                        jQuery.post(thumbs_rating_ajax.ajax_url, data, function(response) {
                            var voting_up_class = " .thumbs-rating-up";
                            var voting_down_class = " .thumbs-rating-down";

                            // remove old elements
                            jQuery(container + voting_up_class).remove();
                            jQuery(container + voting_down_class).remove();
                            jQuery(container + " .thumbs-rating-already-voted").remove();

                            // add new elements
                            jQuery(container).prepend(jQuery(response).html());

                            var thumbs_rating_class = type === 1 ? voting_up_class : voting_down_class;
                            // Add the class to the clicked element
                            jQuery(container + thumbs_rating_class).addClass("thumbs-rating-voted");
                        });
                    } else {
                        // Display message if we detect LocalStorage
                        var $voted = jQuery("#thumbs-rating-" + ID + " .thumbs-rating-already-voted");
                        $voted.fadeIn().css({"display": "inline-block", "padding-top": "0"});
                        window.setTimeout(function() {
                            $voted.hide();
                        }, 3000);
                    }
                }
