        var owlCarouselConfig = {
            autoplay: false,
            autoplayHoverPause: true,
            // autoplayTimeout: 1000,
            loop: false,
            animateIn: 'fadeInRight', // using animation from https://daneden.github.io/animate.css/
            // fallbackEasing: 3000,
            // nav: true,
