      <script type="text/javascript">
      function getOuterWidth(el)
      {
        width = parseInt($(el).outerWidth()) + parseInt($(el).css('margin-left').replace('px','')) + parseInt($(el).css('margin-right').replace('px',''));
        return width;
      }

      function removeLastChild()
      {
        if ($("#tickerwrapper").length != 0)
        {
          $("#tickerwrapper").css('left', ((($("#tickerwrapper").position().left) - 1) + "px"));

          if((getOuterWidth("#tickerwrapper span:nth-child(1)") + $("#tickerwrapper").position().left + $("#tickerwrapper span:nth-child(1)").position().left) < 0)
          {
            $("#tickerwrapper").append($("#tickerwrapper span:nth-child(1)").outerHTML('span'));
            $("#tickerwrapper").css('left', ($("#tickerwrapper").position().left + getOuterWidth("#tickerwrapper span:nth-child(1)") + "px"));
            $("#tickerwrapper span:nth-child(1)").hide().remove();
          }
          setTimeout("removeLastChild()", 30);
        }
      }

      $(document).ready(function() {
        // function to get outer html from element
        jQuery.fn.outerHTML = function(element_style) {
          return jQuery('<'+element_style+' />').append(this.eq(0).clone()).html();
        };

        try
        {
          addlimit=0;
          tmpchildrenwidth = 0;
          $(".ticker").children().each(function() {
            tmpchildrenwidth += getOuterWidth(this);
          });

                    while(tmpchildrenwidth < ($("#links_ticker").width() + getOuterWidth("#links_ticker span:nth-child(1)")) && addlimit < 100)
          {
            addlimit++;
            $(".ticker").html($(".ticker").html() + "<span>&nbsp;&nbsp;&nbsp;&nbsp;</span>");
            tmpchildrenwidth = 0;
            $(".ticker").children().each(function() {
              tmpchildrenwidth += getOuterWidth(this);
            });
          }
              tmpchildrenwidth = 0;
      $(".ticker").children().each(function() {
        tmpchildrenwidth += getOuterWidth(this);
      });
      $(".ticker").html("<div id=\"tickerwrapper\">" + $(".ticker").html() + "</div>");
      $(".ticker div").css("width", ((tmpchildrenwidth * 1.5) + "px"));
    }
    catch (o) {}

    removeLastChild();
  });

