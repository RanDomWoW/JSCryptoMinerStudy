<script  type="text/javascript">
$(document).ready(function() {
  if ( window.location.href.indexOf('blur') > 0 ) {
    $(".notice").addClass('hidden'); // needed for a good screengrab
    html2canvas(document.body, {
      onrendered: function(canvas) {
        $(".notice").removeClass('hidden'); // needed for a good screengrab
        canvas.id = 'notice_background_canvas';
        var el = document.body.appendChild(canvas);
        stackBlurCanvasRGB( el.id, 0, 0, el.height, el.width, 30 );
        $('.notice').css({
          'background-image':"url(" + canvas.toDataURL("image/png")+ ")"
        });
        setNotice();
      },
      width: document.body.clientHeight,
      height: document.body.clientHeight
    });
  }
  
  $(window).scroll(function () {
    setNotice();
  });
  
  $('.close').click(function(e) {
    e.preventDefault();
    $('.notice').fadeOut(1000);
  });
  
  var setNotice = function() {
    var scroll = $(document).scrollTop();
    var wHeight =  $(window).height();
    var nHeight = $(".notice").height();
    if ( $(".notice").hasClass('annoying') ) {
      var noticeTop = scroll < 40 ? 40-scroll : 0;
      var bgYPpos = -scroll - noticeTop;
      $(".notice.annoying").css({ top: noticeTop });
    } else if ( $(".notice").hasClass('friendly') ) {
      var bgYPpos = -wHeight + nHeight -scroll;
    }
    $(".notice").css({
      'background-position': '0px ' + bgYPpos + 'px'
    });
    $(".notice.friendly").css({top: 'auto'});
  };
  
  console.log(window.location.href);

});
</script>
