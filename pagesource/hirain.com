function UserLogin(){
	var name = $('#user-login').find('input[name=name]').val();
	var pass = $('#user-login').find('input[name=pass]').val();
	var warning = '';
	if($.trim(name).length ==0){
		warning += '用户名不为空';
	}
	if($.trim(pass).length ==0){
		warning += ' 密码不为空';
	}
	if(warning != ''){
		myDialog({type:0,text:warning,closeBtn:true});return;
		return false;
	}
	var url = '/cmsuser/user/0/login';
	$.post(url, {name:name, pass:pass}, function(ret){
		if(ret && ret.uid > 0){
			window.location.reload(1);
		}else{
			var errors = [];
			for(var j in ret){
				errors.push(ret[j]);
			}
			myDialog({type:0,text:errors[0],closeBtn:true});
		}
	}, 'json');
	return false;
}
