	jQuery(window).scroll(function(){
		if( jQuery(window).scrollTop() > offset ) {
			elem.css({position: 'fixed', top: 0, 'z-index': 99});
		} else {
			elem.css({position: 'static', top: 0});
		}
	});
}
// END

