				<script type="text/javascript">
function tvleft(carousel, dur) {
  dur ? left_tvcarousel(carousel, dur) : left_tvcarousel(carousel, 400);
  return false;
}
function tvright(carousel, dur) {
  dur ? right_tvcarousel(carousel, dur) : right_tvcarousel(carousel, 400);
  return false;
}
function left_tvcarousel(carousel, dur) {
  let block_width = $(carousel).find('.tvcarousel-block').outerWidth();
  $(carousel).find('.tvcarousel-items .tvcarousel-block').eq(-1).clone().prependTo($(carousel).find('.tvcarousel-items'));
  $(carousel).find('.tvcarousel-items').css({'left' : '-'+block_width+'px'});
  $(carousel).find('.tvcarousel-items .tvcarousel-block').eq(-1).remove();
  $(carousel).find('.tvcarousel-items').animate({left: '0px'}, dur);
  return false;
}
function right_tvcarousel(carousel, dur) {
  let block_width = $(carousel).find('.tvcarousel-block').outerWidth();
  $(carousel).find('.tvcarousel-items').animate({left: '-'+block_width+'px'}, dur, function() {
    $(carousel).find('.tvcarousel-items .tvcarousel-block').eq(0).clone().appendTo($(carousel).find('.tvcarousel-items'));
    $(carousel).find('.tvcarousel-items .tvcarousel-block').eq(0).remove();
    $(carousel).find('.tvcarousel-items').css({'left' : '0px'});
  });
  return false;
}

