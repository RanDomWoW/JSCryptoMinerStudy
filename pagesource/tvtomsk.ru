<script type="text/javascript">

  $('#lenta_list').on('mousedown', function() {
    $(this).addClass('grabbing');
  });
  $('#lenta_list').on('mouseup', function() {
    $(this).removeClass('grabbing');
  });
  $('#lenta_list').on('mouseleave', function() {
    $(this).removeClass('grabbing');
  });

  var myScroll;
  var loaded = 1;
  $(document).ready(function() {
    myScroll = new IScroll('#lenta_container', {
      //infiniteElements: '#lenta_list .lenta_item',
      //dataset: requestData,
      //dataFiller: updateContent
      //momentum: false
    });
    //myScroll.maxScrollY = $('#lenta_list').height() - $('#lenta_container').height();
    myScroll.on('scrollStart', function() {
      $('.item_title a').on('click', function() {return false;});
      $('.item_img a').on('click', function() {return false;});
      $('#blackplus').removeClass('minus');
			$('.mobfooter').animate({height: '0px'}, 200);
    });
    myScroll.on('scrollEnd', function() {
      setTimeout(function() {
        $('.item_title a').unbind('click');
        $('.item_img a').unbind('click');
      }, 100);
      scrollending("lenta", $('.lenta_item').last().data('d'), 15, 0)
    });

    function scrollending(action, d, len, beg) {
      setTimeout(function() {$('.item_title a').on('click', function(e) {return true;})}, 10);
      (myScroll.y < 0) ? $('#arrow_up').fadeIn(300) : $('#arrow_up').fadeOut(300);
      console.log('Ended');
      console.log(myScroll.maxScrollY, myScroll.y);
      if(myScroll.maxScrollY >= myScroll.y || beg == 1) {
        ms = myScroll.maxScrollY;
        console.log("===");
        if(loaded == 1) {
          loaded = 0;
          if(action == "lenta") {
            $.get('/engine/lenta_load.php', {action: 'lenta', idl:d, len:len, d:d}, function(data) {
              $('#lenta_list').append(data);
              if (myScroll) {
                myScroll.destroy();
                myScroll = null;
                console.log('Destroy');
              }
              myScroll = new IScroll('#lenta_container');
              if(beg == 1) myScroll.scrollTo(0, 0, 0);
              else myScroll.scrollTo(0, ms-100, 500);
              console.log('date ',$('.lenta_item').last().data('d'));
              myScroll.on('scrollStart', function() {
                $('.item_title a').on('click', function() {return false;});
                $('.item_img a').on('click', function() {return false;});
                $('#blackplus').removeClass('minus');
          			$('.mobfooter').animate({height: '0px'}, 200);
              });
              myScroll.on('scrollEnd', function() {
                setTimeout(function() {
                  $('.item_title a').unbind('click');
                  $('.item_img a').unbind('click');
                }, 100);
                scrollending("lenta", $('.lenta_item').last().data('d'), 15, 0);
              });
              //myScroll.scrollTo(0, ms-100, 500);
              loaded = 1;
              //$.getScript("/engine/classes/highslide/highslide.js", function() {});
            });
          } else if(action == "emergency") {
            $.get('/engine/lenta_load.php', {action: 'emergency', idl:d, len:len, d:d}, function(data) {
              $('#lenta_list').append(data);
              if (myScroll) {
                myScroll.destroy();
                myScroll = null;
                console.log('Destroy');
              }
              myScroll = new IScroll('#lenta_container');
              if(beg == 1) myScroll.scrollTo(0, 0, 0);
              else myScroll.scrollTo(0, ms-100, 500);
              console.log('date ',$('.lenta_item').last().data('d'));
