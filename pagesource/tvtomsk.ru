                $('.item_title a').on('click', function() {return false;});
                $('.item_img a').on('click', function() {return false;});
                $('#blackplus').removeClass('minus');
          			$('.mobfooter').animate({height: '0px'}, 200);
              });
              myScroll.on('scrollEnd', function() {
                setTimeout(function() {
                  $('.item_title a').unbind('click');
                  $('.item_img a').unbind('click');
                }, 100);
                scrollending("emergency", $('.lenta_item').last().data('d'), 15, 0);
              });
              //myScroll.scrollTo(0, ms, 0);
              loaded = 1;
            });
          } else if(action == "topweek") {
            $.get('/engine/lenta_load.php', {action: 'topweek', idl:d, len:len, d:d}, function(data) {
              $('#lenta_list').append(data);
              if (myScroll) {
                myScroll.destroy();
                myScroll = null;
                console.log('Destroy');
              }
              myScroll = new IScroll('#lenta_container');
              if(beg == 1) myScroll.scrollTo(0, 0, 0);
              else myScroll.scrollTo(0, ms-100, 500);
              console.log('date ',$('.lenta_item').last().data('d'));
              myScroll.on('scrollStart', function() {
                $('.item_title a').on('click', function() {return false;});
                $('.item_img a').on('click', function() {return false;});
                $('#blackplus').removeClass('minus');
          			$('.mobfooter').animate({height: '0px'}, 200);
              });
              myScroll.on('scrollEnd', function() {
                setTimeout(function() {
                  $('.item_title a').unbind('click');
                  $('.item_img a').unbind('click');
                }, 100);
                scrollending("topweek", $('.lenta_item').last().data('d'), 15, 0);
              });
              //myScroll.scrollTo(0, ms, 0);
              loaded = 1;
            });
          }
        }
      }
    }


    function requestData(start, count) {
      $.get('/engine/lenta_load.php', {action:'lenta', idl:$('.lenta_item').last().data('d')}, function(data) {
        return data;
      });
    }
    function updateContent (el, data) {
     el.innerHTML = data;
    }

    $('.minus_plus').live('click', function() {
      if($(this).hasClass('minus')) {
        th = $(this).parent().parent().find('.item_img');
        th.slideUp({duration: 500, easing:'linear'}, function(){th.removeClass('show')});
        //$(this).parent().parent().find('.item_img').removeClass('show');
        $(this).removeClass('minus');
        if( myScroll.y == 0 ) {

        }
        //myScroll.scrollTo(0, myScroll.y + $(this).parent().parent().find('.item_img img').height() + 15, 500, IScroll.utils.ease.linear);
        myScroll.maxScrollY += $(this).parent().parent().find('.item_img img').height()+15;
      } else {
        th = $(this).parent().parent().find('.item_img');
        th.slideDown({duration: 500, easing:'linear'}, function(){th.addClass('show')});
        $(this).addClass('minus');
        if( myScroll.y == 0) {

        }
        //myScroll.scrollTo(0, myScroll.y - $(this).parent().parent().find('.item_img img').height() - 15, 500, IScroll.utils.ease.linear);
        myScroll.maxScrollY -= $(this).parent().parent().find('.item_img img').height()+15;
      }
      console.log("(-+)"+myScroll.maxScrollY, myScroll.y);
    });

    $('#arrow_down').on('click', function() {
      if(loaded == 1)
        (myScroll.y - myScroll.maxScrollY < 200) ? myScroll.scrollTo(0, myScroll.maxScrollY, 800, IScroll.utils.ease.linear) : myScroll.scrollTo(0, myScroll.y - 200, 800, IScroll.utils.ease.linear);
        //myScroll.scrollTo(0, myScroll.y - 200, 500, IScroll.utils.ease.linear);
    });
    $('#arrow_up').on('click', function() {
      (myScroll.y > -200) ? myScroll.scrollTo(0, 0, 500, IScroll.utils.ease.linear) : myScroll.scrollTo(0, myScroll.y + 200, 800, IScroll.utils.ease.linear);
    });

    //закладки
    $('#lenta1').on('click', function() {
      if( !$('#lenta').hasClass('active_lm') ) {
        myScroll.scrollTo(0, 0, 0); //$('#arrow_up').fadeOut();
        $('.lenta_menu a').removeClass('active_lm');
        $('.lenta_menu div').removeClass('active_tab');
        $('#lenta').addClass('active_lm');
        $('#lenta').parent().addClass('active_tab');
        $('#lenta_list').empty();
        //d ===== $('.lenta_item').last().data('d')
        scrollending("lenta", 0, 30, 1);
      }
      return false;
    });
    $('#emergency1').on('click', function() {
      if( !$('#emergency').hasClass('active_lm') ) {
        myScroll.scrollTo(0, 0, 0);//$('#arrow_up').fadeOut();
        $('.lenta_menu a').removeClass('active_lm');
