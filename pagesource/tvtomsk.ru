        $('#emergency').addClass('active_lm');
        $('#emergency').parent().addClass('active_tab');
        $('#lenta_list').empty();
        //d ===== $('.lenta_item').last().data('d')
        scrollending("emergency", 0, 30, 1);
      }
      return false;
    });
    $('#topweek1').on('click', function() {
      if( !$('#topweek').hasClass('active_lm') ) {
        myScroll.scrollTo(0, 0, 0);//$('#arrow_up').fadeOut();
        $('.lenta_menu a').removeClass('active_lm');
        $('.lenta_menu div').removeClass('active_tab');
        $('#topweek').addClass('active_lm');
        $('#topweek').parent().addClass('active_tab');
        $('#lenta_list').empty();
        //d ===== $('.lenta_item').last().data('d')
        scrollending("topweek", 0, 30, 1);
      }
      return false;
    });
    $('#lenta_list img').error(function() {
      $(this).attr('src', '/templates/newgeneration/images/NoImage.jpg');
    });
    $('.main_img img').error(function() {
      $(this).attr('src', '/templates/newgeneration/images/NoImage.jpg');
    });
  });
</script>

