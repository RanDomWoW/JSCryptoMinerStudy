<script>
    function getcities() {

        var scountry = $("#scountry").val();
        if (scountry == '') {
            var sdata = '<option value="">Select City</option>';
            $("#scity").html(sdata);
        }
        if (scountry != '') {
            $.ajax({
                type: "POST",
                url: "https://www.theteflacademy.com/includes/cities.php",
                data: {action: 'yes', scountry: scountry},
                success: function (data) {
                    if (data != '') {
                        $("#scity").html(data);
                    }
                }
            });
        }
    }

    function searchcampus() {

        $("#scountry").addClass('');
        $("#scity").addClass('');
        var scountry = $("#scountry").val();
        var country = scountry.toLowerCase();
        var scity = $("#scity").val();
        if (scountry == "") {
            $("#scountry").addClass('error');
            $("#scountry").focus();
            return false;
        }
        if (scity == "") {
            $("#scity").addClass('error');
            $("#scity").focus();
            return false;
        }

        var url = "https://www.theteflacademy.com/course-location/" + scity;
        window.location.href = url;
    }
