<script>
    function sendnewsletter() {
        $("#fld_first_name").removeClass('error');
        $("#fld_last_name").removeClass('error');
        $("#fld_emailid").removeClass('error');
        $("#ncountry").removeClass('error');
        $("#selecttype").removeClass('error');
        var fld_first_name = $("#fld_first_name").val();
        var fld_last_name = $("#fld_last_name").val();
        var fld_emailid = $("#fld_emailid").val();
        var ncountry = $("#ncountry").val();
        //var typegroup=$("#typegroup").val();
        var typegroup = $('input[name=typegroup]:checked').map(function () {
            return this.value;
        }).get();

        /*alert(typegroup);
         if(document.getElementById('Jobs').checked){
         var jobs=$("#Jobs").val();
         }else{
         var jobs='';
         }

         if(document.getElementById('Blogs').checked){
         var Blogs=$("#Blogs").val();
         }else{
         var Blogs='';
         }
         if(document.getElementById('News').checked){
         var News=$("#News").val();
         }else{
         var News='';
         }
         if(document.getElementById('internvolun').checked){
         var internvolun=$("#internvolun").val();
         }else{
         var internvolun='';
         }

         var checkedValues = $('input[name=typegroup]:checked').map(function () {
         return this.value;
         }).get();*/
        var re = /[A-Z0-9._%+-]+@[A-Z0-9.-]+.[A-Z]{2,4}/igm;
        var alpha = /^[a-zA-Z\s]+$/;
        if (fld_first_name == "") {
            $("#fld_first_name").addClass('error');
            $("#fld_first_name").focus();
            return false;
        } else if (!alpha.test(fld_first_name)) {
            $('#fld_first_name').addClass('error');
            $('#fld_first_name').focus();
            return false;
        } else if (fld_last_name == "") {
            $('#fld_last_name').addClass('error');
            $('#fld_last_name').focus();
            return false;
        } else if (!alpha.test(fld_last_name)) {
            $('#fld_last_name').addClass('error');
            $('#fld_last_name').focus();
            return false;
        } else if (fld_emailid == "") {
            $("#fld_emailid").addClass('error');
            $("#fld_emailid").focus();
            return false;
        } else if (!re.test(fld_emailid)) {
            $("#fld_emailid").addClass('error');
            $("#fld_emailid").focus();
            return false;
        } else if (typegroup == "") {
            $("#selecttype").addClass('error');
            $("#selecttype").focus();
            return false;
        } else if (ncountry == "") {
            $("#ncountry").addClass('error');
            $("#ncountry").focus();
            return false;
        }

        if (fld_first_name != '' && ncountry != '' && fld_emailid != '' && fld_last_name != '' && typegroup != '') {
            $.ajax({
                type: "POST",
                url: "https://www.theteflacademy.com/includes/newsletter.php",
                data: {action: 'yes', fld_first_name: fld_first_name, fld_last_name: fld_last_name, ncountry: ncountry, fld_emailid: fld_emailid, typegroup: typegroup},
                success: function (data) {
                    //alert(data);
                    if (data == 1) {
                        window.location.href = "https://www.theteflacademy.com/newsletter/thankyou";
                    } else if (data == 0) {
                        $("#newsletter_form").html('<h3 class="nwstitle"><b>You are already a subscribed user.</b></h3>');
                    } else if (data == 'n') {
                        $("#fld_first_name").addClass('error');
                        $("#fld_first_name").focus();
                    } else if (data == 'nu') {
                        $('#fld_first_name').addClass('error');
                        $('#fld_first_name').focus();
                    } else if (data == 'ln') {
                        $('#fld_last_name').addClass('error');
                        $('#fld_last_name').focus();
                    } else if (data == 'lnu') {
                        $('#fld_last_name').addClass('error');
                        $('#fld_last_name').focus();
                    } else if (data == 'e') {
                        $("#fld_emailid").addClass('error');
                        $("#fld_emailid").focus();
                    } else if (data == 'en') {
                        $("#fld_emailid").addClass('error');
                        $("#fld_emailid").focus();
                    } else if (data == 'c') {
                        $('#ncountry').addClass('error');
                        $('#ncountry').focus();
                    } else if (data == 'ntj') {
                        $('#selecttype').addClass('error');
                        $('#selecttype').focus();
                    }
                }
            });
        }
    }


