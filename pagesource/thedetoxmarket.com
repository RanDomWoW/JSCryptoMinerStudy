  $(document).ready(function(){
    var searchTimeoutThrottle = 100;
    var searchTimeoutID = -1;


    
	$('.search-brands input').each(function() {
      $(this).bind('keyup change', function(){
		var results = [];
      var $resultsBox = $(this).parent().find('.results');
      
        if($(this).val() != $(this).data('oldval')) {
        	$(this).data('oldval', $(this).val());
          if($(this).val() == "") { 
            $resultsBox.empty();
          } else {
        	 clearTimeout(searchTimeoutID);
        	 var term = $(this).val();
             var regex = new RegExp("^"+term, "i");
        	 searchTimeoutID = setTimeout(function(){
				$.each(brandList.brands, function(i, v) {
                    if (v.name.search(regex) != -1) {
                      results.push({"name":v.name,"handle":v.handle});
                        return;
                    }
                });
               if(results.length > 0) {
                 $resultsBox.empty();
                 $.each(results.slice(0,5), function(index, item) {
//                   console.log(item.name);
                   $resultsBox.append('<div class="result"><a href="'+item.handle+'">'+item.name+'</a></div>');
                 });
               } else {
                   $resultsBox.empty().append('<div class="result no-results">No results found</div>');
               }
               $resultsBox.fadeIn(200);
            }, searchTimeoutThrottle);
          }
        }
        });
    }).attr('autocomplete', 'off').data('oldval', '');


                             
      $('body').bind('click', function(){
      $('.search-brands .results').hide();
    });


  });
  

