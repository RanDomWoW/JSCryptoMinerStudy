<h1>オンライン旅行ガイドブック</h1>

<p>国内旅行も海外旅行も楽しみ方がわかればもっと充実！トラベルタウンズは人気の旅行地のおすすめ情報を提供するオンラインガイドブックです。</p>

<div style="float: left;">
<div id="map" style="width: 615px; height: 280px; margin: 0 0 0 0;"></div>

<script type="text/javascript" src="//maps.google.com/maps/api/js?sensor=false"></script>

<script type="text/javascript">
  function attachMessage(marker, msg) {
    google.maps.event.addListener(marker, 'click', function(event) {
      new google.maps.InfoWindow({
        content: msg,
		padding: 0,
      }).open(marker.getMap(), marker);
    });
	google.maps.event.trigger(marker,"click"); //最初から吹き出しを表示
	marker.setVisible(false); //マーカーを非表示
  }

  var data = new Array();
  
  data.push({position: new google.maps.LatLng(59.329323, 18.068581), content: '<a href="/overseas/nordic/">北欧</a>',mytitle: '北欧'});
  data.push({position: new google.maps.LatLng(41.171418, 26.147461), content: '<a href="/overseas/eastern-europe/">東欧</a>',mytitle: '東欧'});
  data.push({position: new google.maps.LatLng(38.416476, -10.140381), content: '<a href="/overseas/europe/">ヨーロッパ</a>',mytitle: 'ヨーロッパ'});
  data.push({position: new google.maps.LatLng(1.352083, 103.819836), content: '<a href="/overseas/southeast-asia/">東南アジア</a>',mytitle: '東南アジア'});
  data.push({position: new google.maps.LatLng(-40.900557, 174.885971), content: '<a href="/overseas/oceania/">オセアニア</a>',mytitle: 'オセアニア'});
  data.push({position: new google.maps.LatLng(-30.064768, -62.127806), content: '<a href="/overseas/south-america/">南米</a>',mytitle: '南米'});
  data.push({position: new google.maps.LatLng(50.640439, -112.884642), content: '<a href="/overseas/north-america/">北米</a>',mytitle: '北米'});
  data.push({position: new google.maps.LatLng(42.038717, 137.208904), content: '<a href="/overseas/asia/">アジア</a>',mytitle: 'アジア'});
  data.push({position: new google.maps.LatLng(25.317645, 82.973914), content: '<a href="/overseas/south-asia/">南アジア</a>',mytitle: '南アジア'});
  data.push({position: new google.maps.LatLng(21.306944, -157.858333), content: '<a href="/states/usa-hawaii/">ハワイ</a>',mytitle: 'ハワイ'});

  var myMap = new google.maps.Map(document.getElementById('map'), {
    zoom: 1,
    center: new google.maps.LatLng(13.444304, 144.793731),
    scrollwheel: true,
    mapTypeId: google.maps.MapTypeId.ROADMAP
  });

  for (i = 0; i < data.length; i++) {
    var image = '';
	var myMarker = new google.maps.Marker({
      position: data[i].position,
      map: myMap,
	  icon: image,
	  title: data[i].mytitle
    });
    attachMessage(myMarker, data[i].content);
  }
google.maps.event.trigger(marker,"click");//show map
</script>
