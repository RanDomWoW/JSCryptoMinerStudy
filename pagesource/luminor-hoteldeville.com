	<script type="text/javascript">

	function cnil_load(){
		$('#bandeau_cookies').show();
		$('#bandeau_cookies').css("position","fixed");
		$('#bandeau_cookies').css("bottom", "0px");
		$('#bandeau_cookies').css("right", "0px");
