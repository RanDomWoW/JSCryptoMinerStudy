			cnil_disable();
			$.cookie('cookie_cnil_v2', 'viewed', {expires: 400, path: '/'}); // une fois tous les 13 mois
			return false;
		});
	}

	$(document).ready(function() {
		// est-ce que le bandeau a déjà été affiché ?
		if (!$.cookie('cookie_cnil_v2')) {
			cnil_initPopup();
		}
	});

