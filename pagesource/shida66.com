	        useEasing: true,
	        useGrouping: true,
	        separator: '',
	        decimal: '.',
	        prefix: '',
	        suffix: ''
	    };
	    var countUp1 = new CountUp("count-video", 0, videoNum, 0, 2.5, options);
	    var countUp2 = new CountUp("count-teacher", 0, teacherNum, 0, 2.5, options);
	    var countUp3 = new CountUp("count-user", 0, userNum, 0, 2.5, options);

	    $(window).scroll(function () {
	        var scrollTop = jQuery(window).scrollTop();

	        if ($(window).scrollTop() >= jQuery("#count-video").offset().top - jQuery(window).height()) {
	            countUp1.start();
	            countUp2.start();
