        mapContainer.bind("get_whole_site_map", function (event, doneCallback) {
            $(".whole_site_extra").html($(".header__extra-inner").html());
            mapContainer.html("Загружается..."); 
            $.ajax(
                '/Catalog/SiteMap/GetWholeSiteHtml?cultureName=ru-RU',
                {
                    type: "GET",
                    traditional: true,
                    error: function () { mapContainer.html("Не удалось загрузить карту сайта. Попробуйте ещё раз."); },
                    success: function (data) {
                        mapContainer.html(data);

                        $('.whole_site_map_container .for_branch_1').addClass('selected');
                        $('.whole_site_map_container .branch_1').addClass('selected');
                        $('.whole_site_map_container *[data-catalogid=1] > .link').remove();
                        $('.whole_site_map_container *[data-catalogid!=1] > .current').remove();

                        doneCallback();
                    }
                });
        });
    });
