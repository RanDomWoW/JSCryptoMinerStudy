<script>
function reverseDate(reverseOrderDay,x){
    var year = reverseOrderDay.slice(0,4);
    var month = reverseOrderDay.slice(5,7);
    var day = reverseOrderDay.slice(8,10);
    //HTML5 date wants the format to be YYYY-MM-DD - but then the reformat it to DD/MM/YYYY in the input. You can format it however you want and the date will still work and show up in the value, but it won't show up in input
    if(x == true){  
        var fullYear = month + '/' + day + '/' + year; 
    }else{
        var fullYear =  year + '-' + month + '-' + day; 
    }     

    return fullYear;
}

function removeClasses(){
    jQuery('.sidebar-wrapper').removeClass('active');
    jQuery('.sidebar-wrapper').removeClass('bottom-active');
}

function getQueryVariable() {
    var query = window.location.search.substring(1);
    //check to make sure there's a query   
    if(query){
        var vars = query.split("&");
        var arrivalInput = vars[1].split("=");
        var departureInput = vars[2].split("=");
        var bedroomsInput = vars[3].split("=");
        var locationsInput = vars[4].split("=");
        var loctaionReplace = locationsInput[1].replace('+', ' ').replace('+', ' ').replace('%27', "'");

        //arrivals - check to make sure it exists on the page
        if(jQuery( "#gform_1" ).length){
            jQuery('#input_1_2').val(arrivalInput[1]);
            jQuery('#input_1_1').val(departureInput[1]);  
            jQuery('#input_1_3').val(bedroomsInput[1]);  
            jQuery('#input_1_4').val(loctaionReplace);  
        }  
           
    }   
}

