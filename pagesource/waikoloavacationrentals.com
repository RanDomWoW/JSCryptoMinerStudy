                    jQuery(window).on('resize', function(e) {
                        removeClasses(); 
                        jQuery('.sidebar-wrapper.active').css({"width": sidebarWidth});
                    });

                    if(windowWidth > 1000){ 
                        
                        var topDistance = jQuery(document).scrollTop() + jQuery('.footer').height();
                        
