                prefix='ab: ';
                name = item.attributes.seg_parent_name;
                image = item.attributes.seg_parent_image;
            }

            tmp = '';
            tmp += '<li class="result selectable-item ' + item.type + '">';
            tmp += '	<a href="' + item.attributes.deeplink + '">';
            tmp += '	<div class="res-pic">';
            tmp += '		<img width="60" height="60" src="' + image + '">';
            tmp += '	</div>';
            tmp += '	<div class="res-cont">';
            tmp += '		<div class="res-prod">';
            tmp +=                  this.highlightsearch(name);
            tmp += '		</div>';
            tmp += '		<div class="res-price">';


            if (parseFloat(finalPrice)< parseFloat(regularPrice) && parseFloat(finalPrice)!=0){
                tmp += '			<span class="amount red">' + prefix + this.formatPrice(finalPrice) + '</span>';
                tmp += '			<span class="currency red">Eur</span>';
                tmp += '			<div class="old-price">' + this.formatPrice(regularPrice) + ' Eur</div>';
            } else {
                tmp += '			<span class="amount">' + prefix + this.formatPrice(regularPrice) + '</span>';
                tmp += '			<span class="currency">Eur</span>';
