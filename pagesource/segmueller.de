                }
                if (categoryHitCounts){
                    categoryList += '<div class="f-right">'+countLabel+'</div>';
                }
                if (brandHitCounts){
                    brandList += '<div class="f-right">'+countLabel+'</div>';
                }
                searchTermList += '</div><div class="short-results"><ul>' + searchTermElements;
                categoryList   += '</div><div class="short-results"><ul>' + categoryElements;
                brandList      += '</div><div class="short-results"><ul>' + brandElements;


                if (productCount==0){
                    productList += '<li class="result empty-result">Es konnten leider keine Produkt-Vorschläge gefunden werden.</li>';
                }
                if (categoryCount == 0){
                    categoryList += '<li class="result empty-result">Es konnten leider keine Kategorie-Vorschläge gefunden werden.</li>';
                }
                if (brandCount == 0){
                    brandList += '<li class="result empty-result">Es konnten leider keine Marken-Vorschläge gefunden werden.</li>';
                }
                if (searchTermCount == 0){
                    searchTermList += '<li class="result empty-result">Es konnten leider keine Such-Vorschläge gefunden werden.</li>';
                }


                productList += '</ul>';
                categoryList += '</ul></div>';
                brandList += '</ul></div>';
                searchTermList += '</ul></div>';

                var categoryColumn = '<div class="result-col categories">' + searchTermList + categoryList + brandList + '</div>',
                    productColumn  = '<div class="result-col products">' + productList + '</div>';

