        },

        formatPrice: function (strPrice)
        {
            var fPrice = parseFloat(strPrice);
            strPrice = fPrice.toFixed(2);
            strPrice = strPrice.replace('.', ',');
            return strPrice
        },

        loadDataCallback : function(data) {
