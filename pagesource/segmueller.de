        <script type="text/javascript">
    //<![CDATA[
    document.observe('dom:loaded', updateDynamic);

    function updateDynamic() {
        var url = 'https://www.segmueller.de/seg/dynamic/index/';
        var productIdsVal='';
        if ($$('input[name="related_ids"]').length){
            productIdsVal=$$('input[name="related_ids"]')[0].getValue();
        }
        new Ajax.Request(url.replace(/\w+:/, ''), {
            method : 'get',
            parameters: {related_ids: productIdsVal},
            onSuccess : function(transport) {
                var response=JSON.parse(transport.responseText);
                updateMiniCart(response['cart']);
                updateTopLinks(response['toplinks']);
                updateMaxQtys(response['maxqtys']);
                updateGlobalMessages(response['global_messages']);
            }
        });
    }
    
    function updateMiniCart(dynamicCartContent) {
        var tmp = $('j2t-temp-div');
        if (!tmp) {
            tmp = new Element('div');
            tmp.hide();
            $(document.body).insert(tmp);
        }
        tmp.innerHTML = dynamicCartContent;

        ['.top-link-cart', '.mini-cart'].each(function(selector) {
            var elem = tmp.down(selector);
            if (!elem) {
                return;
            }
            var content = elem.innerHTML;
            $$(selector).each(function(elem) {
                if (elem.up('#' + tmp.identify())) {
                    return;
                }
                elem.innerHTML = content;
            });
        });
    }
    
    function updateTopLinks(dynamicToplinksContent) {
        var menu = $('u-menu');
        if(menu) {
            menu.outerHTML = dynamicToplinksContent;
        }
    }


    function updateMaxQtys(dynamicMaxqtysContent) {
        if (isProductpage() && typeof(updateQtySelectProductpage)=='function'){ // on productpage if CTA is used
            maxQtys = dynamicMaxqtysContent; // get MaxQtys array from ajax response
