                    brandHitCounts      = 0,
                    searchTermHitCounts = 0;

                for(var i = 0; i < suggestions.length; i++) {

                    var item = suggestions[i];

                    switch (item.type){
                        case 'searchTerm':
                            searchTermElements += this.getRightColItemHtml(item);
                            searchTermCount++;
                            searchTermHitCounts += item.hitCount;
