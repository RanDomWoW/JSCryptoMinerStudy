    document.observe("dom:loaded", function () {
        var checkLengthFields = $$('.validate-maxlength, .validate-minlength');
        var reMax = new RegExp(/^maximum-length-[0-9]+$/);
        var reMin = new RegExp(/^minimum-length-[0-9]+$/);
        checkLengthFields.each(function (elem, index) {
            var classes = (elem.className).split(' ');
            classes.each(function (name, index) {
                if (name.match(reMax)) {
                    var length = name.split('-')[2];
                    addMaxLengthValidation(length);
                }
                if (name.match(reMin)) {
                    var length = name.split('-')[2];
                    addMinLengthValidation(length);
                }
            });
        });
    });

    function addMaxLengthValidation(length) {
        var message = 'Bitte verwenden Sie maximal %s Zeichen.';
        message = message.replace("%s", length);
        Validation.add('maximum-length-' + length, message, function (v) {
            return (v.length <= length);
        });
    }

    function addMinLengthValidation(length) {
        var message = 'Bitte verwenden Sie mindestens %s Zeichen (Buchstaben, Zahlen, Sonderzeichen).';
        message = message.replace("%s", length);
        Validation.add('minimum-length-' + length, message, function (v) {
            return (v.length >= length);
        });
    }

    Validation.addAllThese([

        ['validate-digits-signs', 'Bitte verwenden Sie nur Zahlen (einschließlich "+", "-" oder "/")', function(v) {
            return Validation.get('IsEmpty').test(v) || /^[0-9\+-\/]+$/.test(v)
        }],
    ]);

