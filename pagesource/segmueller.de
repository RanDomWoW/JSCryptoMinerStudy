            jQuery('#search_autocomplete').css('width', Math.max(width, jQuery('#header-search').width()));
        },

        getRightColItemHtml : function(item){
            var tmp='';
            tmp += '<li class="short-res-row selectable-item ' + item.type + '">';
            tmp += '<span class="products"><a href="'+this.getItemUrl(item)+'">' + this.highlightsearch(item.name) + '</a></span>';
            var style='';
            if (!item.hitCount){
                style='style="display: none;"';
            }
            tmp += '<span class="number"' + style + '><a href="'+this.getItemUrl(item)+'">' + item.hitCount + '</a></span>';
            tmp += '</li>';
            return tmp;
        },
        getProductHtml : function(item){

            var finalPrice=item.attributes.seg_final_price;
            var regularPrice=item.attributes.seg_regular_price;
            var prefix='';
            var name = item.name;
            var image = item.image;

            if(item.attributes.seg_is_combined_view != 0){
                finalPrice = item.attributes.seg_parent_final_startingat_price;
