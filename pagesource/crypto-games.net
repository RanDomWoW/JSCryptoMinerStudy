        $(document).ready(function () {

            $(".a_index").fancybox({
                autoscale: true,
                transitionIn: 'none',
                transitionOut: 'none',
                type: 'iframe',
            });

            $(".gameSelect").click(function (e) {
                e.preventDefault();

                var game = $(this).data("game");

                $("#diceCoins").hide();
                $("#slotCoins").hide();
                $("#bjCoins").hide();
                $("#lottoCoins").hide();
                $("#rouletteCoins").hide();
                $("#pokerCoins").hide();
                $("#plinkoCoins").hide();
                $("#sliderMenu1").find("span").removeClass("activeItem");
                $("#sliderMenu2").find("span").removeClass("activeItem");

                if (game == "dice") {
                    $("#diceCoins").fadeIn("fast");
                    $("#sliderMenu1").find("[data-game='dice']").find("span").addClass("activeItem");
                    $("#sliderMenu2").find("[data-game='dice']").find("span").addClass("activeItem");
                    $(".btnPlay").attr("href", "/dice/bitcoin");
                }
                else if (game == "slot") {
                    $("#slotCoins").fadeIn("fast");
                    $("#sliderMenu1").find("[data-game='slot']").find("span").addClass("activeItem");
                    $("#sliderMenu2").find("[data-game='slot']").find("span").addClass("activeItem");
                    $(".btnPlay").attr("href", "/slot/bitcoin");
                }
                else if (game == "blackjack") {
                    $("#bjCoins").fadeIn("fast");
