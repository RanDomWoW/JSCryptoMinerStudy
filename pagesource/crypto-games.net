                    $("#sliderMenu2").find("[data-game='roulette']").find("span").addClass("activeItem");
                    $(".btnPlay").attr("href", "/roulette/bitcoin");
                }
                else if (game == "poker") {
                    $("#pokerCoins").fadeIn("fast");
