<style type="text/css">
a:hover{
    color: #0084ff;
    text-decoration: none;
}

.hero.home .hero-text {
    margin-top: 50px;
}
.hero.home h1.page-title {
    font-size: 1.875rem;
}
span.inner-2018 {
    font-size: 1.625rem;
    font-weight: 300;
}
section.efectivo h2.section-heading {
    margin-bottom: 20px;
}
section.efectivo .texto h3 {
    margin-top: 0;
}
section.efectivo .information-set:first-child {
    margin-top: 20px;
}
section.efectivo .information-set {
    margin-top: 35px;
}
section.efectivo .information-set .media-left {
    padding-right: 45px;
min-width: 120px;
}
.conocer-mas {
    font-size: 1.375rem;
}
section.efectivo .andres-effectivo {
    margin-top: -60px;
}
.section.quieroaprender {
    padding: 35px 0 15px;
    background-color: #ff7519;
    color: #fff;
}
.section.quieroaprender .stats {
    line-height: 1.750;
}
.section.quieroaprender .stats::after {
    content: "";
    position: absolute;
    z-index: 1;
    top: 0;
    bottom: 0;
    right: 0;
    border-left: 1px solid #ff8c40;
    transform: translate(-50%);
    min-height: 155px;
}
.section.quieroaprender .stats:last-child::after {
    display: none;
}
.section.quieroaprender .stats img {
    margin-bottom: 20px;
}
.section.quieroaprender .btn-primary.blue {
    margin-top: 35px;
}
.btn-primary.blue {
    background: #0084ff;
    color: #fff;
}
.stripe-btn {
    margin-top: 35px;
}
section.como-funciona {
    padding: 140px 0 120px;
    background-color: #edf6ff;
    background-position: center bottom;
    background-repeat: no-repeat;
    background-size: cover;
}
section.testiominals {
    padding: 60px 0 0 0;
}
h3.testimonials-subtitle {
    margin-bottom: 40px;
    margin-top: 0;
    font-weight: 300;
    font-size: 1.5rem;
}
.testimonials-home .carousel-control {
    width: 5%;
}
.testimonials-home .carousel-control.left,
.testimonials-home .carousel-control.right {
    background-image: none;
}
.testimonials-home .caption {
    display: table-cell;
    height: 535px;
    vertical-align: middle;
    text-align: center;
}
.testimonials-home blockquote {
    border-left: 0;
    padding: 0;
    font-family: 'Nunito', sans-serif;
    color: #0084ff
}
.testimonials-home .arrow {
    position: absolute;
    top: 50%;
}
.testimonials-home .arrow.left {
    right: 0;
}
section.partners {
    border-top: 1px solid #e6e6e6;
    padding: 50px 0;
}
section.partners img:first-child {
    margin-left: 45px;
}
section.partners img {
    margin-right: 60px;
    display: inline-block;
}
section.partners img:last-child {
    margin-right: 0;
}
@media (max-width:440px) {
    .hero.home {
        min-height: 865px;
    }
}
@media (min-width:440px) and (max-width:750px) {
    .hero.home {
        min-height: 915px;
    }
}
@media (min-width:751px) and (max-width:991px) {
    .hero.home {
        min-height: 875px;
    }
}
@media(max-width:767px) {
    .hp-icons {
        margin-bottom: 20px;
    }
    .section.quieroaprender .stats {
        margin-bottom: 55px;
        border-bottom: 1px #ff8c40 solid;
        padding-bottom: 20px;
    }
    .section.quieroaprender .stats::after {
        display: none;
    }
}
@media (min-width:991px) and (max-width:1200px) {
    img.andres-effectivo {
        margin-top: 50px !important;
    }
    .testimonials-home .caption {
        height: 450px;
    }
    section.partners img:first-child {
        margin-left: 0px;
    }
    section.partners img {
        margin-right: 30px;
        display: inline-block;
    }
}
@media(max-width:991px) {
    .hero.home {
        background: linear-gradient(45deg, #22d7e1 0%, #0084ff 100%);
    }
    .hero.home .hero-text {
        text-align: left;
    }
    .hero.home h1.page-title {
        font-size: 1.625rem;
    }
    .hero.home .hero-icon img {
        max-height: 350px;
    }
    .hero.home .lf {
        margin-top: 0;
    }
    section.efectivo {
        text-align: center;
        padding-top: 70px;
    }
    section.efectivo .information-set {
        margin-top: 0;
        margin-bottom: 50px;
    }
    .information-set .media {
        padding: 0 10px;
    }
    section.efectivo .information-set .media-left {
        display: block;
        margin-right: auto;
        margin-left: auto;
        margin-bottom: 20px;
        padding-right: 0;
    }
    section.efectivo .andres-effectivo {
        margin-top: 0px;
        max-height: 450px;
    }
    .section.quieroaprender .stats:last-child {
        margin-bottom: 0;
        border-bottom: none;
    }
    .section.quieroaprender .stats img {
        margin-bottom: 20px;
    }
    .section.como-funciona {
        text-align: center;
        background-image: none !important;
        padding: 40px 0 0;
    }
    .section.como-funciona img{
        margin-top: 30px;
    }
    .carousel-img {
        border-bottom: 1px solid #e6e6e6;
    }
    .carousel-img img{
        max-height: 295px;
    }
    .testimonials-home .caption {
        height: auto;
        padding: 30px 0;
    }
    .testimonials-home .carousel-control {
        top: 70%;
    }
    section.partners {
        padding-bottom: 0;
        text-align: center;
    }
    section.partners img:first-child {
        margin-left: 0px;
    }
    section.partners img {
        margin-right: 40px;
        display: inline-block;
        margin-bottom: 50px;
    }
    section.partners img:nth-child(even) {
        margin-right: 0;
    }
}

</style>

