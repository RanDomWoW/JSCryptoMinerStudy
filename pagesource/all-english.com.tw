      var swfRegExp=/(.*\.swf)\??(.*)/;
      var mediaRegExp=/(.*\.(mp4|flv))\??(.*)/;
      var youtubeRegExp=/.*youtube.*/;
      var imgRegExp=/\.(jpg|gif|png|bmp|jpeg)(.*)?$/i;
      var matches;
      var href;
      var swf={};
      if(matches=this.href.match(swfRegExp)){
        href=matches[1];
        type="swf";
        swf={'wmode':'transparent','allowfullscreen':'true','allowscriptaccess':'true'};
        var tmp=matches[2].split("&");
        for(x in tmp){
          var t=tmp[x].split("=");
          swf[t[0]]=decodeURIComponent(t[1]);
        }
      }else if(matches=this.href.match(mediaRegExp)){
        href="player.swf";
        type="swf";
        swf={'wmode':'transparent','allowfullscreen':'true','allowscriptaccess':'true'};
        swf["flashvars"]="file="+matches[1];
        if(matches[3]==""){
          swf["flashvars"]+="&autostart=true";
        }else{
          swf["flashvars"]+="&"+matches[3];
        }
      }else if(this.href.match(youtubeRegExp)){
        href=this.href.replace(new RegExp("watch\\?v=", "i"), 'v/');
        swf={'wmode':'transparent','allowfullscreen':'true'};
        type="swf";
      }else if(this.href.match(imgRegExp)){
        href=this.href;
      }else{
        href=this.href;
        type="iframe";
      }
      $.fancybox({'autoScale':false,'width':800,'height':450,'overlayOpacity':0.7,'href':href,'swf':swf,'type':type,'centerOnScroll':false,'next':false});return false;
