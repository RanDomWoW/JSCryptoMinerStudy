                $('#fancybox-content').height(hhh);
                $('#fancybox-content').width(www);
              });
            }
          });
        });

        $("#menu li").click(function(){
          switch($(this).index()){
            case 0:
              location.href="class_all.php?m=1";
              break;
            case 1:
              location.href="class_all.php?m=2";
              break;
            case 2:
              location.href="class_all.php?m=3";
              break;
            case 4:
