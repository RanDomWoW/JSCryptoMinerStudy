<script type="text/javascript">
$(function(){
    $('#slideshow').cycle({
        fx:     'scrollLeft',
        speed:  '1500',
        timeout: 5000,
        pager:  '#nav',
		pause : 1,
        pagerAnchorBuilder: function(idx, slide) {
            // return sel string for existing anchor
            return '#nav li:eq(' + (idx) + ') a';
        }
    });

$(document).ready(function() {
    //ホバーイベント発火
    $(".tip_trigger").hover(function(){
        //class="tip_trigger"内からclass="preview"を探す
        tip = $(this).find('.preview');
        tip.show(); //表示
    }, function() {
        tip.hide(); //非表示   

    //ここからマウスムーブイベント
    }).mousemove(function(e) {
        var mousex = e.pageX + 20; //位置を取得
        var mousey = e.pageY + 20; //同上
        var tipWidth = tip.width(); //幅を取得
        var tipHeight = tip.height(); //高さを取得

        var tipVisX = $(window).width() - (mousex + tipWidth);
        var tipVisY = $(window).height() - (mousey + tipHeight);

        if ( tipVisX < 20 ) { //X座標を超えた場合は幅を調節
            mousex = e.pageX - tipWidth - 20;
        } if ( tipVisY < 20 ) { //Y座標を超えた場合は高さを調節
            mousey = e.pageY - tipHeight - 20;
        }
        tip.css({  top: mousey, left: mousex });
    });
});


});
</script>
