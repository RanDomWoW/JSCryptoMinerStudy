		function rotateTerm() {
			var goodbyes = $(".js-goodbye").data("term") || 0;
			$(".js-goodbye").data("term", goodbyes == terms.length -1 ? 0 : goodbyes + 1).text(terms[goodbyes]).fadeIn()
			.delay(2000).fadeOut(200, rotateTerm);
		}
		$(rotateTerm);
	});
</script>

