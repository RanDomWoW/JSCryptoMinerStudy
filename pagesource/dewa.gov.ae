<script type="text/javascript">
    jQuery(document).ready(function () {
        jQuery(document).on('change', '#searchcategory input:checkbox', setcategory);
        jQuery(document).on('change', '.m31-search--filter-checkbox__all input:checkbox', setall);

        setcat();

    });
    function setcat() {
        //debugger;
        //var url_string = window.location.href;
        //var url = new URL(url_string);
        var filter = getParameterByName("filter");
        var term = getParameterByName("term");

        $("#site_search_filter").val(filter);

        if (term != null) {
            $("#site_search").val(term);

        }
        if (filter == null || filter == "" || filter == "All") {
            $("#site_search_filter").val("All");
            $(".m31-search--filter-checkbox__all input").prop("checked", true);
            jQuery(document).find("#searchcategory input:checkbox").prop('checked', true);
        } else {
            fq = filter.split(',');
            $.each(fq, function (i, v) {
                var f = "";
                jQuery(document).find("#searchcategory input:checkbox").each(function (index) {
                    f = $(this).data('text');
                    if (f == fq[i]) {
                        $(this).attr("checked", "checked");
                    }
                });
            });
            $(".m31-search--filter-checkbox__all input").prop("checked", false);


        }
        $("#site_search").focus();
    }

    function setcategory(e) {
        //debugger;
        var f = "";
        if ($('#searchcategory input:checked').length == $('#searchcategory input:checkbox').length) {
            $("#site_search_filter").val("All");
            $(".m31-search--filter-checkbox__all input").prop("checked", true);

        } else {
            jQuery(document).find("#searchcategory input:checked").each(function (index) {
                if (index > 0) {
                    f = f + "," + $(this).data('text');
                } else {
                    f = $(this).data('text');
                }
                // console.log(f);
            });
            $("#site_search_filter").val(f);
            $(".m31-search--filter-checkbox__all input").prop("checked", false);

        }
        $("#site_search").focus();
    }

    function setall(e) {

        $("#site_search_filter").val("All");
        jQuery(document).find("#searchcategory input:checkbox").prop('checked', this.checked);

        $("#site_search").focus();
    }
    function getParameterByName(name, url) {
        if (!url) url = window.location.href;
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
            results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, " "));
    }
</script>

