<script> 
    
    var lnk = document.getElementById('lnkLocation');
    var txt = document.getElementById('BodyContent_BodyContent_FeaturedEvent_txtLocation2');

    function initAutoComplete() { 
        var input = document.getElementById('BodyContent_BodyContent_FeaturedEvent_txtLocation2');

        var options = {
            types: ['(regions)'], 
        };

        var autocomplete = new google.maps.places.Autocomplete(input, options); 

        google.maps.event.addListener(autocomplete,'place_changed', function () {
            var place = autocomplete.getPlace();
            if (place.geometry) { 
                var lat = place.geometry.location.lat();
                var lng = place.geometry.location.lng();
                var latLng = new google.maps.LatLng(lat, lng); 
            }  
            lnk.style.display = 'inline-block';
            txt.style.display = 'none';
            lnk.innerHTML = txt.value + ' <i class="fa fa-times-circle"></i>'; 

            var place = autocomplete.getPlace();
                if (!place.geometry) {
                    window.alert("We cannot find the place you typed, enter a different address");
                    return;
                }
                  
            $('#BodyContent_BodyContent_FeaturedEvent_hdnCountry').val('');
            $('#BodyContent_BodyContent_FeaturedEvent_hdnCity').val('');
            $('#BodyContent_BodyContent_FeaturedEvent_hdnProvince').val('');

                for (var i = 0; i < place.address_components.length; i++) {
                    var addressType = place.address_components[i].types[0];
                      
                    if (addressType === 'country') {
                        $('#BodyContent_BodyContent_FeaturedEvent_hdnCountry').val(place.address_components[i].long_name);
                    }

                    if (addressType === 'locality') {
                        $('#BodyContent_BodyContent_FeaturedEvent_hdnCity').val(place.address_components[i].long_name);
                    }

                    if (addressType === 'administrative_area_level_1') {
                        $('#BodyContent_BodyContent_FeaturedEvent_hdnProvince').val(place.address_components[i].long_name);
                    } 
                }

            updateEvents();
        });

         
    }

    function changeLocation(){ 
        lnk.style.display = 'none';
        txt.style.display = 'inline-block'; 
        txt.value = "";
        return false;
    }
     
    function updateEvents(){
        document.getElementById('BodyContent_BodyContent_FeaturedEvent_btnUpdate').click();
    }

    $('document').ready(function(){ 
        initAutoComplete();  
    });
           
</script>

