<script type="text/javascript">
    $(document).ready(function () {
        // Replace the following values by your ApplicationID and ApiKey.
        var algolia = new AlgoliaSearch('QAUIPDMSEL', 'be52999d67a149e55cf02ee1b60fb356');
        // replace YourTeamsIndexName & YourPlayersIndexName by the name of the indexes you want to query.
        var products = algolia.initIndex('products');
        var categories = algolia.initIndex('categories');
        var localities = algolia.initIndex('localities');

        // Mustache templating by Hogan.js (http://mustache.github.io/)
        var templateProducts = Hogan.compile('<div class="products">' +
          '<div class="ProductName"><a title="{{{ ProductName }}}" href="{{{ ProductUrl }}}">{{{ ProductName }}}</a></div>' +
        '</div>');
        var templateCategories = Hogan.compile('<div class="categories">' +
          '<div class="Category"><a href="{{{ CategoryUrl }}}">{{{ Category }}}</a></div>' +
        '</div>');
        var templateLocalities = Hogan.compile('<div class="localities">' +
          '<div class="Locality"><a href="{{{ LocalityUrl }}}">{{{ Locality }}}</a></div>' +
        '</div>');

        // typeahead.js initialization
        $('#BodyContent_BodyContent_ctrlSearch_txtSearch').typeahead({ hint: false },
        [
          {
              source: products.ttAdapter({ hitsPerPage: 5 }),
              displayKey: 'ProductName',
              templates: {
                  header: '<div class="category">Events</div>',
                  suggestion: function (hit) { return templateProducts.render(hit); },
                  footer: '<div class="divSearchAll"><a id="lnkSearchAll">See more events...<\a></div>'
              }
          },
          {
              source: categories.ttAdapter({ hitsPerPage: 2 }),
              displayKey: 'Category',
              templates: {
                  header: '<div class="category">Categories</div>',
                  suggestion: function (hit) { return templateCategories.render(hit); }
              }
          },
          {
              source: localities.ttAdapter({ hitsPerPage: 2 }),
              displayKey: 'Locality',
              templates: {
                  header: '<div class="category">Places</div>',
                  suggestion: function (hit) { return templateLocalities.render(hit); }
              }
          }
        ]).on('typeahead:selected', function (e, datum) {
            $(this).keyup(function (e) {
                if (e.keyCode == 13) {
                    if (datum.ProductUrl != undefined) {
                        window.location = datum.ProductUrl;
                    }
                    else if (datum.CategoryUrl != undefined) {
                        window.location = datum.CategoryUrl;
                    }
                    else if (datum.LocalityUrl != undefined) {
                        window.location = datum.LocalityUrl;
                    }
                }
            });
        });        
    });

    $(document).on('click', '#lnkSearchAll', function (e) {
        window.location = '/events?search=' + $('#BodyContent_BodyContent_ctrlSearch_txtSearch').val();
    });
</script>


