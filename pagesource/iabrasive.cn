	<script type="text/javascript">
		$(function(){
			$("#a1").show().siblings('a').hide();
			$("#div ul").show();
			$("#div ul").find('li').hide().eq(0).show();
			$("#div ul").bind({
				mouseover:function(){
					$(this).find('li').show();

				},
				mouseout:function(){
					$(this).find('li').hide().eq(0).show();

				}});
			$("#div li").each(function() {
				$(this).bind("mouseover",function(){
					$(this).css("background-color", "#F3F3F6");
				});
				$(this).bind("mouseout",function(){
					$(this).css("background-color", "#fff");
				});
				$(this).bind("click",function(){
					$("#search_keywords").attr("placeholder","请输入汽车名称");
					$("#div ul").prepend(this).find('li').hide().eq(0).show();
					$(this).siblings('li').find('a').removeAttr("id");
					$(this).find('a').attr('id',"bjtpxl");
					var a = $(this).find('a').attr("value");
					if( a == "product" ){
						$("#search_keywords").attr("placeholder","请输入产品名称");
						$("#search_form").attr("action","/showproducts");
					}else if( a == "buy" ) {
						$("#search_keywords").attr("placeholder","请输入采购名称");
						$("#search_form").attr("action","/buyinfo");
					}else if( a == "car" ){
						$("#search_keywords").attr("placeholder","请输入汽车名称");
						$("#search_form").attr("action","/auto/list");
					}else if( a == "question" ){
						$("#search_keywords").attr("placeholder","请输入问答标题");
						$("#search_form").attr("action","/question");                 
					}
				});

			});

});
</script>
