<script type="text/javascript">

function setActiveChecks(checkIDs) 
{
	$(".add_cart").attr("checked",false);
	$(".remove_cart").attr("checked",false);
	
	if(checkIDs!==null && checkIDs!=='') 
	{
		var checkIDsList = checkIDs.split('_');	
	
		$.each(checkIDsList, function() 
		{ 
			if (this.length > 1) 
			{
				$("#add_cart_"+ this).attr("checked","checked");
				$("#remove_cart_"+ this).attr("checked","checked");
				$("#cart").css("display","block");
				var ticketArtSrc = $("#"+this + " img").attr("src");
				var ticketName = $("#"+this + " h3").html();
				$("#cart_contents").append("<li class='cart_"+this+"'><img src='"+ticketArtSrc+"' /><h3>"+ticketName+"</h3></li>");
				$("#cart_contents .add_cart").attr("checked","checked");
			}
		});
		
	}
}



/**** CHECK CART ****/
function checkCart() 
{
	if($.cookie('scratch_compare')!==null) 
	{
		var scratchItems = $.cookie('scratch_compare');
		setActiveChecks(scratchItems);
	}
}

/**** CHECK CART ****/
function viewToggle() { 
	if($.cookie('view_choice') == 'slide') 
	{
		$.cookie('view_choice','list');
	}
	else 
	{
		$.cookie('view_choice','slide');
	}
	
	return ($.cookie('view_choice'))
}


function getView() 
{
	if($.cookie('view_choice') == null) 
	{
		$.cookie('view_choice','slide');
	}
	
	return ($.cookie('view_choice'));
}


/**** ADD SELECTION ****/
function addUpdateCart(myID) 
{
	if($.cookie('scratch_compare')!==null) 
	{
		var scratchItems = $.cookie('scratch_compare');
		$.cookie('scratch_compare', null, { path: '/' });
		var parseCartList = scratchItems.split("_");
		var newScratchItems = "";
		$.each(parseCartList, function(e) { 
			var myCartItem = parseCartList[e];
			if(myCartItem!==myID && myCartItem!=="" && myCartItem!==null) {
				newScratchItems += (myCartItem + "_");
			}
		});
		var newCartItems = newScratchItems+myID;
		$.cookie('scratch_compare', newCartItems, { path: '/' });
		var ticketArtSrc = $("#"+myID + " img").attr("src");
		var ticketName = $("#"+myID + " h3").html();
		$("#cart_contents").append("<li class='cart_"+myID+"'><img src='"+ticketArtSrc+"' /><h3>"+ticketName+"</h3></li>");
		$("#cart_contents .add_cart").attr("checked","checked");
	}
	else {
		$.cookie('scratch_compare', myID, { path: '/' });
		var ticketArtSrc = $("#"+myID + " img").attr("src");
			var ticketName = $("#"+myID + " h3").html();
			$("#cart_contents").append("<li class='cart_"+myID+"'><img src='"+ticketArtSrc+"' /><h3>"+ticketName+"</h3></li>");
			$("#cart_contents .add_cart").attr("checked","checked");
	}
	
		$("#cart").css("display","block");
}

/**** REMOVE SELECTION ****/
function removeSelection(myID) {
	if($.cookie('scratch_compare')!==null) 
	{
		var scratchItems = $.cookie('scratch_compare');
		$.cookie('scratch_compare', null, { path: '/' });
		var parseCartList = scratchItems.split("_");
		var newScratchItems = "";
		
		
		$.each(parseCartList, function(e) { 
			var myCartItem = parseCartList[e];
			if(myCartItem!==myID && myCartItem!=="" && myCartItem!==null) {
				if (parseCartList.length > 1)
					newScratchItems += (myCartItem + "_");
				else
					newScratchItems += (myCartItem);
			}
		});
		var newCartItems = newScratchItems;
		$.cookie('scratch_compare', newCartItems, { path: '/' });
		
		$("#add_cart_"+myID).removeAttr("checked");
		$("#cart_contents li.cart_"+myID).remove();
		
		$(".compare #"+myID).css("display","none");
		
		if($.cookie('scratch_compare')=="")
			$("#cart").css("display","none");
	}
}

/**** REMOVE SELECTION ****/
function removeAll() {
	if($.cookie('scratch_compare')!==null) {
		$.cookie('scratch_compare', '', { path: '/' });
		$("#cart_contents li").remove();
		$("#cart").css("display","none");
	}
}
/**** REMOVE SELECTION ****/
function compareGames() {
	if($.cookie('scratch_compare')!==null) {
		var compareSet = $.cookie('scratch_compare');
		window.location.href= "/games/scratchoffs/compare/?compareSet="+compareSet;
	}
}
	

$(document).ready(function() {
	// LOAD CART 
	checkCart();
	
	
	// SELECTIONS ON PAGE - IF AN INPUT IS CLICKED 
	$(".add_cart").click(function()
	{
		var myIDpre 	= $(this).attr("id");
		var myIDsplit 	= myIDpre.split('add_cart_');
		var myID		= myIDsplit[1];
		if($(this).is(':checked')==false) 
		{
			removeSelection(myID);
		}
		else 
		{
			addUpdateCart(myID);
		}				
	});
	
	
	// SELECTIONS ON PAGE - IF AN INPUT IS CLICKED 
	$(".remove_cart").click(function()
	{
		var myIDpre 	= $(this).attr("id");
		var myIDsplit 	= myIDpre.split('remove_cart_');
		var myID		= myIDsplit[1];
		if($(this).is(':checked')==false) 
		{
			removeSelection(myID);
		}
		else 
		{
			addUpdateCart(myID);
		}				
	});

	
	//SELECTIONS ON CART - IF AN INPUT IS CLICKED USER MUST BE REMOVING IT FROM LIST 
	$(".cart_selection").click(function()
	{		
		var myIDpre 	= $(this).attr("id");
		var myIDsplit 	= myIDpre.split('cart_selection_');
		var myID		= myIDsplit[1];
		removeSelection(myID);			
	});
						
});
</script>


