<script type="text/javascript">

function trackSoliloquy(link) 
{
	var category = $(link).closest('ul').attr('id');
	var action = $(link).find('img').attr('alt');  			
	var label = String($(link).parent().index()); 
	try { _gaq.push(['_trackEvent', category, action, label]); } catch(err){}
	try { ga('send', 'event', category, action, label); } catch(err){}
	
	window.location = link.href;
}

function trackInternalLink(link, category, action) 
{
	try { ga('send', 'event', category, action, link); } catch(err){}
	try { _gaq.push(['_trackEvent', category , action]); } catch(err){}
	window.location = link.href;
}

function trackOutboundLink(link, category, action) 
{
	try { ga('send', 'event', category, action, link); } catch(err){}
	try { _gaq.push(['_trackEvent', category , action]); } catch(err){}
	window.open(link.href);
}

