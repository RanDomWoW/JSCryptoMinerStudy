<script>
$(function(){
	//快讯列表
   get_realtime();
   function get_realtime(){
	  $.ajax({
        type:"GET",
        url:"http://www.bjnews.com.cn/realtime.json?t=" + Math.random(),
        dataType:"JSON",
        success:function(data) {
            var rows = data.length;
            if (rows == 0) {
                	
            }else if (rows > 0) {
                for (var i = 0; i < rows; i++) {
                    append_realtime(data[i]);
					$("#klist li span").each(function() {
					  var pubDate=$(this).data("time");
					  var pubDate_new=new Date(pubDate.replace(/-/g, '/'));
					  var timetamp=Date.parse(pubDate_new)/1000;
                      $(this).html(c(timetamp, K))
                     });
                }
				
            }
        },
        error:function(data) {
            //alert("很抱歉，系统出现异常，您的操作没有被正常处理！");
        }
    }); 
   }
   //追加快讯列表
   function append_realtime(data){
	  var htmlcode='<li><a href="' + data.url + '" target="_blank">' + data.title + '</a><span data-time="' + data.pubDate + '"></span></li>'; 
	 $("#klist ul").append(htmlcode); 
	}
	//多少天新闻
   function c(a, b) {
         var c = parseInt((b - 1E3 * a) / 1E3);
         if (0 <= c && 60 > c) return c + "\u79d2\u524d";
         if (60 <= c && 3600 > c) return c = parseInt(c / 60),
         c + "\u5206\u949f\u524d";
         if (3600 <= c && 86400 > c) return c = parseInt(c / 3600),
         c + "\u5c0f\u65f6\u524d";
         c = parseInt(c / 86400);
         return c + "\u5929\u524d"
    }
   var K = (new Date).getTime();
   //快讯弹层 点击换一换
    var klist=$("#klist");
    var k_timer = null;
    var k_change=$(".change")
    k_change.click(function(e) {
	   clearInterval(k_timer)
	   $(".change span").addClass("imgrotate");
			setTimeout(function(){
			  $(".change span").removeClass("imgrotate");
			  
	   },500) 
	   start_change() 
	});
	k_change.mouseenter(function(){
			stop_change();
		}).mouseleave(function(){		
			auto_change()
	});
	function start_change(){
	  klist.find("ul:first").stop().animate({
		  marginTop:"-61px"
		  },500,function(){
		  $(this).css({marginTop:"0px"}).find("li:first").appendTo(this);
		  });	
	}
	function stop_change(){
	  clearInterval(k_timer)
	}
	function auto_change(){
	  k_timer = setInterval(function(){
		  start_change()
	  },5000);
	}
	$(document).ready(function(){
	   k_timer = setInterval(function(){
		  start_change()
	  },5000);
	});
   //点关闭按钮快讯弹层消失，图标出现
   $(".close").click(function(e) {
	   $("#layerid").val("0")
       $(".layer_box").animate({
		  "left":"-30%" 
	    },800,function(){
		  $("#layer_icon").removeClass('rollIn rollOut rollOut1').addClass('rollIn').show(); 
	   })
	   clearInterval(k_timer)
   });
   //点击快讯图标,显示快讯弹层
   $("#layer_icon").click(function(e) {
	  $("#layerid").val("1")
	  $("#layer_icon").removeClass('rollIn rollOut rollOut1').addClass('rollOut1') 
     $(".layer_box").animate({
		"left":"50%" 
	 },1500,function(){
		
	})
  });
})
</script>
