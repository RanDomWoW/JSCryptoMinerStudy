	var link = document.createElement("a");
  link.download = $fname;
  link.href = b64;
  document.body.appendChild(link);
  link.click();
 	document.body.removeChild(link);
  delete link;
  try{gatrack("_CHROMEDOWN", "CHROMEDOWN","");}catch(err){}
}

function checkIncognito() {
	
	if (isIncognito!=undefined) {
		return isIncognito;
	}
	if (isChrome==undefined) {
		isChrome = /Chrome/.test(navigator.userAgent) && /Google Inc/.test(navigator.vendor);
	}
	if (isChrome == false) {
		isIncognito = false;
		return false;
	}
	
	var fs = window.RequestFileSystem || window.webkitRequestFileSystem;
	if (!fs) {
	  isIncognito = false;
	} else {
	  fs(window.TEMPORARY,
	     10,
	   	function() {isIncognito=false;},
	    function() {isIncognito=true;});
	}
	
	return isIncognito;
	
}

