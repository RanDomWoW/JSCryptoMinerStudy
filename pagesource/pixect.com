function startWRTC() {
	window.LOC = 'US';
	window.UIP = '54.92.172.79';


	if (window.CAMJS) {
    	window.CAMJS.showCamera();
    	return;
    }


	$.when(
	    $.getScript( "https://cdnjs.cloudflare.com/ajax/libs/webrtc-adapter/6.1.1/adapter.min.js" ),
	    $.getScript( "https://www.gstatic.com/firebasejs/4.9.0/firebase.js" ),
	    $.getScript( "/static/js/wrtc11.js?cache=2" )
	).done(function(){

	    //place your code here, the scripts are all loaded

	    if (window.CAMJS) {
	    	window.CAMJS.showCamera();
	    }

	});
}
function stopWRTC() {
	if (window.CAMJS) {
		window.CAMJS.stopCameraTrack();
	}
}
						
