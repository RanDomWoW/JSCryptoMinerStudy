                $.ajax({
                    url: "/Home/Getdate",
                    data: { placeId: PlaceId, nightCount: nightCount },
                    cache: false,
                    type: "POST",
                    success: function (data) {
                        var markup = "<option selected>تاریخ رفت</option>";
                        for (var x = 0; x < data.length; x++) {
                            markup += "<option value=" + data[x].Value + ">" + data[x].Text + "</option>";
                        }
                        $("#date").html(markup).show();
                    },
                    error: function (reponse) {
                        alert("error : " + reponse);
                    }
                });
            }

        }

