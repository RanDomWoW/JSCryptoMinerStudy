function addTrustedShopRichSnipped(tsId) 
{
when = sessionStorage[ 'TSwhen' ];
var apiUrl = 'https://api.trustedshops.com/rest/public/v2/shops/' + tsId +'/quality/reviews.json';
var xmlhttp = new XMLHttpRequest();
var rep;

xmlhttp.onreadystatechange = function() {
  if (xmlhttp.readyState == XMLHttpRequest.DONE) {   // XMLHttpRequest.DONE == 4
    if (xmlhttp.status == 200) {
      rep = JSON.parse(xmlhttp.responseText);
      sessionStorage[ 'TSwhen' ] = new Date (new Date().setDate(new Date().getDate()+1));
      sessionStorage[ 'TScount' ] = rep.response.data.shop.qualityIndicators.reviewIndicator.activeReviewCount;
      sessionStorage[ 'TSshop' ] = rep.response.data.shop.name;
      sessionStorage[ 'TSresult' ] = rep.response.data.shop.qualityIndicators.reviewIndicator.overallMark;
      writeData()
    }
  }
};

if(!when || !sessionStorage[ 'TScount' ] || !sessionStorage[ 'TSresult' ] || !sessionStorage[ 'TSshop' ]  || new Date(when) < new Date())
{
  xmlhttp.open("GET", apiUrl, true);
  xmlhttp.send();
}
else
writeData();
