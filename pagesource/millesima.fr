<script>
    
    //Display WineBox, and initialize clic events
    function showWineBox() {
        
        jQuery(document).ready(function(){
            var winebox_arrow = jQuery('#winebox-arrow');
            winebox_arrow.removeClass('winebox-arrow-up-init').addClass('winebox-arrow-up');
            
            jQuery('#content_winebox').slideToggle('slow');  
    
            // event : clic sur la flèche pour afficher le détail de la winebox
            jQuery('#winebox-expand').on('click', function (e) {
                var winebox_cart = jQuery('#winebox-cart');
                var winebox_arrow = jQuery('#winebox-arrow');
    
                if (winebox_cart.hasClass('expanded')) {
                    winebox_cart.removeClass('expanded');
                    winebox_arrow.removeClass('winebox-arrow-down').addClass('winebox-arrow-up');
                }
                else {
                    winebox_cart.addClass('expanded');
                    winebox_arrow.removeClass('winebox-arrow-up').addClass('winebox-arrow-down');
                }
                e.preventDefault();
                return false;
            });
    
            // event : clic sur la partie haute pour afficher le détail de la winebox
            jQuery('#winebox-header').on('click', function (e) {
                var winebox_cart = jQuery('#winebox-cart');
                var winebox_arrow = jQuery('#winebox-arrow');
    
                if (winebox_cart.hasClass('expanded')) {
                    winebox_cart.removeClass('expanded');
                    winebox_arrow.removeClass('winebox-arrow-down').addClass('winebox-arrow-up');
                }
                else {
                    winebox_cart.addClass('expanded');
                    winebox_arrow.removeClass('winebox-arrow-up').addClass('winebox-arrow-down');
                }
                e.preventDefault();
                return false;
            });
        });
    }
    
    /**
     * Display the confirmation popin.
     * @param message: (String) message to display.
     * @param timer: (int) time before popin close
     * @param displayAdd2CartButton: (boolean) must display the button or not
     */
    function showDynamicKitConfirmationPopin(message, timer, displayAdd2CartButton, configurationId) {
        if(displayAdd2CartButton == null){
            displayAdd2CartButton = false; //Default value
        }
        
        var blockMessage = '<img id="logoMillesimaPopin" src="https://static.millesima.com/wcsstore/MillesimaSAS/images/millesima/logo_small.png"><br/><br/><div class="content-blockui"><span style="font-size:14px">';
        blockMessage = blockMessage + message;
        blockMessage = blockMessage + '</span><br/><br/>';
        if(displayAdd2CartButton){
            blockMessage = blockMessage + '<a id="processDynamicKit" class="button_primary" onClick="javascript:shoppingActionsJS.AddDynamicKit2ShopCartAjax(\''+configurationId+'\',true);" href="#"><div class="left_border"></div><div class="button_text block_ui_cart_button"><span class="txt">Ajouter la caisse au panier</span></div><div class="right_border"></div></a>';
        }
        
        blockMessage = blockMessage + '</div><a id="manualClose" class="button_primary" href="javascript:;"><div class="left_border"></div><div class="button_text"><span class="txt">Continuer mes achats';
        if(!displayAdd2CartButton){
            blockMessage = blockMessage + '<span id="countdown" style="margin-left:10px;">('+timer+')</span>';
        }
        blockMessage = blockMessage + '</span></div><div class="right_border"></div></a>'; 
        
        jQuery('#msgAfterActionOnDynamicKits').html(blockMessage);
        
        var timer=5;
        var interval=null;
        
        jQuery(document).ready(function () {
            jQuery.fancybox({
                href: '#msgAfterActionOnDynamicKits',
                autoScale : false,
                padding : 0,
                closeBtn : false,
                helpers: {
                    overlay: {
                        closeClick: false,
                        locked: false
                    }
                  },
                afterLoad: function(){
                    if(!displayAdd2CartButton){
                        if (interval!=null){
                            try {
                                clearInterval(interval);
                            }
                            catch (e){}
                        }
                        interval = setInterval(function() {
                            timer--;
                            // Display 'counter' wherever you want to display it.
                            try {    
                                var span = document.getElementById("countdown");
                                span.innerHTML = '('+timer+')';
                                if (timer == 0) {
                                    jQuery.fancybox.close();
                                    clearInterval(interval);
                                }
                            } catch (err) {
                                try {
                                    clearInterval(interval);
                                }
                                catch (err2){}
                            }
                        }, 1000);
                    }
                },
                afterShow: function () {
                    jQuery("#manualClose").on("click", function(){
                        clearInterval(interval);
                        jQuery.fancybox.close();
                    });
                    
                    jQuery("#processDynamicKit").on("click", function(){                    
                        clearInterval(interval);
                        jQuery.fancybox.close();
                    });
                }
            });
        });     
    }
</script>

