<script type="text/javascript">
$(document).ready(function() {
  // =================================
  //    Retailer Search
  // =================================

  if ($('.Haendlersuche').length > 0) {
    if (typeof(navigator.geolocation) === 'undefined') {
      return;
    }
    if (!('matchMedia' in window) || window.matchMedia('only screen and (min-width: 781px)').matches) {
      return;
    }

    navigator.geolocation.getCurrentPosition(function(position) {
      var coords = position.coords.latitude + ',' + position.coords.longitude;

      $.getJSON('http://maps.googleapis.com/maps/api/geocode/json?latlng=' + coords + '&sensor=true', function(data) {
        var components = data.results[0].address_components, i, component;
        for (i = 0; i < components.length; i++) {
          component = components[i];
          if (component.types[0] === 'postal_code') {
            $('.InputTextPLZ').val(component.long_name);
          }
        }
      });

    });
  }
});
</script>

