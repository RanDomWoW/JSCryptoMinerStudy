<script type="text/javascript">

function fetchTireDimCombinations(vehicleType, seasonElement, widthElement, profileElement, rimSizeElement, brandElement, adjustedComboElement)
{
	if (adjustedComboElement)
	{
		adjustedComboElement.data('dimset', true);
	}

	seasonElement.prop('disabled', true);
	widthElement.prop('disabled', true);
	profileElement.prop('disabled', true);
	rimSizeElement.prop('disabled', true);
	brandElement.prop('disabled', true);
	
	$.ajax({
		url: 'https://shop.vergoelst.de/json/dimjsonp.jsp',
		data: {
			vehicleType: vehicleType,
			//season: seasonElement ? seasonElement.val() : '',
			lastSeason: seasonElement ? (seasonElement.data('last') || '') : '',
			width: widthElement.val(),
			widthSet: widthElement.data('dimset') ? 1 : 0,
			profile: profileElement.val(),
			profileSet: profileElement.data('dimset') ? 1 : 0,
			rimSize: rimSizeElement.val(),
			rimSizeSet: rimSizeElement.data('dimset') ? 1 : 0,
			brand: brandElement.val() === null ? firstSlideSelectedBrand : brandElement.val(),
			season: seasonElement.val() === null ? firstSlideSelectedSeason : seasonElement.val()
		},
		async: true,
		cache: false,
		crossDomain: true,
		dataType: 'jsonp',
		timeout: 3000,
		success: function(tireDimensions)
		{
			if (tireDimensions.reset)
			{
				widthElement.removeData('dimset');
				profileElement.removeData('dimset');
				rimSizeElement.removeData('dimset');
				brandElement.removeData('dimset');
			}

/* ****************************************** */
			// Unqualified Brands: 
			// 1028 = GISLAVED
			// 2056 = PLATIN
			// 1232 = PREMADA
			// 1011 = SPORTIVA

			var unqualifiedBrands = ['1028', '2056', '1232', '1011'];
			var tmpBrand = [];

			for (var i = 0; i < tireDimensions.brand.length; i++) {
				if (jQuery.inArray(tireDimensions.brand[i].key, unqualifiedBrands) == -1) {
					tmpBrand[tmpBrand.length] = tireDimensions.brand[i];
				}
			}

			tireDimensions.brand = tmpBrand;

/* ****************************************** */

			setComboData(tireDimensions.season, tireDimensions.selectedSeason, seasonElement, adjustedComboElement);
			setComboData(tireDimensions.width, tireDimensions.selectedWidth, widthElement, adjustedComboElement);
			setComboData(tireDimensions.profile, tireDimensions.selectedProfile, profileElement, adjustedComboElement);
			setComboData(tireDimensions.rimSize, tireDimensions.selectedRimSize, rimSizeElement, adjustedComboElement);
			setComboData(tireDimensions.brand, tireDimensions.selectedBrand, brandElement, adjustedComboElement);
		}
	});
}

function setComboData(availableValues, selectedValue, comboElement, adjustedComboElement)
{
	comboElement.prop('disabled', false);

	if (adjustedComboElement && selectedValue !== '0' && comboElement.attr('id') === adjustedComboElement.attr('id'))
	{
		comboElement.val(selectedValue);
	}
	else
	{
		comboElement.empty();
		for (var i in availableValues)
		{
			if (availableValues.hasOwnProperty(i))
			{
				var dim = availableValues[i];
				var option = $('<option />').attr('value', dim.key).text(dim.value);
				if (selectedValue === dim.key)
				{
					option.prop('selected', true);
				}

				comboElement.append(option);
			};
		}
	}
}

function fetchDimension()
{
	// Fetch car tire dimension combinations
	// 1 /*Car*/
	// 2 /*Offroad*/
	// 3 /*Cargo*/
	// 4 /*Bike*/
	fetchTireDimCombinations($('#category').val(), $('#season'), $('#width'), $('#profile'), $('#rimSize'), $('#brand'), this === window ? undefined : $(this));
}


function doTireSearch(vehicleType, season, width, profile, rimSize, brand)
{
	if (!width || width === '0' || !profile || profile === '0' || !rimSize || rimSize === '0')
	{
		alert('Bitte w\u00E4hlen Sie Reifenbreite, Querschnitt und Zollgr\u00F6\u00DFe aus!');
		return;
	}
	// var language = b2c.locale.language;

	window.location.href = 'https://shop.vergoelst.de/de/shop/sr.html?p02=' + encodeURIComponent(vehicleType)
						+ '&p03=' + encodeURIComponent(season)
						+ '&p06=' + encodeURIComponent(width)
						+ '&p07=' + encodeURIComponent(profile)
						+ '&p08=' + encodeURIComponent(rimSize)
						+ '&p0B=' + encodeURIComponent(brand);
}

$(document).ready(function()
{
    if ($('.searchtabs').length > 0)
    {
        $('.seasonCombo').focus(function()
        {
            $(this).data('last', $(this).val());
        });
    
        $('#season, #width, #profile, #rimSize, #brand').change(fetchDimension);
    
        // On page load, fetch car tire dimension combinations
        fetchDimension();
        
        // Enable menu tab logic
        $('.search').click(function()
        {
            $('.search').removeClass('active');
            $(this).addClass('active');

            var id = $(this).attr('id');
            var oldCategory = $('#category').val();
            // 1 /*Car*/
            // 2 /*Offroad*/
            // 3 /*Cargo*/
            // 4 /*Bike*/
            if (id === 'searchCar')
            {
                $('#category').val(1);
            }
            else if (id === 'searchOffroad')
            {
                $('#category').val(2);
            }
            else if (id === 'searchBike')
            {
                // $('#category').val(4);
				window.location.href = 'https://shop.vergoelst.de/de/shop/configurator-bike.html';
            }
            else if (id === 'searchCargo')
            {
                $('#category').val(3);
            }

            // Dimensionsauswahl nur zurücksetzten, wenn sich die Auswahl ändert
            if (oldCategory != $('#category').val())
            {
                $('#season').val('');
                $('#width').val('');
                $('#profile').val('');
                $('#rimSize').val('');
                $('#brand').val('');
            }

            fetchDimension();
        });

        // Tire search
        $('#doTireSearch').click(function()
        {
            doTireSearch($('#category').val(), $('#season').val(), $('#width').val(), $('#profile').val(), $('#rimSize').val(), $('#brand').val());
        });
    }
});
</script>


