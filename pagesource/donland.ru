                <!--Баннеры с прокруткой -->                
                <script type="text/javascript">

                    var v_banners_upp = 0;
                    var v_banners_upp2 = 0;
                    var v_banners_doun = 0;
                    var v_speed = 25;


                    

                    function fun_banners_up() {
                        if (v_banners_upp < 86) {
                            v_banners_upp = v_banners_upp + 2;
                            v_banners_upp2 = v_banners_upp2 + 2;
                            document.getElementById("DivBannersBlok").style.marginTop = -v_banners_upp2+"px";
                            setTimeout(fun_banners_up, v_speed);
                        }
                    }

                    function fun_banners_up_start() {                        
                        if ((v_banners_upp > 0) & (v_banners_upp < 86)) {

                        } else {
                            if ((v_banners_upp2 >= 430)) { //ограничение прокрутки! 172
                                v_banners_upp2 = 0;// -86;
                            }
                            v_banners_upp = 0;
                            setTimeout(fun_banners_up, 1);                            
                        }
                    }
                    
                    function fun_banners_down() {
                        if (v_banners_doun < 86) {
                            v_banners_doun = v_banners_doun + 2;
                            v_banners_upp2 = v_banners_upp2 - 2;
                            document.getElementById("DivBannersBlok").style.marginTop = -v_banners_upp2 + "px";
                            setTimeout(fun_banners_down, v_speed);
                        }
                    }

                    function fun_banners_down_start() {                        
                        if ((v_banners_doun > 0) & (v_banners_doun < 86)) {
                        } else {
                            
                            if (v_banners_upp2 > 0) {
                                v_banners_doun = 0;
                                setTimeout(fun_banners_down, 1);
                            } else {
                                v_banners_upp2 = 430;//ограничение прокрутки! 258
                                v_banners_doun = 0;
                                setTimeout(fun_banners_down, 1);
                            }
                        }
                    }


                    function fun_scroll_banners(n, v) {

                        clearInterval(IdIntervalBannerColumn);

                        v_speed = v;
                        if (n > 0) {
                            fun_banners_up_start();
                        } else {
                            fun_banners_down_start();
                        }

                        IdIntervalBannerColumn = setInterval(AnimateListBanner, 10000);
                    }

                    function AnimateListBanner() {
                        fun_scroll_banners(2, 25);
                    }

                    var IdIntervalBannerColumn=setInterval(AnimateListBanner, 10000);

                </script>

