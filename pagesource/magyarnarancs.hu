*/

                    $('.top-story .releated-articles a').hover(function(e){
                        clearInterval(rotate);
                        var id = parseInt($(this).closest('li').data('id'));
                        var $activeArticle = $('.top-story').find('article.active');
                        var $activeTab = $('.top-story').find('.releated-articles li.active');

                        if(id > 0 && id != $activeTab.data('id')){
                            var $otherTab = $(this).closest('li');
                            var $otherArticle = $('.top-story').find('article[data-id="'+ id +'"]');

                            displaySlide($activeArticle, $otherArticle, $activeTab, $otherTab);
                        }
                        e.preventDefault();
                    });

                    $(".slideshow-pager a.button-arrow-prev").click(function(e){
                            changeSlide('prev');
                            clearInterval(rotate);
                            e.preventDefault();
                    });

                    $(".slideshow-pager a.button-arrow-next").click(function(e){
                        changeSlide('next');
                        clearInterval(rotate);
                        e.preventDefault();
                    });

                });
            </script>
