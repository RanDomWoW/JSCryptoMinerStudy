            <script>

                var slideLock = false;

                function displaySlide($activeArticle, $otherArticle, $activeTab, $otherTab){

                    if(false == slideLock){
                        slideLock = true;
                        $otherArticle.css({
                            'opacity': '1'
                        });

                        $activeArticle.animate(
                            {
                                'opacity': 0
                            },
                            500,
                            function(){
                                $activeArticle.removeClass('active');
                                $otherArticle.addClass('active');
                                slideLock = false;
                            }
                        );

                        $activeTab.removeClass('active');
                        $otherTab.addClass('active');
                    }
                }

                function changeSlide(action){
                    var $activeArticle = $('.top-story').find('article.active');
                    var $activeTab = $('.top-story').find('.releated-articles li.active');

                    var $otherArticle = null;
                    var $otherTab = null;
                    if('prev' == action){
                        $otherArticle = $activeArticle.prev('article');
                        $otherTab = $activeTab.prev('li');;
                    }
                    else if('next' == action){
                        $otherArticle = $activeArticle.next('article');
                        $otherTab = $activeTab.next('li');;
                    }

                    if("undefined" == typeof $otherArticle || !$otherArticle.length){

                        if(action == 'prev'){
                            $otherArticle = $('.top-story').find('article').last();
                            $otherTab = $('.top-story').find('.releated-articles li').last();
                        }
                        else if(action == 'next'){
                            $otherArticle = $('.top-story').find('article').first();
                            $otherTab = $('.top-story').find('.releated-articles li').first();
                        }
                    }

                    displaySlide($activeArticle, $otherArticle, $activeTab, $otherTab);
                }

                $(document).ready(function(){

                    // Rotate top story articles
                    var rotate = setInterval(
                        function(){
                            changeSlide('next');
                        },
                        5000
                    );

                    // Show/hide pagers
                    $('.top-story').hover(
                        function(){
                            $(this).find('.slideshow-pager').show();
                        },
                        function(){
                            $(this).find('.slideshow-pager').hide();
                        }
                    );

                    // Change slide by click on tabs
/*
                    $('.top-story .releated-articles a').click(function(e){
