	function PageSlide(p){
		if((p=='0') && (gl_page>1)) gl_page--;
		if((p=='1') && (gl_page<max_page)) gl_page++;
		if(gl_page<2) {
			$(".p-left").addClass("no-active");
		} else {
			$(".p-left").removeClass("no-active");
		}
		if(gl_page == max_page) {
			$(".p-right").addClass("no-active");
		} else {
			$(".p-right").removeClass("no-active");
		}
		$("tr[class^=sl6]").css('display','none');
		$("tr[class=sl6_"+gl_page+"]").css('display','table-row');
	}
	function PageSlideNum(p){
		$("tr[class^=sl6]").css('display','none');
		$("tr[class=sl6_"+p+"]").css('display','table-row');
	}

