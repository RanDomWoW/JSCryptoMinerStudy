<script type="text/javascript">

$(document).ready(function(){

    function snifferIe()
    {
        var renifleur = new Object();
        var agentActuel = navigator.userAgent.toLowerCase();
        renifleur.ie = (agentActuel.search("msie")>=0);
        if(renifleur.ie)
        {
            renifleur.agentActuel = navigator.userAgent;
            var extend = renifleur.agentActuel.substring(renifleur.agentActuel.lastIndexOf("MSIE"));
            var version = parseInt(extend.substring(5,7));
            return version;
        }
    }


    function displayButton(width,versionIE)
    {
        if(snifferIe() < versionIE)
        {
            var screenW = $(window).width();
            if(screenW <= width)
            {
                $("#btn_spartooPlus").fadeOut("swing");
                $("#btn_feedback").fadeOut("swing");
            }
            else
            {
                $("#btn_spartooPlus").fadeIn("swing");
                $("#btn_feedback").fadeIn("swing");
            }
         }
    }

    $(window).resize(function(){
        displayButton(1024,9);
        return false;
    });
    displayButton(1024,9);


    $('.icon-flecheBas').each(
        function()
        {
            var li = $(this).parent().parent();
            $(this).css('left', (li.position().left + li.innerWidth() / 2 - 10) + "px");

        }
    );

});

</script>

