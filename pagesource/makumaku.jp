<script>
    // Initialize
    $(document).ready(function () {
        topBannerFunc();
    });

    // When Resize window
    $(window).resize(function () {
        topBannerFunc();
    });

    // When Scroll window
    $(window).scroll(function () {
        topBannerFunc();
    });

    // Function
    var topBannerFunc = function () {
        var scrlY = (window.pageYOffset !== undefined) ? window.pageYOffset : document.documentElement.scrollTop;
        var winWidth = window.innerWidth;
        if (480 < winWidth) {
            if (scrlY < 50) {
                $('.topbox').css({"position": "relative"});
                $('#nheader').css({'margin-top': '0'});
            } else {
                $('.topbox').css({"position": "fixed"});
                $('#nheader').css({'margin-top': '35px'});
            }
        } else {
            $('.topbox').css({"position": "fixed"});
            $('.mainlogo--new').css({'margin-top': '40px'});
        }
    };
</script>



