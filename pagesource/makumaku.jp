<script>
	$("#recommendPlacardsubmit").click(loadRecommendedResult)

	function loadRecommendedResult() {
		if (!validRecommendPlacard()) return false;

		var url = "/User/SubMain/RecommendedPlacardResult";
		var params = {};
		params.useType = $("#recommendedPlacardForm select[name=useType]").val();
		params.width = $("#recommendedPlacardForm input[name=width]").val();
		params.height = $("#recommendedPlacardForm input[name=height]").val();

		$("#recommendPlacardResultDiv").loadCallback(url, params, function () { })
	}

	function validRecommendPlacard() {
		var useType = $("#recommendedPlacardForm select[name=useType]").val();
		var width = parseInt($("#recommendedPlacardForm input[name=width]").val());
		var height = parseInt($("#recommendedPlacardForm input[name=height]").val());

		if (!useType) {
			$("#recommendedPlacardForm select[name=useType]").centerFocus();
			alert("用途・場所を選択してください。");
			return false;
		}

		if (width > 0) {
			$("#recommendedPlacardForm input[name=width]").val(width);
		}
		if (height > 0) {
			$("#recommendedPlacardForm input[name=height]").val(height);
		}
		return true;
	}

	$("#recommendedPlacardForm input[type=text]").keyup(function (e) {
		if (e.keyCode == 13) {
			loadRecommendedResult();
		}
	})
</script>
