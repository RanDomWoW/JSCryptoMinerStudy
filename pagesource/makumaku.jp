<script>
	$(document).ready( function() {
		fixTopPos('#bannerFloat');
		fixTopPos('#bannerFloatLeft');
	});

	function fixTopPos(idNm) {
		var floatBox = $(idNm);
		var offset = floatBox.offset().top;
		var defaultTop = parseInt(floatBox.css('top'));
		var startFix = offset - ($(".topbox").height());
		var fixedTop = 5; // default value == defaultTop

		//when window scroll
		$(window).scroll(function(){
			var scrAmt = $(window).scrollTop();
			var newPos = ((scrAmt - startFix) + (defaultTop + fixedTop)) + "px";

			if (scrAmt >= startFix) {
				floatBox.css('top', newPos);
			} else {
				floatBox.css('top', defaultTop + "px");
			}
		});
	}
</script>


