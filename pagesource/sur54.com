		var resp = $('#mensajeRespuesta');
		resp.html('');

		if(valida){
			console.log('Entre aca');
			$.ajax({
				url: 'http://www.sur54.com/_ajax/recomendarSitio',
				data: 'de='+de.val()+'&para='+para.val()+'&mail='+mail.val()+'&msj='+msj.val(),
				type: 'POST',
				success: function(res){
					console.log(res);
					if(res == 1){
						resp.html('Gracias por recomendar Sur54.com');
						de   = $('#de').val('');
						para = $('#para').val('');
						mail = $('#para_email').val('');
						msj  = $('#comentarios').html('');
					}else{
						resp.html('Ha ocurrido un error, por favor, intentalo nuevamente.');
					}
				}
			})
		}else{
			resp.html('Debes completar con los campos obligatorios para poder recomendarnos.');
		}
	})

	function validarCampos(){
		var de   = $('#de');
		var para = $('#para');
		var mail = $('#para_email');
		var msj  = $('#comentarios');

		if($.trim(de.val()) != "" && $.trim(para.val()) != "" && $.trim(mail.val()) != ""){
			return true;		
		}else{
			return false;
		}
	}
