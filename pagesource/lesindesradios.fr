	function testLoaded() {
		var imgToLoad = $("#pageContent img").size();
		if (imgToLoad > 1) {
			$("#pageContent img").each(function() {
				$(this).on("load", function(){checkAllLoaded('load',$(this), imgToLoad);});
				$(this).on("error", function(){checkAllLoaded('error',$(this), imgToLoad);});
				$(this).on("abort", function(){checkAllLoaded('abort',$(this), imgToLoad);}); 
			});
		} else {
			imgToLoad = 1;
			$("#loadTest").on("load", function(){checkAllLoaded('load',$("#loadTest"), imgToLoad);});
			$("#loadTest").on("error", function(){checkAllLoaded('error',$("#loadTest"), imgToLoad);});
			$("#loadTest").on("abort", function(){checkAllLoaded('abort',$("#loadTest"), imgToLoad);});
		}
	}

	function checkAllLoaded(operation, img, imgToLoad) {
		imgLoaded++;
//		console.debug('=> ajax img '+operation+' ' + imgLoaded + "/" + imgToLoad+" | "+ img.attr('src'));
		if (imgLoaded == imgToLoad) {
//			console.debug('=> all ajax img loaded');
			if (typeof (onPageShown) != 'undefined') {
				//console.debug('run onPageShown');
				$('#loadTest').hide();
				onPageShown();
			}
		}
	}
</script>
