    <script type="text/javascript">

      var startSocialRadioOpened = false;
      var initSocialRadioDone = false;
      var socialRadioClosedByUser = false;

      // Sous IE, la modification d'URL par history.js utilise des ancres, donc sur un F5, il faut faire une redirection manuelle
      if (window.location.hash) {
        var hash = window.location.hash.substring(1); //Puts hash in variable, and removes the # character
        var n = hash.indexOf('&');
        hash = hash.substring(0, n != -1 ? n : hash.length);
        if (hash.indexOf('.html') > -1) {
          var redirectUrl = hash;
          window.location.href = redirectUrl;
        }
      }

      var newsLoaded = false;
      var actus = null;
      var promoLoaded = false;
      var promos = null;
      var confLoaded = false;
      var allLoaded = false;
      var configuration = new Object();
      var userPosition = new Object();
      var noGeolocText = '';
      var maxDptCount = 10;

      var lastVisitedWall = "MDS";

      function launchPodcastRadio(type) {
        togglePlay(radio.id, true, returnPodcastObject(type));
      }

      function returnPodcastObject(type) {
        var podcast = null;
        if (type == "INFO") {
          podcast = {
            type: "INFO",
            url: podcastInfo.url,
            meta: podcastInfo.titreInfo,
          };
        } else if (type == "METEO") {
          podcast = {
            type: "METEO",
            url: podcastMeteo.url,
            meta: podcastMeteo.titreInfo,
          };
        }
        return podcast;
      }

      var isUserOnHome = true;
      // Pour surveiller si l'internaute a cliqué sur quelque chose
      var actionInternaute = null;

      //La radio affichée sur la fiche radio (car ce n'est pas toujours la radio en cours de lecture)
      var shownRadio;

      function setShowRadio(radio) {
        shownRadio = radio;
      }

      //On sauvegarde d'ou on venait quand on arrive sur une fiche radio
      // previousState.value = "murduson" ou "radios"
      // Si "radios", on aura previousState.datas (array), qui contiendra region & filter
      var previousState = new Object();


      function initKeyPress() {
        $(document).keypress(function (event) {
          var tag = event.target.tagName.toLowerCase();
          if (event.which == 32 && tag != 'input' && tag != 'textarea') {
            // Play / Pause par la barre d'espace
            togglePlay(null, true, null);
          }
        });
      }

      function buy() {
        if (currentArtist != TITLE_PLACEHOLDER) {
          
          
          $('#dialog-buy-modal')
              .html('<hgroup class="title popinTitle">'
                  + '<h4>Acheter ce titre</h4>'
                  + '</hgroup>'
                  + '<nav class="shopsList">'
                  + '<ul>'
                  + '<li><a class="iOS" target="_blank" href="' + Partners.getAmazonLink(currentArtist, currentTitle) + '" title=""><img src="/imgv3/buttonBuy_on_Amazon.png" title="Acheter sur Amazon" alt="Amazon"/></a><img src="http://www.assoc-amazon.fr/e/ir?t=wwwlesindesra-21&l=ur2&o=8" width="1" height="1" border="0" alt="" style="border:none !important; margin:0px !important;" /></li>'
                  + '<li><a class="iOS" target="_blank" href="' + Partners.getItunesLink(currentArtist, currentTitle) + '" title=""><img src="/imgv3/buttonBuy_on_Itunes.png" title="Acheter sur Itunes" alt="Itunes"/></a></li>'
                  + '</ul>'
                  + '</nav>')
              .dialog({
                dialogClass: "buyModal",
                modal: false
              });
          setSlider($("#dialog-buy-modal .shopsList"));
          
          openHrefiOS();
        }
      }

      function closeBuy() {
        //$('#dialog-buy-modal').dialog("close");
      }

      function twitter_partager() {
        if (currentArtist != TITLE_PLACEHOLDER) {
          u = location.href;
          t = document.title;
          //var texte = "J'écoute "+currentTitle+" de "+currentArtist+" sur Les Indés Radios www.lesindesradios.fr";
          var texte = "J’écoute " + currentTitle + " de " + currentArtist + " sur l'application Les Indés Radios http://www.lesindesradios.fr/appli.html";
          window.open('http://twitter.com/home?status=' + encodeURIComponent(texte), 'sharer', 'toolbar=0,status=0,width=780,height=436');
        }
      }

      function facebook_partager() {
        if (currentArtist != TITLE_PLACEHOLDER) {
          FB.ui({
            method: 'feed',
            name: currentArtist + ' - ' + currentTitle,
            caption: 'Les Indés Radios',
            message: 'J\'ai entendu ce titre sur ' + radio.nom + '. ' + radio.nom + ' est une Indé Radio. Téléchargez l\'application Les Indés Radios et découvrez « le mur du son ! » une innovation qui permet de visualiser les artistes en cours de diffusion sur + de 130 radios FM indépendantes de France.',
            picture: 'http://www.lesindesradios.fr/services/image?version=1&width=100&iid=' + currentCoverId.replace(new RegExp(/\//gi), ".") + '&sep=.',
            link: 'http://www.lesindesradios.fr'
          });
        }
      }

      function launchPodcastRadio(type) {
        if (type == 'METEO') {
          togglePlay(radio.id, true, podcastMeteo);
        } else if (type == 'INFO') {
          togglePlay(radio.id, true, podcastInfo);
        }
      }

      function addNews(titre, radio, contenu, image, url) {
        var html = '<section class="news resizeHeight9">' +
            ((url && url != '') ? '<a target="_blank" href="' + url + '" title="">' : '<span>') +
            '<figure class="thumb">' +
            '<img src="' + image + '" alt="">' +
            '</figure>' +
            '<section class="caption">' +
            '<hgroup>' +
            '<h2>' + titre + '</h2> ' +
            '<h3>' + radio + '</h3>' +
            '</hgroup>' +
            '<p>' + contenu + '</p>' +
            '</section>' +
            ((url && url != '') ? '</a>' : '</span>') +
            '</section>';

        $('#newsContainer').append(html);
        $('#newsContainer').show();
      }

      function addPromo(image, url) {
        var html = '<section class="add">' +
            ((url && url != '') ? '<a' + (url.startsWith('http') ? ' target="_blank" ' : ' ') + 'href="' + url + '" title="">' : '<span>') +
            '<figure class="thumb">' +
            '<img id="homePushImg" src="' + image + '" alt="">' +
            '</figure>' +
            ((url && url != '') ? '</a>' : '</span>') +
            '</section>';
        $('#promosContainer').append(html);
        $('#promosContainer').show();
      }

      function pullNews() {
        $.ajax({
          type: "GET",
          url: DOMAIN + '/service/actu.do?code=web',
          async: true,
          success: function (data) {
            actus = data.actus;
            preloadActusImg(actus);
            newsLoaded = true;
            if (typeof (displayActus) != 'undefined') {
              displayActus();
            }
            tryInit();
          },
          error: function (request, textStatus, errorThrown) {
            newsLoaded = true;
            tryInit();
          }
        });
      }

      function pullPromo() {
        $.ajax({
          type: "GET",
          url: DOMAIN + '/service/promo.do?code=web',
          async: true,
          success: function (data) {
            promos = data.promos;
            preloadPromosImg(promos);
            promoLoaded = true;
            if (typeof (displayPromos) != 'undefined') {
              displayPromos();
            }
            tryInit();
          },
          error: function (request, textStatus, errorThrown) {
            promoLoaded = true;
            tryInit();
          }
        });
      }

      function pullConfiguration() {
        window.logger.debug('Chargement configuration');
        $.ajax({
          type: "GET",
          url: DOMAIN + '/service/configuration.do?code=web',
          async: true,
          success: function (data) {
            window.logger.debug('Configuration chargée');
            configuration.titlePlaceholderDuration = data.titlePlaceholderDuration;
            configuration.titlePlaceholderLabel = data.titlePlaceholderLabel;
            if (typeof(TITLE_PLACEHOLDER) != 'undefined' && TITLE_PLACEHOLDER) {
              TITLE_PLACEHOLDER = configuration.titlePlaceholderLabel;
              var dashPos = TITLE_PLACEHOLDER.indexOf(' -');
              if (dashPos > 0) {
                TITLE_PLACEHOLDER = TITLE_PLACEHOLDER.substr(0, dashPos);
              }
            }
            configuration.promoRotationDuration = data.promoRotationDuration;
            confLoaded = true;
            tryInit();
          },
          error: function (request, textStatus, errorThrown) {
            window.logger.error('Echec de chargement de la configuration : Utilisation des valeurs par défaut');
            configuration.titlePlaceholderDuration = 120;
            configuration.promoRotationDuration = 5;
            confLoaded = true;
            tryInit();
          }
        });
      }

      function getMyApp(appName) {
        if (navigator.appName.indexOf("Microsoft") != -1) {
          return window[appName];
        } else {
          return document[appName];
        }
      }

      var playerStatus = -2;

      function statusCallback(status) {
        playerStatus = status;
//		window.logger.debug("Status : "+playerStatus);
        updatePlayerStatus();
      }

      function updatePlayerStatus() {
        if (playerStatus == -1) {
          // Pas de lecture en cours => Affichage du bouton PLAY pour lancer la radio par défaut
          $('.buttonPlayer').each(function () {
            $(this).removeClass('buttonPause');
            $(this).addClass('buttonPlay');
            $(this).removeClass('buttonWait');
          });
        } else if (playerStatus == 0) {
          // En pause => Affichage du bouton PLAY pour reprise
          $('.buttonPlayer').each(function () {
            $(this).removeClass('buttonPause');
            $(this).addClass('buttonPlay');
            $(this).removeClass('buttonWait');
          });
        } else if (playerStatus == 1) {
          // Chargement en cours => Afficahge du picto d'attente
          $('.buttonPlayer').each(function () {
            $(this).removeClass('buttonPause');
            $(this).removeClass('buttonPlay');
            $(this).addClass('buttonWait');
          });
        } else if (playerStatus == 2) {
          // Lecture en cours => Affichage du bouton PAUSE
          $('.buttonPlayer').each(function () {
            $(this).addClass('buttonPause');
            $(this).removeClass('buttonPlay');
            $(this).removeClass('buttonWait');
          });
        }
      }

      function tryInit() {
        window.logger.debug("Init Status. Player:" + IndesRadiosPlayer.playerLoaded + " / News:" + newsLoaded + " / Promo:" + promoLoaded + " / Conf:" + confLoaded);
