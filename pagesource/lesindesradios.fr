      var hasPageScroll =true;
      var curLoadAjax = null;

      function loadPage(url, urlRef, toggleLoading) {
        // Si on est sur une transition entre 2 pages, on affiche le picto de chargement
        if (toggleLoading) {
          startLoading();
        }
        // Mis à jour du booléen permettant de savoir si on est sur la home
        if (url === '/contenu/accueil.do') {
          isUserOnHome = true;
        } else {
          isUserOnHome = false;
        }
        window.logger.warn("loadPage isUserOnHome!", isUserOnHome, "lastVisitedWall = " + lastVisitedWall);

        // Désactivation du scroll avant le chargement
        removeMainScroll();

        // Si on était déjà en train de charger une page, on arrête ce chargement
        if (curLoadAjax != null) {
          var oldLoadAjax = curLoadAjax;
          curLoadAjax = null;
          oldLoadAjax.abort();
        }

        // Déclenchement du chargement
        var localUrl = DOMAIN + url;
        launchedLoadAjax = curLoadAjax = $.ajax({
          type: "GET",
          url: localUrl,
          async: true,
          success: function (data) {
            // Chargement OK
            curLoadAjax = null;
            // On masque l'ancienne page, et on affiche la nouvelle
            var pageBody = $('#pageBody');
            pageBody.fadeOut(500, function () {
              // Si on est sur une transition entre 2 pages, on enlève le picto de chargement
              if (toggleLoading) {
                stopLoading();
              }
              pageBody.empty();
              pageBody.append(data);
              pageBody.show(0);
              if (urlRef != null) {
                History.pushState(urlRef, urlRef, urlRef);
                _gaq.push(['_trackPageview', urlRef]);
              }
            });
          },
          error: function (request, textStatus, errorThrown) {
            if (launchedLoadAjax == curLoadAjax) {
              window.logger.error("error on " + localUrl + " : " + textStatus + " / " + errorThrown + " / " + request.status + " / " + request.readyState);
              if (toggleLoading) {
                stopLoading();
              }
              curLoadAjax = null;
            }
          }
        });
      }

