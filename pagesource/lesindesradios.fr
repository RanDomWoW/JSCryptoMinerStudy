      var useGeoloc = true;

      function getGeoloc() {
        
        useGeoloc = true;
        var dptCode = null;
        var dptCount = 10;
        try {
          dptCode = reactv4.getUserDepartement();
          dptCount = reactv4.getDepartementCount();
        } catch (err) {
          window.logger.warn('Echec de lecture dptCode/dptCount : ' + err.message);
        }

        if (dptCode && dptCode > 0 && dptCount < maxDptCount) {
          try {
            reactv4.updateDepartementCount();
          } catch (err) {
            window.logger.warn('Echec de d\'écriture dptCount : ' + err.message);
          }
        } else {
          if ("geolocation" in navigator) {
            setTimeout(
                function () {
                  navigator.geolocation.getCurrentPosition(successCallback, errorCallback, {
                    enableHighAccuracy: true,
                    timeout: 10000,
                    maximumAge: 0
                  });
                }, 1000);

          } else {
            window.logger.warn("Geoloc : Pas de service de geolocalisation sur ce navigateur");
            noGeolocText = "Votre navigateur ne dispose pas d'une fonction de géolocalisation";
          }
        }
        
        
      }

      function isGeolocActive() {
        return !(reactv4.getUserDepartement() == "-1");
      }

      function successCallback(position) {
        userPosition.lat = position.coords.latitude;
        userPosition.lon = position.coords.longitude;
        window.logger.debug("Latitude : " + position.coords.latitude + ", longitude : " + position.coords.longitude);
        //Et maintenant, dans quel département sommes-nous?
        var url = "/services/dept?version=1&lat=" + userPosition.lat + "&lon=" + userPosition.lon;
        $.ajax({
          type: "GET",
          url: DOMAIN + url,
          async: true,
          success: function (data) {
            //window.logger.debug("success - "+data.type);
            if (data.status == 'ok') {
              userPosition.deptCode = data.data.code;
              userPosition.deptName = data.data.name;
              try {
                reactv4.updateDepartementCount(0)
                reactv4.setUserDepartement(data.data.code);
              } catch (err) {
                window.logger.warn('Echec de d\'écriture dptCode/dptCount : ' + err.message);
              }
            } else {
              noGeolocText = "Attention, la fonction géolocalisation n'est disponible qu'en France métropolitaine";
            }
          },
          error: function (request, textStatus, errorThrown) {
            window.logger.error("Error on geoloc url " + url + " : " + textStatus + " / " + errorThrown + " / " + request.status + " / " + request.readyState);
            noGeolocText = "Votre navigateur n'a pas réussi à vous localiser";
          }
        });
      };

      function errorCallback(error) {
        switch (error.code) {
          case error.PERMISSION_DENIED:
            noGeolocText = "Afin de profiter pleinement de ce site web, veuillez autoriser la géolocalisation";
            window.logger.warn("Geoloc : L'utilisateur n'a pas autorisé l'accès à sa position");
            break;
          case error.POSITION_UNAVAILABLE:
            window.logger.warn("Geoloc : L'emplacement de l'utilisateur n'a pas pu être déterminé");
            noGeolocText = "Votre navigateur n'a pas réussi à vous localiser";
            break;
          case error.TIMEOUT:
            window.logger.warn("Geoloc : Le service n'a pas répondu à temps");
            noGeolocText = "Votre navigateur n'a pas réussi à vous localiser";
            break;
        }
      };

