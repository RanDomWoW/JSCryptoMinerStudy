        <script type="text/javascript">
            global = {
                // DatePicker Internationalization
                locale: 'fr'
            };

            $(document).ready(function(){
                mdx.initPopover();
                mdx.initTooltip();
                mdx.initCbAndRadio();
                mdx.initDatePicker();
                mdx.initConfirmDeleteActions();
                mdx.initCookies();

                
                                var publication = null;
                $(document).on('click', '.btn-publication', function(e){
                    e.preventDefault();
                    if(false) {
                        $(location).attr('href', $("#home-link").attr('href'));
                    } else {
                        publication = BootstrapDialog.show({
                            size: BootstrapDialog.SIZE_NORMAL,
                            type: BootstrapDialog.TYPE_PRIMARY,
                            nl2br: false,
                            title: $(this).data('title'),
                            message: $('<div>').append($('#connexion-modale')).html(),
                            animate: false,
                            closable: true,
                            autodestroy: false,

                            onshow: function(dialog) {

                                $(document).on('click', '.submit-login', function(e) {

                                    e.preventDefault();

                                    var formLogin = $("#form_login");

                                    $.ajax({
                                        method: formLogin.attr('method'),
                                        url: formLogin.attr('action'),
                                        data: formLogin.serialize(),
                                        success: function(data, textStatus, jqXHR) {

                                            if (data.success) {

                                                dialog.close();
                                                window.location.href = data.url;

                                            } else {

                                                // Ce n'est pas une erreur mais un moyen de contourner
                                                // le problème de header "Location" appelé en ajax
                                                if ('http' === data.message.substr(0, 4)) {

                                                    window.location.href = data.message;

                                                } else {

                                                    $('.login-error').text(data.message);
                                                }
                                            }
                                        },
                                        error: function(jqXHR, textStatus, errorThrown) {

                                            responseJSON = jqXHR.responseJSON;
                                            mdx.changeDialogToAlert(dialog, responseJSON.data);
                                        }
                                    });
                                });
                            }

                        });
                        mdx.initPopover();
                    }
                });

                $(document).on('click', '#btn-inscription', function(e){
                    e.preventDefault();

                    publication.close();

                    BootstrapDialog.show({
                        size: BootstrapDialog.SIZE_NORMAL,
                        type: BootstrapDialog.TYPE_PRIMARY,
                        nl2br: false,
                        title: $(this).data('title'),
                        message: $('<div>').append($('#inscription-modale').clone()).html(),
                        animate: false,
                        closable: true
                    });
                    mdx.initPopover();
                });
            });
        </script>

    

    <script type="text/javascript">
    $(document).ready(function() {

        var header = $("header");

        updateScroll(header);

        $(window).scroll(function() {
            updateScroll(header);
        });

        function updateScroll(header){
            if($(window).scrollTop() > 0) {
                if(!header.hasClass('scrolled')){
                    header.addClass('scrolled');
                }
            }else{
                header.removeClass('scrolled');
            }
        }

        $('#more').click(function() {
            $('html, body').animate({
                scrollTop:$('#more').offset().top
            }, 'slow');
        });
    });
</script>

