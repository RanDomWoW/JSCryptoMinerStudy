	  <script type="text/javascript">
//<![CDATA[ 
  $(window).load(function(){
  // DOMContentLoaded
$(function() {

    var couleurFilter = $('#couleurFilter');
    var destinationFilter = $('#destinationFilter');
	var formatFilter = $('#formatFilter');
    var aspectFilter = $('#aspectFilter');
	var technologieFilter = $('#technologieFilter');
	var solmurFilter = $('#solmurFilter');

    var originalCollection = $('#courseList');
    var data = originalCollection.clone();
    var filteredData = null;

    // call quicksand on every change to the drop down
    couleurFilter.add(destinationFilter).add(formatFilter).add(aspectFilter).add(technologieFilter).add(solmurFilter).change(function(e) {
         filteredData = data.find('li');
        
        if (couleurFilter.val() !== 'Couleur') {
            filteredData = filteredData.filter('li[data-couleurs~=' + couleurFilter.val() + ']');
        }
        
       if (destinationFilter.val() !== 'Destination') {
            filteredData = filteredData.filter('li[data-destination~=' + destinationFilter.val() + ']');
        }
		
		 if (formatFilter.val() !== 'Format') {
            filteredData = filteredData.filter('li[data-format~=' + formatFilter.val() + ']');
        }
		
		if (aspectFilter.val() !== 'Aspect') {
            filteredData = filteredData.filter('li[data-aspect~=' + aspectFilter.val() + ']');
        }
		
		if (technologieFilter.val() !== 'Technologie') {
            filteredData = filteredData.filter('li[data-technologie~=' + technologieFilter.val() + ']');
        }
		
		if (solmurFilter.val() !== 'Sol ou Mur') {
            filteredData = filteredData.filter('li[data-solmur~=' + solmurFilter.val() + ']');
        }

        // finally, call quicksand
        originalCollection.quicksand(filteredData, {
            duration: 800,
            easing: 'easeInOutExpo'
        });
    });
});
  });
  //]]> 
</script>

