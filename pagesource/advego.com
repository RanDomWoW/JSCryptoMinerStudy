<script type="text/javascript">
$(document).ready(function() {
    $("#equality_mode-").click(updateEqualityMode);
});

function updateEqualityMode() {
    if ($('#mode1label-').hasClass("active")) {
        $('#shingle_params- input').prop("disabled", false);
        $('#segment_params- input').prop("disabled", false);
    }
    if ($('#mode2label-').hasClass("active")) {
        $('#shingle_params- input').prop("disabled", false);
        $('#segment_params- input').prop("disabled", true);
    }
    if ($('#mode3label-').hasClass("active")) {
        $('#shingle_params- input').prop("disabled", true);
        $('#segment_params- input').prop("disabled", false);
    }
}

function submit_params(id_job) {
    if ($('#mode1').is(':checked')) {
        $("input[name='mode']").attr("value", "all");
    }
    if ($('#mode2').is(':checked')) {
        $("input[name='mode']").attr("value", "shingle");
    }
    if ($('#mode3').is(':checked')) {
        $("input[name='mode']").attr("value", "segment");
    }
    if ($('#report1').is(':checked')) {
        $("input[name='report']").attr("value", "equality");
    }
    if ($('#report2').is(':checked')) {
        $("input[name='report']").attr("value", "matches");
    }
    if ($('#report3').is(':checked')) {
        $("input[name='report']").attr("value", "detail");
    }

    var form_data = $("#form-params-"+id_job).serialize();

    $.ajax({
        type: "POST",
        url: "/antiplagiat/save_params/"+id_job,
        dataType: "html",
        data: form_data,
        success: function(data) {
            // $("#save-params-ok-"+id_job).show();
            // $("#save-params-err-"+id_job).hide();
            $("#form-params-"+id_job).find("#result*").hide();
            $("#form-params-"+id_job).append(data);
        },
        error: function() {
            $("#save-params-err-"+id_job).show();
            $("#save-params-ok-"+id_job).hide();
        }
    });
    return false;
}

function delete_rule(elem) {
console.log(elem)
    $(elem).closest(".rule").remove();
    return false;
}

function add_rule(elem) {
    // var new_rule = $(elem).closest("#rules_block").find(".rule:last").clone(true);
    var rule_last = $(elem).closest("#rules_block").find(".rule:last");
    
    rule_last = $(elem).closest("#rules_block").find(".new_rule:last");
    new_rule = rule_last.clone(true);
    new_rule.show();
    new_rule.attr('class', 'rule');
    new_rule.find("input").attr('name', 'rule_url');
    new_rule.find("select").attr('name', 'rule_list');
    
    if (rule_last.length === 0) 
        new_rule.insertBefore($(rule_last));
    else
        new_rule.insertAfter($(rule_last));
    return false;
}

</script>
