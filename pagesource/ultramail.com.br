    <!-- Javascripts do menu like huge inc -->
    
<div id="ads"></div>

        <!--Suporte aos navegadores-->
        <script type="text/javascript">
          $(document).ready(function() {

            $.fn.fnBrowserDetect({
              fingerprint: true, // retornar o identificador do dispositivo
              onBrowserDetected: function(browser, version, valid, fingerprint) {

                console.log('Browser detected is ' + browser, 'Version ' + version, 'Fingerprint ' + fingerprint);

                if (!valid) { // exibe a barra de aviso de atualização do navegador
                  $('<div class="old-browser-bar request-response warning"><strong>Atenção:</strong> Este navegador não é mais suportado e pode causar problemas. Considere trocar ou atualizar seu navegador para uma versão mais recente.</div>').prependTo('article.main');
                }

                // insere o fingerprint no formulário de login
                $('form').append('<input name="StFingerPrint" type="hidden" value="' + fingerprint + '" />');
              }
            });

          });
        </script>
        
