<script>
	var headlineSlider;
	var banners = [{id: 'russ-br', bgColor: '#0c1013'},];
	
    window.addEventListener('message', function(event){
        if (event.data.about == 'redirect') location.href = event.data.value;
        if (event.data.about == 'height') $("#iframe-" + event.data.id).css({'height' : event.data.value});
    }, false);
	var FADE_TIME = 500;
    var counter = 0;
	var slideTimeout = 1;
	var slider = function(){
        if (!banners.length) return;
        var prev = banners[counter % banners.length];
        var cur = banners[++counter % banners.length];

        $('body').removeClass('start-slide');
        $('.banner-body-layer').fadeOut(FADE_TIME);
        $('#headline-banner-' + cur.id).fadeIn(FADE_TIME);

        $('body').removeClass(prev.id).addClass(cur.id);
        var prevElem = $('#leonsiteholder .headline-promo .' + prev.id).css('z-index', 0);
        var curElem = $('#leonsiteholder .headline-promo .' + cur.id).css('z-index', 100);
        slideBanners(prevElem, curElem, FADE_TIME)
	};
	var slideBanners = function(prev, cur, fadeTime){
	    prev.fadeOut(fadeTime);
	    cur.fadeIn(fadeTime)
	}
</script>

<script>
    $(function(){
        
        if (banners.length > 1 && slideTimeout > 999){
            for (var k in banners){
                var nxBanner = $('#headline-banner-' + banners[k].id);
                nxBanner.prependTo($('body > .body'));
                if (k == 0) nxBanner.show();
            }
			headlineSlider = window.setInterval(slider, slideTimeout);
        }


    })
</script>


    


    
