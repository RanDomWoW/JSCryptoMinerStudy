<script>
    var adTarget = {
        _category       : mm_globals.category.replace(/-/g, '_'),
        _subCategory    : mm_globals.sub_category,
        _subSubCategory : mm_globals.sub_sub_category,
        _channel        : mm_globals.channel,
        _postId         : mm_globals.post_id,
        _tags           : mm_globals.tags,
        _template       : mm_globals.template_type
    };
    
    if(mm_globals.category == '') {
        if(mm_globals.pageView.indexOf('/?s=') > -1 && mm_globals.template_type == 'list') {
            adTarget._category = 'search';
        } else if(window.location.href.indexOf('privacy-policy') > -1 || window.location.href.indexOf('terms-of-use') > -1) {
            adTarget._category = 'legal';
        } else {
            adTarget._category = 'uncategorized';
        }
    }

    var adUnit = MMUI.globals.isMobile() ? '/4678/brk.mdmn.mob/' : '/4678/brk.mdmn/',
    adZone = buildAdZones(adTarget._category),
    adSlot = {
        _320x50     : 'div-gpt-ad-320x50',
        _728x90     : 'div-gpt-ad-728x90',
        _970x90     : 'div-gpt-ad-970x90',
        _300x250_1  : 'div-gpt-ad-300x250-1',
        _300x250_2  : 'div-gpt-ad-300x250-2',
        _300x250_3  : 'div-gpt-ad-300x250-3',
        _2000x420   : 'div-gpt-ad-2000x420',
        _1x1        : 'div-gpt-ad-interstitial',
        _200x200    : 'div-gpt-ad-200x200',
        _300x250_editorial_1  : 'div-gpt-ad-editorial-300x250-1',
        _300x250_editorial_2  : 'div-gpt-ad-editorial-300x250-2',
        _300x250_editorial_3  : 'div-gpt-ad-editorial-300x250-3'
    },
    adRender = {},
    adCreative = [], // Record ad creative IDs and line item IDs that load. Use this for troubleshooting ads.
    adRefreshSlots = [],
    adRefreshGallerySlots = [],
    adRefreshInGallerySlot = [],
    hasAdd_300x250_1 = false,
    hasAdd_970x90 = false;


    

    // Use swapAdZone for testing
    if(MMUI.globals.getQueryVariable('swapAdZone')) {
        adZone._320x50 = adZone._728x90 = adZone._970x90 = adZone._300x250_1 = adZone._2000x420 = adZone._1x1 = adZone._200x200 = MMUI.globals.getQueryVariable('swapAdZone');
    }

    if(MMUI.globals.getQueryVariable('swapAdZone2')) {
        adZone._300x250_2 = MMUI.globals.getQueryVariable('swapAdZone2');
    }

    if(MMUI.globals.getQueryVariable('swapAdZone3')) {
        adZone._300x250_3 = MMUI.globals.getQueryVariable('swapAdZone3');
    }

    if(MMUI.globals.getQueryVariable('cid')) {
        adTarget._channel = MMUI.globals.getQueryVariable('cid');
    }

    /**
     * Can we track the ad ? (was defined as a global but when defined MMUI.tracking was never there since it is defined by GTM on the onlad event)
     * @return {Boolean} Returns true if Ad can be tracked. False if not
     */
    function canTrackAd() {
        return !(MMUI.globals.getQueryVariable('swapAdZone') || MMUI.globals.getQueryVariable('swapAdZone2') || MMUI.globals.getQueryVariable('swapAdZone3'));
    }

    function buildAdZones(zone) {
        return {
            _970x90     : zone + '-top',
            _300x250_1  : zone + '-300-top',
            _300x250_2  : zone + '-300-mid',
            _300x250_3  : zone + '-300-bottom',
            _2000x420   : zone + '-brandribbon',
            _1x1        : zone + '-interstitial',
            _300x250_editorial_1  : 'mid_300x250_editorial',
            _300x250_editorial_2  : 'mid_300x250_editorial_2',
            _300x250_editorial_3  : 'mid_300x250_editorial_3',
            _gallery    :'gallery',
            _200x200    :'masthead'
        };
    };
    
    function showAdSlot(zone, dimensions, slot) {
        if($('#' + slot).length == 1) {
            googletag.cmd.push(function () {
                if(dimensions[0] == 1 && dimensions[1] == 1) { //Interstitial
                    googletag.defineOutOfPageSlot(adUnit + zone, slot).addService(googletag.pubads())
                        .setTargeting('subcategory',adTarget._subCategory)
                        .setTargeting('subsubcategory',adTarget._subSubCategory)
                        .setTargeting('channel',adTarget._channel)
                        .setTargeting('postid',adTarget._postId)
                        .setTargeting('tags',adTarget._tags)
                        .setTargeting('template',adTarget._template);
                } else {
                    adRender[slot] = googletag.defineSlot(adUnit + zone, dimensions, slot).setCollapseEmptyDiv(true, true).addService(googletag.pubads())
                        .setTargeting('subcategory',adTarget._subCategory)
                        .setTargeting('subsubcategory',adTarget._subSubCategory)
                        .setTargeting('channel',adTarget._channel)
                        .setTargeting('postid',adTarget._postId)
                        .setTargeting('tags',adTarget._tags)
                        .setTargeting('template',adTarget._template);
                }

                googletag.enableServices();
                googletag.display(slot);
            });

            // GA Tracking for ad call event
            if(canTrackAd()) {
                adRefreshSlots.push({c : zone, l : mm_globals.template_type}); //Save list of ad units so we can track on refresh
            }
        }
    }

    function showGalleryAdSlot(zone, dimensions, slot) {
        if($('#' + slot).length == 1) {
            googletag.cmd.push(function () {
                adRefreshInGallerySlot[0] = googletag.defineSlot(adUnit + zone, dimensions, slot).setCollapseEmptyDiv(true, true).addService(googletag.pubads())
                    .setTargeting('subcategory',adTarget._subCategory)
                    .setTargeting('subsubcategory',adTarget._subSubCategory)
                    .setTargeting('channel',adTarget._channel)
                    .setTargeting('postid',adTarget._postId)
                    .setTargeting('tags',adTarget._tags)
                    .setTargeting('template',adTarget._template);


                googletag.enableServices();
                googletag.display(slot);
            });
        }
    }

    function showHeaderAd() {
        if(MMUI.globals.isMobile()) {

            if($(window).innerWidth() < 728) {
                showAdSlot(adZone._320x50, [[320, 50], [960, 1]], adSlot._320x50);
            } else {
                showAdSlot(adZone._728x90, [[728, 90], [960, 1]], adSlot._728x90);
            }

        } else {

            showAdSlot(adZone._970x90, [[728, 90],[970, 90],[970, 40],[970, 66],[970, 250],[960, 1]], adSlot._970x90);

        }
    }

    function show300x250Ads() {
        if(adTarget._template == 'Prem-Article-C') {
            showAdSlot(adZone._300x250_1, [[300, 250],[300, 600]], adSlot._300x250_1);
        } else {
            showAdSlot(adZone._300x250_1, [300, 250], adSlot._300x250_1);
        }

        if(adTarget._template !== 'Low-Val-B') {
            showAdSlot(adZone._300x250_2, [300, 250], adSlot._300x250_2);
            showAdSlot(adZone._300x250_3, [300, 250], adSlot._300x250_3);
        }
    }

    $(document).ready(function(){
        showHeaderAd();
        show300x250Ads();

        showAdSlot(adZone._2000x420, [2000, 420], adSlot._2000x420);
        showAdSlot(adZone._300x250_editorial_1, [300, 250], adSlot._300x250_editorial_1);
        showAdSlot(adZone._300x250_editorial_2, [300, 250], adSlot._300x250_editorial_2);
        showAdSlot(adZone._300x250_editorial_3, [300, 250], adSlot._300x250_editorial_3);
        
        // LAMP-3258
        if(MMUI.globals.isMobile()) {
            showAdSlot('300x250_mobileweb_article', [300, 250], 'div-gpt-ad-mobile-leaderboard');
        }

        if(adTarget._channel != '') {
            showAdSlot(adZone._200x200, [200, 200], adSlot._200x200);
        }

        // LAMP-2720; 2017-03-09
        var sidebarLayoutBehaviourSetupHasDone = false;
        var timeoutIndexForSidebarTopAdRenewingRelatedInfo = NaN,
            timeoutDurationInMSForRunningIntervalOfSidebarTopAd = 6789
        ;

        googletag.cmd.push(function () {
            googletag.pubads().addEventListener('slotRenderEnded', function(event) {

                if(!event.isEmpty) {
                    adCreative.push({unit : event.slot.getName(), size : (event.size[0]+'x'+event.size[1]), lineItem : event.lineItemId , creativeId : event.creativeId});
                }

                if(event.slot === adRender[adSlot._970x90] && !mm_globals.refresh) { //If slotRenderEnded event fires from an ad refresh, no need to run this
                    if(event.size[0] == 960 && event.size[1] == 1) {
                        return;
                    } else {
                        setTimeout(function() {
                            if($('.takeover-skin').length < 1) {
                                showAdSlot(adZone._1x1, [1, 1], adSlot._1x1);
                            }
                        }, 2000);
                    }
                }


                // LAMP-2720; 2017-03-09
                // console.log('window.isMobile:', window.isMobile);
                // console.log('window.isTablet:', window.isTablet);
                if (!window.isMobile && !window.isTablet) {
                    if (event.slot === adRender[adSlot._300x250_1]) {
                        // console.info('{StickOnBothEdges} ==> Sidebar top ad slot gets rendered.');
                        setupSidebarAdWhenItsSlotRenderEnded();
                    } else {
                        // No matter which ad slot gets loaded,
                        // the layout of the observing ad slot MIGHT be affected.
                        // So here we shall handle each and every ad slot render ended event
                        // just to update the observing ad slot layout infomation.
                        tryEnablingAndUpdatingSidebarAd();
                    }
                }


                if (mm_globals.template_type === 'Prem-Gallery-C' && mm_globals.pageType === 'Single') {
                    if (typeof adRender['div-gpt-ad-970x90'] !== 'undefined' && !hasAdd_970x90) {
                        adRefreshGallerySlots.push(adRender['div-gpt-ad-970x90'])
                        hasAdd_970x90 = true
                    }

