<div class="sv-vertical sv-layout contact-phone" id="svid10_573585f513cf362b85d7fa"><div class="sv-randomimage-portlet sv-portlet footer-figure sv-skip-spacer" id="svid12_573585f513cf362b85d7f1"><div id="Slumpadbildfooterfigure"><!-- Slumpad bild .footer-figure --></div><img alt="" class="sv-noborder" src="/images/18.6b68fdc613d25ecfd27652/1402917547318/Mikael.png" /></div>
<div class="sv-html-portlet sv-portlet" id="svid12_5d8af7dc154d1799c5ae"><div id="Telefonnummer"><!-- Telefonnummer --></div><div class="sv-text-portlet-content"><h3 class="h3-header" id="h-Telefon">Telefon</h3>
    <p class="normal">Ring oss på telefonnummer: <br/><strong id="contactPhoneNumber">0771-45 30 00<br/></strong></p>
    <p class="normal">Öppet alla dagar 8-21*<br/><a title="" href="/2/privat/kundservice/kontakt/kontakta_oss.html">*övriga öppettider</a></p>
    <p class="normal"><br class="SV_LINE_BREAK"/></p>
</div>
<script type="text/javascript">
   if (isMaklarhuset()) {
      $jq("#contactPhoneNumber").text("0771-45 30 02");
   }
   function isMaklarhuset(){
      return getCurrentBrokerId().match(/^46/);
   }
   function getCurrentBrokerId() {
      var brokerid = sessionStorage.getItem("brokerid");
      if (brokerid === null) {
         brokerid = getParameter("brokerid") || getParameter("fk") || "SBAB";
         sessionStorage.setItem("brokerid", brokerid);
      }
      return brokerid;
   }
    
   function getParameter(paramName) {
     var searchString = window.location.search.substring(1),
         i, val, params = searchString.split("&");
    
     for (i=0; i<params.length; i++) {
       val = params[i].split("=");
       if (val[0] == paramName) {
         return val[1];
       }
     }
     return null;
   }
</script></div>
</div>
