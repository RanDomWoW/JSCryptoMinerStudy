    <script>
        /* IE hack for rendering fonts. Cuz page loads too fast, IE can't get in time fonts render and they r blinking. So we add a cap and remove it after a little delay
           Unfortunately, this script'll execute in every browser(
        */
        var version = detectIE();

        if (version !== false) {
            var cap = document.createElement('div');
            cap.className='ie-hack';
            cap.id='iePreloadHack';
            document.body.appendChild(cap);
            setTimeout(function () {
                cap.parentNode.removeChild(cap);
            },1000);
        }

        function detectIE() {
            var ua = window.navigator.userAgent;

            var msie = ua.indexOf('MSIE ');
            if (msie > 0) {
                return parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)), 10);
            }
            var trident = ua.indexOf('Trident/');
            if (trident > 0) {
                var rv = ua.indexOf('rv:');
                return parseInt(ua.substring(rv + 3, ua.indexOf('.', rv)), 10);
            }
            var edge = ua.indexOf('Edge/');
            if (edge > 0) {
                return parseInt(ua.substring(edge + 5, ua.indexOf('.', edge)), 10);
            }
            return false;
        }
    </script>

