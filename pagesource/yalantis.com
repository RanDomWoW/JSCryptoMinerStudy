    <script defer type="text/javascript">

        function resizeImages(images, format) {

            if (images) {
                Array.prototype.forEach.call( images, function( img ) {

                    var urlParts       = img.src.split('/'),
                        storageUrl     = urlParts.slice(0, 3).join('/'),
                        filename       = urlParts[urlParts.length - 1],
                        fileFormat     = filename.substr(filename.lastIndexOf(".", filename.lastIndexOf(".")) + 1),
                        articleWrapper = document.getElementsByClassName('article-w')[0],
                        categoryImgW   = document.getElementsByClassName('img-w')[0],
                        imgWidth       = (img.width !== 0) ? img.width : (articleWrapper) ? articleWrapper.offsetWidth : categoryImgW.offsetWidth;

                    function isAvailable() {
                        return storageUrl === "http://images.yalantis.com";
                    }

                    function isResizable() {
                        return imgWidth !== 0 && fileFormat.toLowerCase().match(format);
                    }

                    function restoreParams(img) {
                        if (img.style.width.length > 0) {
                            img.style.width = 'auto';
                        }
                        if (img.style.height.length > 0) {
                            img.style.height = 'auto';
                        }
                    }

                    function resizeImg() {
                        var width      = 'w' + imgWidth;
                        urlParts.splice(3, 0, width);
                        img.src    = urlParts.join('/');
                        restoreParams(img);
                    }

                    if (isAvailable() && isResizable()) {
                        resizeImg();
                    }
                });
            }
        }

        var articleAuthors    = document.querySelectorAll('.article-image .person img'),
            articlePostImages = document.querySelectorAll('.post img, .blog-section img');

        resizeImages(articleAuthors, /^(png|jpg|jpeg)$/);
        resizeImages(articlePostImages, /^(jpg|jpeg)$/);

    </script>

