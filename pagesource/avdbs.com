<script type="text/javascript">
$(document).ready(function(){
	gnb.init();
});

var gnb = {
	offset: null
	,init: function(){
				this.offset = $('#main-menu').offset().top;
		$( window ).scroll( function(){
			if ($(document).scrollTop() > gnb.offset){
								if ($("#all-menu").is(":visible"))
					gnb.close_all_menu();

				$('#main-menu').addClass('menuFixed');
			}else{
				$('#main-menu').removeClass('menuFixed');
			}
		});

				$("#lnk_all_menu").click(function(){
			if (!$("#all-menu").is(":visible")){
				if ($(document).scrollTop() > gnb.offset)
					$(document).scrollTop(gnb.offset);
			}

			$("#all-menu").toggle("blind", null, 200);
		});
	}

		,close_all_menu: function(){
		$("#all-menu").hide("blind", null, 200);
	}

	,on_menu: function(menu_no){
				if (menu_no == 3)
			av_api.genre_av();
			}
}

