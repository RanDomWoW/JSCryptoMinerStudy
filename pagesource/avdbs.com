<script>
function go_news_view(tp, idx, recall)
{
	//	조회수 증가
	if (parseInt(tp)<100){
		if (!recall){
			av_api.board_view_counter(idx, function(raw){
				go_detail(tp, idx, true);
			});
		}
	}

	var url = "";
	switch (tp){
	case 1:		url = "/menu/subpage10/free_board_view.php?board_idx="+idx ; break;
	case 4:
	case 5:		url = "/menu/subpage07/review_view.php?board_idx="+idx ; break;
	case 8:		url = "/menu/subpage11/notice_board_view.php?board_idx="+idx ; break;
	case 101:	url = "/menu/actor.php?actor_idx=" + idx;	break;
	case 102:	url = "/menu/dvd.php?dvd_idx=" + idx;	break;
	}
	location.href = url;
}

