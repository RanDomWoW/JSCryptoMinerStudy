<script type="text/javascript">
function ajaxsearchsubmit(form){ 
if(form.w.value == ''){ 
$("[id=sli_search_1]").attr("placeholder", "Please enter a search term").blur();
$("[id=sli_search_2]").attr("placeholder", "Please enter a search term").blur();
$("[id=sli_search_3]").attr("placeholder", "Please enter a search term").blur();
return false; 
}else { 
var search = encodeURIComponent(form.w.value); 
var location = encodeURIComponent(form.location.value); 
if (location) { 
location = " in " + location; 
} 
else { 
location = ""; 
} 
 
window.location="http://results.yachting-pages.com/search?w=" +search+location; 
} 
return false; 
} 
</script>


