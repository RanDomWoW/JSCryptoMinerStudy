<script>

function pauseVideos()
{
	$('#videos-aside video').each(function() {
		this.pause();
		this.currentTime = 0;
		$(this).siblings('.mw-video-play-icon').show();
	});
}

$('#videos-aside video').each(function(e) {
	$(this).siblings('.mw-video-play-icon').click(function() {
		if($(this).siblings('video').length == 0) {
			return;
		}

		$(this).hide();
		$(this).siblings('video')[0].play();
	});
});

$('#videos-aside video').on('ended', function() {
	pauseVideos();
});

$('#videos-aside video').on('click', function() {
	this.pause();
	$(this).siblings('.mw-video-play-icon').show();
});

$('#videos-aside .article-thumb-video').click(function() {
	pauseVideos();
});
</script>

				
				
			
