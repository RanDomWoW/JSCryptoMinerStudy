function fn_changeTab(l_name, l_no){
	var old_no = eval("old_"+l_name+"_no");
	if(old_no == l_no){
		return;
	}

	var tab_obj = document.getElementById(l_name + "_" + l_no);
	var body_obj = document.getElementById(l_name + "_b" + l_no);
	var old_tab = document.getElementById(l_name + "_" + old_no);
