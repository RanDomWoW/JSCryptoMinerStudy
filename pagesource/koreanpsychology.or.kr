	tab_obj.src = "img/main/m_boardbtn_02/"+l_name+"_ptoz0"+l_no+"_on.gif";
	old_tab.src = "img/main/m_boardbtn_02/"+l_name+"_ptoz0"+old_no+".gif";

	body_obj.style.display = "";
	old_body.style.display = "none";

	eval("old_"+l_name+"_no = " + l_no);
}

function fn_changeDiv(divName) {
	var ele = document.getElementsByName(divName);

	if (ele.length > 1) {
		for (var i=0; i < ele.length; i++) {
			if (ele[i].style.display == "none") {
				ele[i].style.display = "block";
			} else {
				ele[i].style.display = "none";
			}
		}
	}
}

function changeTabRandom(prefix) {
