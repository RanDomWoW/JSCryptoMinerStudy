   //console.log("商城在此取得回傳推薦商品資料後，動態於網頁中組合與呈現推薦商品的程式碼");
   //console.log(products);
   
   if(products === undefined || products.length === 0) {
     return;
   }
   
   // 推薦１-首頁建議版位１
   function appendProduct(product, idx, target) 
   {
      var product_li = document.createElement("li"),
            click_url_a = document.createElement("a"),
              img = document.createElement("img"),
            title_p = document.createElement("p"),
              title_a = document.createElement("a");
      
      // 
      title_a.href = product.clickurl;
      title_a.innerHTML = product.title;
      title_a.setAttribute("target","_blank");
      
      //
      img.src = product.imgurl;
      img.setAttribute("border","0");
      
      click_url_a.href = product.clickurl;
      click_url_a.setAttribute("target","_blank");
      
      // assembly node
      product_li.appendChild(click_url_a);
      click_url_a.appendChild(img);
      
      product_li.appendChild(title_p);
      title_p.appendChild(title_a);
            
      // append to target    
      target.appendChild(product_li);
   } 
      
   var target1 = document.getElementById("personalRecommend1");   
   var i =  ((function(a,b) { return a > b ? b : a; })(products[0].length - 1, 9));
   while(i >= 0) {
      appendProduct(products[0][i], i+1, target1);
      i -= 1;
   } 
   


   // 推薦２-首頁建議版位２
   function appendProduct2(product, idx, target) 
   {
      var click_url_a = document.createElement("a"),
            cover_div = document.createElement("div"),            
              img = document.createElement("img"),
            title_h3 = document.createElement("h3"),
            desc_p = document.createElement("span");

      img.src = product.imgurl;
      img.alt = product.title;
      img.setAttribute("class","mainbook_box_cover");

      cover_div.setAttribute("class", "mainbook_box_covershaw");
      title_h3.setAttribute("class", "suggestion");
      
      title_h3.innerHTML = product.title;
      desc_p .innerHTML = product.subtitle;
            
      click_url_a.href = product.clickurl;
      click_url_a.setAttribute("class","mainbook_box");
            
      // assembly node
      click_url_a.appendChild(cover_div);
      click_url_a.appendChild(title_h3);
      //click_url_a.appendChild(desc_p);
      cover_div.appendChild(img);
       
      // append to target
      //target.appendChild(click_url_a);
      target.insertAdjacentElement("beforebegin", click_url_a)
   }
   
   var target2 = document.getElementById("personalRecommend2Items");
   var target3 = target2.getElementsByTagName("fieldset")[0];
   var i =  ((function(a,b) { return a > b ? b : a; })(products[1].length - 1, 9));
   while(i >= 0) {
      appendProduct2(products[1][i], i+1, target3);
      i -= 1;
   }   

   $('.infiniteCarousel').infiniteCarousel();
    

   } // end:success
});
</script>
