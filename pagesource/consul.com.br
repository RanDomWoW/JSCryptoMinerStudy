jQuery( document ).ready(function(){
var cleed_visible = false,
cleed = jQuery( '#cLeed' );
//Controle de tempo de Leed
var cleed_time = new Date().getTime(),
cleed_calc = ( ( 3600 ) * 19 ) * 1000,
cleed_result =( cleed_time + cleed_calc );
cleed.each( function(){
cleed.find( '.button' ).on( 'click', function(){
cleed.removeClass( '-active' );
if ( hasLocalStorage() ) localStorage.cleed_result = cleed_result;
});
});
if ( hasLocalStorage() ){
if( !localStorage.cleed_result || cleed_time >= localStorage.cleed_result ) {
setTimeout( function () {
cleed_visible = true;
cleed.addClass( '-active' );
});
}else {
cleed_visible = false;
};
};
});
// função para validar se existe localStorage
function hasLocalStorage () {
try {
var mod = 'hasLocalStorage42';
localStorage.setItem(mod, mod);
localStorage.removeItem(mod);
return true;
} catch(e) {
return false;
}
}
</script>
