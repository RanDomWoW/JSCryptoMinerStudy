<script>
    jQuery(document).ready(function() {
        $(window).on('resize',function(){
            yerlestirmegaMenuSolDiv();
            menuScroll();
            bayrakVideosuAyarla();
        });
        menuScroll();
        yerlestirmegaMenuSolDiv();
        bayrakVideosuAyarla();
    });
    $('body').on('show.bs.modal', '.modal', function () {
        $(this).find('video').trigger('play');
    });
    $('body').on('hidden.bs.modal', '.modal', function () {
        $('video').trigger('pause');
    });
    function bayrakVideosuAyarla(){
        var pencere = window.innerWidth;
        if( pencere < 600 ){
            $('#bayrak').css('margin-top', '0');
        }else if(pencere > 600 && pencere < 1200){
            $('#bayrak').css('margin-top', '-50px');
        }else if(pencere > 1200 && pencere < 1600){
            $('#bayrak').css('margin-top', '-250px');
        }else if(pencere > 1600 && pencere < 1900){
            $('#bayrak').css('margin-top', '-200px');
        }else{
            $('#bayrak').css('margin-top', '-250px');
        }
    }
    function yerlestirmegaMenuSolDiv(){
        var position  = $('.megaMenuSolDivRepeat').offset();
        $('.megaMenuSolDiv').width( ( position.left + 34) + 'px');
    }
    function menuScroll(){
        var elemanlar = $('.menuScroll');
        var asxx = $('.barScroll');
        asxx.height('auto');
        elemanlar.height('auto');
        for( var i = 0; i < elemanlar.length; i++ ) {
            eleman = $(elemanlar[i]);
            if( eleman.height() > window.innerHeight - 100 ){
                eleman.height( window.innerHeight - 100 );
                eleman.find('.barScroll').height( eleman.height() - 50);
            }else{
                eleman.height('auto');
                eleman.find('.barScroll').height('auto');
            }
        }
    }
</script>
