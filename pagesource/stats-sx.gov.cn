<!-- 检索跳转 --> 
<script>
	/*过滤特殊字符*/
function stripscript(s) {
    var pattern = new RegExp("[`~!@#$^&*()=|{}':;',\\[\\].<>/?~！@#￥……&*（）&mdash;—|{}【】‘；：”“'。，、？]")
         var rs = "";
    	 for (var i = 0; i < s.length; i++) 
    	 	{
         		rs = rs + s.substr(i, 1).replace(pattern, '');
    		}
    		return rs;
	}
	/*特殊字符报错*/
	
	 function textCounter(maxlimit)
	  {
	  		var text=$('#j_search').val();
	  		var len=text.length;
	  		if(len>maxlimit)
	  		{
	  			var text1=text.substring(0,maxlimit);
	  			$('#j_search').val(text1);
	  		} 
	  }
function checks(t)
	{   
                              
        t = t.replace(/(^\s+)|(\s+$)/g,""); 
		if(t=="" || t=="请输入关键字检索")
		{
			alert('请输入有效内容');
			return false;
		}

	   szMsg="[#_%&'/",";:=!^]";
	   alertStr="";
	   for(i=1;i<szMsg.length+1;i++)
	   {
	   	 if(t.indexOf(szMsg.substring(i-1,i))>-1)
	   	 	{
	    		 alertStr="请勿包含非法字符如[#_%&'/",";:=!^]";
	    		 alert(alertStr);
	     	 	 // break;
	     	 	 return false;
	    	}
	   }
	    var searchBtn = document.getElementById("j_search_btn");
        var key = document.getElementById("j_search").value;
        /*window.location.href = "http://new.sousuo.gov.cn/s.htm?t=gov&q=" + key; */
        window.location.href="http://www.stats-sx.gov.cn/was5/web/search?channelid=248927&perpage=&templet=&token=16.1442975927773.20&searchword="+key;
	   	return true;
	  }
   	/*字数限制函数*/
	$(function()
	{
		$('#j_search_btn').click(function()
		{

			var str=$(this).prev().val();
			checks(str);
			
		});
	});
</script>
