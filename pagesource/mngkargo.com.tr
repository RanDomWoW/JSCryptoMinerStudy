                 $('#titlePOPUP').attr("href", "https://itunes.apple.com/tr/app/mng-kargo/id381676782?mt=8");
                return 'iOS';

            }
            else if( userAgent.match( /Android/i ) )
            {
                var windSize = $( window ).width();  

			     $('.mobileFeedBox').show();
                 $('#titlePOPUP').text("ANDROID Uygulamamızı indirmek istermisiniz?");
