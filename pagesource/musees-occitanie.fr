<script language="javascript" type="text/javascript">

function formulaire_menu_musees()
	{
	// récupération des données
	l_menu_musee_departement = document.getElementById("f_menu_musees_departement").value ;
	l_menu_musee_theme = document.getElementById("f_menu_musees_theme").value ;
	l_menu_musee_nom = document.getElementById("f_menu_musees_nom").value ;
	// traitement des valeurs par défaut
	if ( l_menu_musee_departement == "" )
		{
		l_menu_musee_departement = "region" ;
		}
	if ( l_menu_musee_theme == "" )
		{
		l_menu_musee_theme = "tous" ;
		}
	if ( l_menu_musee_nom == "nom du musée ou de la ville" )
		{
		l_menu_musee_nom = "" ;
		}
	// si les trois valeurs ne sont pas vides
	if ( l_menu_musee_nom+l_menu_musee_departement+l_menu_musee_theme != "" )
		{
		l_url = "/musees/" ;
		if ( ( l_menu_musee_departement != "region" ) || ( l_menu_musee_theme+l_menu_musee_nom != "tous" ) )
			{
			l_url += "liste/"+l_menu_musee_departement+"/" ;
			}
		if ( ( l_menu_musee_theme != "tous" ) || ( l_menu_musee_nom != "" ) )
			{
			l_url += l_menu_musee_theme+"/" ;
			}
		if ( l_menu_musee_nom )
			{
			l_url += encodeURI(l_menu_musee_nom)+"/" ;
			}
		document.location.href = l_url ;
		}
	return false ;
	}

</script>

<form action="/musees/" method="post" onsubmit="return formulaire_menu_musees();">
