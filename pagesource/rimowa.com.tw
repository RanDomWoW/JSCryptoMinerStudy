<script type="text/javascript">
  $(document).ready(function(){
	var speed = 300;
    var indx = -1;
    var count01 = 0;

	  var delay = (function(){
	    var timer = 0;
	    return function(callback, ms){
	      clearTimeout (timer);
	      timer = setTimeout(callback, ms);
	    };
	  })();

		$(".mainMenu > li .main_xmenu01").hover(function(){
		     if(count01 == 1){
						  var idx01 = $(".mainMenu > li .main_xmenu01").index(this);
						  var clstr = $(".mainMenu > li").eq(idx01).attr('class');		     
						delay (function () {
						  $(".mainMenu > li").children("div").slideUp(speed);
						  $(".mainMenu > li").eq(idx01).children("div").slideDown(speed);	    
						}, 500);
		     }
			   if(parseInt(indx) != parseInt($(".mainMenu > li .main_xmenu01").index(this)) && count01 == 0){
			   	count01++;
			   	  var idx01 = $(".mainMenu > li .main_xmenu01").index(this);
				    var clstr = $(".mainMenu > li").eq(idx01).attr('class');
				    if(clstr == "menuBlog"){
				    	$(".mainMenu > li").children("div").slideUp(speed);
				    }else{
						  $(".mainMenu > li").children("div").slideUp(speed);
						  $(".mainMenu > li").eq(idx01).children("div").slideDown(speed);	    
				    }
			  }
			   if(indx == '' || parseInt(indx) != parseInt($(".mainMenu > li .main_xmenu01").index(this))){
			    indx = $(".mainMenu > li .main_xmenu01").index(this);
			   }
			   
		},function(){
				delay (function () {
				    count01 = 0;
				}, 100);
		});

		$("#header").hover(function(){
		},function(){
				$(".mainMenu > li").children("div").slideUp(speed);
				count01 = 0;
				indx = -1;
		});

    var xshopvw_time = 7; //顯示時間
    var xshopck01 = 0;
    $.extend({
        shopvw01_div:function(vct01){
            //$('#indicator').html(vct01); //顯示計數
             if(xshopck01 == 1){
                vct01=0;
                xshopck01=xshopck01+1;
            }
            if(xshopck01 == 3 || vct01 >= 50){
                vct01=0;
            }
            if(vct01 >= xshopvw_time && xshopck01 > 0){
              vct01=0;
              $.xshowshop_vw01('close');
            }
            setTimeout("$.shopvw01_div("+(vct01+1)+")",1000);
        }
    });
    $.shopvw01_div(0);

  $(document).click(function(e){
    $.xshowshop_vw01('close');
	});

	$("#seachBar ul").click(function(e) {
	  e.stopPropagation();
	});

 $(".edmDiv ul").hover(function(){
  	 xshopck01 = 3;
  },function(){
  	 xshopck01 = 5;
  });

    $.extend({
        xshowshop_vw01:function(event01){
        if($.trim($("#ordmail").val()) != ''){
        	var vwstr = $(".edmDiv ul").css('display');
				  if(event01 == "close"){
				    if(vwstr == "block"){
				      $(".edmDiv ul").slideUp("slow");
				    }
				  }else{			  
					  var urlstr = "ordmail_check.php?opt=ordmail&ordmail="+$.trim($("#ordmail").val());
					  $.ajax({
					    url: urlstr,
					    type: "GET",
					    cache: false,
					    dataType: "html",
					    error:function (xhr, ajaxOptions, thrownError){
					      alert(xhr.status);
					      alert(xhr.responseText);
					      alert(thrownError);
					    },
					    success : function(var1){
					      $(".edmDiv ul").html(var1);
					    }
					  });
					  if(event01 == "auto"){
					    if(vwstr == "none"){
					      xshopck01 = 1;
					      $(".edmDiv ul").slideDown("slow");
					      $(".edmDiv ul").setfocus();
					    }else{
					    	if($("#ordmail").attr("value") == "請輸入E-Mail")
					        $(".edmDiv ul").slideUp();
					    }
					  }else{
					    if(vwstr == "none"){
					      xshopck01 = 1;
					      $(".edmDiv ul").slideDown();
					      $(".edmDiv ul").setfocus();
					    }
				    }
				  }
				}
        }
    });

  $("#ordmail").focus(function () {
	  if($("#ordmail").attr("value") == "" || $("#ordmail").attr("value") == "請輸入E-Mail" )
	    $("#ordmail").attr("value","");
  });

  $("#ordmail").blur(function () {
  	if($("#ordmail").attr("value") == "")
	    $("#ordmail").attr("value","請輸入E-Mail");
  });

  $('#ordmail').click(function(e) {
	  e.stopPropagation();
  });

  $('#ordmailbt01').click(function(e) {
	  e.stopPropagation();
	  $.xshowshop_vw01('auto');
  });

  });
</script>

