<script type="text/javascript">
  // Objeto Mapa
var map;
function initialize() {
    var mapOptions = {
        zoom: 16,
        mapTypeId: google.maps.MapTypeId.ROADMAP,
        disableDoubleClickZoom: false,
        draggable: true,
        scrollwheel: false
    };

    // Define o mapa, chamado map.
    map = new google.maps.Map(document.getElementById('map-canvas'),mapOptions);
    // Cria o marcador
    var myPosition = new google.maps.LatLng(-3.7396312,-38.5249362,20);

    // Cria o objeto (myMarker)
    var myMarker = new google.maps.Marker({
        animation: google.maps.Animation.DROP,
    draggable: false,	
    position: myPosition,
    map: map,
    title: 'Você!'
    });
    // Centraliza o Mapa na sua posição
    map.setCenter(myPosition); 
}
$(document).ready(function(){
    initialize();
});
</script>

<script type="text/javascript">
  $(document).ready(function(){
    $('#defaultReal').realperson({}); 
    $(".telefone").mask("(00)0000-0009");	
    
  });

  $("#nome").focus(function() {
    $("#nome").next().removeClass("erro");
  });

  $("#email").focus(function() {
    $("#email").next().removeClass("erro");
  });

  $( "#telefone" ).focus(function() {
    $("#telefone").next().removeClass("erro");
  });

  $( "#estado" ).focus(function() {
    $("#estado").next().removeClass("erro");
  });   

  $( "#cidade" ).focus(function() {
    $("#cidade").next().removeClass("erro");
  }); 

  $( "#assunto" ).focus(function() {
    $("#assunto").next().removeClass("erro");
  }); 

  $( "#mensagem" ).focus(function() {
    $("#mensagem").next().removeClass("erro");
  }); 
  
  
  
  
  $('#bt_enviar').click(function (){ 

    var email = $("#email").val();	
    var nome = $("#nome").val();                                  
    var mensagem = $("#mensagem").val();
    var assunto = $("#assunto").val();
    var cidade = $("#cidade").val();
    var estado = $("#estado").val();
    var telefone = $("#telefone").val();
    var url = "";

    if(email == ""){
      //$("#email2").next().addClass("erro");
       alert("Preencha o campo E-mail!");
    }     
    if(nome == ""){
      //$("#nome2").next().addClass("erro");
      alert("Preencha o campo Nome!");
    }     
    if(mensagem == ""){
      //$("#mensagem2").next().addClass("erro");
      alert("Preencha o campo Mensagem!");
    }     
    if(assunto == ""){
      //$("#assunto2").next().addClass("erro");
      alert("Preencha o campo Assunto!");
    }  
     if(cidade == ""){
      //$("#cidade2").next().addClass("erro");
       alert("Preencha o campo Cidade!");
    } 
     if(estado == ""){
      //$("#estado2").next().addClass("erro");
      alert("Preencha o campo Estado!");
    } 
    if(telefone == ""){
      //$("#telefone2").next().addClass("erro");
      alert("Preencha o campo Telefone!");
    } 
    
    if(assunto == 'FDR'){
      url = 'http://app.opovo.com.br/apps,142,68/fale-conosco-fdr';
    }

    if(assunto == 'UANE'){
      url = "http://app.opovo.com.br/apps,142,73/fale-conosco-uane"
    }

    if(assunto == 'Projetos'){
      url = "http://app.opovo.com.br/apps,142,81/fale-conosco-projetos"
    }

    if(assunto == 'EDR'){
      url = "http://app.opovo.com.br/apps,142,79/fale-conosco-edr"
    }

    if(assunto == 'TV'){
      url = "http://app.opovo.com.br/apps,142,80/fale-conosco-tv"
    }
    
    

    if((nome != "") && (email != "") && (mensagem != "") && (assunto != "") && (cidade != "") && (estado != "") && 

(telefone != "")){    
      $.getJSON2Script("http://app.opovo.com.br/apps,142,26/new-captcha",{defaultRealHash:$("input[name=defaultRealHash]").val(), defaultReal:$("#defaultReal").val()}
        ,function(res){           
            if(res['ok']['ok'] === 1){            
                $("#exifviewer-img-9").show();
                $("#textBt2").hide();
                $.getJSON2Script(url,

{email:email,nome:nome,mensagem:mensagem, assunto:assunto, cidade: cidade, estado:estado, telefone:telefone},
                  function(ret){	                  
                    if(ret['ok'] === 0){
                      alert("Email enviado com sucesso!");                    
                      /*$("#exifviewer-img-9").hide();
                      $("#textBt2").show();*/
                      $('#defaultReal').removeAttr('value');                      
                      $('input').attr('value','');
                    }
                    else {
                      alert("Ops, ocorreu um erro. Tente novamente mais tarde!");
                    }
                  }
                ,'json');                             
            }           
         }
      );
    }  
    return false; 
  });

</script>
