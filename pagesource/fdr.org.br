<script type="text/javascript">
  //expandir super banner
  function expandSuperbanner(altura){
      $('.case-sb div').css('height',altura+'px').css('overflow','');
  }
  //contrair super banner
  function contractSuperbanner(){
      $('.case-sb div').css('height','90px').css('overflow','hidden');
  }
  //expandir super banner html5
  function expandSuperbannerHtml5(altura){
      $('.case-sb div').css('overflow','visible');
  }
  //contrair super banner html5
  function contractSuperbannerHtml5(){
      $('.case-sb div').css('overflow','hidden');
  }  
  
  $(function(){
    $('body').find('.banner').each(function(){
        //var b = 0;
        //$(this).find('div').each(function(){ b = b + 1 });
        //if(b == 0){ $(this).hide(); }
        if(!$(this).find('img').length && (!$(this).find('embed').length || !$(this).find('object').length))
        {
                $(this).hide();
            }    

            //EXIBE BANNER SE EXISTIR UM (EMBED OU OBJECT) 
            if($(this).hasClass('bannerBarrafull'))
            {
                if($(this).find('embed').length || $(this).find('object').length)
                {
                    $(this).show();
                    $('.bgBarrafull').show();
                }
            }


    });
    $('#geral').find('.vitrineFiat').each(function(){
        var vf = 0;
        $(this).find('div div').each(function(){ vf = vf + 1 });
        if(vf == 0){ $(this).hide(); }
    });

    $('#geral').find('.bannerVertical').each(function(){
        var bv = 0;
        $(this).find('div').each(function(){ bv = bv + 1 });
        if(bv == 0){ $('.adsense').hide(); }
    });

    $('.bannerDhtml a').find('img').each(function(){
      if(this.width > 76){
        $('.bannerDhtml').css('display', 'block');
      }
    });

    //Verificar a existencia de dhtml, caso exista, exibir.
    //versão para todos os navegadores, menos o IE
    $('.bannerDhtml').find('embed').each(function(){
        $('.bannerDhtml').css('display', 'block');
    });

    $('.bannerDhtml').find('iframe').each(function(){
        $('.bannerDhtml').css('display', 'block');
        setTimeout(function() {fechaDHTML();}, 25000);
    });

    //versão para o IE
    $('.bannerDhtml').find('object').each(function(){
        $('.bannerDhtml').css('display', 'block');
    });
    $('.bannerOutlimit a').find('img').each(function(){
      if(this.width > 76){
        $('.bannerOutlimit').css('display', 'block');
      }
    });
    $('.bannerOutlimit').find('#Stage').each(function(){
        $('.bannerOutlimit').css('display', 'block');
    });
    //Verificar a existencia de outlimit, caso exista, exibir.
    //versão para todos os navegadores, menos o IE
    $('.bannerOutlimit').find('embed').each(function(){
        $('.bannerOutlimit').css('display', 'block');
    });

    $('.bannerOutlimit').find('iframe').each(function(){
        $('.bannerOutlimit').css('display', 'block');

        if (!$('.bannerOutlimit iframe').hasClass('video')) {
            setTimeout(function() {fechaOutlimit();}, 15000);
        }     

    });

    //versão para o IE
    $('.bannerOutlimit').find('object').each(function(){
        $('.bannerOutlimit').css('display', 'block');
    });
    $('.superBanner').find('object, embed, iframe').each(function(){
      if ($(this).css("height") == "300px"){
        $('.superBanner div').css('height','90px').css('overflow','hidden');
      }
    }); 

});
  
</script>
