<script>
    $(function() {

        window.__codice = null;
        window.__prev = null;

        window.setInterval(function() {
            if (window.__prev) {
                $("#flash-nav-" + window.__prev).removeClass("flash-nav-current");
                window.__prev = null;
            }
            if (window.__codice && window.__codice != $(".flash-nav-active").data("codice")) {
                $("#flash-nav-" + window.__codice).addClass("flash-nav-current")
                $(".flash-nav-active").removeClass("flash-nav-current")
                adjustOffset($("#flash-nav-" + window.__codice));
            }
            else {
                $(".flash-nav-active").addClass("flash-nav-current")
                adjustOffset($(".flash-nav-active"));
            }
        }, 300);

        window.__flashNavTimeout = -1;
        $(".flash-nav").hover(
            function() {
                window.__codice = $(this).data("codice");
            },
            function() {
                if (window.__prev == null)
                    window.__prev = window.__codice; 
                window.__codice = null;
            });
        $(".nav-bar-item").hover(
            function() {
                window.__codice = $(this).data("codice");
            },
            function() {
                if (window.__prev == null)
                    window.__prev = window.__codice; 
                window.__codice = null;
            });
    });

    function adjustOffset(el) {
        var t = $(".item-" + el.data("codice"))
        if(t.length==0)return;
        var left = t.offset().left - t.parent().offset().left;
        left += (t.outerWidth() - el.find("a:first").outerWidth()) / 2; 
        el.find(".arrowunderline").css("margin-left", left + "px")
    }
</script>

