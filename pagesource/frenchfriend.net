<script  type="text/javascript">
var i,sbxt=0;
var csobj; // object to initialize 
var n=12, datou=1, zongkuan=982, buchang=4, jiange=200;
var startpos=300;

var xjdv =new Array(n+1);
var xjdvkd= new Array(n+1);
var xjdvzb=new Array(n+1);
var xjdvzht=new Array(n+1);

xjdv[1]="Div1"; xjdv[2]="Div2"; xjdv[3]="Div3";xjdv[4]="Div4"; xjdv[5]="Div5";xjdv[6] = "Div6";
xjdv[7] = "Div7"; xjdv[8] = "Div8"; xjdv[9] = "Div9"; xjdv[10] = "Div10"; xjdv[11] = "Div11";xjdv[12] = "Div12";

xjdvkd[1] = 242; xjdvkd[2] = 479; xjdvkd[3] = 480; xjdvkd[4] = 480; xjdvkd[5] = 476; xjdvkd[6] = 474;
xjdvkd[7] = 474; xjdvkd[8] = 481; xjdvkd[9] = 480; xjdvkd[10] = 480; xjdvkd[11] = 480; xjdvkd[12] = 335;

function xjdvzmd_onmouseover() {
sbxt=1;
}

function xjdvzmd_onmouseout() {
sbxt=0;
}

function xjinit()
{
  xjdvzb[1]=startpos;
  for (i=2; i<=n; i++)
  {xjdvzb[i]=xjdvzb[i-1]+xjdvkd[i-1];}
  for (i=1;i<=n;i++)
  {   
     csobj=document.getElementById(xjdv[i]);
      if (csobj.style.top!="0px") csobj.style.top="0px";
     if (xjdvzb[i]<=zongkuan-1) 
        {       
         csobj.style.left=xjdvzb[i]+"px"
        }
     else
        {
        csobj.style.left=zongkuan+"px"
        }   
  }
}

function xjyd()
{
 var dangqian, dqduiwei,xindatou;
 var dqobj;
 if (sbxt==0)
   { 
         for (i=1; i<=n; i++)
           {
             dangqian=i+datou-1;
             if (dangqian>n) dangqian=dangqian-n;
             xjdvzb[dangqian]=xjdvzb[dangqian]-buchang;
             xjdvzht[dangqian]=2;  //2= in the display area
             if (xjdvzb[dangqian]+xjdvkd[dangqian]<0) xjdvzht[dangqian]=1; //1= out of the left side
             if (xjdvzb>zongkuan-1) xjdvzht[dangqian]=3; //3= out of the right side
            } 
             // complete moveleft
           xindatou=datou; // very important
         for (i=1;i<=n; i++)
          {
           dangqian=i+datou-1;
           if (dangqian>n) dangqian=dangqian-n;
           dqduiwei=dangqian-1;
           if (dqduiwei==0) dqduiwei=n; 

           if (xjdvzht[dangqian]==1) 
             {
              if (xjdvzb[dqduiwei]+xjdvkd[dqduiwei] >zongkuan-1)      
              xjdvzb[dangqian]=xjdvzb[dqduiwei]+xjdvkd[dqduiwei]; 
              else
              xjdvzb[dangqian]=zongkuan;
              //keep the xjdvzht=1(out of the left side)
              xindatou=dangqian+1;
              if (xindatou>n) xindatou=1;
             }
          }
          // when out of left side, then move to then end
          
          //begin to render
         
         for (i=1;i<=n; i++)
          {
           dangqian=i+datou-1;
           if (dangqian>n) dangqian=dangqian-n;
           dqobj=document.getElementById(xjdv[dangqian]);
          
          
           if (xjdvzht[dangqian]==1 || xjdvzht[dangqian]==3) dqobj.style.left=zongkuan.toString()+"px";
           if (xjdvzht[dangqian]==2) dqobj.style.left=xjdvzb[dangqian].toString()+"px";
          }
         
           datou=xindatou; //very important
   }  //end if sbxt
 setTimeout("xjyd()",jiange); 
}


</script>

