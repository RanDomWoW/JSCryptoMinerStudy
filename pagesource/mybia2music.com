				$(function(){
					$(".arrow").click(function() {
						if ($(this).hasClass("active")) {
							$(this).removeClass("active");
							$("#fxplayer").animate({bottom:"-121px"});
							$(this).children("i").removeClass("fa-angle-double-down");
							$(this).children("i").addClass("fa-angle-double-up");
						}
						else {
							$(this).addClass("active");
							$("#fxplayer").animate({bottom:"0px"});
							$(this).children("i").removeClass("fa-angle-double-up");
							$(this).children("i").addClass("fa-angle-double-down");
						}
					} );
					move = 0;
				$("#jp_player2").jPlayer({
					ready: function () {
					  $(this).jPlayer("setMedia", {
						mp3:"http://sv2.mybia2music.com/s2/Music/1397/03/02/Milad%20Baran%20-%20Mane%20Divane%20%5b128%5d.mp3"
					  });
					  },
					  loadeddata: function(event){ // calls after setting the song duration
							$("#fxplayer #durationtime").text($.jPlayer.convertTime(event.jPlayer.status.duration));
						},
					  timeupdate: function(event) {
							$("#fxplayer .seek .seekbar").css("width",event.jPlayer.status.currentPercentAbsolute+"%");
							$("#fxplayer #time").text($.jPlayer.convertTime(event.jPlayer.status.currentTime));
						},
					  play: function(event) {
							$("#fxplayer #time").text($.jPlayer.convertTime(event.jPlayer.status.duration));
							$("#fxplayer .fa-play").addClass("fa-pause");
							$("#fxplayer .fa-play").removeClass("fa-play");
						},
						pause: function(event) {
							$("#fxplayer .fa-pause").addClass("fa-play");
							$("#fxplayer .fa-pause").removeClass("fa-pause");
						},
						swfPath: "http://www.mybia2music.com/wbh/wp-content/themes/bia2music_v444/jplayer",
						supplied: "mp3",
						cssSelectorAncestor:"#player",
						wmode: "window",
						preload:"none"
						
				});
				$("#fxplayer .fa-repeat").click( function() {
					if ( $(this).hasClass("active") ) {
						$(this).removeClass("active");
						$("#jp_player2").unbind($.jPlayer.event.ended + ".jp-repeat");
					}
					else {
						$(this).addClass("active");
						$("#jp_player2").bind($.jPlayer.event.ended + ".jp-repeat", function(event) { 
							$(this).jPlayer("play"); 
						});
					}
				 });
				$("#fxplayer .fa-play").click(function(){
					if($(this).hasClass("fa-pause")){
						$("#jp_player2").jPlayer("pause");
					}
					else {
						$("#jp_player2").jPlayer("play");	
					}
				});
				$("#fxplayer .fa-backward").click(function(e){
				   var where = (parseInt($("#jp_player2").data('jPlayer').status.currentTime)-5);
					$("#jp_player2").jPlayer("play",where);
				});
				$("#fxplayer .fa-forward").click(function(e){
					var where = (parseInt($("#jp_player2").data('jPlayer').status.currentTime)+5);
					$("#jp_player2").jPlayer("play",where);
				});
				$("#fxplayer .seek").click(function(e){
				// Get div offset position (top and left)
				var offset = $(this).offset();
				// Get the full width of the seek bar
				var totalWidth = $(this).outerWidth();
				// Get seek bar click position 
				var left = e.pageX - offset.left;
				// Get the track total duration
				var totalDuration = $("#jp_player2").data("jPlayer").status.duration;
				// Get the current time position 
				var currentPosition = (left * totalDuration) / totalWidth;
				// Set the player to start for the new psotion
				$("#jp_player2").jPlayer("play", currentPosition);
				});
				$("#fxplayer .seek_volume").click(function(e){
				x = e.pageX-$(this).offset().left;
				go = (x*100)/100;
				vol = go/100;
				$(this).children(".seekbar_volume").animate({"width":go+"%"});
				$("#jp_player2").jPlayer( "volume",vol);
				});
				});
			</script>

