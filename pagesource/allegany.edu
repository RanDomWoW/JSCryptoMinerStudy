    var output = "";
	var nothingposted = "Nothing Posted!";
    var i;

    for(i = 0; i<arr.length; i++) {
			
			var title = arr[i].title;
			var annlink = arr[i].annlink;
			var image = arr[i].image;
			var url = 'https://services.allegany.edu/acmnow/'+image+'';
			var description = arr[i].description;

output+='<div class="event"><div class="nowimage"><img src="'+url+'" alt="Announcement Arrow" width="50" height="50" /></div><div class="nowitemtitle"><h3><a href="'+annlink+'" target="_parent">'+title+'</a></h3></div><div class="nowdescription"><p>'+description+'</p></div></div>';

    }


	document.getElementById("acmannouncementdata").innerHTML=output;
}
</script>
