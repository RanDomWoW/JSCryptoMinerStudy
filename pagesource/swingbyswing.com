  $(document).ready(function(){
    $("#hide").click(function(){
      var x = document.getElementById('hide');
      var y = document.getElementById('show');
      if (x.style.display === 'block') {
        $("#hide").hide();
        $("#show").show();
        y.style.display = 'block';
      }

    });

    $("#show").click(function(){
      var x = document.getElementById('show');
