            <script type="text/javascript">
                //<![CDATA[

                //hack for expected behavior of facebook login process
                //https://developers.facebook.com/bugs/318390728250352/ - here is description of this bug
                if (window.location.hash == '#_=_')
                {
                    window.location.href = window.location.href.replace('#_=_', '');
                }

                $(document).ready(function($)
                {
                    var csrftoken = $.cookie('csrftoken');
                    function csrfSafeMethod(method)
                    {
                        // these HTTP methods do not require CSRF protection
                        return (/^(GET|HEAD|OPTIONS|TRACE)$/.test(method));
                    }

                    $.ajaxSetup({
                        crossDomain: false, // obviates need for sameOrigin test
                        beforeSend: function(xhr, settings)
                        {
                            if (!csrfSafeMethod(settings.type))
                            {
                                xhr.setRequestHeader("X-CSRFToken", csrftoken);
                            }
                        }
                    });

                    $(".language-dropdown .dropdown-menu a, .suggestion-site-popover a").click(function () {
                        var url = $(this).attr("href");
                        var haveGetParams = (url.indexOf('?') != -1);
                        var concatSymbol = haveGetParams ? "&" : "?";

                        var crossDomainParams = {
                            ssp: 1
                        };

                        var pv = $.cookie("pv");

                        if (pv !== null) {
                            crossDomainParams.pv = pv;
                        }

                        var MDS_UPID = $.cookie("MDS_UPID");
                        var MDS_UPVK = $.cookie("MDS_UPVK");

                        if (MDS_UPID !== null && MDS_UPVK !== null)
                        {
                            crossDomainParams.MDS_UPID = MDS_UPID;
                            crossDomainParams.MDS_UPVK = MDS_UPVK;
                        }

                        var redirectUrl = url + concatSymbol;
                        if (logasterNS.urlParams.length > 0)
                        {
                            redirectUrl += logasterNS.urlParams + "&";
                        }
                        redirectUrl += $.param(crossDomainParams);
                        window.location.href = redirectUrl;

                        return false;
                    });

                    
    $("a.create-logo").click(function()
    {
        logasterTrack.trackGA("HomePage", $(this).attr("ga-action"), $(this).attr("ga-data"));
    });

    var ratingCookiePrefix = 'rated_';
    $(".gallery-logotype-info .score").each(function(idx, el)
    {
        $(el).raty({
            path: 'https://s1.logaster.com/static/images/stars/', //refactor this to avoid hardcoded urls
            half: true,
            readOnly:  $.cookie(ratingCookiePrefix + $(el).data('id')) == 'true',
            hintList: ["malo","pobre","regular","bueno","hermoso"],
            start: function()
            {
                return $(this).data('score');
            },
            click: function(score, event)
            {
                var ratyContainer = $(this);
                var galleryLogotypeId = ratyContainer.data('id');
                if ($.cookie('rated_' + galleryLogotypeId) != 'true')
                {
                    $.post(
                        'https://www.logaster.com.es/gallery/set_gallery_score/',
                        {'galleryLogotypeId': galleryLogotypeId, 'score': score},
                        function(data)
                        {
                            ratyContainer.raty('start', data.score);
                            ratyContainer.raty('readOnly', true);
                            $.cookie('rated_' + galleryLogotypeId, 'true', { expires: 1 * 365 * 10, path: '/' }); //10 years cookie
                        }
                    );
                }
            }
        });
    });

    $(".comments .score").each(function (idx, el)
    {
        $(el).raty({
            path: 'https://s2.logaster.com/static/images/stars/', //refactor this to avoid hardcoded urls
            half: true,
            readOnly: true,
            hintList: ["malo", "pobre", "regular", "bueno", "hermoso"],
            start: function()
            {
                return $(this).data('score');
            }
        });
    });

    var logoCounter = {
        run: function(realLogoCount)
        {
            logoCounter.logoCount = realLogoCount - 40;
            logoCounter.realLogoCount = realLogoCount;
            logoCounter.animate();
        },
        animate: function()
        {
            logoCounter.setLogoCount(logoCounter.logoCount++);
            var timeout = 100;

            if (logoCounter.logoCount >= logoCounter.realLogoCount) return;

            if (logoCounter.logoCount < logoCounter.realLogoCount - 20)
            {
                timeout = (logoCounter.realLogoCount - logoCounter.logoCount - 20);
                timeout = 40 * (20-timeout);
            }
            else
            {
                timeout = (Math.floor(Math.random() * (7)) + 2)*1000;
            }

            //var timeout = logoCounter.logoCount < logoCounter.realLogoCount - 20?100:Math.floor(Math.random() * (7)) + 2*1000;
            window.setTimeout(logoCounter.animate, timeout);
        },

        setLogoCount: function(value)
        {
            $("#logo-count").text(logoCounter.addCommas(value));
        },
        addCommas: function(nStr)
        {
            nStr += '';
            x = nStr.split('.');
            x1 = x[0];
            x2 = x.length > 1 ? '.' + x[1] : '';
            var rgx = /(\d+)(\d{3})/;
            while (rgx.test(x1))
            {
                x1 = x1.replace(rgx, '$1' + ',' + '$2');
            }
            return x1 + x2;
        }
    };

    logoCounter.run(7542135);

    $('#gallery-carousel').on('slide.bs.carousel', function(event)
    {
        loadImages('#gallery-carousel .gallery-logotype img:not(.loaded)');
    });

    function loadImages(elements)
    {
        $(elements).each(function(idx, el)
        {
            if (idx < 6)
            {
                $(el).attr('src', $(el).data('src')).load(function()
                {
                    $(el).addClass('loaded');
                    $(el).css('background', 'none')
                });
            }
        });
    }

    loadImages('#gallery-carousel .gallery-logotype img:not(.loaded)');

    $(window).bind('load resize', function()
    {
        if ($(window).width() < 992)
        {
            $('#gallery-carousel').carousel('pause');
        }
        else
        {
            $('#gallery-carousel').carousel('cycle');
        }
    })

