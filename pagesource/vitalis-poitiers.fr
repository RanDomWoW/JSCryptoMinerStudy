<script type="text/javascript">

  function RecupStopArea()
  {
    if(($("#lineExternalCode").val()!="") && ($("#sens").val()!=""))
    {
      loadingScreen();
      var appelUrl = "/layout/set/rss/horaires/recherche?isAjaxCall=true&sens="+ $('#sens').val()+"&typeList=RoutePointList&lineExternalCode=" + $("#lineExternalCode").val();
      $.ajax({url: appelUrl, async: true, success: function(html) {
        // Success
        $("#lineStopAreaCode").replaceWith(html);
        clearLoadingScreen();
      }, error: function() {
        // Fail
        clearLoadingScreen();
      }});
     }
  }
  function RecupSens()
  {
    if($("#lineExternalCode").val()!="")
    {
      $('#lineStopAreaCode').attr("disabled","disabled");
      $('#lineStopAreaCode').val('');
      loadingScreen();
      var appelUrl = "/layout/set/rss/horaires/recherche?isAjaxCall=true&lineExternalCode=" + $("#lineExternalCode").val();
      $.ajax({url: appelUrl, async: true, success: function(html) {
        // Success
        $("#sens").replaceWith(html);
        $("#sens").bind('change',RecupStopArea);
        clearLoadingScreen();
      }, error: function() {
        // Fail
        clearLoadingScreen();
      }});
     }
  }

  function checkItiForm () {
    var dep = $('#Departure').val();
    var arr = $('#Destination').val();
    var error = '';
    if(dep.trim() == '') {
      error += 'Veuillez saisir un d&eacute;part';
      $('#Departure').addClass('error');
    } else {
      $('#Departure').removeClass('error');
    }

    if(arr.trim() == '') {
      if(error == '') {
        error += 'Veuillez saisir une arriv&eacute;e';
      } else {
        error += ' et une arriv&eacute;e';
      }
      $('#Destination').addClass('error');
    } else {
      $('#Destination').removeClass('error');
    }

    return displayError(error, 'error-iti');
  }

  function checkHoraireForm () {
    var ext = $('#lineExternalCode').val();
    var sens = $('#sens').val();
    var stop = $('#stopAreaExternalCodes').val();
    var error = '';

    if(ext.trim() == '') {
      error += 'Veuillez choisir une ligne, un sens et un arr&ecirc;t';
      $('#lineExternalCode').addClass('error');
      $('#sens').addClass('error');
      $('#lineStopAreaCode').addClass('error');
      return displayError(error, 'error-horaire');
    } else {
      $('#lineExternalCode').removeClass('error');
      $('#sens').removeClass('error');
      $('#lineStopAreaCode').removeClass('error');
    }

    if(sens.trim() == '') {
      if(error == '') {
        error += 'Veuillez choisir un sens et un arr&ecirc;t';
      } else {
        error += ', un sens et un arr&ecirc;t';
      }
      $('#sens').addClass('error');
      $('#lineStopAreaCode').addClass('error');
      return displayError(error, 'error-horaire');
    } else {
      $('#sens').removeClass('error');
      $('#lineStopAreaCode').removeClass('error');
    }

    if(stop.trim() == '') {
      if(error == '') {
        error += 'Veuillez choisir un arr&ecirc;t';
      }
      $('#lineStopAreaCode').addClass('error');
    } else {
      $('#lineStopAreaCode').removeClass('error');
    }

    return displayError(error, 'error-horaire');
  }

  /**
   * Affiche les erreurs dans la div souhaitée
   * @param error string : l'erreur à afficher
   * @param divID string : la div contenant le message d'erreur à afficher
   **/
  function displayError(error, divID) {
    if (error != '') {
      $('#' + divID + ' span').remove();
      $('#' + divID).append('<span>' + error + '</span>');
      $('#' + divID).addClass('attention');
      return false;
    }
    return true;
  }

  $(document).ready(function() {
    $("#lineExternalCode").bind('change',RecupSens);
    $("#sens").bind('change',RecupStopArea);
    $('#submit-iti').bind('click', checkItiForm);
    $('#submit-horaire').bind('click', checkHoraireForm);
  });
  jQuery('#Departure').attr('autocomplete', 'off');
  jQuery('#Destination').attr('autocomplete', 'off');

  var opt = {
      'access':'CTPPRODVITALIS',
      'css_url':'/extension/poitiers/design/poitiers/stylesheets/FirstLetter.css'
  };

  var dep_msw = new MswFLetter(opt);
  var arr_msw = new MswFLetter(opt);

  var dep_options_method = {'width':300,'max_items':10,'filter':'StopArea;Address;Site;City','loader_use':false,'formattype':'htmlV2insee', 'numberSearch' : true, 'numberSearchByParam' : 'externalcode' , 'numberSearchDisplay' : true , 'edtValue':true , 'edtTargetId' : 'edtDepartureRappel' , 'chk_delay' : 0, 'customCallBack' : true, 'boldResult' : true, 'buildInBlockId':'Departure_fl_container'};
  var arr_options_method = {'width':300,'max_items':10,'filter':'StopArea;Address;Site;City','loader_use':false,'formattype':'htmlV2insee', 'numberSearch' : true, 'numberSearchByParam' : 'externalcode' , 'numberSearchDisplay' : true , 'edtValue':true , 'edtTargetId' : 'edtDestinationRappel' , 'chk_delay' : 0, 'customCallBack' : true, 'boldResult' : true,'buildInBlockId':'Destination_fl_container'};

  dep_msw.buildFirstLetterOnInputTxt('Departure',dep_options_method);
  arr_msw.buildFirstLetterOnInputTxt('Destination',arr_options_method);
</script>

