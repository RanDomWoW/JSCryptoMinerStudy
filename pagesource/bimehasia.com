jQuery(window).scroll(function() {
	mobileElem1 = jQuery(".wNormal .MainMenu");
	mobileElem2 = jQuery(".wNormal .Logo");
	mobileElem3 = jQuery(".CuteUL");
	mobileElem4 = jQuery(".wNormal .CuteUL");
	mobileElem5 = jQuery(".MapContainer");
	mobileElem6 = jQuery(".InsInfo");
	
	if(jQuery(this).scrollTop() < 82)
	{
		mobileElem1.removeClass("Fix");
		mobileElem2.removeClass("Fix");
		mobileElem4.removeClass("Flow");
	}
	else
	{
		mobileElem1.addClass("Fix");
		mobileElem2.addClass("Fix");
		mobileElem4.addClass("Flow");
	}
	
	if(jQuery(this).scrollTop() < 2)
	{
		mobileElem3.removeClass("Flow");
		mobileElem5.removeClass("Drop");
	}
	else
	{
		mobileElem3.addClass("Flow");
		mobileElem5.addClass("Drop");
	}
	
	if(jQuery(this).scrollTop() < 110)
	{
		mobileElem6.removeClass("Scroll");
	}
	else
	{
		mobileElem6.addClass("Scroll");
	}
});

