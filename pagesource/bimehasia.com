function CheckWindowSize() {
    var width = jQuery(window).width(),
    ac = width > 1600 ? 'wLarge' :
		 width > 1200 ? 'w1200 wLarge' :
		 width > 1130 ? 'w1130 wNormal' :
		 width > 960 ? 'w960 wNormal' :
		 width > 800 ? 'w800 wSmall' :
		 width > 600 ? 'w600 wSmall' :
		 width > 350 ? 'w400 wSmall' : 'wMicro';
	mc = width > 960 ? 'NormalMenu' : 'HamMenu';
    jQuery('.All').removeClass('wLarge wNormal wSmall wMicro w1200 w1130 w960 w800 w600 w400').addClass(ac);
    jQuery('.MenuItemUL').removeClass('HamMenu').addClass(mc);
}
