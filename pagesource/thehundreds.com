$(document).ready(function() {
  var fsb_count = 0;
  var fsb_interval = setInterval(function() { check_fsb() }, 100);
  
  var close_html = '<svg version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="12.627px" height="12.627px" viewBox="0 0 12.627 12.627" enable-background="new 0 0 12.627 12.627" xml:space="preserve"><g><defs><rect id="SVGID_1_" width="12.627" height="12.627"></rect></defs><clipPath id="SVGID_2_"><use xlink:href="#SVGID_1_" overflow="visible"></use></clipPath><line clip-path="url(#SVGID_2_)" fill="none" stroke="#ffffff" stroke-miterlimit="10" x1="0.353" y1="0.354" x2="12.273" y2="12.273" class="xline"></line><line clip-path="url(#SVGID_2_)" fill="none" stroke="#ffffff" stroke-miterlimit="10" x1="12.273" y1="0.354" x2="0.353" y2="12.273" class="xline"></line></g></svg>';

  function check_fsb() {
    fsb_count++;
    if ($('#fsb_container').length > 0) {
      clearInterval(fsb_interval);
      $('#fsb_close').html(close_html);
    }
    if (fsb_count > 100) {
      // 10 seconds and no shipping bar still? just stop trying
      clearInterval(fsb_interval);
    }
  }
});
</script>
