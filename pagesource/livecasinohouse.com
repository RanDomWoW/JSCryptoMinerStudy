<script type="text/template-dot" id="js-dot-game-list-detail">
   {{~ it.games :game:index }}
    <div class="col mix games  {{= String(game.type).toLowerCase() }} {{~ game.tags :tag }}{{= tag.toLowerCase()+' ' }}{{~}} {{? game.user_favourite }}favourite{{?}} {{? game.jackpot_value }}jackpot{{?}}  {{? game.in_maintenance }}in_maintenance{{?}} " data-score="0" data-game-id="{{= game.name_url }}" data-name="{{= String(game.name).toLowerCase() }}" 

    {{ for(var sort_key in game.sort_order) { }}
      data-order-{{= sort_key }}="{{= game.sort_order[ sort_key ] }}"
    {{ } }}

    data-order="{{= game.sort_order.default }}" 
    > 
      <div class="game-item">
        <a href="/game/{{= game.name_url }}" title="{{= game.name }}" data-id="{{= game.name_url }}" class="popup" {{? game.play_for_fun }}data-play-for-fun="1"{{?}}>

          <div class="row">
            <div class="game-image">
              <img src="https://c4.contentfun.net/files/repository/3/9/75/all/all/image/game-icon-placeholder_1519811002.png?nPsa&IBs4&mNgr" data-src="{{= game.image }}" alt="{{= game.name }}" class="lazyImage">
            </div>
            <div class="game-info">
              <div class="game-text">
                {{? game.jackpot_value }}
                  <div class="gameDetailItem-jackpot">
                    {{= game.jackpot_string }}
                  </div>
                {{?}}
                <div class="game-name" title="{{= game.name }}">{{= game.name }}</div>
                <div class="game-teaser">{{= game.description }}</div>
              </div><!-- game-text -->

              <div class="game-status">
                {{~ game.tags :tag }}<span class="icn icn-game-{{= tag.toLowerCase() }}"></span>{{~}}
                {{? game.on_tournament }}<span class="icn icn-game-tournament"></span>{{?}}
                {{? game.user_favourite }}<span class="icn icn-game-favourite"></span>{{?}}
                {{? game.jackpot_value }}<span class="icn icn-game-jackpot"></span>{{?}}
                {{? game.in_maintenance }}<span class="icn icn-game-maintenance"></span>{{?}}
                <div class="game-rating">
                  {{? game.user_rating }}
                  <span class="icn icn-star-{{? game.user_rating >= 1 }}filled{{?? }}empty{{?}}-small"></span>
                  <span class="icn icn-star-{{? game.user_rating >= 2 }}filled{{?? }}empty{{?}}-small"></span>
                  <span class="icn icn-star-{{? game.user_rating >= 3 }}filled{{?? }}empty{{?}}-small"></span>
                  <span class="icn icn-star-{{? game.user_rating >= 4 }}filled{{?? }}empty{{?}}-small"></span>
                  <span class="icn icn-star-{{? game.user_rating == 5 }}filled{{?? }}empty{{?}}-small"></span>
                  {{??}}
                  <span class="icn icn-star-{{? game.rating >= 1 }}filled{{?? }}empty{{?}}-small"></span>
                  <span class="icn icn-star-{{? game.rating >= 2 }}filled{{?? }}empty{{?}}-small"></span>
                  <span class="icn icn-star-{{? game.rating >= 3 }}filled{{?? }}empty{{?}}-small"></span>
                  <span class="icn icn-star-{{? game.rating >= 4 }}filled{{?? }}empty{{?}}-small"></span>
                  <span class="icn icn-star-{{? game.rating == 5 }}filled{{?? }}empty{{?}}-small"></span>
                  {{?}}
                </div>

              </div>
            </div><!-- game-info -->
          </div><!-- row -->
        </a>
      </div>
    </div>
  {{~}}
