	}
	divFacet.html(render);

	_rechercheformationmenuportlet_WAR_rechercheportlet_toggleAllSelectedFilters();
};


function _rechercheformationmenuportlet_WAR_rechercheportlet_toggleAllSelectedFilters() {

	// Gestion du click sur les boutons permettant de cacher(X)/afficher(+) les valeurs des facets
	jQuery(".toggleFacet").each(function(index) {
		$(this).click(function(event){_rechercheformationmenuportlet_WAR_rechercheportlet_onCloseFilterButtonClick(event);});
	});

	// Click sur les facettes dont au moins une valeur est sÃ©lectionnÃ©e
	jQuery(".filterContent").each(function() {
		var content = jQuery(this);
		var link = content.prev("a");
		if (isMobile && !link.hasClass("close")) {
			// Sur mobile toutes les facets sont fermÃ©es par dÃ©faut
			_rechercheformationmenuportlet_WAR_rechercheportlet_toggleFilter(link);
		} else if (link.hasClass("close")) {
			// Sur desktop toutes les facets sont ouvertes par dÃ©faut
			link.hasClass("close");
			_rechercheformationmenuportlet_WAR_rechercheportlet_toggleFilter(link);
		}
		if (content.find("li.selected").length) {
			if (isMobile) {
				_rechercheformationmenuportlet_WAR_rechercheportlet_toggleFilter(link);
			}
			_rechercheformationmenuportlet_WAR_rechercheportlet_toggleFilter(link);
		}
	});
};

//Gestion du click sur les boutons permettant de cacher(X)/afficher(+) les valeurs des facets
var _rechercheformationmenuportlet_WAR_rechercheportlet_onCloseFilterButtonClick = function( event ) {
	var target = jQuery(event.target);
	_rechercheformationmenuportlet_WAR_rechercheportlet_toggleFilter(target);
};


function _rechercheformationmenuportlet_WAR_rechercheportlet_toggleFilter(target) {
	var content = target.next(".filterContent");
	var title = target.prev("h4");
	if (target.hasClass("close")) {
		content.show();
		title.addClass("active");
		content.find("li").show();
		target.removeClass("close");
	} else {
		title.removeClass("active");
		target.addClass("close");
		content.find("li").hide();
		if(content.find("li.selected").length) {
			content.find("li.selected").show();
		} else {
			content.hide();
		}
	}
};

// verifie si la valeur d'une facet est selectionnÃ©e
function selectedValueFacet(item) {

	var selected = false;

	if (item.values.length > 0) {
		// parcours des valeurs
		for ( var j in item.values) {
			var value = item.values[j];

			selected = facetFormationValues[item.facet] != undefined && facetFormationValues[item.facet].indexOf(value.term) != -1;
			if(selected){
				return selected;
			}
	   	}
    }
   	return selected;
};

