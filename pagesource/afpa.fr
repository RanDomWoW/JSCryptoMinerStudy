	function _rechercheevenementhomepageportlet_WAR_rechercheportlet_INSTANCE_rechercheEvenementHome_submitForm() {

		var searchKeywords = jQuery("#_rechercheevenementhomepageportlet_WAR_rechercheportlet_INSTANCE_rechercheEvenementHome_searchKeywords").val();

		// Si le champ n'est pas vide, on continue vers l'appel ajax puis l'envoi du formulaire
		if(searchKeywords != '') {

			// On recopie la valeur depuis le champs de saisie vers le champs caché qui a le bon nom pour que le formulaire soit renseigné
			jQuery("#_rechercheevenementportlet_WAR_rechercheportlet_INSTANCE_4ONof6W5P5AJ_ville").val(searchKeywords);

			// On declare les parametres pour l'appel ajax
			var fieldName = "_rechercheevenementhomepageportlet_WAR_rechercheportlet_INSTANCE_rechercheEvenementHome_searchKeywords";
			var keywordValue = jQuery("#" + fieldName).val();

			// Méthode AJAX qui récupère les centres à proximité de la ville donnée en paramètre
			// et prépare tout ça en champ caché pour envoi vers l'asset publisher de l'agenda
			jQuery.ajax({
				type: "GET",
				url: "https://www.afpa.fr/home-page?p_p_id=rechercheevenementhomepageportlet_WAR_rechercheportlet_INSTANCE_rechercheEvenementHome&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&p_p_resource_id=listerCentresProcheVille&p_p_cacheability=cacheLevelPage&p_p_col_id=column-1&p_p_col_count=1&_rechercheevenementhomepageportlet_WAR_rechercheportlet_INSTANCE_rechercheEvenementHome_action=actionListerCentres&_rechercheevenementhomepageportlet_WAR_rechercheportlet_INSTANCE_rechercheEvenementHome_USER_GEOLOC_OBJET=" + "&" + fieldName + "=" + keywordValue,
				dataType : "json",
				success : function(data, textStatus, jqXHR) {

					var hiddenFields = "";
					var assetPublisherId = "101_INSTANCE_agenda";
					var structureEvenement = "EVENEMENT";
					var structureEvenementId = "22997";
					var dateDuJour = "24/03/2018";

					// On ajoute les filtres des centres proches de la ville pour l'asset publisher
					for (var index in data.arrayCategoryIdCentre) {
						var categoryId = data.arrayCategoryIdCentre[index];
						if(categoryId){
							hiddenFields += "<input type=\"hidden\" id=\"_" + assetPublisherId + "_centrecategoryId\" name=\"_" + assetPublisherId + "_centrecategoryId\" value=\"" + categoryId + "\" />";
						}
					}

					// On ajoute les filtres sur la structure EVENEMENT et sur la date du jour pour l'asset publisher
					hiddenFields += "<input type=\"hidden\" id=\"_" + assetPublisherId + "_afpa_ddmStructureKey\" name=\"_" + assetPublisherId + "_afpa_ddmStructureKey\" value=\"" + structureEvenement + "\" />";
					hiddenFields += "<input type=\"hidden\" id=\"_" + assetPublisherId + "_afpa_ddm__" + structureEvenementId + "__DateDebut_en_US\" name=\"_" + assetPublisherId + "_afpa_ddm__" + structureEvenementId + "__DateDebut_en_US\" value=\"" + dateDuJour + "\" />";

					jQuery("#hiddenCategoriesIdCentre").html(hiddenFields);

					jQuery("#_rechercheevenementhomepageportlet_WAR_rechercheportlet_INSTANCE_rechercheEvenementHome_searchForm").submit();
				},
