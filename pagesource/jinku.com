        <script type="text/javascript">
        //快速购买
        function immediateBuy(){
        	popHide();
        	if($("#goodId").val() != ''){
        		window.location.href = '/shoppingcart/confirm_Immediate.action?goodId='+$("#goodId").val();	
        	}
        }
        
        //商品详情
        function goodDetail(){
        	popHide();
        	if($("#goodId").val() != ''){
        		window.location.href = '/goods/findGoodsDetail.action?goodsId='+$("#goodId").val();
        	}
        }
        
        //跳去考试辅导页面
        function goLiveSearch(){
        	closeIndexPop();
        	window.open('/live/liveSearch.action');
        }
        
        //弹出首页直播课提示框
        function popBroadcast(data){
        	var isHaveBroadcastLive = data.isHaveBroadcastLive;
        	var watchLimit = data.watchLimit ;
        
        	if(isHaveBroadcastLive){
        		$("#classImg").attr("src", data.classPicture);
        		if(watchLimit == null || watchLimit == ''){
        			$("#watchLimit").css("display", "none");
        		}else{
        			$("#watchLimit").css("display", "display");
        			$("#watchLimit").html(watchLimit);
        		}
        		
        		$("#className").html(data.classTitle);
        		$("#classId").val(data.classId);
        		popShow('popAlert03');
        	}
        }
        
        //首页--直播弹窗--关闭
        function closeIndexPop(){
        	creditPopHide('popAlert03');
        	saveCookie('isCloseIndexPop', 'true', '0.25', 'jinku.com');
        }
        
      	//首页--直播弹窗--关闭记住状态
      	//浏览器端记cookie cookieExpires:天数
        function saveCookie(cookieKey, cookieValue, cookieExpires, cookieDomain){
        	var d = new Date();
      	  	d.setTime(d.getTime()+(cookieExpires*24*60*60*1000));
      	  	var expiresDate = d.toGMTString();
        	document.cookie=cookieKey+"="+cookieValue+"; expires="+expiresDate+"; path=/;domain="+cookieDomain;
        }
      	
      	//浏览器端读取cookie
      	function getCookie(cname){
		  var name = cname + "=";
		  var ca = document.cookie.split(';');
		  for(var i=0; i<ca.length; i++) 
		  {
		    var c = ca[i].trim();
		    if (c.indexOf(name)==0) return c.substring(name.length,c.length);
		  }
		  return "";
		}
        
        function intoWatchPop(){
        	var id = $("#classId").val();
        	if(id != ''){
        		intoWatchParent(id);
        	}
        	
        }
        
        //进入直播室 是否购买、是否是预告
		function intoWatchParent(id){
			$.ajax({
				url : "/live/isCanIntoWatch.action", // 后台处理程序
				type : "post", // 数据发送方式
				async : false,
				data : {id : id},
				success : function(data) {
					if(data == 'IsAjax'){
						toLogin();
					}else{
						var jsonData = eval('(' + data + ')');
						
						if(jsonData.isCanIntoWatch == 0){
							var classUrl = jsonData.classUrl;
							if(classUrl != ''){
								if(classUrl.indexOf(".mp4") > -1){
									$("#path").val(classUrl);
									$("#watchLiveForm").submit();
								}else{
									window.open(classUrl);
								}
							}
							
						}else if(jsonData.isCanIntoWatch == -1 || jsonData.isCanIntoWatch == -2){
							_noBuy(jsonData);
						}else{
							var html = '课程尚未开始，请在'+jsonData.classDate+' '+jsonData.classTime+'进入观看！';
							$("#errorMsg").html(html);
							popShow('popAlert01');
						}
					}
				}
			}); 
		}
        
        function _noBuy(jsonData) {
        	if(jsonData.isCanIntoWatch == -1 || jsonData.isCanIntoWatch == -2 ){
                $("#goodId").val(jsonData.good.id);
                $("#goodPic").attr("src",jsonData.good.goodsPicture);
                $("#goodName").html(jsonData.good.name);
                if(jsonData.good.isDiscount == 2){
                    $("#goodDiscount").css("display", "display");
                    $("#goodPrice").html('<del>价格：'+jsonData.good.price+'</del>');
                    $("#goodDiscount").html('优惠价：<span>'+jsonData.good.discount.discountPrice+'</span>');
                }else{
                    $("#goodDiscount").html('');  
                    $("#goodPrice").html('价格：'+jsonData.good.price);
                }
                popShow('popAlert04');
            }
        }
        
		
		</script>
    </body>
    
    
