						//焦点滑动移动
						$(".slideractive").animate({left: 100 + "%"},500,function(){
							$(this).removeClass("slideractive");
							$(this).css("left",100+"%");
						});
						$(".mySliderItem:eq(" + index + ")").css("left",-100 + "%");
						$(".mySliderItem:eq(" + index + ")").animate({left: 0},500,function(){
							$(this).addClass("slideractive");
							animating = true;
						});
