			});
			
			
			
			//向左移动函数
			
			function sliderLeft() {
				currentIndex = $(".mySliderContent .slideractive").index()+1;
				if (animating) {
					animating = false;
					if (currentIndex !== sliderLength) {
						//焦点滑动
						$(".mySliderCircleItem:eq(" + (currentIndex-1) + ")").removeClass("mySliderCircleItemCur");
						$(".mySliderCircleItem:eq(" + (currentIndex-1) + ")").next("li").addClass("mySliderCircleItemCur").siblings("li").removeClass("mySliderCircleItemCur");
						
						//内容部分滑动
						$(".slideractive").animate({left: -100 + "%"},500,function(){
							$(this).removeClass("slideractive");
							$(this).css("left",100+"%");
						});
						$(".slideractive").next(".mySliderItem").animate({left: 0},500,function(){
							$(this).addClass("slideractive");
							animating = true;
						});
					}else {
						//焦点移动到头
						$(".mySliderCircleItem:eq(" + 0 + ")").addClass("mySliderCircleItemCur").siblings("li").removeClass("mySliderCircleItemCur");
						
						//内容部分滑动
						$(".slideractive").animate({left: -100 + "%"},500,function(){
							$(this).css("left",100+"%");
							$(this).removeClass("slideractive");	
						});	
						$(".mySliderContent .mySliderItem:eq(" + 0 + ")").css("left",100 + "%");
						$(".mySliderContent .mySliderItem:eq(" + 0 + ")").animate({left:0},500,function(){
							$(this).addClass("slideractive");
							animating = true;
						})
					}	
				}
			}
			//向右移动函数
			function sliderRight() {
				currentIndex = $(".mySliderContent .slideractive").index()+1;
				if (animating) {
					animating = false;
					if (currentIndex !== 1) {
						//焦点移动到头
						$(".mySliderCircleItem:eq(" + (currentIndex-1) + ")").removeClass("mySliderCircleItemCur");
						$(".mySliderCircleItem:eq(" + (currentIndex-1) + ")").prev("li").addClass("mySliderCircleItemCur").siblings("li").removeClass("mySliderCircleItemCur");
						
						//内容部分滑动
						$(".slideractive").animate({left: 100 + "%"},500,function(){
							$(this).removeClass("slideractive");
							$(this).css("left",100+"%");
						});
						$(".slideractive").prev(".mySliderItem").css("left",-100 + "%");
						$(".slideractive").prev(".mySliderItem").animate({left: 0},500,function(){
							$(this).addClass("slideractive");
							animating = true;
						});
					}else {
						//焦点移动到头
						$(".mySliderCircleItem:eq(" + (sliderLength - 1) + ")").addClass("mySliderCircleItemCur").siblings("li").removeClass("mySliderCircleItemCur");
						
						//内容部分滑动
						$(".slideractive").animate({left: 100 + "%"},500,function(){
							$(this).css("left",-100+"%");
							$(this).removeClass("slideractive");	
						});	
						$(".mySliderContent .mySliderItem:eq(" + (sliderLength - 1) + ")").css("left",-100 + "%");
						$(".mySliderContent .mySliderItem:eq(" + (sliderLength - 1) + ")").animate({left:0},500,function(){
							$(this).addClass("slideractive");
							animating = true;
						})
					}	
				}
			}
			
