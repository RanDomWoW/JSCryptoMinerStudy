var $this = jQuery(this),
pos = $this.position();
pos.width = $this.width();
pos.height = $this.height();
// Center point
pos.cx = pos.left + parseInt(pos.width / 2);
pos.cy = pos.top + parseInt(pos.height / 2);
mx = Math[isRTL ? 'min' : 'max'](mx, pos.cx);
