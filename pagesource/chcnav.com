      $(".head-menu ul li").mouseleave(function(){
        $(this).children(".head-m-a").removeClass("current");
        $(this).children(".head-m-a").next().stop(true,true).slideUp(200);
      })
      //phone header
      $(".p-h-icon").click(function(){
        $(".p-h-menu").slideToggle()
      })
      $(".p-h-menu ul li").click(function(){
        $(this).find(".p-h-right").toggleClass("current").siblings(".p-h-sub").slideToggle().parent("li")
        .siblings().find(".p-h-right").removeClass("current").siblings(".p-h-sub").hide(500)
        // $(this).toggleClass("current").siblings(".p-h-sub").slideToggle().parent()
        // .siblings("li").children(this).removeClass("current").siblings(".p-h-sub").hide(500)
      });
      // content height
      $(".page-right").height($(".page-left").height());
	  //product item
      $(".pro-i-tit").click(function(){
        $(this).toggleClass("current").next().slideToggle().parent(".pro-item").siblings(".pro-item")
        .children(".pro-i-tit").removeClass("current").next().slideUp();
      })
    })
	
	//jq add by huangwen start
	$(".hw-cs-list li .ipor-img").click(function(){
		$(this).next().next().fadeIn();
		$("body").css("overflow","hidden");
	})
	$(".hw-slide-box .close").click(function(){
		$(this).parents(".hw-slide-box").hide();
		$("body").css("overflow","visible");
		$(".nicescroll-rails").hide();
	})
	
	//jq add by huangwen end
</script>
