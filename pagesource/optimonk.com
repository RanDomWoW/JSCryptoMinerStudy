    function conversificSubmit(event) {
        function isValidEmail(element) {
            return (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,4})+$/.test(element.val()));
        }
    
        function validateForm() {
            var emailInput = $('#conversific-email'),
                error = [];
    
            if (!isValidEmail(emailInput)) {
                error.push('Please check your email address!');
            }
            return error.length === 0;
        }
    
        function sendFormData() {
            var serializedData = $('#conversific_subscribe').serialize();
    
            $.ajax({
                url: 'https://www.optimonk.com/application/optimonk/ajax/ajaxPost.php',
                type: 'post',
                dataType: 'json',
                data: serializedData
            }).done(
                function (data) {
                    window.location.href = "/ecommerce-conversion-mastery-thankyou";
                }
            );
        }
    
        event.preventDefault();
        if (validateForm()) {
            sendFormData();
        }
    }
    $(document).ready(function ($) {
        $('#submit-popup2').on('click', conversificSubmit);
        $('#conversific_subscribe').on('submit', conversificSubmit); 
    });
</script>

