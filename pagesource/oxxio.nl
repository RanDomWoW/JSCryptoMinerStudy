                            <div ng-class="{'input-group-focus': focusValue}" class="input-group input-group--meter">
                                <span class="input-group-addon">
                                    <span class="svg -energy">
                                        <!--  energy.svg  -->
                                        <svg class="svg-energy" width="13" height="22" viewBox="0 0 13 22" xmlns="http://www.w3.org/2000/svg"><path d="M13 9.186H7.108L10.178 0 0 12.814h5.892L2.822 22 13 9.186z" /></svg>
                                    </span>
                                </span>
                                <input name="electricitySingleValue" max-five-digits ng-required="!calculator.form.isEstimate && calculator.form.tariffChoice === 0" ng-focus="focusValue = true" ng-blur="focusValue = false" ng-model="calculator.form.electricitySingleValue" min="1" step="1" type="number" class="form-control" placeholder="0000" maxlength="5">
                                <span class="input-group-addon">
