    <!-- Login: Modal - loginModal -->
<script type="text/ng-template" id="loginModal.html">
    <modal ng-cloak class="modal__login">
        <modal-header title="Inloggen"></modal-header>
        <modal-body>
            <form name="loginForm" method="post" ng-submit="login()" ng-class="{ 'has-error has-feedback': login.errorMessage }" ng-cloak>
                <div class="form-group" ng-class="{ 'has-error has-feedback' : loginForm.password.$invalid && !loginForm.password.$pristine || errorLogin, 'has-success has-feedback' :  loginForm.password.$valid && !loginForm.password.$pristine && !errorLogin }">
                    <small class="help-block" ng-if="errorMessage">{{ errorMessage.data.message }}</small>
                </div>

                <!-- userName -->
                <div class="form-group" ng-class="{ 'has-error has-feedback' : loginForm.userName.$invalid && !loginForm.userName.$pristine || errorLogin, 'has-success has-feedback' :  loginForm.userName.$valid && !loginForm.userName.$pristine && !errorLogin }">
                    <input type="text" class="form-control input-lg" id="userName" name="userName" placeholder="Gebruikersnaam"
                            ng-model="login.form.userName"
                            ng-model-options="{ updateOn: 'default blur', debounce: { 'default': 800, 'blur': 0 } }"
                            required>
                </div>
                
                <!-- Password -->
                <div class="form-group" ng-class="{ 'has-error has-feedback' : loginForm.password.$invalid && !loginForm.password.$pristine || errorLogin, 'has-success has-feedback' :  loginForm.password.$valid && !loginForm.password.$pristine && !errorLogin }">
                    <input type="password" class="form-control input-lg" id="password" name="password" placeholder="Wachtwoord"
                            ng-model="login.form.password"
                            ng-model-options="{ updateOn: 'default blur', debounce: { 'default': 800, 'blur': 0 } }"
                            required>
                </div>

                <div class="form-group">
                    <button type="submit" class="btn btn-block btn-lg" ng-class="{ 'btn-success' : isLoggedIn, 'btn-default' : !isLoggedIn }" ng-switch on="isLoggedIn">
                        <span ng-switch-default>Inloggen</span>
                        <span ng-switch-when="true">Ingelogd</span>
                    </button>
                </div>

                <div class="row">
                    <p class="center-block text-center">
                        <a class="underlined font-weight--bold" href="https://mijn.oxxio.nl/dmz/registreren/je-gegevens" target="_blank">Maak een Oxxio-account</a>
                    </p>
                    <p class="center-block text-center">
                        <a class="underlined font-weight--medium" href="https://mijn.oxxio.nl/dmz/lostpassword.aspx" target="_blank">Ik ben mijn inloggegevens vergeten</a>
                    </p>
                    <p class="center-block text-center">
                        <a class="underlined font-weight--medium" href="https://mijn.oxxio.nl/dmz/mainloginpage.aspx?&showlogin=true" target="_blank">Inloggen met je social login?</a>
                    </p>
                </div>
            </form>
        </modal-body>
    </modal>
</script>
