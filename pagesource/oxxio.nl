                    <a class="superdeal-label__more-info" href="#" modal-small="" modal-text="<p>Dit heeft betrekking op ons 1-jarig of 3-jarig contract voor stroom en gas, alle vaste en variabele kosten daarin meegenomen.</p>" aria-hidden="true" class="ng-scope">
                        <span class="svg -info">
                            <!--  info.svg  -->
                            <svg class="svg-info" xmlns="http://www.w3.org/2000/svg" width="22" height="22" viewBox="2 2 22 22"><path d="M13 24C6.934 24 2 19.065 2 13 2 6.934 6.934 2 13 2c6.065 0 11 4.934 11 11 0 6.065-4.935 11-11 11zm0-21.12C7.42 2.88 2.88 7.42 2.88 13S7.42 23.12 13 23.12 23.12 18.58 23.12 13 18.58 2.88 13 2.88z" /><path d="M14.2 11.814v-1.26h-3.12v1.293h.72v4.66h-.72V17.8h3.84v-1.294h-.722v-4.692zm-1.21-1.986c.662 0 1.2-.58 1.2-1.294 0-.714-.537-1.293-1.197-1.293-.663 0-1.2.58-1.2 1.296s.536 1.294 1.2 1.294h-.004z" /></svg>
                        </span>
                    </a>
                </div>
            </div>
        </div>
    </div>
    <div class="panel-heading u-gradient-background--blue" aria-role="button" ng-click="collapsed = !collapsed">
        <h4 class="text-white calculator__title hidden-sm hidden-xs">Bereken je deal in 1 minuut</h4>
        <h4 class="text-white calculator__title hidden-md hidden-lg">Start je berekening</h4>
    </div>
    <div class="panel-body">

        <form novalidate ng-submit="calculator.submit('/stroom-en-gas/aanmelden')" name="calculator.calculatorForm" ng-model="calculator.form">
            <input ng-disabled="!calculator.calculatorForm.$valid || calculator.isUnknownAddress" type="submit" style="position: absolute; left: -9999px; width: 1px; height: 1px;" />

            <div class="row">

                <div class="col-xs-7 col-sm-4">
                    <div class="form-group">
                        <label>Jouw postcode</label>
                        <input class="form-control form-control--underlined form-control--uppercase" style="text-transform: uppercase;" type="text" placeholder="1234AB" name="zipCode"
                               ng-pattern="/^[1-9][0-9]{3}[a-zA-Z]{2}$/"
                               maxlength="6"
                               ng-model="calculator.form.zipCode"
                               ng-model-options="{ updateOn: 'blur' }"
                               ng-change="calculator.getAddress(calculator.form.zipCode, calculator.form.houseNumber, false); calculator.getHouseNumberSuffix(calculator.form.zipCode, calculator.form.houseNumber, false)"
                               trim-spaces
                               uppercase-only
                               required>
                    </div>
                </div>

                <div class="col-xs-5 col-sm-3">
                    <div class="form-group">
                        <label>Huisnr</label>
                        <input class="form-control form-control--underlined" type="number" name="houseNumber"
                               min="0"
                               maxlength="10"
                               ng-model="calculator.form.houseNumber"
                               ng-model-options="{ updateOn: 'blur' }"
                               ng-change="calculator.getAddress(calculator.form.zipCode, calculator.form.houseNumber, false); calculator.getHouseNumberSuffix(calculator.form.zipCode, calculator.form.houseNumber, false); calculator.isDataCompleted=true"
                               only-numbers
                               placeholder="123"
                               inputmode="numeric"
                               pattern="[0-9]*"
                               required>
                    </div>
                </div>

                <div class="col-xs-7 col-sm-5">
                    <div class="form-group" ng-if="calculator.isEdsnOnline == true" ng-cloak>
                        <div ng-if="calculator.houseNumberSuffixList.length > 1 || (calculator.houseNumberSuffixList.length == 1 && calculator.houseNumberSuffixList[0] != null)" class="form-group">
                            <label>Toevoeging</label>
                             <select ng-model="calculator.form.houseNumberSuffix" class="form-control" ng-cloak name="houseNumberSuffix">
                               <option ng-repeat="item in calculator.houseNumberSuffixList" ng-value="item == null ? '' : item">{{item == null ? 'Geen' : item}}</option>
                            </select>
                        </div>
                    </div>
					 <div class="form-group" ng-if="calculator.isEdsnOnline == false" ng-cloak>
                        <div class="form-group" ng-if="calculator.isDataCompleted">
                            <label>Toevoeging</label>
                            <input class="form-control form-control--underlined" type="text" name="houseNumberSuffix"
                                   min="1"
                                   maxlength="10"
                                   ng-model="calculator.form.houseNumberSuffix">
                        </div>
                    </div>
                </div>
            </div>

            <!-- Error can find address -->
