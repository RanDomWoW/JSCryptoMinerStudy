<div class="panel panel-default calculator calculator--dp" ng-controller="CalculatorController as calculator">
   <div class="superdeal ">
        <div class="superdeal__labels">
            <div class="superdeal-label">
                <div class="superdeal-label__ribbon" after="als bij een prijsvergelijker" before="Dezelfde">
