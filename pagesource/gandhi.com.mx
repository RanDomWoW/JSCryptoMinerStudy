<script type="text/javascript">

/** function to hide the children of the element with active class in the responsive design */
function hideElement(element){
    if(element.hasClass('help')){
        element.removeClass('active');
        element.children('div.submenu').hide(500);
    } else if(element.hasClass('find')){
        element.removeClass('active');
        jQuery('.SearchtopCart').hide(500);
    } else if(element.hasClass('menu')){
        element.removeClass('active');
        jQuery('.nav-container').hide(500);
    } else if(element.hasClass('account-menu')){
        element.removeClass('active');
        element.children('div.submenu').hide(500);
    }
}

jQuery(document).ready(function(){
    initPos = 81;
    jQuery(window).scroll(function(){

        jQuery(this).scrollTop();
        if(jQuery('.fixed-button').length && jQuery(this).scrollTop() <= initPos){
            jQuery('.top-cart').removeClass('fixed-button');
        }else if(jQuery(this).scrollTop() >= initPos){
            jQuery('.top-cart').addClass('fixed-button');
        }
    });

    jQuery( document ).scroll(function(){
        if(jQuery(document).width() < 800){
            hideElement(jQuery('.menu'));
            hideElement(jQuery('.help'));
            hideElement(jQuery('.account-menu'));
            Enterprise.TopCart.hideCart();
            if(jQuery( document ).width() < 600){
//                hideElement(jQuery('.find'));
                jQuery('#narrow-by-list').hide();
                jQuery('#sort-list').hide();
                jQuery('#options-filter').removeClass('active');
                jQuery('#sort-filter').removeClass('active');
            }
        }else{
            Enterprise.TopCart.hideCart();
        }
    });

    jQuery( '*' ).click(function(event){
        var clicked = jQuery(event.target);
        if(jQuery(document).width() < 800){
            if(!clicked.parents().is('.menu') && !clicked.parents().is('.nav-container')){
                hideElement(jQuery('.menu'));
            }
            if(!clicked.parents().is('.help') && !clicked.parents().is('.submenu')){
                hideElement(jQuery('.help'));
            }
            if(!clicked.parents().is('.account-menu') && !clicked.parents().is('.submenu')){
                hideElement(jQuery('.account-menu'));
            }
            if(!clicked.parents().is('.top-cart')){
                Enterprise.TopCart.hideCart();
            }
            if(jQuery(document).width() < 600){
                if(!clicked.parents().is('.find') && !clicked.parents().is('.SearchtopCart')){
                    hideElement(jQuery('.find'));
                }
            }
        }else{
            if(!clicked.parents().is('.top-cart')){
                 Enterprise.TopCart.hideCart();
            }
        }

    });
    /** Make the dropdown function of submenus with responsive design */
    jQuery('.account-menu').click(function(event){
        if(jQuery(this).hasClass('active')){
            if(jQuery(event.target).hasClass('account-img')){
                jQuery(this).removeClass('active');
                jQuery(this).children('div.submenu').hide(500);
            }
        }
        else{
            if(jQuery(this).parent().children('.active').length > 0){
                element = jQuery(this).parent().children('.active');
                hideElement(element);
            }
            Enterprise.TopCart.hideCart();
            jQuery(this).addClass('active');
            jQuery(this).children('div.submenu').show(1000);
        }
    });

    jQuery('.help').click(function(event){
        if(jQuery(this).hasClass('active')){
            if(jQuery( document ).width() < 600){
                if(jQuery(event.target).hasClass('help-img')){
                    jQuery(this).removeClass('active');
                    jQuery(this).children('div.submenu').hide(500);
                }
            }else{
                jQuery(this).removeClass('active');
                jQuery(this).children('div.submenu').hide(500);
            }
        }
        else{
            if(jQuery(this).parent().children('.active').length > 0){
                element = jQuery(this).parent().children('.active');
                hideElement(element);
            }
            Enterprise.TopCart.hideCart();
            jQuery(this).addClass('active');
            jQuery(this).children('div.submenu').show(1000);
        }
    });
// ------------------------------------------------------------------------------
    jQuery('.menu').click(function(event){
        if( !jQuery(this).hasClass('active') ){
                jQuery(this).addClass('active');
            jQuery('.nav-container').slideDown(300);
        }
        else{
            
            Enterprise.TopCart.hideCart();
            jQuery(this).removeClass('active');
            jQuery('.nav-container').slideUp(300);
        }
    });

// ------------------------------------------------------------------------------

    jQuery('.ContenttopCart .top-cart').click(function(){
        if(jQuery('.btn-mobil ul').children('li.active').length > 0){
            element = jQuery('.btn-mobil ul').children('li.active');
            hideElement(element);
        }
    });

    /** Comment entire fancybox block of hotsale promotion begin */
     jQuery("#trigger-click").fancybox({
         'width'  : 500,
         'height' : 500,
     });

     var cookie = jQuery.cookie('cookie');

     if(cookie != 'active'){
         var date = new Date();
         var minutes = 360;
         date.setTime(date.getTime() + (minutes * 60 * 1000));
         jQuery.cookie('cookie', 'active', {expires: date, path: '/'});
         jQuery("#trigger-click").trigger('click');
         jQuery("#contentImage").css("border", "1px solid #eeeeee");
         if(jQuery('.fancybox-wrap:has(div#contentImage)').length){
             jQuery('.fancybox-wrap').addClass('first-ligthbox');
             setTimeout(closeLightboxHome, 7000);
         }
     }
    /** Comment entire fancybox block of hotsale promotion end */

    // set active class to main category
    var current_url     = window.location.href;
    var parent_category = current_url.match(/http:\/\/[a-z.]+\/([a-z\-]+)/i);

    if(parent_category){
        jQuery('.nav-container').find('a[href="'+parent_category[0]+'"]').eq(0).parent().addClass('active');
