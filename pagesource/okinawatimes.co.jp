<script>
var OTPLIB0 = OTPLIB0 || {};
OTPLIB0.eachSlideWidth = [364, 364, 364, 364, 364, 364];
OTPLIB0.eachSlideHeight = [137, 485, 229, 216, 327, 267];
OTPLIB0.eachSlideRatio = [2.656934306569343, 0.7505154639175258, 1.5895196506550218, 1.6851851851851851, 1.1131498470948011, 1.3632958801498127]
OTPLIB0.slideAreas = $('#top-feature .slide-block');
OTPLIB0.slideArea = OTPLIB0.slideAreas.eq(0);
OTPLIB0.slideAreaWidth = OTPLIB0.slideArea.width();
OTPLIB0.mainSlides = OTPLIB0.slideArea.find('.slide .photo');
OTPLIB0.slideSize = OTPLIB0.mainSlides.size();
OTPLIB0.thumbsArea = OTPLIB0.slideArea.find('.thumbnail');
OTPLIB0.thumbs = OTPLIB0.slideArea.find('.thumbnail span');
