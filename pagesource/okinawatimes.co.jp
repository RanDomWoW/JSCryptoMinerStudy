OTPLIB0.currentSlide = 0;
OTPLIB0.nextSlide = 1;
OTPLIB0.duringAnimation = false;

OTPLIB0.defaultSlideWidth = 364;
OTPLIB0.defaultSlideHeight = 250;
OTPLIB0.defaultImgRatio = (OTPLIB0.defaultSlideWidth / OTPLIB0.defaultSlideHeight);
OTPLIB0.smallSlideHeight = 162;
OTPLIB0.smallSlideWidth = 243;
OTPLIB0.slideInterval = 6000;
OTPLIB0.slideDuretion = 300;

OTPLIB0.switchSlide = function(idx,reverse){
  if ((reverse != 'undefined') && reverse ) {
    reverse = true;
  } else {
    reverse = false;
  }
  OTPLIB0.duringAnimation = true;
  OTPLIB0.currentSlide = idx;
  var leftidx = OTPLIB0.getNextIdx(idx);

  OTPLIB0.thumbs.not(idx).removeClass('current');
  OTPLIB0.thumbs.eq(idx).addClass('current');

  var cw = OTPLIB0.eachSlideWidth[idx] + 'px';
  var ch = OTPLIB0.eachSlideHeight[idx] + 'px';
  var cleft = '0';
  var cright = '0';
  if ( (OTPLIB0.eachSlideWidth[idx] <= OTPLIB0.defaultSlideWidth) && (OTPLIB0.eachSlideHeight[idx] <= OTPLIB0.defaultSlideHeight) ) {
    cw = OTPLIB0.eachSlideWidth[idx] + 'px';
    ch = OTPLIB0.eachSlideHeight[idx] + 'px';
  } else if ( (OTPLIB0.eachSlideWidth[idx] <= OTPLIB0.defaultSlideWidth) && (OTPLIB0.eachSlideHeight[idx] > OTPLIB0.defaultSlideHeight) ) {
    cw = Math.ceil(OTPLIB0.defaultSlideHeight * OTPLIB0.eachSlideRatio[idx]).toString() + 'px';
    ch = OTPLIB0.defaultSlideHeight + 'px';
  } else if ( (OTPLIB0.eachSlideWidth[idx] > OTPLIB0.defaultSlideWidth) && (OTPLIB0.eachSlideHeight[idx] <= OTPLIB0.defaultSlideHeight) ) {
    cw = OTPLIB0.defaultSlideWidth + 'px';
    ch = Math.ceil(OTPLIB0.defaultSlideWidth / OTPLIB0.eachSlideRatio[idx]).toString() + 'px';
  } else if ( (OTPLIB0.eachSlideWidth[idx] > OTPLIB0.defaultSlideWidth) && (OTPLIB0.eachSlideHeight[idx] > OTPLIB0.defaultSlideHeight) ) {
    if (OTPLIB0.eachSlideRatio[idx] > OTPLIB0.defaultImgRatio) {
      cw = OTPLIB0.defaultSlideWidth + 'px';
      ch = Math.ceil(OTPLIB0.defaultSlideWidth / OTPLIB0.eachSlideRatio[idx]).toString() + 'px';
    } else {
      cw = Math.ceil(OTPLIB0.defaultSlideHeight * OTPLIB0.eachSlideRatio[idx]).toString() + 'px';
      ch = OTPLIB0.defaultSlideHeight + 'px';
    }
  }
  var currentCond = {zIndex:'5',width:cw,height:ch,margin:'auto',opacity:'1',left:cleft,right:cright,top:'0',bottom:'0'};

  if (reverse) {
    var precurrentidx = OTPLIB0.getNextIdx(idx);
    var hideidx = OTPLIB0.getNextIdx(precurrentidx);
    var nextleftidx = precurrentidx;
    var nextrightidx = OTPLIB0.getPrevIdx(OTPLIB0.getPrevIdx(precurrentidx));
  } else {
    var precurrentidx = OTPLIB0.getPrevIdx(idx);
    var hideidx = OTPLIB0.getPrevIdx(precurrentidx);
    var nextleftidx = OTPLIB0.getNextIdx(idx);
    var nextrightidx = precurrentidx;
  }
 
  var lw = OTPLIB0.eachSlideWidth[nextleftidx] + 'px';
  var lh = OTPLIB0.eachSlideHeight[nextleftidx] + 'px';
  var lleft = '0';
  var lright = 'auto';
  if ( (OTPLIB0.eachSlideWidth[nextleftidx] <= OTPLIB0.smallSlideWidth) && (OTPLIB0.eachSlideHeight[nextleftidx] <= OTPLIB0.smallSlideHeight) ) {
    lw = OTPLIB0.eachSlideWidth[nextleftidx] + 'px';
    lh = OTPLIB0.eachSlideHeight[nextleftidx] + 'px';
    lright = (OTPLIB0.slideAreaWidth - OTPLIB0.eachSlideWidth[nextleftidx]) + 'px';
  } else if ( (OTPLIB0.eachSlideWidth[nextleftidx] <= OTPLIB0.smallSlideWidth) && (OTPLIB0.eachSlideHeight[nextleftidx] > OTPLIB0.smallSlideHeight) ) {
    lw = Math.ceil(OTPLIB0.smallSlideHeight * OTPLIB0.eachSlideRatio[nextleftidx]).toString() + 'px';
    lh = OTPLIB0.smallSlideHeight + 'px';
    lright = (OTPLIB0.slideAreaWidth - Math.ceil(OTPLIB0.smallSlideHeight * OTPLIB0.eachSlideRatio[nextleftidx])).toString() + 'px';
  } else if ( (OTPLIB0.eachSlideWidth[nextleftidx] > OTPLIB0.smallSlideWidth) && (OTPLIB0.eachSlideHeight[nextleftidx] <= OTPLIB0.smallSlideHeight) ) {
    lw = OTPLIB0.smallSlideWidth + 'px';
    lh = Math.ceil(OTPLIB0.smallSlideWidth / OTPLIB0.eachSlideRatio[nextleftidx]).toString() + 'px';
    lright = (OTPLIB0.slideAreaWidth - OTPLIB0.smallSlideWidth).toString() + 'px';
  } else if ( (OTPLIB0.eachSlideWidth[nextleftidx] > OTPLIB0.smallSlideWidth) && (OTPLIB0.eachSlideHeight[nextleftidx] > OTPLIB0.smallSlideHeight) ) {
    if (OTPLIB0.eachSlideRatio[nextleftidx] > OTPLIB0.defaultImgRatio) {
      lw = OTPLIB0.smallSlideWidth + 'px';
      lh = Math.ceil(OTPLIB0.smallSlideWidth / OTPLIB0.eachSlideRatio[nextleftidx]).toString() + 'px';
      lright = (OTPLIB0.slideAreaWidth - OTPLIB0.smallSlideWidth).toString() + 'px';
    } else {
      lw = Math.ceil(OTPLIB0.smallSlideHeight * OTPLIB0.eachSlideRatio[nextleftidx]).toString() + 'px';
      lh = OTPLIB0.smallSlideHeight + 'px';
      lright = (OTPLIB0.slideAreaWidth - Math.ceil(OTPLIB0.smallSlideHeight * OTPLIB0.eachSlideRatio[nextleftidx])).toString() + 'px';
    }
  }
  var leftCond = {zIndex:'3',display:'block',width:lw,height:lh,left:lleft,right:lright,top:'0',bottom:'0',opacity:'0.2'};
  
  var rw = OTPLIB0.eachSlideWidth[nextrightidx] + 'px';
  var rh = OTPLIB0.eachSlideHeight[nextrightidx] + 'px';
  var rleft = 'auto';
  var rright = '0';
  if ( (OTPLIB0.eachSlideWidth[nextrightidx] <= OTPLIB0.smallSlideWidth) && (OTPLIB0.eachSlideHeight[nextrightidx] <= OTPLIB0.smallSlideHeight) ) {
    rw = OTPLIB0.eachSlideWidth[nextrightidx] + 'px';
    rh = OTPLIB0.eachSlideHeight[nextrightidx] + 'px';
    rleft = (OTPLIB0.slideAreaWidth - OTPLIB0.eachSlideWidth[nextrightidx]) + 'px';
  } else if ( (OTPLIB0.eachSlideWidth[nextrightidx] <= OTPLIB0.smallSlideWidth) && (OTPLIB0.eachSlideHeight[nextrightidx] > OTPLIB0.smallSlideHeight) ) {
    rw = Math.ceil(OTPLIB0.smallSlideHeight * OTPLIB0.eachSlideRatio[nextrightidx]).toString() + 'px';
    rh = OTPLIB0.smallSlideHeight + 'px';
    rleft = (OTPLIB0.slideAreaWidth - Math.ceil(OTPLIB0.smallSlideHeight * OTPLIB0.eachSlideRatio[nextrightidx])).toString() + 'px';
  } else if ( (OTPLIB0.eachSlideWidth[nextrightidx] > OTPLIB0.smallSlideWidth) && (OTPLIB0.eachSlideHeight[nextrightidx] <= OTPLIB0.smallSlideHeight) ) {
    rw = OTPLIB0.smallSlideWidth + 'px';
    rh = Math.ceil(OTPLIB0.smallSlideWidth / OTPLIB0.eachSlideRatio[nextrightidx]).toString() + 'px';
    rleft = (OTPLIB0.slideAreaWidth - OTPLIB0.smallSlideWidth).toString() + 'px';
  } else if ( (OTPLIB0.eachSlideWidth[nextrightidx] > OTPLIB0.smallSlideWidth) && (OTPLIB0.eachSlideHeight[nextrightidx] > OTPLIB0.smallSlideHeight) ) {
    if (OTPLIB0.eachSlideRatio[nextrightidx] > OTPLIB0.defaultImgRatio) {
      rw = OTPLIB0.smallSlideWidth + 'px';
      rh = Math.ceil(OTPLIB0.smallSlideWidth / OTPLIB0.eachSlideRatio[nextrightidx]).toString() + 'px';
      rleft = (OTPLIB0.slideAreaWidth - OTPLIB0.smallSlideWidth).toString() + 'px';
    } else {
      rw = Math.ceil(OTPLIB0.smallSlideHeight * OTPLIB0.eachSlideRatio[nextrightidx]).toString() + 'px';
      rh = OTPLIB0.smallSlideHeight + 'px';
      rleft = (OTPLIB0.slideAreaWidth - Math.ceil(OTPLIB0.smallSlideHeight * OTPLIB0.eachSlideRatio[nextrightidx])).toString() + 'px';
    }
  }
  var rightCond = {zIndex:'3',display:'block',width:rw,height:rh,left:rleft,right:rright,top:'0',bottom:'0',opacity:'0.2'};
  var hiddenCond = {zIndex:'1',display:'block',opacity:'0',top:'0',bottom:'0',left:'0',right:'0'};
  
  OTPLIB0.mainSlides.eq(hideidx).stop(true,true).animate(
    hiddenCond,
    {
      duration: OTPLIB0.slideDuretion,
      easing: 'easeInOutExpo',
      complete: function(){
        //mainSlides.eq(prerightidx).css(hiddenCond);
        OTPLIB0.duringAnimation = false;
      }
    }
  );
  OTPLIB0.mainSlides.eq(nextrightidx).stop(true,true).animate(
    rightCond,
    {
      duration: OTPLIB0.slideDuretion,
      easing: 'easeInOutExpo',
      complete: function(){
        //mainSlides.eq(precurrentidx).css(rightCond);
        OTPLIB0.duringAnimation = false;
      }
    }
  );
  OTPLIB0.mainSlides.eq(idx).show().stop(true,true).animate(
    currentCond,
    {
      duration: OTPLIB0.slideDuretion,
      easing: 'easeInOutExpo',
      complete:function(){
        //mainSlides.eq(idx).css(currentCond);
        OTPLIB0.duringAnimation = false;
      }
    }
  );

  OTPLIB0.mainSlides.eq(nextleftidx).show().stop(true,true).animate(
    leftCond,
    {
      duration: OTPLIB0.slideDuretion,
      easing: 'easeInOutExpo',
      complete:function(){
        //mainSlides.eq(nextleftidx).css(leftCond);
        OTPLIB0.duringAnimation = false;
      }
    }
  );

  OTPLIB0.switchTitle(idx);

};

OTPLIB0.switchTitle = function(idx){
  OTPLIB0.titleAreas.fadeOut('fast',function(){OTPLIB0.titleAreas.eq(idx).fadeIn('slow');});
};

OTPLIB0.getNextIdx = function(idx){
  var ni = idx + 1;
  if ( ni > OTPLIB0.slideSize - 1 ){
    ni = 0;
  }
  return ni;
}

OTPLIB0.getPrevIdx = function(idx){
  var pi = idx - 1;
  if ( pi < 0 ){
    pi = OTPLIB0.slideSize - 1;
  }
  return pi;
}

OTPLIB0.prepareNext = function(idx,reverse){
  if ((reverse != 'undefined') && reverse ) {
    OTPLIB0.switchSlide(idx,true);
  } else {
    OTPLIB0.switchSlide(idx);
  }
  OTPLIB0.nextSlide = OTPLIB0.getNextIdx(OTPLIB0.currentSlide);
};

//$(window).load(function(){
$(function(){

  if (OTPLIB0.slideAreas.length) {
    OTPLIB0.nextSlide = 0;
    OTPLIB0.switchSlide(OTPLIB0.nextSlide);
    OTPLIB0.nextSlide = OTPLIB0.getNextIdx(OTPLIB0.nextSlide);

    var autoSlide = setInterval('OTPLIB0.prepareNext(OTPLIB0.nextSlide)',OTPLIB0.slideInterval);

    OTPLIB0.thumbs.click(function(){
      if ($(this).hasClass('current')){
        //do nothing
      } else {
        clearInterval(autoSlide);
        var n = OTPLIB0.thumbs.index(this);
        OTPLIB0.switchSlide(n,OTPLIB0.currentSlide);
        OTPLIB0.nextSlide = OTPLIB0.getNextIdx(n);
        autoSlide = setInterval('OTPLIB0.prepareNext(OTPLIB0.nextSlide)',slideInterval);
      }
      return false;
    });

    OTPLIB0.next.click(function(){
      if (OTPLIB0.duringAnimation){return false;}
      clearInterval(autoSlide);
      var n = OTPLIB0.getNextIdx(OTPLIB0.currentSlide);
      OTPLIB0.switchSlide(n);
      OTPLIB0.nextSlide = OTPLIB0.getNextIdx(n);
      autoSlide = setInterval('OTPLIB0.prepareNext(OTPLIB0.nextSlide)',OTPLIB0.slideInterval);
    });

    OTPLIB0.prev.click(function(){
      if (OTPLIB0.duringAnimation){return false;}
      clearInterval(autoSlide);
      var p = OTPLIB0.getPrevIdx(OTPLIB0.currentSlide);
      OTPLIB0.switchSlide(p,true);
      OTPLIB0.nextSlide = OTPLIB0.getPrevIdx(p);
      autoSlide = setInterval('OTPLIB0.prepareNext(OTPLIB0.nextSlide)',OTPLIB0.slideInterval);
    });

  }

});
</script>


