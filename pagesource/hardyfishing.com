<script type="text/javascript">
    $(document).ready(function() {
        $(".hotspot .hs-trigger").on("mouseenter mouseleave", function() {
            var data = $(this).parent().data("ls-hotspot");
            var content = $(this).siblings(".hs-content");
            if ($(this).parent().hasClass("open")) {
                content.toggle('blind', {
                    direction: data.direction
                }, data.duration ? data.duration : 400, function() {
                    content.hide();
                    $(this).parent().removeClass("open");
                    hs_timer.clear();
                });
            } else {
                content.toggle('blind', {
                    direction: data.direction
                }, data.duration ? data.duration : 400, function() {
                    content.show();
                    $(this).parent().addClass("open");
                    hs_timer.start(400, $(this).parent());
                });
            }
        });
        var hs_timer = {
            id: null,
            clear: function() {
                window.clearInterval(hs_timer.id);
                delete hs_timer.id;
            },
            start: function(duration, target) {
                hs_timer.id = setInterval(function() {
                    var data = target.data("ls-hotspot");
                    var content = target.children(".hs-content");
                    if (target.hasClass("open") && !target.children(".hs-trigger").is(':hover')) {
                        content.toggle('blind', {
                            direction: data.direction
                        }, data.duration ? data.duration : 400, function() {
                            content.hide();
                        });
                        target.removeClass("open");
                        hs_timer.clear();
                    }
                }, duration);
            }
        };
        $('#layerslider-thumbs').jcarousel({
            scroll: 1,
            itemFallbackDimension: '100%',
            buttonNextHTML: '<div><span></span></div>',
            buttonPrevHTML: '<div><span></span></div>'
        });
        $('#layerslider-thumbs li a').on('click', function(e) {
            e.preventDefault();
            $('#layerslider').layerSlider($(this).parent().index() + 1);
            $('#layerslider').layerSlider('stop');
        });
        $('#layerslider .ls-bottom-slidebuttons a').on('click', function(e) {
            $('#layerslider').layerSlider('stop');
        });
    });
    </script>
