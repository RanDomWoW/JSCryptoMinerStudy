<script type="text/javascript">
document.getElementById('symbol').focus();
function check()
{
    var $symbol = document.getElementById('symbol').value;
    if('' == $symbol)
    {
        alert('請填寫所要查詢的公司代碼。');
        document.getElementById('symbol').focus();
