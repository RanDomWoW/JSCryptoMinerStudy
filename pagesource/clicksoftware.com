<script type="text/javascript">
    $(document).ready(function ($) {
        // delegate calls to data-toggle="lightbox"
        $(document).delegate('*[data-toggle="lightbox"]', 'click', function (event) {
            event.preventDefault();
            $('.loader').show();
            return $(this).ekkoLightbox({
                onShown: function () {
                    if (window.console) {
                        return console.log('Checking our the events huh?');
                    }
                }
            });
        });

        $('.top_navs_desktop').on('click', function (e) {
            $("#show_all").hide();
        });
    });
    $(document).ready(function (e) {
        $('.icon-image').mouseover(function () {
            var this_offset = $(this).offset();

            var href = $(this).children("img").attr('href');
            console.log("href", href);
            $(".testimonial-box-contant a.btn-case").attr("href", href);

            var hover_item = $(".subs_div").children(".testimonial-box-contant");
            hover_item.show();
            hover_item.css({top: this_offset.top - 125, left: this_offset.left - 82, position: 'absolute'});
        });

        $('.icon-image').mouseout(function () {
            $(".subs_div").children(".testimonial-box-contant").hide();
        });

        var width = $(window).width();
        if ((width <= 1025) && (width > 768)) {
            $('.autoplay').slick({
                slidesToShow: 3,
                slidesToScroll: 1,
                autoplay: true,
                autoplaySpeed: 2000,
            });

        }
        else if ((width <= 768) && (width > 500)) {
            $('.autoplay').slick({
                slidesToShow: 2,
                slidesToScroll: 1,
                autoplay: true,
                autoplaySpeed: 2000,
            });

        }
        else if ((width <= 500) && (width > 280)) {
            $('.autoplay').slick({
                slidesToShow: 1,
                slidesToScroll: 1,
                autoplay: true,
                autoplaySpeed: 2000,
            });
            $('.icon-image').mouseover(function () {
                var this_offset = $(this).offset();
                var hover_item = $(".subs_div").children(".testimonial-box-contant");
                hover_item.show();
                hover_item.css({top: this_offset.top - 125, left: this_offset.left - 142, position: 'absolute'});
            });

            $('.icon-image').mouseout(function () {
                var hover_item = $(".subs_div").children(".testimonial-box-contant").hide();
            });
        }
        else {
            $('.autoplay').slick({
                slidesToShow: 5,
                slidesToScroll: 1,
                autoplay: true,
                autoplaySpeed: 2000,
            });

        }
    });
</script>
<script type="text/javascript">

    $(window).scroll(function () {

        var scroll = $(window).scrollTop();
        if (scroll >= 60) {
            $(".company-page-menu").addClass("darkHeader");
        }
        else {
            $(".company-page-menu").removeClass("darkHeader");
        }

        if (scroll >= ($(document).height() - 1000)) {
            $(".message-box").addClass("show-message");
        }
        else {
            $(".message-box").removeClass("show-message");
        }


    });
    $(".message-close").click(function () {
        $(".message-box").toggle();
    });

    $('.tab-responsive-view a').on('click', function () {
        $('.tab-responsive-view a').removeClass('active');
        $(this).addClass('active');
    });

</script>
