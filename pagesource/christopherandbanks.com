		if ((cbk_clickedPosition.indexOf('cbk-button') == -1) && (cbk_clickedPosition.indexOf('hidden-button-dropdown-wrap') == -1) && (cbk_clickedPosition.indexOf('hidden-button-dropdown') == -1) && (cbk_clickedPosition.indexOf('button-default') == -1) && (cbk_clickedPosition.indexOf('fa-plus') == -1) && (cbk_clickedPosition.indexOf('fa-minus') == -1) && (cbk_clickedPosition.indexOf('button-inner-framework') == -1) && (cbk_clickedPosition.indexOf('button-framework') == -1) && (cbk_clickedPosition.indexOf('cbk-universal-button') == -1)) {
			$('.cbk-universal-button .fa-minus').parent('.button-default').next('.hidden-button-dropdown').slideUp(300);
			$('.cbk-universal-button .fa').removeClass('fa-minus');
			$('.cbk-universal-button .fa').addClass('fa-plus');
		}
	});
	$('.cbk-universal-button .button-default').click(function(){
