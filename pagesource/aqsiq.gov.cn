<script type="text/javascript">

$(function () {		
	 //搜索地址以及热搜索词获取范围
	getHotWord("http://so.aqsiq.gov.cn/","all");
});



function getHotWord(url,database){

	$.ajax({
	     type : "post",  
	     async:false,  
	     url : url+"hotword?database="+database,
		 dataType : "jsonp",  
	     jsonp: "callbackparam",//服务端用于接收callback调用的function名的参数  
	     jsonpCallback:"success_jsonpCallback",//callback的function名称  
		
	     success : function(data){
		
	    	 var html = "";
	    	 if(data.response=="true"){
	    		 var hotword = data.hotword.split(",");
				 var maxhotwordlist=3;
					if(maxhotwordlist>hotword.length){
					maxhotwordlist=hotword.length;
					};
	    		 for(var i=0;i<maxhotwordlist;i++){
	    			 html += "<a target='_blank' style='font-size: 12px;padding:0px 10px; color:#fff; text-decoration:none;' href='"+url+"s?q=1&database="+encodeURIComponent(database)+"&qt="+encodeURIComponent(hotword[i])+"'>" + hotword[i] + "</a>";
	    		 }
	    	 }else{
	    		 html += data.msg;
	    	 }
	    	 $("#hotword").html(html);
	    	  
	     }
		 
	
	 }

	 
	 );
}
</script>

