  var HeaderApp = {
    cacheElms: function(){
      HeaderApp.elms = {};
      HeaderApp.elms.subscriber = $('.header_subscriber').first();
    },
    init: function(SP){

      //check for sailplay sdk
      if(!SP) return;

      var lead = SP.leads.getLead('subscribe');

      lead.onCancel = function(){
        HeaderApp.closeSubscriber();
      };

      lead.onSubmit = function(res){
        if (res.status == 'ok') {
										$('.header-form').hide();
										$('.header-form-submit').show();
          SP.tags({action: 'add', tags: ['Подписка из шапки', 'Subscribe - Whitepaper 1', 'lang-ru'], email: lead.container.querySelector("[name=email]").value});
          $('.subscribe_buttons').html('<p class="subscriber_thanks">Спасибо!</p>');
          // old
          typeof window.Google !== 'undefined' && window.Google.analytics.event('form', 'submit', 'header_subscribe');
          // new
          typeof window.Google !== 'undefined' && window.Google.analytics.event('form', 'subscribe', 'subscribed_to_newsletters');
          typeof window.yaCounter24590510 !== 'undefined' && window.yaCounter24590510.reachGoal('subscribed_to_newsletters');

          setTimeout(HeaderApp.closeSubscriber, 2000);
          var form = $('<form></form>');

          form.append($('<input>', {
              type: 'text',
              val: lead.container.querySelector("[name=email]").value,
              name: 'email'
          }));
          form.append($('<input>', {
              type: 'text',
              val: 'Подписка из шапки',
              name: 'help'
          }));
//          form.append($('<input>', {
//              type: 'text',
//              val: 'ru',
//              name: 'lang'
//          }));
          form.append($('<input>', {
              type: 'text',
              val: $('[name=csrfmiddlewaretoken]').first().val(),
              name: 'csrfmiddlewaretoken'
          }));
          window.ContactUsApp && window.ContactUsApp.submitForm(form[0]);
        }
      };

      this.cacheElms();

      var $window = $(window);

      var show = false;

      $window.on('scroll', function(){
        var scr = $window.scrollTop();
        if(scr > 50 && lead.status() == 'initial'){
          if(!show) {
            show = true;
            // new
            typeof window.Google !== 'undefined' && window.Google.analytics.event('form', 'see', 'see_newsletters_form');
            typeof window.yaCounter24590510 !== 'undefined' && window.yaCounter24590510.reachGoal('see_newsletters_form');
          }
          HeaderApp.elms.subscriber.slideDown('fast');
        }
        else {
          HeaderApp.elms.subscriber.slideUp('fast');
        }
      });

    },
    closeSubscriber: function(){
      HeaderApp.elms.subscriber.slideUp('fast');
						setTimeout(function(){
								$('.header-form').show();
								$('.header-form-submit').hide();
						},500);
      // old
      typeof window.Google !== 'undefined' && window.Google.analytics.event('form', 'close', 'header_subscribe');
      // new
      typeof window.Google !== 'undefined' && window.Google.analytics.event('form', 'close', 'close_newsletter_form');
      typeof window.yaCounter24590510 !== 'undefined' && window.yaCounter24590510.reachGoal('close_newsletter_form');
    }
  };

  var LanguageSelector = {
  default_lang: 'ru',
  elements: {
    selector_wrapper: $('#lang_selector'),
    lang_list: $('#lang_selector').find('.lang_list').first(),
    langs: {}
  },
  init: function(){
    var langs = LanguageSelector.elements.lang_list.find('a');
    $.each(langs, function(index, lang){
      var l = $(lang).data().lang;
      if(l == LanguageSelector.default_lang){
        $(lang).prependTo(LanguageSelector.elements.selector_wrapper);
      }
    });
  }
};
LanguageSelector.init();

var LoginApp = {
  login_button: $('.login_button'),
  login_overlay: $('.login_overlay'),
  init: function(){
    var self = this;
    self.login_button.on('click', function(){
