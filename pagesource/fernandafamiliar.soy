<script>

function cambiaPodcast(nombreArchivo)
    {
      eliminarTodosLosNodos();
    
      var source=document.createElement('source');
      var embed=document.createElement('embed');
      var audio=document.getElementById("podcastHome");
      
      try
      {
        if (audio.canPlayType('audio/mpeg')) 
        {       
          source.type= 'audio/mpeg';
          source.src= "/podcast/" + nombreArchivo + ".mp3";
          embed.src = "podcast/" +  nombreArchivo + ".mp3";
          
        }   
        else 
        {     
          source.type= 'audio/ogg';                 
          source.src= "/podcast/" +  nombreArchivo + ".ogg";
          embed.src = "/podcast/" +  nombreArchivo + ".ogg";        
        }
      }
      catch(error)
      {     
        source.type= 'audio/mpeg';
        source.src= "/podcast/" +  nombreArchivo + ".mp3";      
        embed.src = "/podcast/" +  nombreArchivo + ".mp3";
      }
      
      audio.appendChild(source);
      audio.appendChild(embed);
      
      try
      {
        audio.pause();        
      }
      catch(error)
      {
      }
              
      try
      {
        audio.load();
        audio.play();     
      }
      catch(error)
      {
      }
      
    }
    
    function eliminarTodosLosNodos()
    {
      
      var cell = document.getElementById("podcastHome");
      if ( cell.hasChildNodes() )
      {
        while ( cell.childNodes.length >= 1 )
        {
          cell.removeChild( cell.firstChild );       
        } 
      }
    }
    
    function muestra(elemento)
    {
      elementoHTML=document.getElementById(elemento);
      
      if(elementoHTML.style.display=='block')
        elementoHTML.style.display='none';
        
      else
        elementoHTML.style.display='block';
      
    }
  
  </script>
