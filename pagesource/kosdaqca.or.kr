						getTooltipBoundary(_tooltip, e, _target);
					});
			});
		}// 행사가 있는 달일 경우 
		
		function getTooltipBoundary(_target, _e, _link) {
			if(_target[0].getBoundingClientRect) {
				var rect = _target[0].getBoundingClientRect();
				console.log("rect", rect.left, "client", _e.clientX);
				if( rect.left < _e.clientX && rect.right > _e.clientX && rect.top < _e.clientY && rect.bottom > _e.clientY)
					return;
				_link.css({'z-index': '1'});
				_target.stop().fadeOut();
			}  
		}

	})(jQuery);
