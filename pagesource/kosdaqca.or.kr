							'position': 'relative'
						}); 
						var _tooltip = $(_target.attr('data-target'));
						_tooltip.addClass('tooltip-box-show');
						_tooltip.css({
							'left': _targetBoundary.left+"px",
							'top': _targetBoundary.top+"px"
						});
						_tooltip.stop().fadeIn();
