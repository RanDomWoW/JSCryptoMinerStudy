    <script>
    // Recently Viewed Flyout
    // ------------------------------------------------
    var recentlyViewed = {
        build: function (settings) {
            var recentlyViewedUrl = $('body').data('recently-viewed-url') || '/recently-viewed.html',
                self = this;

            $.extend(recentlyViewed.settings, settings);

            $.ajax({url: recentlyViewedUrl}).success(function (response) {
                var tempFrame = $('<irame/>').addClass("tempFrame").css({
                    'display': 'none',
                    'position': 'fixed',
                    'top': '100%',
                    'left': '100%'
                }).html(response).appendTo('body');
                if (tempFrame.find('.recently-viewed-flyout').length === 1) {
                    $('.site-main').append(tempFrame.html());
                    tempFrame.remove();
                }
            }).complete(function () {
                if ($('.recently-viewed-flyout').length === 1) {
                    self.init();
                }
            });
        },
        init: function () {
            var self = this,
                element = $('.recently-viewed-flyout'),
                maxTitleLength = 48;

            element.find('.product-item').each(function (i) {
                var title = $(this).find('.title'),
                    text = title.text();
                if (text.length > maxTitleLength) {
                    title.text(text.substr(0, maxTitleLength - 1) + '...');
                }
            });

            element.on('click', '.open', function () {
                self.open();
                return false;
            });

            element.on('click', '.close', function () {
                recentlyViewed.close();
                return false;
            });

            element.on('click', function (e) {
                e.stopPropagation();
            });

            $(window).on('click', function () {
                recentlyViewed.close();
            });
        },
        open: function () {
            $('.recently-viewed-flyout').addClass('active').find('.recently-viewed-expanded').slideDown('fast');
        },
        close: function () {
            $('.recently-viewed-flyout').find('.recently-viewed-expanded').slideUp('fast', function () {
                $(this).closest('.active').removeClass('active');
            });
        }
    };

    recentlyViewed.build();
</script>



