<script>
    var setValue = function(elem, value, inc, shift, speed){
      if (elem!=null) {
        var interval = false;
        if (inc) {
            interval = setInterval(function(){
                if (elem.innerHTML*1+shift >= value) {
                    elem.innerHTML = value;
                    clearInterval(interval);
                } else {
                    elem.innerHTML = elem.innerHTML*1+shift;
                }
            }, speed);
        } else {
            interval = setInterval(function(){
                if (elem.innerHTML*1-shift <= value) {
                    elem.innerHTML = value;
                    clearInterval(interval);
                } else {
                    elem.innerHTML = elem.innerHTML*1-shift;
                }
            }, speed);
        }
      }
    };
