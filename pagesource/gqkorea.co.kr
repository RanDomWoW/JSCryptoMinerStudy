	<script>
		var startdate	= "201706190145";  
		var enddate		= "201706190245";   
				 
		var now = new Date();   //현재시간
				 
		year = now.getFullYear();   //현재시간 중 4자리 연도
		month = "0"+(now.getMonth()+1);   //현재시간 중 달. 달은 0부터 시작하기 때문에 +1 
		month = month.substring(month.length-2,month.length);   
		date = "0"+(now.getDate());      //현재 시간 중 날짜.
		date = date.substring(date.length-2,date.length);   
		hour = "0"+(now.getHours());   //현재 시간 중 시간.
		hour = hour.substring(hour.length-2,hour.length);   
		min = "0"+(now.getMinutes());   //현재 시간 중 분.
		min = min.substring(min.length-2,min.length);   
		today = year + "" + month + "" + date+ "" +hour+ "" +min;      //오늘 날짜 완성.

		if ((eval(today) >= eval(startdate)) && ((eval(today) < eval(enddate))))  {  
			var pop_w = 960;
