    <script type="text/javascript">
        function mobileCartTriggered() {
            $('#mobileCartOverlay').addClass('active');
            $('#mobileCartOverlayAdding').addClass('active');
            $('body').addClass('basketOpened');
            hideMobileMenu();
        };

        $(document).ready(function() {
            if($(window).width() > 768){
                $('.basket-wrapper').hover(
                    function() {
                        if (!$(this).hasClass('open')) {
                            cartContent();
                        };
                        $(this).addClass('open');
                        $('#desktopPageOverlay').addClass('active');
                    },
                    function() {
                        $(this).removeClass('open');
                        $('#desktopPageOverlay').removeClass('active');
                    }
                );

            }else{
                $('.basket-wrapper').find('.dropdown-toggle').click(function() {
                    $('.basket-wrapper').addClass('open');
                    mobileCartTriggered();
                });

            };

            $('#mobileCartOverlay').click(function() {
                $('.basket-wrapper').removeClass('open');
                $(this).removeClass('active');
                $('#mobileCartOverlayAdding').removeClass('active');
                $('body').removeClass('basketOpened');
            });

            $('#mobileCartOverlayAdding').click(function() {
                $('.basket-wrapper').removeClass('open');
                $(this).removeClass('active');
                $('#mobileCartOverlay').removeClass('active');
                $('body').removeClass('basketOpened');
            });

            $('.middle-header.sticky-on-mobile .overlayed').click(function() {
                $('.basket-wrapper').removeClass('open');
                $('#mobileCartOverlay').removeClass('active');
                $('body').removeClass('basketOpened');
            });

            //Sticky basket wrapper action
            $('.sticky-basket-wrapper').hover(
                function() {
                    cartContent();
                    $(this).addClass('open');
                },
                function() {
                    $(this).removeClass('open');
                }
            );
        });
    </script>


