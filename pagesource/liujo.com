            var setCookie = function (c_name, value, exdays) {
                var exdate = new Date();
                exdate.setDate(exdate.getDate() + exdays);
                var c_value = escape(value) + ((exdays == null) ? "" : "; expires=" + exdate.toUTCString());
                string = c_name + "=" + c_value + ";domain=" + '';
                document.cookie = string;
            };

            var setCookiesStore = function (country, locale) {
                setCookie('geoip_country', country, 7);
                setCookie('geoip_locale', locale, 7);
                return false;
            };
        </script>
