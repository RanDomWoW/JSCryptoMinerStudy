<script type="text/javascript">

    var ricercaEffettuata = false;
    function DoSearch(query) {
        if (!ricercaEffettuata) {
            var searchUrl = '/it-IT-it/Ricerca.aspx?ev0='
            var txtSearch = query;
            txtSearch = txtSearch.replace(/[^a-zA-Z 0-9 \-]+/g, ' ');
            txtSearch = jQuery.trim(txtSearch);
            if (txtSearch != "") {
                var cleanSearch = encodeURI(txtSearch);
                document.location.replace(searchUrl + cleanSearch);
            } else {
                return false;
            }
        }
        ricercaEffettuata = true;
    }

    // Inizio funzione autocompletamento
    // La ricerca dei suggerimenti è effettuata tramite una chiamata json al servizio di lucene. 
    // La funzione gestisce:
    // 1) la cache a livello di pagina per le ricerche effettuate, in modo tale da limitare le chiamate al servizio.
    // 2) l'autofiltro dei suggerimenti in base al risultato della richiesta precedente nel caso in cui la ricerca attuale non è stata ancora indicizzata, in modo tale da restituire 
    //    comunque dei risultati in linea con la ricerca precedente
    var cache = {}, lastdata, lastterm, dataSource = [], fineRicerca = false;

    function getdata(term, process) {
        $.getJSON('/api/SuggerimentiRicerca.ashx?numeroMassimoSuggerimenti=10&term=' + term, function (data, status, xhr) {
            if (data == null && lastdata != null && term.match("^" + lastterm) == lastterm) {
                // La ricerca non ha dato risultati ma è congruente con la ricerca precedente che ha dato risultati => ritorna ricerca precedente filtrata da autocomplete
                dataSource = lastdata;
                fineRicerca = true;
            }
            else if (data == null) {
                // Non ha dato risultati e le ricerche precedenti non hanno dato risultati
                lastdata = [];
                dataSource = [];
                lastterm = term;
            }
            else {
                // la ricerca ha dato risultati, quindi viene messa in cache
                cache[term] = data;
                lastdata = data;
                dataSource = data;
                lastterm = term;
            }
            return process(dataSource);
        });
    };

    function SearchKeyPress(event, sender) {
        if (event.which == 13) {
            DoSearch($(sender).val());
            return false;
        }
    }

</script>
