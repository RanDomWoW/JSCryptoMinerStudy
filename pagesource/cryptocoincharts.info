					<script>
					//toggle class after clic on +
					$(document).on("click", ".watch_btn", function(e){
						e.preventDefault();
						$(this).toggleClass('minus');
					});
					
					$(document).on("click", ".vote_btn", function(e){
						e.preventDefault();
					});
					
					function alert_box(){
						$('.alert_box .alert_item:first-child').hide(400, function() {
							$(this).remove();
						});
					}
					
					$('.watch_btn').click(function(){
						var coinID = $(this).data('coinid'),
							crypto = $(this).data('crypto'),
							$t = $(this);
						
						$.ajax
						({ 
							url: '/fast/2fa_watchaddrem.php',
							data: {"coinID": coinID, "crypto": crypto},
							type: 'post',
							success: function(result)
							{
								if(result['status']==1)
								{
									if(result['action']=='remove')
									{
										$('.alert_box').append('<div class="alert_item alert_minus"><span class="check"></span>'+result['msg']+' <span class="close"></span></div>');
										$t.removeClass('minus');
										$t.addClass('plus');
									}
									else
									{
										$('.alert_box').append('<div class="alert_item alert_plus"><span class="check"></span>'+result['msg']+' <span class="close"></span></div>');
										$t.removeClass('plus');
										$t.addClass('minus');
									}
								}
								else
								{
									$('.alert_box').append('<div class="alert_item alert_minus">'+result['msg']+' <span class="close"></span></div>');
								}
								
								setTimeout(alert_box, 2500);
							}
						});
					});
					
					
					$(".vote_btn[data-updown='down']").click(function(){
						var coinID = $(this).data('coinid'),
							crypto = $(this).data('crypto'),
							updown = 'down',
							$t = $(this);
						
						$.ajax
						({ 
							url: '/fast/2fa_voteupdown.php',
							data: {"coinID": coinID, "crypto": crypto, "updown": updown},
							type: 'post',
							success: function(result)
							{
								if(result['status']==1)
