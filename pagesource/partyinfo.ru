<script>

	var button = $('#req_event_4');
	$(button).click(function(){
		var win = _.template($('#add_req_form').text(), {cid:'', name: '', fam:'', otch:'', telefon:'', email:'', dolzhnost:''});
		window_open.up('', win, 1);
		check_contact_error();
		$("[name=contact_telefon]").mask("+9 999 999 99 99");
		$( "#datepicker_ord" ).datepicker();
	});


	function add_req_offer(form)
	{
		$('#form_add_contact label.error').remove();
		$('#form_add_contact input.error').removeClass('error');
		var formInputs = $('#form_add_contact').find('input, select, textarea');
		var cid = $(form).data('cid');
		var error = false;
		var data = {};
		for (var k in formInputs) {
			if (formInputs[k].type == 'hidden' || empty(formInputs[k].tagName))
				continue;
			var par = $(formInputs[k]).parents('.in_val_small');
			var name = $(par).find('label:not(.error)').html();

			if ($(formInputs[k]).attr('name') == 'contact_email') {
				//continue;
			}

			if (empty($(formInputs[k]).val())) {
				if ($(formInputs[k]).attr('name') == 'data_ord') {
					$(formInputs[k]).after('<label class="error">Вы не указали дату</label>');
					$(formInputs[k]).addClass('error');
				} else if ($(formInputs[k]).attr('name') == 'guest_count') {
					$(formInputs[k]).after('<label class="error">Укажите количество гостей</label>');
					$(formInputs[k]).addClass('error');
				}	else {
					$(par).append('<label class="error">Заполните поле</label>');
					$(par).find('input').addClass('error');
				}
				error = true;
			}
			else {
				data[formInputs[k].name] = $(formInputs[k]).val();

				if ($(formInputs[k]).attr('name') == 'req_all') {
					data[formInputs[k].name] = $('[name=req_all]').is(":checked");
				}
				if ($(formInputs[k]).attr('name') == 'req_artist') {
					data[formInputs[k].name] = $('[name=req_artist]').is(":checked");
				}
					if ($(formInputs[k]).attr('name') == 'req_mesto') {
					data[formInputs[k].name] = $('[name=req_mesto]').is(":checked");
				}
				if ($(formInputs[k]).attr('name') == 'contact_telefon') {
					var re  = /^[\+\d][\d\(\)\ -]{4,19}\d$/;
					if (!re.test($(formInputs[k]).val())) {
						$(par).append('<label class="error">Номер телефона введен неправильно</label>');
						$(par).find('input').addClass('error');
					}
				}
				if ($(formInputs[k]).attr('name') == 'contact_email') {
					if (!validateEmail($(formInputs[k]).val())){
						$(par).append('<label class="error">Некорректный E-mail адрес</label>');
						$(par).find('input').addClass('error');
						error = true;
					}
				}
			}

		}
		if (error)
			return false;

		$.ajax({
			url: baseUrl +'ajax=req_offer',
			async: false,
			type: 'POST',
			data: data,
			success: function (data) {
				try {
					try {
						var result = jQuery.parseJSON(data);
						if (result.errors) 
							alert(result.errors);
					}catch (e) {
						$('.reveal-modal.large .off_container').html(data);
						$("html, body").animate({ scrollTop: 0 }, "fast");
					}
				}
				catch (e) {
					str = !empty(e.message) ? e.message : '';
					window_open.up('Системное уведомление','Ошибка данных!<br>'+str);
					console.log('error_name: '+e.name);
					console.log('error_stack: '+e.stack);
				}
				return false;
			}
		});
		return false;
	}


	function check_contact_error() {
		$('#form_add_contact').click(function(event){
			if (checkButton(event, 'error')){
				var input = checkButton(event, 'error');
				if (!empty(input[0].type)) {
					$(input).removeClass('error');
					$(input).parents('.in_val_small').find('label.error').remove();
				}
			}
		});
	}
</script>


