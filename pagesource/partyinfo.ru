<script>
	var no_valid_email = "Некорректный E-mail адрес";
	var button = $('#subscribe_btn');

	$(button).click(function(){

		var sub_email = $("#subscribe_email").val();
		if (!sub_email) return false;

		if (!validateEmail(sub_email)) {
			return alert(no_valid_email), false;
		}
		_subsribe(sub_email);
		//var win = _.template($('#add_subscribe_form').text(), {email:sub_email});
		//window_open.up('', win, 1);
		//check_contact_error();
	});

	function _subsribe(email)
	{
		var data = {
			email:email
		};
		$.ajax({
			url: baseUrl +'ajax=news_subscribe',
			async: false,
			type: 'POST',
			data: data,
			success: function (data) {
				var data = $.parseJSON(data);
				if (data.errors) {
					window_open.up('Системное уведомление',data.errors +'<br>');
				} else if (data.success){
					$('.reveal-modal-bg').trigger('click');
					if (data.success == "need_name") {
						var win = _.template($('#add_subscribe_form').text(), {email:email});
						window_open.up('', win, 1);
					} else if (data.success == "need_pass") {
						var win = _.template($('#add_subscribe_form_with_pass').text(), {email:email});
						window_open.up(' ', win, 1);
					} else {
						var win = _.template($('#subscribe_complete').text(), {});
						window_open.up(' ', win, 1);
					}
				}

			}
		})
	}

	function add_req_subsribe(form)
	{
		$('#form_add_subscr label.error').remove();
		$('#form_add_subscr input.error').removeClass('error');

		var email =$("[name=subscribe_email]").val();
		var name = $("[name=subscribe_name]").val();
		var pass =$("[name=subscribe_pass]").val();

		var cid = $(form).data('cid');
		var error = false;
		var data = {
			email:email,
			pass:pass,
			name:name,
			form:'subsr'
		};
		$.ajax({
			url: baseUrl +'ajax=news_subscribe&form=subsc',
			async: false,
			type: 'POST',
			data: data,
			success: function (data) {
				var sub_email =  $("[name=subscribe_email]").val();
				try {
					var data = $.parseJSON(data);
					if (data.errors) {
						window_open.up('Системное уведомление',data.errors +'<br>');
					} else if (data.success){
						$('.reveal-modal-bg').trigger('click');
						if (data.success == "need_pass") {
							var win = _.template($('#add_subscribe_form_with_pass').text(), {email:sub_email});
							window_open.up(' ', win, 1);
						} else if (data.success == "need_name") {
						var win = _.template($('#add_subscribe_form').text(), {email:email});
						window_open.up('', win, 1);
						} else {
							var win = _.template($('#subscribe_complete').text(), {});
							window_open.up(' ', win, 1);
						}
					}
				}
				catch (e) {
					str = !empty(e.message) ? e.message : '';
					window_open.up('Системное уведомление','Ошибка данных!<br>'+str);
					console.log('error_name: '+e.name);
					console.log('error_stack: '+e.stack);
				}
				return false;
			}
		});
		return false;
	}


	function check_contact_error() {
		$('#form_add_contact').click(function(event){
			if (checkButton(event, 'error')){
				var input = checkButton(event, 'error');
				if (!empty(input[0].type)) {
					$(input).removeClass('error');
					$(input).parents('.in_val_small').find('label.error').remove();
				}
			}
		});
	}
</script>
