<script>
	window.YaMetrikaTargetsKontakt = function () {};

	window.YaMetrikaTargetsKontakt.prototype.targetRef = false;

	window.YaMetrikaTargetsKontakt.prototype.init = function ()
	{
		this.processTagLink = $.proxy(this.processTagLink, this);
		this.kontaktClick = $.proxy(this.kontaktClick, this);

		$('a.info[data-info=contacts]').click(this.kontaktClick);
	};

	window.YaMetrikaTargetsKontakt.prototype.processTagLink = function ($el)
	{
		var href = $el.prop('href');
		var regExp = /(?:http\:\/\/.*?partyinfo\.ru)?\/([a-z]+)(\_\d+)?(\=([a-z\_]+))?/;
		var res = href.match(regExp);

		if (!res) return;
		if (!Array.isArray(res)) return;
		if (!res[1]) return;
		if (!res[4]) return;

		var eventName = res[1] + '_kontakt_' + res[4];

		return this.makeEvent(eventName);
	};

	window.YaMetrikaTargetsKontakt.prototype.kontaktClick = function (e)
	{
		var th = this;
		var $target = $(e.target);
		var $tags = $target.parents('.place_item').find('.desc .tags a');

		if (this.targetRef)
			return this.makeEvent(this.targetRef);

		$tags.each(
			function (k, el)
			{
				var $el = $(el);
				th.processTagLink($el);
			}
		)
	};

	window.YaMetrikaTargetsKontakt.prototype.makeEvent = function (name)
	{
		if (typeof window[this.yaCounter] != 'object') return;

		var yaCounter = window[this.yaCounter];

		if (typeof yaCounter.reachGoal == 'function')
		{
			console.log('target: ', name);
			yaCounter.reachGoal(name);
		}
	};

	yaMetrikaTargetsKontakt = new YaMetrikaTargetsKontakt();
	yaMetrikaTargetsKontakt.targetRef = false;
	yaMetrikaTargetsKontakt.yaCounter = 'yaCounter11476786';
	yaMetrikaTargetsKontakt.init();

</script>
