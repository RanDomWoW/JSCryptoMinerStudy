                '<div class="sku" style="display: none">{{sku}}</div>' +
                '</div>' +
                '<div class="id" style="display: none">{{id}}</div>' +
                '</div>' +
                '</li>',
        templateAfterText:
                '',
        initialize: function(data, currentBlockSelector) {
            this.data = data;
            this.container = $$(currentBlockSelector)[0];
            this.container.innerHTML = '';

            if (this.data.length > 0) {
                var textToInsert = this.templateBeforeText;

                this.template = new Template(this.templateText, this.templateSyntax);
                for (var i = 0; i < this.data.length; i++)
                {
                    textToInsert += this.template.evaluate(this.data[i]);
                }
                textToInsert += this.templateAfterText;
                this.container.insert({'bottom': textToInsert});
            } else {
                this.container.innerHTML = '<li>There is no products matching the selection.</li>';
            }
        }
    });
