        templateBeforeText:
                '',
        templateText:
                '<li class="item">' +
                '<a class="thumbnail" href="{{url}}">' +
                '<img width="50" height="75" src="{{image}}" alt="{{name}}" title="{{name}}"/>' +
                '</a>' +
                '<div class="caption">' +
