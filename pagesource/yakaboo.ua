    // Перевіряємо чи треба питати Email
    function checkAskEmail(firstName, lastName, id)
    {
        var url = 'http://www.yakaboo.ua/clnetworking/customer_account/checkVkontakte/';
        var parameters = {
            id: id
        };
        new Ajax.Request(url, {
            method: 'post',
            parameters: parameters,
            onSuccess: function(data) {
                if(data && data.responseJSON)
                {
                    var res = data.responseJSON;
                    if (res.error)
                    {
                        alert(res.message);
                        return;
                    }
                    if (res.require)
                    {
                        askEmail(firstName, lastName, id);
                        return;
                    }
                    var url = 'http://www.yakaboo.ua/clnetworking/customer_account/vkontakte/' + '?name=' + firstName + '&last=' + lastName + '&id=' + id;
                    window.location = url;
                }
            },
            onError: function(data) {
                aller("Ошибка запроса");
            }
        });
    }
