    jQuery(window).load(function() {

        jQuery('a[data-toggle="tab"]').on('shown', function(e) {
            var currentTabSelector = '.bestsellers > div.content-primary > div.active';
            var tabId = jQuery(currentTabSelector).attr('id');
            var currentDivId = jQuery('#' + tabId + ' div.active').attr('id');
            var currentBlockSelector = '#' + currentDivId + ' ul';

            if (jQuery(currentTabSelector).find('div.content-secondary > div.active ul').hasClass('ajax-content-loaded')) {
                //console.log('Ajax already loaded.');
                return;
            }

            var urlBestSellers = 'http://www.yakaboo.ua/yakaboo_homepage/ajax/getBestSellers/';
            var urlNewArrivals = 'http://www.yakaboo.ua/yakaboo_homepage/ajax/getNewArrivals/';
            var urlForSale = 'http://www.yakaboo.ua/yakaboo_homepage/ajax/getForSale/';

            var hrefParts = this.href.split('_');
            var buttonType = hrefParts[hrefParts.length - 2];
            var categoryId;

            if (buttonType == 'secondary') {
                categoryId = hrefParts[hrefParts.length - 1]; // get the last part, it is category ID
            } else { // get current category id
                var primaryId = jQuery(e.target).attr('href');
                hrefParts = jQuery(primaryId + ' .tabs-secondary .active a').attr('href').split('_');
                categoryId = hrefParts[hrefParts.length - 1];
            }

            /* Depending on chosen tab, using URL with appropriate Action */
            var url = null;
            switch (tabId)
            {
                case 'bestsellers_primary_1': /* Best Sellers */
                    url = urlBestSellers;
                    break;
                case 'bestsellers_primary_2': /* New Arrivals */
                    url = urlNewArrivals;
                    break;
                case 'bestsellers_primary_3': /* For Sale */
                    url = urlForSale;
                    break;
            }

            var parameters = {
                category: categoryId
            };

            new Ajax.Request(url, {
                method: 'get',
                parameters: parameters,
                onComplete: function(transport) {
                    jQuery(currentBlockSelector).addClass('ajax-content-loaded');
                    /* TODO: check if we can avoid creating a new "AjaxProducts" object each ajax call */
                    new Yakaboo.AjaxBestsellersProducts(transport.responseText.evalJSON(), currentBlockSelector);


                    var array_product_view = [];
                    jQuery(currentBlockSelector + ' li.item').hover(function () {
                        var onthe_id = jQuery('.id', this).text();
                        var onthe_url = jQuery('a.thumbnail', this).attr('href');

                        var onthe_price = jQuery('#product-price-' + onthe_id + ' span span.price').first().text();
                        if (!onthe_price) {
                            onthe_price = jQuery('#product-price-' + onthe_id).first().text();
                        }

                        onthe_price = onthe_price.replace(/[^.\d]+/g, "").replace(/^([^\.]*\.)|\./g, '$1');
                        onthe_price = onthe_price * 100;

                        var onthe_category = [];
                        jQuery('ul.breadcrumb li a span').each(function(i) { if (i > 0) onthe_category.push(jQuery(this).text()) });

                        var onthe_name = jQuery('.product-name a', this).text();

                        var onthe_sku = jQuery('.sku', this).text();

                        var onthe_available = 'yes';

                        var try_event = 1;
                        jQuery.each(array_product_view, function (i, val) {
                            if (onthe_id == val) {
                                try_event = 0;
                            }
                        });

                        if (try_event == 1) {
                            array_product_view.push(onthe_id);
                            io_event_hover_product(onthe_id, onthe_url, onthe_name, onthe_category, onthe_price, onthe_available, onthe_sku)
                        }
                    });
                }
            });
        });

        jQuery('.bestsellers > .tabs-primary > li.active a').trigger('shown');

        // All Secondary Tabs switcher
        jQuery(".bestsellers .tabs-secondary a[data-toggle='tab']").on("click", function(event){
            var target = event.target;

            jQuery("section.bestsellers > div.tab-content > div.active .tabs-secondary a[data-categoty-id='" + $(target).attr("data-categoty-id") + "']").tab("show")
        });

    });
