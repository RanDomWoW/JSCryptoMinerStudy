            if (localStorage.getItem('recently_viewed_products') !== null) {

                var viewedProducts = JSON.parse(localStorage.getItem('recently_viewed_products'));
                var countAdded = 0;

                var tplName = 'recent';
                var tpl = new EJS({text: yakaboo.templates.tpl[tplName], name: tplName});

                for(var i = viewedProducts.length - 1; i >= 0; i--) {

                    if( currentProductId == viewedProducts[i]['productId'] ) {
                        continue;
                    }

                    if( countAdded >= 8 ) {
                        break;
                    }

                    var html = tpl.render(viewedProducts[i]);

                    jQuery('#recently-viewed-ul').append(html);

                    countAdded++;
                }

                if( viewedProducts.length ) {
                    jQuery('#recently-viewed').show();
                }
            }
        }

        if(currentProductId)
        {
            /// Счетчик просмотренных товаров для пользователя
            jQuery.ajax({
                url: 'http://www.yakaboo.ua/catalog/product/userViewed/',
                dataType:'json',
                type: 'POST',
                data: {
                    id :  currentProductId
                },
