	<script type="text/javascript">

		var footerRequestBtnParams = new Object();

		//add get params to request button
		function refreshFooterRequestBtn(startDateFooterBtn, durationFooterBtn, personsFooterBtn, locationFooterBtn) {

			//objectNumber
			if(typeof objectNumber != 'undefined') {
				footerRequestBtnParams.objectNumber = objectNumber;
			}

			//startDate
			var startDateFooterBtn = startDateFooterBtn || '';
			if(startDateFooterBtn != '') {
				footerRequestBtnParams.startDate = startDateFooterBtn;
			}
			else {
				if(typeof startDateVal != 'undefined' && startDateVal != '') {
					footerRequestBtnParams.startDate = startDateVal;
				}
				else if(typeof startDateValue != 'undefined' && startDateValue != '') {
					footerRequestBtnParams.startDate = startDateValue;
				}
				else if($('[name="startDate"]').length > 0 && $('[name="startDate"]').val() != '') {
					footerRequestBtnParams.startDate = $('[name="startDate"]').val();
				}
			}

			//duration
			var durationFooterBtn = durationFooterBtn || '';
			if(durationFooterBtn != '') {
				footerRequestBtnParams.duration = durationFooterBtn;
			}
			else {
				if($('[name="duration"]').length > 0 && $('[name="duration"]').val() != '') {
					footerRequestBtnParams.duration = $('[name="duration"]').val();
				}
			}

			//persons
			var personsFooterBtn = personsFooterBtn || '';
			if(personsFooterBtn != '') {
				footerRequestBtnParams.persons = personsFooterBtn;
				footerRequestBtnParams.adults = personsFooterBtn;
			}
			else {
				if($('[name="personen"]').length > 0 && $('[name="personen"]').val() != '') {
					footerRequestBtnParams.persons = $('[name="personen"]').val();
					footerRequestBtnParams.adults = $('[name="personen"]').val();
				}
			}

			//location
			var locationFooterBtn = locationFooterBtn | '';
			if(locationFooterBtn != '') {
				footerRequestBtnParams.fulltext = locationFooterBtn;
			}
			else {
				if($('[name="fulltext"]').length > 0 && $('[name="fulltext"]').val() != '') {
					footerRequestBtnParams.fulltext = $('[name="fulltext"]').val();
				}
			}

			//overwrite href with params url
			$('.requestBtnFooterBar').attr('href', '/request?' + $.param(footerRequestBtnParams));
		}

		$(function () {
			if(typeof breakpoint != 'undefined' && breakpoint == 'xs' && $('.requestBtnFooterBar').length > 0) {

				refreshFooterRequestBtn();

				$(document).on('change', '[name="startDate"]', function(event){
					if($(this).val() != '') {
						refreshFooterRequestBtn($(this).val());
					}
				});
				$(document).on('change', '[name="duration"]', function(event){
					if($(this).val() != '') {
						refreshFooterRequestBtn('', $(this).val());
					}
				});
				$(document).on('change', '[name="personen"], [name="personsSelect"]', function(event){
					if($(this).val() != '') {
						refreshFooterRequestBtn('', '', $(this).val());
					}
				});
				$(document).on('change', '[name="fulltext"]', function(event){
					if($(this).val() != '') {
						refreshFooterRequestBtn('', '', '', $(this).val());
					}
				});
			}
		});
	</script>

