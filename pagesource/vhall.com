<script type="text/javascript">
    //判断弹出层链接是否和当前地址相同，如果相同，弹出框不弹出
    function addCookie(cname, cvalue, exdays){
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + d.toUTCString();
        var cookieStr = getCookie(cname);
        cookieStr += '|' + cvalue;
        document.cookie = cname + "=" + cookieStr + "; " + expires + ';path=/';
    }

    function getCookie(cname){
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') c = c.substring(1);
            if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
        }
        return "";
    }
    var userUrl = $("#infiniteModal a").attr("href");
    var pageUrl = window.location.href;
    if(getCookie('infiniteModalShow') == '') {
        var showFlage = true;
        $(window).on('scroll', function() {
            var opa = $(this).scrollTop();
            if(opa >= 450 && showFlage && $(".applyTopSection").length == 0 && pageUrl.indexOf(userUrl)<0) {
                addCookie("infiniteModalShow", 1,525600);
                $("#infiniteModal").fadeIn();
                showFlage = false;
            }
        })
        $(".infiniteClose").on('click', function() {
            // $(window).off('scroll');
            $("#infiniteModal").fadeOut();
        })
    }
