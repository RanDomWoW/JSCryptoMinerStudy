    var registrationLinkId = null;
    
    function sendRegistrationTracking(){
        var oldcontentId = wt.contentId;
        wt.contentId = "registrierung.uebersicht";
        wt.contentGroup = {
            1 : "Registrierung",
            10: "registrierung"
        };
