        if (registrationLinkId != null){
            wt.linkId = registrationLinkId;
        }else if (oldcontentId == 'Ihr-Vorteil'){
            wt.linkId = 'registration_Ihr_Vorteil';
        }else if (oldcontentId == 'So geht´s'){
            wt.linkId = 'registration_so_gehts';
        }else if (oldcontentId == 'Info-Anmeldung'){
            wt.linkId = 'registration_info_anmelden';
        }
        wt.sendinfo();

        wt.linkId = null;
    };
    
    function setRegistrationLinkId(linkId){
        registrationLinkId = linkId;
    }
