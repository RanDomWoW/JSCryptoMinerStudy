<script>
    var imgAll = 0;
    var conv = 0;
    var bconv = 0;
    var qLimageContainer = "";
    $(function () {
        qLimageContainer = $("<div></div>").appendTo("body").css({
            display: "none",
            width: 0,
            height: 0,
            overflow: "hidden"
        });
        conv = $(".lx-index-area").size();
        imgAll = $(".lx-focus .swiper-slide").size();
        $(".lx-focus .swiper-slide").each(function () {
            findImageInElement(this);
        });
    });

    var findImageInElement = function (element) {
        var url = "";
        if ($(element).css("background-image") != "none") {
            url = $(element).css("background-image");
            url = url.replace(/url\(\"/g, "");
            url = url.replace(/url\(/g, "");
            url = url.replace(/\"\)/g, "");
            url = url.replace(/\)/g, "");
        }

        var image = $("<img />").attr("src", url).bind("load error", function () {
            imgAll--;
            if (imgAll == 0 && conv > 0) {
                findImageNext();
            }
        }).appendTo(qLimageContainer);

    }

    var findImageNext = function () {
        imgAll = $(".lx-index-area").eq(bconv).find("[data-src]").size();
        $(".lx-index-area").eq(bconv).find("[data-src]").each(function () {
            var imgdoc = $(this);
            var image = $("<img />").attr("src", $(this).attr("data-src")).bind("load error", function () {
                imgAll--;
                imgdoc.attr("src", imgdoc.attr("data-src"));
                if (imgAll == 0) {
                    if (bconv < conv) {
                        bconv++;
                        findImageNext();
                    } else {
                        $(qLimageContainer).remove();
                    }

                }
            }).appendTo(qLimageContainer);
        });
    }
</script>





