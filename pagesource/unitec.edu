<script>
	$(window).load(function(){
		var $screen = $(this),
			backtotop = $('.backtotop'),
			winHeight = $screen.height()/3;
		
		function scrollHandler(){
			var scrollTop = $screen.scrollTop();
			if(scrollTop > winHeight) backtotop.fadeIn(500);
			else backtotop.fadeOut(500);
		}
		
		$screen.bind('scroll', scrollHandler);
		scrollHandler();
		
		$('.backtotop').click(function(){
            $("html, body").animate({ scrollTop: 0 }, 1000);
            return false;
        });
	});
	
	$("a[href='#calendario-actividades']").click(function() {
		//$("html, body").animate({ scrollTop: $(document).height() }, 1000);
		$("html, body").animate({ scrollTop: $(document.getElementById("home")).height()+120 }, 1000);
		return false;
	});
</script>    




