            <script>
/* ***************************

    setupExpandingAd: Example usage.

    This only changes the height of a dfp ad's container for ads that expand. Can handle delay on hover event before triggering expansion


    Example javascript to implement:

    jQuery(document).ready(function($){
            var pushdown_options = {
                'dfp_id' : 'dfp-hp-pushdown',   // required
                'open_height' : 415,            // required
                'default_height' : 90,          // required
                'delay' : 400,                  // optional, if you don't put any value it will expand immediately on hover
            };
            BWY.setupExpandingAd(pushdown_options);
    });


*****************************/

(function($){

    BWY.setupExpandingAd = function ( options ) {
    
        var options = options || {};
    
        var open_height    = options.open_height    || 415;
        var default_height = options.default_height || 90;
        var delay          = options.delay          || 0;
        var $container = $('#'+options.dfp_id);
    
        var timeouts = [];
    
        var notTouch = document.documentElement.className.indexOf('no-touch') > -1;
    
        /* We have to change the heights on both the iframe and its containing div otherwise there's overflow clipping */
    
        function expandElement( $el, key ) {
            /* since multiple elements need to be expanded we create different keys for their respective timeouts so we can
                clear those timeouts if necessary */
    
            timeouts[key] = setTimeout(function(){
                $el.height(open_height);
            }, delay)
        }
    
        function collapseElement( el, key ) {
            clearTimeout(timeouts[key]);
            $(el).height(default_height);
        }
    
        if ( notTouch ){
            $container.on('mouseover', 'iframe', function(){
                var $el = $(this);
                /* this has to be in the on handler instead of outside because the iframe is added asynchronously */
                var w = $el.width();
                if ( w <=300 || w >= 940  ){
                    expandElement($el, 'iframe');                        
