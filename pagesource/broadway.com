                <script>
                    (function($){
                        var $searchField =  $('#search-input')
                        var showsEngine = new Bloodhound({
                            datumTokenizer: function(datum){
                                var show_names      = Bloodhound.tokenizers.whitespace(datum.value);
                                var alternate_terms = Bloodhound.tokenizers.whitespace(datum.alternate_terms.split(','));
                                return show_names.concat(alternate_terms);
                            },
                            queryTokenizer: Bloodhound.tokenizers.whitespace,
                            limit: 3,
                            prefetch: {
                                url: '/api/v1/typeahead/?domain=1',
                                ttl: 1800000,
                                filter: function(data){
                                    return $.map(data.objects, function(item) {
                                        var showImage = item.show.lead_image;
                                        if (showImage === null){
                                            showImage =  "https://d1069swflop3lk.cloudfront.net/img/onyx/search/curtain-ph-289-86.5ac2bb5e57d2.jpg"
                                        }
                                        return {
                                            value: item.show.name,
                                            url: item.show.get_absolute_url,
                                            image: showImage,
                                            tickets: item.display_buy_tickets,
                                            id: item.id,
                                            alternate_terms: item.show.typeahead_terms
                                        }
                                    })
                                }
                            }
                        });

                        // kicks off the loading/processing of `local` and `prefetch`
                        showsEngine.initialize();
                        $searchField.typeahead(
                            {
                                hint: false,
                                highlight: false,
                                minLength: 3
                            },
                            {
                            value: 'showsEngine',
                            displayKey: 'value',
                            source: showsEngine.ttAdapter(),
                            templates: {
                                suggestion: function(data){
                                    return '<a class="relative" href="' + data.url + '" title="'+  data.value + '">' +
                                                '<img src="'+ data.image + '"/>' +
                                                '<div class="tt-text-container phx">' +
                                                    '<div class="tt-text-wrapper">' +
                                                        '<div class="tt-text white-thick-text">' + data.value + '</div>' +
                                                    '</div>' +
                                                '</div>' +
                                            '</a>'
                                },
                                footer: function(){
                                    var searchInstance = $searchField.typeahead('val');
                                    return '<span class="tt-menu-addon text-center">' +
                                                '<a href="/search/?q='+ searchInstance + '#nav-auto-all' + '">' +
                                                    '<span class="font-14">View All Results for <br></span>' +
                                                    '<span class="font-hv font-18">"'+ searchInstance + '"</span>' +
                                                '</a>' +
                                            '</span>'
                                }
                            }
                        });

                        $searchField.on('typeahead:selected', function(obj, datum, name){
                            if(datum.url == undefined){
                                window.location = 404;
                            }
                            //send users that click with keyboard to -e
                            window.location = datum.url;
                        });

                        $searchField.one('focus', function(){
                            var category = 'Search';
                            var action = category + ': Global Nav Show Search focused';
                            // label is not used for this event tracking
                            ga('send','event',{
                                'eventCategory' : category,
                                'eventAction'   : action
                            });
                        });
                    }(jQuery));
