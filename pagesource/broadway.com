                jQuery(document).ready(function($){
                    // NAV Clicking JS for Desktop/Touch
                    if (BWY.userAgent.isTouchDevice){
                        var this_tab,
                            this_clicked = null;

                        $('.navbar-global-js').on('touchstart', '.navbar-wrap-js ul li .trigger ', function(e){

                            // first click should activate drop-down, 2nd should go to href or deactivate hover if no href
                            var $this = $(this).parent('li');
                            var no_flyout = $this.hasClass('no-flyout');
                            this_tab = $this.text();

                            if (this_clicked === this_tab || no_flyout ) {
                                // has clicked on this tab before or has no flyout
                                this_clicked = null; // in case tab doesn't have href
                                $this.removeClass('hover');
                                return true;
                            } else {
                                // clicked on a different tab previously
                                // OR clicked on a tab with no href
                                e.preventDefault();
                                this_clicked = this_tab;
                                $this.parent('.navbar-list-js').children('li').addBack().removeClass('hover');
                                $this.parent('.navbar-list-js').siblings().children('li').removeClass('hover');
                                $this.addClass('hover');
                            }
                        });
                        $('body').on('touchstart', function(e) {
                            // Legacy support to be removed after onyx integration

                            if ( !$(e.target).is( '.trigger, .navbar-flyout-menu li *, .trigger span, .navbar-list-js .flyout * ') ) {
                                $('.navbar-list__item-js, .navbar-list-js li').removeClass('hover');
                                this_clicked = null;
                            }
                        })
                    } else {
                        $('.contains-flyout').mouseleave(function(){
                            $(this).removeClass('hover');
                        });
                    }
                });
            </script>

            

            
