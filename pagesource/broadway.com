            <script>
            jQuery(document).ready(function($){

                var $select = $('.selectlist-js');

                
                
                var full = window.location.host;
                var parts = full.split('.');
                var sub = parts[0];

                
                
                var languageOptions = [];
                $select.each(function(){
                    languageOptions[$(this).val()] = true;
                });
                sub = languageOptions[sub] || 'en';
                $select.selectlist('selectByValue', sub);


                $select.on('changed.fu.selectlist', function(evt, data){
                    var language;
                    var currentUrl;
                    var currentJoin;
                    var dropDownList = document.querySelectorAll(".selectlist-js li");
                    var vals = [];
                    var check;
                    var currentUrlParts;

                    language = data.value;
                    if (language === 'en') {
                        language = '';
                    } else {
                        language += '.';
                    }
                    currentUrl = window.location.href;
                    for (var i = 0; i< dropDownList.length; i++) {
                        vals.push(dropDownList[i].getAttribute('data-value'));
                    }

                        

                        var check = function(){

                            if (sub == 'www' || '0' == '2'){
                                
                                var currentUrlParts = currentUrl.split('.');
                                currentUrlParts.splice(0,1);
                                currentJoin = currentUrlParts.join(".");
                                return currentJoin;
                            } else {

                                for (i=0;i<vals.length;i++){
                                     if (sub == vals[i]){
                                     
                                          var currentUrlParts = currentUrl.split('.');
                                          currentUrlParts.splice(0,1);
                                          currentJoin = currentUrlParts.join(".");
                                          return currentJoin;
                                     }

                                 }
                            
                            
                            currentJoin = currentUrl.replace(/.*?:\/\//g, "");
                            return currentJoin;
                        }
                    };

                    window.location = "https://" + language + check();
                });

              });
