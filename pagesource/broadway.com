                        function vanillaModal( element, closer, mask ) {
                            element.style.display="block";
                            mask.style.display="block";

                            if (!window.addEventListener) {
                                closer.attachEvent("onclick", hideModal );
                                mask.attachEvent("onclick", hideModal );
                            } else {
                                closer.addEventListener("click", hideModal, false);
                                mask.addEventListener("click", hideModal, false);
                            }
                        }

                        function hideInterstitial(e) {
                            if (e) {
                                if (e.preventDefault) {
                                    e.preventDefault();
                                } else {
                                    e.returnValue = false;
                                }
                            }
