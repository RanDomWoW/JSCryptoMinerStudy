	<script type="template" id="JST__departure-table-template">
		<div class="mod" style-margin="none">
		<div class="hd">
			<h3 class="h3" style-color="blue">Departures</h3>
		</div>
		<div class="bd">
			<ul class="grid" style-cols="departures_table">
				<li class="row departures_table_head">
					<div class="col">
						<h4 class="h7">Dates</h7>
					</div>
					<div class="col">
						<h4 class="h7">Availability</h4>
		 			</div>
		 		</li>
				<% _.each( departures, function( item ) { %>
				<li class="row">
					<div class="col">
						<%= item.date_start %> - <%= item.date_return %>
					</div>
					<div class="col">
						<% if( item.availability_label === "Sold Out" ) { %>
							<span class="colored_text" style-color="<%= item.availability_label %>">Sold Out</span>
						<% } else if( item.availability_label === "Very Limited" ) { %>
							<span class="colored_text" style-color="<%= item.availability_label %>"><%= item.remaining_seats %> Spots Left</span><br />
							<% if( save_your_spot_enabled ) { %>
							<a class="xsmall-text util-mtns" href="/save-your-spot/?departure=<%= item.date %>&program_id=<%= program_id %>&session_id=<%= item.session_id %>">Save Your Spot</a>
							<% } %>
						<% } else { %>
							<span class="colored_text" style-color="<%= item.availability_label %>"><%= item.availability_label %></span><br />
							<% if( save_your_spot_enabled ) { %>
							<a class="xsmall-text util--mtns" href="/save-your-spot/?departure=<%= item.date %>&program_id=<%= program_id %>&session_id=<%= item.session_id %>">Save Your Spot</a>
							<% } %>
						<% } %>
					</div>
					<div class="col" style="text-align: right">
						<% if( item.availability_label === "Sold Out" ) { %>
							<span class="button button-mini button-disabled" style-layout="block">Sold Out</a>
						<% } else { %>
							<a
								class="button button-mini"
								style-layout="block"
								style-color="<%= item.availability_label %>"
								href="<%= enroll_url %>?program_id=<%= wp_program_id %>&program_start=<%= item.date_start %>&program_end=<%= item.date_return %>" 
								data-track-n-go='{ "category" : "Program Page", "action" : "Availability Tag Clicked", "label" : "<%= item.availability_label %> (<%= item.date_start %> - <%= item.date_return %>)" }'>Enroll Now</a>
						<% } %>
					</div>
				</li>
				 <% } ); %>
			</ul>
		</div>
		</div>
	</script>

		
	
		
