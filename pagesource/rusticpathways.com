	<script type="text/template" id="tooltip-table">
		<% if( obj.title ) { %>
			<h5 class="tooltip__title h5">
				<%= title %>
			</h5>
		<% } %>
		<table class="info-table info-table-micro">
		<% _.each(options, function(v, k) { %>
			<tr><td class="label"><%= k %></td><td><%= v %></td></tr>
		<% }); %>
		</table>
		<% if( obj.learn_more_url ) { %>
			<a class="tooltip__learn-more" href="<%= learn_more_url %>">
				Learn More &rarr;
			</a>
		<% } %>
	</script>

	<script type="text/template" id="dropdown-template">
		<a href="#" class="toggle">
			<%= title %>
		</a>
		<ul class="menu">
			<% if (hasEmpty) { %>
				<li>	
					<a
						href="#"
						class="default"
						data-action="clear"
						<% if(!hasActive) { %> data-active="true"<% } %>
					><% if( defaultText ) { %><%= defaultText %><% } else { %>All<% } %></a>
				</li>
			<% } %>
			<% _.each(items, function(item) { %>
				<li>
					<a
						<% if (item.href) { %> href="<%= item.href %>"<% } else { %> data-action="changeValue"<% } %>
						<% if (item.active) { %> data-active="true"<% } %>
						<% _.each(item.attrs, function(value, key) { %><%= key %>=<%= value %><% }) %>
					><%= item.label %></a>
				</li>
			<% }) %>
		</ul>
	</script>

	<script type="text/template" id="lightbox-template">
		<a rel="lightbox" data-lightbox-params='{ "action" : "close" }' class="close" href="#"></a>
		<div class="lightbox-inner"></div>
	</script>

	<script type="text/template" id="lightbox-video-template">
		<div class="lightbox-video">
			<iframe width="560" height="315" src="//www.youtube.com/embed/<%= videoID %>?autoplay=1" frameborder="0" allowfullscreen autoplay></iframe>
		</div>
	</script>

	<script type="text/template" id="lightbox-cta-template">
		<div class="lightbox-wrapper">
			<div class="lightbox-cta-block">
				<% if (obj.image) { %>
					<img src="<%= RP.utilities.imgixify(image.url) %>?w=640&h=480" class="lightbox-wrapper__image" />
				<% } %>

				<div class="lightbox-wrapper__body">
					<div class="mod">
						<% if (obj.title) { %>
							<div class="hd">
								<h2 class="h2"><%= title %></h2>
							</div>
						<% } %>

						<% if (obj.text) { %>
							<div class="bd">
								<%= text %>
							</div>
						<% } %>

						<% if (obj.link_text && obj.link_url) { %>
							<div class="ft">
								<a class="button lightbox-wrapper__cta" style-restrict-width="true" target="_blank" href="<%= link_url %>"><%= link_text %></a>
							</div>
						<% } %>

						<% if (obj.no_thanks_text) { %>
							<a class="lightbox-wrapper__no-thanks" href="#" rel="lightbox" data-lightbox-params='{ "action" : "close" }'><%= no_thanks_text %></a>
						<% } %>
					</div>
				</div>
			</div>
		</div>
	</script>

	<script type="text/template" id="lightbox-quiz-template">
		<div class="lightbox-wrapper lightbox-wrapper--full">
			<div class="lightbox-quiz">

				<div class="lightbox-wrapper__body">
					<div class="mod">
						<% if (obj.title) { %>
							<div class="hd">
								<h2 class="h2"><%= title %></h2>
							</div>
						<% } %>

						<% if (obj.quiz_id) { %>
							<div class="bd">
								<div class="quizz-container" style="height: 600px" data-width="100%" <% if (obj.title) { %>data-iframe-title="<%= obj.title %>"<% } %> data-height="auto" data-quiz="<%= obj.quiz_id %>"></div>
							</div>
						<% } %>

						<% if (obj.no_thanks_text) { %>
							<a class="lightbox-wrapper__no-thanks" href="#" rel="lightbox" data-lightbox-params='{ "action" : "close" }'><%= no_thanks_text %></a>
						<% } %>
					</div>
				</div>
			</div>
		</div>
	</script>

