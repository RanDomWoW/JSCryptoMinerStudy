<script>
    
$(function() {

 function nextNews() {
  
  if($("tr.act .btn_item.act").hasClass('last'))     
    btnnewsClick($("tr.act .btn_item:first"));  
  else
    btnnewsClick($("tr.act .btn_item.act").next());
 }
 

 var timer = setInterval(nextNews,10000);
 
 function btnnewsClick(ob) {
   $(".main-content").fadeOut(400);      
   $(".btn_item").each(function() { 
      $(this).removeClass('act');      
   });
   $(ob).addClass('act');
   
   $(".main-content."+$(".btn-news.act").attr('rel')+'.'+$(".btn_item.act").attr('rel')).fadeIn(300);
 }
 
 function mainbtnClick(ob) {
  
   $(".btn-news").each(function() {
      $(this).removeClass('act');
      $("tr."+$(this).attr('rel')).hide();
   });
       
    
   $("tr.nn").each(function() {
      $(this).removeClass('act');       
    });
    $(ob).addClass('act');
    
    $("tr."+$(ob).attr('rel')).show().addClass('act');
    
    var first = $("tr."+$(ob).attr('rel')).find(".btn_item:first").addClass('act');
    
    btnnewsClick($(first));
    
    clearInterval(timer);
    if($(ob).attr('rel')!='videos')
     timer = setInterval(nextNews,10000);
    
 }
 
 $(".btn-news").click(function() {
   
    mainbtnClick(this);    
 });
 
 $(".btn_item").click(function() {   
   clearInterval(timer);
   btnnewsClick(this);
 });
 
});
</script>
   
