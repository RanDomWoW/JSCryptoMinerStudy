<script>
    function hideads() {
        $('#Banner_balloon').hide();
    }
    function hideads_2() {
        $('#Banner_admicro').hide();
    }

    $(document).ready(function () {
		var datenow = parseInt((new Date).getTime()/1000);
		var expired_stop = 1510628392;
		var expired_start = 1510022152;
		if(expired_stop >=datenow && expired_start <=datenow){
			var rnd_number_ballon = '';
			var count_ads_ballon = 2;
			rnd_number_ballon = Math.floor(Math.random() * count_ads_ballon) + 1;
			number_current_ballon = getCookie("ads_rnd_ballon");
			  number_current_ballon = "1";
			//console.log(rnd_number_ballon);
			// console.log(number_current_ballon);
			if (number_current_ballon == null) {
				setCookie("ads_rnd_ballon", rnd_number_ballon);
				switch (rnd_number_ballon) {
					case 2:
						$('#Banner_admicro').show();
						break;
					case 1:
						$('#Banner_balloon').show();
						break;
					default:
						$('#Banner_admicro').show();
				}
			} else {
				switch (number_current_ballon) {
					case "2":
						rnd_number_ballon = count_ads_ballon-1;
						$('#Banner_balloon').show();
						break;
					case "1":
						rnd_number_ballon = count_ads_ballon;
						$('#Banner_admicro').show();
						break;
					default:
						$('#Banner_admicro').show();

				}
			//	console.log(rnd_number_ballon);
				setCookie("ads_rnd_ballon", rnd_number_ballon);
			}
		}else{
			$('#Banner_balloon').show();
		}
    });
</script>
	
