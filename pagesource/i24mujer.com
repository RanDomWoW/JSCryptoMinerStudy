           count = count + "K";
         }
       }
       $("#number_of_shares").text(count);

     });

    }

    

    $(window).scroll(function(){var a=$(window).scrollTop(),b=$("body").height(),c=$(window).height(),d=a/(b-c)*100;$(".KW_progressBar").css("width",d+"%")});

    $(".plus_content").on("click", function(){
      var id = $(this).data("menu");
      if(!$("#"+id).is(":visible")){
        $(".menu_hide").each(function(){
          if($(this).is(":visible")){
            $(this).slideToggle();
          }
        });
        $("#"+id).slideToggle();
      } else {
        $("#"+id).slideToggle();
      }
    });

    if($("#ad_fix").length && l >= 768){
      $("#ad_fix").stick_in_parent({offset_top: 75});
    }

    $(".btn_sign").on("click", function(){
      FB.login(function(response) {
        if (response.authResponse) {
          FB.api('/me?fields=name,email', function(response) {
            createCookie();
            UploadEmail(response.name, response.email);
          });
        }
      }, {scope: 'email,public_profile', return_scopes: true});
    });

    $("#createEmail").on("click", function(){
      if(validateEmail($("#email_create").val())){
        UploadEmail("usuario anonimo", $("#email_create").val());
      } else {
        new Noty({
          text: 'Debes ingresar un correo válido',
          type   : 'error',
          timeout: 5000,
        }).show();
      }
      
    });

    function validateEmail(email) {
      var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
      return re.test(email);
    }

    function UploadEmail(name, email){
      ajax.post('http://wordpress.i24mujer.com/crear.php' , {nombres: name, correo: email }, function(data) {
        var data = JSON.parse(data);
        if(data.status == "ok"){
          HideSuscribeForm();
          new Noty({
            text: 'Gracias ' + email + ' por suscribirte.',
            type   : 'alert',
            timeout: 5000,
          }).show();
        } else {
          new Noty({
            text: data.error,
            type   : 'error',
            timeout: 5000,
          }).show();
        }
        
      });
    }

    function createCookie(){
      var d = new Date();
      d.setTime(d.getTime() + (7*24*60*60*1000));
      var expires = "expires="+ d.toUTCString();
      document.cookie = "isLogued=true";
    }

    var isLogued = readCookie( "isLogued" );
    if(isLogued){
      HideSuscribeForm();
    }

    function HideSuscribeForm(){
      document.getElementById("suscribirse_form").style.display = "none";
      document.getElementById("sidebar_removed_form").style.marginTop = "50px";
    }

    function readCookie(name) {
      var nameEQ = name + "="; 
      var ca = document.cookie.split(';');
      for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) {
          return decodeURIComponent( c.substring(nameEQ.length,c.length) );
        }
      }
      return null;
    }


  });

