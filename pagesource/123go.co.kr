$(document).ready(function(){

	$(".LayerPop").each( function() {
		var brwWidth, brwHeight;
		var iLeft, iTop;

		brwWidth = document.documentElement.clientWidth;
		brwHeight = document.documentElement.clientHeight;

		if (fnCookieGet($(this).attr("id")) != 'true') {

		// 중앙 팝업 여부
		if (  $(this).attr("scroll") == "Y" ) {
			iLeft = (brwWidth - $(this).find("iframe").css("width").replace("px", "")) / 2;
			iTop = (brwHeight - $(this).find("iframe").css("height").replace("px", "")) / 2;

			if ( brwWidth <= $(this).find("iframe").css("width").replace("px", "") ) iLeft = 0;
			if ( brwHeight <= $(this).find("iframe").css("height").replace("px", "") ) iTop = 0;

			$(this).css( "left", iLeft );
			$(this).css( "top", iTop );
		}

		$(this).css( "visibility", "visible" );
		$(this).show("slow");

		}
	});

