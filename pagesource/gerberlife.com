<script type="text/template" id="quoter-table">
<div class="quoter-tr <@ if(tabs){ @> tabs <@ } @>" style="background-color: #f6f6f6">
<div class="quoter-th col-xs-2" style="background-color: #f6f6f6"></div>
<div class="quoter-th col-xs-5" style="background-color: #f6f6f6">Total <b>Coverage</b></div>
<div class="quoter-th col-xs-5" style="background-color: #f6f6f6">Monthly
<b>Cost<sup>1</sup></b></div>
</div>
<@ _.each(items, function(item) { @>
<div class="quoter-tr  <@ if(item.faceAmount == defaultFacedAmount){@> highlighted<@}@>"
data-faced-amount="<@= item.faceAmount/1e3 @>">
<div class="col-xs-2 gl-radio">
<label class="radio">
<input type="radio" name="quote" value="5" <@ if(item.faceAmount == defaultFacedAmount){@>
checked="checked"<@}@>><span class="radio-text"></span>
</label>
</div>
<div class="col-xs-5">$<@= formattedAmount(item.faceAmount) @></div>
<@ if(typeof(item.pacPremium) !== 'undefined') { @>
<div class="col-xs-5">$<@= formatPremium(item.pacPremium.monthly)@></div>
<@ } @>
<@ if(typeof(item.standardPremium) !== 'undefined' && typeof(item.pacPremium) === 'undefined') { @>
<div class="col-xs-5">$<@= formatPremium(item.standardPremium.monthly)@></div>
<@ } @>
</div>
<@ }); @>
</script>
