<script>
            $(document).ready(function () {
                var left = 0;
                var range = 195;
                var count = 16;
                var curr = 0;
                $('.gd_l').on('click', function () {
                    if (!$('.gd_imgbox ul').is(":animated")) {
                        curr++;
                        if (curr >= count - 4) {
                            curr = 0;
                        }
                        left = range * curr;

                        move();
                    }
                })
                $('.gd_r').on('click', function () {
                    if (!$('.gd_imgbox ul').is(":animated")) {
                        if (curr == 0) {
                            curr = count - 4;
                        }
                        curr--;
                        left = range * curr;

                        move();
                    }
                })

                function move() {
                    $('.gd_imgbox ul').animate({"left": "-" + left + "px"}, 700);
                }
                function automove(){
                     if (!$('.gd_imgbox ul').is(":animated")) {
                        curr++;
                        if (curr >= count - 4) {
                            curr = 0;
                        }
                        left = range * curr;

                        move();
                    }
                    setTimeout(automove,5000);
                }
                
                setTimeout(automove,1000);


            });
        </script>