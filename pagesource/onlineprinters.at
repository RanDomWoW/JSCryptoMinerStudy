	<script type="text/javascript">
		//noscript fÃ¼r Hover Ã¼ber Produkte ohne JS
		$(function(){
			$('body').removeClass('nojQuery');
		});
		//fÃ¼gt die Klasse touchHover hinzu, Ã¶ffnet SubmenÃ¼
		$(document).ready(function(event) {
			$('.stickyHeaderMainNavLi').on("touchstart", function (event) {
				'use strict';
				var link = $(this);
				if (link.hasClass('touchHover')) {
					return true;
					} 
				else {
					link.addClass('touchHover');
					$('ul > li').not(this).removeClass('touchHover');
					$(".stickyHeaderMainNavUl li ul").css("display", "none");
					$(".stickyHeaderMainNavUl li").css("background", "none");
					$(".stickyHeaderMainNavSub ul li:hover").css({ "color":"#464646","content":"none","background":"none !important"});
					$(".stickyHeaderMainNavProduct .level1 li:hover").css({ "color":"#464646","content":"none","background":"none !important"});
					event.preventDefault();
					event.stopPropagation();
					return false;
					}
			});
		});
		//schlieÃt SubmenÃ¼ wenn auÃerhalb geklickt wird
		$(document).on("touchstart", function(event){
			if ( !$(event.target).closest('.stickyHeaderMainNavLi').length ) {
				$('ul > li').not(this).removeClass('touchHover');
			}
		});
		//Ãffnet das SubmenÃ¼ nur wenn sich die Maus bewegt
		$(function() {
			var moveCount = 0;
			$("html").mousemove(function () {
			if(moveCount++ === 0) return;
			$(function(event) {
				$('.stickyHeaderMainNavLi').hover(function (){
					$(this).addClass("touchHover");
				},function(event){
					$(this).removeClass("touchHover");
				})
			});
			$(this).unbind('mousemove');
			});
		});
	</script>
	
