		<script type="text/javascript" src="//userlike-cdn-widgets.s3-eu-west-1.amazonaws.com/f34b66e46541005113bdaa2c7dd04a0e8ccf49c5a513824740fdd373f70108c9.js"></script>
		<script type="text/javascript">
			var proactiveModeProducts = [/*Plakate*/'000579','000580','000581','009396','000584','011900','000920','000582','000583','000585','002129','009397','011526','011959','000934','002501','000502','002533',/*Broschüren*/'000607','000608','009973','008352','000610','000954','000970','000971','000972',/*RollupDisplays*/'008969','008970','008971','009406'];
			var getChatMetadata = function()
			{
				var chatMetadata = {firstname: "",lastname: "",email: ""};

				if(!sessionStorage)
					return chatMetadata;
				
				if(sessionStorage.getItem('opChatMetadata') == null)
					return chatMetadata;
				
				return JSON.parse(sessionStorage.getItem('opChatMetadata'));
			}
			var getChatMode = function()
			{
				if(localStorage && localStorage.getItem('CHAT_PROACTIVE_WINDOW_CLOSED') === "true")
					return 'STANDARD';

				if(['Artikel','Kategorie'].indexOf(window.ws_page_name) != -1 && proactiveModeProducts.indexOf(ws_cat_index) != -1)
					return 'PROACTIVE';

				if(window.ws_page_name == "Bestellung")
					return 'PROACTIVE';

				return "STANDARD";
			}

			function kontaktwidgetChatOnline(widgetStatus){
				$('#chatWidgetTab.contactWidget').addClass('activ');
				$('#chatWidgetTab.contactWidget').removeClass('inactiv');
			}
			function kontaktwidgetChatOffline(){
				$('#chatWidgetTab.contactWidget').addClass('inactiv');
				$('#chatWidgetTab.contactWidget').removeClass('activ');
			}

			var userlikeCheckChatStatus = function(){
				//Wir benÃ¶tigen den Status des Chats - also verfÃ¼gbar oder nicht verfÃ¼gbar. Diesen bekommen wir Ã¼ber einen JSONP-Request
				//(oplib kommt aus der global.js)				
				oplib.jsonpRequest({
					jsonpCallback: 'chatstatus',
					url: "https://api.userlike.com/api/chat/slot/available/?customer_id=32691&chat_widget_id=47693&callback=chatstatus",
					timeout: 1000
				}, 
				function (result){
					var widgetStatus = userlike.userlikeChatState();  //MÃ¶glich: "Idle", "Chat", "Offline"
					var apistatus = result.message;
					var userlikeWindowOpen = $('#userlike-popup').length > 0;

					if(widgetStatus == "ChatMinimized" && apistatus == "NO_SLOT" ){
						kontaktwidgetChatOnline();
					}
					else if(widgetStatus == "Offline" && apistatus == "GROUP_ONLINE" && userlikeWindowOpen ){
						kontaktwidgetChatOnline();
					}
					else if(widgetStatus == "Offline" && apistatus == "GROUP_ONLINE" ){
						kontaktwidgetChatOffline();
					}
					else if(widgetStatus == "Chat" || apistatus == "GROUP_ONLINE")
					{
						kontaktwidgetChatOnline(widgetStatus);
					}
					else if(widgetStatus == "Idle" && (apistatus == "GROUP_OFFLINE" || apistatus == "NO_SLOT") ){
						kontaktwidgetChatOffline();
					}	
					else if(widgetStatus == "Idle" ){
						kontaktwidgetChatOnline();
					}	
					else{
						kontaktwidgetChatOffline()
					}
				});
			}

			var randomName = Math.random().toString(36).substring(2,7);
			var chatMetadata = getChatMetadata();
			var loginUsername						= " ";
			var loginUsermail						= "";
			var userIndex 							= "0" == "0" ? "" : "0";
			var wcUsername							= "".trim();
			var firstname							= "".trim() || chatMetadata.firstname;
			var lastname							= "".trim() || chatMetadata.lastname || randomName;
			var wcUsermail							= "".trim() || chatMetadata.email;
			var username = loginUsername.trim() || wcUsername.trim() || (firstname + " " + lastname).trim();
			var email = loginUsermail.trim() || wcUsermail.trim() || "";
			
			function userlikeCallbackMessageReceived()
			{
				if(userlike.userlikeChatState() == "ChatMinimized")
					userlike.userlikeStartChat();
			}
			
			userlikeRemoteReady = function () {
				var chatMode = getChatMode();
			
				if(chatMode == "PROACTIVE")
					userlike.userlikeRemoteApiModeProactivePassive(10);
				else
					userlike.userlikeRemoteApiModeNormal();
			};  

			userlikeReady = function(){
				var chatData = {
					user: { 
						name: username,
						email: email,
						additional02: opWsVars.isPremiumKunde ? "Ja" : "Nein"
					},
					custom: "  \nVorname%$" + firstname + "  \nNachname%$" + lastname + "  \nEmail%$" + email + "  \nSubShopID%$" + window.opWsVars.subShopId + "  \nUserIndex%$" + userIndex + "  \nchat%$",
				}

				if(window.opWsVars.subShopId == "staging-desktop")
				chatData.custom = {
						"  \nVorname%$": firstname,
						"  \nNachname%$": lastname,
						"  \nEmail%$": email,
						"  \nSubShopId%$": window.opWsVars.subShopId,
						"  \nUserIndex%$": userIndex,
						"  \nchat": "chat%$"
						};
				/*
					Ãbertrage die Metadaten an Userlike und speichere diese lokal, damit der User zukÃ¼nftig identifiziert werden kann.
				*/
				userlike.setData(chatData);
				sessionStorage.setItem('opChatMetadata',JSON.stringify({firstname: firstname, lastname: lastname, email: email}));


				userlikeCheckChatStatus();
				/*
					Merke das SchlieÃen des Chatfensters, damit das Fenster bei Vielnutzer nicht dauernd geÃ¶ffnet wird.
				*/
				$('#userlike-close').on('click',function(){
					if(getChatMode() == 'PROACTIVE' && localStorage)
					{
						localStorage.setItem('CHAT_PROACTIVE_WINDOW_CLOSED',true);
					}
				});

				/*
					Aktualisiere den Status des Chatpanels, wenn darÃ¼ber gehovert wird
				*/
				$('.contactWidgetWrapper').on('hover', _.debounce(userlikeCheckChatStatus,500));

				/*
					Starte den Chat, wenn auf den Chatreiter im Kontaktwidget geklickt wurde und tracke dies
				*/
				$('#chatWidgetTab').on('click', function(){
					if(userlike.userlikeChatState() == "Chat")
						return;

					var chatstatusActive = $('#chatWidgetTab.contactWidget.activ').length > 0;

					if(chatstatusActive)
					{
						userlike.userlikeStartChat();
						//Tracke den Klick, wenn Chat verfÃ¼gbar
						dataLayer.push({ event:'click', eventAction: 'KontaktWidget Chatstart' });
						emosFreeEvent('Target', [['KontaktWidget','Chat geoeffnet',1,'a']]); var emospro={};emospro.marker='KontaktWidget/Chat geoeffnet';window.emosPropertiesEvent(emospro);

						return;
					}	
					
					$('#userlike-close').click();
				});

				/*
					FÃ¼ge das Klickevent zum Abschicken der Nachrichten auf den Bereich des Absendenbuttons
				*/
				$('#userlike-chat-scroll-textarea').on('click', function(e){
					var klicked = false;

					if(e.offsetX < $(this).width() - 40 || e.offsetX > $(this).width() - 10)
						klicked = false;
					else if(e.offsetY < $(this).height() - 35 || e.offsetY > $(this).height() - 2)
						klicked = false;
					else
						klicked = true;

					if(klicked){
						userlike.userlikeInputEnter();
					}
				})

				/*
					Highlight den Chatbutton, wenn Text eingegeben wurde
				*/
				$('#userlike-chat-textarea').on('keyup', function(e){
					var defaultMessage = $('#userlike-chat-textarea').data('default');
					var textareaContent = $('#userlike-chat-textarea').val();
					var highlightSendButton = textareaContent != defaultMessage && textareaContent.length > 0;

					if(highlightSendButton)
						$('#userlike-chat-scroll-textarea').addClass('highlight');
					else
						$('#userlike-chat-scroll-textarea').removeClass('highlight');
				})
			}
		</script>
