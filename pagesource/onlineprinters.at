    <script>
        (function() {
            $(window).load(function() {
                var SLIDE_ANIMATION_DURATION = 600;
                var SLIDE_CHANGE             = 5000;

                var slider;
                var flexSlider = $('.opidMarketingTeaserSlider  .opidMarketingTeaserWrapper').flexslider({
                    namespace:		'marketingTeaserWrapper-',
                    selector:		'.marketingTeaserWrapperSlides > li',
                    manualControls:	'.opidMarketingTeaserSlider .opidMarketingTeaserWrapperControls > li',
                    animation:		'slide',
                    directionNav:	false,
                    pauseOnHover:	false,
                    animationSpeed:	SLIDE_ANIMATION_DURATION,
                    slideshowSpeed:	SLIDE_CHANGE,
                    animationLoop:	true,
                    start: function(e) {
                        slider = e;
                    }
                });

                $('.opidMarketingTeaserSlider').hover(function() {
                    $(this).addClass('s-hover');
                    if (slider) slider.pause();
                }, function() {
                    $(this).removeClass('s-hover');
                    if (slider) slider.play();
                });

                var shineAni = false;

                $('.opidMarketingTeaserSlider button').hover(function() {
                    if (shineAni) return;
                    var shine = $(this).find('.shine');
                    var width = $(this).width() + shine.width();
                    shineAni = true;

                    shine.css('left', '-' + shine.width() + 'px');
                    shine.stop().animate({
                        left: "+="+ width,
                    }, 1000, function()
                    {
                        shineAni = false;
                    });
                });
            });
        })();
    </script>

