function delPhoto(name, id){

	UIkit.modal.confirm("Подтвердите удаление", function(){
		article = $('[name="article"]').val();
		console.log(article)
		$.ajax({
			url: 'http://www.tarixo.com/blocks/deletePhoto.php?name2='+name+'&article='+article,
			success: function(data){
				//console.log($(this))
				if(data==1){
					$('#img'+id).remove();

						i=0;
		      			$('.user-images li').each(function(){

		      				$(this).attr('rel', i);
		      				$(this).attr('id', 'img'+i);
		      				$(this).attr('style', 'background-image: url(http://www.tarixo.com/photos/'+article+'_'+i+'.jpg?update='+Date.now()+')');

		      				$(this).find('a').attr('onclick', "delPhoto('photos/"+article+"_"+i+".jpg', '"+i+"')");
		      				$(this).find('.rt-right').attr('onclick', "rotatePhotoRight('photos/"+article+"_"+i+".jpg', '"+i+"')");
		      				i++;
		      			})


				}
				if(data==2){
					UIkit.modal.alert("Ошибка")
				}
				
			}
		})
	});
}





function rotatePhotoRight(name, id){

	//console.log(name);

	$.ajax({
		url: 'http://www.tarixo.com/blocks/rotatePhoto.php?name2='+name,
		cache: false,
		success: function(data){
			
			if(data==1){
				//console.log(data);
				$('#img'+id).attr('style', 'background-image: url(http://www.tarixo.com/'+name+'?update='+Date.now()+')');

				$('.photostatus').stop().fadeIn(300);
		      	setTimeout(function(){
		      		$('.photostatus').fadeOut(300);
		      	}, 2000)
			}
			if(data==2){
				UIkit.modal.alert("Ошибка")
			}
		}
	})
}







$( ".user-images" ).sortable({

      placeholder: "ui-state-highlight",
