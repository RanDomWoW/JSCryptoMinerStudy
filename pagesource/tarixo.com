		url: 'http://www.tarixo.com/blocks/saveAd.php',
		data: $('.form-data').serialize(),
		method: 'POST',
		beforeSend: function(){
			$('button.saveAd').prop('disabled', true);
		},
		success: function(data){

			function IsJsonString(str) {
			    try {
			        JSON.parse(str);
			    } catch (e) {
			        return false;
			    }
			    return true;
			}
	

			if(data=='caption'){
				UIkit.modal.alert("Введите заголовок объявления")
			}
			else if(data=='emailexist'){
				UIkit.modal.alert("Такой e-mail уже есть в базе. Если он принадлежит Вам, войдите в свою учетную запись и разместите объявление.")
			}
			else if(data=='loginexist'){
				UIkit.modal.alert("Пользователь с таким телефоном уже зарегистрирован. Если он принадлежит Вам, войдите в свою учетную запись и разместите объявление.")
			}
			else if(data=='accesdenied'){
				UIkit.modal.alert("У Вас нет прав на редактирование данного объявления!")
			}
			else if(data=='adnotfound'){
				UIkit.modal.alert("Объявление не найдено")
			}
			else if(data=='emptyarticle'){
				UIkit.modal.alert("Не указан артикул!")
			}
			else if(data=='update'){

				if($('[name="role"]').length==0){
					modal = UIkit.modal.blockUI('Изменения сохранены.<br>Сейчас вы будете перенаправлены в личный кабинет...');
					setTimeout(function(){ 
						modal.hide()
						window.location = "enter.html";
					}, 4000)
				}else{
					UIkit.modal.alert("Изменения сохранены")
				}

			}
			else if(data=='registerInsert'){
				modal = UIkit.modal.blockUI('Ваше объявление опубликовано. На  Ваш email отправлено письмо с данными для входа в Личный кабинет. <br>Сейчас вы будете перенаправлены на главную страницу сайта...');
				setTimeout(function(){ 
					modal.hide()
					window.location = "index.html";
				}, 7000)

			}
			else if( IsJsonString(data) ){

				dataObj = JSON.parse(data);

				if(dataObj.status=='insert'){

					modal = UIkit.modal.blockUI('<h3>Ваше объявление принято <i style="color:#5dca9a" class="uk-icon-check"></i></h3><br>Сейчас вы будете перенаправлены на страницу объявления.');
					setTimeout(function(){ 
						modal.hide()
						window.location = dataObj.link;
					}, 5000)

		
				}else if(dataObj.status=='registerinsert'){

					passraw = dataObj.passraw;
					loginraw = dataObj.loginraw;

					UIkit.modal.confirm('<h3>Ваше объявление принято <i style="color:#5dca9a" class="uk-icon-check"></i></h3>Ваши данные для входа в свой личный кабинет, прошу сохранить их.<p>' +
						"<table><tr><td>Логин:</td><td><b>"+loginraw+"</b></td></tr><tr><td>Пароль:</td><td><b>"+passraw+"</b></td></tr></table></p><p>В личном кабинете вы сможете редактировать, удалять или обновлять свои объявления.</p>", function(){
					    window.location = dataObj.link;
					},function(){
						window.location = dataObj.link;
					});

				}else{
					UIkit.modal.alert("Внутренняя ошибка. Попробуйте позже.")
				}


				
			}
			else{
				UIkit.modal.alert("Внутренняя ошибка. Попробуйте позже.")
				$('button.saveAd').prop('disabled', false);
			}
			//UIkit.modal.alert("Изменения сохранены")
			//console.log(data)
		},
		error: function(){
			UIkit.modal.alert("Внутренняя ошибка. Попробуйте позже.")
			$('button.saveAd').prop('disabled', false);
		},
		complete: function(){
			$('button.saveAd').prop('disabled', false);
		}

	})

})

