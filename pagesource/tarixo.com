      },
      stop: function(event, ui) {
      	newSort = [];
      	$('.user-images li').each(function(){
      		newSort.push($(this).attr('rel'));

      		console.log($(this).attr('rel'));
      	})

      	newSortJSON = JSON.stringify(newSort);
      	article = $('[name="article"]').val()

      	$.ajax({
      		url: 'http://www.tarixo.com/blocks/sortPhoto.php',
      		data: {sortlist: newSortJSON, article: article},
      		method: 'POST',
      		success: function(data){
