<script>
	$(function(){

		$('.comparison2').on('click', function(event){
			divCompareGoods = $(this);
			id = $(this).attr('rel');
			if( $(divCompareGoods).hasClass('active') ){
				$(this).text('Добавить в избранное').attr('title', 'Добавить в избранное')
				removeCompare(id)
			}else{
				$(this).text('В избранном').attr('title', 'В избранном')
				addCompare(id)
			}
			$(this).toggleClass('active');
		})

		$('.comparison3').on('click', function(event){
			divCompareGoods = $(this);
			id = $(this).attr('rel');
			if( $(divCompareGoods).hasClass('active') ){
				$(this).find('span').text('Добавить в избранное').attr('title', 'Добавить в избранное')
				removeCompare(id)
			}else{
				$(this).find('span').text('В избранном').attr('title', 'В избранном')
				addCompare(id)
			}
			$(this).toggleClass('active');
		})

		function addCompare(id){
			$.ajax({
				url: 'http://www.tarixo.com/blocks/compare/compare.php?act=add&id='+id,
				success: function(data){
					if(data > 0){
						$('.b-header__number-of span').html(data)
					}else{
						$('.b-header__number-of span').html("")
					}
				}
			})
		}

		function removeCompare(id){
			$.ajax({
				url: 'http://www.tarixo.com/blocks/compare/compare.php?act=remove&id='+id,
				success: function(data){
					if(data > 0){
						$('.b-header__number-of span').html(data)
					}else{
						$('.b-header__number-of span').html("")
					}
				}
			})
		}

		$.ajax({
			url: 'http://www.tarixo.com/blocks/compare/compare.php?act=check',
			success: function(data){
				if(data > 0){
					$('.b-header__number-of span').html(data)
				}else{
					$('.b-header__number-of span').html("")
				}
			}
		})

	})
</script>
