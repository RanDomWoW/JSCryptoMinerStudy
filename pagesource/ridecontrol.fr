<script type="text/javascript">
var aover = false;
var divover = false;

var aover2 = false;
var divover2 = false;

var aover3 = false;
var divover3 = false;

function affichage(){
	if(aover||divover){
		document.getElementById("deroul").style.display="block";
	}
	if(aover==false && divover==false){
		setTimeout(function(){cachage()},50);
	}
}
function affichage2(){
	if(aover2||divover2){
		document.getElementById("deroul2").style.display="block";
	}
	if(aover2==false && divover2==false){
		setTimeout(function(){cachage2()},50);
	}
}
function affichage3(){
	if(aover3||divover3){
		document.getElementById("deroul3").style.display="block";
	}
	if(aover3==false && divover3==false){
		setTimeout(function(){cachage3()},50);
	}
}

function cachage(){
	if(aover==false && divover==false){
		document.getElementById("deroul").style.display="none";
	}
}
function cachage2(){
	if(aover2==false && divover2==false){
		document.getElementById("deroul2").style.display="none";
	}
}
function cachage3(){
	if(aover3==false && divover3==false){
		document.getElementById("deroul3").style.display="none";
	}
}

function dcacher(){
	divover = false;
	affichage();
}
function dcacher2(){
	divover2 = false;
	affichage2();
}
function dcacher3(){
	divover3 = false;
	affichage3();
}

function dafficher(){
	divover = true;
	affichage();
}
function dafficher2(){
	divover2 = true;
	affichage2();
}
function dafficher3(){
	divover3 = true;
	affichage3();
}

function acacher(){
	aover = false;
	affichage();
}
function acacher2(){
	aover2 = false;
	affichage2();
}
function acacher3(){
	aover3 = false;
	affichage3();
}

function aafficher(){
	aover = true;
	affichage();
}
function aafficher2(){
	aover2 = true;
	affichage2();
}
function aafficher3(){
	aover3 = true;
	affichage3();
}

function toggle_div_open() { 
document.getElementById("menu_mob").style.left= "-3px"; 
document.getElementById("boutonO").style.display= "none"; 
document.getElementById("boutonC").style.display= "inline"; 
}

function toggle_div_close() { 
document.getElementById("menu_mob").style.left= "-230px"; 
document.getElementById("boutonO").style.display= "inline"; 
document.getElementById("boutonC").style.display= "none"; 
}
</script>


