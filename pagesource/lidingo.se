<div class="sv-vertical sv-layout sol-gray-background" id="svid10_6b83838014c0ff33b5711e58"><div class="sv-fluid-grid sv-grid-grid sv-layout sv-skip-spacer" id="svid10_6b83838014c0ff33b5711efe"><div class="sv-layout sv-skip-spacer" id="svid30_2f111fb614cc4016c11ae9b"><div id="DynamiskaNyheterStartsida"><!-- Dynamiska Nyheter - Startsida --></div><div class="sv-script-portlet sv-portlet sv-skip-spacer" id="svid12_2f111fb614cc4016c11ae9a"><div id="DynamiskaNyheterSkript"><!-- Dynamiska Nyheter - Skript --></div><div class="sol-startpage-news-container sol-css-text">
   <div class="sol-news-listing"></div>
   <div class="sv-row">
      <div class="sv-column-6 sv-push-3 sol-button">
         <a href="#" class="sol-js-fetch sol-fetch-more">Visa fler nyheter</a>
      </div>
   </div>
   
</div>


<script type="text/javascript">
(function(){
   var jq = jQuery; 

   var config = {
      num: 4,
      pathIds: ['1'].join(';'),
      imgMaxWidth: 700,
      descLength: 75,
      colSize: 3,
      startRows: 2,
      maxRows: 10
   }

   var currNum = 0;
   var fetching = false;

   var newsContainer = jq('.sol-startpage-news-container > .sol-news-listing')

   String.prototype.shortenString = function(length){
      if(this.length > length) {
         return this.substring(0, length).concat('...');
      }else{
         return this;
      }
   }
   Array.prototype.chunk = function(chunkSize) {
       var R = [];
       for (var i=0; i<this.length; i+=chunkSize)
           R.push(this.slice(i,i+chunkSize));
       return R;
   }
     
   var fetchNews = function(start, num) {
      fetching = true;
      jq.get("/91.2e2f4c114d5435c2fd7f01/12.2e2f4c114d5435c2fd7f0b.json", {start: start, num: num, pathIds: config.pathIds, imgMaxWidth: config.imgMaxWidth}, "json")
         .done(function(data) {
            if(data.hits.length > 0){
               if(currNum >= (4*2)){
                  jq('.sol-startpage-news-row').fadeIn();   
               }  
               handleNews(data.hits);                            
            }else{
               disableButton();
            }
         })
         .fail(function(data) {
            fetching = false;
            console.log(data);
         });
      if((currNum/config.num) + 1 >= config.maxRows){
         disableButton();
      }
   };
   var handleNews = function(hits){
      if(hits.length > config.num){
         var rows = hits.chunk(config.num);
         for(var i = 0; i < rows.length; i++){
            addNews(rows[i]);
         }
      }else{
         addNews(hits);
      }
   }
   var getMissingCols = function(length){
      var html = '';
      var numLeft = config.num - length;      
      if(numLeft > 0){
         for(var i = 0; i < numLeft; i++){
            html += '<div class="sv-column-' + config.colSize + '"></div>';
         }
         disableButton();
      }
      return html;
   }
   var addNews = function(hits){      
      var html = '';
      html += '<div class="sv-row c86" >';
      jq.each(hits, function(i, hit) { 
         html += '<div class="sv-column-' + config.colSize + '">';
        
         html += '   <a href="' + hit.uri + '">'
         if(hit.img){
            html += '   <div class="sol-startpage-news-img">';         
            html += '      <img src="' + hit.img +'">';
            html += '   </div>'
            html += '   <div class="sol-startpage-news-text normal">';   
         }else{
            html += '   <div class="sol-startpage-news-text normal sol-no-img">';   
         }  
        
         html += '         <span class="sol-news-title">' + hit.title + '</span>';
         html += '         <span class="sol-news-date-desc">' + hit.date + ' | ' + hit.desc.shortenString(config.descLength) + '</span>';
         html += '      </div>'
         html += '   </a>'
         html += '</div>';
      });
      
      html += getMissingCols(hits.length);

      html += '</div>'

      jq(newsContainer).append(html);

      if(currNum === 0) {
         jq(newsContainer).children('.sv-row').show();
      } else{
         jq(newsContainer).children('.sv-row').fadeIn();
      } 
      fetching = false;       
   }

   var disableButton = function(){
      jq('.sol-js-fetch').addClass('sol-disabled').text('Det finns inga fler nyheter att visa');
      
   }

   jq('.sol-js-fetch').click(function(e){
      e.preventDefault();      
      if(!fetching){ 
         currNum = currNum + config.num;
         fetchNews(currNum, config.num);
      }
   });   

   /*for(var i = 0; i < config.startRows; i++){
      if(i > 0) currNum = currNum + config.num;
      fetchNews(currNum, config.num);
   }*/   
   fetchNews(currNum, (config.startRows * config.num));
   currNum = (config.startRows * config.num) - config.num;

   jq(document).ready(function(){
      jq('.sol-js-hide-news').click(function(e){
         e.preventDefault();      
         var rows = jq('.sol-news-listing .sv-row').splice(config.startRows);
         jq(rows).fadeOut(function(){
            jq(rows).remove();
         });
         jq('.sol-startpage-news-row').fadeOut();
         jq('.sol-js-fetch').removeClass('sol-disabled').text('Visa fler nyheter');
         currNum = (config.num * config.startRows) - config.num;
      });
   });
   
   
})();
   
</script></div>
</div>
