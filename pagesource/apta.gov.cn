    <script src="/Scripts/CJL.0.1.min.js"></script>

    <script src="/Scripts/AlertBox.js"></script>

    <script type="text/javascript">
        (function () {
            //右下角消息框
            var timer, target, current,
	ab = new AlertBox("idBox2", {
	    fixed: true,
	    onShow: function () {
	        clearTimeout(timer); this.box.style.bottom = -this.box.offsetHeight; this.box.style.right = 0;
	    },
	    onClose: function () { clearTimeout(timer); }
	});

            function hide() {
                ab.box.style.bottom = --current + "px";
                if (current <= target) {
                    ab.close();
                } else {
                    timer = setTimeout(hide, 10);
                }
            }

            function show() {
                ab.box.style.bottom = ++current + "px";
                if (current < target) {
                    timer = setTimeout(show, 10);
                }
            }

            $$("idBoxClose2").onclick = function () {
                target = -ab.box.offsetHeight; current = 0; hide();
            }

            ab.show();
            target = 0; current = -ab.box.offsetHeight;
            show();

        })()
    </script>

