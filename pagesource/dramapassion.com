    $(document).ready(function() {
        var open_login_modal = "";
                if ($('#myLoginModal').find('.incorrect').val() || open_login_modal == 1) {
            $('#myLoginModal').addClass('show');
            setTimeout(function () {
                $('#myLoginModal').addClass('in loginbox');
                $('#myLoginModal').on('click','.close,.btn-default',function () {
                    $(this).parents('#myLoginModal').removeClass('in show loginbox');
                    $(this).parents('#myLoginModal').find('.rouge').empty();
                });
            },1000);
        }

        var open_reg_modal = "";
                if (open_reg_modal != "") {
            $('#myRegisterSuccessModal').addClass('show');
            setTimeout(function () {
                $('#myRegisterSuccessModal').addClass('in loginbox');
                $('#myRegisterSuccessModal').on('click','.close,.btn-default',function () {
                    $(this).parents('#myRegisterSuccessModal').removeClass('in show loginbox');
                    $(this).parents('#myRegisterSuccessModal').find('.rouge').empty();
                });
            },1000);
        }

        if ($('#myRegisterModal').find('.incorrect').val()) {
            $('#myRegisterModal').addClass('show');
            setTimeout(function () {
                $('#myRegisterModal').addClass('in loginbox');
                $('#myRegisterModal').on('click','.close,.btn-default',function () {
                    $(this).parents('#myRegisterModal').removeClass('in show loginbox');
                    $(this).parents('#myRegisterModal').find('.rouge').empty();
                });
            },1000);
        }


        if ($('#myPasswordModal').find('.incorrect').val()) {
            $('#myPasswordModal').addClass('show');
            setTimeout(function () {
                $('#myPasswordModal').addClass('in loginbox');
                $('#myPasswordModal').on('click','.close,.btn-default',function () {
                    $(this).parents('#myPasswordModal').removeClass('in show loginbox');
                    $(this).parents('#myPasswordModal').find('.rouge').empty();
                });
            },1000);
        }


        $('.mypass-word').click(function () {
            if($('#myLoginModal').hasClass('loginbox')){
                $('#myLoginModal').removeClass('in show loginbox');
                $('#myLoginModal').find('.rouge').empty();
            }

        });


        //User info modifier modals

        $('body').on('click', '.submit_button',function(e){
            e.preventDefault();
            //alert($(this).parents('.modal.fade').attr('id'));
            var modal_id = $(this).parents('.modal.fade').attr('id');
            var mod = modal_id.replace('ModifierModal',"");

            if(mod=="newsInsc"){
                var modif_news_val = $('#'+modal_id).find('input[name="modif_news"]').val();

                var data_arr = {modif_type: "newsletter", modif_news: modif_news_val};
            } else if(mod=="news"){
                var modif_news_val = $('#'+modal_id).find('input[name="modif_news"]').val();

                var data_arr = {modif_type: "newsletter", modif_news: modif_news_val};
            } else if(mod=="annif"){
                var annif_val = $('#'+modal_id).find('input[name="annif"]').val();

                var data_arr = {modif_type: "annif", annif: annif_val};
            } else if(mod=="pass"){
                var anc_pass_val = $('#'+modal_id).find('input[name="anc_pass"]').val();
                var new_pass_val = $('#'+modal_id).find('input[name="new_pass"]').val();
                var new_pass_conf_val = $('#'+modal_id).find('input[name="new_pass_conf"]').val();

                var data_arr = {modif_type: "pass", anc_pass: anc_pass_val, new_pass: new_pass_val,new_pass_conf:new_pass_conf_val};
            } else if(mod=="mail"){
                var new_email_val = $('#'+modal_id).find('input[name="new_email"]').val();
                var new_email_conf_val = $('#'+modal_id).find('input[name="new_email_conf"]').val();

                var data_arr = {modif_type: "mail", new_email: new_email_val, new_email_conf: new_email_conf_val};
            } else if(mod=="sexe"){
                var new_sexe_val = $('#'+modal_id).find('select[name="new_sexe"]').val();

                var data_arr = {modif_type: "sexe", new_sexe: new_sexe_val};
            } else if(mod=="addr"){
                var addr1_val = $('#'+modal_id).find('input[name="addr1"]').val();
                var addr2_val = $('#'+modal_id).find('input[name="addr2"]').val();
                var zip_val = $('#'+modal_id).find('input[name="zip"]').val();
                var city_val = $('#'+modal_id).find('input[name="city"]').val();
                var province_val = $('#'+modal_id).find('input[name="province"]').val();
                var new_pays_val = $('#'+modal_id).find('select[name="new_pays"]').val();

                var data_arr = {modif_type: "addr", addr1: addr1_val, addr2: addr2_val, zip: zip_val, city:city_val, province: province_val, new_pays:new_pays_val};
            } else if(mod=="pays"){
                var pays_new_val = $('#'+modal_id).find('select[name="pays_new"]').val();

                var data_arr = {modif_type: "pays", pays_new: pays_new_val};
            }

            $.ajax({
                type:'POST',
                url:'http://www.dramapassion.com/modif_compte_action.php',
                data: data_arr,

                success:function(msg){

                    $('#'+modal_id).find('.modal-body').html(msg);
                    $('#'+modal_id).addClass('show');
                }
            });
        });


        $('body').on('click', '.submit_button_action',function(e){
            e.preventDefault();
            console.log('action dp');

            var modal_id = $(this).parents('.modal.fade').attr('id');
            var mod = modal_id.replace('ModifierModal',"");

            if(mod=="newsInsc"){
                var data_arr = {mod: "newsletter", news_type: 1};
            } else if(mod=="news"){
                var data_arr = {mod: "newsletter", news_type: 0};
            } else if(mod=="annif"){
                var data_arr = {mod: "annif"};
            } else if(mod=="pass"){
                var data_arr = {mod: "pass"};
            } else if(mod=="mail"){
                var data_arr = {mod: "mail"};
            } else if(mod=="sexe"){
                var data_arr = {mod: "sexe"};
            } else if(mod=="addr"){
                var data_arr = {mod: "addr"};
            } else if(mod=="pays"){
                var data_arr = {mod: "pays"};
            }

            $.ajax({
                type:'GET',
                url:'http://www.dramapassion.com/modif_compte.php',
                data: data_arr,

                success:function(msg){
                    console.log(msg);
                    $('#'+modal_id).find('.modal-body').html(msg);
                    $('#'+modal_id).addClass('show');
                }
            });
        });


        $('.close.modify').on('click',function(){
            location.reload();
        });

        //User info modifier modals

        $('body').on('click', '.playlist_supprimer',function(){

            var drama_id = $(this).attr('data-drama');

            $.ajax({
                type:'GET',
                url:'http://www.dramapassion.com/supprimer_playlist.php',
                data: {dramaID: drama_id},

                success:function(msg){
                    console.log(msg);
                    $('#deletePlaylist').find('.modal-body').html(msg);
                    $('#deletePlaylist').addClass('show');
                    setTimeout(function () {
                        $('#deletePlaylist').addClass('in loginbox');
                        $('#deletePlaylist').on('click','.close,.btn-default',function () {
                            $(this).parents('#deletePlaylist').removeClass('in show loginbox');
                        });
                    },1000);
                }
            });
        });

    });

</script>
