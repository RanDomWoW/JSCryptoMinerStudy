            if ($('#vido_id').offset().top >= $(window).scrollTop() && $('#vido_id').offset().top <= $(window).scrollTop() + ($(window).height() / 2)) {
                show = true;
            }
            if ($('#vido_id').offset().top + $('#vido_id').height() <= $(window).scrollTop()) {
                show = false;
            }
            if (show) {
                $('#vido_id').get(0).play();
            } else {
                $('#vido_id').get(0).pause();
            }
