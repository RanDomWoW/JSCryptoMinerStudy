</div>

<script type="text/javascript">
var timer;
var timer2;
function overfunc(pageid){
	if (pageid != ''){
	var data = 'pageid=' + pageid;
	$('map area').hover(
	function(event) {
		if(timer) {
			clearTimeout(timer);
			timer = null
		}
		var left = event.pageX -550 ;
		var top = event.pageY-650 ;
		timer = setTimeout(function() 
		{
			$('#mappopup').innerHTML = "";
			$('#mappopup').hide();
			$.ajax({
				url: "/?option=loader",	
				type: "GET",		
				data: data,		
				cache: true,
				success: function (Response) {	
					document.getElementById("mappopup").style.left=(left)+"px";
					document.getElementById("mappopup").style.top=(top)+"px";
					$('#mappopup').html(Response);
					$('#mappopup').show();
				}, dataType: 'html'		
			});	
		}, 1000);
	}, function() {
			if(timer2) {
				clearTimeout(timer2);
				timer2 = null
			}
			timer2 = setTimeout(function() 
			{
				$('#mappopup').innerHTML = "";
				$('#mappopup').hide();
			}, 10000);
		}
	);
	

	$('#map').mouseLeave(
	function(event) {
		$('#mappopup').innerHTML = "";
		$('#mappopup').hide();
	}, function() {
		}
	);
}
}

</script>		
