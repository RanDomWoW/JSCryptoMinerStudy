        <script>
          (function(){
            var w = window,
              d = document,
              e = d.documentElement,
              g = d.getElementsByTagName('body')[0];

            var windowsWidth = function() {
              return w.innerWidth || e.clientWidth || g.clientWidth;
            };

            var self = document.getElementById('hero-video');
            var playingTriggered = false;

            var posterURLMobile = self.getAttribute('data-poster-url-mobile');
            var posterURLTablet = self.getAttribute('data-poster-url-tablet');
            var posterURLDesktop = self.getAttribute('data-poster-url-desktop');
            var firstFrameURL = self.getAttribute('data-first-frame');
            var videoMp4URL = self.getAttribute('data-video-url-mp4');
            var videoWebmURL = self.getAttribute('data-video-url-webm');

            var changePosterImage = function () {
              if (windowsWidth() < 768) {
                self.style.backgroundImage = 'url(' + posterURLMobile + ')';
              } else if (windowsWidth() < 992) {
                self.style.backgroundImage = 'url(' + posterURLTablet + ')';
              } else {
                self.style.backgroundImage = 'url(' + posterURLDesktop + ')';
              }
            };

            window.changePosterImage = changePosterImage;

            if (windowsWidth() >= 992) {
              self.style.backgroundImage = 'url(' + firstFrameURL + ')';
              self.innerHTML = '<video class="hero__video" preload="auto" id="heroVideo"><source src="' + videoWebmURL + '" type="video/webm; codecs=vp9,vorbis"><source src="' + videoMp4URL + '" type="video/mp4" /></video>';

              objectFitVideos();
              var video = document.getElementById('heroVideo');
              video.load();
              video.addEventListener('canplaythrough', function () {
                video.play();
                document.getElementsByClassName('hero')[0].classList.add('hero_playing');
              }, false);
              video.addEventListener('playing', function () {
                if (!playingTriggered) {
                  playingTriggered = true;
                  document.getElementsByClassName('hero__loading')[0].classList.add('hero__loading_hide');
                }
              }, false);
              video.addEventListener('ended', function () {
                document.getElementsByClassName('hero')[0].classList.add('hero_played');
              }, false);
            } else {
              changePosterImage();
              document.getElementsByClassName('hero__loading')[0].classList.add('hero__loading_hide');
              document.getElementsByClassName('hero')[0].classList.add('hero_played');
            }
          }());
        </script>
    
