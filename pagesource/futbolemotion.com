    var _timeoutBusquedaID;

    $(function() {
        $('.caja_buscador').keyup(function(e) {
        	if ($.inArray(e.which, [13,37,38,39,40]) == -1) { // No realizamos la búsqueda si la tecla pulsada es "enter" o una flecha
                window.clearTimeout(_timeoutBusquedaID);
                _timeoutBusquedaID = window.setTimeout(function(){
                	buscarProductos(1, '', null, null, true);
                }, 300);
        	}
        	else {
        		if ($('#caja_buscador_top_trends').is(':visible')) {
            		// Seleccionar top trend anterior o posterior dependiendo de la flecha pulsada o realizar la búsqueda si
            		// se ha pulsado "enter" sobre un top trend.
                    var current = $('#caja_buscador_top_trends ul li.buscador_top_trend_activo');

                    if (e.keyCode == 38) {
                        previo = current.prev();

                        if (previo.length) {
                        	$('#caja_buscador_top_trends ul li').removeClass('buscador_top_trend_activo');
                        	previo.addClass('buscador_top_trend_activo');
                        }
                    }
                    else if (e.keyCode == 40) {
                        siguiente = current.next();

                        if (siguiente.length) {
                            $('#caja_buscador_top_trends ul li').removeClass('buscador_top_trend_activo');
                            siguiente.addClass('buscador_top_trend_activo');
                        }
                    }
                    else if (e.keyCode == 13) {
                    	$('#caja_buscador').val(current.data('texto'));
                    	buscarProductos(1, '', null, null);
                    }
            	}
        	}
        });

        $(document).click(function(e) {
            if ((e.target.id != 'caja_buscador') && (e.target.id != 'caja_buscador_movil') && !$(e.target).hasClass('buscador_top_trend')) {
            	$('#caja_buscador_top_trends').hide();
        		$('#caja_buscador_top_trends').empty();
        		$('#caja_buscador_movil_top_trends').hide();
        		$('#caja_buscador_movil_top_trends').empty();
            }
            else if($(e.target).hasClass('buscador_top_trend')) {
            	if ($('#caja_buscador').is(':visible')) {
            		$('#caja_buscador').val($(e.target).data('texto'));
            	}
            	else {
            		$('#caja_buscador_movil').val($(e.target).data('texto'));
            	}

            	buscarProductos(1, '', null, null);
            }
        });
    });
</script>

