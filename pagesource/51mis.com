<script type="text/javascript">
    var myDate = new Date();
    $("#year").html(myDate.getFullYear());

    $(document).ready(function () {
        $("#ex_wechat input[type='text']").on("focus", function () {
            $(this).prev().hide()
        }).on('blur', function () {
            if (!$(this).val()) {
                $(this).prev().show()
            }
        });
        $(".close").on("click", function () {
            $("#modal").hide()
        });
    });
    function experience_wechat() {
        $("#modal").show();
        $("#ex_wechat").show();
        $("#success").hide();
        $("input").val("");
        $("label").show();
        $(".error").hide();
    }
    $('.nav-rig a[tag=freeuse]').unbind('click').on('click', function (event) {
        $(".nav-rig .menu-programme").toggle();
    });
    
    //鼠标移入
    $('.nav-rig a[tag=freeuse]').mouseenter(function(){//这里使用mouseover也可以，使用mouseenter是防止冒泡
        $(".nav-rig .menu-programme").show();//这里的show可以改成animate，toggle都可以
    });
    
    //鼠标移出
    $('.nav-rig .menu-programme').mouseleave(function(){//这里使用mouseover也可以，使用mouseenter是防止冒泡
        $(".nav-rig .menu-programme").hide();//这里的hide可以改成animate，toggle都可以
    });
</script>
