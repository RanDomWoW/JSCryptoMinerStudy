//手机端弹出下拉
$(function(){
	$(document).on('click','#superNatant1,#superNatant2',function(){//需要几个下拉到这个加id
		var id = $(this).prop('id');
		var state = $(this).siblings().css('display');
		$(this).find('img').hide();
		var self = $(this);
		var action = (state == 'none' ? '1' : '0');
		function slide(state){
			if(state=='1'){
				self.siblings().slideDown();
			}else if(state=='0'){
				self.siblings().slideUp();
			}
			$('#'+id).find('img').eq(state).show();	
		}
		return slide(action);
	})
	
})
			
