$(function(){
	var screenW = $(document).width()-15;
	function CreateDom(id,num,topHeight,topNum,col){
		//id为图片id,num为图片划分的块数,topHeight为上面那块的高度,topNum为上面划分成几块(一般情况为1块)，col为下面一排分为几块(一般情况为2块)
		if($("#"+id).length==0){
			return false;
		}
		var arr = [];
		var oDiv = $("<div></div>");
		var imgSrc = ($("#"+id).attr("data-original") || $("#"+id).attr("data-urlImg") || $("#"+id).attr("src"));
		var image = new Image()
	    var timerGetImg = id +"img";
	    image.src = imgSrc;
	    for(var i=0;i<num;i++){
	    	arr.push(arguments[i+5]);
	    }
	    
	    timerGetImg = setInterval(function(){
	    	if(image.height != 0){
		    	clearInterval(timerGetImg);
		    	var imgH = (image.height)*(screenW/640);
				var surplus = 100-topHeight;
				oDiv.attr({"id":id + "area","class":"positionWrapper clearfix"});
				$("#"+id).after(oDiv);
				oDiv.css({"height":imgH,"marginTop":-imgH,"position":"relative","z-index":"2"});
				
				for(var i=0;i<num;i++){
					oDiv.append($("<a href='/category/4.html'></a>"));	
					var target = oDiv.find("a").eq(i);
					target.css({"height":surplus/((num-topNum)/col)+"%","width":100/col+"%"});
					if(typeof parseInt(arr[i]) == 'number' && !isNaN(parseInt(arr[i]))){//传入的是数字
						target.attr({"href":"/category/"+arr[i]+".html"});
					}else if(arr[i] == ''||arr[i] == ' '){//未传入
						
						target.removeAttr('href');
						
					}else{//其他，则按传入原样输出
						target.attr({"href":arr[i]});
					}
				}
				
				if(topNum != 0){
					for(var i=0;i<topNum;i++){
						oDiv.find("a").eq(i).css({"height":topHeight/topNum+"%","width":"100%"});
					}
				}
				
		    }
	    },50);
	    
	}
