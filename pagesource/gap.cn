//首页top商品单品轮播
$(function(){
	var screenWidth = $(window).width()-14;
	var imgTogether = (screenWidth/2)*18;
	var imgOne = screenWidth/2;
	$('[id=sliderImgMb]:eq(1)').css("width",imgTogether);
	$('[id=sliderImgMb]:eq(1) li').css("width",imgOne);
	$('[id=sliderMb]:eq(1)').animate({'scrollLeft':"0px"});
	var product = {
	    scroll:function(imgTogether,imgOne){
	        var scrollLeft = $('[id=sliderMb]:eq(1)').scrollLeft();
	        var scrollMax = imgTogether - imgOne*2;
			scrollLeft = scrollLeft+imgOne+"px";
			$('[id=sliderMb]:eq(1)').animate({'scrollLeft':scrollLeft});
			if(parseInt(scrollLeft)>0){
				$('[id=leftArrowMb]:eq(1)').show();
			}else{
				$('[id=leftArrowMb]:eq(1)').hide();
			}
			if(parseInt(scrollLeft)>=scrollMax){
				$('[id=rightArrowMb]:eq(1)').hide();
			}else{
				$('[id=rightArrowMb]:eq(1)').show();
			}
		},
		autoMatic:function(imgTogether,imgOne,direction){
		  	TimerMb= setInterval(function(){
				var scrollLeft = $('[id=sliderMb]:eq(1)').scrollLeft();
		        var scrollMax = imgTogether - imgOne*2;
		        if(parseInt(scrollLeft)>=scrollMax){

		        	direction = -1;
		        }else if(parseInt(scrollLeft)<=0){
		        	direction = 1;
		        }
				if(direction == -1){
					product.scroll(imgTogether,-imgOne);
				}else if(direction == 1){
					product.scroll(imgTogether,imgOne);
				}	
			},1200)
		}
	}
	product.autoMatic(imgTogether,imgOne,1);
	$('[id=rightArrowMb]:eq(1)').click(function(){
		product.scroll(imgTogether,imgOne);
		clearInterval(TimerMb);
		setTimeout(product.autoMatic(imgTogether,imgOne,1),240);
	})
	$('[id=leftArrowMb]:eq(1)').click(function(){
		product.scroll(imgTogether,-imgOne);
		clearInterval(TimerMb);
		setTimeout(product.autoMatic(imgTogether,imgOne,-1),240)
	})
})
