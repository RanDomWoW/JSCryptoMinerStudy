{ name: "ty", logo: "ty-logo.png", screenshot: "ty-screenshot.jpg", url: "http://www.kromestudios.com/TY/", pixelart: false },
{ name: "neurovoider", logo: "neurovoider-logo.png", screenshot: "neurovoider-screenshot.jpg", url: "http://www.neurovoider.com/", pixelart: true },
