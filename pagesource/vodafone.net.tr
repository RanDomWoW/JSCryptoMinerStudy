<script type="text/javascript">
$(document).ready(function(){
galeriInitializeCounter();
$("div#galeriGoster").click(function(e){
$("#galeriGoster").fadeOut("slow", function(){
$("#resimGalerisiCerceve").slideDown("slow");
});
});
$("a", "#galeriKontrolleri").click(function(e){
e.preventDefault();
var myTrigger = $(e.target);
var myTriggerId = $(myTrigger).attr("alt");
if(myTriggerId != undefined) {
galeriResimDegistir(myTriggerId);
} else {
var myTriggerClass = $(myTrigger).attr("class");
switch(myTriggerClass) {
case 'geri':
if(galeri_image_active <= 1) {
galeri_image_active=galeri_image_max;
} else {
galeri_image_active--;
}
galeriResimDegistir(galeri_image_active);
break;
case 'ileri':
if(galeri_image_active >= galeri_image_max) {
galeri_image_active=1;
} else {
galeri_image_active++;
}
galeriResimDegistir(galeri_image_active);
break;
case 'durdur':
galeri_active = 0;
$("a.durdur", "#galeriKontrolleri").hide();
$("a.oynat", "#galeriKontrolleri").show();
break;
case 'oynat':
$("a.oynat", "#galeriKontrolleri").hide();
$("a.durdur", "#galeriKontrolleri").show();
galeriResimDegistir();
galeri_active = 1;
break;
case 'kapat':
$("#resimGalerisiCerceve").slideUp("normal", function(){
$("div#galeriGoster").fadeIn();
});
break;
default:
break;
}
}
return false;
});
$("a.oynat", "#galeriKontrolleri").click();
});
var galeri_count_value = 0;
var galeri_image_active = 0;
var galeri_image_max = 1;
var galeri_active = 1;
function galeriValidateImgId(id) {
id = parseInt(id);
if(id > galeri_image_max || id < 1) {
id = 1;
}
return id;
}
function galeriInitializeCounter() {
if(galeri_count_value==0 || galeri_count_value>60) {
galeri_count_value=0;
}
if(galeri_count_value > 0 && galeri_count_value%3 == 0 && galeri_active){
galeriResimDegistir();
}
galeri_count_value += 1;
return setTimeout("galeriInitializeCounter();",1000);
}
function galeriResimDegistir(id) {
galeri_count_value = 0;
var yeni_resim_id = 0;
if(id == '' || id==null || id==undefined) {
yeni_resim_id = galeri_image_active + 1;
} else {
yeni_resim_id = id;
}
yeni_resim_id = galeriValidateImgId(yeni_resim_id);
galeri_image_active = yeni_resim_id;
$("img", "#galeriResimleri").each(function(){
if($(this).attr("alt") == yeni_resim_id) {
$(this).show();
} else {
$(this).hide();
}
});
$("a", "#galeriKontrolleri").each(function(){
if($(this).attr("alt") == yeni_resim_id) {
$(this).addClass("aktif");
} else {
$(this).removeClass("aktif");
}
});
}
</script>
