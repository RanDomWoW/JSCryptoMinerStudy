       ///HOME PAGE VIDEO - PAUSE AFTER 3 ITERATIONS, SHOW PLAY BUTTON

        var iterations = 1;
        document.getElementById('iteration').innerText = iterations;

        mainVideo.addEventListener('ended', function () {

            if (iterations < 3) {

                this.currentTime = 0;
                this.play();
                iterations++;

                document.getElementById('iteration').innerText = iterations;

            } else if (iterations = 3) {
                this.currentTime = 1.5;
                this.stop;

                $('#play-button').toggle();

                $('#play-button').on('click', function () {
                    mainVideo.play();
                    $('#play-button').toggle();
                });


            }

        }, false);
      
            
				//START CAROUSEL
      
        $('.carousel').carousel();

      
      
      
        //MAIN NAVBAR ON MOBILE
        $('#main-nav').addClass('js').before('<div id="main-nav-mobile">&#9776</div>');

        $('#main-nav-mobile').on('click', function (event) {
            event.preventDefault();
            $('#main-nav').toggleClass('mobile');
            $('#main-nav').toggle();
        });

        $(window).resize(function () {
            if (window.innerWidth > 991) {
                $('#main-nav').removeAttr("style");
            }
        });
        
      
      
       //MOVE NAV AND SEARCH ON MOBILE
        
    
            if ($(window).width() <= 1024) {     
                $('#top-bar').detach().prependTo('#main-nav');
                $('#search').detach().prependTo('#main-nav');
                $('#audience-nav').removeClass('desktop').detach().appendTo('#main-nav');
                $('#top-bar').show();
                $('#audience-nav').show();
