    <script>

        $(function () {

            var $conferenceDetailsButton = $("#conference-details-button"),
                $carouselCounter = $("#carousel-counter");

            if (typeof $conferenceDetailsButton.attr("href") === "undefined") {
                var $activeSlide = $("#conference-carousel .active");
                $conferenceDetailsButton.attr("href", $activeSlide.attr("data-url"));
            } 

            $("#conference-carousel").on("slid.bs.carousel", function (e) {              

                var $slide = $(e.relatedTarget);
                var bannerText = $slide.find(".banner-text-big");
                var bannerTextHeight = parseInt(bannerText.css("line-height")) * 3;

                bannerText.dotdotdot({
                    height: bannerTextHeight
                });

                $conferenceDetailsButton.attr("href", $slide.attr("data-url"));
                $carouselCounter.text($slide.attr("data-index"));

                
            });
        });

    </script>

