function addCookie(name,value,expireDay)
{
	var cookie_value = getCookie(name);
	
	if(cookie_value && cookie_value != '')
	{
		var cookie_value_arr = cookie_value.split(escape('/'));
		var sign = 0;
		for(var i=0;i<cookie_value_arr.length;i++)
		{
			if(cookie_value_arr[i] == value)
			{
				sign = 1;
				break;
			}
		}
		
		if(sign == 1)
		{
			var cookieString=name + '=' + escape(cookie_value);
		}
		else
		{
			var cookieString=name + '=' + escape(cookie_value + '/' + value);
		}
	}
	else
	{
		var cookieString=name + '=' + escape(value);
	}
	
	if(expireDay > 0)
	{
		var date=new Date();
		date.setTime(date.getTime+expireDay*24*3600*1000);
		cookieString=cookieString+"; expire="+date.toGMTString();
	}
	document.cookie=cookieString + '; domain=' + location.hostname;
}

function getCookie(name)
{
	var strCookie=document.cookie;
	var arrCookie=strCookie.split("; ");
	for(var i=0;i<arrCookie.length;i++)
	{
		var arr=arrCookie[i].split("=");
		if(arr[0]==name)return arr[1];
	}
	return ""; 
}

function deleteCookie(name,value,expireDay)
{
	var cookie_value = getCookie(name);
	
	if(cookie_value != '')
	{
		var new_cookie_arr = new Array();
		var j = 0;
		var cookie_value_arr = cookie_value.split(escape('/'));
		for(var i=0;i<cookie_value_arr.length;i++)
		{
			if(cookie_value_arr[i] != value)
			{
				new_cookie_arr[j] = cookie_value_arr[i];
				j++;
			}
		}
		
		var new_cookie_value = new_cookie_arr.join('/');
		var date=new Date();
		if(new_cookie_value == '')
		{
			var cookieString=name + '=';
			date.setTime(date.getTime()-10000);
		}
		else
		{
			var cookieString=name + '=' + escape(new_cookie_value);
			date.setTime(date.getTime+expireDay*24*3600*1000);
		}
		
		document.cookie=cookieString + '; domain=' + location.hostname +'; expire='+date.toGMTString();
	}
}

