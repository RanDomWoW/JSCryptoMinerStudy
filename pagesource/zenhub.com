var mixpanelDelayHandler = (function() {
  var hasLoaded = false;
  var delayedCallbacks = [];

  return {

    // Pass to mixpanel's .init(..., {loaded: $})
    callOnInit: function(mixpanel) {
      hasLoaded = true;
      for (var index = 0, n = delayedCallbacks.length; index < n; index++) {
        delayedCallbacks[index](mixpanel);
      }
