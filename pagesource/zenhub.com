      if (!this.hasLoaded()) {
        return mixpanelDelayHandler.addToCallbacks(job);
      }

      job(mixpanel);
    },

    addToCallbacks: function(job) {
      delayedCallbacks.push(job);
    },

    hasLoaded: function() {
      return hasLoaded;
    }
  };
})();
