			jQuery("#recipeindex_sidebar_terms3").slideDown(); 
			jQuery("#recipeindex_sidebar_title3 .arrow_terms_down").show(); 
			jQuery("#recipeindex_sidebar_title3 .arrow_terms_right").hide(); 
		}
		if (dataOpen4 == 'yes') {
			jQuery("#recipeindex_sidebar_terms4").slideDown(); 
			jQuery("#recipeindex_sidebar_title4 .arrow_terms_down").show(); 
			jQuery("#recipeindex_sidebar_title4 .arrow_terms_right").hide(); 
		}
		
	});	
   	function openCloseRecipeHeader(openDiv, showArrow) {
			if (jQuery("#" + openDiv).is(':visible')) {
				jQuery("#" + openDiv).slideUp();
				
				jQuery("#" + showArrow + " .arrow_terms_down").hide(); 
				jQuery("#" + showArrow + " .arrow_terms_right").show();
			}
			else {
				jQuery("#"+openDiv).slideDown();

				jQuery("#" + showArrow + " .arrow_terms_down").show(); 
				jQuery("#" + showArrow + " .arrow_terms_right").hide();
			}


   	}


	
