	//RECIPE INDEX PAGE
   	function submitRecipeIndex(term) {  
		//check to see if current term clicked is checked or unchecked
		thisChecked = jQuery('.'+term+' input').attr( 'checked');
		if (thisChecked == "checked") {
			jQuery('.'+term+' input').iCheck('uncheck');
			//alert ('this was checked - now unchecked');
		}
		else {
			jQuery('.'+term+' input').iCheck('check');
			//alert ('this was not checked - now checked');
		} 

		//get terms for all currently checked checkboxes
		var checkedTermsObject = jQuery('.recipeindex_check:checked')
		var allCheckedTerms = [];
		var allCheckedTaxonomies = [];
		var allCheckedNames = [];
		jQuery.each(checkedTermsObject, function(index, value) {
	    	terms = jQuery(this).attr('selected-term');
	    	taxonomy = jQuery(this).attr('selected-taxonomy');	    	
	    	names = jQuery(this).attr('selected-name');	    	
		    allCheckedTerms.push(terms);
		    allCheckedTaxonomies.push(taxonomy);
		    allCheckedNames.push(names);
		}); 
		//alert (allCheckedTerms);

   		//add hidden field for recipe terms
		var hiddenInput = document.createElement("input");
		hiddenInput.setAttribute("type", "hidden");
		hiddenInput.setAttribute("name", "i");
		hiddenInput.setAttribute("value", allCheckedTerms);
		document.getElementById('recipeindex_form').appendChild(hiddenInput);
   		//add hidden field for recipe taxonomies
		var hiddenInput2 = document.createElement("input");
		hiddenInput2.setAttribute("type", "hidden");
		hiddenInput2.setAttribute("name", "t");
		hiddenInput2.setAttribute("value", allCheckedTaxonomies);
		document.getElementById('recipeindex_form').appendChild(hiddenInput2);
   		//add hidden field for recipe name
		var hiddenInput3 = document.createElement("input");
		hiddenInput3.setAttribute("type", "hidden");
		hiddenInput3.setAttribute("name", "n");
		hiddenInput3.setAttribute("value", allCheckedNames);
		document.getElementById('recipeindex_form').appendChild(hiddenInput3);
 		//submit form		
		document.getElementById('recipeindex_form').submit();
   	}

	//LISTEN FOR CLICKS ON RECIPE INDEX CHECKBOXES
	jQuery(document).ready(function(){
		jQuery('#r_index_left input').on('ifClicked', function(event){
			//get current term
			var term = jQuery(this).attr('selected-term');
			//alert(term);

			//submit form with term
			submitRecipeIndex(term);
			
			//NEEDS TO SUBMIT AN ARRAY OF ALL CHECKED TERMS

		});
	});
	
