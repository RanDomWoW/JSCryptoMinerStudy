	//SAVE RECIPE
   	function sfb_save_recipe() {

		var photoOnOff = jQuery('.content_save_choose input:checkbox:checked').val();	
		//"on"
		
		var hiddenInput = document.createElement("input");
		hiddenInput.setAttribute("type", "hidden");
		hiddenInput.setAttribute("name", "photo");
		hiddenInput.setAttribute("value", photoOnOff);
		document.getElementById("save_recipe_form").appendChild(hiddenInput);

		document.getElementById("save_recipe_form").submit();
   	}

	//SAVE RECIPE
   	function sfb_print_recipe(url) {

		var photoOnOffTemp = jQuery('.content_print_choose input:checkbox:checked').val();	
		if (photoOnOffTemp == 'on') {
			photoOnOff = 1;
		}
		else {
			photoOnOff = 0;		
		}		
		
		window.open(
  			url + "?photo=" + photoOnOff,
			'_blank'
		);
	}
   	
