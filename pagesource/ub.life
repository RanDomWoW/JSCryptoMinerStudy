        $(function(){
            var atbottom = false;
            var start = 20;
            $("#loading").hide();
            $(window).scroll(function() {
                if(atbottom == false){
                    var hT = $('#loader').offset().top,
                       hH = $('#loader').outerHeight(),
                       wH = $(window).height(),
                       wS = $(this).scrollTop();
                    if (wS > (hT+hH-wH)){                       
                       atbottom = true;
                       $("#loading").show();                                           
                       $.ajax({
                            url:"http://www.ub.life/getnewsmain2",
                            type:'POST',
                            data:{start:start},
                            success:function(data){
                                start+=5;
                                $('#slidenews').append(data);
                                $("#loading").hide();
                                if(data != "" && data != null){
                                    atbottom = false;
                                } else {
                                    atbottom = true;
                                }
                            }
                       });
                    }
                }
            });
        });
