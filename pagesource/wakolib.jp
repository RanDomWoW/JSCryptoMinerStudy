	function getKey(){
		var k = "";
		var seed = "abcdefghijklmnopqrstuvwxyz_!?[]{}^:;%$#=+-*0123456789";
		var seedLen = seed.length;
		
		for(var i=0;i<32;i++){
			var frag=Math.floor(Math.random()*seedLen);
			k += seed.charAt(frag);
		}
		return k;
	}

	function encryptBlowfish(data, key){
		var usr = {
			data    : data,
			key     : key,
			mode    : "cbc",
			round   : 16,
			iv      : blowfish.mkIV(),
			pchar   : "\0" 
	    }; 
	    encrypted = blowfish.encrypt(usr);
	    return base64.encode(encrypted);
	} 
