<script type="text/javascript">

    $('#select_weather_city').change(function(){
        $('.css-loader').fadeIn('slow');
        $.ajax({
            url: "/weather_status",
            type: 'post',
            data: {city_id: $(this).val()},
            success: function(weather_reault) {

                $('.css-loader').fadeOut('slow');

                weather_reault = $.parseJSON(weather_reault);

                $('.weather-head .currentStatusText').text(weather_reault['current_status']['text']).parent().find($('.w-curr-tmp')).text(weather_reault['current_status']['temp'] + '°')
                $('.weather-head .currentStatusText').text(weather_reault['current_status']['text']).parent().addClass('animated bounceIn');

                $('.weather-current-forecast-icon').find($('i.current-icon')).removeClass().addClass('current-icon '+weather_reault['current_status']['icon']).addClass('animated bounceIn');
                $('.weather-head .weather-current-temp .w-curr-tmp').fadeIn();
                $('.weather-head .weather-current-temp .forecast-status').fadeOut();
                $.each(weather_reault['weather_forecast'], function(index, cast_day){
                    if(index < 4){
                        $('.forecast-dayname-'+index + ' a').text(cast_day['day']);
                        $('.forecast-dayname-'+index + ' a').attr('data-index', index);
                        $('.forecast-dayname-'+index + ' a').attr('data-tmphigh', cast_day['high']);
                        $('.forecast-dayname-'+index + ' a').attr('data-tmplow', cast_day['low']);
                        $('.forecast-dayname-'+index + ' a').attr('data-icon', cast_day['icon']);
                    }
                });


                $('.weather-forecast .col-md-3').addClass('animated bounceIn');
                setTimeout(function() {
                    $("div, span, i").removeClass('animated');
                    $("div, span, i").removeClass('bounceIn');
                }, 2000);


            },
            error: function(err){
                $('.css-loader').fadeOut('slow');
            }
        });
    });

    $('.forecast-dayname a').on('click', function(){
        $('.weather-forecast .forecast-dayname a.active').removeClass();
        $(this).addClass('active');
        $('.weather-current-forecast-icon i').removeClass().addClass('current-icon ' + $(this).attr('data-icon') );
        $('.weather-current-temp .day').text($(this).text());
        var temp = '<div class="high">'+ $(this).attr('data-tmphigh') +' °</div>';
        temp = temp + '<div class="low">'+ $(this).attr('data-tmplow') +' °</div>';
        $('.weather-head .weather-current-temp .w-curr-tmp').fadeOut();
        $('.weather-head .weather-current-temp .forecast-status').fadeIn();

        $('.weather-current-temp .forecast-status').empty().append(temp);

        return false;
    });

</script>
