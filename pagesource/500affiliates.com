    // Show and hide testemonials
    function rotateTestemonials(testemonial) {
        testemonial.delay(7000).fadeOut(500, function () {
            var nextOne = $(this).next().get(0) ? $(this).next() : $(this).prevAll().last();
            nextOne.fadeIn(500, function () { rotateTestemonials($(this)); });
        })
    }
    rotateTestemonials($('#testemonials blockquote:first'));
</script>


