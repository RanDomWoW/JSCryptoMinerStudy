<script type="text/javascript">

// ==== START PERF (FE Performance testscript)
(function(window) {
    function perf() {

        this.point = {};

        this.getResultObj = function(name) {
            if(name && this.point[name]) {
                returnVal = this.point[name];
            } else {
                returnVal = this.point;
            }
            return returnVal;
        }

        this.setStart = function(name) {
            if(name) {
                this.point[name] = {'name': name};
                this.point[name]['start'] = performance.now();
            }
        }

        this.setEnd = function(name) {
            if(name) {
                this.point[name].end = performance.now();
            }

            if (this.point[name].start && this.point[name].end) {
                this.point[name].duration = (this.point[name].end - this.point[name].start).toFixed(1)
            }
        }

        this.getRunTime = function(name) {
            // returns a specific result if name is defined
            // otherwise an object w/ all results
            var msg = '',
                mpoints = this.point,
                time;
            // object/ point exists
            if (name && mpoints[name]) {
                time = mpoints[name].end - mpoints[name].start
                msg += name + ': ' + time;
            } else {
                for ( var mp in mpoints) {
                    time = mpoints[mp].end - mpoints[mp].start;
                    msg += mp + ': ' + time + ' \n';
                }
            }
            return mpoints;
        }

        this.printResults = function() {
            var mpoints = this.point;

            for (let key in mpoints) {
                console.log("%c" + key + ' %ctook %c' + mpoints[key].duration  + ' Milli Seconds', 'color:green;font-weight:700', 'color: black', 'color:blue; font-weight: 700');
            }
        }
    }
    window.perf = window.perf || new perf();

})(window);
// ==== END PERF (FE Performance testscript)

//perf.setStart('head');
//perf.setStart('head+body');
</script>
    
