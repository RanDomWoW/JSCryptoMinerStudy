                  console.log("Socket ID: " + socket.id);
                });
                socket.on('updateMinute', function (data) {
                    var newArticle = $.parseJSON(data.body);
                    var lastNewsArray = $("#home-last-news").find(".minute-article");
                    if (newArticle.id != window.LastArticleID) {
                        var template = $.templates("#minute-item");
