                        var mostReadHeight = $('#home-most-read').height();
                        var lastNewsHeight = $('#home-last-news').height();
                        var newColumnHeight = Math.max(mostReadHeight, lastNewsHeight);

                        $('#home-last-news').height(newColumnHeight);
                        $('#home-most-read').height(newColumnHeight);
                    }
                });
                socket.on('updateTrend', function (data) {
                    console.log("Socket ID (Update Trend):");
                    $("#trend-ajax").LoadingOverlay("show");
                    var template = $.templates("#trend-row");
                    $('#trend-ajax').empty();
                    $.each( $.parseJSON(data.body), function( key, val ) {
                        if (val.views > lscache.get(val.link)) {
                            perc = ((lscache.get(val.link)/val.views) * 100).toFixed();
                            if (perc > 90) {
                                dynicon = "fa-angle-double-up";
                                dynopacity = "0.8";
                            } else {
                                dynicon = "fa-angle-up";
                                dynopacity = "0.5";
                            }
                            val.state = "up";
                            val.icon = "<span class='trend-icon fa-stack fa-lg animated fadeInUp'><i class='fa fa-circle fa-stack-2x' style='color: #84cf96; opacity: " + dynopacity + ";'></i><i class='fa " + dynicon + " fa-stack-1x fa-inverse'></i></span>";
                        } else if (val.views < lscache.get(val.link)) {
                            perc = ((val.views/lscache.get(val.link)) * 100).toFixed();
                            if (perc > 90) {
                                dynicon = "fa-angle-double-down";
                                dynopacity = "0.8";
                            } else {
                                dynicon = "fa-angle-down";
                                dynopacity = "0.5";
                            }
                            val.state = "down";
                            val.icon = "<span class='trend-icon fa-stack fa-lg animated fadeInDown'><i class='fa fa-circle fa-stack-2x' style='color: #f58974; opacity: " + dynopacity + ";'></i><i class='fa " + dynicon + " fa-stack-1x fa-inverse'></i></span>";
                        } else {
                            val.state = "eq";
                        }
                        var htmlOutput = template.render(val);
                        lscache.set(val.link, val.views);
                        $("#trend-ajax").append(htmlOutput);
                    });
                    $("#trend-ajax").LoadingOverlay("hide", true);
                });
                
