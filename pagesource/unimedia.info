                                    Cookies.set(uniArticleID, {
                                        reacted: 'true',
                                        reaction: reactImg
                                    });
                                    $('.reaction-ico').attr('data-active', 0);
                                    $this.attr('data-active', 1);
                                    var newSRC = 'http://unimedia.info/assets/uni/img/emoji/b_' + reactImg + '.jpg';
                                    reactIco.attr('src', newSRC);
                                    var oldSRC = 'http://unimedia.info/assets/uni/img/emoji/g_' + reaction + '.jpg';
                                    $(".reaction-ico[data-react=" + reaction + "]").find("img").attr('src', oldSRC);
                                    $.each(data, function(key, val) {
                                        $("#count-" + key).html(val);
                                    });
                                });
                            }
                        } else {
                            $.getJSON('https://labs.altfel.md/uni/react.php?action=up&react=' + reactImg + '&articleID=' + uniArticleID, function(data) {
                                Cookies.set(uniArticleID, {
                                    reacted: 'true',
