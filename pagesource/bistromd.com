<script>
  (function ($) {
    $(document).ready(function () {
      $.slidebars();
    });
  })(jQuery);

  (function () {
    "use strict";
    var toggles = document.querySelectorAll(".c-hamburger");
    for (var i = toggles.length - 1; i >= 0; i--) {
      var toggle = toggles[i];
      toggleHandler(toggle);
    };
    function toggleHandler(toggle) {
      toggle.addEventListener("click", function (e) {
        e.preventDefault();
        (this.classList.contains("is-active") === true) ? this.classList.remove("is-active") : this.classList.add("is-active");
      });
    }
  })();

  $(document).foundation();

  $(document).ready(function () {

    $('.preRender').show();

    $(function () {
      $('#main-menu').smartmenus({
        subMenusSubOffsetX: 1,
        subMenusSubOffsetY: -8
      });
    });

    $('#newsLetterProcessingLabel').hide();

    function isValidEmailAddress(emailAddress) {
      var pattern = new RegExp(/^(("[\w-+\s]+")|([\w-+]+(?:\.[\w-+]+)*)|("[\w-+\s]+")([\w-+]+(?:\.[\w-+]+)*))(@((?:[\w-+]+\.)*\w[\w-+]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][\d]\.|1[\d]{2}\.|[\d]{1,2}\.))((25[0-5]|2[0-4][\d]|1[\d]{2}|[\d]{1,2})\.){2}(25[0-5]|2[0-4][\d]|1[\d]{2}|[\d]{1,2})\]?$)/i);
      return pattern.test(emailAddress);
    };

    //newsletter popup

    $("#joinNewsletterInput input[type='text']").keydown(function (event) {
      if (event.keyCode == 13) {
        $('#footerSignUpButton').trigger('click');
        return false;
      }
    });

    jQuery("#footerSignUpButton").click(function () {
      try {
        var emailid = jQuery("input[name='email__']").val();

        if (emailid == "") {
          $('#newsletterSuccessDiv').text("Please enter your email address.");
          showPopup();
        }
        else if (!isValidEmailAddress(emailid)) {
          $('#newsletterSuccessDiv').text("Invalid email address.");
          showPopup();
        }
        else {

          $('#newsLetterProcessingLabel').show();
          var email = jQuery("input[name='email__']").val();

          $.ajax({
            //type: 'POST',
            //url: document.location.protocol + '//' + document.location.host + "/RequestHandler.ashx",
            //data: { FirstName: "FirstName", LastName: "LastName", EmailAddress: email, MethodName: "NewsLetter" },
            type: 'GET',
            url: "http://asmx.bistromd.com/requesthandler.asmx/NewsLetter",
            data: { MethodName: "News Letter", FirstName: "FirstName", LastName: "LastName", EmailAddress: emailid, CampaignId: "701d0000000TGih" },
            success: function (data, textStatus, jqXHR) {
              $('#newsLetterProcessingLabel').hide();
              if (data) {
                $('#newsletterSuccessDiv').text("Thank You for signing up for the Weekly Dish on Health.");
                showPopup();
                //$('.footer .newsletter input[type="text"]').val('');
                $('#joinNewsletterInput').val('');
              }
              else {
                $('#newsletterSuccessDiv').text("Error occured: " + data.ErrorResponse);
                showPopup();
              }
              $('.footer .newsletter input[type="text"]').val('');

            },
            error: function (jqXHR, textStatus, errorThrown) {
              $('#newsLetterProcessingLabel').hide();
              alert("Sorry, something went wrong, Please try again later.");
            }
          });
        }
      } catch (err) {
        $('#newsLetterProcessingLabel').hide();
        console.log(err);
      }

      return false;
    });

    $("#newsletterOKButton").click(function () {
      hidePopup();
    });
  });

  jQuery.fn.center = function () {

    this.css("position", "fixed");
    this.css("top", "50%");
    this.css("left", "50%");
    this.css("transform", "translate(-50%, -50%)");
    return this;
  }

  function showPopup() {
    $("#newsletterMessage").show().center();
    $("#backDiv").show();
    setTimeout(hidePopup, 3000);
  }

  function hidePopup() {
    jQuery("input[name='email__']").focus();
    $("#newsletterMessage").hide();
    $("#backDiv").hide();
  }

</script>

