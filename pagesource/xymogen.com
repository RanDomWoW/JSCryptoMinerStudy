<script type="text/javascript">
    function SendContactUs() {

        if (!validateForm())
            return;

        var dataToPost = {};
        dataToPost.contactName = $("#tbContactName").val();
        dataToPost.contactEmail = $("#tbContactEmail").val();
        dataToPost.contactPhone = $("#tbContactPhone").val();
        dataToPost.contactAccount = $("#tbContactAccount").val();
        dataToPost.contactInquiry = $("#ddlContactInquiry").val();
        dataToPost.contactMessage = $("#tbContactMessage").val();
        var dataStr = JSON.stringify(dataToPost);
        $.ajax({
            type: "POST",
            cache: false,
            url: "/UserControls/Utilities/eStoreServices.asmx/ContactUs",
            contentType: "application/json; charset=utf-8",
            data: dataStr,
            dataType: "json",
        }).done(function (data) {
            $("#lblStatus").text(data.d);
            clearContactFields();
        }).fail(function (jqXHR, textStatus) {
            alert('error: ' + jqXHR.responseText);
        });
    };

    function clearContactFields() {
        $("#tbContactName").val("");
        $("#tbContactEmail").val("");
        $("#tbContactPhone").val("");
        $("#tbContactAccount").val("");
        $("#ddlContactInquiry").val("");
        $("#tbContactMessage").val("");
    }

    function validateForm() {
        var valPass = true;
        if (document.getElementById("tbContactName").value == "") {
            document.getElementById("divContactNameValidation").innerHTML = "Name is Required";
            valPass = false;
        }
        else
            document.getElementById("divContactNameValidation").innerHTML = "";

        if (document.getElementById("tbContactEmail").value == "") {
            document.getElementById("divContactEmailValidation").innerHTML = "Email is Required";
            valPass = false;
        }
        else if (!validateEmail(document.getElementById("tbContactEmail").value)) {
            document.getElementById("divContactEmailValidation").innerHTML = "Improper Email Format";
            valPass = false;
        }
        else
            document.getElementById("divContactEmailValidation").innerHTML = "";
        if (document.getElementById("tbContactMessage").value == "") {
            document.getElementById("divContactMessageValidation").innerHTML = "Message is Required";
            valPass = false;
        }
        else
            document.getElementById("divContactMessageValidation").innerHTML = "";
        return valPass;
    };

    function validateEmail(email) {
        var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return re.test(email);
    };
</script>
