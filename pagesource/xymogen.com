            <script type="text/javascript">
                $(document).ready(function () {
                    $("[id=Password]").on("keypress", function (e) {
                        checkSubmitSignIn(e)
                    });

                    $("[id=UserName]").on("keypress", function (e) {
                        if (e.keyCode == 13) {
                            e.preventDefault();
                        }
                    });

                    LoadMiniCart();

                });

                var getPointerEvent = function (event) {
                    return event.originalEvent.targetTouches ? event.originalEvent.targetTouches[0] : event;
                };
                var $touchArea = $('#touchArea'),
                    touchStarted = false, // detect if a touch event is sarted
                    currX = 0,
                    currY = 0,
                    cachedX = 0,
                    cachedY = 0;

                //setting the events listeners
                $touchArea.on('touchstart mousedown', function (e) {
                    e.preventDefault();
                    var pointer = getPointerEvent(e);
                    // caching the current x
                    cachedX = currX = pointer.pageX;
                    // caching the current y
                    cachedY = currY = pointer.pageY;
                    // a touch event is detected      
                    touchStarted = true;
                    $touchArea.text('Touchstarted');
                    // detecting if after 200ms the finger is still in the same position
                    setTimeout(function () {
                        if ((cachedX === currX) && !touchStarted && (cachedY === currY)) {
                            // Here you get the Tap event
                            $touchArea.text('Tap');
                        }
                    }, 200);
                });
                $touchArea.on('touchend mouseup touchcancel', function (e) {
                    e.preventDefault();
                    // here we can consider finished the touch event
                    touchStarted = false;
                    $touchArea.text('Touchended');
                });
                $touchArea.on('touchmove mousemove', function (e) {
                    e.preventDefault();
                    var pointer = getPointerEvent(e);
                    currX = pointer.pageX;
                    currY = pointer.pageY;
                    if (touchStarted) {
                        // here you are swiping
                        $touchArea.text('Swiping');
                    }

                });

                ShowMainAlert();

                if ("False" == "True") {
                    var sessionTimeoutWarning = "100";
                    var sessionTimeout = "120";
                    var timeOnPageLoad = new Date();
                    var sessionWarningTimer = null;
                    var redirectToWelcomePageTimer = null;
                    //For warning
                    var sessionWarningTimer = setTimeout('SessionWarning()',
                            parseInt(sessionTimeoutWarning) * 60 * 1000);
                    //To redirect to the welcome page
                    var redirectToWelcomePageTimer = setTimeout('RedirectToWelcomePage()',
                                parseInt(sessionTimeout) * 60 * 1000);

                    //Session Warning
                    function SessionWarning() {
                        $("#divTimeOutModal").empty();
                        //minutes left for expiry
                        var minutesForExpiry = (parseInt(sessionTimeout) -
                                parseInt(sessionTimeoutWarning));
                        var messageModal = '<div class="modal fade" id="MessageModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"><div class="modal-dialog" role="document"><div class="modal-content"><div class="modal-body">';
                        messageModal += '<p class="lead-24">Your session will expire in ' + minutesForExpiry + ' minute';
                        if (minutesForExpiry > 1) {
                            messageModal += 's';
                        }
                        messageModal += '.</p><div class="pi-row pi-text-center"><div class="pi-col-xs-6 pi-margin-bottom-10">';
                        messageModal += '<a href="/logout/" class="btn pi-btn-base">Log Out</a></div><div class="pi-col-xs-6 pi-margin-bottom-10"><a href="#" class="btn pi-btn-base" data-dismiss="modal" onclick="answerFromSessionModal(true);">Continue</a></div>';
                        messageModal += '</div></div></div></div></div>';
                        $("#divTimeOutModal").append(messageModal);
                        $("#divTimeOutModal #MessageModal").modal('show');
                        //overlay.css("background", "#000").css("opacity", "1");
                        $("#MessageModal").on("hidden.bs.modal", function () {
                            //$(".ui-widget-overlay").css("background", baseBackground).css("opacity", baseOpacity);
                            answerFromSessionModal(false);
                        });
                    }

                    function answerFromSessionModal(answer) {
                        //if yes, extend the session.
                        if (answer) {
                            var img = new Image(1, 1);
                            img.src = '/?date=' + escape(new Date());

                            //Clear the RedirectToWelcomePage method
                            if (redirectToWelcomePageTimer != null) {
                                clearTimeout(redirectToWelcomePageTimer);
                            }
                            //reset the time on page load
                            timeOnPageLoad = new Date();
                            sessionWarningTimer = setTimeout('SessionWarning()',
                            parseInt(sessionTimeoutWarning) * 60 * 1000);
                            //To redirect to the welcome page
                            redirectToWelcomePageTimer = setTimeout('RedirectToWelcomePage()', parseInt(sessionTimeout) * 60 * 1000);
                        }

                        //*************************
                        //Even after clicking ok(extending session) or cancel button, 
                        //if the session time is over. Then exit the session.
                        var currentTime = new Date();
                        //time for expiry
                        var timeForExpiry = timeOnPageLoad.setMinutes(timeOnPageLoad.getMinutes() +
                            parseInt(sessionTimeout));

                        //Current time is greater than the expiry time
                        if (Date.parse(currentTime) > timeForExpiry) {
                            $("#divTimeOutModal #MessageModal").modal('hide');
                            RedirectToWelcomePage();
                        }
                        //**************************
                    }

                    //Session timeout
                    function RedirectToWelcomePage() {
                        $("#divTimeOutModal").empty();
                        var returnurl = '/logout/?signin=1&ReturnUrl=' + encodeURI(window.location.href);
                        var messageModal = '<div class="modal fade" id="MessageModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"><div class="modal-dialog" role="document"><div class="modal-content"><div class="modal-body">';
                        messageModal += '<p class="lead-24">Session expired. You will be redirected to the login page.</p><div class="pi-row pi-text-center">';
                        messageModal += '<a href="' + returnurl + '" class="btn pi-btn-base">Log Out</a></div></div></div></div></div>';
                        $("#divTimeOutModal").append(messageModal);
                        $("#divTimeOutModal #MessageModal").modal('show');
                        //overlay.css("background", "#000").css("opacity", "1");
                        $("#MessageModal").on("hidden.bs.modal", function () {
                            window.location = returnurl;
                        });
                    }
                }

                $(function () {
                    window.addEventListener('storage', function (e) {
                        if (e.key == "orderAsAPatientId" && e.newValue == "") {
                            StopWorkingWithPatient();
                        }
                    });
                });

                $('#CheckOrderUnderPatientModal').on('hidden.bs.modal', function () {
                    localStorage.setItem('orderAsAPatientId', '');
                    StopWorkingWithPatient();
                });

                function StopWorkingWithPatient() {
                    $.ajax({
                        type: "POST",
                        url: "/usercontrols/utilities/CustomerCare.asmx/doctorStopOrderAsCustomer",
                        dataType: "json",
                        contentType: "application/json; charset=utf-8",
                        success: function (data) {
                            //window.location = "/practitioners/practitioner-dashboard/my-patients/";
                            location.reload(true);
                        }
                    });
                }

                function StopShowingMainAlert() {
                    $.cookie('MainSiteAlertHide', 'true', { path: '/', expires: 1 });
                }

                function ShowMainAlert() {
                    if ($.cookie('MainSiteAlertHide') == null || $.cookie('MainSiteAlertHide') != 'true') {
                        $("#DivMainSiteAlert").removeClass('hidden');
                    }
                }
            </script>
            
