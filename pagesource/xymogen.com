<script>
    function validateReqFields(data) {
        var isValid = true;
        var regExp = /^[a-zA-Z0-9\s-]+$/;

        if (data.NewFirstName == '') {
            $("#spFirstNameRequired").text('* required');
            isValid = false;
        }
        else{
            if (!regExp.test(data.NewFirstName)) {
                $("#spFirstNameRequired").text('* No special characters allowed');
                isValid = false;
            }
            else $("#spFirstNameRequired").text('');
        }

        if (data.NewLastName == '') {
            $("#spLastNameRequired").text('* required');
            isValid = false;
        }
        else {
            if (!regExp.test(data.NewLastName)) {
                $("#spLastNameRequired").text('* No special characters allowed');
                isValid = false;
            }
            else $("#spLastNameRequired").text('');
        }

        if (data.NewAddress1 == '') {
            $("#spStreet1Required").text('* required');
            isValid = false;
        }
        else {
            if (!regExp.test(data.NewAddress1)) {
                $("#spStreet1Required").text('* No special characters allowed');
                isValid = false;
            }
            else $("#spStreet1Required").text('');
        }

        if (data.NewCity == '') {
            $("#spCityRequired").text('* required');
            isValid = false;
        }
        else {
            if (!regExp.test(data.NewCity)) {
                $("#spCityRequired").text('* No special characters allowed');
                isValid = false;
            }
            else $("#spCityRequired").text('');
        }

        if ((data.NewCountryCode == "en-US" && data.NewStateCode <= 0) || (data.NewCountryCode != "en-US" && data.NewState == "")) {
            $("#spStateRequired").text('* required');
            isValid = false;
        }
        else {
            if (!regExp.test(data.NewState)) {
                $("#spStateRequired").text('* No special characters allowed');
                isValid = false;
            }
            else $("#spStateRequired").text('');
        }

        if (data.NewZip == '') {
            $("#spZipRequired").text('* required');
            isValid = false;
        }
        else {
            if (!regExp.test(data.NewZip)) {
                $("#spZipRequired").text('* No special characters allowed');
                isValid = false;
            }
            else $("#spZipRequired").text('');
        }

        if (data.NewPhoneNumber1 == '') {
            $("#spPhoneRequired").text('* required');
            isValid = false;
        }
        else {
            if (!regExp.test(data.NewPhoneNumber1)) {
                $("#spPhoneRequired").text('* No special characters allowed');
                isValid = false;
            }
            else $("#spPhoneRequired").text('');
        }

        return isValid;
    }

    function saveAddressClick() {

        var divShippingChangeForm = $("#divShippingChangeForm");
        var reload = $("#hdfModalReload", divShippingChangeForm).val();
        var addressType = $("#hdfModalAddressType", divShippingChangeForm).val();

        var updatedForm = {};
        updatedForm.AddressName = $.trim($("#tbxAddressName", divShippingChangeForm).val());
        updatedForm.NewCountryCode = $("#dplShippingCountry", divShippingChangeForm).val();
        updatedForm.NewCountryName = $("#dplShippingCountry", divShippingChangeForm).children("option:selected").text();
        updatedForm.NewFirstName = $.trim($("#tbxShippingFirstName", divShippingChangeForm).val());
        updatedForm.NewMiddleName = $.trim($("#tbxShippingMiddleInitial", divShippingChangeForm).val());
        updatedForm.NewLastName = $.trim($("#tbxShippingLastName", divShippingChangeForm).val());
        updatedForm.NewCompany = $.trim($("#tbxShippingCompany", divShippingChangeForm).val());
        updatedForm.NewAddress1 = $.trim($("#tbxShippingAddress1", divShippingChangeForm).val());
        updatedForm.NewAddress2 = $.trim($("#tbxShippingAddress2", divShippingChangeForm).val());
        updatedForm.NewCity = $.trim($("#tbxShippingCity", divShippingChangeForm).val());
        if (updatedForm.NewCountryCode == "en-US") {
            updatedForm.NewState = $("#dplShippingState", divShippingChangeForm).children("option:selected").text();
            updatedForm.NewStateCode = $("#dplShippingState", divShippingChangeForm).val();
        }
        else {
            updatedForm.NewState = $.trim($("#tbxState", divShippingChangeForm).val());
            updatedForm.NewStateCode = -1;
        }
        updatedForm.NewZip = $.trim($("#tbxShippingZip", divShippingChangeForm).val());
        updatedForm.NewPhoneNumber1 = $.trim($("#tbxShippingPhone", divShippingChangeForm).val());
        updatedForm.NewDefaultShipTo = $("#cbxShippingDefaultShipTo", divShippingChangeForm).is(':checked');
        if (validateReqFields(updatedForm)) {
            var dataStr = JSON.stringify(updatedForm);
            $.ajax({
                type: "POST",
                cache: false,
                url: "/UserControls/Utilities/UserAddress.asmx/UpdateCustomerAddress",
                contentType: "application/json; charset=utf-8",
                data: dataStr,
                dataType: "json",
            }).done(function (data) {
                if (data.d) {
                    if (addressType != '' && reload) {
                        __doPostBack(addressType, updatedForm.AddressName);
                    }
                    else {
                        if (reload) __doPostBack('');
                        else $('#divShippingChangeForm').modal('hide');
                    }
                }
                else {
                    alert('error');
                }
            }).fail(function (jqXHR, textStatus) {
                alert('error');
            });
        }
    }

    function onChangeCountry(dpl) {
        if ($(dpl).val() == "en-US") {
            $("#dplShippingState", divShippingChangeForm).show();
            $("#tbxState", divShippingChangeForm).hide();
        } else {
            $("#dplShippingState", divShippingChangeForm).hide();
            $("#tbxState", divShippingChangeForm).show();
        }
    }
</script>
