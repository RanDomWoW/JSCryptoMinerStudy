var featured_news_timer = false;
var featured_news_count = false;
var featured_news_pos = 1;

$j(document).ready(function() {
	featured_news_count = $j('.thumbsHolder li').length;
	featured_news_pos = Math.floor(Math.random()*featured_news_count);

	// init walker
	walk_featured_news();

	// add over events
	$j('.thumbsHolder a').hover(
		function() {
			clearInterval(featured_news_timer);
			$j('.thumbsHolder li').removeClass('selected');
			
			var title = $j(this).find('h3').text();
			var imgsrc = $j(this).find('.mainImage img').attr('src');
			
			$j('#titleLink').attr('href', $j(this).attr('href'));
			$j('#titleLink').text(title);
			$j('.mainNewsBig .img img').attr('src', imgsrc);
		},

		function() {
			featured_news_timer = setTimeout(function() {
				walk_featured_news();
			}, 1500);
		}	
	);
});

function walk_featured_news() {
	$j('.thumbsHolder li').removeClass('selected');
	featured_news_pos++;

	if (featured_news_pos > featured_news_count)
		featured_news_pos = 1;

	var el = $j('.thumbsHolder li').eq(featured_news_pos-1).find('a');
	$j('.thumbsHolder li').eq(featured_news_pos-1).addClass('selected');
	
	var title = el.find('h3').text();
	var imgsrc = el.find('.mainImage img').attr('src');
	
	$j('.article_link').attr('href', el.attr('href'));
	$j('#titleLink').text(title);
	$j('.mainNewsBig .img img').attr('src', imgsrc);
 
	// create timer for walk
	featured_news_timer = setTimeout(function() {
		walk_featured_news();
	}, 3000);
}
</script>



