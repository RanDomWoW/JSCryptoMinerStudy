    function getCookie(c_name)
	{
		var c_value = document.cookie;
		var c_start = c_value.indexOf(" " + c_name + "=");
		if (c_start == -1){
	  		c_start = c_value.indexOf(c_name + "=");
	  	}
		if (c_start == -1){
	  		c_value = null;
	  	}else{
	  		c_start = c_value.indexOf("=", c_start) + 1;
	  		var c_end = c_value.indexOf(";", c_start);
	  		if (c_end == -1){
				c_end = c_value.length;
			}
			c_value = unescape(c_value.substring(c_start,c_end));
		}
		return c_value;
	}

    function updateCookie(c_name)
	{
		var c_value = document.cookie;


		var c_start = c_value.indexOf(" " + c_name + "=");
		var c_end = c_value.indexOf(";", c_start);
		c_value = c_value.substring(c_start,c_end);

		if(c_value.indexOf('base_domain') == -1){
			var date = new Date();
        	date.setTime(date.getTime()+(600*24*60*60*1000));
        	value_cookie = getCookie(c_name);
			document.cookie = c_name +"="+value_cookie+";expires=" + date.toGMTString() + ";domain="+conf['domain']+";path=/";
		}

		return c_value;
	}

