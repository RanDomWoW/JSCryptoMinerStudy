      jQuery('#banner-find-store').on('click', function(e) {
        e.preventDefault();
        jQuery(this).toggleClass('open');
        if(jQuery(this).hasClass('open')) {
          jQuery('#banner-stores-container').slideDown();
          jQuery('#banner-stores-container .list-item-areas').first().show();
          jQuery('#banner-stores-container .nav-list-item').first().addClass('hover').show();
          jQuery('#mobile-nav-stores-container').appendTo('#banner-stores-container-mobile');
          jQuery('#mobile-nav-stores-container').slideDown();
        }
        else {
          jQuery('#banner-stores-container').hide();
          jQuery('#mobile-nav-stores-container').insertAfter('#mobile-nav-stores');
          jQuery('#mobile-nav-stores-container').hide();
        }
      });

      jQuery('#banner-stores-container').hover(function(e) {
        e.preventDefault();
        jQuery('#banner-stores').addClass('hover');
        jQuery('#banner-stores-container').show();
      }, function(e) {
        jQuery('#banner-stores').removeClass('open');
        jQuery('#banner-stores-container').hide();
        jQuery('#banner-find-store').trigger('click');
      });

      jQuery('#banner-stores-container .nav-list-item').hover(function(e) {
        jQuery("#banner-stores-container .nav-list-item").removeClass('hover');
        jQuery('#banner-stores-container .list-item-areas').hide();
        var areaId = jQuery(this).data('area-id');
        jQuery("#blist-item-area-" + areaId).fadeIn();
      }, function(e) {
        /*var areaId = jQuery(this).data('area-id');
        jQuery('#banner-stores-container .list-item-areas').hide();*/
      });

      jQuery('#banner-stores-container .list-item-areas').hover(function(e) {
        var areaId = jQuery(this).data('area-id');
        jQuery("#blist-item-area-" + areaId).show();
        jQuery("#bnav-list-item-" + areaId).addClass('hover');
      }, function(e) {
        var areaId = jQuery(this).data('area-id');
        jQuery("#bnav-list-item-" + areaId).removeClass('hover');
      });
    });
  </script>

