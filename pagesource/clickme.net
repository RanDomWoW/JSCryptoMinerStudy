		    //this is for member feature : 很多頁面都需要收藏統一放在index
		    function setMemberFeature(articleIndex,save,whereFrom) {
		    	var login = false;
		        if(login) {
		        	//step 1 : 避免重複按 => 先將按鈕功能取消
			    	//頁面樣式分成兩種 內頁(detail) 、其他頁面 
			    	if(whereFrom == 'detailPage') {
			        	$('.favorite-'+articleIndex).html(
			        		"<a href='javascript:void(0);'style='color:yellow;'>\
		                        <i class='fa fa-star' aria-hidden='true' style='color: rgba(255, 196, 31, 0.9);opacity: 1;'></i>\
		                    </a>"
		                );
			    	}
			    	else {
			    		$('.favorite-'+articleIndex).html(
	                        "<a href='javascript:void(0);'>\
	                            <div class='info-star'>\
	                                <i class='fa fa-star' aria-hidden='true'></i>\
	                            </div>\
	                        </a>"
	                    );
			    	}
		            $.ajax({
		                url:"//api.clickme.net/user/addfeature?key=clickme",
		                data:{
		                    index:articleIndex,
		                    feature :save,
		                },
		                dataType:'json',
		                method:'POST',
		                success:function(result) {
		                    if(result.data == false){
		                        alert('新增錯誤:請先登入');
		                    }
		                    //修改按鈕樣式
		                    else {
		                        if(result.data['feature']){
		                            if(whereFrom == 'detailPage') {
		                                $('.favorite-'+articleIndex).html("<a href='javascript:setMemberFeature(`"+articleIndex+"`,true,`detailPage`);'style='color:yellow;'>\
		                                    <i class='fa fa-star' aria-hidden='true' style='color: rgba(255, 196, 31, 0.9);opacity: 1;'></i>\
		                                </a>");
		                            }
		                            else {
		                                $('.favorite-'+articleIndex).html(
		                                    "<a href='javascript:setMemberFeature(`"+articleIndex+"`,true,`featurePage`);'>\
		                                        <div class='info-star'>\
		                                            <i class='fa fa-star' aria-hidden='true'></i>\
		                                        </div>\
		                                    </a>"
		                                );
		                            }
		                        }
		                        else {
		                            if(whereFrom == 'detailPage') {
		                                $('.favorite-'+articleIndex).html("<a href='javascript:setMemberFeature(`"+articleIndex+"`,false,`detailPage`);'>\
		                                    <i class='fa fa-star' aria-hidden='true'></i>\
		                                </a>");
		                            }
		                            else {
		                                $('.favorite-'+articleIndex).html(
		                                    "<a href='javascript:setMemberFeature(`"+articleIndex+"`,false,`featurePage`);'>\
		                                        <div class='info-star'>\
		                                            <i class='fa fa-star-o' aria-hidden='true'></i>\
		                                        </div>\
		                                    </a>"
		                                );
		                            }
		                        }    
		                    }
		                },
		                error:function(xml){
		                    console.log(xml);
		                }
		            })
		        }
		        else {
		            loginCheck(0);
		        }
		    }
		</script>
