        function showNextTabmorgen(id, max) {
            document.getElementById("kartemorgen" + (id) + "").style.visibility = 'hidden';
            document.getElementById("kartemorgen" + (id) + "").style.display = 'none';
            $("#weather_cardsmorgen .kartemorgen" + id).hide();
            $('[id^=kartemorgen]').removeClass('card_period_visible').addClass('card_period_invisible');
            id++;
            if (id > (max - 1)) {
                id = 0;
