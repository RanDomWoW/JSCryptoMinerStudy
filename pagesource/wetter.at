<script type="text/javascript">
function getCookie( name ){
    var c = document.cookie;
    var start = c.indexOf(" " + name + "=");
    if (start == -1){
        start = c.indexOf(name + "=");
    }
    if (start == -1){
        c = null;
    }else{
        start = c.indexOf("=", start) + 1;
        var end = c.indexOf(";", start);
        if (end == -1){
            end = c.length;
        }
        c = unescape(c.substring(start,end));
    }    
    return c;
}

function delCookie( name, path, domain ){
    if(getCookie(name)) {        
        document.cookie = name + "="
                        + ( (domain) ? ";domain=" + domain : "")
                        + ";path="+path+";expires=" + new Date((new Date()).getTime() - 1 * 24 * 60 * 60 * 1000).toGMTString();
    }
}

function backToMobile(){    
    delCookie('desktop', '/', 'wetter.at');   
    delCookie('desktop', '/', 'wetter-deutschland.com');           
    //delCookie('desktop', '', '');                   
    location.reload();
}

function close_backToMobile(){
    document.getElementById('backToMobile').style.display = 'none';
}

if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {     
    if(getCookie('desktop')){        
        document.write('<div id="backToMobile" onclick="backToMobile();">zur&uuml;ck zur mobilen Version<div id="close_backToMobile" onClick="close_backToMobile();">X<\/div><\/div>');
    } 
}
</script>
