            }

            this.slides.eq(currentIndex).fadeOut(this.opts.speed);
            this.slides.eq(nextIndex).fadeIn(this.opts.speed);
            this.slideCounter.html((nextIndex + 1) + '/' + this.slides.length);
        };

        WeatherConsole.prototype.start = function () {
            var self = this;
            this.stop();
            this.intervalID = setInterval(function () {
                self.moveToSlide(/* null */);
            }, this.defaults.interval);
        };

        WeatherConsole.prototype.stop = function () {
            clearInterval(this.intervalID);
        };

        WeatherConsole.prototype.loadImages = function () {
            var self = this, counter = 0, len = $('img[data-lazy]', this.slider).length;
            $('img[data-lazy]', this.slider).each(function (index, el) {
                var image = $(this);
                image.load(function () {
                    counter++;
                    if (counter >= len) {
                        // alle Images sind geladen
                        self.running = true;
                        self.start();
                    }
                }).attr('src', $(this).attr('data-lazy')).removeAttr('data-lazy');
            });
        }

        $.fn.weatherConsole = function (opts) {
            return this.each(function () {
                new WeatherConsole(this, opts);
            });
        };

