<option value="CA">Canada</option>
</select>
    </div>
    <div class="select-wrapper">
        <span class="arrow fa fa-caret-down"></span>
        <div id="drop-languages">
            <select class="clslanguages" id="languages" name="languages"><option selected="selected" value="en">English</option>
</select>
        </div>
