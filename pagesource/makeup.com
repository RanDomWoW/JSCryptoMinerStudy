<script src="/Assets/Makeup/Scripts/Source/Vendor/jquery.cookie.min.js"></script>
<script type="text/javascript">
    $(document)
           .ready(function () {
               $("#countries").change(setLanguageForSelectedCountry);
              // $("select[name='languages']").change(handleChange);
           });
    $(document).on('change', '.clslanguages', function () { handleChange($(this)); });


    var countriesJsonString = '[{"ItemId":"2EED46EF79F647729D17E40AEFEC1345","Name":"United States of America","CountryCode":"US","Languages":[{"ItemId":"AF58419145C9420187405409F4CF8BDD","CountryItemId":null,"Name":"English","IsoCode":"en","RegionalIsoCode":"en"}]},{"ItemId":"849003C9BAD6419D827DE7B0411059E6","Name":"Canada","CountryCode":"CA","Languages":[{"ItemId":"EB363323985142EDA2DFD402C71ED098","CountryItemId":null,"Name":"en-CA","IsoCode":"en","RegionalIsoCode":"en-ca"},{"ItemId":"70944BBB330F42A88A7FD796D4958AB7","CountryItemId":null,"Name":"fr-CA","IsoCode":"fr","RegionalIsoCode":"fr-ca"}]}]';

    var countries = JSON.parse(countriesJsonString);

    var getSelectedCountry = function (countryCode) {
        var country;
        var countriesLength = Object.keys(countries).length;
        for (var i = 0; i < countriesLength; i++) {
            country = countries[i];
            if (country.CountryCode === countryCode) break;
        }
        return country;
    };

    var defaultEnglishCode = 'en';
    var usLangCodeQueryParam = 'sc_lang=en';
    var languageCookieName = 'makeup#lang';

    // Modify the language drop down based on selected country
    var setLanguageForSelectedCountry = function () {
        var selectedCountryCode = $('#countries').find(':selected').val();
        var country = getSelectedCountry(selectedCountryCode);
        var $languageDropDown = $('.clslanguages');
        $languageDropDown.empty();
        $.each(country.Languages, function (val, languageObj) {
            var languageCode = languageObj.RegionalIsoCode;
            var langName = languageObj.Name;
            $languageDropDown.append(
                $('<option></option>').val(languageCode).html(langName)
            );
        });

        // For URL RESOLVING
        handleChange($languageDropDown);
    };

    var handleChange = function (element) {
        var selectedLanguageCode = $(element).find(':selected').val();
        
        if (selectedLanguageCode === '' || selectedLanguageCode.length <= 0) {
            console.log('The selected Language Regional ISO Value is empty');
            return;
        }
        Cookies.set(languageCookieName, selectedLanguageCode, { path: '/' });
        var languageSegment = extractLanguageFromUrl();
        if (languageSegment === null) {

            if (selectedLanguageCode === defaultEnglishCode) {
                // Algorithm Case 3
                var cookieVal = $.cookie(languageCookieName);
                if (cookieVal !== selectedLanguageCode) {
                    resolveUrl(selectedLanguageCode);
                } else {
                    return;
                }
            }
            else {
                // Algorithm Case 5
                resolveUrl(selectedLanguageCode);
            }
        } else {

            if (languageSegment === selectedLanguageCode) {
                return;
            }
            if (languageSegment !== selectedLanguageCode) {
                // Algorithm Case 6
                resolveUrl(selectedLanguageCode);
            }
        }
        // Algorithm Case 4
        return;
    };

    // Extracting language code from the URL
    var extractLanguageFromUrl = function () {
        var hostName = document.location.hostname;
        var url = document.location.href;
        var hostNameIndex = url.indexOf(hostName);
        var languageSegment = url.substring(hostNameIndex).split('/')[1];
        if (languageSegment.length === 5 && languageSegment.indexOf('-') >= 0) {
            // url contains the language code
            return languageSegment;
        }
        if (languageSegment.length === 2) {
            return languageSegment;
        }
        return null;
    };

    // Update URL with the Selected language code
    var resolveUrl = function (languageCode) {
        var hostName = document.location.hostname;
        var url = document.location.href;
        var urlLanguageCode = extractLanguageFromUrl();
        url = stripLLanguageCode(urlLanguageCode, url);
        url = stripQueryParam(url);
        if (languageCode === defaultEnglishCode) {
            //in this case we dont have to add this to the url. Rather just send the values to the sitecore using query string params.
            //url += url.indexOf('?') === -1 ? '?' : '&';
            //url = url + usLangCodeQueryParam;  NOT DOING THIS ANYMORE, INSTEAD UPDATING THE COOKIE VALUE
            var cookieValue = Cookies.get(languageCookieName);
            if (cookieValue !== undefined && cookieValue !== defaultEnglishCode) {
                Cookies.set(languageCookieName, defaultEnglishCode, {path: '/' });
            }
        }
        else {
            var index = url.indexOf(languageCode);
            if (index === -1) {
                languageCode = '/' + languageCode;
                var hostNameIndex = url.indexOf(hostName);
                var indexOfFirstSlashAfterHost = hostNameIndex + url.substring(hostNameIndex).indexOf('/');
                url = url.slice(0, indexOfFirstSlashAfterHost) + languageCode + url.slice(indexOfFirstSlashAfterHost);
            }
        }
        document.location.href = url;
    };

    var stripLLanguageCode = function (languageCode, url) {
        if (url.indexOf(languageCode + '/') !== -1) {
            return url.replace(languageCode + '/', '');
        }
        if (url.indexOf(languageCode)) {
            return url.replace(languageCode, '');
        }
        return url;
    }

    var stripQueryParam = function (url) {
        if (url.indexOf('?' + usLangCodeQueryParam) !== -1) {
            return url.replace('?' + usLangCodeQueryParam, '');
        }
        if (url.indexOf('&' + usLangCodeQueryParam) !== -1) {
            return url.replace('&' + usLangCodeQueryParam, '');
        }
        return url;
    }
    //Algo:On DropDown Changes
    // 1: Only Manipulate URL based on the drop down selection
    // 2: When Country Dropdown is changed : grab the selected langugage Regional ISO CODE
    //                                  OR
    // 2: When Language dropdown is changed grab the selected language Regional ISO CODE
    // 3: After grabbing the value Resolve URL

    // Algo for Resolve URL:
    // 1: Extract Language from the current URL.
    // 2: Compare the language with the selected language Regional ISO CODE
    // 3: if the selected langugae is en and there is no language code in the url then check the cookie if cookie value is same as selected language do nothing.
        // else resolve the url to the selected language.
    // 4: If selected language is not en but it is there inside the url then do nothing.
    // 5: If selected language is not en and is not there in the url then add the language to the url
    // 6: If selected language is not en and is not equal to what is in the url then also replace the language in url with the selected one.

</script>
