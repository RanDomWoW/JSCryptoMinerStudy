<script language="javascript">

    function newsletterValidation() {
        var result = true;

        var isValidEmail = /^[\w\-\.\+]+\@[a-zA-Z0-9\.\-]+\.[a-zA-z0-9]{2,4}$/.test($('#tbNewsletterEmailAddress').val());
        if (!isValidEmail) {
            $('#spNewsletterEmailAddress').show();
            $('#tbNewsletterEmailAddress').addClass('error-msg');
            result = false;
        }
        else {
            $('#spNewsletterEmailAddress').hide();
            $('#tbNewsletterEmailAddress').removeClass('error-msg');
        }
        return result;
    }
    /*Newsletter Button Click Event*/
    var newletterPlaceholder = $('#tbNewsletterEmailAddress').attr('placeholder');

    $('#tbNewsletterEmailAddress').focusin(function () {
        $(this).removeClass('error-msg');
        $('#tbNewsletterEmailAddress').attr('placeholder', newletterPlaceholder);
    })
    $('#newsletterbutton').on('click', function () {
        var email = $('#tbNewsletterEmailAddress').val();
        var language = $('#tbNewsletterLanguage').val();
        var signupModel = {};
        signupModel.Emailaddress = email;
        signupModel.Language = language;
        if (!newsletterValidation(email)) {
            $('.validationmsg ').hide();
            $('#tbNewsletterEmailAddress').val('');
            $('#tbNewsletterEmailAddress').addClass('error-msg');
            $('#tbNewsletterEmailAddress').attr('placeholder', 'ENTER VALID EMAIL ADDRESS');
            return false;
        } else {
            $.ajax({
                type: "POST",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(signupModel),
                url: MDCAppCtx.getHttpsBaseURL() + "api/NewsletterSignupApi/NewsletterSignup",
                dataType: "json"
            }).done(function (data) {
                if (data == "success") {
                    $("#newsletter-signup-form").css("display", "none");
                    $("#newsletter-form-submit-success").css("display", "block");
                    //GTM Starts
                    GTMTagging.GTMVar.emailId = email;
                    GTMTagging.GTMVar.location = 'footer';
                    GTMTagging.newsletterSubscriptionEventTag();
                    //GTM Ends
                } else if (data == "already") {
                    $('.validationmsg ').hide();
                    $('#tbNewsletterEmailAddress').val('');
                    $('#tbNewsletterEmailAddress').addClass('error-msg');
                    $('#tbNewsletterEmailAddress').attr('placeholder', 'This email is already signed up!');
                    return false;
                } else {
                    $("#newsletter-signup-form").css("display", "none");
                    $("#newsletter-form-submit-failed").css("display", "block");                    
                    return false;
                }
            });
        }
    });
</script>


        

