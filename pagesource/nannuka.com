 $(function(){

    
        var catcache = {},
            lastXhr;
            var companyCatCache = {};




        if( $('#searchs').length ){
            $( "#searchs" ).autocomplete({
                minLength: 3,
                source: function( request, response ) {
                    var term = request.term;
                    if ( term in catcache && jQuery('#whattosearch').val() === "0" ) {
                        response( $.map( catcache[term], function( item ) {
                            return {
                               label: item.label ,
                                    url: item.url ,
                                   // id: question.id,
                                   // pillar: item.pillar,
                                  //  fullname: item.fullname,
                                  questionid: item.questionid
                            };
                        }));
                        return;
                    }

                    lastXhr = $.post( "https://www.nannuka.com/en/ajax/searchFaqs", request, function( data, status, xhr ) {
                        if(data != undefined && !data.length) {
                          // $('.sug').hide();
                           $('.quests').html('');

                        }
                        //console.log(data);
                        catcache[ term ] = data ;
                        if ( xhr === lastXhr ) {
                            response( $.map( data, function( item ) {
                                return {
                                    label: item.label ,
                                    url: item.url ,
                                   // id: question.id,
                                   // pillar: item.pillar,
                                  //  fullname: item.fullname,
                                    questionid: item.questionid
                                };
                            }));
                        }
                    }, 'json');

                },
                select: function(event, ui) {
                    $( "#searchs" ).val(ui.item.value);
                    $( "#searchs").prop("readonly",false);
                 //   $( "#category").val(ui.item.categoryid);
                 //   $( "#pillar").val(ui.item.pillar);
                }
            }).data( "ui-autocomplete" )._renderItem = function( ul, item ) {
                if(!$("#faqarticles .quests #"+item.questionid+"").length) {
                   // $('.quests').html('');
                    $('.sug').hide();
                    return $( "<li id='"+item.questionid+"' style='margin-bottom: -16px;width: 58%;font-size:18px' >" )
                        .append( "<a href='https://www.nannuka.com/en/faqs/"+item.url+"'>" + item.label + '<img class="arrows" style="position:relative;left: 10px;width: 6px;" src="https://www.nannuka.com/themes/main/img/redesign/arrow_right.png">' +"<br><br></a></li>" )
                        .appendTo( '#faqarticles .quests' );
                }
            };

        }
    $( "#faqmodal" ).on('shown.bs.modal', function (e) {
        $(' #mainheader #headerLoginForm, #mainheader #mainMenu').hide();
