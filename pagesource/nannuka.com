            if (data === "1"){
                validEmail = false;
                $("#emailExists").css("display", "block");
                $("#email").addClass('inputInRed');
            }
            else {
                validEmail = true;
                $("#emailExists").css("display", "none");
                $("#email").removeClass('inputInRed');
            }
        }
    });
}





function addEmailValidation()
{
    jQuery("#lightboxregisterform").submit(function(){
        if (validEmail === false){
            return false;
        }
    });
    jQuery("#lightboxemail").change(function(){
        checkemail();
    });
}





