*/
    var imgPos = 170; // mainImgのtopの値(CSS)



    console.log( "wH" + wH );
    console.log( "dH" + dH );
    console.log( "wW" + wW );
    console.log( "imgH" + imgH );
    console.log( "imgPos" + imgPos );

  $(window).on("scroll", function() {
    if (wW > 1023) {
      // 画像がウィンドウ高さより大きい場合のみ

      if (imgH + imgPos > wH) {

        var sT = $(document).scrollTop();


        console.log( "sT" + sT );


        var scrollPercent = sT / ( dH - wH );
        var translate = Math.round(( imgH - wH + imgPos ) * scrollPercent);


        console.log( "scrollPercent" + scrollPercent );
        console.log( "translate" + translate );


        mainImg.css({
          'transform': 'translate3d(0,' + -translate + 'px,0)'
        });

      } else {

        mainImg.css({
          'transform:': 'none'
        });

      }
    }
    });
  });
