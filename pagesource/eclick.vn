<script type="text/javascript">
    $(document).ready(function () {
        var form_contact = $("#form-contact");
        $(".sent_contact").on("click", function () {
            form_contact.submit();
        });
        form_contact.validate({
            rules: {
                fullname: {
                    required: true
                },
                email: {
                    required: true,
                    email: true
                },
                phone: {
                    required: true,
                    number: true,
                    minlength: 10,
                    maxlength: 15
                },
                content: {
                    required: true
                }
            },
            messages: {
                fullname: {
                    required: "Your name/surname is required"
                },
                email: {
                    required: "Your email is required",
                    email: "Invalid email"
                },
                phone: {
                    required: "Your phone number is required",
                    number: "Please enter a valid number",
                    minlength: "Please enter a valid number (min 10 characters)",
                    maxlength: "Please enter a valid number (max 15 characters)"

                },
                content: {
                    required: "The content must not be blank"
                }
            },
            submitHandler: function () {
                var $serialize = form_contact.serializeArray();
                $serialize = jQuery.param($serialize);
                $.ajax({
                    url: form_contact.attr('action'),
                    type: 'POST',
                    data: $serialize,
                    success: function (resp) {
                        $('#form-contact')[0].reset();
                        $('#bg_opacity, #box_pop').fadeIn().delay(3000).fadeOut('slow');
                        $('#box_pop_body').html("Thank you for your interest, we will contact you soon.");

                    }
                });
            }
        });
    });

</script>
