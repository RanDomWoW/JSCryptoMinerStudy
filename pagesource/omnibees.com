        // *only* if we have anchor on the url
        if(window.location.hash) {
            // smooth scroll to the anchor id
            var offsetHeight = document.getElementById('altura-header').offsetHeight;
            $('html, body').animate({
                scrollTop: $(window.location.hash).offset().top - offsetHeight
            }, 'slow', 'linear');
