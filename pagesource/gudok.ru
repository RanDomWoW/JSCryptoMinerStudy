			$slick.on('beforeChange', function(e, slick, currentSlide, nextSlide) {
				//console.log($('.p-photoInner .thumbs > *').get(0));
				$('.p-photoInner .thumbs > *').eq(currentSlide).removeClass('isActive');
				$('.p-photoInner .thumbs > *').eq(nextSlide).addClass('isActive');
			});

			$('.p-photoInner .thumbs > *').click(function(){
				var $this = $(this);
				$slick.slick('slickGoTo', $this.index());
			});
		}

		// детальная страница события
		if($('#eventNavigation a').size()){
			$('.eventNavigation-tab').first().show();
			$('#eventNavigation a.nav-tab').on('click', function(){
				$('.eventNavigation-tab').hide();
				var id = $(this).prop('id') + '_text';
				$('.eventNavigation-tab#'+id).show();
				return false;
			});
		}
		// Подгрузка новых записей аяксом
		$(function(){
		    //путь к файлу с компонентом. Указываем параметр
		    if ( window.location.search ){
		    	var path = window.location + "&ajax=Y";
	    	} else {
	    		var path = window.location.pathname + "?ajax=Y";
	    	}

		    //счетчик страниц
		    var currentPage = 1;

		    var count = $(".items-total-counter").data("total-count");
		    var news_show = $(".items-show-counter").data("show-count");

		    if( currentPage * news_show < count){
			    $(".load-more").show();
			}
			//console.log(currentPage, news_show, count);
		    $(".load-more").click(function(e){
				var thi = this;
				//отключим скролл к верху документа
				e.preventDefault();
		        //делаем ajax запрос и сразу инкремент номера страницы

				//console.log(path);
		        $.ajax({
		        	url: path,
		        	type: 'POST',
		        	data: {PAGEN_1: ++currentPage},
					beforeSend: function()
					{
				        $('.ajaxSpinner_bottom').show();
				    },
				    complete: function()
				    {
				    },
				    success: function(data)
				    {
				        $('.ajaxSpinner_bottom').hide();
						var a = $('.load-more').parents('.pagi');
						if ( a.length == 0 ) {
							$(".items-list").append(data);
						}else {
							$(thi).parents('.pagi').siblings('.publication').find(".items-list").children('.row').append(data);
						}

        			    if(currentPage * news_show >= count){
						    $(".load-more").hide();
						}
				    }
		        });

		    });
		});

		$(".vote-form").submit(function(e) {
		
			var url = "/opros/?ajax=Y";

			$.ajax({
				   type: "POST",
				   url: url,
				   data: $(".vote-form").serialize(),
					xhrFields: {
						withCredentials: true
					},
				   success: function(data)
				   {
					   $(".vote-form").html(data);
				   }
				 });
			e.preventDefault(); // avoid to execute the actual submit of the form.
		});

		$(function(){
			//загрузка баннеров
			var banner_count = 7;
			function getCookie(name) {
				var matches = document.cookie.match(new RegExp(
					"(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
				));
				return matches ? decodeURIComponent(matches[1]) : undefined;
			}
			if ( $('div').is('.banner') ) {
				$.each([ 1, 2 ], function( index, value ) {
					$.ajax({
						url: '/banner/?ajax=Y',
						type: 'POST',
						data: {
							B_NUM: value
						},
						beforeSend: function () {
							$('.ajaxSpinner_banners').show();
						},
						complete: function () {
						},
						success: function (data) {
							$(".bannerss" + value + ' .ajaxSpinner_banners').hide();
							$(".bannerss" + value).append(data);
						}
					});
				});
			}
			$.each([ 0, 1, 2, 3, 4, 5, 7], function( index, value ) {
				$.ajax({
					url: '/banners/?ajax=Y',
					type: 'POST',
					data: {
						B_NUM: value
					},
					beforeSend: function () {
						$('.ajaxSpinner_banners').show();
					},
					complete: function () {
					},
					success: function (data) {
						$(".banner" + value + ' .ajaxSpinner_banners').hide();
						$(".banner" + value).append(data);
					}
				});
			});
			// полноэкранный баннер #6
			if ( getCookie("B_SHOWN") !== "true" ){
				$.ajax({
					url: '/banners/?ajax=Y',
					type: 'POST',
					data: {
						B_NUM: 6
					},
					success: function (data) {
						if ( data.length > 10 ) {
							$('#b_fullscreen').modal('show');
							var counterVal = 10;
							var counterInt = setInterval(function () {
								if (counterVal == 0) {
									clearInterval(counterInt);
									$('#b_fullscreen').modal('hide');
								} else {
									$('#b_fullscreen .b_counter_value').text(counterVal);
									counterVal--;
								}
							}, 1000);
							$(".banner6").append(data);
						}
					}
				});
			}
		});
	});
</script>
<!-- photo gallery -->
<script>
		var sync1 =jQuery(".gallery-top");
		var sync2 = $(".gallery-thumbs");
		var flag = false;
		var duration = 300;

		var galleryInit = function(sync1, sync2){
			sync1
				.owlCarousel({
					nav: true,
					items: 1,
					URLhashListener: true,
					startPosition: 'URLHash',
					navText: ['<i class="glyphicon glyphicon-chevron-left"></i>', '<i class="glyphicon glyphicon-chevron-right"></i>']
				})
				.on('changed.owl.carousel', function (e) {
					if (!flag) {
						flag = true;
						sync2.trigger('to.owl.carousel', [e.item.index, duration, true]);
						flag = false;
					}
					if (e.namespace && e.property.name === 'position'
						&& e.relatedTarget.relative(e.property.value) === e.relatedTarget.items().length - 1) {
						$(".owl-restart, .owl-related").addClass('active');
					} else {
						$(".owl-restart, .owl-related").removeClass('active');
					}
				}).on('translate.owl.carousel', function(e){
					idx = e.item.index;
					$('.gallery-thumbs .owl-item').eq(idx-1).removeClass('current');
					$('.gallery-thumbs .owl-item').eq(idx+1).removeClass('current');
					$('.gallery-thumbs .owl-item').eq(idx).addClass('current');
				});

//		var html = $('.photo-related').html();
//		sync1.trigger('add.owl.carousel', [html, 99])
//			.trigger('refresh.owl.carousel');

			sync2
				.owlCarousel({
					items:5,
					nav:false,
					URLhashListener: true,
					startPosition: 'URLHash',
					responsiveClass:true,
					responsive : {
						0 : {
							items:3
						},
						480 : {
							items:4
						},
						768 : {
							items:5
						},
						992 : {
							items:5
						},
						1199 : {
							items: 5
						}
					}
				})
				.on('click', '.owl-item', function () {
					sync1.trigger('to.owl.carousel', [$(this).index(), duration, true]);

				})
				.on('changed.owl.carousel', function (e) {
					if (!flag) {
						flag = true;
						sync1.trigger('to.owl.carousel', [e.item.index, duration, true]);
						flag = false;
					}
				});

			jQuery(document.documentElement).keyup(function (event) {
				if (event.keyCode == 37) {
					// go left
					sync1.trigger('prev.owl.carousel');
				} else if (event.keyCode == 39) {
					// go right
					sync1.trigger('next.owl.carousel');
				}
			});
		}
		galleryInit(sync1, sync2);

		$(".gallery-preview_related").append($(".photo-related").html());

		$(document).on('click', '.gallery-top .owl-stage-outer', function(){
			$(".gallery-wrapper").addClass("gallery-preview");
			sync1.trigger('refresh.owl.carousel');
			sync2.trigger('refresh.owl.carousel');
			$(".gallery-preview_bg").show();
		});

		$(document).on('click', '.owl-close, .gallery-preview_bg', function(){
			$(".gallery-wrapper").removeClass("gallery-preview");
			sync1.trigger('refresh.owl.carousel');
			sync2.trigger('refresh.owl.carousel');
			$(".gallery-preview_bg").hide();
			$('.gallery-preview_related').removeClass('active');
			$('.gallery-preview_element').show();
		});

		$(document).on('click', '.owl-restart', function(){
			$('.gallery-preview_element').show();
			$('.gallery-preview_related').removeClass('active');
			sync1.trigger('to.owl.carousel', 0);
			sync2.trigger('to.owl.carousel', 0);
		});
		$(document).on('click', '.owl-related', function(){
			$('.gallery-preview_element').hide();
			$('.gallery-preview_related').addClass('active');
		});
</script>


