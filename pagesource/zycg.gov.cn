<script >
  function on_mouse(on_tag,on_con)   //onload 事件句柄，文档装载结束时调用

  {
    var tag=document.getElementById(on_tag).children; //获取Tag下的li，即Tag标签
    var content=document.getElementById(on_con).children; //获取Tag标签对应的内容
    content[0].style.display = "block"; //默认显示第一个标签的内容
    var len= tag.length;
    for(var i=0; i<len; i++)   //无论点击谁都能实现当前显示，其余隐藏
      {
      tag[i].index=i; //为何如此？（看下面解释）
      tag[i].onmouseover = function()     //0级DOM的事件句柄注册

          {

                 for(var n=0; n<len; n++)

                 {
                    tag[n].className="";
                    content[n].style.display="none";
                  }   //首先将全部的div隐藏
              tag[this.index].className = "current_over";
              content[this.index].style.display = "block";
        　　}
     　}

  }


  $(function(){

    var sliderElement = $('#fsD1');
    var slidernav = sliderElement.find('.prev,.next');
    slidernav.hide();
    sliderElement.hover(function() {
        slidernav.show()
    },function() {
        slidernav.hide()
    });

    //网上竞价倒计时
    count_down();

    $('#ysd_no').focus(function(){
      if($('#ysd_no').val() == '请输入验收单编号或合同号') $('#ysd_no').val('');
    });
    $('#ysd_no').blur(function(){
      if($('#ysd_no').val() == '') $('#ysd_no').val('请输入验收单编号或合同号');
    });
    $('#ysd_money').focus(function(){
      if($('#ysd_money').val() == '请输入总金额') $('#ysd_money').val('');
    });
    $('#ysd_money').blur(function(){
      if($('#ysd_money').val() == '') $('#ysd_money').val('请输入总金额');
    });
    $('#item_code').focus(function(){
      if($('#item_code').val() == '请输入项目编号') $('#item_code').val('');
    });
    $('#item_code').blur(function(){
      if($('#item_code').val() == '') $('#item_code').val('请输入项目编号');
    });
    $('#dep_name').focus(function(){
      if($('#dep_name').val() == '请输入单位名称') $('#dep_name').val('');
    });
    $('#dep_name').blur(function(){
      if($('#dep_name').val() == '') $('#dep_name').val('请输入单位名称');
    });
  })

  function submit_query_ysd(){
    if($("#ysd_no").val() == "请输入验收单编号或合同号" || $("#ysd_no").val() == ""){
      alert("请先输入验收单编号或合同号");
      return false
    }
    if($("#ysd_money").val()=="请输入总金额" || $("#ysd_money").val() == ""){
      alert("请输入总金额");
      return false
    }
    $("#check_ysd").submit();
  }

  //网上竞价倒计时
  function count_down(){
    $('.project_time').each(function(index, item){
      var tmp_item = $(item)
      var before_begin_tmp = parseInt($(item).children('.before_begin').text());
      var before_end_tmp = parseInt($(item).children('.before_end').text());
      item_count_down(tmp_item,before_begin_tmp,before_end_tmp)
    });
  }
    function item_count_down(tmp_item,before_begin,before_end){
    if(before_end > 0){
      before_end = before_end - 1;
      tmp_item.children('.time_str').html("<span style=''>倒计时："  +"<span style='color:#fff;background:red;padding:0px 2px;font-weight:600;font-size:14px;'>"+get_time_str(before_end) +"</span>"+ "</span>");
      setTimeout(function(){item_count_down(tmp_item,before_begin,before_end)},1000);
    }
  }

  // 把时间转换成时分秒格式
  function get_time_str(time_value){
    var seconds = Math.floor(time_value % 60);
    var minutes = Math.floor((time_value / 60) % 60);
    var hours = Math.floor((time_value / 3600) % 24);
    var days = Math.floor((time_value / 3600)/ 24);
    var hour = hours + days*24
    return [hour,':',minutes,':',seconds,''].join('');
  }
</script>
