<html>
    <script type="text/javascript">
        function isIE() {
            var myNav = navigator.userAgent.toLowerCase();
            return (myNav.indexOf('msie') != -1) ? parseInt(myNav.split('msie')[1]) : false;
        }
        function redirect(){
            //alert(isIE());
            if (isIE()) {
                if (isIE() <= 6) {
                    //alert('1');
                    window.location.href= "http://www.bb.th.com";
                } else {
                    //alert('2');
                    window.location.href="http://www.bb.go.th/bbweb";
                }
            } else {
                //alert('3');
                //window.location.href ="http://www.bb.go.th/budget/index.html";
                window.location.href ="http://www.bb.go.th/bbweb";

            }
        }
        
    </script>
<body onload="javascript:redirect();">

</body>
</html>