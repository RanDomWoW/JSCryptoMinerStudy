<script>
    $(function () {
        NAATI.verifyPractitionerCredentials(verifyContainer).initialise();
    });

    function feedback(result) {

        if (result.success) {
            NAATI.verifyPractitionerCredentials.instance.search();
        } else {
            var error = "Error";

            if (result.error) {
                error = result.error;
            }

            $("#recaptcha_reload").trigger("click");
            NAATI.verifyPractitionerCredentials.instance.showError(error);
        }
    }
</script>

<script id="pratictionerTemplate" type="text/x-jquery-tmpl">
    <h4>Search Results</h4>
    <table class="table table-striped">
        <tr>
            <td>
                Practitioner ID
            </td>
            <td>
                ${practitionerNumber}
            </td>
        </tr>
        <tr>
            <td>
                Name
            </td>
            <td>
                {{if deceased }}
                <strong>(This person is deceased)</strong>
                {{else}}
                ${surname}, ${givenName}
                {{/if}}
            </td>
        </tr>
        <tr>
            <td>
                Current Certifications
            </td>
            <td>
                {{if skills}}
                {{html skills}}
                {{else}}
                No current certifications
                {{/if}}
            </td>
        </tr>
        <tr>
            <td>
                Previous Certifications
            </td>
            <td>
                {{if expiredCredentialSkills}}
                {{html expiredCredentialSkills}}
                {{else}}
                Not applicable
                {{/if}}
            </td>
        </tr>
        {{if isRevalidationScheme}}
        <tr>
            <td>
                Revalidation Scheme
            </td>
            <td>
                {{if isRevalidationScheme}}
                Yes
                {{else}}
                No
                {{/if}}
            </td>
        </tr>
        {{/if}}
        {{if address && address.country}}
        <tr>
            <td>
                Country
            </td>
            <td>
                ${address.country}
            </td>
        </tr>
        {{/if}}
        {{if showPhotoOnline && photo}}
        <tr>
            <td>
                Photo
            </td>
            <td>
                <img src=${photo} height="177">
            </td>
        </tr>
        {{/if}}
    </table>
</script>

<script id="accreditationTemplate" type="text/x-jquery-tmpl">
    <h4>Search Results</h4>
    <table class="table table-striped">
        <tr>
            <td>
                NAATI Number
            </td>
            <td>
                ${id}
            </td>
        </tr>
        <tr>
            <td>
                Name
            </td>
            <td>
                {{if deceased }}
                <strong>(This person is deceased)</strong>
                {{else}}
                ${surname}, ${givenName}
                {{/if}}
            </td>
        </tr>
        <tr>
            <td>
                Current Accreditations
            </td>
            <td>
                {{if skills}}
                {{html skills}}
                {{else}}
                No current accreditations
                {{/if}}
            </td>
        </tr>
        <tr>
            <td>
                Previous Accreditations
            </td>
            <td>
                {{if expiredCredentialSkills}}
                {{html expiredCredentialSkills}}
                {{else}}
                Not applicable
                {{/if}}
            </td>
        </tr>
        {{if isRevalidationScheme}}
        <tr>
            <td>
                Revalidation Scheme
            </td>
            <td>
                {{if isRevalidationScheme}}
                Yes
                {{else}}
                No
                {{/if}}
            </td>
        </tr>
        {{/if}}
        {{if address && address.state}}
        <tr>
            <td>
                State
            </td>
            <td>
                ${address.state}
            </td>
        </tr>
        {{/if}}
        <tr>
            <td>
                Note
            </td>
            <td>
                "Accreditations not displaying a &quot;Valid to&quot; date are not part of the Revalidation Scheme."
            </td>
        </tr>
    </table>
</script>;
