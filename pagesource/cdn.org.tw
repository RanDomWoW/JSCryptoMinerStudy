    <script type="text/javascript">
        //依當前時間顯示特別推薦
        $(document).ready(function showSpRecom() {
            $("#morning").hide();
            $("#noon").hide();
            $("#night").hide();


            SpRecom();

            var now = new Date();
            if (now.getHours() >= 7 && now.getHours() <= 14) {
                $("#morning").show();
            }
            else if (now.getHours() >= 15 && now.getHours() <= 21) {
                $("#noon").show();
            }
            else if (now.getHours() >= 22 || now.getHours() <= 6) {
                $("#night").show();
            }
        });

        function SpRecom() {
            //早安三明治
            $.ajax({
                type: 'post',
                url: 'ASHX/GetSpRecom.ashx',
                data: {
                    type: '0700',
                    ti: new Date().getTime()
                },
                success: function(data) {
                    $('#morning')[0].innerHTML = data;
                },
                error: function(xhr, ajaxOptions, thrownError) {
                    //alert(xhr.responseText);
                }
            });

            //晚安維他命
            $.ajax({
                type: 'post',
                url: 'ASHX/GetSpRecom.ashx',
                data: {
                    type: '2200',
                    ti: new Date().getTime()
                },
                success: function(data) {
                    $('#night')[0].innerHTML = data;
                },
                error: function(xhr, ajaxOptions, thrownError) {
                    //alert(xhr.responseText);
                }
            });

            //跟上帝喝杯茶
            $.ajax({
                type: 'post',
                url: 'ASHX/GetSpRecom.ashx',
                data: {
                    type: '1500',
                    ti: new Date().getTime()
                },
                success: function(data) {
                    $('#noon')[0].innerHTML = data;
                },
                error: function(xhr, ajaxOptions, thrownError) {
                    //alert(xhr.responseText);
                }
            });


        }

        function showMorning() {
            $("#morning").show();

            //其他功能隱藏
            $("#noon").hide();
            $("#night").hide();
        }

        function showNoon() {
            $("#noon").show();

            //其他功能隱藏
            $("#morning").hide();
            $("#night").hide();
        }

        function showNight() {
            $("#night").show();

            //其他功能隱藏
            $("#morning").hide();
            $("#noon").hide();
        }
    </script>

    
