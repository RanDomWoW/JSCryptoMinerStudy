        //每日金句左右切換動畫
        $(document).ready(function() {
            var block_w = 200;
            var speed = 700;

            dailyBible();

            //一開始先移動到今日(速度為0)
            $("#dailyBibleMain").animate({
                scrollLeft: $("#dailyBibleMain").scrollLeft() + block_w
            }, 0, function() { });

            //右移
            $("#dailyBibleRight").click(function() {
                $("#dailyBibleMain").animate({
                    scrollLeft: $("#dailyBibleMain").scrollLeft() + block_w
                }, speed, function() { });
            });

            //左移
            $("#dailyBibleLeft").click(function() {
                $("#dailyBibleMain").animate({
                    scrollLeft: $("#dailyBibleMain").scrollLeft() - block_w
                }, speed, function() { });
            });

        });



        function dailyBible() {
            //昨天
            $.ajax({
