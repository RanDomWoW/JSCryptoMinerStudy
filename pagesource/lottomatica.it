	    /** Redirect if browser is not supported. **/
	    
		    var chromeMinVer = 0;
		    var ffMinVer = 0;
		    var safariMinVer = 0;
		    var ieMinVer = 10;
		    var operaMinVer = 0;
		
		
		    //return [userAgent version]
		    var browser = (function(){
		        var ua= navigator.userAgent, tem, 
		        M= ua.match(/(opera|chrome|safari|firefox|msie|trident(?=\/))\/?\s*(\d+)/i) || [];
		        if(/trident/i.test(M[1])){
		            tem=  /\brv[ :]+(\d+)/g.exec(ua) || [];
		            return 'IE '+(tem[1] || '');
		        }
		        if(M[1]=== 'Chrome'){
		            tem= ua.match(/\bOPR\/(\d+)/);
		            if(tem!= null) return 'Opera '+tem[1];
		        }
		        M= M[2]? [M[1], M[2]]: [navigator.appName, navigator.appVersion, '-?'];
		        if((tem= ua.match(/version\/(\d+)/i))!= null) M.splice(1, 1, tem[1]);
		        return M;
		    })();
		
		
		    var ua = browser[0];
		    var ver = Number(browser[1]);
		    var redirectPath = "/aggiorna-browser"; //retrieve from homepage configuration
			var currentPage = "/";
			
			if(redirectPath && redirectPath != "" && currentPage != redirectPath){//avoid redirect if currentPage is the updateBrowser page
		    	
		    	if((ua === 'Chrome' && ver<chromeMinVer) || ((ua === 'MSIE' || ua === 'IE') && ver<ieMinVer) || (ua === 'Firefox' && ver<ffMinVer) || (ua === 'Safari'&& ver<safariMinVer) ||(ua === 'Opera' && ver<operaMinVer))
		    		window.location.href=redirectPath;
		
		    }
	    /** !Redirect if browser is not supported. **/
		    
	    </script>
    
    
    
