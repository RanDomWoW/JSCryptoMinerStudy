                                <img src="" ng-if="!game.isPokerGame && !game.isLotteryGame" ng-src="{{game.picture}}" alt="" -->
                                <img ng-if="game.isBetterGame" ng-src="{{loadImgGame(game)}}" alt="{{game.pictureAlt}}">
                                <img ng-if="!game.isBetterGame" ng-src="{{loadImgGame(game)}}" alt="{{game.pictureAlt}}">
                                <p class="jackpot-label" ng-if="jackpotFactory.isJackpotLoaded && (game.jackpotId | jackpotAmounts)">
                                    <span class="title">Jackpot :</span><span class="number">{{game.jackpotId | jackpotAmounts }} &euro;</span>
                                </p>
                            </div>
                                <!-- {{ ( (isTabletMobileDevice && (game.isTabletAvailable=='true' || game.androidAvailable=='true' || game.appleAvailable=='true' || game.type=='casinolive' )) || (!isTabletMobileDevice && (game.isBetterGame == 'true' || game.isPokerGame == 'true' || game.isBingoGame == 'true' || game.isSkillGame == 'true' || game.isLotteryGame == 'true' || game.isFantasysportGame == 'true' || (game.isCasinoGame == 'true' && game.isTabletAvailable !='true')))) ? '' : 'notablet'}} -->
                            <div
                                class="roll {{
                                (
                                    (isTabletMobileDevice &&
                                        (
                                            game.isTabletAvailable === true ||
                                            game.isTabletAvailable === 'true' ||
                                            game.androidAvailable === true ||
                                            game.androidAvailable === 'true' ||
                                            game.appleAvailable === true ||
                                            game.appleAvailable === 'true' ||
                                            game.type === 'casinolive'
                                        )
                                    )
