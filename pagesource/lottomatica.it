                                                <div
                                                    class="col-sm-4 col-md-3 col-lg-2"
                                                    ng-repeat="filter in gameFilters"
                                                    ng-class="{'col-lg-offset-{{((12 - (gameFilters.length * 2)) / 2) + 1}}': (gameFilters.length < 6 && $index == 0), 'col-md-offset-{{((12 - (gameFilters.length * 3)) / 2) + 1}}': (gameFilters.length < 4 && $index == 0), 'col-sm-offset-{{((12 - (gameFilters.length * 4)) / 2) + 1}}': (gameFilters.length < 3 && $index == 0)}">
                                                    <div class="checkbox">
                                                        <label class="ng-binding">
                                                            <div class="icheckbox_lottomatica">
