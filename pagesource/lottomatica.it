                                        <h3 ng-if="game.title">{{game.title}} </h3>
                                        <h3 ng-if="game.name && !game.isPokerGame">{{game.name}}</h3>
                                        <h3 ng-if="game.gameTypeID">{{game.gameTypeID}}</h3>

                                        <div ng-if="game.isCasinoGame">
                                            <fnt-show-rating rating="game.rating" bg-star-color="white"></fnt-show-rating>
