                                            <button
                                                type="button"
                                                ng-if="game.isCasinoGame && isMac && game.download"
                                                class="button-default"
                                                alert-mac-casino-client>Scarica</button>

                                        </div>
                                    </div>
                                    <img ng-if="!game.isPokerGame && !game.isLotteryGame && !game.isFantasysportGame && !game.isSkillGame && !game.isBingoGame && !game.isBetterGame" ng-src="{{game.pictureHover}}" alt=""/>
                                </div>
                            </div>
                            <a ng-if="!game.isPokerGame && !game.isLotteryGame && !game.isFantasysportGame && !game.isSkillGame" ng-href="{{game.pagePath}}" class="arrow-label">{{game.name}}</a>
                            <a ng-if="game.isLotteryGame || game.isFantasysportGame || game.isSkillGame" ng-href="{{game.pagePath}}" class="arrow-label">{{game.title}}</a>

