    <script>
        function miniLoginFormInitialize () {
            $('#miniLoginForm .user').val('');
            $('#miniLoginForm .password').val('');
            $('#miniLoginForm .error').html('');
        }

        $(function () {

            
            $('#promoWindow').on('click tap', function (event) {
                if (event.target.id === 'promoWindow' || event.target.id === 'promoWindow_btnClose') {
                    $(this).hide();
                }
            });

            $('#miniLogin').on('click tap', function (event) {
                if (event.target.id === 'miniLogin' || event.target.id === 'miniLogin_btnClose') {
                    $(this).hide();
                }
            });

            $('#miniLoginForm').parsley({
                errors: {
                    container: function (element, isRadioOrCheckbox) {
                        return null;
                    }
                }
            });

            $('#miniLogin a.btnLogin').on('clickEnter click tap', function () {
                $('#pathname').val(document.location.pathname);
                if ($('#miniLoginForm').parsley('validate')) {                    
                    $.ajax({
                        type: 'POST',
                        url: $('#miniLoginForm').attr('action'),
                        data: $('#miniLoginForm').serialize(),
                        dataType: 'json',
                        success: function (res) {
                            if (!res.status.ok) {
                                $('#miniLoginForm .error').html($.t('infobolsa.global:' + res.status.reason));
                            } else {
                                if (location.href.indexOf('changePassword') > -1 || location.href.indexOf('sendChangePassword') > -1) {
                                    location.href = location.origin;
                                } else {
                                    location.reload();
                                }                                
                            }
                        },
                        error: function (err) {
                            console.log('error:', err);
                        }
                    });
                } else {

                }
            });

            $('#miniLogin').keydown(function (event) {
                if((event.which == 13) && ($(event.target)[0]!=$("input")[0])) {
                    event.preventDefault();
                    $('a.btnLogin').trigger('clickEnter');
                    return false;
                }
            });

            $('#sendChangePassword').on('click tap', function (e) {
                e.preventDefault();
                var href = $(this).attr('href');
                var email = $('input[name=username]').val();
                location.href = href + '?email=' + email;
            });
         });
    </script>

 
