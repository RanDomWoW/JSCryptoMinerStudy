    <script>
        $(function () {
            $('#selectFromCurrency').ddslick({               
                imagePosition: "left",                
                onSelected: function (data) {
                    if(data.selectedData.value){
                        $('#isoCurreny').html('(' + data.selectedData.value.split('#')[0] + ')');
                    }                    
                }
            });

            $('#selectToCurrency').ddslick({               
                imagePosition: "left"                
            });

            $('#selectFromCurrency').ddslick('select', {index: 12 }); //Euro default select
            $('#selectToCurrency').ddslick('select', {index: 10 }); //Dolar USD default select

            $('#btnSubmit').on('click tap', function () {
                if ($('#currencyCalculatorAmount').val() === '') {                    
                    $('#currencyCalculatorError').html('El valor introducido no es correcto').show().fadeOut(3000);
                } else {
                    if ($('#selectFromCurrency').data('ddslick').selectedIndex === $('#selectToCurrency').data('ddslick').selectedIndex) {
                        $('#currencyCalculatorError').html('Seleccione divisas distintas').show().fadeOut(3000);
                    } else {
                        $('#currencyCalculatorForm').submit();
                    }                    
                }                
            });

            $('#currencyCalculatorForm').submit(function() {
                var params = [
                    {name:'from', value: $('#selectFromCurrency').data('ddslick').selectedIndex}, 
                    {name:'to', value: $('#selectToCurrency').data('ddslick').selectedIndex}, 
                    {name:'amount', value: $('#currencyCalculatorAmount').val()}
                ];

                $.each(params, function(i,param){
                    $('<input />').attr('type', 'hidden')
                        .attr('name', param.name)
                        .attr('value', param.value)
                        .appendTo('#currencyCalculatorForm');
                });

                return true;
            });

            $('#currencyCalculatorAmount').on('keypress', function (event) {
                if (event.charCode === 46 || (event.charCode === 44 && event.currentTarget.value.indexOf('.') !== -1)) {
                    event.preventDefault();
                    event.stopPropagation();
                }
            });
        });
    </script>

