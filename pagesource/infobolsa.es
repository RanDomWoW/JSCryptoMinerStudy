        <script>
            $(function() {
                
                //initializeWidgetsState();

                var menuLeft = document.getElementById('ifb-menu'),
                    showLeftPush = document.getElementById('showLeftPush'),
                    body = document.body;
                showLeftPush.onclick = function () {
                    classie.toggle(this, 'active');
                    classie.toggle(body, 'ifb-menu-push-toright');
                    classie.toggle(menuLeft, 'ifb-menu-open');
                    disableOther('showLeftPush');
                };

                var menuRight = document.getElementById('ifb-widgets'),
                    showRightPush = document.getElementById('showRightPush');
                    showCloseWidgets = document.getElementById('showCloseWidgets');
                
                showRightPush.onclick = function () {
                    classie.toggle(this, 'active');
                    classie.toggle(body, 'ifb-menu-push-toleft');
                    classie.toggle(menuRight, 'ifb-widgets-open');
                    disableOther('showRightPush');
                };

                showCloseWidgets.onclick = function () {                    
                    classie.toggle(this, 'active');
                    classie.toggle(this, 'hide');
                    $('#ifb-widgets').toggleClass('hideWidgets');
                    updateWidgetsState();
                    ga('send', 'event', 'ClickMenuDrch', 'MDrch_OpenClose', '');
                };

                function disableOther(button) {
                    if (button !== 'showLeftPush') {
                        classie.toggle(showLeftPush, 'disabled');
                    }
                }

                function updateWidgetsState () { 
                    document.cookie="infobolsa.widgetsvisible=" + !$('#ifb-widgets').hasClass('hideWidgets');
                }

                function initializeWidgetsState () {
                    if (window.sessionStorage.getItem('widgetsVisible') === 'true' || document.body.clientWidth < 1640) {
                        $('#ifb-widgets').removeClass('hideWidgets');
                        $('#showCloseWidgets').removeClass('active').removeClass('hide');
                    } else {
                        $('#ifb-widgets').addClass('hideWidgets');
                        $('#showCloseWidgets').addClass('active').addClass('hide');
                    }
                    
                }
            });


            $('nav .rollbar').rollbar({zIndex: 80, wheelSpeed: 30});
        </script>
        
