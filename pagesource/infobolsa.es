        $(function() {
            var sliderContentSelector = '#sliderContent';
            var pagerContentSelector = '#swiper-pager';
            var elementCurrentSelector = pagerContentSelector + ' li.current';
            var listObject = $(pagerContentSelector + ' li');

            var pager = new Swiper('#swiper-container',{
                 speed: 750,
                 mode: 'horizontal',
                 onSlideChangeEnd: function () {
                    var activeSlide = arguments[0].activeSlide;

                    $(sliderContentSelector + ' .rightArrow').off().one('click', function() {
                        pager.swipeNext();
                        if ($(elementCurrentSelector).next()[0]) {

                            swiperPagerButtonClass(listObject, $($(elementCurrentSelector).next()[0]), elementCurrentSelector, sliderContentSelector);
                        }
                        //_gaq.push(['_trackEvent', 'IndexesComponent', 'SliderRightArrow', '-']);
                        ga('send', 'event', 'IndexesComponent', 'SliderRightArrow', '-');
                    });

                    $(sliderContentSelector + ' .leftArrow').off().one('click', function(){
                        pager.swipePrev();
                        if ($(elementCurrentSelector).prev()[0]) {

                            swiperPagerButtonClass(listObject, $($(elementCurrentSelector).prev()[0]), elementCurrentSelector, sliderContentSelector);
                        }
                        //_gaq.push(['_trackEvent', 'IndexesComponent', 'SliderLeftArrow', '-']);
                        ga('send', 'event', 'IndexesComponent', 'SliderLeftArrow', '-');
                    });

                    swiperPagerButtonClass(listObject, $(listObject[activeSlide().dataset.position]), elementCurrentSelector, sliderContentSelector);
                },

                onSlideReset : function () {
                    var activeSlide = arguments[0].activeSlide;

                    swiperPagerButtonClass(listObject, $(listObject[activeSlide]), elementCurrentSelector, sliderContentSelector);
                }
            });

            $(sliderContentSelector + ' .leftArrow').one('click', function(){
                pager.swipePrev();
                if ($(elementCurrentSelector).prev()[0]) {

                    swiperPagerButtonClass(listObject, $($(elementCurrentSelector).prev()[0]), elementCurrentSelector, sliderContentSelector);
                }
                //_gaq.push(['_trackEvent', 'IndexesComponent', 'SliderLeftArrow', '']);
                ga('send', 'event', 'IndexesComponent', 'SliderLeftArrow', '');
            });
            $(sliderContentSelector + ' .rightArrow').one('click', function(){
                pager.swipeNext();
                if ($(elementCurrentSelector).next()[0]) {

                    swiperPagerButtonClass(listObject, $($(elementCurrentSelector).next()[0]), elementCurrentSelector, sliderContentSelector);
                }
                //_gaq.push(['_trackEvent', 'IndexesComponent', 'SliderRightArrow', '']);
                ga('send', 'event', 'IndexesComponent', 'SliderRightArrow', '');
            });
            $(pagerContentSelector + ' li:nth-child(1)').click(function(){
                pager.swipeTo(0);

                swiperPagerButtonClass(listObject, $(this), elementCurrentSelector, sliderContentSelector);
                //_gaq.push(['_trackEvent', 'IndexesComponent', 'SummaryButton', '']);
                ga('send', 'event', 'IndexesComponent', 'SummaryButton', '');
            });
            $(pagerContentSelector + ' li:nth-child(2)').click(function(){
                pager.swipeTo(1);

                swiperPagerButtonClass(listObject, $(this), elementCurrentSelector, sliderContentSelector);
                //_gaq.push(['_trackEvent', 'IndexesComponent', 'SessionChartButton', '']);
                ga('send', 'event', 'IndexesComponent', 'SessionChartButton', '');
            });
            $(pagerContentSelector + ' li:nth-child(3)').click(function(){
                pager.swipeTo(2);

                swiperPagerButtonClass(listObject, $(this), elementCurrentSelector, sliderContentSelector);
                //_gaq.push(['_trackEvent', 'IndexesComponent', 'HistoricalChartButton', '']);
                ga('send', 'event', 'IndexesComponent', 'HistoricalChartButton', '');
            });
        });

        function swiperPagerButtonClass(container, button, elementCurrentSelector, sliderContentSelector) {
            container.removeClass('current');
            button.addClass('current');
            if ($(elementCurrentSelector).prev()[0]) {
                $(sliderContentSelector + ' .leftArrow').show();
            } else {
                $(sliderContentSelector + ' .leftArrow').hide();
            }

            if ($(elementCurrentSelector).next()[0]) {
                $(sliderContentSelector + ' .rightArrow').show();
            } else {
                $(sliderContentSelector + ' .rightArrow').hide();
            }

        }
    </script>
                    
