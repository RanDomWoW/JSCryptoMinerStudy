            var expires = "; expires=" + date.toGMTString();
        }
        else var expires = "";
        document.cookie = name + "=" + value + expires + "; path=/";
    }

    function readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') c = c.substring(1, c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
        }
        return "";
    }

    function eraseCookie(name) {
        createCookie(name, "", -1);
    }

    function checkEnter(e) {
        var characterCode;

        if (e && e.which) {
            e = e;
            characterCode = e.which;
        }
        else {
            e = event;
            characterCode = e.keyCode;
        }

        if (characterCode == 13) {
            epluslogin.submit();
            return false;
        }
        else {
            return true;
        }
    }

    function passwordReminder() {
        window.open("https://myaccount.entergy.com/eplus/portlets/passwordAssistance/begin.do", "login_entergy",
		    "status=1, height=530, width=800, resizable=0")
    }
    function passwordHint() {
        window.open("/eplus/portlets/PasswordHint/PasswordHintController.portlet", "login_entergy",
		    "status=1, height=375, width=400, resizable=0")
    }
    function securityQuestion() {
        window.open("/eplus/portlets/securityQuestion.jsp", "login_entergy",
		    "status=1, height=450, width=400, resizable=0")
    }
