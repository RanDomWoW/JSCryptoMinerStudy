<script>
	jQuery(document).ready(function() {

		var owl = jQuery("#slider-principal");

		owl.owlCarousel({
			pagination: false,
			responsive: false,
			mouseDrag: false
		});

		jQuery(".prox").click(function() {
			if ($(window).width() > 1600) {
				if (!$('.item').last().visible(true))
					owl.trigger('owl.next');
			}
			else
				owl.trigger('owl.next');
		});
		jQuery(".ant").click(function() {
			if ($(window).width() > 1600) {
				if (!$('.item').first().visible(true))
					owl.trigger('owl.prev');
			}
			else
				owl.trigger('owl.prev');
		});


		$(".geral-item-destaque").mouseover(function() {
			$(this).find('h3').css('max-height', '98px');
		});

		$(".geral-item-destaque").mouseout(function() {
			$(this).find('h3').css('max-height', '30px');
		});



	});

</script>
