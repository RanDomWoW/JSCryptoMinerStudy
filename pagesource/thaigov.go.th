jQuery('#plus_font_big').click(function(){

  jQuery('#minus_font_default').hide();
  jQuery('#minus_font_medium').show();
  jQuery('#plus_font_medium').hide();
  jQuery('#plus_font_big').show();

});



function change_lang(lang)
{
 $.post('http://www.thaigov.go.th/main/Contents/change_lang',
  {lang:lang, ci_csrf_token : ''},function(data){
    if(data.success == '1'){
     location.reload();	
   }
 },'json');
}

</script>



