        var searchStart;

        function checkChannel(obj) {

            clearTimeout(searchStart);

            var object = $(obj);
            var form = object.closest('form');
            var searchVal = form.find(".channelName").val();
            var sonuc = form.find('.sonucMesaj');
            var submitBtn = form.find('input[type="submit"]');

            form.attr('action', MAIN_URL + "registration/choose-channel/#" + searchVal);
            $.ajax({
                url: MAIN_URL + 'registration/checkChannel/',
                type: 'get',
                data: {channel: searchVal},
                dateType: "json",
                success: function (data) {

                    dataObj = $.parseJSON(data);

                    submitBtn.prop('disabled', true);
                    sonuc.removeClass('green');
                    if (dataObj.success != false) {
                        sonuc.addClass('green');
                        submitBtn.prop('disabled', false);
                    }

                    sonuc.html(dataObj.message);
                    sonuc.fadeIn('normal');

                }
            });

        }

        $(".channelName").on('keypress', function (e) {
            clearTimeout(searchStart);
            var obj = $(this);
            var searchChannel = obj.val();
            if (e.keyCode == 13 || searchChannel.length >= '3' || searchChannel.length == 0) {
                searchStart = setTimeout(checkChannel, 1000, this);
            }
        }).on('keydown', function (e) {
            clearTimeout(searchStart);
            var obj = $(this);
            var searchChannel = obj.val();
