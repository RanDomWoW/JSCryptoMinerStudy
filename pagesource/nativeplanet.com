     var file_pattern = /(.*)\.html/i;
     var ga_value = '';
     var domain_varifier = /(.*)\.(.*)\.(.*)/g;
     var file_chunk = '';
     var value = '';



     for(var i=1; i<url_section.length;i++)
     {
         ga_value = url_section[i];
         if(ga_value.length > 0 )
