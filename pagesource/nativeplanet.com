                 //googletag.pubads().setTargeting("topic",RegExp.$1);
                 target_words.push(RegExp.$1);
             }
             else if(ga_value.match(file_pattern))
             {
                 ga_value = ga_value.replace('_','-');
                 ga_value = ga_value.replace('.html','');
	     target_words.push(ga_value);
	    console.log("ga - added value "+ga_value);
                 file_chunk = ga_value.split('-');
                 for(var x in file_chunk)
                 {
                     if(!file_chunk[x].match(file_pattern))
                     {
                         //googletag.pubads().setTargeting("topic",file_chunk[x]);
                         target_words.push(file_chunk[x]);
                     }
                 }
             }
             else if(ga_value.match(search_pattern))
             {
                 //googletag.pubads().setTargeting("topic",RegExp.$1);
                 target_words.push(RegExp.$1);
             }
             else
             {
                 if(url_section[i].match(domain_varifier))
                 {
                     if(RegExp.$1 == 'www')
                         value = RegExp.$2;
                     else
                         value = RegExp.$1;
                 }
                 else
                     value = url_section[i];

                 //googletag.pubads().setTargeting("topic",value);
                 target_words.push(value);
             }
         }
