<script>
    $(document).ready(function() {

        var bgImageArray = ["/media/image/homepage/hero/easter-slider.jpg", "/media/image/homepage/hero/beach.png", "/media/image/homepage/hero/slider-teddy-flipped.jpg"];

        bgImageArray.forEach(function(img){
            new Image().src = img;
            // caches images, avoiding white flash between background replacements
        });

        secs = 6;

        $("#quicksearch-promo-0").addClass("current");
        $("#quicksearch-promo-0").data("next",1);
        $("#quicksearch-promo-1").data("next",2);
        $("#quicksearch-promo-2").data("next",0);

        function backgroundSequence() {
            setTimeout(function () {

                var current = $(".current");
                $('#cover').css('background-image', 'url(' + bgImageArray[current.data("next")] + ')');
                current.fadeOut("slow", "linear");
                $("#quicksearch-promo-" + current.data("next")).fadeIn("slow", "linear");
                $("#quicksearch-promo-" + current.data("next")).addClass("current");

                current.removeClass("current");

                setTimeout(function() { backgroundSequence() }, (secs * 1000));

            });
        }

        setTimeout(function() { backgroundSequence() }, (secs * 1000));



    });
</script>