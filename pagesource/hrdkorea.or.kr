	// 팝업띄울려면 여기를 수정하면 됩니다.
    function openPopup() {

        var noticeCookie1 = getCookie("hrd_popup");  // 쿠키네임 지정
        if (noticeCookie1 != "done") {
            window.open('/main/Recruit.html','hrd_popup','width=600,height=822,top=50,left=150');
            //window.open("/main/popup/Nlivecam.html","hrd_popup","width=850,height=872,top=50,left=100");
        }

    }

    window.onload = function() {
        var diffDate = "20180330";
        var now = new Date();
        var year = now.getFullYear(); // 현재 년도 가져오기
        var month = now.getMonth() + 1; // 현재 월 가져오기

        if((month+"").length < 2) {// 월이 한자리 수인 경우 (1,3,5) 앞에 0 붙인다 01,03,05
            month = "0"+month;
        }

        var date = now.getDate(); // 현재 날짜 가져오기

        if((date+"").length < 2) {// 일이 한자리 수인 경우 (1,3,5) 앞에 0 붙인다 01,03,05
            date = "0"+date;
        }
        var today = year+""+month+""+date;

        if(parseInt(today) < parseInt(diffDate) ) { // int 형으로 변환하여 비교

            openPopup();

        }

    }
	
	
