<!-- google onclick -->
<script type="text/javascript">
$(function(){
	$("body a").each(function(){
		$(this).click(function(){
			//alert("href："+$(this).attr('href'));
			var href = $(this).attr('href');
			var target = $(this).attr('target');
			if($(this).text() != "" && $(this).text() != undefined ){
				var text = $(this).text();
				text = text.replace(/\r?\n/g,"");
				text = text.replace(/\s+/g,"");
				text = text.substring(0, 20);
				//alert("TEXT："+text);
				var label = text+"_"+ href;
				ga('send', 'event', {
					'eventCategory' : 'TOP',
					'eventAction' : 'LINK',
					'eventLabel' : label,
					'hitCallback' : function () {
						if(target != "_blank"){
							document.location = href;
						}
					}
				});
			}
			else{
				var $alttext = $(this).find("img");
				if($alttext.attr("alt")){
					//alert("ALT TEXT："+$alttext.attr("alt"));
						var label = $alttext.attr("alt")+"_"+ href;
						ga('send', 'event', {
							'eventCategory' : 'TOP',
							'eventAction' : 'LINK',
							'eventLabel' : label,
							'hitCallback' : function () {
								if(target != "_blank"){
									document.location = href;
								}
							}
						});
				}
			}
			if(target == "_blank"){
				window.open(href);
			}
			return false;
		});
	});

});
</script>
<!-- /google onclick -->

