<script>
    (function (exports, d) {
        function domReady(fn, context) {

            function onReady(event) {
                d.removeEventListener("DOMContentLoaded", onReady);
                fn.call(context || exports, event);
            }

            function onReadyIe(event) {
                if (d.readyState === "complete") {
                    d.detachEvent("onreadystatechange", onReadyIe);
                    fn.call(context || exports, event);
                }
            }

            d.addEventListener && d.addEventListener("DOMContentLoaded", onReady) ||
                d.attachEvent && d.attachEvent("onreadystatechange", onReadyIe);
        }

        exports.domReady = domReady;
    })(window, document);

    domReady(function () {
        function updateBookATestDriveAnchor(anchors) {
            for (var i = 0; i < anchors.length; i++) {
                var anchor = anchors[i];
                var href = anchor.getAttribute('href') || '';
                if (href.match(/booktestdrive\.html/gi)) {
                    testDriveButton.setAttribute('href', href);
                }
            }
        }

        var testDriveButton = document.getElementsByClassName('testDriveButton')[0];
        testDriveButton.setAttribute('href', pathToRoot + testDriveButton.getAttribute('href'));

        var desiredSourceRegex = /helios_scripts\/main.js/gi;
        var scripts = document.getElementsByTagName('script');
        var isHelios = false;

        if (scripts.length) {
            for (var scriptIndex in scripts) {
                console.log(scripts[scriptIndex].src);
                if (!isHelios && desiredSourceRegex.test(scripts[scriptIndex].src)) {
                    isHelios = true;
                }
            }
        }
        if (isHelios) {
            testDriveButton.firstChild.setAttribute('data-icotestdrive-before', '');
        }

        // check if we have booktestdrive in usefullinks
        var menuAside = document.getElementById('main-slider-aside-menu');
        if (menuAside) {
            var anchors = menuAside.getElementsByTagName('a');
            updateBookATestDriveAnchor(anchors);
        }
        // check if we have booktestdrive in action panel
        var actionPanels = document.getElementsByClassName('action-panel');
        if (actionPanels && actionPanels.length > 0) {
            for (var j = 0; j < actionPanels.length; j++) {
                var anchors = actionPanels[0].getElementsByTagName('a');
                updateBookATestDriveAnchor(anchors);
            }
        }
        // check if we have booktestdrive in prices-block
        var pricesBlocks = document.getElementsByClassName('prices-block');
        if (pricesBlocks && pricesBlocks.length > 0) {
            for (var j = 0; j < pricesBlocks.length; j++) {
                var anchors = pricesBlocks[0].getElementsByTagName('a');
                updateBookATestDriveAnchor(anchors);
            }
        }
    });
</script>
