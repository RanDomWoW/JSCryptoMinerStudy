<script type="text/javascript">
(function(window) {
    var braintreeInstance;
    var visaCheckoutInstance;
    var dataCollectorInstance;
    var clientToken = '';
    var buttonSource = 'https://secure.checkout.visa.com/wallet-services-web/xo/button.png?acceptCanadianVisaDebit=false&cobrand=true&size=154';

    function triggerCallback(callback, args) {
        if (callback && typeof callback === 'function') {
            callback(args);
        }
    }

    function init(callback) {
        if (braintreeInstance) {
            triggerCallback(callback);
            return;
        }

        braintree.client.create(
            { authorization: clientToken },
            function braintreeClientInstanceCallback(clientCreateError, clientInstance) {
                braintreeInstance = clientInstance;

                triggerCallback(callback);
            }
        );
    }

    function initVisaCheckout(callback) {
        if (visaCheckoutInstance) {
            triggerCallback(callback);
            return;
        }

        braintree.visaCheckout.create(
            { client: braintreeInstance },
            function(vcoClientCreateError, clientInstance) {
                visaCheckoutInstance = clientInstance;

                triggerCallback(callback);
            });
    }

    function initDataCollector(callback) {
        if (dataCollectorInstance) {
            triggerCallback(callback);
            return;
        }

        braintree.dataCollector.create(
            {
                client: braintreeInstance,
                kount: true,
                paypal: true,
            },
            function dataCollectorCallback(dataCollectorError, clientInstance) {
                dataCollectorInstance = {};

                if (!dataCollectorError) {
                    dataCollectorInstance = clientInstance;
                }

                triggerCallback(callback);
            }
        );
    }

    function createOptions(shouldCollectShipping) {
        var options = {
            paymentRequest: {
                currencyCode: 'USD',
                subtotal: 0,
            },
            shipping: {
                collectShipping: shouldCollectShipping === undefined ? true : shouldCollectShipping
            },
        };

        return visaCheckoutInstance.createInitOptions(options);
    }

    function tokenize(payment, deviceData) {
        visaCheckoutInstance.tokenize(payment, function(tokenizeError, payload) {
            if (tokenizeError) {
                handleError();
                return;
            }

            var billing = payload.billingAddress || payment.billingAddress;
            var shipping = payload.shippingAddress || payment.shippingAddress;
            var shippingAddress = JSON.stringify({
                email: payload.userData.userEmail,
                first_name: shipping.firstName,
                last_name: shipping.lastName,
                phone_number: shipping.phoneNumber,
                address_line_1: shipping.streetAddress,
                address_line_2: shipping.extendedAddress,
                city: shipping.locality,
                state: shipping.region,
                country_code: shipping.countryCode,
                postal_code: shipping.postalCode,
            });
            var billingAddress = JSON.stringify({
                email: payload.userData.userEmail,
                first_name: billing.firstName,
                last_name: billing.lastName,
                phone_number: billing.phoneNumber,
                address_line_1: billing.streetAddress,
                address_line_2: billing.extendedAddress,
                city: billing.locality,
                state: billing.region,
                country_code: billing.countryCode,
                postal_code: billing.postalCode,
            });
            var cardInformation = JSON.stringify({
                type: payload.details.cardType,
                number: payload.details.lastTwo,
            });

            postPaymentData({
                type: payload.type,
                provider: 'braintreevisacheckout',
                action: 'set_external_checkout',
                nonce: payload.nonce,
                deviceData: deviceData,
                billingAddress: billingAddress,
                shippingAddress: shippingAddress,
                cardInformation: cardInformation,
            });
        });
    }

    function handleError(errorMessage) {
        var message = !errorMessage ? lang.BraintreeGenericError : errorMessage;

        alert(message);
    }

    function insertVisaCheckoutButton() {
        var buttonClass = 'visa-checkout-wrapper';
        var containerClass = 'CheckoutButtonBraintreevisacheckout';
        var buttonTemplate =
            '<img ' +
                'alt="Visa Checkout"' +
                'class="v-button"' +
                'role="button"' +
                'src="' + buttonSource + '"' +
            ' />' +
            '<a class="v-learn v-learn-default" href="#" data-locale="en_US">Tell Me More</a>';

        var buttonNode = document.createElement('div');
        buttonNode.className = buttonClass;

        var buttonContainer = document.getElementsByClassName(containerClass)[0];

        if (buttonContainer && !hasVisaCheckoutButton(buttonContainer, buttonClass)) {
            buttonNode.innerHTML = buttonTemplate;
            buttonContainer.appendChild(buttonNode);
        }
    }

    function hasVisaCheckoutButton(parentNode, childClass) {
        if (!parentNode || !parentNode.childNodes) {
            return false;
        }

        return Array.prototype.some.call(parentNode.childNodes, function(child) {
            return child.className && child.className.indexOf(childClass) >= 0;
        });
    }

    function postPaymentData(data) {
        var form = FormPoster.createFormPoster();

        var paymentData = {
            payment_type: data.type,
            provider: data.provider,
            action: data.action,
            nonce: data.nonce,
            billing_address: data.billingAddress,
            shipping_address: data.shippingAddress,
            card_information: data.cardInformation,
        };

        // Omit device data if it was not supplied
        if (!!data.deviceData) {
            paymentData.device_data = data.deviceData;
        }

        form.postForm('http://www.opt-7.com/checkout.php', paymentData);
    }

