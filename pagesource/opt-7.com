    }

    function onVisaCheckoutReady(callback) {
        init(function() {
            initVisaCheckout(function() {
                initDataCollector(callback);
            });
        });
    }

    window.initButtonBraintreevisacheckout = function(callback) {
        onVisaCheckoutReady(function() {
            insertVisaCheckoutButton();
            displayButtons();

            V.init(createOptions());

            V.on('payment.success', function(payment) {
                tokenize(payment, dataCollectorInstance.deviceData);
            });

            V.on('payment.error', function() {
                handleError();
            });

            triggerCallback(callback);
        });
    };

    window.initEditBraintree = function(billingAddress, shippingAddress, callback) {
        onVisaCheckoutReady(function() {
            V.init(createOptions(false));

            V.on('payment.success', function(payment) {
