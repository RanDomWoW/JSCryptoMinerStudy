<script type="text/javascript">
$(document).ready(function(){
    $('.productAttributeLabel').each(function() {
        var name = $('label .name', this).html();
        if (name.indexOf('Bulb Type:') >= 0 || name.indexOf('Bulb Size:') >= 0) {
            $('label', this).append('<span> <a href="#bulb-finder-modal" id="bulb-finder-trigger">?</a> </span>');
        }
    });

    //initiates the bulb finder modal
    $("#bulb-finder-trigger").leanModal({
        closeButton: ".modal_close" 
    });

    //restores original look of html selects
    $(function(){
        console.log('remove styling');
        $.uniform.restore("#bulb-finder select");
    });

    $.ajax({
        url: '//carbulbfinder.com/bulbs/BulbWebhook.php',
        type: 'GET',
        dataType: 'jsonp',
        jsonpCallback: 'mycallback',
        beforeSend: function() {
            console.log("Sending");
        },
        success: function(data){
            console.log("success");
            var response = data.response;
            //console.log(response);
            var makes = '';
            for (var i = 0; i <= response.length-1; i++) {
                //console.log(response[i]);
                makes += '<option value="' + data.response[i] + '">' + data.response[i] + '</option>';
            };
            $('#make').append(makes);
        }
    });


    //function that listens for choice of make and sets list for models
    $('#make').change(function(){
        $('#model').html('<option>- choose model -</option>').prop('disabled', true);
        $('#year').html('<option>- choose year -</option>').prop('disabled', true);
        $('#option').html('<option>- choose option -</option>').prop('disabled', true);
        var option1 = $( "#make" ).val();
        $.ajax({
            url: '//carbulbfinder.com/bulbs/BulbWebhook.php',
            type: 'GET',
            data: { "mk": option1 },
            dataType: 'jsonp',
            jsonpCallback: 'mycallback',
            beforeSend: function() {
                console.log("Sending");
            },
            success: function(data){
                console.log("success");
                var response = data.response;
                //console.log(response);
                var models = '';
                for (var i = 0; i <= response.length-1; i++) {
                    //console.log(response[i]);
                    models += '<option value="' + data.response[i] + '">' + data.response[i] + '</option>';
                };
                $('#model').append(models);
            }
        });
        $('#model').prop('disabled', false);
    });

    $('#model').change(function(){
        $('#year').html('<option>- choose year -</option>').prop('disabled', true);
        $('#option').html('<option>- choose option -</option>').prop('disabled', true);
        var option1 = $( "#make" ).val();
        var option2 = $( "#model" ).val();
        $.ajax({
            url: '//carbulbfinder.com/bulbs/BulbWebhook.php',
            type: 'GET',
            data: { "mk": option1, "mdl": option2 },
            dataType: 'jsonp',
            jsonpCallback: 'mycallback',
            beforeSend: function() {
                console.log("Sending");
            },
            success: function(data){
                console.log("success");
                var response = data.response;
                //console.log(response);
                var years = '';
                for (var i = 0; i <= response.length-1; i++) {
                    //console.log(response[i]);
                    var stringNum = response[i].toString();
                    years += '<option value="' + stringNum + '">' + stringNum + '</option>';
                };
                $('#year').append(years);
            }
        });
        $('#year').prop('disabled', false);
    });

    $('#year').change(function(){
        $('#option').html('<option>- choose option -</option>').prop('disabled', true);
        var option1 = $( "#make" ).val();
        var option2 = $( "#model" ).val();
        var option3 = $( "#year" ).val();
        $.ajax({
            url: '//carbulbfinder.com/bulbs/BulbWebhook.php',
            type: 'GET',
            data: { "mk": option1, "mdl": option2, "yr": option3 },
            dataType: 'jsonp',
            jsonpCallback: 'mycallback',
            beforeSend: function() {
                console.log("Sending");
            },
            success: function(data){
                console.log("success");
                var response = data.response;
                //console.log(response);
                var options = '';
                for (var i = 0; i <= response.length-1; i++) {
                    //console.log(response[i]);
                    options += '<option value="' + response[i] + '">' + response[i] + '</option>';
                };
                $('#option').append(options);
            }
        });
        $('#option').prop('disabled', false);
    });

    $('#option').change(function(){
        $('#application').html('<option>- choose application -</option>').prop('disabled', true);
        var option1 = $( "#make" ).val();
        var option2 = $( "#model" ).val();
        var option3 = $( "#year" ).val();
        var option4 = $( "#option" ).val();
        $.ajax({
            url: '//carbulbfinder.com/bulbs/BulbWebhook.php',
            type: 'GET',
            data: { "mk": option1, "mdl": option2, "yr": option3, "opt": option4 },
            dataType: 'jsonp',
            jsonpCallback: 'mycallback',
            beforeSend: function() {
                console.log("Sending");
            },
            success: function(data){
                console.log("success");
                var response = data.response;
                //console.log(response);
                for (var i = 0; i <= response.length-1; i++) {
                    var className = response[i]['application'].split(' ').join('_');
                    $('.' + className).html(response[i]['application'] + "<br><span>" + response[i]['bulb'] + "</span>").show();
                }
                $('#bulb-table-container').slideDown();
                $('#resetFinder').show();
            }
        });
    });

    $('#resetFinder').click(function(e){
        e.preventDefault();
        $('#bulb-table-container').slideUp();
        $('#bulb-table .bulb-table-column li').empty().hide();
        $('#model').html('<option>- choose model -</option>').prop('disabled', true);
        $('#year').html('<option>- choose year -</option>').prop('disabled', true);
        $('#option').html('<option>- choose option -</option>').prop('disabled', true);
        $('#make').val("default");
        $('#resetFinder').hide();
    });
});
</script>
