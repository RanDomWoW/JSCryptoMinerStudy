        if (window_w > 1200) {
            duilian.show();
        }
        else {
            duilian.hide();
        }
        $(window).scroll(function () {
            var scrollTop = $(window).scrollTop();
            duilian.stop().animate({top: scrollTop + 200});
        });
        duilian_close.click(function () {
            $(this).parent().hide();
            return false;
        });
    });
</script>
