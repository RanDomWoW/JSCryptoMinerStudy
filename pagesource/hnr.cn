<script type="text/javascript">
    $(function () {
        part21.init();
    })
    var channerId = 0, zhankaiBId = 0;
    var date_ch = new Array();

    var part21 = {
        init: function () {
            $(".chaCom .zhankaiB").on('mouseover', function () {
                var $_this = $(this);
                clearTimeout(channerId);
                clearTimeout(zhankaiBId);
                channerId = window.setTimeout(function () {
                    //var num = $_this.index(".chaCom .zhankaiB") + 1;
                    var num = $_this.attr('iframe');
                    var topHeight = $_this.closest(".chaCom").position().top;
                    part21.getDatas(num, topHeight);
                }, 300);
                $("#channel").mouseleave(function () {
                    clearTimeout(channerId);
                    clearTimeout(zhankaiBId);
                    $(".moudleBox").stop(true, true).animate({"width": "0"}, 500);
                    setTimeout(function () {
                        $(".moudleBox").empty();
                    }, 300);
                });
                $(".chaCom .zhankaiB").on('mouseleave', function () {
                    clearTimeout(channerId)
                    clearTimeout(zhankaiBId);
                    zhankaiBId = window.setTimeout(function () {
                        clearTimeout(channerId);
                        $(".moudleBox").stop(true, true).animate({"width": "0"}, 500);
                        setTimeout(function () {
                            $(".moudleBox").empty();
                        }, 300);
                    }, 300);
                });
                $(".moudleBox").on('mouseover', function () {
                    clearTimeout(zhankaiBId);
                });
            });
        },
        getDatas: function (_num, topHeight) {
            var num = _num;
            if (date_ch[num] != undefined) {
                $(".moudleBox").empty().html(date_ch[num]);
                $(".moudleBox").css({"top": topHeight}).stop(true, true).animate({"width": "1000px", "height": "380px"}, 500);
            }
            else {
                $.ajax({
                    type: "GET",
                    global: false,
                    url: 'http://www.hnr.cn/block/syiframe/iframe' + num, //'http://www.news.cn/part21/chabox1
                    dataType: "html",
                    success: function (data) {
                        date_ch[num] = data;
                        $(".moudleBox").empty().html(data);
                        $(".moudleBox").css({"top": topHeight}).stop(true, true).animate({"width": "1000px", "height": "380px"}, 500);
                    },
                    error: function () {
                        part21.getData(num);
                    }
                });
            }
            $(".moudleBox").mouseleave(function () {
                clearTimeout(channerId);
                $(this).stop(true, true).animate({"width": "0"}, 800);

            });
        }
    }
</script>
