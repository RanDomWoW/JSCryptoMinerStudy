              <script>
            jQuery(document).ready(function($){
              if($('.page-template-actus').length){
                  var currentPage = 2;
                  var containerItemSelector = "#actus_lp";
                  var postItemSelector = ".item";
                  var theTime=0;
                  
                    $('.all_news a').click(function(e){
                        e.preventDefault();
                        $.ajax({
                          type: "GET",
                          url: "http://www.groupe-efrei.fr/archives-actus/page/" + currentPage + "/",
                          data: "",
                          success: function(results){
                            var item = $(containerItemSelector + ' > ' + postItemSelector, results);
                            var $container = $('#actus_lp');
                            item.each(function() {
                                $(containerItemSelector).append($(this));
                                var $newElems = jQuery( $(this) ).hide();
                                $($newElems).imagesLoaded(function() {
                                  $container.isotope( 'appended', $newElems )
                                });
                            });
                            currentPage++;
                          },
                          error: function(results){
                            if(results.status == 404) {
                              $(".xhrStatus").html('No more posts to show...');
                            }
                            else{
                              $(".xhrStatus").html('Error retriving posts...');
                            }
                          }
                        });
                    });
              }
            
          });
        </script>
