	//Summary: Returns the absolute URL to use for prefixing any Ajax URL call.
	//Description: Dojo does not handle the case where the parameters in the URL are delimeted by the "/" forward slash. Therefore, in order to
	//             workaround the issue, all AJAX requests must be done using absolute URLs rather than relative.
	//Returns: The absolute URL to use for prefixing any Ajax URL call.
	
	//Scriptlet para obtener rutaSEO de request y setearla a jsp para usarla mas abajo
	
	
	function getAbsoluteURL() {
		//GMOSTACERO: ABCDIN - 20150507 - CORRECCIONES EN CAMBIO DE PUERTO - INICIO
		var protocol = window.location.protocol;
		var host = window.location.host;
		if(protocol.indexOf(":") != -1){
			protocol = protocol + "//";
		}else{
			protocol = protocol + "://";
		}
		
		
		//Sobreescribir ruta absoluta con rutaSEO
		var rutaSEO = '/tienda';
		if (absoluteURL != "") {
		
			if(rutaSEO){
				absoluteURL = protocol + host +  rutaSEO + "/";
			}
			else{
				absoluteURL = protocol + host + "/webapp/wcs/stores/servlet/";
			}
			
		}
		return absoluteURL;	
		//GMOSTACERO: ABCDIN - 20150507 - CORRECCIONES EN CAMBIO DE PUERTO - FIN
	}
