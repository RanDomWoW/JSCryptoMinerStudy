<script>  
  function ExibeBannerAviso(divBanner, divMaskBanner) {

    var maskHeight = jQuery(document).height();
    var maskWidth = jQuery(window).width();

    var imgSrc;
    jQuery(window).width() <= 500 ? imgSrc = 'popin-presidenciaveis-ciro-2018-mobile.jpg' : imgSrc = 'popin-presidenciaveis-ciro-2018-desktop.jpg';

    var box = "<div id='boxesBanner2'><div id='dialogBanner2' class='windowBanner'><a href='#' class='closeBanner'>Fechar[X]</a><a href='https://www.amcham.com.br/aovivo'><img src='http://estatico.amcham.com.br/arquivos/2018/" + imgSrc + "' /></a></div><div id='maskBanner2'></div></div>";

    jQuery('body').prepend(box);

    jQuery(divMaskBanner).css({'width':maskWidth,'height':maskHeight});

    jQuery(divMaskBanner).fadeIn(1000);
    jQuery(divMaskBanner).fadeTo("slow", 0.8);

    jQuery(window).width() <= 500 ? jQuery(divBanner).css({ 'top': '15vh', 'left': '12vw' }) : jQuery(divBanner).css({ 'top': '22vh', 'left': '22vw' });

    jQuery(divBanner).fadeIn(2000);

    jQuery('.windowBanner .closeBanner').click(function (e) {
      e.preventDefault();
      jQuery('.windowBanner').fadeOut(1000);
      jQuery(divMaskBanner).fadeOut(1000);
    });

    jQuery(divMaskBanner).click(function () {
      jQuery('.windowBanner').fadeOut(1000);
      jQuery(divMaskBanner).fadeOut(1000);
    });

    setTimeout( function() {
      jQuery('.windowBanner').fadeOut(1000);
      jQuery(divMaskBanner).fadeOut(1000);
    }, 10000 );
  }
</script>

