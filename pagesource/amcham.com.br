            <script type="text/javascript">
                /* https://developer.mozilla.org/en-US/docs/Web/Events
                   Samples:
                       Refresh All.
                           adsTool.refreshAds();
                       Refresh slot4 and slot5.
                           adsTool.refreshAds([window.slot4,window.slot5]);
                       Refresh slot4 only.
                           adsTool.refreshAds([window.slot4]);
                */
                adsTool = {};
                adsTool.refreshAds = function(refresh_slots) {
                    if(typeof(refresh_slots) == "undefined") {
                        googletag.pubads().refresh();
                    } else {
                        googletag.pubads().refresh(refresh_slots);
                    }
                };
                adsTool.refreshAdsByEvent = function(event,sec){
                    sec = sec || 250;
                    if(typeof(event) != "undefined") {
                        window.addEventListener(event, function(){
                            adsTool.refreshAdsByTimeout(sec);
                        });
                    } else {
                        adsTool.refreshAdsByTimeout(sec);
                    }
                };
                adsTool.refreshAdsByTimeout = function(sec) {
                    sec = sec || 5000;
                    var refreshTimer;
                    clearTimeout(refreshTimer);
                    refreshTimer = setTimeout(function(){adsTool.refreshAds()}, sec);
                };
            </script>
            
