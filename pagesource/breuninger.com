<script id="rr_recommendations" type="text/x-jsrender">
  {{if title.mobile}}
    <div class="carouselTitle carouselTitle--mobile">{{:title.mobile}}</div>
    <div class="carouselTitle carouselTitle--desktop">{{:title.desktop}}</div>
  {{else}}
    <div class="carouselTitle">{{:title}}</div>
  {{/if}}
  <div class="sliderContainer">
    <div class="slider eb-slider">
      {{for products}}
        <div class="product slide">
          <a href="{{:pod_url}}" data-param="{{:~urlParams(id, syan, ~root.econdaMarker)}}" data-rr-tracking-url="{{:rr_url}}" data-marker="{{>~root.econdaMarker}}">
