                            <script type="text/javascript">
	jQuery(document).ready(function() {
		jQuery('#trackingno').click(function() {
			jQuery('#message').attr('placeholder', 'Enter up to 20 tracking numbers here (one per line) and click Track now');
		});
		jQuery('input#referenceno').click(function() {
			jQuery('#message').attr('placeholder', 'Please enter only ONE Master Airway Bill Number at a time');
		});
	})

	function validate(){

		if(document.getElementById('message').value.trim() == ''){
			document.getElementById('msgValidate').setAttribute("class","msgValidate show"); 
			return false;
		}
		return true;
	}

	function checkFollowItem() {
		if(jQuery('#message').val() == '') {
			jQuery('#msgValidate').attr("class","msgValidate show"); 
			return false;
		} else { 
			var arrayJoin = '';
			var url;
			var value = jQuery('#message').val();
			var array = value.split(/[,;\n]/);

			var count = 0;
			for(var i = 0; i < array.length; i++) {
				if(array[i] != '') {
					arrayJoin = arrayJoin + ',' + array[i];
					count++;
				}
			}

			var service_url = "http://track.quantiumsolutions.com";
			var finalString = encodeURIComponent(arrayJoin);
			if (jQuery('#trackingno').is(':checked')) {
				if (count > 20) {
					jQuery('#msgValidate').text('Maximum 20 Tracking Numbers are Allowed');
					jQuery('#msgValidate').attr("class","msgValidate show");
					return false;
				}
				url = service_url + '/?TrackNo=' + finalString;
			} else if (jQuery('#referenceno').is(':checked')) {
				if (count > 1) {
					var enter_bill_number = 'Please enter only 1 MAWB at a time';
					jQuery('#msgValidate').text(enter_bill_number);
					jQuery('#msgValidate').attr("class","msgValidate show");
					return false;
				}
				url = service_url + '/?MAWB=' + finalString;
			}

			window.open(url,'_blank');
			return false;
		}

		
	}
	
	function removeVal(){
		document.getElementById('msgValidate').setAttribute("class","msgValidate"); 
	}
	
</script>
