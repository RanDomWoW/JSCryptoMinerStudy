{
    background:white;
    color:Green;
    }
/* INNER UL HIDE */
nav.vertical ul ul{
  position:absolute;
  left:0%;
  top:50;
  width:100%;
   background:white;display:none;
  opacity:0;
  transition: transform 0.2s;
  transform: translateX(50px);
}
/* INNER UL SHOW */
nav.vertical li:hover > ul{
  left:10%;
  top:100%;
  position:absolute;
  z-index:1;
  visibility:visible;
  width:100%;
  opacity:100;
  transform: translateX(0px);
}
}
