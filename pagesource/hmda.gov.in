*{padding:0;margin:0;}
body{}


/*VERTICAL MENU*/
nav.vertical{
  position:relative;
  width:260px;
}       

/* ALL UL */
nav.vertical ul{
  list-style: none;
  width:260px;
}
/* ALL LI */
nav.vertical li{
  position:relative;
  width:260px;
}
/* ALL A */
nav.vertical a{
  display:block;
  color:blue;
  text-decoration:none;
  padding:12px 15px;
  background:green;
  transition:0.2s;
}
/* ALL A HOVER */
nav.vertical li:hover > a{
  background:blue;
}

/* INNER UL HIDE */
nav.vertical ul ul{
  position:absolute;
  left:0%;
  top:50;
  width:100%;
   background:green;display:none;
  opacity:0;
  transition: transform 0.2s;
  transform: translateX(50px);
}
/* INNER UL SHOW */
nav.vertical li:hover > ul{
  left:100%;
  position:absolute;
  z-index:1;
  visibility:visible;
  width:100%;
  opacity:100;
  transform: translateX(0px);
}
@media only screen and (min-width: 300px) and (max-width:1024px) {
    
/* ALL A HOVER */
nav.vertical li:hover > a{
  background:blue;
  color:White;
}
