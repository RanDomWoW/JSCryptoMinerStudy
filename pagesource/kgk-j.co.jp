#flashcontent .altArea .logo{
	text-align:center;
	margin-bottom:40px;
}
#flashcontent .altArea .adobe{
	width:138px;
	float:left;
}
#flashcontent .altArea .txt{
	width:338px;
	font-family:"メイリオ","Meiryo", "ヒラギノ角ゴ Pro W3", "Hiragino Kaku Gothic Pro",Osaka, "ＭＳ Ｐゴシック", "MS PGothic", sans-serif;
	font-size:13px;
	line-height:21px;
	color:#333;
	float:right;
}
#flashcontent .altArea .btn{
	text-align:center;
	padding-top:40px;
	clear:both;
}
#flashcontent .altArea .btn a:hover img{
    filter: alpha( opacity=70 );  /* IE */
    opacity:0.7;                 /* Firefox,Opera,Safari */
