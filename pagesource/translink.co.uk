                }
            })
        );
    }

    function JP_addZero(i) {
        if (i < 10) {
            i = "0" + i;
        }
        return i;
    }

    //This has to be an object class because some properties need to be conditional
    //i.e. train and bus method of transports - if properties are present in URL they
    //get included in departure results, regardless of what value is provided!
    function originLiveDepartureParams(sDT) {
        var omc = jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_hfFromSelectedOmc').val();
        var placeId = jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_hfFromSelectedPlaceId').val();
        var stateless = jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_hfFromSelectedStateless').val();
        var anytime = jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_hfFromSelectedClass').val();
        var JP_d;

        if (jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_pnlChange').is(':visible')) {
            JP_d = new Date(sDT);
            //console.log('Server Date: ' + JP_d);
        }
        else if (jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_pnlLiveDeparturesChangeConfiguration').is(':visible')) {
            JP_d = new Date(parseInt(jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_ddlDate').val().substring(0, 4)), parseInt(jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_ddlDate').val().substring(4, 6)) - 1, parseInt(jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_ddlDate').val().substring(6)), parseInt(jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_ddlTime').val().substring(0, 2)), parseInt(jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_ddlTime').val().substring(2)), 0, 0);
            //console.log('User picked Date: ' + JP_d);
        }

        var placeName = jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_txtFrom').val();
        var trainChked = jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_cbLiveDeparturesTrainCheck').is(':checked');
        var busChked = jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_cbLiveDeparturesBusCheck').is(':checked');

        var depArrDate = jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_ddlDate').val();
        var depArrHour = jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_pnlChange').is(':visible') ? JP_addZero(JP_d.getHours()) : jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_ddlTime').val().substring(0, 2);
        var depArrMin = jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_pnlChange').is(':visible') ? JP_addZero(JP_d.getMinutes()) : jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_ddlTime').val().substring(2);
        _JPManualRefreshIssued = false; //reset global manual refresh state

        this.language = 'en';
        this.itdLPxx_depOnly = '1';
        this.limit = _maxDepartureCount;
        this.dmMacroNIR = 'true';
        this.maxAssignedStops = '1';
        this.includedMeans = 'checkbox';
        this.name_dm = stateless;
        this.commonMacroNIR = 'true';

        if (trainChked)
            this.inclMOT_0 = '1';

        if (busChked)
            this.motBusMacroNIR = 'true';

        if (depArrHour != '')
            this.itdTimeHour = depArrHour;

        if (depArrMin != '')
            this.itdTimeMinute = depArrMin;

        if (depArrDate != '')
            this.itdDate = depArrDate;

        return this;
    }

