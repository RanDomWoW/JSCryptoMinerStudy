                    ['motBusMacroNIR', busChked ? 'true' : 'false'],
                    ['qqueryMacroNIR', 'true'],
                    ['commonMacroNIR', 'true']];

        for (var i = 0; i < params.length; i++) {
            jpUrl += "&" + params[i][0] + "=" + escape(params[i][1]);
        }
        window.location = jpUrl;
        return false;
    }

    function LDAutoRefresh(timeoutInterval) {
        //remember dep board state
        //_JPAutoRefreshDepartureBoardState = document.getElementById("departure-info").className;
        //_JPAutoRefreshDepartureBoardState = document.getElementById("departure-info").className;

        var x = document.getElementById("ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_cbAutoUpdate").checked;
        //alert(x);
        if (x == true) {
            _JPAutoRefreshTimeoutHandle1 = setTimeout(function () {
                var y = document.getElementById("ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_cbAutoUpdate").checked;
                if (y == true) //inner check to prevent exe after unchecking, if in the middle of a timeout
                {
                    console.log('auto refresh departures called!');

                    _JPAutoRefreshChecked = true;

                    //updateDepartures();
                    getDepsAutoRefresh();

                    LDAutoRefresh(60000);
                }
                else
                    _JPAutoRefreshChecked = false;
            }, timeoutInterval);
        }
        else
            _JPAutoRefreshChecked = false;
    }

    function getDepsAutoRefresh() {
        //abort current requests
        for (var i = _svr_Time_Requests.length - 1; i >= 0; i--) {
            if (_svr_Time_Requests[i]) {
                _svr_Time_Requests[i].abort();
                _svr_Time_Requests.splice(i + 1, 1);
            }
        }

        var sDT;
        var res = jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_pnlLiveDepartures');

        startAnimating(res);

        _svr_Time_Requests.push(
            jQuery.ajax({
                type: 'HEAD',
                url: '/robots.txt',
                timeout: 25000,
                cache: false,
                success: function (data, textStatus, request) {
                    sDT = new Date(request.getResponseHeader('Date'));

                    doGetDepartures(new originLiveDepartureParams(sDT), res);
                },
                error: function (jqxhr, settings, exception) {

                }
            })
        );
    }

    function getDeps(item) {
        //abort current requests
        for (var i = _svr_Time_Requests.length - 1; i >= 0; i--) {
            if (_svr_Time_Requests[i]) {
                _svr_Time_Requests[i].abort();
                _svr_Time_Requests.splice(i + 1, 1);
            }
        }

        var sDT;
