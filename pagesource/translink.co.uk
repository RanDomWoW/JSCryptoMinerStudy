        jQuery(document).ready(function () {

            if (Cookies.get('EPi:NumberOfVisits') != undefined) {
                var epiNumberOfVisits = Cookies.get('EPi:NumberOfVisits');
                var res = epiNumberOfVisits.split(',');
                var visits = parseInt(res[0]);
                var expiryVisits = parseInt(jQuery("#ctl00_hfFavouritesToolipCookieExpiry").val());
                if (visits < expiryVisits) {
                    if (visits == 1) {
                        if (Cookies.get('tl_myfavourites_tooltip') == undefined || parseInt(Cookies.get('tl_myfavourites_tooltip')) !== visits) {
                            showFavsTooltip(visits);
                        }
                    }
                    else
                        jQuery(".hide").hide();
                }
                else {
                    if (visits % expiryVisits === 0) {
                        if (Cookies.get('tl_myfavourites_tooltip') == undefined || parseInt(Cookies.get('tl_myfavourites_tooltip')) !== visits) {
                            showFavsTooltip(visits);
                        }
                    }
                    else
                        jQuery(".hide").hide();
                }

            }

        });

        function showFavsTooltip(visits) {
            jQuery('.notification').delay("slow").fadeIn(1000);
            jQuery(".hide")
                .click(function () {
                    jQuery(".notification").fadeOut(100);
                    Cookies.set('tl_myfavourites_tooltip', visits); //Upon hiding, record/match visits for this session
                });
        }
    </script>
