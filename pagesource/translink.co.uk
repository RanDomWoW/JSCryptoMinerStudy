        var errMsg = "Invalid email address";
        var emailAddress = document.getElementById("ctl00_PageFooter1_txtEmailAddress").value;
        var cvEmailAddress = document.getElementById("ctl00_PageFooter1_cvtxtEmailAddress");

        var emailPat = "^[0-9a-zA-Z]+([0-9a-zA-Z]*[-._+])*[0-9a-zA-Z]+@[0-9a-zA-Z]+([-.][0-9a-zA-Z]+)*([0-9a-zA-Z]*[.])[a-zA-Z]{2,6}$";
        var EmailmatchArray = emailAddress.match(emailPat);

        if (EmailmatchArray == null) {
            cvEmailAddress.innerHTML = errMsg;
            args.IsValid = false;
        } else {
            args.IsValid = true;
        }
    }

