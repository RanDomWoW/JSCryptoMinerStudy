        function SetScrollerPosition() {
            if (document.getElementById(GetClientId("XPos")).value != "") {
                var x = document.getElementById(GetClientId("XPos")).value;
                var y = document.getElementById(GetClientId("YPos")).value;
                window.scrollTo(x, y);
            }
            document.getElementById(GetClientId("XPos")).value = "0";
            document.getElementById(GetClientId("YPos")).value = "0";
        }

        function GetScollerPosition() {
            var scrollX, scrollY;

            if (document.all) {

                if (!document.documentElement.scrollLeft)
                    scrollX = document.body.scrollLeft;
                else
                    scrollX = document.documentElement.scrollLeft;

                if (!document.documentElement.scrollTop)
                    scrollY = document.body.scrollTop;
                else
                    scrollY = document.documentElement.scrollTop;
            }
            else {
                scrollX = window.pageXOffset;
                scrollY = window.pageYOffset;
            }

            document.getElementById(GetClientId("XPos")).value = scrollX;
            document.getElementById(GetClientId("YPos")).value = scrollY;
        }   
    </script>
