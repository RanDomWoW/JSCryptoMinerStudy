        });

    });

    function planATripTransportMethodChange(objCheckBox) {

        var cbTrain = jQuery('#ctl00_cphBannerMainRegion_BannerContentArea_ctl01_cbPlanATripTrainCheck');
        var cbBus = jQuery('#ctl00_cphBannerMainRegion_BannerContentArea_ctl01_cbPlanATripBusCheck');

        if (objCheckBox.checked == false) //make sure the other one is checked
        {
            //train unchecked
            if (objCheckBox.id == 'ctl00_cphBannerMainRegion_BannerContentArea_ctl01_cbPlanATripTrainCheck') {
                cbBus.prop('checked', true);
            }

            //bus unchecked
            if (objCheckBox.id == 'ctl00_cphBannerMainRegion_BannerContentArea_ctl01_cbPlanATripBusCheck') {
                cbTrain.prop('checked', true);
            }
        }
    }
