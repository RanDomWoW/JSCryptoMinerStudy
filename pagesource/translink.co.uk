    function LDClearAutoRefresh() {
        try {
            _JPAutoRefreshChecked = false;
            clearTimeout(_JPAutoRefreshTimeoutHandle1);

            if (document.getElementById("ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_cbAutoUpdate").checked)
                jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_cbAutoUpdate').click();
        }
        catch (err) {
            console.log(err);
        }
    }

    jQuery(document).ready(function () {

        jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_cbLiveDeparturesTrainCheck').click(function () {
            LDClearAutoRefresh();
        });

        jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_cbLiveDeparturesBusCheck').click(function () {
            LDClearAutoRefresh();
        });

        jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_cbAutoUpdate').click(function () {
            jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_pnlChange').show();
            jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_pnlLiveDeparturesChangeConfiguration').hide();
            LDAutoRefresh(60000);
        });

        jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_btnAction').click(function () {
            _JPAutoRefreshDepartureBoardState = "collapsed";
            jQuery('#departure-info').removeClass('collapsed');
            jQuery('#departure-info').removeClass('expanded');
            jQuery('#departure-info').addClass(_JPAutoRefreshDepartureBoardState);

            getDeps(this);
            return false;
        });

        jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_btnSeeMore').click(function () {
            _JPAutoRefreshDepartureBoardState = "expanded";
            jQuery('#departure-info').removeClass('collapsed');
            jQuery('#departure-info').removeClass('expanded');
            jQuery('#departure-info').addClass(_JPAutoRefreshDepartureBoardState);

            jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_btnSeeMore').hide();
            jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_btnOffToMDV').show();
            return false;
        });

        jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_btnOffToMDV').click(function () {
            var sDT;
            jQuery.ajax({
                type: 'HEAD',
                url: '/robots.txt',
                timeout: 25000,
                cache: false,
                success: function (data, textStatus, request) {
                    sDT = new Date(request.getResponseHeader('Date'));

                    liveDeparturesGoToJourneyPlanner(this, sDT);
                },
                error: function (jqxhr, settings, exception) {

                }
            });

            return false;
        });

    });

    function liveDeparturesTransportMethodChange(objCheckBox) {

        var cbTrain = jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_cbLiveDeparturesTrainCheck');
        var cbBus = jQuery('#ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_cbLiveDeparturesBusCheck');

        if (objCheckBox.checked == false) //make sure the other one is checked
        {
            //train unchecked
            if (objCheckBox.id == 'ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_cbLiveDeparturesTrainCheck') {
                cbBus.prop('checked', true);
            }

            //bus unchecked
            if (objCheckBox.id == 'ctl00_MainRegion_ContentArea_ctl01_LeftArea_ctl00_cbLiveDeparturesBusCheck') {
                cbTrain.prop('checked', true);
            }
        }
    }

