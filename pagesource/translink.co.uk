    function planATripGoToJourneyPlanner(anchor, sDT) {

        //set configuration selection cookie if leaving for the JP
        var disValueToSave = jQuery('#ctl00_cphBannerMainRegion_BannerContentArea_ctl01_pnlPlanATripChangeConfiguration').css('display');
