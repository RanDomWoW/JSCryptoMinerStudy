<script>
    var storedFiles = [];

    document.addEventListener("DOMContentLoaded", init, false); 

    function init() 
    {
        document.querySelector('#file_attachment').addEventListener('change', handleFileSelect, false);
    }
    
    jQuery(document).ready(function() 
    {
        jQuery("#file_attachment").on("change", handleFileSelect);
    });
        
    function handleFileSelect(e) 
    {   
        var files = e.target.files, i = 0, j = files.length, file, reader;
        var filesArr = Array.prototype.slice.call(files);
 
        var good_size = false;
        var maxsize = 1024*1024*5;
        var iSize = 0;
        var el = document.getElementById('file_attachment');
        var inputFiles = document.getElementById('file_attachment').files;
        var fileAttachmentList = document.getElementById('file_attachment_list');

        filesArr.forEach(function(f) 
        {
            if (f.size<maxsize)
            {
                storedFiles.push(f);
            }
            var reader = new FileReader();
            reader.readAsBinaryString(f);
        });
    
        var error = '';
        for(index=0; index<inputFiles.length ;++index)
        {
            iSize = inputFiles.item(index).size;
            if(maxsize > iSize)
            {
                good_size = true;
            }        
            if((!good_size)&&(error==''))
            {
                error += "Размер файла не должен превышать 5 Мбайт.";
            }
        }
        if((storedFiles.length>5)&&(error==''))
        {
            error += "Максимальное количество прикрепляемых файлов равно 5 (пяти).";
            while (storedFiles.length>5)
            {
                var deleted=storedFiles.pop();
            }
        }
        if(error != '') 
        {
            el.value="";
            alert(error);
        }
        fileAttachmentList.innerHTML = "";
        for(i=0;i<storedFiles.length;++i)
        {
            if(i>0)
            {
                fileAttachmentList.innerHTML += ", ";
            }
            fileAttachmentList.innerHTML += storedFiles[i].name;
        }
    }
</script>
