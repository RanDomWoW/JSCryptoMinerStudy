$(window).resize(function(){
	var logo_width, parent_width, doc_width = $(window).width();

	var agreement_modal = $('#AgreementModal'), agreement_modal_width = 560;
	if(doc_width <= 480 ) {
		agreement_modal_width = 0.95 * doc_width; 
	}
	else {
		if(doc_width <= 650) {
			agreement_modal_width = 0.85 * doc_width; 
		}
	}
	agreement_modal.css('left', (doc_width - agreement_modal_width) / 2);
	if($('#AgreementModal.modal.in').length > 0) {
		agreement_modal_body = agreement_modal.find('.modal-body');
		agreement_modal_header = agreement_modal.find('.modal-header');
		header_padding_top = agreement_modal_header.css('padding-top');  
		header_padding_top = Number(header_padding_top.substr(0, header_padding_top.length - 2));
		header_padding_bottom = agreement_modal_header.css('padding-bottom');  
		header_padding_bottom = Number(header_padding_bottom.substr(0, header_padding_bottom.length - 2));
		max_height = jQuery(window).height() - agreement_modal_header.height() - header_padding_top - header_padding_bottom;
		margin_top = agreement_modal.css('margin-top');
		margin_top = Number(margin_top.substr(0, margin_top.length - 2));
		top_position = agreement_modal.css('top');
		top_position = Number(top_position.substr(0, top_position.length - 2));
		if(margin_top == 0) {
			max_height = max_height - top_position - 20;
		}
		else {
			max_height = max_height - top_position - margin_top - 20;
		}
		agreement_modal_body.css('max-height', max_height);	
	}

	if($(".section5-video").length > 0) {
		$(".section5-video").height($(".section5-video").width() * 0.75);	
	}
