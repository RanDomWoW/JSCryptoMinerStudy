(function($){
  $(document).ready(function(){
/*---------------------------------------------- создаем элементы и переменные ------------------------------------------------*/
  var base_mail = "1@nt-rt.ru",
  //основной div с формой
  $div_base_form = $('#base_form_cont'),
  $modal_form = $('#modal'),
  $select = $('#select_city'),
  $text_in_mod = $('#text_base_form');
  color = $('.section-title > h2 > span').css('background-color');

//заполняем select городами

$('.jpanelContent').find('.base_contact').each(function(i, e){
      var cont = $(e).attr('name').split('_');
      $('<option/>', {
        val: $(e).attr('name'),
        text: cont[0]
      }).appendTo($select);
    });



/*--------------- создаем элементы -----------------------------*/

/*--------------- функции -----------------------------*/
//замена текста с контактами
function contact_text(text){
  //создаем html разметку текста
  function text_form (in_text){
      if (in_text == 'Выберите город'){
        return '';
      }
      var ts = in_text.split('_');
      if (ts.length == 2){
        var html = '<div class="row mod_form"><div class="span12"><div class="span6 mod_form_txt"><p class="contact_form">Телефон:</p></div><div class="span6"><span class="tel_form" style="color: ' + color + ';">'+ ts[1] +'</span></div></div><div class"span12"><div class="span6 mod_form_txt"><p class="contact_form">Почта:</p></div><div class="span6"><a class="contact_form" href="mailto:'+ base_mail +'" title="'+ base_mail +'">'+ base_mail +'</a></div></div></div>';
        return html;
      }
      else return '';
    }
  var $text_in_mod = $('#text_base_form');
  if ($text_in_mod.html()){
    $text_in_mod.html('').append(text_form(text));
  }
  else{
    $text_in_mod.append(text_form(text));
  }
}
/*--------------- функции -----------------------------*/

/*--------------- вешаем события -----------------------------*/
    //событие для селекта, меням текст контакта
    $select.on('change', function(){
        if($(this).val() == 0) return false;
        //раз значение select изменилось, то... меняем текст в форме
        contact_text($(this).val());       
      });

    //вешаем событие на ссылки городов, что бы менялся селект и текст в форме на выбранный город
    $('.top_jplink').on('click', function(){
      $select.val($(this).attr('name'));
      contact_text($(this).attr('name'));
    });

    //событие для модального окна, что бы перетащить форму при её появлении
    $modal_form.on('show', function(e){
        //открепляем форму
        $bf = $div_base_form.detach();
        //вешаем её на модальное окно
        $bf.attr('class', 'span11').appendTo($('#modal-body'));
        return e;
        })
      //и всё обратно, как было
      .on('hide', function(){
        $bf = $div_base_form.detach();
        $bf.attr('class', 'span6').insertBefore($('#modal'));
      });

  mod_win = function mod(){
    $modal_form.modal('show');
  }

/*--------------- вешаем события -----------------------------*/
$('#select_city').removeAttr('color').css('color' , color);

  });
  
$(window).scroll(function(){
	var doc_width = Number($(document).width());
	if(doc_width <= 767) {
		var top = Number($(document).scrollTop());
		var header_bottom = Number($('.tp_h_line').height());
		if(top <= header_bottom) {
			if($(".ja-mainnav .nav-collapse").height() == 0) {
				$(".ja-mainnav .navbar .btn-navbar").click();
			}	
			
		}else{
			if($(".ja-mainnav .nav-collapse").height() > 0) {
				$(".ja-mainnav .navbar .btn-navbar").click();
			}	
		}
	}		
});

$(window).load(function(){
	// cоздаем таблицу для мобильного меню
	nav_ul = $(".ja-mainnav ul.nav");
	var nav_table = "", active_class_name = " class = 'current active' ", presentation_link = "";
	$(nav_ul).find("li").each(function(index) {
		if(index < 6) {
			if((index == 0) || (index == 3)) 
			{
				nav_table = nav_table + "<tr>";
			}
			if($(this).hasClass("active")) {
				is_active = active_class_name;
			}
			else {
				is_active = "";
			}
			nav_table = nav_table + "<td" + is_active + ">" + $(this).html() + "</td>";
			if((index == 2) || (index == 5)) 
			{
				nav_table = nav_table + "</tr>";
			}
		}
		else {
			if($(this).find("a.presentation-menu-item").length > 0) {
				presentation_link = '<div class="presentation-menu-item">' + $(this).html() + "</div>";
			}			
		}
	});
	after_nav_ul = "<table style = 'display: none' class = 'nav nav-mobile'><tbody>" + nav_table + "</tbody></table>";
	if(presentation_link != '') {
		after_nav_ul = after_nav_ul + presentation_link;
	}
	$(nav_ul).after(after_nav_ul);
