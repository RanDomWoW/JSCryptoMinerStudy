<div class="adv-video">
	<div id="video-trigger" data-show-delay="10000">
<!--	    <h4 class="small">Посмотрите Видео-<br/>презентацию о компании</h4>
	    <h4 class="small-collapsed">Видео-презентация о компании</h4> -->
	    <h4 class="normal">Посмотрите Видео-презентацию о компании</h4>
	    <h4 class="collapsed">Посмотрите Видео-презентацию о компании</h4>
	    <img class="play" src="/templates/ja_onepage/images/video-play.png" alt="">
  		<div class="click-zone"></div>
	    <div class="sound-switcher on"><span class="off-action">Выключить звук при просмотре</span><span class="on-action">Включить звук при просмотре</span></div>
	    <div class="close-trigger"><img src="/templates/ja_onepage/images/video-trigger-close.png" alt=""></div>
	    <div class="maximize-trigger"><img src="/templates/ja_onepage/images/video-trigger-maximize.png" alt=""></div>
	</div>
	
	<!-- Modal -->
	<div class="modal fade" id="adv-video-container" tabindex="-1" role="dialog" aria-labelledby="adv-video-label" data-ratio-x="960" data-ratio-y="720" data-mobile-res="767" data-scale-width="60" data-scale-width-mobile = "80" data-source1="/templates/ja_onepage/etc/video/ntrt_720.mp4">
	  <div class="modal-dialog" role="document">
	    <div class="modal-content">
	        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
			<img class="video-repeat-play" src="/templates/ja_onepage/images/repeat-play.png" alt="">
	    	<div class="modal-body">
				<div id="adv-video-player">This text will be replaced with a player.</div>
	    	</div>
	    </div>
	  </div>
	</div>		
	<script>

		(function($) {

			var poster = $(".adv-video #video-trigger");
			var container = $('#adv-video-container'), content = $('#adv-video-container .modal-body');
			var repeat_play = $('#adv-video-container img.video-repeat-play');
			
			if((poster.length > 0) && (container.attr("data-source1") !== undefined)) {
				var window_width = $(window).width(), window_height = $(window).height(), window_scale = window_width / window_height;
				var scale_height, container_width, container_height;
		
				var sources = [{ "file": container.attr("data-source1") }];
				if(container.attr("data-source2") !== undefined) {
					sources.push({ "file": container.attr("data-source2") });
				}	
				
				var ratioX = (container.attr("data-ratio-x") !== undefined) ? container.attr("data-ratio-x") : "1";
				var ratioY = (container.attr("data-ratio-y") !== undefined) ? container.attr("data-ratio-y") : "1";			
				var ratioYX = ratioY / ratioX, aspectratio = ratioX + ":" + ratioY; 
				var scale_width = container.attr('data-scale-width');
				if(scale_width === undefined) {
					scale_width = 50;
				}
				var scale_width_mobile = container.attr('data-scale-width-mobile');
				if(scale_width_mobile === undefined) {
					 scale_width_mobile = 75;
				}
				var mobile_res = container.attr('data-mobile-res');
				if(mobile_res === undefined) {
					 mobile_res = 0;
				}

				var poster_bottom_offset = 125, poster_bottom_offset_mobile = 85;

				var player = $("#adv-video-player");

				player.css('width', '');
				player.css('height', '');

				jwplayer("adv-video-player").setup({
				    "sources": sources,
				    "width": "100%",
				    "aspectratio": aspectratio,
				    "stretching": "fill",
				    "repeat": false,
				    "mute": false,
				    "preload": "auto",
				    "image": "/templates/ja_onepage/images/video_poster.jpg" 
				});

				var is_player_started = false;
				var video_play_completed = false;

				poster.click(function (event) {
					if($('#video-trigger .close-trigger:hidden').length > 0) {
						$('#video-trigger .maximize-trigger').hide();
						$('#video-trigger .close-trigger').show();
						$('#video-trigger h4.collapsed').hide();
						$('#video-trigger h4.normal').show();
						$(this).animate({'bottom':'0'}, 1200);							
					}
					else {
						yaCounter21392731.reachGoal('click_video_start');
						ga('send', 'event',  'Button', 'click_video_start');
						var container = $('#adv-video-container');
						window_width = $(window).width(), window_height = $(window).height();
						container_width = window_width * ((window_width > mobile_res) ? scale_width : scale_width_mobile) / 100;
						container_height = container_width * ratioYX;
						var top = (window_height - container_height - 5) / 2;
						if(top < 0 ) {
							top = 0;
						}						
						container.attr('style', 'top: ' + top + 'px !important');
						container.css('left', (window_width - container_width) / 2);
						container.css('width', container_width);
						container.css('height', container_height);
						content.width(container_width);
						content.height(container_height);
				
						$('#adv-video-container').modal('show');
					}
				});

				container.on('shown.bs.modal', function (event) {
					poster.hide();
					if(video_play_completed || (jwplayer("adv-video-player").getState() == 'paused') || (is_player_started == false)) {
						jwplayer("adv-video-player").play();								
						if(video_play_completed) {
							video_play_completed = false;
						}
						if(!is_player_started) {
							is_player_started = true;								
						}
					}
					
				});				
				
				function show_collapsed_poster(is_animated) {
					var poster = $(".adv-video #video-trigger");
					var bottom_offset = ($(window).width() > mobile_res) ? poster_bottom_offset : poster_bottom_offset_mobile;
					poster.find('.close-trigger').hide();
					poster.find('.maximize-trigger').show();
					poster.find('h4.normal').hide();
					poster.find('h4.collapsed').show();
					if(!is_animated) {
						poster.css('bottom', -bottom_offset);
					}
					if(poster.css('display') == 'none') {
						poster.show();	
					}
					if(is_animated) {
						poster.animate({'bottom':'-' + bottom_offset + 'px'}, 700);					
					}
				}
				
				container.on('hidden.bs.modal', function (event) {
					is_player_started = true;
					show_collapsed_poster(false);
					if(video_play_completed) {
						jwplayer("adv-video-player").stop();
						jwplayer("adv-video-player").setControls(true);
						repeat_play.css('opacity', 0);
					}
					else {
						if(jwplayer("adv-video-player").getState() == 'playing') {
							jwplayer("adv-video-player").pause();											
						}
					}
				});

				var video_duration = 0;

				jwplayer("adv-video-player").on('time', function (event) {
					if(video_duration == 0) {
						video_duration = jwplayer("adv-video-player").getDuration() - 1;
					}
					if((video_duration > 0) && (this.getPosition() >= video_duration)) {
						this.pause();
						this.setControls(false);
						repeat_play.show();
						repeat_play.animate({'opacity':'0.5'}, 2000);
						video_play_completed = true;
					}	
				});

				$('#video-trigger div.sound-switcher').click(function (event) {
					$(this).toggleClass('on').toggleClass('off');
					jwplayer("adv-video-player").setMute();
					event.stopPropagation();
				});

				$('#video-trigger .close-trigger').click(function (event) {
					show_collapsed_poster(true);
					event.stopPropagation();
				});

				repeat_play.click(function (event) {
					$(this).css('opacity', 0);
					$(this).hide();
					jwplayer("adv-video-player").stop();
					jwplayer("adv-video-player").play();
					jwplayer("adv-video-player").setControls(true);
					event.stopPropagation();
				});
				
				var timeout = (poster.attr("data-show-delay") !== undefined) ? poster.attr("data-show-delay") : "10000";
				window.setTimeout(function() {
					poster.animate({'bottom':'0'}, 2000);
				},timeout);
			}
		})(jQuery);

	</script>
</div>
