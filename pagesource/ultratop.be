@media screen and (max-width: 664px) {

	body {
    	width: 100%;
    }
    
    #logo {
    	width: 100%;
        height: 50px;
        background-size: 200px 50px;
        background-position: center top; 
        top: 40px;
    }
    
    #webcontainer {
    	width: 100%;
    }
    
    table {
    	width: 100% !important;
    }
    
    #sprachwahl {
    	width: 100% !important;
        top: 100px;
    }
    
    img {
