$('ul.nav li.dropdown, ul>li.dropdown-submenu').hover(function() {
  $(this).find('.dropdown-menu, .dropdown-submenu').stop(true, true).delay(200).fadeIn(500);
}, function() {
  $(this).find('.dropdown-menu, .dropdown-submenu').stop(true, true).delay(200).fadeOut(500);
});


$('a[href^="#"]').click(function () {
    $('html, body').animate({
        scrollTop: $('[name="' + $.attr(this, 'href').substr(1) + '"]').offset().top
    }, 700);

    return false;
});

var mainbottom = $('#mainNav').offset().top - 0;



$(window).on('scroll',function(){ 
  stop = Math.round($(window).scrollTop()); 
  if (stop > mainbottom) { 
     $('.navbar-shrink').addClass('fixed-top'); 
  } else { 
     $('.navbar-shrink').removeClass('fixed-top'); 
  } 
});
    
$(document).on('ready', function() {
 $(".variable").slick({
