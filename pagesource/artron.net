$(function () {
		function setcookie(name,value){
			var exp  = new Date(),
					year = exp.getFullYear(),
					month = exp.getMonth(),
					day = exp.getDate() + 1,
					newExp = new Date(year, month, day);
			document.cookie = name + "="+ escape (value) + ";expires=" + newExp.toGMTString();
		}

		function getcookie(name){
			var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)"));
			if(arr != null){
				return (arr[2]);
			}else{
				return "";
			}
		}

