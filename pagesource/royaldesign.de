    <script type="text/javascript">
        $(document).ready(function () {
            // Initiate slider
            (function() {
                var i = 0;
                var imglen = $('#startSlider img').length;

                // Only load Swipe when all images have been loaded
                $('#startSlider img').one('load', function() {}).each(function() {
                    i++;
                    if (i == imglen) {

                        // Create Swipe element
                        var swipeElem = document.getElementById('startSlider');
                        window.mySwipe = Swipe(swipeElem, {
                            auto: 6000,
                            speed: 600,
                            callback: function (index, element) { syncThumbnail(index) }
                        });

                        // Sync thumbnails when Swipe changes active image
                        function syncThumbnail(index) {
                            $('.swipe-thumbs .thumb').removeClass('active');
                            var activeThumb = $('.swipe-thumbs .thumb')[index];
                            $(activeThumb).addClass('active');
                        };

                        // Bind event delegate to the thumbnails
                        $('.swipe-thumbs .thumb').on('click', function() {
                            mySwipe.slide($(this).index());
                        })

                        // Add is-loaded class to show all slides
                        $(swipeElem).addClass('is-loaded');
                    }
                })
            })();

            $.ajax({
                type: "GET",
                dataType: "jsonp",
                cache: false,
                url: "https://api.instagram.com/v1/users/270736375/media/recent/?client_id=290468fdfe2e4a308a1a31b119df2f3b&count=20",
                success: function (data) {
                    if (data.meta.code == 200) {
                        $("#instagramFeed").css('display', 'block');

                        for (var i = 0; i < 20; i++) {
                            $("#instagramFeed").append("<div class='instagram-image item'><a href='" + data.data[i].link + "' target='_blank'><img src='" + data.data[i].images.thumbnail.url + "' alt=''></a></div>");
                        }
                        $(".instagram-image").wrapAll("<div id='owlCarousel' class='owl-carousel' />");

                        $("#owlCarousel").owlCarousel({
                            items: 6,
                            itemsDesktop: [1000, 8],
                            itemsDesktopSmall: [700, 6],
                            itemsTablet: [550, 4],
                            itemsMobile: [400, 3]
                        });

                        $("#owlCarousel").trigger('owl.play', 3000);
                    }
                }
            });
        });
    </script>

