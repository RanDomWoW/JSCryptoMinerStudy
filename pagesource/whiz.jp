		$("#topslider").append('<div id="paginationOuter"></div>');
		$("#paginationOuter").append('<ul class="pagination"></ul>');
		var slide_su = $("#topslider #container .slides div").length;
		for(var i=0; i < slide_su; i++){
			var i2=i+1;
			$(".pagination").append('<li><a href="#">'+i2+'</a></li>');
