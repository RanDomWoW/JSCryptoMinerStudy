<script>
function init(){
	var setHeight=0;
	if( $('#movie>ul').height()>setHeight ){	 setHeight=$('#movie>ul').height();	}
	if( $('#tv>ul').height()>setHeight ){		setHeight=$('#tv>ul').height();	}
	if( $('#star>ul').height()>setHeight ){		setHeight=$('#star>ul').height();	}

	setHeight=setHeight+5;

	$('#movie>ul').height(setHeight);
	$('#tv>ul').height(setHeight);
	$('#star>ul').height(setHeight);
};

$(function(){
	$('.popcorn-tab > .tab > li').click(function(){
		var index=$(this).index();

		//$('.popcorn-tab > .tab > li').removeClass('active');
		$(this).parent('ul').find('li').removeClass('active');
		$(this).addClass('active');

		$(this).parent('.tab').parent('.popcorn-tab').find('.panel').hide();
		$(this).parent('.tab').parent('.popcorn-tab').find('.panel:eq('+index+')').show();

		$('.listview-2').hide();
		$('.listview-2:eq('+index+')').show();
	});

	init();

	var rightPos=$('#section-3').height();
	$('#right-section').height(rightPos);

});
</script>

