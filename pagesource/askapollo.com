        function saveuserdata() {
            var name = document.getElementById("txtName").value;
            var email = document.getElementById("txtEmail").value;
            var mobileno = document.getElementById("txtMobileNumber").value;
            var message = document.getElementById("txtQuery").value;

			var isfocusset = false;
        if (name.trim() == "") {
            $("#lblValidationMsg").css({ 'display': 'block', 'color': 'red' });
            $("#lblValidationMsg").text("Enter Name");
            isfocusset = true;
            return false;
        }
        if (email.trim() == "") {
            $("#lblValidationMsg").css({ 'display': 'block', 'color': 'red' });
            $("#lblValidationMsg").text("Enter Email");
            isfocusset = true;
            return false;
        }
        else {
            var re = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
            if (!(re.test(email.trim()))) {
                $("#lblValidationMsg").css({ 'display': 'block', 'color': 'red' });
                $("#lblValidationMsg").text("Enter a valid 'Email'(Ex:test@yahoo.com)");
                isfocusset = true;
                return false;
            }
        }
        if (mobileno.trim() == "") {
            $("#lblValidationMsg").css({ 'display': 'block', 'color': 'red' });
            $("#lblValidationMsg").text("Enter Mobile Number");
            isfocusset = true;
            return false;
        }
        else {
            if (mobileno.trim() != "") {
                var re = /^(\+91-|\+91|0)?\d{10}$/;
                if (mobileno.trim() != "") {
                    if (!(re.test(mobileno.trim()))) {
                        $("#lblValidationMsg").css({ 'display': 'block', 'color': 'red' });
                        $("#lblValidationMsg").text("Enter a valid Mobile Number");
                        isfocusset = true;
                        return false;
                    }
                }
            }
        }
        if (message.trim() == "") {
            $("#lblValidationMsg").css({ 'display': 'block', 'color': 'red' });
            $("#lblValidationMsg").text("Enter Your Message");
            isfocusset = true;
            return false;
        }
if (name.trim() != "" && email.trim() != "" && mobileno.trim() != "" && message.trim() != "" && isfocusset == false) {
            $.ajax({
                type: "POST",
                url: "https://www.askapollo.com/physical-appointment/apollousers.asmx/AddApolloUsers",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify({ name: name, email: email, mobileNumber: mobileno, message: message }),
                dataType: "jsonp",
                success: function (data) {
                    document.getElementById("txtName").value = "";
                    document.getElementById("txtEmail").value = "";
                    document.getElementById("txtMobileNumber").value = "";
                    document.getElementById("txtQuery").value = "";
