// Return Form - temp
$j('#my-shipping-table tr:first-of-type strong:first-of-type').text('Drop Off at FedEx Location');

  // Change RETURN link to button on "My ORDERS" pg
  $j('.sales-order-history #my-orders-table .a-right .nobr a:nth-child(5)').html('<button class="returnbttn">RETURN</button>');

   // Insert 'Start Return' button on "MY DASHBOARD" and "MY RETURNS" pg
   $j('<div class="startrma-wrap"><a href="/sales/order/history/">&#10097; <button>Start A New Return</button></a></div>').insertBefore('.enterprise-rma-return-history .my-account, .customer-account-index .my-account').insertAfter('.customer-account-index .my-account');

   // Checkout - hide DOB and Gender
   $j('#billing-new-address-form .customer-dob').parents('.fields').hide();

   // Search Results Page - remove Size facet
   if (window.location.pathname == '/catalogsearch/result/') { $j("dt:contains('Size')").hide(); $j('dd.size').hide(); }

   // PDP
   $j('.product-view .slick-track [index=3]').insertAfter('.product-view .slick-track [index=0]');
   if (window.location.pathname.match(/gen-k-icon-knit/g)) { $j('.add-to-cart-buttons').append('<center><b>Runs large; order half size down.</b></center>'); }

   // PDP - change 'Unisex' to 'Mens' in size dropdown
