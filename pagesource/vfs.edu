<script>		

jQuery(function($){
		
	function getCookie(cname) {
		var name = cname + "=";
		var decodedCookie = decodeURIComponent(document.cookie);
		var ca = decodedCookie.split(';');
		for(var i = 0; i <ca.length; i++) {
			var c = ca[i];
			while (c.charAt(0) == ' ') {
				c = c.substring(1);
			}
			if (c.indexOf(name) == 0) {
				return c.substring(name.length, c.length);
			}
		}
		return "";
	}	
		
	function checkCookie() {
		var country_code = getCookie("country_code");
		if (['CO', 'BR', 'CL', 'PE', 'EC', 'AR', 'VE', 'UY', 'PY', 'SR', 'GY', 'GF', 'MX', 'GT', 'HN', 'SV', 'CR', 'PA', 'BZ', 'NI'].indexOf(country_code) >=0) {
			$("#latin").css("display","block");
			$("#latin").css("opacity","1");
		} else {
			$("#non-latin").css("display","block");
			$("#non-latin").css("opacity","1");
		}
	}	
	
	checkCookie();
					
	h = $(window).height();
	$w = $(window).width();
	
	//SLIDER CONTROLS//
	
    $('.bxslider').bxSlider({
		auto: true,
		speed: 1000,
		autoDelay: 1000,
		pause: 10000,
		adaptiveHeight: true,
		video: true,
		infiniteLoop:true,
		mode:'fade'
    });
	if (isMobile) {
		$("#homepage p").css("opacity", "1");	
	}

	//MOBILE VIEWPORTS FUNCTIONS//
		
	if ($w < 481 || isMobile) {									
		$("#homepage").addClass("mobile-layout");
	} else {
		$("#homepage").addClass("desktop-layout");
	}	
		
	//WINDOW LOAD FUNCTION//
	
	$(window).load(function() {
		
		if (!isMobile) {
		
		//LOAD FIRST VIDEO//
		
		$("#non-latin #slide-2 .desktop").append("<video laysinline='' autoplay='' muted='' id='vid' preload='auto' loop='' class='slider-vid' style='align-self:center; opacity:0; transition:1s opacity;'><source src='/sites/all/themes/vfs/img/hero/02-14-18/REVIEW_Social_Video_E05.mp4' type='video/mp4'></video>");	
		setTimeout(function() {$("#non-latin #slide-2 video").css("opacity","1");}, 500);
		$("#latin #slide-3 .desktop").append("<video laysinline='' autoplay='' muted='' id='vid' preload='auto' loop='' class='slider-vid' style='align-self:center;'><source src='/sites/all/themes/vfs/img/hero/02-14-18/REVIEW_Social_Video_E05.mp4' type='video/mp4'></video>");
		
		//LOAD VIDEOS NOT IF BROWSER IS FIREFOX//
	
		if(navigator.userAgent.toLowerCase().indexOf('firefox') == -1 ){	
			//alert("not firefox!");
			$("#non-latin #slide-3 .desktop").append("<video laysinline='' autoplay='' muted='' id='vid' preload='auto' loop='' class='slider-vid' style='align-self:center;'><source src='/sites/all/themes/vfs/img/hero/01-18-18/WRforGamesCarousel_Desktop_video-3.mp4' type='video/mp4'></video>");
			$("#non-latin #slide-6 .desktop").append("<video laysinline='' autoplay='' muted='' id='vid' preload='auto' loop='' class='slider-vid' style='align-self:center;'><source src='/sites/all/themes/vfs/img/hero/Creative Break_Desktop.mp4' type='video/mp4'></video>");
			$("#latin #slide-7 .desktop").append("<video laysinline='' autoplay='' muted='' id='vid' preload='auto' loop='' class='slider-vid' style='align-self:center;'><source src='/sites/all/themes/vfs/img/hero/Creative Break_Desktop.mp4' type='video/mp4'></video>");
			$("#latin #slide-4 .desktop").append("<video laysinline='' autoplay='' muted='' id='vid' preload='auto' loop='' class='slider-vid' style='align-self:center;'><source src='/sites/all/themes/vfs/img/hero/01-18-18/WRforGamesCarousel_Desktop_video-3.mp4' type='video/mp4'></video>");	
		}
		
		//FORCE PLAY SLIDER VIDEOS THAT DON'T START BY DEFAULT ON CERTAIN BROWSERS//	
		
		$('.slider-vid').get(0).play();
		
		if(navigator.userAgent.toLowerCase().indexOf('firefox') == -1 ){
			$('.slider-vid').get(1).play();
			$('.slider-vid').get(2).play();				
		}
		
		$('.icon.reveal').click(function() {
			if ($(this).hasClass("open")) {
				$(this).next().removeClass("testopacity");
			} else {
				$(this).next().addClass("testopacity");
			}
		});	
			
		}
		
		$(window).on("resize", function () {
		
			if (w < 481 && $("#homepage").hasClass("desktop-layout")) {
				$("#we-are .black-line").addClass("appear");
				setTimeout(function() {$("#we-are p").addClass("fade-in");}, 500);
			}
		});

	});
	
	//INITIATE MASONRY//

	$('.grid').masonry({
		itemSelector: '.grid-block',
		columnWidth: '.grid-sizer',
		percentPosition: true
	});
	
	$(".grid-block").mouseover(function() {
			$(this).find("figcaption").removeClass("testopacity");
		});
	$(".grid-block").mouseout(function() {
		$(this).find("figcaption").addClass("testopacity");
	});	
	
	//+ BOX FOR PROGRAM GRID//	
	
	$("#caption-grid figcaption").after("<div class='plus'></div>");
	
	if ($("#homepage").hasClass("desktop-layout")) {
		//SCROLL FUNCTIONS (if not ipad or iphone) //
		
		if (!isMobile) {
			
			setTimeout(function() {$("#homepage-hero-vid .white-line").addClass("appear");} ,500);	
			setTimeout(function() {$("#homepage-hero-vid p").addClass("fade-in");}, 1500);
			$(document).on('scroll', function() {
		
		    //INITIATE MASONRY WHEN GRID IS IN VIEWPORT//
		
			if($(this).scrollTop()>=$('#our-programs').position().top) {
				$('.grid').masonry({
					itemSelector: '.grid-block',
					columnWidth: '.grid-sizer',
					percentPosition: true
				});		
			}
		
			if($(this).scrollTop()>=$('#education-model').position().top + 500) {
				$('.grid').masonry({
					itemSelector: '.grid-block',
					columnWidth: '.grid-sizer',
					percentPosition: true
				});		
			}
			
			//NON-MOBILE VIEWPORTS FUNCTIONS//
				
			$("#hover-grid p").addClass("fade-in");
			$("#streams p").addClass("fade-in");
			$("#after-grad p").addClass("fade-in");
			$("#work p").addClass("fade-in");
			
				//SCROLL ANIMATIONS//
				
				if($(this).scrollTop()>=$('#homepage-hero-vid').position().top +250){
					$("#we-are .black-line").addClass("appear");
					setTimeout(function() {$("#we-are p").addClass("fade-in");}, 500);
				}
				if($(this).scrollTop()>=$('#homepage-hero-vid').position().top +600){
					$("#our-programs").addClass("darken");
					setTimeout(function() {$("#our-programs").addClass("darkened");}, 100)
				}
				if($(this).scrollTop()>=$('#homepage-hero-vid').position().top +750){
					$("#our-programs .white-line").addClass("appear");
					setTimeout(function() {$("#our-programs p").addClass("fade-in");}, 500);
				}
				if($(this).scrollTop()>=$('#streams').position().top){
					$("#alumni-success .white-line").addClass("appear");
					setTimeout(function() {$("#alumni-success p").addClass("fade-in");},1000);
				}
				if($(this).scrollTop()>=$('#after-grad').position().top - 75){
					$("#education-model").addClass('darken');
					setTimeout(function() {$("#education-model").addClass("darkened");}, 100)
				}			
				if($(this).scrollTop()>=$('#after-grad').position().top + 75){
					$("#education-model .white-line").addClass("appear");
					setTimeout(function() {$("#education-model p").addClass("fade-in");}, 1000);
				}
				if($(this).scrollTop()>=$('#work').position().top - 75){
					$("#facilities").addClass('darken');
					setTimeout(function() {$("#facilities").addClass("darkened");}, 100)
				}	
				if($(this).scrollTop()>=$('#work').position().top + 75){
					$("#facilities .white-line").addClass("appear");
					setTimeout(function() {$("#facilities p").addClass("fade-in");}, 1000);
				}
				if($(this).scrollTop()>=$('#facilities').position().top +300){
					$("#call-to-action .white-line").addClass("appear");
					setTimeout(function() {$("#call-to-action p").addClass("fade-in");}, 1000);
				}

			})
		
		}
		
		
	} else {
		$(document).on('scroll', function() {
			$('.grid').masonry({
				itemSelector: '.grid-block',
				columnWidth: '.grid-sizer',
				percentPosition: true
			});			
		});
	}
	
});
</script>

