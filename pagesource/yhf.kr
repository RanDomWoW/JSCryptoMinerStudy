	<script type="text/javascript">
	function isMobile() {
		var filter = "win16|win32|win64|mac|macintel";
		
		if( navigator.platform  ){
			if( filter.indexOf(navigator.platform.toLowerCase())<0 ){
				return true;
			}else{
				return false;
			}
		}
	}
    function PlayPauseMov() {
		var img1 = document.getElementById("img1");
		
		if(player.getPlayerState()==0 || player.getPlayerState()==2 || player.getPlayerState()==5) {
			img1.src= "/images/gate/pause_icon.png";
			player.playVideo();
		} else if(player.getPlayerState()==1) {
			img1.src= "/images/gate/play_icon.png";
			player.pauseVideo();
		}
	}
    function stopMov() {
		var img1 = document.getElementById("img1");
		
		if(player.getPlayerState()==1 || player.getPlayerState()==2) {
			img1.src= "/images/gate/play_icon.png";
			player.stopVideo();
		}
	}
    function muteMov() {
		var img2 = document.getElementById("img2");
		if(player.isMuted()) {
			img2.src= "/images/gate/speakerOn_icon.png";
			player.unMute();
		} else {
			img2.src= "/images/gate/speakerOff_icon.PNG";
			player.mute();
		}	
	}
    function PauseMov() {
		var img1 = document.getElementById("img1");
		
		if(player.getPlayerState()==1) {
			img1.src= "/images/gate/play_icon.png";
			player.pauseVideo();
		}
	}
	$(document).ready(function(){
		if(isMobile()) {
			location.href="/index_mobile.jsp";
		}
	});
	</script>
