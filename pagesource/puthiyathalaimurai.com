<script type="text/javascript">
$(window).ready(function() {
	$('.feedback, .advertise').click(function(){
		$(".feedback_response").html('');
		$('.feedback_title').html($(this).attr('title'));
		$('#ftype').val($(this).attr('data-id'));
		$('#feedback').modal('show');
	});
	$('.valid-msg-box-feedback').hide();
	$(window).scroll(function() {
		/* Scroll to top Start */
		if ($(this).scrollTop() > 100) {
			$('.scrollup').fadeIn();
		} else {
			$('.scrollup').fadeOut();
		}
	});
	$('.scrollup').click(function(){ $("html, body").animate({ scrollTop: 0 }, 600); return false; });
	$('.full-ad-close').click(function(){
		$(".lft-fullpage-ad, .rht-fullpage-ad").html(''); 
	});	
	
	$('.feedback_submit').click(function(){
		if($('#f_email').val()!='' && $('#feedback_comment').val()!=''){
			$(".feedback_response").html('');
			$(".feedback_submit").html('Please wait...');
			$.ajax({
				url:'http://www.puthiyathalaimurai.com/ajax/common.php?act=fed&do=feedback',
				type:'POST',
				data:$("#feedform").serialize(),
				success:function(result){
					$(".feedback_response").html(result);
					$('#f_email').val('');
					$('#f_mobile').val('');
					$('#feedback_comment').val('');
					$(".feedback_submit").html('SEND');
				}
			});
		}else{
			$(".feedback_response").html('Please enter all fields');
		}
		return false;
	});
	
	$('.newsletter_submit').click(function(){
		if($('#ns_email').val()!=''){
			$(".newsletter_response").html('');
			$(".feedback_submit").html('Please wait...');
			$.ajax({
				url:'http://www.puthiyathalaimurai.com/ajax/common.php?act=fed&do=nwsletter',
				type:'POST',
				data:$("#newletterform").serialize(),
				success:function(result){
					$(".newsletter_response").html(result);
					$('#ns_email').val('');
					$(".feedback_submit").html('REGISTER');
				}
			});
		}else{
			$(".newsletter_response").html('Please enter Valid Email Address');
		}
		return false;
	});
	
	$('#notify_close_btn').click(function(){
		$.ajax({
			url:'http://www.puthiyathalaimurai.com/ajax/common.php?act=notify&do=notify_close&ckie=',
			type:'POST',
			success:function(result){
				$(".notify-bottom").hide('slow');
			}
		});
		return false;
	});
	
	/*lazyload*/
	$("img.lazy").lazyload({ effect : "fadeIn" });
	
});

