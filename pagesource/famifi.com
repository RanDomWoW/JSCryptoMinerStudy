    <div id="fsSignupWidget" class="fsModality mobile-adopted" style="display: none">
  <div class="modal-wrapper">
    <div class="signup-start">
      <input type="button" class="cancel-mobile-button" onclick="$.fn.fsCloseModal();" value="Cancel" />
      <img class="logo" src="/images/logos/small_english.png" />
      <div class="h2">Create account</div>

      <form method="post" action="/signup" autocomplete="off">
        <input type="text" name="fullname" data-mark="Full Name" />
        <input type="text" name="username" data-mark="Username / Screen Name">
        <input type="text" name="email" data-mark="Your Email Address..." />
        <input type="password" name="password" data-mark="New Password" />
        <input type="password" name="passwordConfirm" autocomplete="off" data-mark="Confirm Password" />

        <a class="side-link" rel="nofollow" href="#" onclick="return fs.widget('fsLoginWidget');">Login to account</a>

        <input type="submit" value="Sign Up Now" data-active="Saving" />
        <div class="newsletter"><input type="checkbox" name="newsletter" CHECKED/><span>Keep me updated via email</span></div>

        <ul class="errors"></ul>

        <div class="ordivide"> -------------------------- or --------------------------</div>
        
        <a href="/login/facebook" rel="nofollow" class="facebook2">
