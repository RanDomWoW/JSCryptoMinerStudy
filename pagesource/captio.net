<script src="//cdn2.hubspot.net/hubfs/273205/scripts/jquery.cookie.js?t=1521192278809"></script>

<script>
    var cookieName = 'gviaje';
    var nMh = $.cookie(cookieName); // => no mostrar home
    var showPopup = true;
    var submitIt = false;
    //console.log("nom " + nMh);

    function popupCenterC() {
            var winH = $(window).height();
            var winW = $(window).width();

        if($('.pu-wrapper').height() > winH){
            //console.log('més gran, scroll');
                $('.pu-wrapper').css({
                    'height':'99%',
                    'position': 'fixed',
                    'top': 1,
                    'left': parseInt((winW / 2) - ($('.pu-wrapper').width() / 2)),
                    'overflow-y':'auto',
                    'overflow-x':'hidden'
                });
            
        } else {
            //console.log('més petit, ajustar');
                $('.pu-wrapper').css({
                    'height':'auto',
                    'position': 'fixed',
                    'top': parseInt((winH / 2) - ($('.pu-wrapper').height() / 2)),
                    'left': parseInt((winW / 2) - ($('.pu-wrapper').width() / 2)),
                    'overflow-y':'auto',
                    'overflow-x':'hidden'                    
                });            
        }
        
    }


    if (nMh == undefined) {
        //console.log('entra false noshow');

        function timer() {
            setTimeout(function () {
                $(document).on("mousemove", function (e) {
                    var scroll = $(window).scrollTop();
                   // console.log("mousemove " + cookieName);
                    if ((e.pageY - scroll) < 100 && $.cookie(cookieName) != 'gviaje' && showPopup) {
                        showPopup = false;
                        $('#puExitIntent').fadeIn();
                        $('#modalBk').fadeIn();
                        popupCenterC();
                    }
                });
            }, 5000);
        }
        timer();

        $(window).resize(function () {
            popupCenterC();
        });

        $('#modalBk').click(function (e) {

            $('#puExitIntent').fadeOut();
            $('#modalBk').fadeOut();


            if ($('#puExitIntent').is(":visible")) {

                if ($.cookie(cookieName) != 'gviaje' && submitIt == false) {
                    $.cookie(cookieName, 'nMh', {
                        expires: 15
                    });

                }
            }
        });

        $('#puExitIntent .creu').click(function () {
            $('#puExitIntent').fadeOut().hide();
            $('#modalBk').fadeOut().hide();

            if ($.cookie(cookieName) != 'gviaje' && submitIt == false) {
                $.cookie(cookieName, 'nMh', {
                    expires: 15
                });
            }
        });

    }

    if (nMh) {
        //console.log('entra true nMh');
        $('#puExitIntent').hide();
        $('#modalBk').hide();
    }
</script></span>
