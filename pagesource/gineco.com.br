	function labelInside(idCampo, label){
		$('#' + idCampo).val(label);
		$('#' + idCampo).on('blur', function(){
			if($('#' + idCampo).val() == ""){
				$('#' + idCampo).val(label);
			}
		});
		$('#' + idCampo).on('focus', function(){
			if($('#' + idCampo).val() == label){
				$('#' + idCampo).val('');
			}
		});
	}
	
