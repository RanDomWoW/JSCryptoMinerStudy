function refreshPubAds()
{
  var refreshAds = false;
  var idsOnPage = [];
  var adsOnPage = [];

 addLazyLoadedAttributeForLazyLoadAdsInView();

  _.each(googletag.pubads().getSlots(), function (slot) {
    var slotInfo = adjustAdDivPosition(slot.getSlotElementId());

    if ( slotInfo ) {
      idsOnPage.push(slotInfo.id);
      adsOnPage.push(slotInfo.slot);

      if ( slotInfo.refreshAds ) {
        refreshAds = slotInfo.refreshAds;
      }
    }
