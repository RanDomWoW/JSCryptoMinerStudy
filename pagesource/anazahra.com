<script type="text/javascript">	
	function initiate_components_effects(){
		$( ".picinside").mouseenter(function(){
			$(this).find(".boxtext").animate({ "height": "95px" }, "slow" );
			$(this).find(".boxtext11").animate({ "height": "90px" }, "slow" );
		});
		$( ".picinside").mouseleave(function(){
			$(this).find(".boxtext").animate({ "height": "25px" }, "slow" );
			$(this).find(".boxtext11").animate({ "height": "25px" }, "slow" );
		}); 
	}
	var nh_gal_pic_clk = 1;
	var rotate_gal_pics = true;
	$(document).ready(function(){
		setTimeout(function() {
			auto_rotate_pics();			
		}, 7000); 
	});
	
	function auto_rotate_pics(){		
		if(nh_gal_pic_clk == 4){
				nh_gal_pic_clk = 1; 
		} else {
			nh_gal_pic_clk = parseInt(nh_gal_pic_clk) + 1;
		}		
		if(rotate_gal_pics ==true){
			$("#ph_gal_li_"+nh_gal_pic_clk+" a").trigger("click"); 
		}
		setTimeout(function() {
			auto_rotate_pics();
		}, 7000);		 
	}
	
	function tog_gal_items(li_val,article){ 
		nh_gal_pic_clk = li_val; 
		var current_li = $("#ph_gal_"+li_val);
		var current_li_img = $("#ph_gal_img_"+li_val).val();
		var current_li_link = $("#ph_gal_link_"+li_val).val();
		var current_li_title = $("#ph_gal_title_"+li_val).html();
		var ph_gal_vid_url = $("#ph_gal_vid_url_"+li_val).val();
		var title_for_alt = current_li_title.replace(" ","-");
		
		for (var i =1;i<=4;i++){
			if(li_val == i){
				$("#ph_gal_li_"+i).addClass('selected');
				$("#ph_gal_"+i).show(); 
			}else{
				$("#ph_gal_li_"+i).removeClass('selected');
				$("#ph_gal_"+i).hide();
			}
		}		 
		$("#photo_video_place").html('<a href="'+current_li_link+'" title="'+current_li_title+'"><img src="'+current_li_img+'" class="photogalImgs" title="'+current_li_title+'" alt="'+title_for_alt+'" /></a>');
		
		 if(article == 'video'){  
			$("#homephotoGalleryHolder .more-bt .morebutton").attr('href',site_rel_path+'/video');  
			var video_new_url = $('#ph_gal_vid_url_'+li_val).val(); 			
		} else { 
			$("#homephotoGalleryHolder .more-bt .morebutton").attr('href',site_rel_path+'/pics');  
			$("#photo_video_place").html('<a href="'+current_li_link+'" title="'+current_li_title+'"><img src="'+current_li_img+'" class="photogalImgs" alt="'+title_for_alt+'" /></a>'); 
		}		 
		$('.gattext').html('<a class="anchorCss" href="'+current_li_link+'" title="'+current_li_title+'" alt="'+title_for_alt+'">'+current_li_title+'</a>');		 
	}
			
	function reload_photo_component(cat_id, widget_id) {		
		console.log(ajaxurl);
		var loader_img='<div class="loading"></div>';
		$("#cat").html(loader_img);	
		$('.galtitles ul li a').removeClass('selected');
		var is_all = false;
		var is_vids = false; 
		var article_type = 'photo';
		rotate_gal_pics = true;
		if(cat_id == "video"){
			article_type = cat_id; 
			is_vids = true;				
			$("#homephotoGalleryHolder .more-bt .morebutton").attr('href',site_rel_path+'/video');
		}		
		if(is_vids == false){			
			var c_all = cat_id.search(",");
			if(c_all != -1){ is_all = true;	}
			if(is_all == true){
				$("#vid_gal_title_all").addClass("selected");					
			} else {
				$("#vid_gal_title_"+cat_id).addClass("selected");			 
			}
		}else{
				$("#vid_gal_title_videos").addClass("selected");	
		}
		$.ajax({
			type: "POST",
			cache: false,
			async: true, 
			url: ajaxurl,	
			data:'action=reload_photo_articles_new_home_ajax_call&cat_id='+ cat_id+"&widget_id="+widget_id+"&article_type="+article_type,
			success:function(html){	
				nh_gal_pic_clk = 1;
				$('.galtitles ul li a').removeClass('selected');	
				$("#cat").html(html); 					
				if(is_vids == false){
					if(is_all ==true){
						$("#vid_gal_title_all").addClass("selected"); 
					} else {
						$("#vid_gal_title_"+cat_id).addClass("selected");			 
					}
				} else {
					$("#vid_gal_title_videos").addClass("selected");	
				}
			 }
			
		});
		return false;	
	}
</script>
