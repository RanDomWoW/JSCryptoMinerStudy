function reloadPhotoArticles(cat_id, widget_id) {		
		 
		var loader_img='<div class="loading"></div>';
		$("#cat").html(loader_img);	

		$('#photoGallery ul li').removeClass('active');
		var is_all = false;
		if(typeof cat_id == "number")
		{
			$("#vid_gal_title_"+cat_id).addClass("active");
		}
		else
		{
			$("#vid_gal_title_all").addClass("active");
			is_all = true;
		}
		
		 

		 $.ajax({
	    	type: "POST",
	    	cache: false,
			async: true, 
	   	 	url: 'http://www.anazahra.com'+'/'+'wp-admin'+'/admin-ajax.php',	
		   	data:'action=reload_photo_articles_ajax_call&cat_id='+ cat_id+"&widget_id="+widget_id,
		   	success:function(html)
			 {	   
		   		$("#cat").html(html);		
				if(is_all)
				{
					$("#photogal_footer").attr('href', $("#cat_footer_link_all").val());
				}
				else
				{	
					$("#photogal_footer").attr('href', $("#cat_footer_link").val());
				}
			 }
			
		});
				return false;
		
	} 

function subscribeUser(widget_id) {	
	
		var checkeditems = $('input:checkbox[name="checkboxCats[]"]:checked')
                       .map(function() { return $(this).val() })
                       .get()
                       .join(",");

		
		//commented previous code for radio buttons
		//var sel_option  = $("input[name='radioOptions']:checked").val();

		//new code  for check boxes starts here
		var sel_option  = $("input[name='radioOptions']:checked")
						.map(function() { return $(this).val() })
						.get()
						.join(",");
		//new code for checkboxes ends here


		if(typeof sel_option == 'undefined') 
			sel_option = 0;

		var email = $("#subscribeEmail").val(); 

		var display = $("#newsletterSlide").css('display');
		
		var expanded = 0;
		if(display == "block")
			expanded = 1;
	
		var ctrcode = $("#userCountry").val();

		var data_str  = 'action=subscribe_ajax_call&email='+ email+"&cat_ids="+checkeditems+"&sel_option="+sel_option+"&widget_id="+widget_id+"&expanded="+expanded+"&ctrcode="+ctrcode;


		if(is_em != ''){
 			data_str += '&is_em=1';
		}
		
		
		 $.ajax({
	    	type: "POST",
	    	cache: false,
			async: true, 
	   	 	url: 'http://www.anazahra.com'+'/'+'wp-admin'+'/admin-ajax.php',	
		    data:data_str,
		   	success:function(html)
			 {	   
				   $("#newsletter").html(html);		 

				   $("input.inputNewsletter").click(function(){
						$("#newsletterSlide").slideToggle("fast");
						$(this).toggleClass("active"); return false;
				  });

				  $("#subscribeEmail").focus(function(){
					 if($("#subscribeEmail").val() == "ادخلي عنوان بريدك هنا")
					 {
						$("#subscribeEmail").val('');
					 }
					 
				});

				$("#subscribeEmail").blur(function(){
					 if($("#subscribeEmail").val() == "")
					 {	
						 $("#subscribeEmail").val("ادخلي عنوان بريدك هنا");
					 }
				});

			 }
			
		});

		 return false;

		
	} 
    
	function updateSubscriptText()
	{
		var catchecks =new Array(); 
		var checkBoxes = $(".newsTable input:checkbox");    
		var i = 0;
			checkBoxes.each ( function () {
				if($(this).attr ( "checked" ))
				{
					catchecks[i] = 1;
				}
				else
				{
					catchecks[i] = 0;
				}
				i++;
			 });
		
		if($.inArray(1, catchecks) == -1)
		{
			 $("#btn_subscribe").val("تحديث ");//unsubscribe text
		}
		else
		{
			$("#btn_subscribe").val("ارسلي"); //subscribe
		}

	}

	
	function checkRadio (frmName, rbGroupName) { 
		 var radios = document[frmName].elements[rbGroupName]; 
		 for (var i=0; i <radios.length; i++) { 
		  if (radios[i].checked) { 
		   return radios[i].value; 
		  } 
		 } 
		 return 0; 
		} 


