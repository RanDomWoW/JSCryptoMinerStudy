<!-- Script per Box Link Network Meteogiornale -->
<script language="javascript" type="text/javascript">
$(document).ready(function() {
	$(".contenuto_tab-g").hide(); //Nascondo tutti i contenuti
	$("ul.tabs-g li:first").addClass("active").show(); //Attivo il primo tab
	$(".contenuto_tab-g:first").show(); //Mostro il primo contenuto (contenutio del primo tab)

	$("ul.tabs-g li").click(function() {
		$("ul.tabs-g li").removeClass("active"); //Rimuovo tuttle le classi "active"
		$(this).addClass("active"); //Attivo il tab selezionato
		$(".contenuto_tab-g").hide(); //Nascondo tutti i contenuti

		var tabAttivo = $(this).find("a").attr("href"); //Prendo il valore dell'attributo href del tab per attivare il realtivo contenuto
		$(tabAttivo).fadeIn(); //Faccio un Fade in per mostrare il contenuto dell'ID appena trovato
		return false;
	});
});
</script>
<!-- end script Box Link Network -->

