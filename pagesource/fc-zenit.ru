<script>
    $(window).load(function(){
        var RESPONSE_DATE_FORMAT = 'dd.MM.yyyy HH:mm:ss';
        var OUT_DATE_FORMAT = 'dd.MM.yyyy';
        var DISPLAY_THEMES_COUNT = 3;
        function loadTopThemes(limit){
            $.ajax({
                url             : 'https://forum.fc-zenit.ru/widget/comments.php?callback=?',
                data            : {cnt : limit},
                dataType        : 'jsonp',
                jsonpCallback   : 'popular_thems_list',
                cache           : true,
                success         : function(data) {
                    if (!data) return;
                    var holderBlock = $('.home-forum__list');
                    holderBlock.html('');
                    for(var key in data) {
                        var book = data[key];
                        book.post_date = formatDate(getDateFromFormat(book.post_date, RESPONSE_DATE_FORMAT), OUT_DATE_FORMAT);
                        book.posts = '{0} {1}'.format(book.posts, parseInt(book.posts).declination(['сообщение', 'сообщения', 'сообщений']));
                        holderBlock.append(ich.book(book));
                    }
                }
            });
        }
        loadTopThemes(DISPLAY_THEMES_COUNT);
    });
</script>
