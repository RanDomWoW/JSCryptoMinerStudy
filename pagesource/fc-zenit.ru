		<script>
			$(document).ready(function () {
				var btn = $('.js-acRegister');
				var formLogin = $('#loginPop');
				var $colorbox = $("#colorbox");
				var resizeWidth = 0;
				var resizeTimeout = 0;
				$(".js-acRegister").on("click", function() {
					$.ajax({
						type: 'GET',
						url: '/local/includes/areas/ru/footer_auth_form.php',
						success: function (data) {
							$("#loginPop").html(data);
							login();
						},
						error: function (xhr, str) {
							console.log('Error: ' + xhr.responseCode);
						}
					});
				});
			});
			function login() {
				var btn = $('.js-acRegister');
				var formLogin = $('#loginPop');
				var $colorbox = $("#colorbox");
				var resizeWidth = 0;
				var resizeTimeout = 0;
				var setElements = function () {
					applyEvents();
				};
				var applyEvents = function () {
					var $tabControls = $colorbox.find('.js-tabControl');
					var $tabs = $colorbox.find('.js-tab');
					var $checkboxes = $colorbox.find('.js-styledInput');
					var $select = $colorbox.find('select');
					var $mobile = $(".js-mobile-exchanger");
					var $jsbutton = $(".js-tabControl");
					// Tabs
					$tabControls.on('click', function (e) {
						var $this = $(this);
						e.preventDefault();
						$tabs
							.removeClass('m-active')
							.filter($this.attr('href'))
							.addClass('m-active');
						$tabControls.removeClass('m-active');
						$this.addClass('m-active');
						colorboxResizerRepeat();
						setTimeout(colorboxResizerRepeat, 200);
					});
					$mobile.on("click", function () {
						$(this).closest(".js-compareTabs").find(".mobile-exchanger-container").toggleClass('m-active');
					});
					$jsbutton.on("click", function () {
						var $parent = $(this).closest(".js-compareTabs").find(".m-selected-link"),
							text = $(this).html();
						$parent.html(text);
					});
					// Checkbox inputs
					$checkboxes.on('change', function () {
						if (this.checked) {
							$(this).parent().addClass('m-checked');
						} else {
							$(this).parent().removeClass('m-checked');
						}
					}).trigger('change');
					// Timezone Select
					$select.styler();
				};
				var colorboxResizerRepeat = function () {
					if ($colorbox.width() > document.documentElement.clientWidth) {
						$.fn.colorbox.resize({
							width: document.documentElement.clientWidth
						});
					} else {
						$.fn.colorbox.resize({
							width: resizeWidth
						});
					}
					$('#cboxLoadedContent').css({overflow: 'visible'});
				};
				var colorboxResizer = function (e) {
					resizeWidth = e.data.width;
					clearTimeout(resizeTimeout);
					resizeTimeout = setTimeout(colorboxResizerRepeat, 1000);
				};
				var width = 580;
				$.colorbox({
					html: formLogin.html(),
					maxWidth: width,
					reposition: false,
					scrolling: false,
					onOpen: function () {
					},
					onComplete: function () {
						setElements();
						resizeWidth = width;
						colorboxResizerRepeat();
						$(window).on("resize.cboxLogin", {width: width}, colorboxResizer);
						$('a[href="#popupRegisterTab"]').click()
					},
					onClosed: function () {
						$colorbox.off('click change submit');
						$(window).off("resize.cboxLogin");
					}
				});
			}
		</script>
