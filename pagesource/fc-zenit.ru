	<script type="text/javascript">
		$(function() {
			if (!$.cookie("source_object_id"))
			{
				
				var usr_session_id = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {var r = Math.random()*16|0,v=c=='x'?r:r&0x3|0x8;return v.toString(16);});
				$.cookie("source_object_id", usr_session_id);
			}
		});
		function sendToRestAdapter(options){
			var sap_data_container = {
			"Interactions": {
				"ID_ORIGIN" : options.id_origin || 'WEB',  
				"ID" : options.id || $.cookie("ecaids"),
				"RMID" : options.rmid || $.cookie("RMID"),
				"COMM_MEDIUM" : options.comm_medium || "WEB",
				"IA_TYPE" : options.ia_type || "ZWEBVISIT_MAIN",
				"TIMESTAMP" : options.timestamp || Math.floor(Date.now() / 1000),
				"SOURCE_OBJECT_TYPE" : options.source_object_type || "SITE_VISIT",
				"SOURCE_OBJECT_ID" : options.source_object_id || $.cookie("source_object_id"),
				"SOURCE_DATA_URL" : window.location.href,
				"DEVICE_FT" : navigator.userAgent,
				}
			};
			if (options.tags) {
				sap_data_container['Interactions']['TAGS'] = options.tags;
			}
			if (options.tags1) {
				sap_data_container['Interactions']['TAGS1'] = options.tags1;
			}
			if (options.tags2) {
				sap_data_container['Interactions']['TAGS2'] = options.tags2;
			}
			if (options.content_title) {
				sap_data_container['Interactions']['CONTENT_TITLE'] = options.content_title;
			}
			$.ajax({
				url: "https://fc-zenit.ru/RESTAdapter/site/interactions",
				type: "POST",
				dataType: 'json',
				crossDomain: true,
				data: JSON.stringify(sap_data_container)
			});
			
		}
		
