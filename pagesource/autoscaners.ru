        idBlockLogo.children(".prev_sl").on("click", function () {
            var margin = blockWidthLogo;
            var currentLogo = parseInt(listLogo.css("margin-left"));
            if (currentLogo == -margin) {
                $(this).hide();
            }
            margin = currentLogo + margin;
            listLogo.animate({marginLeft: margin + "px"}, 200);
            idBlockLogo.children(".next_sl").show();
