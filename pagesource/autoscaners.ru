        var block_width = list.children('.group_img').width();
        var groupCount =  list.children('.group_img').length;

        idBlock.children(".prev_sl").on("click", function () {
            var margin = block_width;
            var current = parseInt(list.css("margin-left"));
            if (current == -margin) {
                $(this).hide();
            }
            margin = current + margin;
            list.animate({marginLeft: margin + "px"}, 200);
            idBlock.children(".next_sl").show();
        });

        idBlock.children(".next_sl").on("click", function () {
            var margin = -block_width;
            var current = parseInt(list.css("margin-left"));
            var margin0 = (current + margin );

            list.animate({marginLeft: margin0 + "px"}, 200);
            idBlock.children(".prev_sl").show();
            if (current <= ((groupCount - 5) * (margin))) {
                $(this).hide();
            }
        });
    });
</script>

