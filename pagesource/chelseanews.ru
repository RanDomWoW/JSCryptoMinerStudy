        $(document).on("click", ".tabs_menu a", function(e) {
            e.preventDefault();
            $('.tabs_menu .active').removeClass('active');
            $(this).addClass('active');
            var tab = $(this).attr('href');
            $('.tab').not(tab).css({'display':'none'});
            $(tab).fadeIn(400);
        });
            
        $(document).on("click", ".pop_menu a", function(e) {
            e.preventDefault();
            $('.pop_menu .actived').removeClass('actived');
            $(this).addClass('actived');
            var tabs = $(this).attr('href');
            $('.pop_box .tabs').not(tabs).css({'display':'none'});
            $(tabs).fadeIn(400);
        });    
	});
