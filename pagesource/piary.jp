<script type="text/javascript">
  $(document).ready(function(){
	// $('.cateList').hide().find('p.ct:first').addClass('ck').find('input[type="radio"]').attr('checked','checked');
	$('.cateList').hide();
	$('.searchSelect').not('#searchSelect1').hide();

	$('.searchSelect').click(function(){
		// $('.cateList').stop(true,true).delay(100).slideDown(100);
		$(this).children('.cateList').stop(true,true).delay(100).slideDown(100);
	}).mouseleave(function(){
		// $('.cateList').stop(true,true).fadeOut(100);
		$(this).children('.cateList').stop(true,true).fadeOut(100);
	});


	$('.cateList p.ct').live('click',function(){
		$('input[type="radio"]').removeAttr('checked').parent().removeClass('select');
		$(this).addClass('select');
		$('input[type="radio"]',this).attr('checked','checked');
		// $('.searchSelect p.head span').html($('label',this).html());
		$(this).parents('.searchSelect').find('p.head span').html($('label',this).html());
		$('.cateList').stop(true,true).fadeOut(100);

	});
	$('.cateList p.allct').click(function(){
		$(this).parents('.searchSelect').nextAll('.searchSelect').hide();
		$('.cateList p.ct').not('.allct').children('input[type="radio"]').removeAttr('checked').parent().removeClass('select');
		$('#searchSelect2').hide();
		$('#searchSelect3').hide();
	});
	$('#searchSelect1 p.ct:not(.allct)').click(function(){

		$('#searchSelect2').find('div.cateList').children().remove();

		var categ = $('input[type="radio"]',this).val();
		$('#getTopSearchFormChildren').children().each( function() {
			var $clone = $(this).clone();
			var kocateg = $(this, 'p').attr('parent');
			if ( categ == kocateg )
				$('#searchSelect2').find('.cateList').append($clone);
		});
		$('#searchSelect2').show();
		$('#searchSelect2').find('p.head span').html('カテゴリーを選択');
		$('#searchSelect3').find('p.head span').html('カテゴリーを選択');
		$('#searchSelect3').hide();

	});

	$('#searchSelect2 p.ct').live('click',function(){
		$('#searchSelect3').find('div.cateList').children().remove();
		var categ = $('input[type="radio"]',this).val();
		var cnt = 0;
		$('#getTopSearchFormChildren').children().each( function() {
			var $clone = $(this).clone();
			var kocateg = $(this, 'p').attr('parent');
			console.log( categ.split('/')[1] + "---" + kocateg );
			if ( categ.split('/')[1] == kocateg ) {
				cnt++;
				$('#searchSelect3').find('.cateList').append($clone);
			}
		});
		if ( cnt > 0 ) $('#searchSelect3').show();
	});
  });
</script>



