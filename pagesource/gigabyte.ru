function CheckIsIE() {
var ua = window.navigator.userAgent;
var msie = ua.indexOf('MSIE ');
if (msie > 0) {
// 回傳版本 <=10 的版本
var getIeOld = parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)), 10);
switch(getIeOld) {
case 8:
window.isIE8 = true;
//alert("8");
break;
case 9:
window.isIE9 = true;
//alert("9");
break;
case 10:
window.isIE10 = true;
//alert("10");
break;
}
window.isIE = true;
}
var trident = ua.indexOf('Trident/');
if (trident > 0) {
// 回傳版本 >=11 的版本
var rv = ua.indexOf('rv:');
var getIe11 =  parseInt(ua.substring(rv + 3, ua.indexOf('.', rv)), 10);
if (getIe11 === 11) {
window.isIE11 = true;
window.isIE = true;
}
}
var edge = ua.indexOf('Edge/');
if (edge > 0) {
// 判斷Edge
var getIeEdge = (parseInt(ua.substring(edge + 5, ua.indexOf('.', edge)), 10));
if (getIeEdge === 11) {
window.isIE11 = true;
window.isIE = true;
//alert(window.navigator.userAgent);
}
}
};
