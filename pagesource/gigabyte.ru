function readCookieUrl() {
var cookie = document.cookie;
var isRember = false;
var isRwdGlobalDropdown = false;
if (cookie.indexOf('RemberDefaultUrl') > -1) {
var temp = cookie.split(';');
var strUrl = "";
for (var i = 0; i < temp.length; i++) {
if (temp[i].indexOf('RemberDefaultUrl') > -1) {
var test = temp[i].split(',');
if (test[0].indexOf('Yes') > -1) {
isRember = true;
} else if (test[0].indexOf('RWDGlobalDropdown') > -1) {
isRwdGlobalDropdown = true;
}
strUrl = test[1].replace("CookieDefaultUrl=", "");
}
}
//Add RWD select Country dropdown list "Global" into cookie "RWDGlobalDropdown" for redirect to 'www.gigabyte.com' instead of  'www.gigabyte.com/lauguage'
//Add By Cody.Yang 2017/05/08
if (!isRwdGlobalDropdown) {
if (location.hostname !== strUrl && isRember) {
location.href = "http://" + strUrl;
} else if (location.hostname !== strUrl && !isRember) {
location.href = "/Language";
}
}
}
if (cookie.indexOf('RemberDefaultUrl') <= -1 && !isRwdGlobalDropdown) {
location.href = "/Language";
}
}
