var setBannerRand = function(sourceJson) {
var targetJson = [];
while (targetJson.length < sourceJson.length) {
var randIndex = parseInt(Math.random() * sourceJson.length);
//console.log(randIndex);
var randJson = sourceJson[randIndex];
if (randJson.IsRand === false) {
targetJson.push(sourceJson[randIndex]);
randJson.IsRand = true;
}
}
//console.log('%o', targetJson);
return targetJson;
};
var setBannerTypeJson = function(objectType, sourceJson) {
var targetJson = [];
var textIndex = "";
switch (objectType.toLowerCase()) {
case "big":
textIndex = "BigImage";
break;
case "small":
textIndex = "SmallImage";
break;
}
if (textIndex !== "") {
$.each(sourceJson, function(key, val) {
if (val[textIndex] !== "") {
val.IsRand = false; // default IsRand
val.IsSet = false; // default IsSet
targetJson.push(val);
}
});
}
//console.log('%o', targetJson);
return targetJson;
}
