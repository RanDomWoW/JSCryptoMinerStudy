var setA1TopBottomBanner = function(tempA1, isRwd) {
var intA1Length = tempA1.A1Json_all.length;
//先確認設定A1 Top / Bottom
for (var i = 0; i < intA1Length; i++) {
switch (tempA1.A1Json_all[i].Position) {
case 1:
tempA1.A1Id_Top.push(tempA1.A1Json_all[i].Seq);	//偵錯用,可直接看ID
tempA1.A1Json_Top.push(tempA1.A1Json_all[i]);
tempA1.A1Id_Selected.push(parseInt(tempA1.A1Json_all[i].Seq));
break;
case 2:
//大於兩顆才強制擺於Bottom
if (!isRwd && intA1Length > 1 ) {
tempA1.A1Id_Bottom.push(tempA1.A1Json_all[i].Seq);	//偵錯用,可直接看ID
tempA1.A1Json_Bottom.push(tempA1.A1Json_all[i]);
tempA1.A1Id_Selected.push(parseInt(tempA1.A1Json_all[i].Seq));
}
break;
}
};
