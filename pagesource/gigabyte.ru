function setCookie(isRwd) {
var d = new Date();
//d.setTime(d.getTime() + (1 * 24 * 60 * 60 * 1000));
d.setTime(d.getTime() + (1 * 30 * 60 * 1000));
//d.setTime(d.getTime() - 1);刪除cookie
var expires = "expires=" + d.toGMTString();
//document.cookie = "isRwd=true" + "; " + expires + ';domain=' + "www.gigabyte.com" + '; path=/';
document.cookie = "isRwd=" + isRwd + "; " + expires + ';domain=' + "www.gigabyte.ru" + '; path=/';
location.reload();
}
function getRwdCookie() {
var cookie = document.cookie;
if (cookie.indexOf('isRwd') > -1) {
var cookies = cookie.split(';');
for (var i = 0; i < cookies.length; i++) {
if (cookies[i].indexOf('isRwd') > -1) {
var value = cookies[i].split('=');
if (value[1] === "false") {
$(".topBg").removeClass("Rwd");
} else {
$(".topBg").addClass("Rwd");
}
}
}
}
/*console.log(cookie);
console.log(cookie.indexOf('isRwd'));*/
}
