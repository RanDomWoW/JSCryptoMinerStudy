a.A2Ck_Selected.push(a.A2JsonAuto_all[randomIndex].Classkey);
}
var arrCookie = document.cookie.split(';');
var lenth = arrCookie.length;
for (var j = 0; j < lenth; j++) {
if (arrCookie[j].indexOf('ckIndex'+ a.A2JsonAuto_all[randomIndex].Classkey +'=')>-1) {
var ckBk = arrCookie[j].replace('ckIndex'+ a.A2JsonAuto_all[randomIndex].Classkey +'=', "").trim();
a.A2JsonAuto_all[randomIndex].AutoBackGroundUrl = '/common/images/templates/2017/auto-bg'+ ckBk +'.jpg';
break;
}
}
var isNoCookie = true;
for (var r = 0; r < lenth; r++) {
if (arrCookie[r].indexOf('ckIndex'+ a.A2JsonAuto_all[randomIndex].Classkey +'=')>-1) {
isNoCookie = false;
