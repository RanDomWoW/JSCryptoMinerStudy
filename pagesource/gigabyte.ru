if (tempA1.A1Id_Top.length ===	0) {
setRadomBannerA1(tempA1, tempA1.A1Id_Top, 1);
}
//Rwd 版面將 Bottom 換回 Top Banner 區塊
if (isRwd) {
for (var r = 0; r < intA1Length; r++) {
switch (tempA1.A1Json_all[r].Position) {
case 2:
if (tempA1.A1Id_Selected.indexOf(parseInt(tempA1.A1Json_all[r].Seq)) < 0) {
tempA1.A1Id_Top.push(tempA1.A1Json_all[r].Seq);	//偵錯用,可直接看ID
