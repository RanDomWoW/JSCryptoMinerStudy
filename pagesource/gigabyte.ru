//set pc sub menu li height
function setmenuCss(thisObj, classname, num) {
var liMenu = thisObj.find("." + classname);
var heightCss = 0;
var submenuWid = 0;
for (var i = 0; i < liMenu.length; i++) {
submenuWid = submenuWid + liMenu.eq(i).outerWidth();
if (i % num == 0) {
heightCss = 0;
}
if (liMenu.eq(i).height() > heightCss) {
heightCss = liMenu.eq(i).height();
}
if ((i + 1) % num == 0) {
for (var j = 0; j < num; j++) {
liMenu.eq(i - j).css("height", heightCss + "px");
}
}
}
}
