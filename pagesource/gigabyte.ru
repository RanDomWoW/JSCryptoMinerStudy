//	A2AutoBk_Selected:[],
//	intLimit:limit
//});
for (var i = 0; i < limit; i++) {
var randomIndex = parseInt(Math.random() * a.A2JsonAuto_all.length);
if (a.A2JsonAuto_all[randomIndex].Seq) {
if (a.A2Id_Selected.indexOf(parseInt(a.A2JsonAuto_all[randomIndex].Seq)) < 0
&& a.A2Ck_Selected.indexOf(parseInt(a.A2JsonAuto_all[randomIndex].Classkey)) < 0  ) {
