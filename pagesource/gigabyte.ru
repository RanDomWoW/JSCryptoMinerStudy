tempStr =
tempStr.substring(0,beginIndex) //頭
+tempStr.substring(beginIndex,beginIndex+ciKeyword[k].length).toUpperCase() //符合字串
+tempStr.substring(beginIndex+ciKeyword[k].length,tempStr.length); //尾
}
}
str = tempStr;
return str;
},
//限制英文長度
limitEn: function(str, maxNumber, ciKeyword) {
