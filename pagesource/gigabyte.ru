//限制中文GIGABYTE全大寫
limitTwHkGIGABYTE: function(str, ciKeyword) {
var tempStr = str;
for (var k = 0; k < ciKeyword.length; k++) {
if (tempStr.toLowerCase().indexOf(ciKeyword[k]) > -1) {
//var tempReg=new RegExp(ciKeyword[k],"g");   //加上"g"為全部取代
//tempStr=tempStr.replace(tempReg ,ciKeyword[k].toUpperCase());  //取代為CI全大寫
