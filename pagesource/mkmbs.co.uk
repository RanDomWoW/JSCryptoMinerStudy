			function DisableLink(link, usertype)
			{
				if ($(link).length>0)
				{
					$(link).attr('href', '#');
					$(link).removeClass('requiredLogin');
					$(link).click(function () {
						if (typeof(usertype) != "undefined" && usertype != "WEBCASH")
						{
							showGeneralMessage("No Access Right", "You need to be an Account Administrator to be able to view this page!", 400, 150);
						}
						else
						{
							showGeneralMessage("No Access Right", "As you have a Cash account - you are unable to view this area.", 400, 150);
						}
					});
				}
			}

			function registerRequireLoginEvent()
			{
				jQuery('div#manageMKMAccount a').each(function () {jQuery(this).addClass('requiredLogin');}); 
jQuery('ul#accountLinks a').each(function () {jQuery(this).addClass('requiredLogin');}); 


				jQuery('.requiredLogin').each(function () {

					if (typeof (jQuery(this).attr('BindOnClickEvent')) == 'undefined') {
						var redirectlink;

						if (jQuery(this).attr('href')) {

							redirectlink = jQuery(this).attr('href');
							//admin user
							if('' != '' && 'False' != 'True' && $.inArray(redirectlink, accountAdminPages) > -1){
								DisableLink(this);   
								jQuery(this).attr('BindOnClickEvent', 'true');
								return;
							}   
							
							if ('' != '' && 'True' == 'True' && $.inArray(redirectlink, cashAccountPages) > -1) {
								DisableLink(this);   
								jQuery(this).attr('BindOnClickEvent', 'true');
								return;     
							}                         

							if (redirectlink.indexOf("javascript") >= 0) {
								redirectlink = window.location.pathname;
								//jQuery(this).attr('href', '');
							}
						}
						else if (jQuery(this).attr('src')) {
							redirectlink = jQuery(this).attr('src');
						}
						else if (jQuery(this).attr('onclick')) {
							redirectlink = window.location.pathname;
							//jQuery(this).attr('onclick', '');
						}
						else {
							redirectlink = window.location.pathname;
						}
						if ( $.inArray(redirectlink, excludePages) <= -1)
						{
							jQuery(this).attr('returnUrl', redirectlink);
							jQuery(this).attr('referrerUrl', window.location.pathname);

							jQuery(this).attr('BindOnClickEvent', 'true');

							jQuery(this).bind("click", function (event) {
								if ('False' == 'False') {
									event.preventDefault();

									var extraquery = '';
								
									if ($(this).attr('title') == 'Quote For Collection' || $(this).attr('title') == 'Add For Collection') {
										var quantity = $(this).closest('.basketboxarea').find('input').val();
										var productCode = $(this).closest('.basketboxarea').attr("code");

										extraquery = '&c=' + productCode + '&q=' + quantity + '&t=col';
									}

									if ($(this).attr('title') == 'Quote For Delivery' || $(this).attr('title') == 'Add For Delivery') {
										var quantity = $(this).closest('.basketboxarea').find('input').val();
										var productCode = $(this).closest('.basketboxarea').attr("code");

										extraquery = '&c=' + productCode + '&q=' + quantity + '&t=del';
									}

									window.location = '/Membership/Login/?returnUrl=' + redirectlink + extraquery;
								}
							});
						}
					}
				});
			}

