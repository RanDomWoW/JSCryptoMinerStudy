			function BindSessionTimeOut()
			{
				var timeoutSec = "1200";
				var timeoutThresholdSec = 60;
				$(document).bind("idle.idleTimer", function () {
					// function you want to fire when the user goes idle
					$.timeoutDialog({
						timeout: timeoutSec,
						countdown: timeoutThresholdSec,
						keep_alive_url: '/services/MyAccountServices.asmx/KeepUrlAlive',
						logout_redirect_url: '/Templates/MKM/Pages/Login/Relogin.aspx?loginUrl=/Membership/Login/',
