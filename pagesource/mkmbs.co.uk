<div id="signup">
    <script type="text/javascript">
    //<![CDATA[
        var signUpEmailText = 'enter your email address here';
        var signUpMobileNo = 'enter mobile number';

        jQuery(document).ready(function () {
            jQuery('#emailsignup').val(signUpEmailText);
            jQuery('#mobilesignup').val(signUpMobileNo);
        });

        function signUp() {
            var emailAddress = jQuery('#emailsignup').val();
            var mobile = jQuery('#mobilesignup').val();

            if (emailAddress == signUpEmailText && mobile == signUpMobileNo) {
                addGoogleTracking('EmailSignup-EmailFailure');
                showGeneralMessage("Sign Up", "Please enter your email address or mobile number.", 400, 150);
                return;
            }
            if (emailAddress != signUpEmailText) {
                if (!isValidEmail(emailAddress)) {
                    addGoogleTracking('EmailSignup-EmailFailure');
                    showGeneralMessage('Error', 'Please enter a valid Email address.', 400, 150);
                    return;
                }
            }

            if (mobile == signUpMobileNo) {
                mobile = '';
            }

            jQuery.ajax({
                type: "POST",
                contentType: "application/json; charset=utf-8",
                url: "/Services/EmailSignUpService.svc/AddSignUpDetails",
                data: '{ "emailAddress":"' + emailAddress + '", "mobileNumber":"' + mobile + '" }',
                dataType: "json",
                success: function (msg) {
                    if (msg.IsSuccess) {

                    	addGoogleTracking('EmailSignup-Success');
                        showThankYouMessage('for signing up for all our latest news and promotions.');
                        jQuery('#emailsignup').val(signUpEmailText);
                        jQuery('#mobilesignup').val(signUpMobileNo);
                        //jQuery('#emailsignupthankyou #message').text('Thank you for registering.');
                        //jQuery('#emailsignupthankyou').dialog('open');
                    }
                    else {
                        addGoogleTracking('EmailSignup-EmailFailure');
                        showGeneralMessage("Error", msg.Message, 400, 150);
                    }
                },
                error: function (xhr, data, err) {
                    addGoogleTracking('EmailSignup-EmailFailure');
                    showGeneralMessage("Error", err, 400, 150);
                }
            });
        }

        function checkEmailField(inputBox) {
            if (inputBox.value.search(/^\w+\@\w+\.\w+$/) >= 0 || inputBox.value.length == 0 || inputBox.value == signUpEmailText) {
                jQuery(inputBox).attr('style', 'border-color:#fff;');
            } else {
                jQuery(inputBox).attr('style', 'border-color:#f00;');
            }
        }

        function checkMobileField(inputBox) {
            if (inputBox.value.search(/\d+$/) >= 0 || inputBox.value.length == 0 || inputBox.value == signUpMobileNo) {
                jQuery(inputBox).attr('style', 'border-color:#fff;');
            } else {
                jQuery(inputBox).attr('style', 'border-color:#f00;');
            }
        }
    //]]>
    </script>

    <fieldset>
        <input id="emailsignup" name="emailsignup" type="text" value="" maxlength="40" onkeypress="if ($(this).is(&#39;:focus&#39;) &amp;&amp; event.keyCode == &#39;13&#39;) { event.preventDefault ? event.preventDefault() : event.returnValue = false; $(&#39;#mobilesignup&#39;).focus(); }" onkeyup="checkEmailField(this);" onfocus="if(this.value==signUpEmailText){this.value=&#39;&#39;}" onblur="if(this.value==&#39;&#39;) {this.value=signUpEmailText}" />
        <input id="mobilesignup" name="mobilesignup" type="text" value="" maxlength="40" onkeypress="if ($(this).is(&#39;:focus&#39;) &amp;&amp; event.keyCode == &#39;13&#39;) { event.preventDefault ? event.preventDefault() : event.returnValue = false; signUp(); }" onkeyup="checkMobileField(this);" onfocus="if(this.value==signUpMobileNo){this.value=&#39;&#39;}" onblur="if(this.value==&#39;&#39;) {this.value=signUpMobileNo}" />
        <a id="signupbutton" href="javascript:signUp()"><img src="/templates/mkm/images/footer/signup.gif" alt="SIGN UP" title="SIGN UP" /></a>
