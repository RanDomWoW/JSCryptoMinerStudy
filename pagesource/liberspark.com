<script src="https://code.jquery.com/color/jquery.color-2.1.2.min.js"   integrity="sha256-H28SdxWrZ387Ldn0qogCzFiUDDxfPiNIyJX7BECQkDE=" crossorigin="anonymous"></script>
<script>

    setLoadingIndicator("notification_loading_container");

    var notificationsLoaded = false;
    var notifications;

    function toggleAllNotifications() {
        var postQuery = "";

        for(var i=0;i<notifications.length;i++)
        {
            var notification = notifications[i];

            postQuery += ""+notification.notification_id+"=1&";
        }

        $.post("/notifications", postQuery);
    }

    function getNotifications() {
        $("#notification_badge").remove();
        if(notificationsLoaded == false)
        {
            showLoadingIndicator("notification_loading_container");
            notificationsLoaded = true;

            $.get("/notifications", function(result) {
                if(result.success == true)
                {
                    hideLoadingIndicator("notification_loading_container");
                    notifications = result.notifications;

                    if(notifications.length == 0)
                    {
                        $(".see_all_notifications").css("display", "none");
                        var displayFormat = [" ",
                            "<li style='padding:20px 20px 20px 20px'>",
                            "<p class='share-menu' style='text-align:center'>You have no notifications</p>",
                            "</li>"
                        ];

                        $(".notifications_container").append(displayFormat.join(""));
                        $(".notification_parent").css("height", "100px");
                    }
                    else
                    {
                        for(var i=0;i<notifications.length;i++)
                        {
                            var notification = notifications[i];

                            var param1 = "<li class='individual_notif' style='padding:20px 20px 0px 20px'>";

                            if(notification.is_read == 0)
                                param1 = "<li class='individual_notif' style='padding:20px 20px 0px 20px;background-color:#f2f2f2;border-top:1px solid #e2e2e2'>";

                            var timeSince = getTimeDiff(new Date(notification.created_at + " +0000"));

                            var notificationFormat = [" ",
                                param1,
                                "<p class='share-menu'>"+notification.notification+"</p>",
                                "<p class='share-menu' style='margin:0px 0px 0px 0px;font-size:11px;color:#bfbfbf'>@TheNovelPage  | "+timeSince+"</p>",
                                "</li>",
                                "<hr style='margin:0px 0px 0px 0px'>"
                            ];

                            $(".notifications_container").append(notificationFormat.join(""));
                        }
                        $("#notification_badge").remove();

                        toggleAllNotifications();

                        $(".individual_notif").animate({
                            backgroundColor: "#ffffff"
                        }, 2000);
                    }
                }
                else
                {
                    notificationsLoaded = false;
                }
            })
        }
    }
</script>



