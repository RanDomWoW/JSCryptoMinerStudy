    }

    function showLoadingIndicator(parent_id) {
        $("#"+parent_id+" > #temp_container > #loading_indicator").css("display", "block");
    }

    function hideLoadingIndicator(parent_id) {
        $("#"+parent_id+" > #temp_container > #loading_indicator").css("display", "none");
    }

    function showSuccess(parent_id) {
        $("#"+parent_id+" > #temp_container > #loading_indicator").css("display", "none");
