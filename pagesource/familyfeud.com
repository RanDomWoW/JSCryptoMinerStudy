      var dobMonth = dob.getMonth();
      var dobDate = dob.getDate();

      var yearsDiff = todayYear - dobYear ;
      var age;

      if ( todayMonth < dobMonth ) { 
        age = yearsDiff - 1; 
      } else if ( todayMonth > dobMonth ) 
      {
        age = yearsDiff ; 
      } else {
