		$('#sign-in footer').css('display', 'none');
	}

	function showSignIn() {

		showModal('sign-in');

		$('#createaccount-modal-content').css('display', 'none');
		$('#createaccount-header').css('color', '#707070');

		$('#signin-modal-content').css('display', 'block');
		$('#signin-header').css('color', '#CA1913');

		$('#forgotpassword-modal-content').css('display', 'none');

