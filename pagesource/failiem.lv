					}
					else {
						
						addCouponError('forgotpassword_email', 'forgotpassword_error_msg', 'Šāds e-pasts nav reģistrēts.');
					}
				}
			});
		}
	}

	function validateEmail(email) { 
		var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
		var result = re.test(email);
		return result;
	}


	function addCouponError(strInputId, strErrorMsgId, strErrorMsgText) {
		$('#coupon-form #' + strInputId).css('border-color', '#C0120E');
		$('#coupon-form #' + strErrorMsgId).addClass('error_msg_active');
		$('#coupon-form #' + strErrorMsgId).text(strErrorMsgText);
	}

	function removeCouponError(strInputId) {
		$('#coupon-form #' + strInputId).css('border-color', '#D2D2D2');
	}

	function removeCouponAllErrors(strErrorMsgId) {
		$('#coupon-form #' + strErrorMsgId).text('');
		if ($('#coupon-form #' + strErrorMsgId).hasClass('error_msg_active')) {
			$('#coupon-form #' + strErrorMsgId).removeClass('error_msg_active');
		}
	}
	
