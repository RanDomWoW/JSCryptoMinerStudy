			bolHashRequestInProgress = true;

			var upload_id_url = strProtocol + host_dir +"/server_scripts/get_upload_id.php?show_add_key=1";
			$('#preloader_file_upload').show();
			jQuery.get(upload_id_url, function(txt) {

				global_upload_hash = txt.split(",")[0];
				global_upload_del_key = txt.split(",")[1];
				global_upload_add_key = txt.split(",")[2];

				bolHashRequestInProgress = false;

				E("submit_form").action = "u/"+global_upload_hash;
				E("del_key").value = global_upload_del_key;

				objAfterUploadSave.upload_hash = global_upload_hash;
				objAfterUploadSave.del_key = global_upload_del_key;

				if (strUploadType == 'html5') {

					$('#file_upload').data('uploadifive').settings.uploadScript = upload_save_server + '/save_file.php?PHPSESSID=n46nhfa458rjvposp3qfgimvn4&up_id=' + global_upload_hash + '&ignore_user_abort=1&skip_update=1&key=' +global_upload_add_key + ''+ '' + '';

                    $('#file_upload').data('uploadifive').settings.checkProgressScript = strProtocol + 'pro1.failiem.lv/check_progress.php?PHPSESSID=n46nhfa458rjvposp3qfgimvn4&json_object=1';
				}
				else if (strUploadType == 'flash') {

					$('#file_upload').uploadify('settings','uploader' , upload_save_server + '/save_file.php?PHPSESSID=n46nhfa458rjvposp3qfgimvn4&up_id=' + global_upload_hash + '&skip_update=1&key=' +global_upload_add_key + '');
				}
				else {

				}
				$('#preloader_file_upload').hide();
				$('#savefiles').show();
				$('#file_select_container').addClass('start_upload');

				removeTooltip('#file_select_button');

				intUloadStatus = 1;

			}).fail(function() {

				bolHashRequestInProgress = false;
			});
