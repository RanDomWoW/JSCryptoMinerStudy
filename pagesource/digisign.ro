        <script type="text/javascript">
            var msg = "";
            $(window).load(function () {
                if (msg.length > 0) {
                    alert(msg);
                }
            });

            var noScrollUrl = [/*"intrebari-frecvente",*/
                "promotii",
                "contact",
                "aplicatii-semnare"
            ];
            function scrollToAnchor(id) {
                var h = 0;
                if ($(id).offset().top == 0) {
                    idTmp = id.split("#").join("");
                    Open(idTmp);
                    return;
                } else {
                    h = $(id).offset().top - $("#mainheader").height() * 2;
                }
                $('html,body').animate({scrollTop: h}, "slow");
            }

            $(document).ready(function () {
                var url = window.location.href;
                var arr = url.split('#');
                if (arr.length > 1) {
                    var found = false;
                    for (i = 0; i < noScrollUrl.length; i++) {
                        if (window.location.href.indexOf(noScrollUrl[i]) != -1) {
                            found = true;
                            break;
                        }
                    }
                    if (!found) {
                        var last = arr[arr.length - 1];
                        scrollToAnchor("#" + last);
                    }
                }

                $('a').click(function () {
                    for (i = 0; i < noScrollUrl.length; i++) {
                        if (window.location.href.indexOf(noScrollUrl[i]) != -1) {
                            return;
                        }
                    }

                    if ($(this).hasClass("no-auto-scroll")) {
                        return;
                    }
                    collapseCheck = $(this).parent().parent().parent().children(".collapse");
                    if (collapseCheck.length != 0) {
                        if ($(this).hasClass("collapsed") != 0) {
                            setTimeout(function () {
                                console.log($(this).attr("href"));
                                scrollToAnchor($(this).attr("href"));
                            }, 10);
                            event.preventDefault();
                        }
                        return;
                    }
                    if (typeof $(this).attr('href') == 'undefined' ||
                            isNaN($(this).attr('href')) ||
                            $(this).attr('href').charAt(0) != '#') {
                        return;
                    }
                    event.preventDefault();
                    scrollToAnchor($(this).attr('href'));
                });

                $("#fixed_header").css("height", $("#mainheader").height());
            });
        </script>
