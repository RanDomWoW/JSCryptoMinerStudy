		$('#subLinks .jap-lang').click( function(e) {
			e.preventDefault();
			$(this).closest('li').find('.en-lang').trigger('click');
			
			location.href = $(this).attr('href');
		});
		function fix_language(lang) {
			$('#subLinks .jap-lang .en-lang').trigger('click');
			$('a').each( function() {
				var hr = $(this).attr('href');
				var isLocal = true;
				
				if (!hr || hr.indexOf('lang=') >= 0 || $(this).hasClass('no-lang')) return true;
				if ( hr.indexOf('http') >= 0) {
					if ( hr.indexOf(location.host) < 0 ) isLocal = false;
				}
				
				if (isLocal) {
					if (hr.indexOf('?') >= 0) {
						hr = hr + '&lang=' + lang;
					} else {
						hr = hr + '?lang=' + lang;
					}
					$(this).attr('href', hr);
				}
				

			});
		}
	});
	function wpc_getURL(name) {
	    return decodeURIComponent(
	        (location.search.match(RegExp("[?|&]"+name+'=(.+?)(&|$)'))||[,null])[1]
	    );  
	}
	function wpc_removeParameter(url, parameter) {
	  var fragment = url.split('#');
	  var urlparts= fragment[0].split('?');
	
	  if (urlparts.length>=2)
	  {
	    var urlBase=urlparts.shift(); //get first part, and remove from array
	    var queryString=urlparts.join("?"); //join it back up
	
	    var prefix = encodeURIComponent(parameter)+'=';
	    var pars = queryString.split(/[&;]/g);
		
	    for (var i= pars.length; i-->0;) {               //reverse iteration as may be destructive
	      if (pars[i].lastIndexOf(prefix, 0)!==-1) {   //idiom for string.startsWith
	        pars.splice(i, 1);
	      }
	    }
	    url = urlBase+'?'+pars.join('&');
	    if (fragment[1]) {
	      url += "#" + fragment[1];
	    }
	  }
	  return url;
	}
	function wpc_updateAddressBar(url) {
		history.pushState(null, null, url);
	}
