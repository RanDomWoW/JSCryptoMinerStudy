function refresh_sel(a){
    a = parseInt($('#sac').val()) + a;
    $('#sac').val(a);
    if(a){
        $('#ssa,#asaf,#rsa,#send_selected').css('color','blue');
        $('#ssa').removeClass('empty');
        $('#sca').removeClass('empty');
        $('#send_selected').removeClass('empty');
        $('#ssa span').html('('+a+')');
        $('#sca span').html('('+a+')');
        $('#send_selected span').html('('+a+')');
        $('#rsa').show();
        $('#send_selected').show();
        $('#asaf').show();
        $('#dsaf').show();
        if(a > 1){
            $('#sca').css('color','blue');
        }
        else{
            $('#sca').css('color','');
        }
    }
    else{
        $('#ssa,#asaf,#rsa,#sca,#send_selected').css('color','');
        $('#ssa').addClass('empty');
        $('#sca').addClass('empty');
        $('#send_selected').addClass('empty');
        $('#ssa span').html('');
        $('#sca span').html('');
        $('#send_selected').hide();
        $('#asaf').hide();
        $('#dsaf').hide();
    }
}
function add_sel(i){$.post('',{add_sel:i},function(){});}
function del_sel(i){$.post('',{del_sel:i},function(){});}
function clear_sel(){$.post('',{clear_sel:true},function(){});}
