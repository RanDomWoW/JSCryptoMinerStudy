function msg(text,n){
    $('.popup_alert').show();
    $('.popup_alert p').html(text);
    if(n != null & n != false & n != undefined){
        $('.popup_alert h4').html(n);
    }
    return false;
}
var elan = {};
elan.popup = function(selector){
    var e = $('body').find(selector);
    $(e).appendTo('body');
    $(e).toggle();
    var w = $(e).outerWidth();
    var h = $(e).outerHeight();
    var bh = $('body').height();
    var wh = $(window).height();
    var wp = $(window).scrollTop();
    if(bh > h && Math.round(wp+wh/2) > Math.round(h/2))
        $(e).css({'top':Math.round(wp+wh/2)+'px','marginTop':-Math.round(h/2)});
    else
        $(e).css({'top':'20px'});
    $(e).css('marginLeft',- Math.round(w/2));
}
