				if( "" == mg2ConnextVars.User.State ) {
					mg2ConnextVars.User.State = "notauthorized";
				}
			},
			onAuthorized: function( e ) {
				console.log('onAuthorized.Callback', e);
				if( "" == mg2ConnextVars.User.State ) {
					mg2ConnextVars.User.State = "authorized";
				}
			},
			onHasAccess: function( e ) {
				console.log('onHasAccess.Callback', e);
				if( "" == mg2ConnextVars.User.State ) {
					mg2ConnextVars.User.State = "subscribed";
				}
			},
			onLoggedIn: function( e ) {
				console.log('onLoggedIn.Callback', e, arguments);
				mg2ConnextVars.User.State = "loggedin";
			},
			onHasAccessNotEntitled: function( e ) {
				console.log('onHasAccessNotEntitled.Callback', e, arguments);
				mg2ConnextVars.User.State = "hasaccessnotentitled";
			},
			onHasNoActiveSubscription: function( e ) {
				console.log('onHasNoActiveSubscription.Callback', e, arguments);
				mg2ConnextVars.User.State = "hasnoactivesubscription";
			}
		});
	});
}
function onUnload_SendData() {
	if( dfm.api("data","pageType").toLowerCase() == "article" && Connext && (! mg2ConnextVars.dataSent ) ) {
		console.log( 'mg2ConnextVars', mg2ConnextVars );
		if( "" !== mg2ConnextVars.Campaign.ActionName.trim() ) {
            dataLayer.push( {
                "pw_Event"          : "onPageClosed",
                "event"             : "pw_ActionNoClicks",
                "pw_ActionName"     : mg2ConnextVars.Campaign.ActionName,
                "pw_ActionTypeId"   : mg2ConnextVars.Campaign.ActionTypeId,
                "pw_MeterLevelId"   : mg2ConnextVars.Campaign.MeterLevelId + '-' + mg2ConnextVars.Campaign.MeterLevel,
                "pw_PageViews"      : mg2ConnextVars.Page.Views,
                "pw_UserState"      : mg2ConnextVars.User.State
            } );
        } else {
            dataLayer.push( {
                "pw_Event"          : "onPageClosed",
                "event"             : "pw_No_Modal",
                "pw_MeterLevelId"   : mg2ConnextVars.Campaign.MeterLevelId + '-' + mg2ConnextVars.Campaign.MeterLevel,
                "pw_PageViews"      : mg2ConnextVars.Page.Views,
                "pw_UserState"      : mg2ConnextVars.User.State
            } );
        }
		mg2ConnextVars.dataSent = true;
	}
}
$( document ).ready( function() {
	document.body.onbeforeunload = function() {
		onUnload_SendData();
	}
} );
function DisplayConnextInfo() {
    if( typeof Connext != "undefined" ) {
        console.log( "CONNEXT VERSION IS " + Connext.GetVersion() );
    } else {
        window.setTimeout( DisplayConnextInfo, 1*500 );
    }
}
