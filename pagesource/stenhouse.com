(function ($) {

  var tempref = $.cookie('Drupal.visitor.referral_code');

  if( null !== tempref) {
    ga('set', 'campaignName', tempref);
    ga('set', 'campaignSource', tempref);
    ga('set', 'campaignMedium', tempref);

    ga('set', 'campaignKeyword', tempref);
    ga('set', 'campaignContent', tempref);
  }

  $(function() {

    $('.button').on('click', 'a:contains(Quick View)', function() {
      var category = $(this).attr('href');
      ga('send', 'event', category, 'Quick_view', document.location.pathname );
    });

    $('body').on('click', '.add-to-cart', function() {
      var category = $('.field-name-title h1').text();
      ga('send', 'event', category, 'Add_to_cart', document.location.pathname );
    });

    $('body').on('click', '.product-preview', function() {
      var category = $('.field-name-title h1').text();
      ga('send', 'event', category, 'Product_preview', document.location.pathname );
    });

    $('body').on('click', '.continue-to-preview', function() {
      var category = $('.field-name-title h1').text();
      ga('send', 'event', category, 'Preview_bypass_guest', document.location.pathname );
    });

    $('.callout__action.--phone').on('click', function() {
      ga('send', 'event', 'Call_to_action', 'Call', $(this).attr('href') );
    });

    $('.footer-phone').on('click', 'a', function() {
      ga('send', 'event', 'Footer_bottom', 'Call', 'tel:8009889812' );
    });

    var come_from_review = document.referrer.search('/review');

    if( come_from_review && $('.checkout_completion_message').length ) {

      var transaction_id = $('[data-orderid]').data('orderid');
      var checkout_completed = $.cookie('checkout_completed_' + transaction_id);

      if(checkout_completed) {
        return;
      }

      $.cookie('checkout_completed_' + transaction_id, 1, { expires: 7, path: '/' });

      if( 'undefined' === typeof transaction_id) {
        transaction_id = $('.field-name-field-purchase-order-number').text().trim();
      }

      var revenue_amt = parseFloat( $('.component-type-base-price .component-total').text().replace(/[^0-9\.]/g, ''));

      var shipping_amt = $('.component-type-flat-rate-ground .component-total').text().replace(/[^0-9\.]/g, '');
          shipping_amt += $('.component-type-flat-rate-two-day .component-total').text().replace(/[^0-9\.]/g, '');
          shipping_amt += $('.component-type-flat-rate-free .component-total').text().replace(/[^0-9\.]/g, '');
          shipping_amt = parseFloat(shipping_amt);

      var tax_amt = parseFloat( $('.component-type-exactor-calc .component-total').text().replace(/[^0-9\.]/g, '') );

      var transaction = {
        'id': transaction_id,                     // Transaction ID. Required
        'affiliation': 'Stenhouse Publishers',    // Affiliation or store name
        'revenue': revenue_amt,                   // Grand Total
        'shipping': shipping_amt,                 // Shipping
        'tax': tax_amt                            // Tax
      };
      if( $('[class*=component-type-discount] .component-title').length ) {
        transaction.coupon = $('[class*=component-type-discount] .component-title').text();
        transaction.revenue -= parseFloat($('[class*=component-type-discount] .component-total').text().replace(/[^0-9\.]/g, ''));
      }
      ga('ecommerce:addTransaction', transaction);


      var table_line_items = $('.view-commerce-line-item-table tbody tr, .view-purchase-order-line-items tbody tr');
          table_line_items.each( function() {
            var product = {
              'name': $(this).find('.views-field-line-item-title h4').text().trim(),
              'sku': $(this).find('.views-field-field-stenhouse-product-id').first().text().trim(),
              'category': $(this).find('.views-field-line-item-title p').last().text().trim(),
              'price': $(this).find('.views-field-commerce-unit-price').text().replace(/[^0-9\.]/g, ''),
              'quantity': $(this).find('.views-field-quantity').text()
            };

            var line_item = {
              'id': transaction_id,             // Transaction ID. Required
              'name': product.name,                // Product name. Required
              'sku': product.sku,                    // SKU/code
              'category': product.category,       // Category or variation
              'price': product.price,                 // Unit price
              'quantity': product.quantity                  // Quantity
            };

            ga('ecommerce:addItem', line_item);
          });

        ga('ecommerce:send');
    }

  });
