            $('.header').css('opacity','1').css("box-shadow"," 0 1px 10px 0 rgba(69, 71, 77, 0.2)").css("background","#fff").css("color","#333");
            $(".navtop li a").css("color","#383838");
            $(".loginBtn:not(.bgBTnContainer .loginBtn)").css("border","1px solid #44b8fc").css("color","#44b8fc");
            $(".logo img").attr("src",ctx+"/images/logo_blue.png");
            $(".logo img").attr("srcset",ctx+"/images/logo_blue@2x.png 2x");
            $("#userListCon").css("color","#383838");
        }else{
            if($('#alreadyLogin').length==0){
                $('.header').css('opacity','1').css("box-shadow","0 0 0 0 transparent").css("background","transparent").css("color","#fff");
                $(".navtop li a").css("color","#fff");
                $(".loginBtn:not(.bgBTnContainer .loginBtn)").css("border","1px solid #fff").css("color","#fff");
                $(".logo img").attr("src",ctx+"/images/logoBig.png");
                $(".logo img").attr("srcset",ctx+"/images/logoBig@2x.png 2x");
                $("#userListCon").css("color","#fff");
            }
        }
    }
    </script>
