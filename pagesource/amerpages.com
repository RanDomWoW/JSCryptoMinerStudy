var loopInterval = 6000;

var index = 0;
var itemsCollection = null;
var length = 0;
var itemA = null;

var timer = null;

$(document).ready(function(){
	itemsCollection = $('#homeItems .homeItemContainer').toArray();
	length = $('#homeItems .homeItemContainer').size();
	totalWidth = $('#homeItems').width();

	itemA = itemsCollection[index];
	$('#homeItems .homeItemContainer').hide();
	$(itemA).fadeIn();
	if(length > 1){
        for (i = 0; i < length; i++) {
            $('#led_container').append('<div class="led" id="led_' + i + '" onclick="goto(' + i + ')"></div>');
        }
        showItem(index);
        timer = setTimeout("rotatorLoop()", loopInterval);

        $('#slide_container').hover(
            function () {
                clearTimeout(timer);
            },
            function () {
                timer = setTimeout("rotatorLoop()", loopInterval);
            }
        );
        $(window).bind("focus", function(event) {
            timer = setTimeout("rotatorLoop()", loopInterval);
        }).bind("blur", function(event) {
            clearTimeout(timer);
        });
    }




});

function rotatorLoop(timer) {
	next();
}

function checkIndex(newIndex) {
	if(newIndex <= length - 1 && newIndex >= 0) {
		return newIndex;
	}
	else {
		return 0;
	}
}

function getNextIndex() {
	var newIndex = index;
	if(newIndex < length - 1) {
		newIndex++;
	}
	else {
		newIndex = 0;
	}
	return newIndex;
}

function getPreviousIndex() {
	var newIndex = index;
	if(newIndex >= 1) {
		newIndex--;
	}
	else {
		newIndex = length - 1;
	}
	return newIndex;
}

function goto(next) {
	next = checkIndex(next);
	displayItem(next);
}

function next() {
	var next = getNextIndex(true);
	displayItem(next);
}

function previous() {
	var next = getPreviousIndex();
	displayItem(next);
}

function displayItem(next) {

//    alert(index + ' | ' + next);
	hideItem(index);

	index = checkIndex(next);
	showItem(index);
	clearTimeout(timer);
	timer = setTimeout("rotatorLoop()", loopInterval);
}

function hideItem(itemIndex) {
	itemA = itemsCollection[itemIndex];
	$(itemA).animate({opacity: 'hide'}, 1000, 'swing');
	$('#led_' + itemIndex).removeClass('active');
}

function showItem(itemIndex) {
	itemA = itemsCollection[itemIndex];
	//$(itemA).css('left', -totalWidth);
	$(itemA).animate({opacity: 'show'}, 1000, 'swing');
	//$('#rotator_caption span').html($(itemA).find('.item_desc').html());
	$('#led_' + itemIndex).addClass('active');
}
var timerGalery = null;
showBanner(1);
function showBanner(i){
    if(!($('#home_container #led_p_'+i)).hasClass('active')){
        $('#home_search_bar  .image_home').stop(true, true).hide();

        $('#home_search_bar img#home_main_image_'+i).stop(true, true).fadeIn(600);



        $('#home_container #buttons_rotator span').removeClass('active');
        $('#home_container #led_p_'+i).addClass('active');
        i= Number(i) + 1;
        if(i > 4 ){
            i = 1;
        }
       // $('#home_search_bar #led_'+i).animate({ backgroundColor: 'red'}, 10000);
        clearTimeout(timerGalery);
        timerGalery = setTimeout("showBanner("+i+")", 10000);
    }
}

