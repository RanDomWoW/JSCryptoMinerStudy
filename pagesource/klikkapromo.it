            $("#ddl_Scontrino_Mese").val(dataOggi.getMonth() + 1);
            $("#ddl_Scontrino_Anno").val(dataOggi.getFullYear());

            $(function () {
                $("#ddl_Scontrino_Insegne").searchable({
                    zIndex: 'auto'
                });
            });

            //nascondo div carte
            $("#imgCardAndNumber").hide();

            //funzione on change della select insegne
            $("#ddl_Scontrino_Insegne").change(function () {
                var idSelected = this.value;

                //recupero le carte dei supermercati
                $.getJSON('http://' + KlikkaPromoUrl + '/handlers/doServerAction.ashx?action=cartedisponibili',
                        function (data) {
                            $("#imgCardAndNumber").hide();
                            $('#ddl_Scontrino_Carte').children().remove().end(); //cancello tutto

                            var found = false;

                            if (data != null) {
                                for (i = 0; i < data.length; i++) {
                                    //aggiungo la carta
                                    if (data[i].INSEGNA_ID == idSelected) {
                                        found = true;
                                        $("#cartehidden").css("display", "");
                                        $("#ddl_Scontrino_Carte").append($("<option value=\"" + data[i].CARD_ID + "|" + data[i].CARD_IMG + "\">" + data[i].CARD_DESC + "</option>"));
                                    }
                                }
                            }

                            //se esiste almeno una carta per l'insegna
                            if (found) {
                                viewMessage("");
                                toggleStatus(true);
                                $("#ddl_Scontrino_Carte option:first-child"); //seleziono la prima
                                $("#imgCardAndNumber").show(); //visualizzo il div
                                viewSelectedCard();
                            }
                            else {
                                if ($("#ddl_Scontrino_Insegne").val() != "") {
                                    $("#cartehidden").css("display", "none");
                                    viewMessage("Nessuna carta disponibile per questo supermercato!");
                                    toggleStatus(false);
                                }
                            }
                            if ($("#ddl_Scontrino_Insegne").val() == "") {
                                $("#cartehidden").css("display", "none");
                            }
                        });

                //funzione on change della select scontrini
                $("#ddl_Scontrino_Carte").change(function () {
                    viewSelectedCard();
                });
            });
        });

        function toggleStatus(isEnabled) {
            if (!isEnabled) {
                $('#totalScontrino :input').attr('disabled', true);
                $('#ButtonSalvaScontrino').hide();
            } else {
                $('#totalScontrino :input').removeAttr('disabled');
                $('#ButtonSalvaScontrino').show();
            }
        }

        //funzione visualizzazione messaggio
        function viewMessage(text) {
            if (text != "") {
                $('#erroreGenerico').css("visibility", "visible");
                $('#erroreGenerico').html(text);
            }
            else {
                $('#erroreGenerico').css("visibility", "hidden");
            }
        }

        function validaData() {
            viewMessage("");
            var dataOggi = new Date();
            var dataToVal = new Date($("#ddl_Scontrino_Anno").val() + "/" + $("#ddl_Scontrino_Mese").val() + "/" + $("#ddl_Scontrino_Giorno").val());

            var gg = $("#ddl_Scontrino_Giorno").val();
            var mm = $("#ddl_Scontrino_Mese").val();
            var module = $("#ddl_Scontrino_Anno").val() % 4;

            if (dataToVal > dataOggi) {
                viewMessage("Inserire una data antecedente o uguale a quella odierna");
                return false;
            }
            else if (gg == 31 && (mm == 9 || mm == 6 || mm == 4 || mm == 11)) {
                viewMessage("Data non esistente");
                return false;
            }
            else if (mm == 2) {
                if (module == 0 && gg > 29) {
                    viewMessage("Data non esistente");
                    return false;
                }
                else if (module != 0 && gg > 28) {
                    viewMessage("Data non esistente");
                    return false;
                }
                else {
                    $('#erroreGenerico').css("visibility", "hidden");
                }
            }
            else {
                $('#erroreGenerico').css("visibility", "hidden");
            }

            var importo = document.getElementById('ImportoScontrino')
            if (isNaN(importo.value.replace(',', '.')) || isNaN(isNaN(importo.value.replace(',', '.'))) || importo.value == "" || importo.value == "Es: 12,70" || importo.value == 0 || parseFloat(importo.value) <= 0.0) {
                viewMessage("Inserire un valore corretto per l'importo")
                importo.focus();
                return false;
            }

            return true;
        }

        //function per la selezione della carta
        //visualizza immagine e numero
        function viewSelectedCard() {
            var idcartasel = $("#ddl_Scontrino_Carte").val().split("|")[0];
            var immaginecartasel = $("#ddl_Scontrino_Carte").val().split("|")[1];
            $("#H_IDCard").val(idcartasel); //campo hidden con ID carta
            $("#immagineCarta").attr("src", "/Includes/Imgs/Cards/" + immaginecartasel); //Immagine carta

