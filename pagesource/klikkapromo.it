<script type="text/javascript">

    //variabile per memorizzare il primo item trovato
    var firstItemFound = null;

    function doSearch(id, tipo, testoLinkCliccato, isFromBarra) {
        var citta = document.getElementById("MainContent_HeaderSearch1_lbl_User_citta");
        $.ajax({
            url: "../Handlers/doServerAction.ashx?nocache=" + (new Date()).getTime() + "&searched=" +
				 escape($('#testoRicerca').val()) + "&action=ricerca&testolinkcliccato=" + escape(testoLinkCliccato) + "&tipo=" + tipo + "&id=" + id + "&isFromBarra=" + isFromBarra
        }).done(function (data) {
            var split = data.split("|");
            if (split[0] == "OK") {
                switch (tipo) {
                    case "1":
                        if (isFromBarra == "true") {
                            $(window.location).attr('href', 'ricerca-categoria-menu-' + split[2] + '-' + citta.textContent + "_" + split[3] + "_" + split[4]);
                        }
                        else {
                            $(window.location).attr('href', 'ricerca-categoria-' + split[2] + '-' + citta.textContent + "_" + split[3] + "_" + split[4]);
                        }
                        break;
                    case "2":
                        $(window.location).attr('href', 'ricerca-brand-' + split[2] + '-' + citta.textContent + "_" + split[3] + "_" + split[4]);
                        break;
                    case "3":
                        $(window.location).attr('href', 'ricerca-prodotto-' + split[2] + '-' + citta.textContent + "_" + split[3] + "_" + split[4]);
                        break;
                    default:
                        $(window.location).attr('href', 'offerte-' + split[2] + '-' + citta.textContent + "_" + split[3] + "_" + split[4]);
                }
            }
        });
    }

    function getLink(id, testo, tipo) {
        var testoAltern = testo;
        return "<li class=\"ellipsis\" ><a  title=\"" + testoAltern + "\" href=\"#\" onClick=\"doSearch('" + id + "','" + tipo + "','" + escape(testo) + "','" + "false" + "');\"    >" + testo + "</a></li>";

    }

    function elaboraJSON(json) {
        var found = false;
        firstItemFound = null;

        //RESET
        var linksCAT = ""; var linksMAR = ""; var linksART = "";
        $('#divCATEGORIA').html(""); $('#divMARCA').html(""); $('#divARTICOLO').html("");

        //CATEGORIA
        if (json.final_categ.length == 0) {
            document.getElementById('MainContent_HeaderSearch1_colCategoria').style.display = "none";
        } else {
            document.getElementById('MainContent_HeaderSearch1_colCategoria').style.display = "inline";
            for (i = 0; i < json.final_categ.length; i++) {
                if (!found) { firstItemFound = { id: json.final_categ[i].Id, tipo: json.final_categ[i].tipo }; }
                linksCAT += getLink(json.final_categ[i].Id, json.final_categ[i].Descrizione, json.final_categ[i].tipo);
                found = true;
            }
            linksCAT = "<ul>" + linksCAT + "</ul>"
        }

        //MARCA
        if (json.final_brands.length == 0) {
            document.getElementById('MainContent_HeaderSearch1_colMarca').style.display = "none";
        } else {
            document.getElementById('MainContent_HeaderSearch1_colMarca').style.display = "inline";
            for (i = 0; i < json.final_brands.length; i++) {
                if (!found) { firstItemFound = { id: json.final_brands[i].IdBrand, tipo: json.final_brands[i].tipo }; }
                linksMAR += getLink(json.final_brands[i].IdBrand, json.final_brands[i].Descrizione, json.final_brands[i].tipo);
                found = true;
            }
            linksMAR = "<ul>" + linksMAR + "</ul>"
        }


        //get ARTICOLO
        if (json.final_articoli.length == 0) {
            document.getElementById('MainContent_HeaderSearch1_colArticolo').style.display = "none";
        } else {
            document.getElementById('MainContent_HeaderSearch1_colArticolo').style.display = "inline";
            for (i = 0; i < json.final_articoli.length; i++) {
                if (!found) { firstItemFound = { id: json.final_articoli[i].ITEM_ID, tipo: json.final_articoli[i].tipo }; }
                var descrArt = json.final_articoli[i].ITEM_DESC + " " + json.final_articoli[i].Brand;
                linksART += getLink(json.final_articoli[i].ITEM_ID, descrArt, json.final_articoli[i].tipo);
                found = true;
            }
            linksART = "<ul>" + linksART + "</ul>"
        }

        // found / not found 
        if (found) {
            $('#divCATEGORIA').html(linksCAT); $('#divMARCA').html(linksMAR); $('#divARTICOLO').html(linksART);  //ASSIGN
            $("#divCont").fadeIn("fast");  //VIEW DATA
            $("#divNoCont").hide();
        }
        else {
            $("#divNoCont").fadeIn("fast");  //NO DATA!
            $("#divCont").hide();
        }

        jQuery('#tableContenitore').hideLoading();
    }

    $(document).ready(function () {
        $('.ico-list_arrow_down').click(function () {

            var idItem = $(this).attr("idtoopen");
            $('#tooltip_DIVDETTAGLIO .link_back #sottotitolo1').text($(this).parent().parent().parent().find('h4').text());
            $('#tooltip_DIVDETTAGLIO  #sottotitolo1bis').text($(this).parent().text());

            $.getJSON("../Handlers/doserveraction.ashx?action=menucategorie&idpadre=" + idItem + "&isFromBarra=true"
                        , null, function (json) {
                            var htmlUL = "<div class='col'><ul>";

                            var MaxElemXcol = Math.round(json.length / 4);
                            var ContaElemXcol = 0;

                            for (i = 0; i < json.length; i++) {

                                var desc_item = json[i].Descrizione;
                                var desc_item_rep = desc_item.replace("'", "_");
                                htmlUL += '<li><a href="#"  onClick=\'doSearch("' + json[i].ID + '","1","' + desc_item_rep + '","true' + '");\' >' + json[i].Descrizione + '</a></li>';
                                ContaElemXcol += 1;

                                if (ContaElemXcol == MaxElemXcol) {
                                    htmlUL += "</ul></div><div class='col'><ul>";
                                    ContaElemXcol = 0;
                                }
                            }
                            htmlUL += "</ul></div>";

                            var htmlh4 = "<h4>" + $('#tooltip_DIVDETTAGLIO  h4').html() + "</h4>";
                            $('#tooltip_DIVDETTAGLIO .col').parent().html(htmlh4 + htmlUL);
                        });
        });

        //tooltip sul nome del supermercato / promozione
        $("#divNomePromozione").attr('title', $("#divNomePromozione").text());

        //simula click anche sulla descrizione "+ altri X"
        $("#hrefPromozioniPlusOtherN").click(function (e) { $("#link-popup_promo").click(); });

        //nascondo tooltip
        $("#divCont").hide();
        $("#divNoCont").hide();

        //riassegno azione al button click
        $("#btnStartSearch").click(function (e) {
            //            e.preventDefault();

            //            //lancio una ricerca se trovato qualcosa
            //            if (firstItemFound != null)
            //                doSearch(firstItemFound.id, firstItemFound.tipo);
            btnStartSearch_Click()
        });

        var currentPage = "/home.aspx";

        //assegno azioni onTyping della textbox

        //SE SONO NELL'HP GESTIONE SCRIPT IN CONFLITTO
        if (currentPage == "/home.aspx") {
            var $je = jQuery.noConflict();
            $je(function () {
                $je(
                '#testoRicerca').typing({
                    start: function (event, $elem) {
                        //NONE
                    },
                    stop: function (event, $elem) {

                        if ($('#testoRicerca').val().length >= 2) {

                            $("#divNoCont").hide();
                            $("#divCont").hide(function () {
                                jQuery('#tableContenitore').showLoading(
					         {
					             'afterShow':
						        function () {
						            //leggo dati in base a quanto digitato dopo un delay X
						            $.getJSON("../Handlers/getItemsBySearch.ashx?nocache=" + (new Date()).getTime() +
							        "&searched=" + escape($('#testoRicerca').val()), null, function (json) { elaboraJSON(json); })
						        }
					         }
					        );
                            });
                        }
                    },
                    delay: 200
                });
            });
        }
        else {
            $('#testoRicerca').typing({
                start: function (event, $elem) {
                    //NONE
                },
                stop: function (event, $elem) {

                    if ($('#testoRicerca').val().length >= 2) {

                        $("#divNoCont").hide();
                        $("#divCont").hide(function () {
                            jQuery('#tableContenitore').showLoading(
					         {
					             'afterShow':
						        function () {
						            //leggo dati in base a quanto digitato dopo un delay X
						            $.getJSON("../Handlers/getItemsBySearch.ashx?nocache=" + (new Date()).getTime() +
							        "&searched=" + escape($('#testoRicerca').val()), null, function (json) { elaboraJSON(json); })
						        }
					         }
					        );
                        });
                    }
                },
                delay: 200
            });
        }
    });

    function btnStartSearch_Click() {
        var value = $("#testoRicerca").val();

        if (value) {
            document.getElementById('MainContent_HeaderSearch1_hid_Search').value = value;
            $('#MainContent_HeaderSearch1_btn_Search').click();
        }
    }

