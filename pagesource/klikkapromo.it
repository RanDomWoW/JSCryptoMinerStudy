        function SetChangeValues(res, doBtnClick) {
            for (var i = 0; i < res[0].address_components.length; i++) {

                var type = res[0].address_components[i].types[0];

                switch (type) {
                    case 'route':
                        document.getElementById('hf_Register_Indirizzo').value = res[0].address_components[i].long_name;
                        break;
                    case 'locality':
                        document.getElementById('hf_Register_Comune').value = res[0].address_components[i].long_name;
                        break;
                    case 'administrative_area_level_3':
                        document.getElementById('hf_Register_Comune').value = res[0].address_components[i].long_name;
                        break;
                    case 'administrative_area_level_2':
                        document.getElementById('hf_Register_Provincia').value = res[0].address_components[i].short_name;
                        break;
                    case 'administrative_area_level_1':
                        document.getElementById('hf_Register_Regione').value = res[0].address_components[i].long_name;
                        break;
                }
            }

            document.getElementById('hf_Register_X').value = res[0].geometry.location.lng();
            document.getElementById('hf_Register_Y').value = res[0].geometry.location.lat();

            if (doBtnClick) {
                $('#cambiaCittaPopup').click();
            }
        }

        function OLD_AggiornaPosizione() {
            if (document.getElementById('hf_Register_X').value == '') {
                var firstResult = $(".pac-container .pac-item:first").text(); //***
