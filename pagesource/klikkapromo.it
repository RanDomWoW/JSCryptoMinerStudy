        function SendToPrinter() {
            var oIframe = document.getElementById('printIframe');
            oIframe.style.display = "inherit";
            var oDoc = (oIframe.contentWindow || oIframe.contentDocument);
            if (oDoc.document) oDoc = oDoc.document;
            oDoc.write("<html><head><title>title</title></head>");
            oDoc.write("<style> .HT{ height:39px; background-color:#f2f2f2;} .verde{ font-family:Arial, Helvetica, sans-serif; }</style>")
            oDoc.write("<body onload='this.focus(); this.print();'>");
            oDoc.write(PrintContent);
            oDoc.write("</body></html>");
            oDoc.close();
            PrintContent = "";
            setTimeout(HideIFrame, 1500);
        }
        function HideIFrame() {
            var oIframe = document.getElementById('printIframe');
            oIframe.style.display = "none";
