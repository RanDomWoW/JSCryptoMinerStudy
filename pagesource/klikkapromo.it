<!-- Script banner cookie-->

    <script language="javascript" type="text/javascript">

        $(window).scroll(function() {

            setCookiePrivacyAndClose();

        });

        $("body a").on('click', function () {
            setCookiePrivacyAndClose();
        });

        function setCookiePrivacyAndClose() {
            setCookie('KP_InformativaCookie', 'OK', 364);
            $("#privacy-cookies-banner").css("visibility", "hidden");
        }

        function viewCookiePolicy() {
            setCookiePrivacyAndClose();
            window.open('/informativa-privacy#cookie-policy', '_blank');
        }

        function checkCookiePrivacy() {
            var lInformativaCookie = getCookie('KP_InformativaCookie');
            if (lInformativaCookie != 'OK')
                $("#privacy-cookies-banner").css("visibility", "visible");
        }

        checkCookiePrivacy();

        function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = cname + "=" + cvalue + "; " + expires;
        }

        function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1);
                if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
            }
            return "";
        }

    </script>

