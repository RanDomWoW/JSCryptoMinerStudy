            $("#numeroCarta").val("");
            $.getJSON('/Handlers/doServerAction.ashx?action=carteUtente', function (data) {
                $("#numeroCarta").removeAttr('disabled');
                if (data != null) {
                    for (i = 0; i < data.length; i++) {
                        if (data[i].CARD_ID == idcartasel) {
                            //se c'ÃÂ¨...
                            $("#numeroCarta").val(data[i].Numero);
                            $("#numeroCarta").attr('disabled', true);
                        }
                    }
                }
            });
        }
