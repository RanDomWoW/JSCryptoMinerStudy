        function OLD_getDetailsAdressbyLatLng2(lat, lng, dopostback) {
            try {
                lat = lat.replace(',', '.');
                lng = lng.replace(',', '.');
            } catch (ex) {
                //nel caso di geocoder automatico mi torna un oggetto
                //se faccio il replace mi va in errore
            }

            var geocoder = new google.maps.Geocoder();
            var latlng = new google.maps.LatLng(lat, lng);
            geocoder.geocode({ 'latLng': latlng }, function (results, status) {
                if (status == google.maps.GeocoderStatus.OK) {
                    if (results[1]) {
                        var indice = 0;
                        for (var j = 0; j < results.length; j++) {
                            if (results[j].types[0] == 'locality') {
                                indice = j;
                                break;
                            }
                        }

                        var Town = '';
                        var Country = '';
                        var Province = '';
                        var Region = '';

                        for (var i = 0; i < results[j].address_components.length; i++) {

                            if (results[j].address_components[i].types[0] == "locality") {
                                Town = results[j].address_components[i].long_name;
                            }

                            if (results[j].address_components[i].types[0] == "country") {
                                Country = results[j].address_components[i].long_name
                            }

                            if (results[j].address_components[i].types[0] == "administrative_area_level_2") {
                                Province = results[j].address_components[i].short_name;
                            }

                            if (results[j].address_components[i].types[0] == "administrative_area_level_1") {
                                Region = results[j].address_components[i].long_name;
                            }

                        }
                    }

                    var tb = document.getElementById(CtrlGoogleAutoComplete)
                    tb.value = Town + ", " + Country;



                    document.getElementById('hf_Register_X').value = latlng.lng();
                    document.getElementById('hf_Register_Y').value = latlng.lat();
                    document.getElementById('hf_Register_Regione').value = Region;
                    document.getElementById('hf_Register_Provincia').value = Province;
                    document.getElementById('hf_Register_Comune').value = Town;
                    document.getElementById('hf_Register_Indirizzo').value = "";

                    if (dopostback) {
                        $('#cambiaCittaPopup').click();
                    }
                }
                else {
                    alert("Geocoder failed due to: " + status);
                }
            });
        }



        function OLD_getDetailsAdressbyLatLng(lat, lng) {
            var geocoder = new google.maps.Geocoder();
            var latlng = new google.maps.LatLng(lat, lng);
            geocoder.geocode({ 'latLng': latlng }, function (results, status) {
                if (status == google.maps.GeocoderStatus.OK) {

                    if (results[1]) {
                        var indice = 0;
                        for (var j = 0; j < results.length; j++) {
                            if (results[j].types[0] == 'locality') {
                                indice = j;
                                break;
                            }
                        }

                        var Town = ' ';
                        var Province = ' ';
                        var Region = ' ';

                        if (results[0].address_components[2].types[0] == "locality" || results[0].address_components[2].types[0] == "administrative_area_level_3") {
                            Town = results[0].address_components[2].long_name;
                        }
                        if (results[0].address_components[4].types[0] == "administrative_area_level_2") {
                            Province = results[0].address_components[4].short_name;
                        }
                        if (results[0].address_components[5].types[0] == "administrative_area_level_1") {
                            Region = results[0].address_components[5].long_name;
                        }

                        if (Town == ' ') {
                            Town = results[0].address_components[1].long_name;
                        }

                        if (Province == ' ') {
                            Province = results[0].address_components[2].short_name;
                        }

                        if (Region == ' ') {
                            Region = results[0].address_components[3].long_name;
                        }
                    }

                    var rRegione = document.getElementById('hf_Register_Regione')
                    var rProvincia = document.getElementById('hf_Register_Provincia')
                    var rComune = document.getElementById('hf_Register_Comune')

                    rRegione.value = Region;
                    rProvincia.value = Province;
                    rComune.value = Town;

                }
                else {
                    alert("Geocoder failed due to: " + status);
                }
            });
        }

