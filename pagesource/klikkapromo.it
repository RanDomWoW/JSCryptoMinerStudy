        function initializeGA() {

            if (CtrlGoogleAutoComplete == "tb_register_town") {

                var tmp = $('#hf_Register_Placeholder').val();
                if (tmp == "") {
                    tmp = "Posizione: es. Milano, via Volturno 10";
                }
                var tb = document.getElementById('tb_register_town');
                if (tb.attributes["placeholder"] != null || tb.attributes["placeholder"] != undefined) {
                    tb.attributes["placeholder"] = tmp;
                } else {
                    tb.setAttribute("placeholder", tmp);
                }
            }

            var input = document.getElementById(CtrlGoogleAutoComplete);
            var options = { componentRestrictions: { country: 'it' }, types: ['geocode']
            };
            var autocomplete = new google.maps.places.Autocomplete(input, options);
            google.maps.event.addListener(autocomplete, 'place_changed', function () {

                var place = autocomplete.getPlace();
                if (place.geometry != undefined) {

                    var rX = document.getElementById('hf_Register_X')
                    var rY = document.getElementById('hf_Register_Y')
                    var rIndirizzo = document.getElementById('hf_Register_Indirizzo')

                    rX.value = place.geometry.location.lng();
                    rY.value = place.geometry.location.lat();


                    getDetailsAdressbyLatLng(place.geometry.location.lat(), place.geometry.location.lng());
                    var address = '';
                    if (place.address_components) {

                        rIndirizzo.value = (place.address_components[0] && place.address_components[0].short_name || '')

                    }
                }
            });
        }

