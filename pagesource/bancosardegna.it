<script>
$(function() {
	var autocom = $( "#istBanca-headerInputText" ).autocomplete({
		source: function( request, response ) {
			$.ajax({
				url: '/search',
				type: 'POST',
				dataType: "json",
    			contentType: "application/x-www-form-urlencoded;charset=UTF-8",
				data: {
					ql: "ITA",
					txt: $("#istBanca-headerInputText").val()
				},
				success: function( data ) {
					if(data.autocompletions == null){
						data.autocompletions = '';
					} else if(data.autocompletions[0].length == 1){
						unico = data.autocompletions;
						data.autocompletions =[unico];
					}
					response( $.map( data.autocompletions, function( item ) {
					return {
						label: '<b>' + item.replace('<b>','----').replace('</b>','<b>').replace('----','</b>') + '</b>',
						value: decodeHtml(item.replace('<b>','').replace('</b>',''))
						}
					}));
				}
				});
		},
		minLength: 2,
		select: function( event, ui ) {
			//log( ui.item ? "Selected: " + ui.item.label : "Nothing selected, input was " + this.value);
			forzaRicerca(ui.item.value);
		},
		open: function() {
			$( this ).autocomplete('widget').css('z-index', '9999');
			$( this ).autocomplete('widget').css('border', '1px solid #AAAAAA');
			$( this ).autocomplete('widget').css('right', ($(window).width() - $('#istBanca-headerIconSearch').offset().left - 34) + 'px');
			$( this ).autocomplete('widget').css('left', 'auto');
			$( this ).autocomplete('widget').css('width', 'auto');
			$( this ).autocomplete('widget').css('top', '46px');
			$( this ).removeClass( "ui-corner-all" ).addClass( "ui-corner-top" );
			max = $( this ).autocomplete('widget').width();
			$('.ui-menu .ui-menu-item a').each(function( index ) {
				$(this).css('width', max);
			});
		},
		close: function() {
			$( this ).removeClass( "ui-corner-top" ).addClass( "ui-corner-all" );
		}
	}).data("autocomplete")._renderItem = function (ul, item)
	{
	     return $("<li></li>")
       	   .data("item.autocomplete", item)
	          .append($("<a></a>").html(item.label))
       	   .appendTo(ul);
	}
	
});
function forzaRicerca(txt) {
	obj = document.getElementById("istBanca-headerInputText");
	if(obj == null){
		obj = document.getElementById("istGruppo-headerInputText");
	}
	if(obj != null){
		obj.value = decodeHtml(txt);
		obj.form.submit(); 
	}
}

function decodeHtml(txt) {
	return txt.replace(/&apos;/g, '\'').replace(/&copy;/g, '©').replace(/&reg;/g, '®')
			.replace(/&euro;/g, '\u20ac').replace(/&Agrave;/g, 'À').replace(/&Aacute;/g, 'Á')
			.replace(/&Egrave;/g, 'È').replace(/&Eacute;/g, 'É').replace(/&Igrave;/g, 'Ì')
			.replace(/&Iacute;/g, 'Í').replace(/&Ograve;/g, 'Ò').replace(/&Oacute;/g, 'Ó')
			.replace(/&Ugrave;/g, 'Ù').replace(/&Uacute;/g, 'Ú').replace(/&agrave;/g, 'à')
			.replace(/&aacute;/g, 'á').replace(/&egrave;/g, 'è').replace(/&eacute;/g, 'é')
			.replace(/&igrave;/g, 'ì').replace(/&iacute;/g, 'í').replace(/&ograve;/g, 'ò')
			.replace(/&oacute;/g, 'ó').replace(/&ugrave;/g, 'ù').replace(/&uacute;/g, 'ú')
			.replace(/&amp;/g, '&');
}
function codeHtml(txt) {
	return txt.replace(/&/g, '&amp;').replace(/\'/g, '&apos;').replace(/©/g, '&copy;').replace(/®/g, '&reg;')
			.replace(/\u20ac/g, '&euro;').replace(/À/g, '&Agrave;').replace(/Á/g, '&Aacute;')
			.replace(/È/g, '&Egrave;').replace(/É/g, '&Eacute;').replace(/Ì/g, '&Igrave;')
			.replace(/Í/g, '&Iacute;').replace(/Ò/g, '&Ograve;').replace(/Ó/g, '&Oacute;')
			.replace(/Ù/g, '&Ugrave;').replace(/Ú/g, '&Uacute;').replace(/à/g, '&agrave;')
			.replace(/á/g, '&aacute;').replace(/è/g, '&egrave;').replace(/é/g, '&eacute;')
			.replace(/ì/g, '&igrave;').replace(/í/g, '&iacute;').replace(/ò/g, '&ograve;')
			.replace(/ó/g, '&oacute;').replace(/ù/g, '&ugrave;').replace(/ú/g, '&uacute;')
			;
}
</script>

		 
		
