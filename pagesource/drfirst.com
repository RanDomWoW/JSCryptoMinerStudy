						</div>
						<div ng-if="!item.tweet">
							<p class="event-date" ng-show="item.event_date">{{item.event_date}}</p>
							<h4 class="newsfeed-title"><a ng-href="{{item.link}}" ng-bind-html="item.title"></a></h4>
							<p class="newsfeed-excerpt" ng-bind-html="item.excerpt"></p>
						</div>
					</li>
				</ul>
			</div>
			<span class="hover-zone next hidden-xs"></span>
			<span class="hover-zone prev hidden-xs"></span>
		</div>
	</section>
</div>



