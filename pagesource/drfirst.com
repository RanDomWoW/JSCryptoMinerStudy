			<div class="newsfeed-viewport">
				<i ng-class="{loading: loading === true}" class="loader icon-refresh icon-spin"></i>
				<ul class="newsfeed-list" ng-class="{loading: loading, 'no-scroll': !hasScroll}" newsfeed-scroller ng-cloak>
					<li class="newsfeed-item box" ng-repeat="item in feed | filter: timestamp">
						<div class="newsfeed-icon">
							<i ng-class="'icon-' + item.type"></i>
						</div>
						<div ng-if="item.tweet">
							<h4 class="newsfeed-title"><a target="_blank" ng-href="https://twitter.com/DrFirst/status/{{item.tweet.id}}" ng-bind-html="item.tweet.author"></a></h4>
