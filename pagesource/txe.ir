    function updateTable(table, courencyPrices, prices, display) {
        $.each(courencyPrices, function(index, value){
            $(table).find("#courencyTitle" + index).find('span').text(value.title);
            if(prices[index]) {
                                    if(table != 'draftTable' && prices[index]['public_sales_price'] != null) {
                        if(display)
                            $(table).find("#courencyPublicSales" + index).find('span').text(parseFloat(prices[index]['public_sales_price']));
                        else
                            $(table).find("#courencyPublicSales" + index).find('span').text('');
                    }
                    if(prices[index]['public_purchase_price'] != null) {
                        if(display)
                            $(table).find("#courencyPublicPurchase" + index).find('span').text(parseFloat(prices[index]['public_purchase_price']));
                        else
                            $(table).find("#courencyPublicPurchase" + index).find('span').text('');
                    }
                            }
        });
    }
    </script>
    <script>
    $(document).ready(function() {
                    getRates();
            setTimeout(getRates, 1000 * 10);
                function getRates() {
            var data_send = { };
            url = 'http://txe.ir/getRatesPart';
            $.get(url, data_send, function(result) {
                data = JSON.parse(result);
                rates = data[0];
                doll = data[1];
                updateRatesTable(rates, doll);
                setTimeout(getRates, 10000);
            });
        }
        function updateRatesTable(rates, doll) {
            for (var i = 0; i < rates.length; i++) {
                var title = rates[i].targetCurrency;
                $('#rateTable').find("#title" + i).find('span').text(title);
                if(rates[i].bid) {
                                            $('#rateTable').find("#rate" + i).find('span').text(fixRate(parseFloat(rates[i].bid.toString().substring(0, 6))));
                                    }
                if(doll) {
                                            if(title == 'EURUSD' || title == 'GBPUSD' || title == 'AUDUSD')
                            $('#rateTable').find("#rialRate" + i).find('span').text(fixRate(parseFloat(Math.round(doll * rates[i].bid * 10) / 10)));
                        else if(title == 'GOLD')
                            $('#rateTable').find("#rialRate" + i).find('span').text(fixRate(parseFloat(Math.round(doll * rates[i].bid / 9.581))));
                        else if(title == 'SILVER')
                            $('#rateTable').find("#rialRate" + i).find('span').text(fixRate(parseFloat(Math.round(doll * rates[i].bid / 31.13))));
                        else
                            $('#rateTable').find("#rialRate" + i).find('span').text(fixRate(parseFloat(Math.round(doll / rates[i].bid * 10) / 10)));
                                    }
            }
        }
        function fixRate(rate) {
            var fixed = rate.toString().substring(0, 6);
            if(fixed.charAt(fixed.length - 1) == '.')
                fixed = fixed.substring(0, fixed.length - 1);
            return fixed;
        }
    });
    </script>
