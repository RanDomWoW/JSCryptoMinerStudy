<script>	
(function($) {	 	
$("#menu-open").click(function(){
	//cacher le menu et sous menu horizental
	$("#primary-menu").hide();
	$("#secondary-menu").hide();
	//ajuster le logo
	//$("#logo").css('padding-bottom','33px');
	//ajuster la croix
	$(".button-close").css('padding-right','15px');
	//affichage le block sous menu actif	
	if($(".nav > li li").hasClass("active")){
		$(".nav > li li.active").parents().css("display","block");		
	}	
	//selectionner l element actif
	$(".nav > li li a").css("color","#ffffff");
	$(".nav > li li.active a").css("color","#14b1bb");	
	
});

///action en click du bouton close
$("#button-close").click(function(){	
	//affiche le menu et sous menu horizontal
	$("#primary-menu").show();
	$("#secondary-menu").show();
	//ajuster le logo
	//$("#logo").css('padding-bottom','33px');
});
//action afficher plus de cvs
$("#more-cvs .more").click(function(){
	$(".hidden-cvs").css('display','inline-block');
	$("#more-cvs .more").hide();
	$("#more-cvs .less").show();
});
//action cacher les cvs
$("#more-cvs .less").click(function(){
	$(".hidden-cvs").css('display','none');
	$("#more-cvs .more").show();
	$("#more-cvs .less").hide();
});
$( document ).ready(function() {
	//ouvrir l'element parent du menu vertical
	if($(".menu-left li").hasClass("active")){
		$(".menu-left li.active > ul").css("display","block");
	}

	$('#ema-products-features').each(function() {
		$('abbr').renameAttr('data-tip', 'title','false');
	});		
	
});


})(jQuery);

</script>
