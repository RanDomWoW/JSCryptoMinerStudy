<script type="text/javascript">
	$('#select2').on('change', function() {
	  var value = $(this).val();
	  (changeImg);
	});
	function changeCapitulo(anime,capitulo, f){
		if(f){
			capitulo = capitulo.value;
			f.action = "/"+ anime +"/capitulo/"+ capitulo+"/";
			f.submit();
		}else{
			window.location.href = "/"+ anime +"/capitulo/"+ capitulo+"/";
		}

	}
	$(document).ready(function() {
		
		var results 	= $('#results-filtros');
		
		//Navegação dos Mais lidos
		$('#mais-lidos').on('click','.button', function () {
			var	_	= $(this);
			
			$('#mais-lidos .button' ).removeClass( "azul" );
			$('#mais-lidos .button' ).addClass( "cinza" );
			$('#mais-lidos .button-'+ _.data('id')).addClass( "azul" );
			$('#mais-lidos .button-'+ _.data('id')).removeClass( "cinza" );
			$('#mais-lidos .button-'+ _.data('id')).removeClass( "cinza" );
			
			$('#mais-lidos-dia').addClass( "display-none" );
			$('#mais-lidos-mes').addClass( "display-none" );
			$('#mais-lidos-semana').addClass( "display-none" );
			
			$('#mais-lidos-dia').removeClass( "display-ok" );
			$('#mais-lidos-mes').removeClass( "display-ok" );
			$('#mais-lidos-semana').removeClass( "display-ok" );
			
			if( _.data('id')==1){
				$('#mais-lidos-semana').addClass( "display-ok" );
			}else if( _.data('id')==2){
				$('#mais-lidos-dia').addClass( "display-ok" );
			}else{
				$('#mais-lidos-mes').addClass( "display-ok" );
			}
		});
		//Navegação dos Destaques
		$('#destaque').on('click','.button', function () {
			var	_	= $(this);
			
			$('#destaque .button' ).removeClass( "vermelho" );
			$('#destaque .button' ).addClass( "cinza" );
			$('#destaque .button-'+ _.data('id')).addClass( "vermelho" );
			$('#destaque .button-'+ _.data('id')).removeClass( "cinza" );
			$('#destaque .button-'+ _.data('id')).removeClass( "cinza" );
			
			$('#destaque-capitulos').addClass( "display-none" );
			$('#destaque-top-10').addClass( "display-none" );
			
			$('#destaque-capitulos').removeClass( "display-ok" );
			$('#destaque-top-10').removeClass( "display-ok" );
			
			if( _.data('id')==1){
				$('#destaque-capitulos').addClass( "display-ok" );
			}else{
				$('#destaque-top-10').addClass( "display-ok" );
			}
		
		});
		
		$('#header-filtros').on('click','.filtros', function () {
			var	_	= $(this);
		
			$.ajax({
				url:		"../lista-titulos.php",
				data:		{ string: _.data('filter')},
				type:		'post',
				success:	function (result) {
					results.html(result);
				}
			});
			$('.filtros' ).removeClass( "filtros-active" );
			$('.'+ _.data('filter')+'_filtro' ).addClass( "filtros-active" );
		});
		$('#header-filtros').find('span:first').trigger('click');
		
		$('#header-filtros').on('click','.filtros', function () {
			var	_	= $(this);
		
			$.ajax({
				url:		"../lista-generos.php",
				data:		{ string: _.data('filter')},
				type:		'post',
				success:	function (result) {
					results.html(result);
				}
			});
			$('.filtros' ).removeClass( "filtros-active" );
			$('.'+ _.data('filter')+'_filtro' ).addClass( "filtros-active" );
		});
		$('#header-filtros').find('span:first').trigger('click');
		
		$('#txtNomeBusca').keydown(function () {
			$.ajax({
				url: "/busca.php?act=getSearch&label=" + $(this).val(),
			
				success: function (e) {
					$("#search-results").css("display", "block");
					$("#search-results").html(e);
				}
			});	
		});
		$('#txtNomeBusca').on('click', function () {
			if($(this).val() == 'O que deseja buscar?') {
				$(this).val('');
			}
		}).blur(function () {
			if(!$(this).val()) {
				$(this).val('O que deseja buscar?');
			}
		});
	    $('#imagem').on('click','.imagemCapitulo', function () {
		  if($('.imagemCapitulo').data('total') >= $('.imagemCapitulo').data('next')){
				changeImg('','',$('.imagemCapitulo').data('next'), false, false, 1);
			}
		});
		$(document).keydown(function(e) {
		switch(e.which) {
			case 37: // left
				if($('.imagemCapitulo').data('previous') >= 1){
					changeImg('','',$('.imagemCapitulo').data('previous'), false, false, 1);
				}
			break;
			case 39: // right
				if($('.imagemCapitulo').data('total') >= $('.imagemCapitulo').data('next')){
					changeImg('','',$('.imagemCapitulo').data('next'), false, false, 1);
				}
			break;
			default: return;
		}
		e.preventDefault(); // prevent the default action (scroll / move caret)
		});

		$(document).mouseup(function (e){
			var container = $("#search-results");
		
			if (!container.is(e.target) // if the target of the click isn't the container...
				&& container.has(e.target).length === 0) // ... nor a descendant of the container
			{
				container.hide();
				$("#txtNomeBusca").val('O que deseja buscar?');
			}
		});
	});
	
	function changeImg(anime,capitulo,src,combo,option,view){
		if(combo){
			if(option=='true'){
				src = $(".select1").val();
			}else{
				src = $(".select2").val();
			}
		}
		$.ajax({
			url: "/capitulo.php?act=getImg&anime=" + anime +"&capitulo=" + capitulo + "&src="+ src +"&view="+ view,
		
			success: function (e) {
				$("#imagem").css("display", "inline-block");
				$("#imagem").html(e);
				$("#imgSlow").fadeIn("slow");
				$(document).scrollTop( $("#imgSlow").offset().top ); 				
			}
		});	
	}
</script>
