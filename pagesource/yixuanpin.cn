<!DOCTYPE html >
<html lang="zh-cn" ng-app="starter">

<head>
  <base href="/">
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" pageEncoding="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="renderer" content="webkit">
  <meta http-equiv="X-UA-Compatible" content="chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta HTTP-EQUIV="pragma" CONTENT="no-cache">
  <meta HTTP-EQUIV="Cache-Control" CONTENT="no-cache, must-revalidate">
  <meta HTTP-EQUIV="expires" CONTENT="Wed, 26 Feb 1997 08:21:57 GMT">
  <title ng-bind="title">易选品-跨境电商选品专家</title>
  <meta name="keywords" content="易选品，wish选品，wish，选品，大数据选品，人工智能选品，跨境电商，出口电商，选爆品" />
  <meta name="descriptionkeywords" content="易选品是所问数据基于人工智能和大数据技术开发的面向中国跨境出口电商的选品平台，利用深度学习算法为卖家提供潜力爆款发掘、相同商品/相似商品/行业竞争分析、个性化推荐、流行趋势捕捉等智能分析服务，助力中国卖家走向全球。"
  />
  <link rel="shortcut icon" href="images/favicon.ico" type="images/x-icon" />
  <!--STYLES-->
  <link rel="stylesheet" href="/css/app-85fb5ce665.css">
  <!--STYLES END-->
</head>

<body>
  <div id="cont">
    <div id="askingdata-browser-info" class="as-alert" style="z-index:999">
      <span id="browser-text"></span>
      <i id="cancel-browser" class="fa fa-close pull-right"></i>
    </div>

    <div id="notice" class="as-alert" style="z-index:888">
      <span id="notice-text"></span>
      <span id="cancel-notice" class="pull-right as-cancel">不再提示</span>
    </div>

  </div>
  <img src="/images/common/bgm-index.jpg" alt="易选品-跨境电商选品专家" class="hide">

  <div ui-view style="position: relative" class="main-view">

  </div>

  <!--SCRIPTS-->
  <script src="/js/f1ol-901d7f2b26.js"></script>
  <script src="/js/f2as-b6c6da52e7.js"></script>
  <!--SCRIPTS END-->
  <script type="text/javascript">
    // 流量监控
    var _hmt = _hmt || [];
    (function () {
      var hm = document.createElement("script");
      hm.src = "https://hm.baidu.com/hm.js?b067a69aef80b7d8a08f9f307667d672";
      var s = document.getElementsByTagName("script")[0];
      s.parentNode.insertBefore(hm, s);
    })();

    //监测浏览器类型
    (function () {
      var ua = navigator.userAgent.toLowerCase();
      var version;
      var a = document.getElementById('askingdata-browser-info');
      var b = document.getElementById('browser-text');
      var c = document.getElementById('cont');
      if (!!window.ActiveXObject || "ActiveXObject" in window) {
        try {
          version = ua.match(/msie ([\d.]+)/)[1];
          if (version <= 9.0) {
            // ie8 及以下版本
            c.style.display = 'block';
            a.style.display = 'block';
            b.innerHTML = '为了获得更好的浏览体验，请您升级到IE9以上版本，推荐使用谷歌浏览器（chrome）';
          } else if (version > 9.0) {
            c.style.display = 'none';
            a.style.display = 'none';
          }
        } catch (error) {
          a.style.display = 'none';
          c.style.display = 'none';
        }
      } else if (ua.indexOf("chrome") > -1) {
        version = ua.match(/chrome\/([\d.]+)/)[1];
        if (parseFloat(version) <= 30) {
          // 2015年之后的版本
          c.style.display = 'block';
          a.style.display = 'block';
          b.innerHTML = '为了获得更好的浏览体验，请您升级谷歌浏览器（chrome）到最新版';
        }
      } else if (ua.indexOf("firefox") > -1) {
        version = ua.match(/firefox\/([\d.]+)/)[1];
        if (version <= 30.0) {
          // 火狐浏览器 低于40.0.0时
          a.style.display = 'block';
          c.style.display = 'block';
          b.innerHTML = '为了获得更好的浏览体验，请您升级火狐浏览器（firefox）到最新版';
        }
      } else if (ua.indexOf("safari") > -1) {

      } else {
        a.style.display = 'block';
        c.style.display = 'block';
        b.innerHTML = '为了获得更好的浏览体验，推荐您使用谷歌浏览器（chrome）';
      }
      $('#cancel-browser').click(function () {
        if ($('#notice').css('display') == 'block') {
          a.style.display = 'none';
        } else {
          a.style.display = 'none';
          c.style.display = 'none';
        }
      })
    })();

    //版本更新
    (function () {
      $.ajax({
        async: false,
        url: '/api/version/notice',
        type: "get",
        success: function (data) {
          notice = data.data; //所有公告
        },
      });

      if (notice) {
        if (notice.wish) {
          var wishVersion = localStorage.wishVersion;
          var wish = notice.wish[0].date;
          if (wishVersion != wish) {
            localStorage.setItem('wishVersion', wish);
            localStorage.setItem('wish', true)
          }
        }
        if (notice.update) {
          var updateVersion = localStorage.updateVersion;
          var update = notice.update[0].date;
          if (updateVersion != update) {
            localStorage.setItem('updateVersion', update);
            localStorage.setItem('update', true)
          }
        }
        if (notice.askingdata) {
          var askingdataVersion = localStorage.askingdataVersion;
          var askingdata = notice.askingdata[0].date;
          if (askingdataVersion != askingdata) {
            localStorage.setItem('askingdataVersion', askingdata);
            localStorage.setItem('askingdata', true)
          }
        }
        showType()
      }

      //根据缓存显示相应内容
      function showType() {
        for (var i in notice) {
          if (localStorage.getItem(i)) {
            $('#cancel-notice').attr('name', i)
            $('#cont').css('display', 'block');
            $('#notice').css({ 'display': 'block' });
            if (i == 'wish') {
              $('#notice').css('background', 'rgb(250, 157, 179)');
              $('#cancel-notice').html('不再提示')
            }
            if (i == 'update') {
              $('#notice').css('background', 'rgb(133, 205, 177)')
              $('#cancel-notice').html('我知道了')
            }
            if (i == 'askingdata') {
              $('#notice').css('background', '#f28021')
              $('#cancel-notice').html('不再提示')
            }
            $('#notice-text').html(notice[i][0].message);
            break
          } else {
            $('#cont').css('display', 'none');
            $('#notice').css('display', 'none')
          }
        }
      }


      $('#cancel-notice').click(function () {
        var cancelType = $(this).attr('name');
        localStorage.removeItem(cancelType);
        showType()
      })
    })();
  </script>
</body>

</html>