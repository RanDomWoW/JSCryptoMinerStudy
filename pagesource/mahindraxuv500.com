        $(document).ready(function () {
            'use strict';
            var myVideo = document.getElementById("videoPlay");
            $('a.clickHere').click(function () {
                $(this).addClass("hide");
                myVideo.play();
                if ($('.secondScreen').hasClass('open')) {
                    $('.secondScreen').removeClass('open');
                } else {
                    setTimeout(function () {
                        $('.secondScreen').addClass('open');
                    }, 1000);
                    setTimeout(function () {
                        $("#sunroofPopup").fadeOut("slow", function () {
                            $("#sunroofPopup").remove();
                        });
                    }, 7000);
                }
            });

            $('a.close').click(function () {
                $("#sunroofPopup").fadeOut("slow", function () {
                    $("#sunroofPopup").remove();
                });
            });




            $('.whatnewBtn').click(function () {

                $('html, body').animate({ scrollTop: $('#whatnew').position().top }, 100);

            });
        });
        // Load the IFrame Player API code asynchronously.
        /*var tag = document.createElement('script');
        tag.src = "https://www.youtube.com/player_api";
        var firstScriptTag = document.getElementsByTagName('script')[0];
        firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);*/

        var videoId = 'Umcr6R0_C4o';
        var startSeconds = 0;
        var endSeconds = 9;

        var player;
        var playerConfig = {
            height: '360',
            width: '640',
            videoId: videoId,
            playerVars: {
                autoplay: 1, // Auto-play the video on load
                controls: 0, // Show pause/play buttons in player
                showinfo: 0, // Hide the video title
                modestbranding: 1, // Hide the Youtube Logo
                fs: 1, // Hide the full screen button
                cc_load_policy: 0, // Hide closed captions
                iv_load_policy: 3, // Hide the Video Annotations
                start: startSeconds,
                end: endSeconds,
                autohide: 0, // Hide video controls when playing
            },
            events: {
                'onStateChange': onStateChange,
                onReady: function (e) {
                    e.target.mute();
                }
            }
        };

        function onYouTubePlayerAPIReady() {
            player = new YT.Player('ytplayer', playerConfig);

        }

        function onStateChange(state) {
            if (state.data === YT.PlayerState.ENDED) {
                player.loadVideoById({
                    videoId: videoId,
                    startSeconds: startSeconds,
                    endSeconds: endSeconds
                });
            }
        }

    </script>
