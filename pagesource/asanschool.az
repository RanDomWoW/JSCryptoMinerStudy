            $('#resp').html('Könüllü olduğu mərkəz daxil edilməyib');
            $('#center').focus();
            e.preventDefault();
        }
        if (konullu==undefined) {
            $('#resp').html('Könüllü fəaliyyətində qrup rəhbəri olmusunuz? seçilməyib');
