        if ($.trim(vakansiya).length <= 2) {
            $('#resp').html('Master 2 proqramı üzrə vakaniyalar seçilməyib');
            $('#vakansiya').focus();
            e.preventDefault();
        }

        if ($.trim(name).length <= 2) {
            $('#resp').html('Soyadı, adı, atasının adı daxil edilməyib');
