<script>
    function handleEnter(inField, e)
    {
        var charCode;

        //Get key code (support for all browsers)
        if(e && e.which)
        {
            charCode = e.which;
        }
        else if(window.event)
        {
            e = window.event;
            charCode = e.keyCode;
        }

        if(charCode == 13)
        {
           buscar();
        }
    }

    function buscar()
    {
      var resultado = encodeURI(document.getElementById("input_busca").value);
      document.location = "http://multirio.rj.gov.br/index.php/busca?txt=" + resultado;
    }

    function submit()
    {
        document.getElementById("login-form").submit();
    }
    function login()
    {
        var log = document.getElementById("area_loging");
        log.style.display = "";
        var logb = document.getElementById("login_busca_som_btnlogin");
        logb.style.display = "none";
    }
    function loginb()
    {
        var log = document.getElementById("area_loging");
        log.style.display = "none";
        var logb = document.getElementById("login_busca_som_btnlogin");
        logb.style.display = "";
    }
</script>
