  function SetQuoteHeight()
  {
    var winW = $(window).width();
    if(winW > 992 )
    {
      var maxHimg = 0;
      $('.press-info .col .title').css('height','auto');
      $('.press-info .col .title').each(function(){
        var curH = $(this).height();
        if(maxHimg < curH)
        {
          maxHimg = curH;
        }
      });
      $('.press-info .col .title').height(maxHimg);

      var maxH = 0;
      $('.press-info .col').css('height','auto');
      $('.press-info .col').each(function(){
        var curH = $(this).outerHeight();
        if(maxH < curH)
        {
          maxH = curH;
        }
      });
      $('.press-info .col').height(maxH);
    }
    else
    {
    	$('.press-info .col .title').css('height','auto');
      	$('.press-info .col').css('height','auto');
    }
  }
