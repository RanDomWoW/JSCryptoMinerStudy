<script>
  $(document).ready(function(){
  	//selectbox
    $('.select-no select').selectBoxIt();
    $('.sp-dropdown select').multiselect({
      afterDeselect: function(values){
        UpdateGiftWrapSelection();
      },
      afterInit: function(){
        console.log('inti')
      	$('.giftwrap-container .ms-options').append('<a href="#">Done Adding</a>');
      }
    });
    $('#no-of-products').change(function(){
    	$('.sp-dropdown').hide();
      	$('.sp-dropdown:lt(' + $(this).val() + ')').show();
    });
    $('.sp-dropdown select').change(function(){
      UpdateGiftWrapSelection();
    });
    
  });
  function UpdateGiftWrapSelection(){
  	$('.sp-dropdown select option').removeAttr('disabled');
      $('.sp-dropdown .ms-reflow').show();
      $('.sp-dropdown select').each(function(){
      	var vals = $(this).val();
        var element = this;
        
        if(vals != null){
          vals.forEach(function(value){
              var selects = $('.sp-dropdown select').not(element);
            	var parent = $(selects).closest('.sp-dropdown');
              $('option[value="'+value+'"]',$(selects)).attr('disabled','disabled');
              $('.ms-options [value="'+value+'"]',$(parent)).closest('.ms-reflow').hide();
          });
        }
      });
    
    //custom inject for the case when there's only one product in cart (to save customer from extra clicks ad efforts)
     
     
        
    
        
                
      
           $('.finish-btn').removeClass('add-margin');
      
    //*End of custom Inject
  }
</script>
<script type="text/javascript">
  var giftScroll = 0;
  
  $(document).ready(function(e){
    //fancybox-giftwrap
    $('.gift-card-title').focus(function(event){
      	event.stopPropagation()
    	parent = $(this).closest('.sp-dropdown');
      	$('.sp-dropdown').removeClass('drop-active');
      	$('.ms-options-wrap').removeClass('arrow-switch');
      	$('.ms-options').hide();
      	$(parent).addClass('drop-active');
      	$('.ms-options-wrap',$(parent)).addClass('arrow-switch');
      	$('.ms-options',$(parent)).css('display','block');
      	$('.ms-options',$(parent)).addClass('show-it');
    });
    $(document).on('click','[href="#cart"]',function(e){
    	$('#cart .mm-subtitle.wrap-container').html($('#side-wrap'));
      $('#cart #side-wrap .mm-list').removeClass();
      $('#cart #side-wrap .mm-subtitle').remove();
      
      
      $('#cart [name="attributes[gift-wrapping]"][value=""]').after($('#cart .gift-checkbox'));
      $('#cart .gift-checkbox').removeAttr('id').removeClass('mm-panel').removeClass('mm-hidden');
    });
    
    $(document).on('click','.gift-wrap-trigger:not(.no-trigger)',function(e){
    	e.preventDefault();
      	UpdateGiftWrapSelection();
      	$.fancybox({ href: "#giftwrap-fancybox",
        'transitionIn'	:	'elastic',
              'fitToView'		: 	true,
               wrapCSS : 'giftbox-wrap',
               helpers : {
                    overlay : {
                        locked: true
                    } 
                },
                    beforeLoad: function(){
                   		giftScroll = $(window).scrollTop();
                   },
                   afterLoad: function(){
                   	 	$('.giftwrap-container .ms-options-wrap > .ms-options ul li:last-child').click(function(e){
                            e.preventDefault();
                            var parent = $(this).closest('.sp-dropdown');
                          	$(parent).removeClass('drop-active');
                          $('.ms-options-wrap',$(parent)).removeClass('arrow-switch');
                          $('.ms-options',$(parent)).removeClass('show-it');
                          $('.ms-options',$(parent)).hide();
                        });
                   },
                   beforeClose: function(){
                   	 	$(window).scrollTop(giftScroll);
                   },
                   afterClose: function(){
                   		$(window).scrollTop(giftScroll);
                     setTimeout(function(){
                     	$(window).scrollTop(giftScroll);
                     },250)
                     setTimeout(function(){
                     	$(window).scrollTop(giftScroll);
                     },500);
                   }
        });
    })
    
    
    $('.btn-finish').on('click', function(e){
      //$.fancybox.close();
    });
  });

  
  $(window).on('load', function(e){
    $('.sp-dropdown .ms-options-wrap button').click(function(e){
      $(this).parent('.ms-options-wrap').toggleClass('arrow-switch');
      
      $(document).mouseup(function (e)
      {
        var container = $(".sp-dropdown");
        if (!container.is(e.target)
            && container.has(e.target).length === 0){
          $('.ms-options-wrap').removeClass('arrow-switch');
          $('.ms-options').removeClass('show-it');
        }
      });
    });
    
    $('.gw-list ul').matchHeight();
  });
  $(document).ready(function(){
   $('[name*="gift-wrapping"]').click(function(){
      var parent = $(this).closest('div');
      if($('[name*="gift-wrapping"]:checked',$(parent)).length > 0){
      	$('#checkout').addClass('gift-wrap-trigger');
        $('[href="/checkout"]').addClass('gift-wrap-trigger');
        var giftsCount = $('.giftwrap_list .gw-col .gift-wrap-trigger').length;
        if(giftsCount > 0){
        	$('[href="/checkout"]').addClass('no-trigger')
            $('#checkout').addClass('no-trigger')
        }else{
        	$('[href="/checkout"]').removeClass('no-trigger')
            $('#checkout').removeClass('no-trigger')
        }
        $('.giftwrap_list').show();
        UpdateGiftCartCount();
      }else{
      	$('#checkout').removeClass('gift-wrap-trigger');
        $('[href="/checkout"]').removeClass('gift-wrap-trigger');
        $('[href="/checkout"]').addClass('no-trigger')
            $('#checkout').addClass('no-trigger')
        $('.giftwrap_list').hide();
        Shopify.Cart.GiftWrap.set(0,false);
        ClearGiftWrapList(null,true);
      }
     
    });
    $(document).on('click',function(e){
      var clicktarget = e.target;
      $('.ms-options-wrap').each(function(){
        var parent = $(this).closest('.sp-dropdown');
        if($(this).hasClass('arrow-switch') || $(parent).hasClass('required')){
        	$(parent).addClass('drop-active');
        }else{
        	$(parent).removeClass('drop-active');
        }
        var gvName = $('.gift-card-title',$(parent));
        var gvOpts = $('.ms-options',$(parent));
        
        if(!$(gvName).is(clicktarget) && !$(gvOpts).is(clicktarget) && $(gvOpts).has(e.target).length === 0){
        	$('.ms-options',$(parent)).removeClass('show-it');
        }
      });
    })
    
    $('#giftwrap-fancybox .finish-btn a').click(function(e){
    	e.preventDefault();
      var errors = false;
      $('#giftwrap-fancybox .gift-card-title').each(function(){
      	var val = $(this).val();
        var parent = $(this).closest('.sp-dropdown');
        if($(parent).is(':visible')){
        if(val != ''){
            $(this).closest('.sp-dropdown').removeClass('drop-active').removeClass('required');
        }else{
        	$(this).closest('.sp-dropdown').addClass('drop-active').addClass('required');
          	errors = true;
        }
        }
      });
      if(!errors){
      	$(this).html('Loading...');
      	SetGiftAttributes();
      }
    });
  });
  function SetGiftAttributes(){
  	var count = 0;
      	var giftCount = 0;
      	var dataObj = new Object();
      	
        $('#giftwrap-fancybox .sp-dropdown').each(function(){
          	count++;
			if($(this).is(':visible'))
            {
              var prods = $('[name="gift-wrap-'+count+'-select"]',this).val();
              if(prods != null){
                if(prods.length > 0){
                  giftCount++;
                  
                  var title = $('[name="gift-wrap-'+count+'-title"]',this).val();
                  if(title == '' || title == null){
                      title = "Gift Wrap #"+giftCount;
                  }
                  var prodTitles = [];
                  var prodIDs = [];
                  prods.forEach(function(value){
                  	var valObj = value.split('#');
                    var valIdsObj = valObj[0].split('*');
                    prodIDs.push(valIdsObj[0]);
                    prodTitles.push(valObj[1]);
                  });
                  
                  dataObj['Gift Wrap Titles #'+giftCount] = title;
                  dataObj['Gift Wrap Names #'+giftCount] = prodTitles.join();
                  dataObj['Gift Wrap IDs #'+giftCount] = prodIDs.join();
                }
              }
            }
        });
      	ClearGiftWrapList(dataObj);
  }
  $(window).load(function(){
  	  $('[name*="gift-wrapping"]').each(function(){
      var parent = $(this).closest('div');
        if($(this).is(':visible')){
          if($('[name*="gift-wrapping"]:checked',$(parent)).length > 0){
            $('#checkout').addClass('gift-wrap-trigger');
            $('[href="/checkout"]').addClass('gift-wrap-trigger');
            var giftsCount = $('.giftwrap_list .gw-col .gift-wrap-trigger').length;
            if(giftsCount > 0){
                $('[href="/checkout"]').addClass('no-trigger')
                $('#checkout').addClass('no-trigger')
            }else{
                $('[href="/checkout"]').removeClass('no-trigger')
                $('#checkout').removeClass('no-trigger')
            }
            $('.giftwrap_list').show();
            UpdateGiftCartCount();
          }else{
            $('#checkout').removeClass('gift-wrap-trigger');
            $('[href="/checkout"]').removeClass('gift-wrap-trigger');
            $('[href="/checkout"]').removeClass('no-trigger')
                $('#checkout').removeClass('no-trigger')
            $('.giftwrap_list').hide();
            Shopify.Cart.GiftWrap.set(0,false);
            ClearGiftWrapList(null,true);
          }
        }
    });
     
  });
  function UpdateGiftCartCount(){
    var warpCount = 0;
    $('#giftwrap-fancybox .sp-dropdown').each(function(){
      if($(this).is(':visible'))
      {
        var vals = $('select',this).val();
        if(vals != null){
            if(vals.length > 0){
                warpCount++;
            }
        }
      }
      
    });
    Shopify.Cart.GiftWrap.set(warpCount);
  }
  
  function UpdateGiftWrapList(redirect = true){
  	$('.giftwrap_list .giftwrap-row').html('');
	jQuery.ajax({
		type: 'GET',
		url: '/cart.json', 
		dataType: 'json',
		success: function(cart) {
          $('#giftwrap-fancybox .finish-btn a').html('ok, finished');
			var attrs = cart.attributes;
          	var gCounts= 0;
          $('.giftwrap_list .giftwrap-row').html('');
			for(var i = 1; i <= Math.ceil(Object.keys(attrs).length/3); i++){
				try{
					
					var title = attrs['Gift Wrap Titles #'+i];
					var namesRaw = attrs['Gift Wrap Names #'+i];

					var namesObj =[];
					if(namesRaw != '' && namesRaw != null){
						namesObj = namesRaw.split(',');
					}
					if(namesObj.length > 0){
						var temp = $('.giftwrap_list .template .gw-col').first().clone();

						$('h5',$(temp)).html(title + '<a href="#" class="gift-wrap-trigger"><sub>Edit</sub></a>');
						namesObj.forEach(function(val){
							$('ul',$(temp)).append('<li>'+val+'</li>');
						});

						$('.giftwrap_list .giftwrap-row').append($(temp));
                      gCounts++;
                      	$('.gw-list ul').matchHeight();
                      	$.fn.matchHeight._update()
					}
				}catch(err){console.log(err)}
			}
          if(gCounts > 0){
          	$('.mm-subtitle.wrap-container').show();
            $('[href="/checkout"]').addClass('no-trigger')
            $('#checkout').addClass('no-trigger')
          }else{
          	$('.mm-subtitle.wrap-container').hide();
            $('[href="/checkout"]').removeClass('no-trigger')
            $('#checkout').removeClass('no-trigger')
          }
          $('#cart_form .giftwrap-row .mm-list').removeClass('mm-list');
          $('#cart_form .giftwrap-row .mm-subtitle').removeClass('mm-subtitle');
          $('#cart_form .giftwrap-row .mm-subclose').remove();
        	$('#giftwrap-fancybox .finish-btn a').html('ok, finished'); 
              $.fancybox.close();
          
		},
      error: function(){
        if(redirect){
      	$('#giftwrap-fancybox .finish-btn a').html('ok, finished');
        }
      }
	  });
  }
  function ClearGiftWrapList(updateData, clear = false){
  	jQuery.ajax({
		type: 'GET',
		url: '/cart.json', 
		dataType: 'json',
		success: function(cart) {
			var attrs = cart.attributes;
          	var attrNames = Object.keys(attrs);
          if(attrNames.length > 1){
          	attrNames.forEach(function(value){
              
                var dataObj = new Object();
                  dataObj[value] = null;
              	jQuery.ajax({
                  type: 'POST',
                  url: '/cart/update.js', 
                  data: { attributes: dataObj }, 
                  dataType: 'json',
                  async: false,
                  success: function(cart) {
                    if(Object.keys(cart.attributes).length <= 0){
                      jQuery.ajax({
                        type: 'POST',
                        url: '/cart/update.js', 
                        data: { attributes: updateData }, 
                        dataType: 'json',
                        success: function() {
                          if(!clear){
                            UpdateGiftWrapList();
                            UpdateGiftCartCount();
                          }
                        }
                      });
                    }
                  }
                });
              
            });
          }else{
          	jQuery.ajax({
                        type: 'POST',
                        url: '/cart/update.js', 
                        data: { attributes: updateData }, 
                        dataType: 'json',
                        success: function() {
                          if(!clear){
                            UpdateGiftWrapList();
                            UpdateGiftCartCount();
                          }
                        }
                      });
          }
          
		}
	  });
  }
  $(document).ready(function(){
  	SetGiftAttributes();
   
  });
</script>
