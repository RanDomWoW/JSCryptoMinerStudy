  $(document).ready(function(){
  	waypointsImages = $('.image-lazy-load').waypoint(function(direction) {
      	var curKey = this.key;
      	var nextIndex = curKey.replace('waypoint-','');
  		var curElem = $(this.element);
      	LazyLoadImageTrigger(curElem,nextIndex);
    }, {
      offset: '95%',
      triggerOnce: true 
    });
    
  });
  function LazyLoadImageTrigger(element,curIndex)
  {
  		var src= $(element).attr('data-src');
  		$(element).attr('src',src);
    	
    	nextImgElem = parseInt(curIndex)+1;
    	setTimeout(function(){
          if(waypointsImages.length >= (nextImgElem+1))
          {
            
          	var nextElem = waypointsImages[nextImgElem].element;
            LazyLoadImageTrigger(nextElem,nextImgElem++);
          }
        },3000);
  }
  $(window).load(function(){
  	BindLoadAnimation();
  });
  
</script>

