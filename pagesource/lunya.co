  function SharePinterest(link,media,desc)
  {
    if(!desc || desc=="")
    { 
      desc = $('meta[name="description"]').attr("content");
    }
    if(!link || link=="")
    {
      link="http://www.lunya.co";
    }
    if(!media || media=="")
    {
      //media='http://cdn.shopify.com/s/files/1/0267/0987/t/5/assets/store_featured_image1.jpg?13070';
      media='http:' + '//cdn.shopify.com/s/files/1/0267/0987/t/349/assets/logo.png?11985144711456746919'
    }
    var url=encodeURIComponent(link)+'&media='+encodeURIComponent(media)+'&description='+encodeURIComponent(desc);
        
    window.open('http://www.pinterest.com/pin/create/button/?url='+url,'_blank', 'width=700, height=300');
  }
  
  function ShareTweet(desc,link,name)
