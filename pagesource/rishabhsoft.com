  function deleteData(id,termtype) {
        var tID = id;
        var tType = termtype;
        
        $('.filters .filterlist a').each(function(){
            //alert($(this).attr('id'));
            //alert(tID);
           if($(this).attr('id') == tID) {
              $(this).parent().remove();
           }
        });
        if(tType == 1) {
            $(".terms_ids .checked input[type=checkbox]").each(function() {
                if($(this).val() == tID) {
                  //alert(1);  
                  $(this).parent().removeClass('checked');  
                  $(this).prop('checked',false);
                  var checkedValCat = [];
                    $(".terms_ids .checked input[type=checkbox]").each(function() {
                      checkedValCat.push($(this).val());
                    });
                    
                    $("#cateIdList").val(checkedValCat);

                    var cateIdListVal = $("#cateIdList").val();
                    var tagIdListVal = $("#tagIdList").val();
                    custompost_ajax_get(cateIdListVal, tagIdListVal);
                    //customterms_ajax_get(cateIdListVal, tagIdListVal);
                }
            });
           
        } else if(tType == 2) {
            $(".tag_ids .checked input[type=checkbox]").each(function() {
               if($(this).val() == tID) {
                   //alert(2);
                   $(this).parent().removeClass('checked');
                   $(this).prop('checked',false);
                   var checkedValTag = [];
                    $(".tag_ids .checked input[type=checkbox]").each(function() {
                      checkedValTag.push($(this).val());
                    });
                    
                    $("#tagIdList").val(checkedValTag);

                    var cateIdListVal = $("#cateIdList").val();
                    var tagIdListVal = $("#tagIdList").val();
                    custompost_ajax_get(cateIdListVal, tagIdListVal);
                    //customterms_ajax_get(cateIdListVal, tagIdListVal);
                }
           });
        }
    } 
    
 

