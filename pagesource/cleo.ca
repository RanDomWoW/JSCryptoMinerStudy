<script type="text/javascript">
	/* Makes the pagination links unique for cached pages, for left navigation and top mega menu */ 

	jQuery(document).ready(function() {
		addGsizeLinkParameter();
		changeMenuLinks();
	});

	function addGsizeLinkParameter() {
		jQuery('div.featuredContent a, div.menu-wrapper a, a.breadcrumb-last, a.level-1, a.refinement-link').each(function() {
			var concat = '?';
			if(this.href.indexOf("?") > -1){
				concat = '&';
			} 
			this.href = this.href + concat + 'gsizelink=gsizelinkrep';
		});
	}

	jQuery(window).smartresize(function(){
		changeMenuLinks();
	});

	function changeMenuLinks() {
		var gsizelink = jQuery.cookie("gsize");
		var isfixgridtwo = jQuery.cookie("fixgridtwo");

		if( jQuery('#wrapper').width() <= app.responsive.mobileLayoutWidth   ) {
	   		/* is mobile */
	  			gsizelink = "two";
	  			jQuery.cookie("gsize", "two", { path: '/', expires: 1 });
		} else {
			/* is desktop */
			if((gsizelink == undefined) || (gsizelink == 'two' && isfixgridtwo) ) {
				gsizelink = "three";
				jQuery.cookie("gsize", "three", { path: '/', expires: 1 });
			}
		}

		jQuery('div.featuredContent a, ul#category-level-1 a, div.menu-wrapper a, div.pt_product-search-result a.breadcrumb-last, a.level-1').each(function() {
			this.href = this.href.replace('gsizelinkrep', gsizelink);
			this.href = this.href.replace('two', gsizelink);
			this.href = this.href.replace('three', gsizelink);
			this.href = this.href.replace('four', gsizelink);
		});
	}
</script>

