<script type="text/javascript">
	var automaticCollapse = true;

	var enablebxsliderbticker = function() {
		$('.bticker-slider').bxSlider({
			pager: false,
			slideWidth: 300,
			slideMargin: 10,
			inifiniteLoop: true,
			onSlideNext: function($slideElement, oldIndex, newIndex){ automaticCollapse = false; },
			onSlidePrev: function($slideElement, oldIndex, newIndex){ automaticCollapse = false; },
			maxSlides: 3
		});
	}

	function collapseTicker() {
		// Does not automatically collapse if user touched the slider
		if(automaticCollapse) {
			$('.bticker-container').addClass('collapsed');
			$('.bticker-inner').slideUp();
			$('.bticker-overlay').addClass('collapsed');
			$('.bticker-overlay').removeClass('initial');
		}
	}

	function pushCollapseTicker() {
		$('.bticker-container').addClass('collapsed');
		$('.bticker-inner').removeClass('visually-hidden');
		$('.bticker-inner').hide();
		$('.bticker-overlay').addClass('collapsed');
		$('.bticker-overlay').removeClass('initial');
	}

	$('.bticker-tab').click(function() {
		$( ".bticker-inner" ).slideToggle( "slow", function() {
			$('.bticker-container').toggleClass('collapsed');
			if($('.bticker-container').hasClass('collapsed')) {
				$('.bticker-overlay').addClass('collapsed');
			} else {
				$('.bticker-overlay').removeClass('collapsed');
			}
			
			$('.bticker-overlay').removeClass('initial');
			automaticCollapse = false;
		});
	});

	$(document).ready(function(){
		enablebxsliderbticker();

		if ($.cookie('bTicker')){
			setTimeout(function(){
				pushCollapseTicker();
			}, 50);
		} else {
			$('.bticker-inner').removeClass('visually-hidden');
			$.cookie('bTicker', 'true', { path: '/' });	
		}

		$('.bticker-overlay').click(function(){
			pushCollapseTicker();
		});
		
		$('.bticker-container').on('click', function(e) {
			if (e.target !== this)
				return;
			pushCollapseTicker();
		});

		setTimeout(function(){
			collapseTicker();
		}, 5000);
	});
</script>	
		
	
