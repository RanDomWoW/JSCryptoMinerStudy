<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
<script type="text/javascript">
		
			$(document).ready(function(){
				jQuery.fn.extend({
				    toggleText: function(){
						var text = $(this).text();
						var altText = $(this).attr('data-alttext');
						$(this).text(altText);
						$(this).attr('data-alttext',text);	    	
				        return this;
				    },
				    applyTdColors: function(){
						if($(this).hasClass('preserveColors')) return;
						$(this).each(function(){
					    	var i = 3;
				    		$($('td', this).not('[style*="display: none"]').get().reverse()).each(function() {
				    			$(this).addClass('color'+i);
								if(i > 0) i-=1;
							});
					    	var i = 3;
				    		$($('th', this).not(':first').not('[style*="display: none"]').get().reverse()).each(function() {
				    			$(this).addClass('color'+i);
								if(i > 0) i-=1;
							});
				    	});
				    }
				});

				$('.smsFNav form').on('change', function(event){
					event.preventDefault();
					window.location = $('[name="liens"]').val();
				});

				$('.smsMenu').on('click', function(event){
					$('.smsMenu > nav > .smsMNav').toggle();
				});

				$('.smsMNav > li.sub > a').on('click', function(event){
					event.preventDefault();					
				});

				$('.smsMNav > li.sub').on('click', function(event){
					event.stopPropagation();
					$('.smsMNav li.sub').removeClass("active");
					$(this).addClass("active");
				});
				
				$('.hNav .hasSub').mouseenter(
					function(){
						$('.hNavSub', this).show();
					}).mouseleave(
					function(){
						$('.hNavSub', this).hide();						
					});
					
				$('.assDEtranger .contenttable tr').applyTdColors();
			});
		
		</script>
<script type="text/javascript">
		
		
			var tab = null;
			var target = window.location.hash;
			target = target.replace('#', '');
			
			if(target.indexOf("jqtab") > -1) {
				window.scrollTo(0, 0);
				tab = target.substring(5);
			}

			function tabToggle(tab, params) {
				$('.tabControl > div').removeClass('tabAct');
				$('.tabControl > div:eq('+tab+')').addClass('tabAct');
				$('.tabContent > div').hide();
				$('.tabContent > div').eq(tab).show();
				$('.hideWithTab').show();
				$('.hideWithTab'+tab).hide();
				if ($('.smsHeader .hLogo').is(':visible') && (typeof(params) == 'undefined' || (typeof(params) != 'undefined' && params.scroll == true))){
					$('html, body').animate({
				        scrollTop: $('.tabContent > div').eq(tab).offset().top
				    }, 1000);
				}
			}

			$('.tabControl div').each(function(i){
				$(this).on('click', function(event, params){
					tabToggle(i, params);
				});
				if($('.tabContent > div').eq(i).length == 0) {
					tabContent = $('<div class="assDetails" id="tab'+i+'"></div>').appendTo('.tabContent');
					tabContent.load($('.bc a:last').attr('href') + ' #tab'+i);
				}
			});
			$('.hNSNav .tabToggle').each(function(i){
				$(this).on('click', function(event, params){
					$($(this).closest('.hNavSub')).hide();
					tabToggle(i, params);
				});
			});
			
			$('.hASeo p.onlySMS a').on('click', function(){
				$('.hASeo p.onlySMS a').toggle();
				$('.hASeo2').toggle();
			});
			
			$('.tabContent > div').hide();
			if(tab) {
				$('.tabControl > div:eq('+tab+')').trigger('click', {scroll:true});
			}else{
				$('.tabControl > div:eq(0)').trigger('click', {scroll:false});
			}
		
		</script>
<script type="text/javascript">
	
		$(document).ready(function(){
		
			ajaxNewsletterForm = function(event, form){
				$.ajax({
					url: '?id=1&eID=formhandler-ajaxsubmit&uid='+$('input[name="uid"]', form).val(),
	                type: form.attr('method'),
	                data: form.serialize(),
	                dataType: "json",
	                success: function(html) {
	                	var div = $(form).parent().parent();
						$(form).parent().replaceWith(html.form);
						$('form', div).on('submit', function(event){
							event.preventDefault();
							ajaxNewsletterForm(event, $(this));
						});
	                }
	            });				
			}
		
			$('.formNewsletter').on('submit', function(event){
				event.preventDefault();
				ajaxNewsletterForm(event, $(this));
			});
		});
	
</script>
