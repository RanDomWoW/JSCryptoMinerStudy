        function showCityQ(show, data)
        {
            if(show==true)
            {
                $('#city_q_block1').addClass('hidden');
                $('#city_select_block').addClass('hidden');
                $('#city_q_block').removeClass('hidden');
                $('#city_q_block_a').attr('href',data.city.url);
                $('#city_q_block_a').text(data.city.name);
                $('#city_q_block_bt_yes').attr('href',data.city.url);

            }
            else
            {
                $('#city_q_block1').removeClass('hidden');
                $('#city_select_block').removeClass('hidden');
                $('#city_q_block').addClass('hidden');
            }

        }

        function getCookie(name) {
            var matches = document.cookie.match(new RegExp(
                    "(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
            ));
            return matches ? decodeURIComponent(matches[1]) : undefined;
        }


        function getCityByPos()
        {

            $.ajax({
                url:'/mobi/get-city',
                type: "GET",
                beforeSend: function (xhr) {
                    xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'));
                },
                data: ( {lat: ymaps.geolocation.latitude, lon: ymaps.geolocation.longitude, city: ymaps.geolocation.city,  region: ymaps.geolocation.region, country: ymaps.geolocation.country} ),
                timeout: 360000,
                success: function (data, textStatus, jqXHR) {
                    showCityQ(true, data);

                },
                error: function (jqXHR, textStatus, errorThrown) {
                    showCityQ(false);
                }
            });

        }

        function tryToGetCity()
        {
            var s = getCookie('city');
            var s1 = getCookie('pharm_type');

            if(s != undefined && s!= '')
            {

                $.ajax({
                    url:'/mobi/check-city',
                    type: "GET",
                    beforeSend: function (xhr) {
                        xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'));
                    },
                    data: ( {city: s, pharm_type: s1} ),
                    timeout: 360000,
                    success: function (data, textStatus, jqXHR) {
                        if(data.found == 1 && data.city != undefined && data.city.url != undefined && data.city.url != '')
                        {
                            showCityQ(true, data);
                           // document.location = data.url;
                        }
                        else
                        {
                            getCityByPos();
                        }
                    },
                    error: function (jqXHR, textStatus, errorThrown) {
                        getCityByPos();
                    }
                });
            }
            else
            {
                getCityByPos();
            }
        }

        document.addEventListener("DOMContentLoaded", tryToGetCity);

    //    window.onload = function () {

        //    tryToGetCity();
   //     };


    </script>
