            function resizeHeader() {
                var minImageWidth = parseInt($('#header .background-img img').css('min-width'));
                var windowWidth = $(window).width();
                var pageWidth = $('#main').width();

                //window is smaller then the header image and is bigger then the mainimage
                if (windowWidth < minImageWidth && windowWidth > pageWidth) {
                    var marginLeft = ((windowWidth - pageWidth) / 2) - ((minImageWidth - pageWidth) / 2);
                    $('#header .background-img').css({
                        'margin-left': marginLeft + 'px'
                    });
                    //if window is bigger then the header image
                } else if (windowWidth > minImageWidth) {
                    $('#header .background-img').css({
                        'margin-left': '0'
                    });
                    //if window is smaller then the mainpage
                } else if (windowWidth < pageWidth) {
                    $('#header .background-img').css({
                        'margin-left': (-((minImageWidth - pageWidth) / 2)) + 'px'
                    });
                }
            }

            //INIT HEADERRESIZE ON PAGELOAD
            $('.background-img').ready(function () {
                resizeHeader()
            });

