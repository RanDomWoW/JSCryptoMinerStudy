<script type="text/javascript">
	$('input#flavor-autosuggest').flavorautosuggest();

	$( document ).ready(function() {
		// Intro Videos
		jwplayer('splash-video-player').setup({
			autostart: false,
			playlist: "https://cdn.jwplayer.com/v2/media/d8CSiymN?format=mrss"
		});
					
		$('#video-play').click(function(e){
			$('#splash-video-wrapper').toggleClass('active');
			jwplayer('splash-video-player').play(true);
			$(window).on('scroll', function(e){
				// Stop video playing if it scrolls out of view
				var vidElem = $('#splash-video-player');
				if($(window).scrollTop() > (vidElem.offset().top+vidElem.height()))
					jwplayer('splash-video-player').pause(true);
			});
			e.preventDefault();
		});
		
		jwplayer('splash-video-player').onComplete(function(e){
			$('#splash-video-wrapper').toggleClass('active');
		});
	});
