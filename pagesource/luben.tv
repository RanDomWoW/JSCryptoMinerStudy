<script>
    $(window).load(function () {
        var navbarHeight = $('.scroll-menu').height();
        $('.sidebar-left').stick_in_parent({parent: '.sticky-content', spacer: '.sidebar-wrapper', inner_scrolling: false, offset_top: navbarHeight}).on("sticky_kit:stick", function (e) {


});

        $('.sidebar').stick_in_parent({parent: '.sticky-content', spacer: '.sidebar-wrapper', inner_scrolling: false, offset_top: navbarHeight}).on("sticky_kit:stick", function (e) {

            function reset() {
                $(".sidebar").trigger("sticky_kit:detach");

                $('.last_adv').stick_in_parent({parent: '.sticky-content', offset_top: navbarHeight});
            }
            setTimeout(reset, 8000);
        });

    });
