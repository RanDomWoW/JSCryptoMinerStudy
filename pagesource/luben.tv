<script>
        $('#skin-container').css('margin-top', '163px');
        $(window).on("scroll", function (e) {

            var aTop = $('.header').height();
            if ($(this).scrollTop() >= aTop) {
                var window_width = jQuery(window).width();

                if (window_width > 1200) {
                    $('.logo').hide();

                }

                $('#skin-container').css('margin-top', '50px');
                $('.navbar-nav').css('width', 'auto');
                $('.navbar-header').show();
                $('#showLeftNavBar').show();
                $('.navbar-brand').show();
                $('.navbar-nav').css('float', 'left');
                $('.social-item').css('display', '');
            } else {
                var window_width = jQuery(window).width();
                if (window_width > 1200) {
                        //$('.mobile-logo').hide();
                        $('.logo').show();
                    }
                    $('#skin-container').css('margin-top', '163px');
                    //$('.mobile-logo').show();
//                    $('.logo').show();
                    //$('.logoguide').show();
                    $('#showLeftNavBar').hide();
                    $('.navbar-brand').hide();

                    // $('.navbar-nav').css('width', '100%');
                    $('.navbar-header').hide();
                    $('.navbar-nav').css('float', 'none');
                    $('.social-item').css('display', 'none');
                }

            });
        </script>
<script>
$('.show_youtube').click(function(event) {
    event.preventDefault();

    $(this).next('.video-youtube').show();
    $(this).next('.video-youtube').find('iframe')[0].src += "&autoplay=1";
    $(this).hide();


});
</script>
