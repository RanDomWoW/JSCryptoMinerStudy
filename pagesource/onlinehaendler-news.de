	<script>
	
	// MODAL FEED CONTENT
	var $modal = '<div class="feedModal">' +
					'<div class="closeWidget">x</div>' +
					'<div class="feedWidget"><b>OnlinehändlerNews RSS-Feed.</b> <br /><br />' +
					'<div class="sidebarfeedAboMain">Jetzt abonnieren!</div><br>' +
					'<div class="sidebarmoreheadingfeed">Kategorien abonnieren</div><br>' +
					'<div class="sidebarsingleFeeds"><p class="copyIntroText">Falls ihr Browser kein direkte Abonnier-<br>funktion bietet, kopieren Sie bitte den Link mit einem Klick auf den Button und fügen ihn in Ihren RSS-Reader ein.</p><p class="successMes"><b>Link kopiert</b></p><br /> <input name="sidebarsingleFeed" type="checkbox" value="recht" /> Recht <button data-clipboard-text="https://www.onlinehaendler-news.de/recht?format=feed&type=rss" class="buttonUser1 copyBtn">Link</button> <br /> <input name="sidebarsingleFeed" type="checkbox" value="marketing" /> Marketing <button data-clipboard-text="https://www.onlinehaendler-news.de/marketing?format=feed&type=rss" class="buttonUser1 copyBtn">Link</button> <br /> <input name="sidebarsingleFeed" type="checkbox" value="handel" /> Handel <button data-clipboard-text="https://www.onlinehaendler-news.de/handel?format=feed&type=rss" class="buttonUser1 copyBtn">Link</button><br /> <input name="sidebarsingleFeed" type="checkbox" value="veranstaltungen" /> Veranstaltungen <button data-clipboard-text="https://www.onlinehaendler-news.de/veranstaltungen?format=feed&type=rss" class="buttonUser1 copyBtn">Link</button><br /> <input name="sidebarsingleFeed" type="checkbox" value="interviews" /> Interviews <button data-clipboard-text="https://www.onlinehaendler-news.de/interviews?format=feed&type=rss" class="buttonUser1 copyBtn">Link</button><br /> <input name="sidebarsingleFeed" type="checkbox" value="payment" /> Payment <button data-clipboard-text="https://www.onlinehaendler-news.de/payment?format=feed&type=rss" class="buttonUser1 copyBtn">Link</button><br /> <input name="sidebarsingleFeed" type="checkbox" value="e-commerce-tipp" /> E-Commerce Tipp <button data-clipboard-text="https://www.onlinehaendler-news.de/e-commerce-tipp?format=feed&type=rss" class="buttonUser1 copyBtn">Link</button><br /> <input name="sidebarsingleFeed" type="checkbox" value="start-ups" /> Start Ups <button data-clipboard-text="https://www.onlinehaendler-news.de/start-ups?format=feed&type=rss" class="buttonUser1 copyBtn">Link</button><br /> <input name="sidebarsingleFeed" type="checkbox" value="jobs" /> Jobs <button data-clipboard-text="https://www.onlinehaendler-news.de/jobs?format=feed&type=rss" class="buttonUser1 copyBtn">Link</button><br /><br />' +
					'<div class="sidebarfeedAboSingle">ausgewählte Feeds abonnieren</div></div></div></div>',
					
	$document = $(document);
	
	
	// TRIGGERS
	$(document).ready(function () {	

		// CLICK FEED IN FOOTER
		$("#feedOpener").click(function(){
			    $('html, body').animate({
					scrollTop: $(".topSocials").offset().top
				}, 1000,function(){
					$(".topSocials").find(".modalOpener").click();
				});
		});	
		
		// CLOSE FEED PANEL
		$('body').on("click", ".closeWidget", function(e){			
			$( this ).parent().fadeOut(); 
		});
		
		// OPEN FEED PANEL
		$(".modalOpener").click(function(){			
			
			var insert = $( this ).parent().attr("class");	
			
			if( insert == 'topSocials')
				$(this).parent().find(".feedOption").html( $modal );	
			
			if( insert == 'leftSocialMedia')		
				$(".leftSocialMedia").append( $modal );	
			
			if( insert == 'socialMediaIconWrapper')		
				$(".socialMediaIconWrapper").prepend( $modal );				
		});		
		
		// OPEN CHOSED FEEDS OR DISPLAY ERROR
		$('body').on("click", ".sidebarfeedAboSingle", function(e){		
			for (var t = document.getElementsByName("sidebarsingleFeed"), e = t.length, n = [], i = 0; e > i; i++)t[i].checked && n.push(t[i].value);
			if ("" == n)alert("Bitte mindestens 1 Feedkategorie wählen!"); else for (var i = 0; i < n.length; i++)window.open("https://www.onlinehaendler-news.de/" + n[i] + "?format=feed&type=rss", "_blank")
		});
	
		// OPEN MAIN FEED
		$('body').on("click", ".sidebarfeedAboMain", function(e){		
			window.open("http://www.onlinehaendler-news.de/?format=feed&type=rss", "_blank")
		});		
		
		// SHOW FEEDS AND TOGGLE BUTTON TEXT
		$('body').on("click", ".sidebarmoreheadingfeed", function(e){
			$(".sidebarsingleFeeds").toggle("slow", function () {
			});
			var t = $(".sidebarmoreheadingfeed").text();
			$(".sidebarmoreheadingfeed").text("Kategorien abonnieren" == t ? "Kategorien einklappen" : "Kategorien abonnieren")
		});


		//SETUP COPY TO CLIPBOARD
		var copyRecht = 'https://www.onlinehaendler-news.de/recht?format=feed&type=rss';
		var copyMarketing = 'https://www.onlinehaendler-news.de/marketing?format=feed&type=rss';
		var copyHandel = 'https://www.onlinehaendler-news.de/handel?format=feed&type=rss';
		var copyVeranstaltung = 'https://www.onlinehaendler-news.de/veranstaltungen?format=feed&type=rss';
		var copyInterviews = 'https://www.onlinehaendler-news.de/interviews?format=feed&type=rss';
		var copyPayment = 'https://www.onlinehaendler-news.de/payment?format=feed&type=rss';
		var copyEcommerce = 'https://www.onlinehaendler-news.de/e-commerce-tipp?format=feed&type=rss';
		var copyStartup = 'https://www.onlinehaendler-news.de/start-ups?format=feed&type=rss';
		var copyJobs = 'https://www.onlinehaendler-news.de/jobs?format=feed&type=rss';

		var clipboard = new Clipboard('.copyBtn');

		clipboard.on('success', function(e) {
			$('.copyIntroText').toggle('4',function(){
				$('.successMes').toggle('4',function(){
					$('.successMes, .copyIntroText').delay(2000).fadeToggle();
				});
			});

			console.info('Action:', e.action);
			console.info('Text:', e.text);
			console.info('Trigger:', e.trigger);

			e.clearSelection();
		});

		clipboard.on('error', function(e) {
			console.error('Action:', e.action);
			console.error('Trigger:', e.trigger);
		});
	});

		
	</script>

