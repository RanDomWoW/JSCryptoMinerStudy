// verify if the search item expired
function isExpired(itemData){
  var timeItem = getRSDateObject(itemData.depDate).getTime();
  var isExpired = false;
  var validDate = new Date();
  switch(itemData.category){
      case "domestic":// greater than today
          break;
      case "transtasman":// greater than tomorrow
          validDate.setDate(validDate.getDate() + 1);
          break;
      case "international":// greater than the day after tomorrow
          validDate.setDate(validDate.getDate() + 2);
          break;
  }
  validDate.setHours(0,0,0,0);
  var timeValidDate = validDate.getTime();
  if (timeValidDate > timeItem) isExpired = true;
  return isExpired;
}
// Hotels type product handler

function hotelDisplayDate(hoteldate) {
    var hyear = hoteldate.slice(0, 4),
        hmonth = hoteldate.slice(4, 6),
        hday = hoteldate.slice(6, 8);
    var newDate = hyear + '-' + hmonth + '-' + hday;
    return formatDateNoYear(newDate);
}

function hotelToJSDate(hoteldate) {
    var hyear = hoteldate.slice(0, 4),
        hmonth = hoteldate.slice(4, 6),
        hday = hoteldate.slice(6, 8);
    return new Date(hyear, hmonth - 1, hday);
}

function addTypeHotels(itemData, itemIndex) {
    var recentHotelBlock = '\
  <div class="vignette">\
      <a href="#center-modal-clearone" role="button" class="" data-toggle="modal" style="padding:0px;"><div class="close" onclick="setSelectedSearch({{ITEM_SELECT}})"><span class="wj-icon wj-cross"></span></div></a>\
      <a href="{{SEARCH_LINK}}" target="_blank" class="silent-link" style="padding:0px;margin:0px;">\
      <div class="title">\
          <span class="wj-icon wj-hotel" style="font-size: 20px"></span> {{DESTINATION_NAME}} Hotels\
      </div>\
      <div class="info">\
          <div class="text">{{NUMBER_ROOMS}} - {{NUMBER_TRAVELLERS}} <br/> {{TRAVEL_DATES}} <span style="font-style:italic">{{NUMBER_NIGHTS}}</span></div>\
          <div class="sep">&nbsp;<br/>&nbsp;</div>\
          <div class="link"><span class="wj-icon wj-dbl-arrow-right"></span></div>\
      </div>\
  </a></div>';

    // Do not show items greater than today
    var dtToday = new Date();
    if (dtToday.getTime() > hotelToJSDate(itemData.checkInDate).getTime()) return '';

    // get required info here
    var linkDomain = 'https://services.' + _udn + '/';
    var linkURLTemplate = linkDomain + "web/hotels/search/#/results?searchId=1&checkInDate={{CHECKIN_DATE}}&checkOutDate={{CHECKOUT_DATE}}&paxRequest={{PAX_REQUEST}}&areaId={{AREA_ID}}&area={{DESTINATION_NAME}}&childAge={{CHILDREN_AGE}}";

    var flightTypeReturn = (itemData.type == "return");
    var flight_Class = itemData.cabin;

    var numRooms = itemData.rooms.length;
    var numAdults = 0;
    var numChildren = 0;
    var paxRequest = '';

    var num_rooms = numRooms + (numRooms > 1 ? ' rooms' : ' room');

    var childrenAges = '';
    for (var i = 0, room = null; i < numRooms; i++) {
        room = itemData.rooms[i];
        numAdults += room.adults;
        numChildren += room.children;

        if (i > 0) {
            paxRequest += '%7C';
            childrenAges += '%7C';
        };

        paxRequest += 'A' + room.adults + 'C' + room.children;
        childrenAges += (',' + room.childAge).repeat(room.children).slice(1);
    };

    var traveller_Info = numAdults + (numAdults > 0 ? ' adults' : ' adult');
    if (numChildren > 0) {
        traveller_Info += ((traveller_Info.length > 0 ? ', ' : '') + numChildren + (numChildren > 0 ? ' children' : ' child'));
    };

    var travel_dates = hotelDisplayDate(itemData.checkInDate) + ' - ' + hotelDisplayDate(itemData.checkOutDate);

    // calculate number of nights
    var oneDay = 24 * 60 * 60 * 1000; // hours*minutes*seconds*milliseconds
    var firstDate = hotelToJSDate(itemData.checkInDate);
    var secondDate = hotelToJSDate(itemData.checkOutDate);
    var num_days = Math.round(Math.abs((firstDate.getTime() - secondDate.getTime()) / (oneDay)));;
    var num_nights = num_days + (num_days > 1 ? ' nights' : ' night');

    // insert required info into template
    var tileInsert = recentHotelBlock.replace('{{DESTINATION_NAME}}', itemData.destination);
    tileInsert = tileInsert.replace('{{NUMBER_ROOMS}}', num_rooms);
    tileInsert = tileInsert.replace('{{NUMBER_TRAVELLERS}}', traveller_Info);
    tileInsert = tileInsert.replace('{{NUMBER_NIGHTS}}', num_nights);
    tileInsert = tileInsert.replace('{{TRAVEL_DATES}}', travel_dates);
    tileInsert = tileInsert.replace('{{ITEM_SELECT}}', itemIndex);

    var linkURL = linkURLTemplate;
    linkURL = linkURL.replace('{{AREA_ID}}', itemData.areaId);
    linkURL = linkURL.replace('{{PAX_REQUEST}}', paxRequest);
    linkURL = linkURL.replace('{{CHILDREN_AGE}}', childrenAges);
    linkURL = linkURL.replace('{{CHECKIN_DATE}}', itemData.checkInDate);
    linkURL = linkURL.replace('{{CHECKOUT_DATE}}', itemData.checkOutDate);
    linkURL = linkURL.replace('{{DESTINATION_NAME}}', itemData.destination);

    tileInsert = tileInsert.replace('{{SEARCH_LINK}}', linkURL);

    return tileInsert;
}
function clearRecentSearches()
{
  document.cookie = WJ_RECENT_COOKIEKEY + '=;expires=Thu, 01 Jan 1970 00:00:01 GMT; domain='+WJ_RECENT_COOKIEDOMAIN+'; path=/';
  showRecentSearch([]);
  $('#slider_recent_searches').css('display', 'none');
}

function setSelectedSearch(itemID)
{
wj_recent_search_selected = itemID;
}

function clearOneSearch()
{
var rawCookie = getCookie(WJ_RECENT_COOKIEKEY);
var rawJSON = decodeURIComponent(rawCookie);
var cookieData = JSON.parse(rawJSON);
var newCookieData = new Array();

// reconstruct cookie without selected item
for (var i = 0, item = null; i < cookieData.length; i++) {
  item = cookieData[i];
  if (i != wj_recent_search_selected) newCookieData.push(item);
};
var newCookieString = encodeURIComponent(JSON.stringify(newCookieData));

// set cookie
var d = new Date();
d.setTime(d.getTime() + (30 * 24 * 60 * 60 * 1000)); // expires in 30 days
var expires = "expires=" + d.toUTCString();

document.cookie = WJ_RECENT_COOKIEKEY + "=" + newCookieString + "; " + expires + '; domain='+ WJ_RECENT_COOKIEDOMAIN +'; path=/';

$('.slider_recent .slider-content').slick('unslick');
showRecentSearches();
}

function showRecentSearch(dataList) {
var resultContainer = $('.slider_recent .slider-content');
resultContainer.empty();

var isMobilePlatform = isMobile();
var blockInsert = "";
var validItems = 0;
for (var i = 0, item = null; i < dataList.length; i++) {
  item = dataList[i];
  switch (item.product) {
    case "flights":
      blockInsert += isMobilePlatform ? addTypeMobileFlights(item, i) : addTypeFlights(item, i);
      if (blockInsert) validItems++;
      break;
    case "hotels":
      blockInsert += addTypeHotels(item, i);
      validItems++;
      break;
/*
    case "cars":
      blockInsert += addTypeCars(item, i);
      break;
*/        
  };
};

if (blockInsert != "") resultContainer.append(blockInsert);

return validItems;
}

function initializeSlider() {
  // initialize slider once content has been generated
  $('.slider_recent .slider-content').slick({
    dots: false,
    infinite: false,
    speed: 300,
    arrows: false,
    centerMode: false,
    variableWidth: true
  });
}

function formatTripDate(yyyymmdd) {
var monthNames = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];
var dayNames = ['Sun','Mon','Tue','Wed','Thu','Fri','Sat'];
var dateParts = yyyymmdd.split('-');
var nativeDate = new Date(parseInt(dateParts[0]), parseInt(dateParts[1])-1, parseInt(dateParts[2]));

return dayNames[nativeDate.getDay()] + " " + nativeDate.getDate() + " " + monthNames[nativeDate.getMonth()];
}

function formatURLDate(dateIn) {
var tmp = dateIn.split('-');
return  parseInt(tmp[2]) + '/' + parseInt(tmp[1]) + '/' + parseInt(tmp[0]);
}

function processCookie(rawCookie)
{
var rawJSON = decodeURIComponent(rawCookie);
var cookieData = JSON.parse(rawJSON);

return cookieData;
}

function showRecentSearches()
{
var searchesCookie = getCookie(WJ_RECENT_COOKIEKEY);
if (searchesCookie.length > 9)  {
  wj_recent_search_data = processCookie(searchesCookie);
  var validItems = showRecentSearch(wj_recent_search_data);

  if (validItems > 0) {
    $('#slider_recent_searches').css('display', 'inherit');
    initializeSlider();
  } else {
    $('#slider_recent_searches').css('display', 'none');
  }
} else {
  $('#slider_recent_searches').css('display', 'none');
}
}

// hide recent search until we have some results
$('#slider_recent_searches').css('display', 'none');

$(document).ready(function() {

  var searchesCookie = getCookie(WJ_RECENT_COOKIEKEY);
  if (searchesCookie.length > 9)  {
    showRecentSearches();
  };
});

