function getToday(){
  var d = new Date();
  var hr = d.getHours();
  var min = d.getMinutes();
  if (min < 10) {
      min = "0" + min;
  }
  var ampm = "AM";
  if( hr > 12 ) {
      hr -= 12;
      ampm = "PM";
  }
  var date = d.getDate();
  var month = d.getMonth();
  var year = d.getFullYear();
  var x = date + "/" + month + "/" + year + " " + hr + ":" + min + ":00 " + ampm;
  return x;
  // return '3/11/2017 11:56:50 AM';
}

