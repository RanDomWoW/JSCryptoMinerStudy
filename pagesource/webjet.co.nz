function getRSDateObject(yy_mm_dd) {
var dateFields = yy_mm_dd.split('-');
return new Date(parseInt(dateFields[0]), parseInt(dateFields[1])-1, parseInt(dateFields[2]), 0, 0, 0, 0);
}

function formatDateNoYear(yy_mm_dd) {
var monthNames = ['Jan', 'Feb','Mar','Apr','May','Jun','Jul','Aug','Sep','Oct','Nov','Dec'];
var dayNames = ['Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat'];
var dateFields = yy_mm_dd.split('-');
var dateObj = getRSDateObject(yy_mm_dd);
return dayNames[dateObj.getDay()] + ' ' + parseInt(dateFields[2]) + ' ' + monthNames[dateFields[1]-1];
}

