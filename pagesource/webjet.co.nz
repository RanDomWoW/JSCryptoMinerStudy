      }));
    };
  }

  // updates the category tiles with new data on the screen
  function showCategoryTiles(tileList) {
    var elementID;

    var departureName = qfdf.departureSelect1.children(':selected').text();

    for (var i = 0; i < tileList.length; i++) {
      elementID = '#hpdf-category-tile' + i;
      $(elementID + ' a').attr("href", '#' );
      $(elementID + ' a').attr("click-target", tileList[i].code);
      var imageElement = $(elementID + ' img');
      if (imageElement.hasClass('lazy')) {
        imageElement.attr("src", '');
        imageElement.attr("data-src", tileList[i].img);
        imageElement.lazyLoadXT();
      } else {
        imageElement.attr("src", tileList[i].img);
      };
      $(elementID + ' h4').text(tileList[i].title);
      $(elementID + ' p').html(tileList[i].flight_type);

      if (tileList[i].price !== '') {
        $(elementID + ' span').html('$' + numberWithCommas(tileList[i].price) + '<sup>*</sup>');
      } else {
        $(elementID + ' span').text(departureName);
      };
    };
  }

  function showBusy(enable) {
    $('.flightquickfinder .loader-tiles').css("display", enable ? 'initial' : 'none');
  }

  function arrayFind(searchArray, key, value) {
    for (var i = 0; i < searchArray.length; i++) {
      if (searchArray[i][key] == value) {
        return searchArray[i];
      };
    };
    return null;
  }

  function updateCategory(cityCode) {
    // get valid departure airports for this departure code value
    var validAirports = [];
    var validAirportsArray = DEPARTURE_LIST.filter(function(item) {
      return item.code === cityCode;
    });
    if (validAirportsArray && validAirportsArray[0]) validAirports = validAirportsArray[0].airports;
    var airportCodes = validAirports;

    var cheapestCat, categoryData, cheapestInCity;
    for (var j = 0; j < qfdf.CATEGORY_LIST.length-1; j++) {
      // Set cheapest deal for this category
      cheapestCat = {
        min_price: "",
        flight_type: ""
      };
      cheapestInCity = 999999;
      categoryData = cheapestDeals[ qfdf.CATEGORY_LIST[j+1].code ];
      if (categoryData) {
        for (var k = 0; k < airportCodes.length; k++) { // check each airport in city
          var dealFound = arrayFind(categoryData, 'departure_airport_code', airportCodes[k]);
          if (dealFound && (dealFound.min_price < cheapestInCity)) {
            cheapestCat = dealFound;
            cheapestInCity = dealFound.min_price;
          };
        };
      };
      qfdf.CATEGORY_LIST[j + 1].price = cheapestCat.min_price;
      qfdf.CATEGORY_LIST[j + 1].flight_type = cheapestCat.flight_type == "ONEWAY" ? 'ONE WAY<br />from' : 'RETURN<br />from';
    };

    showCategoryTiles(qfdf.CATEGORY_LIST);
  }

  qfdf.departureSelect1.change(function(evt) {
    if (qfdf.departureSelect1.is(":visible")) {
      qfdf.dealfinder_departure_code = qfdf.departureSelect1.val();
      updateCategory(qfdf.dealfinder_departure_code);
    };
  });

  // action when a tile is clicked
  $('.flightquickfinder .clickable-div').on('click', function(e) {
    var clickTarget = $(this).attr('click-target');
    if (clickTarget) {
      $('.search-start').hide();

      monthSelect[0].selectedIndex = 0;
      monthSelect.selectmenu("refresh", true);
      qfdf.jqselectValue(departureSelect2, qfdf.dealfinder_departure_code);
      qfdf.jqselectValue(destinationSelect, clickTarget);

      $('.search-result').fadeIn();

      qfdf.searchCategory(clickTarget);
    }
  });

  $(document).ready(function() {
    if (SITE_REGION === "NZ") { // change domestic label for NZ
      var cat_au = qfdf.CATEGORY_LIST[1];
      var cat_nz = qfdf.CATEGORY_LIST[2];
      cat_nz.title = "Domestic";
      cat_au.title = "Australia";
    };

    showBusy(true);
    showDepartureCities1(qfdf.DEPARTURE_LIST);
    qfdf.departureSelect1.val(DEFAULT_DEPARTURE);
    qfdf.dealfinder_departure_code = qfdf.departureSelect1.val();

    setTimeout(function() {
      $.ajax({
        type: 'GET',
        url: '/api/get/wj_hpdf/categories/',
        success: function(result, status, xhr) {
          cheapestDeals = result;

          updateCategory(qfdf.departureSelect1.val());
          showBusy(false);
        },
        error: function(xhr, status, error) {
          showBusy(false);
          console.log(error);
        }
      });
    }, 1000); // set > 0 for test
  });

// End code for category view



// ####################### Code for result view goes here

  // controls
  var departureSelect2 = $('#wj-hpdf-departure-city2');
  var destinationSelect = $('#wj-hpdf-destination-type');
  var monthSelect = $('#wj-hpdf-month');
  var priceSlider = $(".flightquickfinder .search-result .slider");
  var apiResults = null;

  var blockTemplate = '\
    <div class="col col col-3 col-m-4 col-s-12">\
         <div class="row side-gutter"> \
            <a class="clickable-div" href="{{DF_LINK}}">\
            <div class="block-tiles">\
             <div class="deal-image-sm-block">\
                <img class="lazy" data-src="{{DEST_IMAGE}}" width="719px" height="394px" alt="" onerror="this.src=\'https://media.webjet.com.au/Images/cities/719x394/default1.jpg\';" /> \
             </div>\
             <div class="info_tile">\
                    <h4>{{DEST_NAME}}</h4>\
                    <div class="info_price">\
                        <p>{{FLIGHT_TYPE}}</p>\
                        <span>{{PRICE}}</span>\
                    </div>\
             </div>\
            </div>\
            </a>\
        </div>\
    </div>';

  function showBusy(enable) {
    $('.flightquickfinder .loader-tiles').css("display", enable ? 'initial' : 'none');
  }

  function showDepartureCities2(cityList) {
    for (var i = 0; i < cityList.length; i++) {
      departureSelect2.append($('<option>', {
        value: cityList[i].code,
        text: cityList[i].name
      }));
    };
  }

  function showDestinationList(destinationList) {
    for (var i = 0; i < destinationList.length; i++) {
      destinationSelect.append($('<option>', {
        value: destinationList[i].code,
        text: qfdf.getShortTitle(destinationList[i].title)
      }));
    };
  }

  
  function showMonthOptions() {
    var monthValue, monthText;
    for (var i = 0; i < 5; i++) {
      var newDate = new Date();
      newDate.setDate(1);
      newDate.setMonth(newDate.getMonth() + i);
      monthValue = newDate.getFullYear() * 100 + (newDate.getMonth() + 1);
      monthText = newDate.toLocaleDateString('en-GB', {
        month: 'short'
      }) + ' ' + newDate.getFullYear();

      monthSelect.append($('<option>', {
        value: monthValue,
        text: monthText
      }));
    };
  }

  function showSearchResults(resultList) {
    var resultContainer = $('#search-results-div');
    resultContainer.empty();

    if (resultList && resultList.length === 0) {
      return resultContainer.append('<div style="text-align: center;color: red;">Your criteria returned no results<p></div>');
    }

    YearMonthValue = monthSelect.val();
    monthValue = (YearMonthValue%100);
    yearValue = Math.floor(YearMonthValue/100);
    searchMonth = ((monthValue<=9)?'0':'') + monthValue + '' + yearValue;

    var blockInsert = "";
    for (var i = 0, item=null; i < resultList.length; i++) {
      item = resultList[i];
      fullLink = item.target_link + '?mdf_month=' + searchMonth + '&mdf_price=' + item.min_price;
      blockInsert = blockTemplate.replace('{{DF_LINK}}', fullLink);
      blockInsert = blockInsert.replace('{{DEST_IMAGE}}', item.target_image);
      blockInsert = blockInsert.replace('{{DEST_NAME}}', item.destination_airport_name);
      blockInsert = blockInsert.replace('{{FLIGHT_TYPE}}', item.flight_type == "ONEWAY" ? 'ONE WAY<br />from' : 'RETURN<br />from');
      blockInsert = blockInsert.replace('{{PRICE}}', '$' + numberWithCommas(item.min_price) + '<sup>*</sup>');

      resultContainer.append(blockInsert);
    };

    // initialize lazy loaded images
    var lazyImages = resultContainer.find('.lazy');
    lazyImages.each(function() {
      $(this).lazyLoadXT();
    });
  }

  function sanitizeDFNames(destination_name) {
      var tmp = destination_name.replace('  ', ' ');  // double spaces to single space
      tmp = tmp.replace(/ /g, '-'); // spaces to dashes
      tmp = tmp.replace(/_/g, '-'); // underscore to dashes
      return tmp.toLowerCase();
  }

  // generates links for each list item
  function processList(rawList) {
    var item, targetURL;
    for (var i = 0; i < rawList.length; i++) {
      item = rawList[i];
      targetURL = '/flights/';
      targetURL += sanitizeDFNames(item.destination_airport_name) + '/' +
                   sanitizeDFNames(qfdf.dealfinder_departure_code);
      item.target_link = targetURL;
      item.target_image = 'https://media.webjet.com.au/Images/cities/719x394/' + item.destination_airport_code.toLowerCase() + '1.jpg';
    };
  }

  // changes the Min Max and step values for the slider based on the current raw data
  function updateFilterFromList(rawList) {

    var minOption = 0,
      maxOption = 1200;
    if (rawList && rawList.length > 0) {
      minOption = parseInt(rawList[0].min_price);
      maxOption = parseInt(rawList[rawList.length - 1].min_price);
    }
    priceSlider.slider('option', 'min', minOption);
    priceSlider.slider('option', 'max', maxOption);
    priceSlider.slider('option', 'step', 1);
    priceSlider.slider('option', 'values', [minOption, maxOption]);

    qfdf.setSliderDisplayValues(priceSlider, [minOption, maxOption]);
  }

  function filterResults(rawList) {
    var ps = priceSlider.slider('option', 'values');
    var minPrice = ps[0],
      maxPrice = ps[1];

    var filteredList = [],
      itemPrice;
    for (var i = 0; i < rawList.length; i++) {
      itemPrice = parseInt(rawList[i].min_price);
      if (minPrice <= itemPrice && itemPrice <= maxPrice) {
        filteredList.push(rawList[i]);
      };
    }

    return filteredList;
  }

  qfdf.getSearchData = function(cityCode, destination, yearmonth) {
    showBusy(true);
    var urlCall = '/api/get/wj_hpdf/deals/' + cityCode + '/' + destination + '/' + yearmonth + '/';

    setTimeout(function() {
      $.ajax({
        type: 'GET',
        url: urlCall,
        success: function(result, status, xhr) {
          apiResults = result;
          processList(apiResults);
          updateFilterFromList(apiResults);
          var filteredResult = filterResults(apiResults);
          showSearchResults(filteredResult);
          showBusy(false);
        },
        error: function(xhr, status, error) {
          showBusy(false);
          console.log(error);
        }
      });

    }, 0);
  }

  departureSelect2.change(function(evt) {
    if (departureSelect2.is(":visible")) {
      qfdf.dealfinder_departure_code = departureSelect2.val();
      qfdf.jqselectValue(departureSelect2, qfdf.dealfinder_departure_code);
      qfdf.getSearchData(qfdf.dealfinder_departure_code, destinationSelect.val(), monthSelect.val());
    };
  });

  destinationSelect.change(function(evt) {
    if (destinationSelect.is(":visible")) {
      qfdf.jqselectValue(destinationSelect, destinationSelect.val());
      qfdf.getSearchData(qfdf.dealfinder_departure_code, destinationSelect.val(), monthSelect.val());
    };
  });

  monthSelect.change(function(evt) {
    if (monthSelect.is(":visible")) {
      qfdf.getSearchData(qfdf.dealfinder_departure_code, destinationSelect.val(), monthSelect.val());
    };
  });

  function categoryFilter(cityCode, destination, month, price_min, price_max) {}

  // called when view is shown from category view
  qfdf.searchCategory = function(category) {
    qfdf.getSearchData(qfdf.dealfinder_departure_code, destinationSelect.val(), monthSelect.val());
  }


  qfdf.searchDeal = function(category) {

  }

  qfdf.updateDeal = function(priceMin, priceMax) {
    var filteredResult = filterResults(apiResults);
    showSearchResults(filteredResult);
  };

  $(document).ready(function() {
    // populate options only once
    showDepartureCities2(qfdf.DEPARTURE_LIST);
    showDestinationList(qfdf.CATEGORY_LIST);
    showMonthOptions();
  });



// Code moved from template file

  var setSlider = function(event, selectedValues) {
    var slider = $(this);
    qfdf.setSliderDisplayValues(slider, selectedValues.values);

    // highlight slider
    var minValueFiltered = selectedValues.values[0] != slider.data("range")[0];
    var maxValueFiltered = selectedValues.values[1] != slider.data("range")[1];
    slider.parents(".range-slider").toggleClass("min-filtered", minValueFiltered);
    slider.parents(".range-slider").toggleClass("max-filtered", maxValueFiltered);

    slider.parents(".range-slider-group").toggleClass(slider.data("type"), minValueFiltered || maxValueFiltered);
  };

  qfdf.setSliderDisplayValues = function(slider, selectedValues) {
    var displayedValues = slider.next(".text");

    var minValue = selectedValues[0];
    displayedValues.find(".from").text('$' + minValue);

    var maxValue = selectedValues[1];
    displayedValues.find(".to").text('$' + maxValue);
  };

  // initilize slider controls for this module
  $(".flightquickfinder .search-result .slider").each(function() {
    var slider = $(this);
    slider.slider({
      range: true,
      min: slider.data("range")[0],
      max: slider.data("range")[1],
      step: 15,
      values: slider.data("values"),
      slide: setSlider
    });

    slider.on("slidestop", function(event, ui) {
      priceMin = ui.values[0];
      priceMax = ui.values[1];
      qfdf.updateDeal(priceMin, priceMax);
    });

    // set slider intial values
    qfdf.setSliderDisplayValues(slider, slider.data("values"));
  });

  $(".range-slider-group").on("click", ".btn-group", function() {
    var btnGroup = $(this);

    setTimeout(function() {
      var filterValue = btnGroup.find("input[type=radio]:checked").val();
      if (btnGroup.data("selection") != filterValue) {
        btnGroup.nextAll(".range-slider").toggleClass("hide");
        btnGroup.data("selection", filterValue);
      }
    }, 0);
  });

  $('body').on('click', '.form_prev-home', function(e) {
    e.preventDefault();
    updateCategory(qfdf.dealfinder_departure_code);
    $('.search-result').hide();
    $('.search-start').fadeIn();
    qfdf.jqselectValue(qfdf.departureSelect1, qfdf.dealfinder_departure_code);
  });

})();
