<script>
function CheckRegion(ipRegion) {
	if (ipRegion && typeof(ipRegion.countryCode) === 'string') {
		switch (ipRegion.countryCode) {
			case 'US':
				ShowRegionAlert('the US', 'webjet.com', '/wj-assets/images/flag_usa.gif',  'https://www.webjet.com');
				break;
      case 'AU':
				if (window.site_region === 'NZ') {
					ShowRegionAlert('Australia', 'webjet.com.au', '/wj-assets/images/flag_au.png',  'https://www.webjet.com.au');
				}
				break;
      case 'NZ':
				if (window.site_region === 'AU') {
					ShowRegionAlert('New Zealand', 'webjet.co.nz', '/wj-assets/images/flag_nz.png',  'https://www.webjet.co.nz');
				}
				break;
		};
	};
}

function clearRegionAlert() {
	setCookie('regionMsgRead', 'closed');
	$(".regionalert").hide();
}

function ShowRegionAlert(countryName, siteRegionName, siteFlag, regionLink) {
	var alertBlock = '<div class="regionalert" style="display:none;">\
	<div class="row">\
		<div class="col col-12 col-m-12 col-s-12 gutter">\
			<img src="{{SITE_FLAG}}" />\
			<div class="region-message">You appear to be from {{COUNTRY_NAME}}. <br />Switch to <a href="{{REGION_LINK}}" target="_blank"><b>{{REGION_NAME}}</b></a></div>\
			<div class="close-alert"><span class="wj-icon wj-s-cross" onclick="clearRegionAlert();"></span></div>\
		</div>\
	</div>\
</div>';

	var alertShown = alertBlock.replace('{{SITE_FLAG}}', siteFlag);
	alertShown = alertShown.replace('{{COUNTRY_NAME}}', countryName);
	alertShown = alertShown.replace('{{REGION_LINK}}', regionLink);
	alertShown = alertShown.replace('{{REGION_NAME}}', siteRegionName);

	$(alertShown).insertAfter("div#navigation");
	$(".regionalert").show();
}

$(document).ready(function() {
	if (getCookie('regionMsgRead') !== 'closed')	{
		getGeoAPI = '/api/get/ip/clientCountry/';
		$.get(getGeoAPI, null, function(data) {
			regionDetectedData = data;
			CheckRegion(data);
		});
	};
});
