<script>
function setInputForSearchPopup(evt, departure, destination, departurecode, destinationcode){
    var tripType = '';
    var travelClass = '';
    if(evt !== ''){
      var outerDiv    = $(evt.target).closest('.searchFormParam');
      var departure   = $(outerDiv).attr('data-departure');
      var destination = $(outerDiv).attr('data-destination');
      var departurecode   = $(outerDiv).attr('data-departurecode');
      var destinationcode = $(outerDiv).attr('data-destinationcode');
      tripType    = $(outerDiv)[0].hasAttribute("data-triptype") ? $(outerDiv).attr('data-triptype') : 'Return';
    //  var travelClass = 'Business';
    //  var tripType = 'One Way';
    travelClass = $(outerDiv)[0].hasAttribute("is_business") ? ($(outerDiv).attr('is_business') == 1 ? 'Business' : 'Economy') : 'Economy';
    travelClass = $(outerDiv)[0].hasAttribute("data-tripclass") ? $(outerDiv).attr("data-tripclass") : travelClass;
  }

    switch (travelClass) {
      case "ECONOMY": travelClass = "Economy"; break;
      case "PREMIUM": travelClass = "Premium Economy"; break;
      case "BUSINESS": travelClass = "Business"; break;
      case "FIRST": travelClass = "First"; break;
    };

    flightSearchParameters = {
      fromCity: departure,
      from    : departurecode,
      toCity  : destination,
      to      : destinationcode,
      tripType: tripType.length ? tripType : 'Return',
      travelClass: travelClass.length ? travelClass : 'Economy'
     }
     //console.log(JSON.stringify(flightSearchParameters));
   $('#wj-flight-search-widget').setWJSearchParameters(flightSearchParameters);
}

function setSearchPopupOpen(size){
    if(size == 's'){
        $('#search-widget-panel').css('padding','15px 24px 12px 24px');
    }
    else if(size == 'w'){
        $('#wj-search-CONTAINER-00 #wj-flight-search-widget').removeClass('wj-widget-wide-layout');
        $('#wj-search-CONTAINER-00 #wj-flight-search-widget').addClass('wj-widget-main-layout');
        $('.wj-search .showin-modal-content .side-gutter').prepend('<div class="title-widget-search"><h2><span class="wj-icon wj-s-flight"></span>Find Cheap Flights</h2></div>');
        //$('.wj-search .showin-modal-content .side-gutter').prepend('<div class="title-widget-search"><h2><span class="wj-icon wj-s-flight"></span>Find Cheap Flights</h2></div>');
    }

    $('.wj-search .showin-modal-dialog').addClass('modal-dialog');
    $('.wj-search .showin-modal-content').addClass('modal-content');
    $('.wj-search .title-widget-search').after('<a type="button" class="close" data-dismiss="modal" aria-hidden="false" href="#">&times;</a>');

    $('body').css('padding-right','15px');
    //$('.modal.in .showin-modal-dialog').draggable();   //make the element draggable
}

function setSearchPopupClose(size){
    if (size == 'w') {
      $('#wj-search-CONTAINER-00 #wj-flight-search-widget').removeClass('wj-widget-main-layout');
      $('#wj-search-CONTAINER-00 #wj-flight-search-widget').addClass('wj-widget-wide-layout');
