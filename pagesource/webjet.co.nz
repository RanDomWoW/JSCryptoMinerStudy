qfdf.getShortTitle = function(longTitle) {
  var words = longTitle.split(" ");
  if (words.length === 1) return words[0];
  if (words.length > 1) return words[0] + ' ' + words[1];
  return longTitle;
};

(function() { // ####################### Code for category view goes here

  // data read from AJAX call
  var cheapestDeals = {};

  function showDepartureCities1(cityList) {
    for (var i = 0; i < cityList.length; i++) {
      qfdf.departureSelect1.append($('<option>', {
        value: cityList[i].code,
