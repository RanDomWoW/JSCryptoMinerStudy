    <script>
        $(function(){
            function CodeRandomNum(min,max){
              return Math.floor( Math.random()*(max-min)+min);
            }
            function CodeRandomColor(min,max){
              var r = CodeRandomNum(min,max);
              var g = CodeRandomNum(min,max);
              var b = CodeRandomNum(min,max);
              return "rgb("+r+","+g+","+b+")";
            }
            function drawPic(doc,d){
              var canvas=doc;
              var width=canvas.width;
              var height=canvas.height;
              var ctx = canvas.getContext('2d');
              var str = d.split("");
              ctx.textBaseline = 'bottom';
              ctx.fillStyle = '#FFF';
              ctx.fillRect(0,0,width,height);
              for(var i=0; i<4; i++){
                var txt = str[i];
                ctx.fillStyle = '#23a96f';
                ctx.font = CodeRandomNum(30,35)+'px SimHei'; //随机生成字体大小
                var x = 20+i*20;
                var y = CodeRandomNum(30,40);
                var deg = CodeRandomNum(-25, 25);
                ctx.translate(x,y);
                ctx.rotate(deg*Math.PI/180);
                ctx.fillText(txt, 0,0);
                ctx.rotate(-deg*Math.PI/180);
                ctx.translate(-x,-y);
              }
              for(var i=0; i<8; i++){
                ctx.strokeStyle = CodeRandomColor(40,180);
                ctx.beginPath();
                ctx.moveTo( CodeRandomNum(0,width), CodeRandomNum(0,height) );
                ctx.lineTo( CodeRandomNum(0,width), CodeRandomNum(0,height) );
                ctx.stroke();
              }
            }
