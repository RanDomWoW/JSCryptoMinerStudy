//                    dataType:'text',
//                    success:function(data){
//                        //drawPic(document.getElementById("checkCode"),''+data);
//                        $('#checkCode').empty().append('<img src="'+data+'">');
//                        codeCtr = true;
//                    }
//                });
//            }
//            
