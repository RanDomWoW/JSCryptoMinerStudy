//            $(document).on('click','#checkCode',function(){
//                getCode();
//            })
            
            $('#WxLogin').on('click',function(){
                window.location.href = $(this).data('url')+'?returnUrl='+encodeURIComponent(window.location.href);
            })
            $('#loginform').aform({
                submit:".m-login-modal .button .u-btn",//提交按钮
                check:{
                    phone:{
                        empty:"手机号码不能为空",
                        correct:"正确",
                        error:"请填写正确的手机号码",
                        re:"^[0-9]{11}$"
                    },
                    phonecode:{
                        empty:"手机验证码不能为空",
                        correct:"正确",
                        error:"手机验证码错误",
                        re:"^[0-9]{4}$",
                        ajax:function(o){
                            $.ajax({
                                url:"/tools/login.html",
                                type:'post',
                                data:{phone:$('#phone').val(),verify:$('#phonecode').val()},
                                dataType:'json',
                                success:function(data){
                                    if(data.status === 0){
                                        o.correct();
                                    }else{
                                        o.error();
                                        $('.m-login-modal').hide();
                                        resetLogin();
                                        $.amsg.a('提示',data.msg,function(){});
                                    }
                                }
                            });
                        }
                    },
                    htcode:{
                        empty:"验证码不能为空",
                        correct:"正确",
                        error:"请填写正确的验证码",
                        re:"^[0-9]{4}$"
                    },
                },
                success:function(){
                    location.reload(true);
                }
            });
            var si;
            var tip = $('.m-login-modal .phonecode .u-btn.tip');
            //获取手机验证码
            $('#getphonecode').on('click',function(){
                _hmt.push(['_trackEvent', 'verify', 'click', 'PC登录获取验证码']);
                if(!$('#htcode').val() || (!/^[0-9]{4}$/.test($('#htcode').val()))){
                    $('#htcode').parents('.formitm').addClass('n-error');
                    return false;
                }
                var correct = $('#phone').parents('.formitm').hasClass('n-correct');
                if(correct){
                    //do some ting
                    //ajax 传递 手机号码 成功后提示
                    $.ajax({
                        url:'/tools/verify.html',
                        type:'post',
                        data:{phone:$('#phone').val(),code:$('#htcode').val()},
                        dataType:'json',
                        success:function(data){
                            if(data.status != 0){
                                $('.m-login-modal').hide();
                                resetLogin();
                                $.amsg.a('提示',data.msg,function(){});
                            }
                        }
                    });
                    var t = 60;
                    tip.show();
                    si = setInterval(function(){
                        tip.text(--t);
                        if(t == 0){
                            clearInterval(si);
                            tip.hide().text(60);
                        }
                    },1000)
                }else{
                    //do some ting
                    $('#phone').parents('.formitm').addClass('n-error');
                }
            });
            
            $(document).on('click','.m-login-modal .close , .m-login-modal .bg',function(){
                $('.m-login-modal').hide();
                resetLogin();
            })
            function resetLogin(){
                clearInterval(si);
                tip.hide();
                $('#phone').val('');
                $('#phonecode').val('');
                $('.m-login-modal .formitm').removeClass('n-error');
            }
        })
    </script>
            
