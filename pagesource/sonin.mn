        var dataId = $('input[name=answer'+ id + ']:checked').val();
        elem.addClass('loading');
        setTimeout(function (){
            $.ajax({
                url: "/ajax/pollSubmit/"+id+"?data_id="+dataId,
                success: function (data) {
                    $('.pollItem'+id).hide();
                    $('.pollresult'+id).html(data);
                    elem.removeClass('loading');
                },
            });
        }, 700);
        return false;
    });
