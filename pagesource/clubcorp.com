<script type="text/javascript">

(function() {
    var animateLength = 1000;
    var duration = 4000;
    var interval;
    var images = [];
    var current;
    var last;
    var resumeWait = 5000;

    function previousImageSwitch(animateLength){

        var prev = (current - 1 + last) % last;;

        // use this to pull the images in the order they appear in the markup
        var $active = $(images[current]);
        var $prev =  $(images[prev]);
        current = prev;

        $active.addClass('last-active');
        $('.club-label').html('<div class="clubinfo">' + $prev.children('.clubdata').html() + '</div>');
        $prev.css({opacity: 0.0})
            .addClass('active')
            .animate({opacity: 1.0}, animateLength, function() {
                $active.removeClass('active last-active');
            });
    }
    function nextImageSwitch(){
        
        var next = (current + 1) % last;

        var $active = $(images[current]);
        var $next =  $(images[next]);
        current = next;

        $active.addClass('last-active');
        $('.club-label').html('<div class="clubinfo">' + $next.children('.clubdata').html() + '</div>');
        $next.css({opacity: 0.0})
            .addClass('active')
            .animate({opacity: 1.0}, animateLength, function() {
                $active.removeClass('active last-active');
            });
    }

    function slideSwitch() {
        nextImageSwitch();
    }
    function goPrevImageSwitch(){
        clearInterval(interval);
        resume();
        previousImageSwitch();
    };
    function goNextImageSwitch( ){
        clearInterval(interval);
        resume();
        nextImageSwitch();
    };

    function resume(wait) {
        if (wait === 0) {
            interval = setInterval( slideSwitch, duration );
        } else {
            var delay = wait || resumeWait;
            interval = setTimeout(function() {
                                    clearTimeout(interval); 
                                    interval = setInterval( slideSwitch, duration );
                                }, delay);
        }
        
    }
    function setupImageList(){
        var $images = $('#corpSlideshow .images div.slideshowimage');
        last = $images.length;
        for (i = 0; i < last ; i++) { 
            images[i] = $images[i];
        }
        current = 0;
        $('.club-label').html('<div class="clubinfo">' + $($images[0]).children('.clubdata').html() + '</div>');
    }



     $(document).ready(function () {
        setupImageList();
        resume(0);

        $("#prev").click( goPrevImageSwitch );
        $("#next").click( goNextImageSwitch );
    });
})();





</script>






