		if(!form) return;

		var wnd = (window.name=='send_frame')?parent:window;
		var note_name_val = document.getElementById('comment_text_id');
		
		form["note_url"].value=unescape(wnd.document.URL);
		form["note_text"].value=strip_tags(str_replace('#', '', sel_text));
		form["note_ref"].value=unescape(top.document.URL);
		form["note_comment"].value=strip_tags(note_name_val.value);
		//form.submit();

		sel_text = str_replace('#', '(*)', sel_text);
		sel_text = str_replace('&nbsp;', ' ', sel_text);
		sel_text = strip_tags(sel_text, 'font');
		sel_text = str_replace(/\n/g, ' ', sel_text);
		sel_text = str_replace(/\t/g, ' ', sel_text);
		sel_text = str_replace(/\0/g, ' ', sel_text);
		sel_text = str_replace(/\ {1,}/g, ' ', sel_text);
		sel_text = str_replace(/(%20){1,}/g, ' ', sel_text);
		
		var ajaxUrl = '/bitrix/components/inoforum/message.send/templates/confirm/ajax.php?IBLOCK_ID=14&'+
			'USE_CAPTCHA=N&MAX_NAME_LENGHT=60&'+
			'EVENT_TYPE=NEW_SITE_ERROR&EVENT_TEMPLATE=44&'+
			'note_url=' + escape(wnd.document.URL) + '&note_text=' + sel_text + '&note_ref=' + escape(top.document.URL) +
			'&note_comment=' + str_replace('#', '', note_name_val.value) + '&note_email=' + str_replace('#', '', form["note_email"].value) + '&note_send=' + str_replace('#', '', form["note_send"].value);
		var errorMsgDiv = document.getElementById('errorContentDiv');
		errorMsgDiv.setAttribute('height', '250px');
		errorMsgDiv.innerHTML = '<p style="text-align:center; vertical-align:middle;"><img src="/bitrix/images/install/wait.gif" alt="Подождите..." /> Подождите...</p>'
		
		jsAjaxUtil.LoadData(ajaxUrl, prepareSelectData);
	}

	function prepareSelectData(data) {
		var errorMsgDiv = document.getElementById('errorContentDiv');
		errorMsgDiv.innerHTML = '<p style="text-align:center; vertical-align:middle;">' + data + '</p>';
		setTimeout("killErrorMsgWnd()",1500);
		//Ошибки ctrl+enter
	}

if(parent)parent.document.onkeypress=on_key_press;
document.onkeypress=on_key_press;
is_ok = true;

