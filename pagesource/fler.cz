<script type="text/javascript">
/*<![CDATA[*/
if (typeof fler == "undefined") fler = {};
if (typeof fler.sections == "undefined") fler.sections = {};
if (typeof fler.sections.frontpage == "undefined") fler.sections.frontpage = {};

fler.sections.frontpage.tabs = {

    currentTabId : null,
    currentOfferId : null,
    IID_autoRefreshTab : null,
    dataKeyPrice : 6,


    /*
      array(
        0 - id,
        1 - name,
        2 - url,
        3 - pic,
        4 - author,
        5 - price czk,
        6 - price eur,
        7 - {},//info
        )
    */
    getProductHTMLSmall : function (data)
    {
        var title = data[1] + " - " + data[this.dataKeyPrice] + " (od " + data[4] + ")";
        title = title.replace(/"/g, '\"');
        var html = '<div class="productbox small"><div class="c1">';
        html += '<a class="linkthru" title="' + title + '" href="' + data[2] + '" target="_blank">';
        if (data[7] && typeof data[7]['thstyle'] !== "undefined")
        {
            html += '<div class="thumb" style="' + data[7]['thstyle'] +'">';
        }
        else
        {
            html += '<div class="thumb">';
            html += '<img src="' +  data[3] + '" alt="" />';
        }
        html += '</div>';

        html += '<div class="thumboverlay"></div>';
        html += '</a>';
        html += '</div></div>';

        return html;
    } ,
    getProductHTMLMedium : function (data)
    {
        var title = data[1] + " - " + data[this.dataKeyPrice] + " (od " + data[4] + ")";

        var dInfo = data[7] || {};
        var isSold = dInfo["is_public"] === 0;
        var cssBox = [];
        if (isSold)
        {
            cssBox.push("itemsold");
        }
        var html = "";
        html = '<div class="productbox medium ' + cssBox.join(" ") + '" id="productbox' + data[0] +'"><div class="c1">';
        html += '<div class="buy"><a title="Vložit do košíku" href="' +  data[2] + '?a=buynow"><span>KOUPIT</span></a></div>';
        html += '<a class="linkthru"  href="' + data[2] + '" target="_blank">';

        html += '<div class="thumb" style="background-image:url(\'' +  data[3] + '\');"></div>';

        html += '<div class="hoverinfo"><div class="c2"><div class="name">' + data[1] + '</div><div class="author">od '+ data[4] + '</div><div class="price">' + data[this.dataKeyPrice] + '</div></div></div>';
        if (isSold)
        {
            html += '<span class="sold"></span>';
        }
        html += '<div class="thumboverlay"></div>';
        html += '</a>';
        html += '</div></div>';


        return html;
    } ,

    init : function()
    {

    },

    onTabChange : function(tabId, tab)
    {
        this.currentTabId = tabId;
        if (this.IID_autoRefreshTab)
        {
            clearInterval(this.IID_autoRefreshTab);
        }

        if (this.currentTabId == "ShopActivityJustViewed")
        {
            var owner = this;
            this.tsTabChanged  = new Date().getTime();
            this.IID_autoRefreshTab = setInterval(function(){
                 var now = new Date().getTime();
                 if (now - owner.tsTabChanged > (5*60*1000))
                 {
                     clearInterval(owner.IID_autoRefreshTab);
                     return;
                 }
                 tab.reloadData();
            }, 25 * 1000);
        }
    }
}


fler.sections.frontpage.tabs.offerArea = function(area)
{
    var owner = this;
    this.area = area;

    this.idArea = area.attr("id").replace("area", "");
    this.stageEl = $(".offerbox-stage", this.area);
    this.stage = $(".stageContentAnim", this.stageEl);
    this.stagePages = {};
    this.skipRollAnim = true;



    this.pagOnPage = 8;

    this.setPageData = function (data)
    {
          this.stage.html("");
        this.pagPage = 1;
        this.pageData = data;
        this.pageRecords = data.length;
        this.pagTotalPages = Math.ceil(this.pageRecords / this.pagOnPage);
    }
    this.setPageData([]);

    this.btnPrev = $(".navleft", this.area);
    this.btnNext = $(".navright", this.area);

    this.initTinyButton = function(btn)
    {
        btn.bind("mousedown", function(){
            if (!btn.hasClass("disabled"))
            {
                btn.css({ opacity: 0.6});
            }
        });
        btn.bind("mouseup", function(){
            if (!btn.hasClass("disabled"))
            {
                btn.css({ opacity: 1});
            }
        });
    }

    this.initTinyButton(this.btnPrev);
    this.initTinyButton(this.btnNext);



    this.btnPrev.bind("click", function(e){
        if($(this).hasClass("disabled"))
        {
            return false;
        }
        owner.showPage(owner.pagPage-1);
        return false;
    });
    this.btnNext.bind("click", function(e){

        if($(this).hasClass("disabled"))
        {
            return false;
        }

        owner.showPage(owner.pagPage+1);
        return false;
    });
    this.enableTinyButton = function(btn)
    {
        btn.css({ opacity: 1, cursor:"pointer" });
        btn.toggleClass("disabled", false);
    }
    this.disableTinyButton = function (btn)
    {
        btn.css({ opacity: 0.2, cursor:"default" });
        btn.toggleClass("disabled", true);
    }

    this.revalidateNav = function()
    {
        this.enableTinyButton(this.btnPrev)
        this.enableTinyButton(this.btnNext)

        if (this.pagPage <= 1)
        {
            this.disableTinyButton(this.btnPrev)
        }
        if (this.pagPage >= this.pagTotalPages)
        {
            this.disableTinyButton(this.btnNext)
        }
    }


    this.showStageInfo = function(txt)
    {
         this.stage.html('<div class="stageContent">' + txt + '</div>');
    }

    this.showPage = function (page)
    {
        //if (this._animated) return;

        //alert(this.pagTotalPages)
        if (this.pagTotalPages == 0)
        {
            this.showStageInfo('Nejsou data');
            this.revalidateNav();
            return;
        }


        if (page <= 0) return;
        if (page > this.pagTotalPages) return;




        var moveSide = page > owner.pagPage ? "right" : "left";
        //alert(moveSide)
        //var boxWidth = owner.stage.outerWidth();
        var boxWidth = owner.stage.outerWidth();
        var boxLeftPos = (boxWidth * (page-1))

        if (!this.stagePages[page])
        {
            var stageContentNew = $('<div class="stageContent"></div>');
            stageContentNew.addClass("page"+page);
            var startIndex = (page - 1) * owner.pagOnPage;
            var endIndex = page * owner.pagOnPage;
            var pos = startIndex;
            //alert(startIndex + " - " + endIndex)
            var html = [];

            while(owner.pageData[pos] && pos < endIndex)
            {
                html.push(fler.sections.frontpage.tabs.getProductHTMLMedium(owner.pageData[pos]));
                pos ++;
            }
            stageContentNew.html(html.join(""))



            stageContentNew.css({left: boxLeftPos  + "px"});

            // init clicks
            fler.sections.frontpage.productOffer.initProductMediumArea(stageContentNew, owner.idArea);

            owner.stage.append(stageContentNew);
            this.stagePages[page] = stageContentNew;
        }



        owner.pagPage = page;
        owner.revalidateNav();
        if (owner.skipRollAnim)
        {
            owner.stage.stop(true, true).css({left: "0"});
            owner.stage.append(stageContentNew);
            stageContentNew.hide().fadeIn("fast");
            owner.skipRollAnim = false;
        }
        else
        {
            var targetLeftPos = moveSide == "left" ? "+="+boxWidth : "-="+boxWidth;
            owner.stage.stop(true, true).animate({
                left : [targetLeftPos, "easeInOutCubic"]
            },
            {
                queue: false,
                duration: 400,
                complete:function(){
                    //owner.stage.css("left", targetLeftPos);
                    //$("body").prepend(owner.stage.css("left") + "<br />")
                }}
            );
        }

    }



    this.loadData = function()
    {
        owner.stagePages = {};
        owner.showStageInfo('Načítá se...');
        owner.skipRollAnim = true;
        if (!owner.pageDataLoaded)
        {
            var url = '/data/static/frontpage/data_' + owner.idTab + '.json?_dummy=' + new Date().getTime();
            $.ajax({
                url:url,
                success: function( data, textStatus ){
                    if (textStatus == "success")
                    {
                        owner.pageDataLoaded = true;
                        owner.setPageData(data);
                        owner.stage.html('');
                        owner.showPage(1);
                    }
                },
                error : function(textStatus)
                {
                    owner.pageDataLoaded = true;
                     owner.showStageInfo('Chyba při načítání!');
                }
            });
        }
        else
        {
            owner.stage.html('');
            owner.showPage(1);
        }

    }
    this.showStageInfo('Načítá se zboží.');
}



