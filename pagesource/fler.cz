fler.pagebuilder.shopmenuLeft = {
    init : function(initOpts)
    {
        initOpts = initOpts || {};

        this.root = $("#shopcattree");
        var owner = this;

        this.root.addClass('sidemenu');

        this.initCat = initOpts['cat'];
        this.initScat = initOpts['scat'];
        this.btnToggleAllScat = $("#btnToggleAllScats");
        this.btnToggleAllScatBottom = $("#btnToggleAllScatsBottom a");

        this.allShopMenu  = $('#shopMenuAllCats');

        this.visibleCat = 0;
        this.visibleScat = 0;


        this.linksScat = $("a.shopscatlink", this.root);
        this.liScat = $("li.shopscat", this.root);
        this.linksCat = $("li.shopcat a", this.root);

        this.ulCat = $("li.shopscat ul", this.root)

        this.linksScat.each(function(){
            var a = $(this);
            var scatId = a.parent("li").attr("id").replace('mnl-s', '');
            a.data("_scat_id", scatId);
        });

        this.linksCat.each(function(){
            var a = $(this);
            var catId = a.parent("li").attr("id").replace('mnl-c', '');
        })

        this.liScat.hide();
        this.root.show();


        this.btnToggleAllScat.hide();
        this.btnToggleAllScat.click(function(){
            owner.showAllScat();
            return false;
        });
        this.btnToggleAllScatBottom.hide();
        this.btnToggleAllScatBottom.click(function(){
            owner.showAllScat();
            return false;
        });


        if (this.initCat)
        {
            this.showCat(this.initCat);
        }
        else if (this.initScat)
        {
            this.showScat(this.initScat);
            $("#mnl-s" + this.initScat + " a.shopscatlink").addClass('state-on');
            // state-on

        }
        else
        {
            this.showAllScat();
        }


        $("#btnToggleAllScatsTitle").click(function(){
            owner.showAllScat();
           return false;
        }).css('cursor', 'pointer');


    } ,

    showAllScat : function()
    {
        this.ulCat.hide();
        this.liScat.show();
        this.root.addClass('fullopen');
        $('.circlebtn', this.root).remove();
        this.btnToggleAllScat.hide();
        this.btnToggleAllScatBottom.hide();
        this.allShopMenu.show();

        this.visibleCat = 0;
        this.visibleScat = 0;

    },

    showScat : function(idScat)
    {
/*
        if(this.visibleScat)
        {
            alert('aa')
            return;
        }
    */

        this.liScat.hide();
        var scatEl = $("#mnl-s" + idScat);
        var scatElLink = $("a.shopscatlink", scatEl).eq(0);

        //scatElLink.after('<span class="circlebtn" href="javascript:;"><em>@</em></span>');

        $("ul", scatEl).show();
        $("ul li", scatEl).show();
        scatEl.addClass('shopscat-on').show();



        this.btnToggleAllScat.show();
        this.btnToggleAllScatBottom.show();
        this.allShopMenu.hide();

        this.root.removeClass('fullopen');




        this.visibleCat = 0;
        this.visibleScat = idScat;

        $('.circlebtn', scatEl).remove();



    },
    showCat : function(idCat)
    {

        //this.liScat.hide();
        var owner = this;
        var catEl = $("#mnl-c" + idCat);

        var parentUl = catEl.parent("ul");

        var catElLink = $("a", catEl).eq(0);


        var parentLiScat = parentUl.parent("li").eq(0);
        if (parentLiScat.length == 0)
        {
            return;
        }
        var parentScatId = parentLiScat.attr("id").replace('mnl-s', '');

        var scatElLink = $("a", parentLiScat).eq(0);
        this.showScat(parentScatId);


        //catElLink.append('<em></em>');
        /*
        var btnCircle = $('<span class="circlebtn" href="javascript:;"><em>@</em></span>');
        btnCircle.attr('title', 'Zobrazit celou kategorii');
        btnCircle.click(function(){
           owner.showScat(parentScatId);
           btnCircle.hide();
           return false;
        });
        scatElLink.after(btnCircle);

        $("li", parentUl).hide();
        */
        catEl.addClass('shopcat-on');


        this.root.removeClass('fullopen');
        this.btnToggleAllScatBottom.show();


        scatElLink.attr('title', 'Zobrazit celou kategorii');

        this.visibleCat = idCat;



    }
}
