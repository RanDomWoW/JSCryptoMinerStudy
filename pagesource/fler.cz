    this.revalidateNav = function()
    {
        this.enableTinyButton(this.btnPrev)
        this.enableTinyButton(this.btnNext)

        if (this.pagPage <= 1)
        {
            this.disableTinyButton(this.btnPrev)
        }
        if (this.pagPage >= this.pagTotalPages)
        {
            this.disableTinyButton(this.btnNext)
        }
    }


    this.showPage = function (page)
    {
        if (this.pagTotalPages == 0)
        {
            this.stage.html('<div class="stageContent">Nejsou data</div>');
            this.revalidateNav();
            return;
        }


        if (page <= 0) return;
        if (page > this.pagTotalPages) return;




        var moveSide = page > owner.pagPage ? "right" : "left";
        var boxWidth = 765;
        var boxLeftPos = (boxWidth * (page-1))

        if (!this.stagePages[page])
        {
            var stageContentNew = $('<div class="stageContent"></div>');
            stageContentNew.addClass("page"+page);
            var startIndex = (page - 1) * owner.pagOnPage;
            var endIndex = page * owner.pagOnPage;
            var pos = startIndex;
            var html = [];

            while(owner.pageData[pos] && pos < endIndex)
            {
                html.push(fler.sections.frontpage.tabs.getProductHTMLSmall(owner.pageData[pos]));
                pos ++;
            }
            stageContentNew.html(html.join(""))
            stageContentNew.css({left: boxLeftPos  + "px"});



            owner.stage.append(stageContentNew);
            this.stagePages[page] = stageContentNew;
        }



        owner.pagPage = page;
        owner.revalidateNav();

        if(owner.pagPage > 1)
        {
            owner._elShowAll.fadeIn();
        }
        else
        {
            owner._elShowAll.fadeOut();
        }


        if (owner.skipRollAnim)
        {
            owner.stage.stop(true, true).css({left: "0"});
            owner.stage.append(stageContentNew);
            stageContentNew.hide().fadeIn("fast");
            owner.skipRollAnim = false;
        }
        else
        {

            owner.stage.stop(true, true).animate({
                left : [moveSide == "left" ? "+="+boxWidth : "-="+boxWidth, "easeOutExpo"]
            },
            {
                duration: 600,
                complete:function(){
                }}
            );
        }

    }


    this.reloadData = function()
    {
        this.pageDataLoaded = false;
        this.loadData();
    }

    this.loadData = function()
    {
        owner.skipRollAnim = true;
        owner.stagePages = {};
        owner.stage.html('<span>Načítá se...</span>');


      var url = '/data/static/frontpage/data_' + owner.idTab + '.json?_dummy=' + new Date().getTime();
      url = url.replace('.json', '_v2.json');
        /*
        if (owner.idTab == 'ShopActivityJustViewed')
        {
            url = url.replace('.json', '_v2.json');
        }
        */

        $.ajax({
            url:url,
            success: function( data, textStatus ){

                if (textStatus == "success")
                {
                    owner.pageDataLoaded = true;
                    owner.setPageData(data);
                    owner.stage.html('');
                    owner.showPage(1);
                    owner.lastLoadTime = new Date().getTime();
                }
            },
            error : function(textStatus)
            {
                owner.pageDataLoaded = true;
                //owner.stage.html('<div class="stageContent">Nejsou data</div>');
            }
        });
    }



    this.onLinkClick = function()
    {
        owner.activateLink();
        owner.showTitle();

        owner.stageEl.attr("id", "stageContent"+owner.idTab);

        $(".tabbedlisting-tabs a", owner.area).toggleClass("on", false);
        owner.link.toggleClass("on", true);


        if (owner.lastLoadTime > 0)
        {
            var diffOld = Math.floor((new Date().getTime() - owner.lastLoadTime) / 1000);
            if (diffOld > 10)
            {
                owner.pageDataLoaded = false;
            }
        }

        if (!owner.pageDataLoaded)
        {
            this.loadData();
        }
        else
        {
            owner.skipRollAnim = true;
            owner.stagePages = {};
            owner.stage.html('<span>Načítá se...</span>');
            owner.showPage(1);
        }

        fler.sections.frontpage.tabs.onTabChange(owner.idTab, owner);

    }

    // INIT
    //this.showPage(1);

}




