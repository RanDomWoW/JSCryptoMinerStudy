  <script src="https://cdnjs.cloudflare.com/ajax/libs/jsrender/0.9.90/jsrender.min.js?v201803211759"></script>
<script type="text/javascript">
    (function(){
        var tmplStr = false;
        var CONTAINER_ID = "skm-recommend__container";
        var LINE_ID = "skm-recommend__line";
        var LOADING = "skm-recommend__loading";
        var LIMIT = 3;
        var loadCount = 0;
        var targetDOM = document.getElementById("skm-recommend");
        var container = document.createElement("div");
        container.setAttribute("id", CONTAINER_ID);
        var line = document.createElement("div");
        var loader = document.getElementById("skm-recommend__loading");
        line.setAttribute("id", LINE_ID);
        targetDOM.appendChild(container);
        targetDOM.appendChild(line);
        var loadedData = [];

        function checkStatus() {
            if (loadCount >= LIMIT) {
                showLoading(loader, false);
                disable();
                return false;
            }
            return true;
        }

        function load(container) {
            container.classList.add(LOADING);
            loadTemplate(function(t) {
                loadData(function(d) {
                    showLoading(loader, false);
                    var template = $.templates(t);
                    var html = template.render(d);
                    var dom = document.createElement("div");
                    dom.innerHTML = html;
                    container.appendChild(dom);
                    $('img').unveil();
                    container.classList.remove(LOADING);
                    if (d.meta) {
                        loadedData = loadedData.concat(d.meta);
                    }
                    if (window._) {
                        _.createArrowInScroll();
                    }
                    loadCount ++;
                    checkStatus();
                })
            });
        }

        function loadTemplate(cb) {
            if (tmplStr) {
                cb(tmplStr);
                return;
            }
            $.ajax({
                url: '/api/book/recommend/template/',
                type: 'GET',
                success: function (templateString, textStatus) {
                    if (textStatus !== "success") {
                        showLoading(loader, false);
                        disable();
                        return;
                    }
                    tmplStr = templateString;
                    cb(templateString)
                },
                error: function() {
                    showLoading(loader, false);
                    disable();
                }
            });
        }

        function loadData(cb) {
            $.ajax({
                url: '/api/book/recommend/hp/',
                data: {
                    'csrfmiddlewaretoken': CSRFTOKEN,
                    data: JSON.stringify(loadedData)
                },
                type: 'POST',
                dataType: 'json',
                success: function(data, textStatus) {
                    if (textStatus !== "success") {
                        showLoading(loader, false);
                        return;
                    }
                    cb(data)
                },
                error: function() {
                    showLoading(loader, false);
                }
            });
        }

        function checkLazyLoad() {
            var container = document.getElementById(CONTAINER_ID);
            if (!container) return;
            if (container.classList.contains(LOADING)) return;
            var line = document.getElementById(LINE_ID);
            if (!line) return;

            var seeHeight = document.documentElement.clientHeight;
            var scrollTop = document.documentElement.scrollTop || document.body.scrollTop;
            var viewing = line.offsetTop;
            if (!checkStatus()) {
                return;
            }
            if(viewing < seeHeight + scrollTop) {
                load(container);
                showLoading(loader, true)
            }
        }

        function showLoading(dom, shouldShow) {
            if (shouldShow) {
                dom.style.display = "block";
            } else {
                dom.style.display = "none";
            }
        }

        function disable() {
            window.removeEventListener('scroll', checkLazyLoad, false);
        }

        window.addEventListener('scroll', checkLazyLoad, false)
    })()
</script>





















































































































<script>
  $(function () {
    var $iconMenu = $("#icon_menu");
    var $header_side = $('#header_side');
    var $navbar = $('#navbarToggleExternalContent');
    var $iconSearch = $("#icon_search");
    var $iconMenuWrap = $('.bg-dark-wrap-menu');
    var $iconSearchWrap = $('.bg-dark-wrap-search');
    $iconMenu.on('click', function () {
      var $verifyIconSearch = $("#icon_search");
      if ($verifyIconSearch.hasClass('icon-close')) {
        $verifyIconSearch[0].click();
      }
      if ($(this).hasClass('icon-menu')) {
        $(this).removeClass('icon-menu');
        $(this).addClass('icon-close');
        $header_side.addClass('left-zero');
        $iconMenuWrap.show();
        _.blockBody();
        sendEvent('ヘッダー操作', 'メニュークリック');
      } else {
        $(this).removeClass('icon-close');
        $(this).addClass('icon-menu');
        $header_side.removeClass('left-zero');
        $iconMenuWrap.hide();
        _.releaseBody();
      }
    });
    $iconSearch.on('click', function () {
      sendEvent('ヘッダー操作', '検索クリック');
    });
    $navbar.on('show.bs.collapse', function () {
      var $verifyIconMenu = $("#icon_menu");
      if ($verifyIconMenu.hasClass('icon-close')) {
        $verifyIconMenu[0].click();
      }
      $iconSearch.removeClass('icon-search');
      $iconSearch.addClass('icon-close');
      $iconSearchWrap.show();
    });
    $navbar.on('hide.bs.collapse', function () {
      $iconSearch.removeClass('icon-close');
      $iconSearch.addClass('icon-search');
      $iconSearchWrap.hide();
    });
    $iconMenuWrap.on('click', function () {
      $iconMenu[0].click();
    });
    $iconSearchWrap.on('click', function () {
      $iconSearch[0].click();
    });

