	function validateForm_371() {	
		jQuery("#contact-form_371").validate({
			rules: {
				input_name_371: {
					required: true
				},input_email_371: {
					required: true,
					email: true
				},input_phone_371: {
					required: true
				},input_country_371: {
					required: true
				},input_services_371: {
					required: true
				},input_comments_371: {
					required: true
				},input_captcha_371: {
					required: true
				}},
			messages: {
				input_name_371: {
					required: "Please enter your name"
				},input_email_371: {
					required: "Please enter your email address"
				},input_phone_371: {
					required: "Please enter your phone number"
				},input_country_371: {
					required: "Please select the country"
				},input_services_371: {
					required: "Please select the services"
				},	input_comments_371: {
					required: "Please enter your comments"
				},input_captcha_371: {
					required: "Please enter the security code"
				}},
			errorClass: "help-block",
			errorElement: "span",
			highlight:function(element, errorClass, validClass) {
				jQuery(element).parents(".control-group").addClass("has-error");
			},
			unhighlight: function(element, errorClass, validClass) {
				jQuery(element).parents(".control-group").removeClass("has-error");
				jQuery(element).parents(".control-group").addClass("has-success");
			},			
			submitHandler: function (form) {
				jQuery.ajax({
					type: "POST",
					cache: false,
					url: "https://www.uaeexchange.com/index.php?option=com_ajax&module=responsive_contact_form&method=savefrom&format=raw&id=Mzcx&Itemid=175",
					dataType: "html",
					data: jQuery("#contact-form_371").serializeArray(),
					beforeSend: function() {
					},
					error: function(data, textStatus, errorThrown) {
						alert("Request failed :" + textStatus + errorThrown);
					},
					success: function(results) {
						if (parseInt(results) > 0) {
							alert("Thank You! Your message has been sent."); 			reloadCaptchaImg("img_captcha_371"); 			clearForm(jQuery("#contact-form_371"));
						} else {
							alert(results);
						}
					}
				});			
				//form.submit();
			}
		}).form();  
		
		if(jQuery("#contact-form_371").valid()){
			jQuery("#contact-form_371").submit();
			return false;
		}	 
	}
