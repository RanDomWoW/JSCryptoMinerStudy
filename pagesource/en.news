<!doctype html>
<!--[if lt IE 7]>
<html class="no-js lt-ie9 lt-ie8 lt-ie7" ng-controller="MainController as mainCtrl"> <![endif]-->
<!--[if IE 7]>
<html class="no-js lt-ie9 lt-ie8" ng-controller="MainController as mainCtrl"> <![endif]-->
<!--[if IE 8]>
<html class="no-js lt-ie9"  ng-controller="MainController as mainCtrl"> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js"  ng-controller="MainController as mainCtrl"> <!--<![endif]-->
<head>
    
    <script src="https://cdn.optimizely.com/js/1411446908.js"></script>
    <!-- Can this be moved to the body tag where the rest of the JS scripts are loaded? -->
    <script src="https://www.google.com/recaptcha/api.js?render=explicit" async defer></script>
    <base href="/">
    <meta charset="utf-8">
    <meta name="viewport" content="initial-scale=1.0,width=device-width"/>
    <meta http-equiv="cache-control" content="max-age=0"/>
    <meta http-equiv="cache-control" content="no-cache"/>
    <meta http-equiv="expires" content="0"/>
    <meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT"/>
    <meta http-equiv="pragma" content="no-cache"/>
    <meta name="theme-color" content="{{mainCtrl.branding.mobileThemeColor}}">
    <meta name="author" content="WeSpeke Inc."/>
    <meta name="copyright" content="&copy; {{mainCtrl.date | date:'yyyy'}} WeSpeke Inc."/>
    <meta name="description" content="Start learning for FREE by reading CNN articles. Read as many as you want and come back every day for new lessons at https://en.news."/>
    <meta property="og:site_name" content="en.news with CNN"/>
    <meta property="og:title" content="Learn English for free with CNN. Today’s news becomes today’s English lessons."/>
    <meta property="og:type" content="website"/>
    <meta property="og:description" content="Start learning for FREE by reading CNN articles. Read as many as you want and come back every day for new lessons at https://en.news."/>
    <meta property="og:image" content="https://content.en.news/assets/img/site-preview.png"/>
    <meta name="twitter:card" content="summary_large_image"/>
    <meta name="twitter:title" content="Learn English for free with CNN. Today’s news becomes today’s English lessons.">
    <meta name="twitter:description" content="Start learning for FREE by reading CNN articles. Read as many as you want and come back every day for new lessons at https://en.news."/>
    <meta name="twitter:image" content="https://content.en.news/assets/img/site-preview.png"/>
    <title ng-bind="mainCtrl.windowTitle"></title>
    <link rel="shortcut icon" href="/images/cnn/favicon.ico" type="image/x-icon">
    <link rel="apple-touch-icon" href="/images/cnn/touch-icon.png">
    <link rel="icon" href="/images/cnn/touch-icon.png">
    <link rel="stylesheet" href="/styles/vendor.css?d=1521493849683">
    <link rel="stylesheet" href="/styles/main.css?d=1521493849683">
</head>
<body ng-class="{'fixed-layout': mainCtrl.LayoutService.layout.fixedLayout, 'mobile-web': mainCtrl.LayoutService.layout.mobileWeb}">
    <div ng-class="{'user-session-changing': mainCtrl.LayoutService.sessionChanging}">
        <ws-navigation show-authenticated-links="mainCtrl.LayoutService.layout.showAuthenticatedLinks"
                       show-non-authenticated-links="mainCtrl.LayoutService.layout.showNonAuthenticatedLinks"
                       header-logo-navigates="mainCtrl.LayoutService.layout.headerLogoNavigates"
                       is-marketing-logo-nav="mainCtrl.LayoutService.layout.isMarketingLogoNav"
                       show-header="mainCtrl.LayoutService.layout.showHeader"
                       right-rail="mainCtrl.LayoutService.layout.rightRail"
                       grid-frame-class="mainCtrl.LayoutService.gridFrameClass"></ws-navigation>
        <general-messages
            class="general-messages-wide general-messages-desktop-responsive {{mainCtrl.LayoutService.gridFrameClass}}"
            ng-class="{'has-right-rail': mainCtrl.LayoutService.layout.rightRail}"></general-messages>
        <div class="content-responsive {{mainCtrl.LayoutService.containerClass}}"
             ng-class="{'has-right-rail': mainCtrl.LayoutService.layout.rightRail,
                    'has-responsive-header': mainCtrl.LayoutService.hasResponsiveHeader,
                    'no-header': !mainCtrl.LayoutService.layout.showHeader}"
             ng-style="{'background-image': 'url('+ mainCtrl.background +')'}"
             id="content-responsive-container"
             ng-cloak>

            <header id="contentResponsiveHeader"
                    class="content-responsive-header"
                    ui-view="contentResponsiveHeader"></header>

            <div class="content-responsive-below-header">
                <div class="main-content-wrap">
                    <div id="contentResponsive"
                         class="content-responsive-subcontainer"
                         ui-view="contentResponsive"></div>
                </div>
            </div>
            <footer ng-if="mainCtrl.LayoutService.layout.showFooter" ui-view="footer"
                class="grid-frame"></footer>
        </div>
        <ws-right-rail-mobile-button class="right-rail-mobile-button"
                         ng-class="{'open':(mainCtrl.LayoutService.layout.rightRailOpen)}"
                         ng-if="mainCtrl.hasRightRailMobileButton()"
                         ng-click="mainCtrl.onMobileRightRailClick()"
                         user-history-list="mainCtrl.userHistoryList"
                         ng-cloak>
        </ws-right-rail-mobile-button>
        <div ui-view="rightRail" class="l-rail l-right"
             ng-class="{'open':(mainCtrl.LayoutService.layout.rightRailOpen)}"
             ng-if="mainCtrl.LayoutService.layout.rightRail"
             ng-cloak>
        </div>

    </div>

    <ws-glass></ws-glass>

    <div loader-message></div>

    <script src="https://static.opentok.com/v2/js/opentok.min.js"></script>
    <script src="https://js.stripe.com/v2/"></script>
    <script src="/bundle/jquery.min.js?d=1521493849683"></script>
    <script src="/bundle/vendor.bundle.js?d=1521493849683"></script>
    <script src="/bundle/app.bundle.js?d=1521493849683"></script>
    <script src="/bundle/uri.js?d=1521493849683"></script>
    <script src="/bundle/settings.js?d=1521493849683"></script>
    <script src="/bundle/translation.js?d=1521493849683"></script>
    <script src="/bundle/branding.js?d=1521493849683"></script>
    <script src="//www.youtube.com/iframe_api"></script>
    <script src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-555364136b45acfb&async=1" async></script>


    <script>
        angular.element(document).ready(function () {
            // Bootstrap the app after TokBox has loaded since it's used to determine if a user has audio/video capabilities
            angular.bootstrap(document, ['wespekeApp']);
        });

        // Attach Foundation
        $(document).foundation();
    </script>

</body>
</html>