			$('.film-list-item', slider).eq(slider_index).show();
			$('.film-list-item a', slider).eq(slider_index).fadeIn(300);
		}
		
		if(slider_index < count_active_sliders - 1){
			setTimeout(function() {
				slider_items_action(type);
			}, 50);
			slider_index = slider_index + 1;
		}else{
			slider_index = 0;
		}
	}
	
	function slider_action_next(){
		var sliders = $('.film-list-item', slider);
		var sliders_active = $('.active-item', slider);

		if(!action_first){
			slider_items_action('hide');
			setTimeout(function() {
				$('.film-list-item', slider).hide();
				sliders.eq(0).appendTo(slider_container);
				sliders = $('.film-list-item', slider);
			}, 600);
		}		
		
		setTimeout(function() {
			slider_items_action('show');
		}, 600);
		
		
		
		action_first = false;
		return false;
	}
	next.click(function(){
		slider_action_next('');
	});
	
	// Prev
	function slider_items_action_prev(type){
		if(type == 'hide'){
			$('.film-list-item a', slider).eq(slider_index_prev).fadeOut(300);
		}else{
			$('.film-list-item a', slider).eq(slider_index_prev).fadeIn(300);
		}
		
		if(slider_index_prev > 0){
			setTimeout(function() {
				slider_items_action_prev(type);
			}, 50);
			slider_index_prev = slider_index_prev - 1;
		}else{
			slider_index_prev = count_active_sliders - 1;
		}
	}
	
	function slider_action_prev(){
		var sliders = $('.film-list-item', slider);
		var sliders_active = $('.active-item', slider);

		if(!action_first){
			slider_items_action_prev('hide');
			setTimeout(function() {
				$('.film-list-item', slider).hide();
				sliders.last().prependTo(slider_container).show().find('a').fadeOut(0);
				sliders = $('.film-list-item', slider);
			}, 600);
		}		
		
		setTimeout(function() {
			sliders.slice(0, 5).show();
			slider_items_action_prev('show');
		}, 600);
		
		
		
		action_first = false;
		return false;
	}
	prev.click(function(){
		slider_action_prev('');
	});
	
	
	$('.film-list-item', slider).hide().removeClass('active-item');
	$('.film-list-item a', slider).fadeOut(0);
	slider_action_next();
	

	// Слайдер в сайтбар
	$('.sidebar-list-films').each(function(){
		var sid_slider = this;
		var sid_slider_container = $('.film-list-conteiner', sid_slider);
		var sid_prev = $('.films-list-nav-prev', sid_slider);
		var sid_next = $('.films-list-nav-next', sid_slider);
		$('.film-list-item', sid_slider).fadeOut(0);
		$('.film-list-item:first', sid_slider).fadeIn(300);
		
		sid_prev.click(function(){
			$('.film-list-item:first', sid_slider).fadeOut(300, function(){
				$('.film-list-item:last', sid_slider).prependTo(sid_slider_container);
				$('.film-list-item:first', sid_slider).fadeIn(300);
			});
		});
		sid_next.click(function(){
			$('.film-list-item:first', sid_slider).fadeOut(300, function(){
				$(this).appendTo(sid_slider_container);
				$('.film-list-item:first', sid_slider).fadeIn(300);
			});
		});
	});
	
});
</script>
