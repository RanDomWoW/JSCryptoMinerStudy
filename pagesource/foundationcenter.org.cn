<script type="text/javascript" charset="utf-8">
    $(document).ready(function() {
        var isFirst = true;
        window.onscroll = function() {
            var top = $("#allcount").parent().parent().parent().offset().top;
            var scrollTop = $(document).scrollTop();
            var docHeight = $(document).height();
            if (scrollTop >= top - 500) {
                isFirst = false;
                window.onscroll = function() { };
                AutoCount($("#allcount"), 22);
                AutoCount($("#money"), 5);
            }
        }
        function AutoCount(that, count) {
            var num = Number($(that).attr("count"));
            var current = 0;
            var interval = setInterval(function() {
                current = Number($(that).html());
                current = current + count;
                if (current <= num) {
                    $(that).html(current);
                } else {
                    $(that).html($(that).attr("count"));
                    clearInterval(interval);
                }
            }, 1);
        }
        moveTopTen();
        $('.announcement-box').slideToggle('slow');
        var moveTo = ($(window).width() / 1.5) * (-1);
