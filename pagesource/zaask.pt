<script>
    var dropContainer2Trigger = document.getElementById('drop-container2-trigger');
    var menuMainDropdown = document.getElementById('menu-main-dropdown');
    var menuOpenClassName = 'open-menu';
    window.addEventListener('click', function(e){
        if (dropContainer2Trigger && !dropContainer2Trigger.contains(e.target)){
            menuMainDropdown.className = menuMainDropdown.className.replace(new RegExp('(^|\\b)' + menuOpenClassName.split(' ').join('|') + '(\\b|$)', 'gi'), ' ');
        }
    });
    
    if(dropContainer2Trigger){
        dropContainer2Trigger.addEventListener('click', function(e){
            var classString = menuMainDropdown.className;
            var nameIndex = classString.indexOf(menuOpenClassName);
            if (nameIndex == -1) {
                classString += ' ' + menuOpenClassName;
            }
            else {
                classString = classString.substr(0, nameIndex) + classString.substr(nameIndex+menuOpenClassName.length);
            }
            menuMainDropdown.className = classString;
        });
    }
</script>

