  var switched_by_pic=false;
  var switched_by_variant=false;

  var selectCallbackQuickShop = function(variant, selector) {
    var product_id = selector.product.id
    if(variant && variant.sku){
      $('.brand-'+product_id).html('SKU: '+variant.sku);
      $('#sku_line_item_'+product_id).val(variant.sku);
    }

    if(ifTemplate == 'pre-order'){
      var submitText = 'Pre-Order';
    }
    else{
      var submitText = 'Add to cart';
    }

    var id = selector.domIdPrefix.split('-')[2];

    if (id) {
      context = $('#quick-shop-' + id)
    } else {
      context = $('#product-area, .mobile-product-title')
    }



    if (variant && variant.available == true) {

      if (id) {
        context.find('.submit').text(submitText)
      } else {
        context.find('.submit').attr('value', submitText)
      }




      if ( variant.compare_at_price > 0 && variant.compare_at_price > variant.price ) {
        context.find('.price').html('<span class="original money">' + Shopify.formatMoney(variant.compare_at_price, settings.moneyFormat) + '</span><span>' + Shopify.formatMoney(variant.price, settings.moneyFormat) + '</span>');
        context.find('.price').attr('data-price',Shopify.formatMoney(variant.compare_at_price, settings.moneyFormat));
//         context.find('.price').html('<span class="original money" data-currency-' + settings.currency + '="' + Shopify.formatMoney(variant.compare_at_price, settings.moneyFormat) + '">' + Shopify.formatMoney(variant.compare_at_price, settings.moneyFormat) + '</span> <span class="money" data-currency-' + settings.currency + '="' + Shopify.formatMoney(variant.price, settings.moneyFormat) + '">' + Shopify.formatMoney(variant.price, settings.moneyFormat) + '</span>');  // update price field
//       	context.find('.price').attr('data-price',Shopify.formatMoney(variant.compare_at_price, settings.moneyFormat));
      } else {
        context.find('.price').html('<span class="money">' + Shopify.formatMoney(variant.price, settings.moneyFormat) + '</span>');  // update price field
      	context.find('.price').attr('data-price',Shopify.formatMoney(variant.price, settings.moneyFormat));
//         context.find('.price').html('<span class="money" data-currency-' + settings.currency + '="' + Shopify.formatMoney(variant.price, settings.moneyFormat) + '">' + Shopify.formatMoney(variant.price, settings.moneyFormat) + '</span>');  // update price field
//       	context.find('.price').attr('data-price',Shopify.formatMoney(variant.price, settings.moneyFormat));
      }

      if (id) {
        context.find('.submit').removeClass('disabled').text(submitText); // remove unavailable class from add-to-cart button, and re-enable button
      } else {
        context.find('.submit').removeClass('disabled').attr('value', submitText); // remove unavailable class from add-to-cart button, and re-enable button
      }

    } else {
      var message = variant ? "Sold Out" : "Please Make A Selection";
      if (id) {
        context.find('.submit').addClass('disabled').text(message)
      } else {
        context.find('.submit').addClass('disabled').attr('value', message)
      }

      context.find('.price').text(message);



    }

   		 var additional_price = parseFloat(context.find('.price').attr('data-additional').replace('$',''));
      	var base_price = parseFloat( context.find('.price').attr('data-price').replace('$',''));
      	var new_price = base_price + additional_price;
        context.find('.price .money:not(".original")').html('$'+new_price);
    if(switched_by_pic==false){
      if (variant && variant.featured_image) {
        var newImage = variant.featured_image;
        var smallImage = Shopify.Image.getSizedImageUrl(newImage.src, 'small');

        if(ifTemplate == 'product'){
        	switched_by_variant = true;
          $('#product-area .thumb img[src*="'+smallImage+'"]').click();
        }
        else{
        	switched_by_variant = true;
          $('.quick-shop-content .thumb img[src*="'+smallImage+'"]').click();
        }
      }
    }
    else {
    	switched_by_pic = false;
    }

    if (settings.currencySwitcher) {
      $(document.body).trigger('switch-currency');
    }

    switched_by_variant = false;
  };

  </script>



