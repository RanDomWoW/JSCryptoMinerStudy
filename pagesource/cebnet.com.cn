<script type="text/javascript">
    //首页焦点图切换
    $(function(){
        //代码初始化
        var size=$(".bd li").size();

        for(var i=1; i<=size; i++){
            var li="<li></li>";
            $(".hd").append(li);
        }

        //手动控制轮播图
        $(".bd li").eq(0).show();
        $(".hd li").eq(0).addClass("active");
        $(".hd li").mouseover(function(){
            $(this).addClass("active").siblings().removeClass("active");
            var index=$(this).index();
            i=index;
            $(".bd li").eq(index).stop().fadeIn(500).siblings().stop().fadeOut(500);
        })

        //自动轮播
        var i=0;
        var t=setInterval(move,2500);

        //核心向左运动函数
        function moveL(){
            i--;
            if(i==-1){
                i=size-1;
            }
            $(".hd li").eq(i).addClass("active").siblings().removeClass("active");
            $(".bd li").eq(i).fadeIn(500).siblings().fadeOut(500);

        }
        //核心向右运动函数
        function move(){
            i++;
            if(i==size){
                i=0;
            }

            $(".hd li").eq(i).addClass("active").siblings().removeClass("active");
            $(".bd li").eq(i).fadeIn(500).siblings().fadeOut(500);

        }
        //左边按钮点击事件
        $(".first_switchover .btn_left").click(function(){

            moveL();
        })

        //右边按钮点击事件
        $(".first_switchover .btn_right").click(function(){
            move();
        })

        //定时器的开始于结束
        $(".first_switchover").hover(function(){
            clearInterval(t);
        },function(){
            t=setInterval(move,2500);
        })
    })
</script>
