	(function(){
		var pageType = document.URL.split("/")[document.URL.split("/").length-2];
		if(pageType == 'product'){
			if(window.location.hash.substring(1) == 'print'){
				window.print();
				window.close();
			}
		}
	})(jQuery);
	/* Detailseite Preise ab 5 Stellen verkleinern */
	(function(){
		var pageType = document.URL.split("/")[document.URL.split("/").length-2];
		if(pageType == 'product'){
			var price = jQuery('meta[property="product:price:amount"]').attr('content');
			if(jQuery.isNumeric(price) && price > 9999){
				jQuery('.price.big').removeClass('big').addClass('p53');
			}
		}
	})(jQuery);
});
