			<div class="sa-section sa-section-products">
				<span class="sa-headline">{{{ primary.products.label }}}</span>
				<ul class="sa-results-list">
					{{#each primary.products.items}}
						<li class="sa-item">
							<a href="{{ this.link }}" class="sa-item-link" data-value="{{ this.value }}" data-type="{{ this.type }}">
								{{#if this.imageSrc}}
									<span class="sa-item-image">
										<img src="{{ this.imageSrc }}" alt="{{ this.label }}" />
									</span>
								{{/if}}
								<span class="sa-item-details">
									<span class="sa-item-label">{{{ this.label }}}</span>
									<span class="sa-item-label-additional">
										{{#if this.price }}
											<span class="sa-item-price">{{ this.price.base }},{{this.price.fraction}} EUR</span>
										{{/if}}
										<span class="sa-item-label-additional-attributes">
											{{#if this.additionalAttributes}}
												{{#each this.additionalAttributes}}
													<span class="sa-item-availability sa-item-attr-{{this.attributeCssClass}} sa-item-val-{{this.valueCssClass}}">{{ this.value }}</span>
												{{/each}}
											{{/if}}
										</span>
									</span>
								</span>
							</a>
						</li>
					{{/each}}
				</ul>
			</div>
			{{# if primary.brands }}
				<div class="sa-section sa-section-brands">
					<span class="sa-headline">{{{ primary.brands.label }}}</span>
					<ul class="sa-results-list">
						{{#each primary.brands.items}}
							<li class="sa-item">
								<a href="#" class="sa-item-link"
									 data-value="{{ this.value }}"
									 data-type="{{ this.type }}"
									 data-params="{{ this.params }}">
									<span class="sa-item-brand-image">
										<img src="{{this.image}}" alt="{{ this.label }}" />
									</span>
								</a>
							</li>
						{{/each}}
					</ul>
				</div>
			{{/if}}
		</div>
	{{/if}}

	{{#if secondaryCounter}}
		<div class="sa-secondary sa-column">
			{{#each secondary}}
				{{#if this.items}}
					<div class="sa-section sa-section-{{@key}}">
						<span class="sa-headline">{{{ this.label }}}</span>
						<ul class="sa-results-list">
							{{#each this.items}}
								<li class="sa-item">
									<a href="#" class="sa-item-link"
										 data-value="{{ this.value }}"
										 data-type="{{ this.type }}"
										 data-top-level-category="{{ this.topLevelCategory }}"
										 data-params="{{ this.params }}">
										{{#if this.isQueryInCategory }}
											<span class="sa-item-important sa-item-query">{{{ this.label }}}</span> in <span class="sa-item-category">{{ this.categoryName }}</span>
										{{else}}
											{{{ this.label }}}
										{{/if}}
									</a>
								</li>
							{{/each}}
						</ul>
					</div>
				{{/if}}
			{{/each}}
