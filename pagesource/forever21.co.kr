<script type="text/javascript">
    function fnConfirm(innerHTML) {
        var message = innerHTML;

        $.confirm({
            title: '',
            content: message,
            animation: 'top',
            closeIcon: true,
            confirmButton: false,
            cancelButton: false,
            columnClass: 'col-md-8'
        });
    }

    function fnShopTheGramDefalutLayer() {
        //데이터를 가져오는데 실패한 경우 신상품 보여지도록
    }
       
    function fnShopTheGramLayer(platFormIdx, device) {
        // Ajax
        $.ajax({
            type: "POST",
            url: "/Ajax/Ajax_ShopTheGram.aspx?type=specific&platformidx=" + platFormIdx + "&device=" + device,
            contentType: "application/json; charset=euc-kr",
            dataType: "json",
            async: false,
            success: function (response) {
                if (response["resultValue"] == "OK") {
                    //popup inline으로 추가하는 로직
                    fnConfirm(response["popupHTML"]);
                }
                else {
                    //품절 상품, 에러 발생, DB 접속 실패 등 성공하지 못한 경우 defalut popup 설정해서 보여지도록
                    fnShopTheGramDefalutLayer();
                }
            },
            error: function () {
                //에러가 발생하거나 데이터를 못가져왔을 경우 defalut popup 설정해서 보여지도록
                fnShopTheGramDefalutLayer();
            }
        });
    }

</script>
