<script type="text/javascript">

    function isEmailValid(email) {
	     var re = /[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?/i;
	     return re.test(email)		
    }
    function subscribeEmailOptin(element) {
        var selector = $(element);
        var promoName = '';
        var popidx = '';
        popidx = '180';
        if ('N' == 'Y' || $("#imgPopupThxNaver").is(':visible') === true) {
            promoName = 'cafe_mkt';
            popidx = '';
        }

        if (!$(".privacyCheck").is(':checked')) {
            alert('개인정보 취급방침에 동의하셔야 이메일 구독신청을 하실 수 있습니다.');
            return;
        }

        if ($(selector).val() == '' || isEmailValid($(selector).val()) == false) {
            alert("이메일 주소를 확인하여 주세요.");
            $(selector).focus();
            return false;
        }

        document.getElementById("pcEmailOptin").disabled = true;
        addSubscribeEamilOptin(element, promoName, popidx);
        return;
    }

    function addSubscribeEamilOptin(selector, promoName, popidx) {
        var eMail = $(selector).val();
        $.ajax({
            type: "POST",
            url: "/Controls/Subscribe/EmailSubscribe.aspx",
            data: {
                Email: eMail,
                subTime: nowTime(),
                emailType: 'forever21',
                promotionName: promoName,
                popidx: popidx
            },
            success: function (sResultText) {
                if (sResultText.toString().indexOf("Thank You") > -1) {
                    $(selector).val('');
                        
                    if($("#imgPopupNaver").is(':visible') === true){
                        $('.signup_enjoy').hide();
                        $('#imgPopupThxDefault').hide();
                        $('#imgPopupThxNaver').show();
                        $('#email_result').show();
                    }
                    else{
                        $('.signup_enjoy').hide();
                        $('#imgPopupThxNaver').hide();
                        $('#imgPopupThxDefault').show();
                        $('#email_result').show();
                    }
                } else {
                    alert(sResultText);
                    $(selector).focus();
                }

                document.getElementById("pcEmailOptin").disabled = false;
                return;
            },
            error: function (sResultText) {
                alert(sResultText);
                $(selector).focus();
                document.getElementById("pcEmailOptin").disabled = false;
                return;
            }
        });
    }
</script>

