    function setCookie(cookieName, cookieValue, expireDate) {
        var today = new Date();
        today.setDate(today.getDate() + parseInt(expireDate));
        document.cookie = cookieName + "=" + escape(cookieValue) + "; path=/; expires=" + today.toGMTString() + ";"
    }

    function deleteCookie(cookieName) {
        var expireDate = new Date();

        expireDate.setDate(expireDate.getDate() - 1);
        document.cookie = cookieName + "= ; expires=" + expireDate.toGMTString() + "; path=/";
    }

    function emailPopupVisible() {
        var ncPop = GetCookie('NCPOP');
        var isNaverOptPage = 'N';
        var isDefaultOptPage = 'Y';

        // 이전 URL에 네이버 트래커(cafe_mkt)포함인지
        if (isDefaultOptPage == "Y") {
            if (document.referrer.indexOf('nms_a') > -1 && document.referrer.toLowerCase().indexOf('product.aspx') > -1) {
                isNaverOptPage = 'Y';
                isDefaultOptPage = 'N';
            } else if (document.referrer.indexOf('ntbox_c01') > -1 && document.referrer.toLowerCase().indexOf('category.aspx') > -1) {
                isNaverOptPage = 'Y';
                isDefaultOptPage = 'N';
