ul#slider_1 {
    margin: 0;
    width: 100%;
    height: 100%;
    max-width: 820px;
    max-height: 375px;
    overflow: visible;
    padding: 0;
}

.slider_1 {
    width: 100%;
    height: 100%;
    max-width: 820px;
    max-height: 375px;
left: 0;}

.huge-it-wrap:after,
.huge-it-slider:after,
.huge-it-thumb-wrap:after,
.huge-it-arrows:after,
