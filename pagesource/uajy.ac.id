    position: absolute;
    top: 142.5px;;
    width: 820px;;
    z-index: 3;
}

.huge-it-wrap {
    opacity: 0;
    position: relative;
    border: 0px solid #ffffff;
    -webkit-border-radius: 0px;
    -moz-border-radius: 0px;
