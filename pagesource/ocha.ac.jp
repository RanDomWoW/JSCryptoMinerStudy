<script type="text/javascript">
$(window).load(function () {
	$.ajaxSetup({ cache: false });
	getdata();
	setInterval("getdata()", 60000);	//60秒
});

function getdata() {

	$.get("/save_energy/data/data-m.csv", function(myData){
		resultArray = myData.split(/\r\n|\r|\n/);

		var vEneGoal = parseFloat(resultArray[1]);
		var vEneUse  = parseFloat(resultArray[2]);
		var vEneDate = resultArray[3];
		var vEneTime = resultArray[4];
		var vEnePerc = Math.round(vEneUse / vEneGoal * 100);

		$("#eneDate").text(parseInt(vEneDate.substr(5, 2)) + "月" + parseInt(vEneDate.substr(8, 2)) + "日 " + vEneTime.substr(0, 5) + " 現在");

		$("#eneUse" ).text(Math.round(vEneUse ).toString().replace(/(\d)(?=(\d{3})+$)/g , '$1,') + "kw");
		$("#eneGoal").text(Math.round(vEneGoal).toString().replace(/(\d)(?=(\d{3})+$)/g , '$1,') + "kw");
		$("#enePerc").text(vEnePerc.toString() + "%");
		if (vEnePerc >= 100) {
			$("#enePerc").addClass("over");
		}else{
			$("#enePerc").removeClass("over");
		}
	});
}
</script>
