        <div class="top-arrow"></div>

        <div class="content">
            <div class="blockFormPopup search-panel">

                <div data-ng-repeat="room in roomsOccupancy" data-ng-class="$index == 0 ? 'room room1' : 'room room2'">
                    <div class="row">
                        <label>Seleziona numero adulti</label>
                        <div class="richInput">
                            <a data-ng-click="removeAdult(room)" class="modifier minus">-</a>
                            <span class="adults" data-ng-bind="room.adultsNumber"></span>
                            <a data-ng-click="addAdult(room)" class="modifier plus">+</a>
                        </div>
                    </div>
                    <div class="row">
                        <label>Seleziona numero Under18</label>
                        <div class="richInput">
                            <a data-ng-click="removeChildren(room)" class="modifier minus">-</a>
                            <span class="babies" data-ng-bind="room.childrenNumber"></span>
                            <a data-ng-click="addChildren(room)" class="modifier plus">+</a>
                        </div>
                    </div>

                    <div class="row baby-age" data-ng-show="room.childrenNumber > 0">
                        <label>Seleziona et&#224;</label>
                        <div class="column left">
                            <label>1&#176; Under18</label>
                            <select name="etaBambino1"
                                data-ng-model="room.child1age"
                                data-ng-options="age as (age + (age != 1 ? ' anni' : ' anno')) for age in availableChildrenAges"
                                data-ng-required="room.childrenNumber > 0"
                                data-ng-change="onAgeChange(room, $index)">
                                <option value="">--</option>
                            </select>
                        </div>

                        <div class="column center" data-ng-show="room.childrenNumber > 1">
                            <label>2&#176; Under18</label>
                            <select name="etaBambino2"
                                data-ng-model="room.child2age"
                                data-ng-options="age as (age + (age != 1 ? ' anni' : ' anno')) for age in availableChildrenAges"
                                data-ng-required="room.childrenNumber > 1"
                                data-ng-change="onAgeChange(room, $index)">
                                <option value="">--</option>
                            </select>
                        </div>
                    </div>

                    
                    <div data-ng-show="ShowAlertAgeMissing[$index] == true" class="row alert age-missing" data-ng-bind-html="'Selezionare et&amp;agrave; under18'">
                    </div>

                </div>

                <a class="confirm next-panel" data-ng-click="confirmWho()" data-ng-disabled="ShowAlertMaxPax || ShowAlertAgeMissing[0] == true || (ShowAlertAgeMissing.length == 2 && ShowAlertAgeMissing[1] == true)">
                    <span>Conferma</span>
                </a>
            </div>
        </div>
        <div class="ngdialog-close"></div>
    </div>
</div>

</script>

