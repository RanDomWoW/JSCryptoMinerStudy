                    }
                    return false;
                } else if (window.opera) {
                    a.href = cjHref;
                    a.title = cjTitle;
                    a.rel = 'sidebar'; // Opera 7+
                    return true;
                } else {    alert(hotKeys());    }
            } else {    throw "Error occured.\r\nNote, only A tagname is allowed!";    }
        }
        catch (err) {    alert(err);    }
    }


    (function(){

        function showClip($clip) {
            if($clip.css("top") == '-90px'){
                $clip.animate({ 'top': '0'}, 'fast');
            }
        }

        function hideClip($clip) {
            if( $clip.css("top") == '0px'){
                $clip.animate({ 'top': '-90px'}, 'fast');
            }
        }
        var $clip = $('.fp_clipTop');
        $(window).scroll( function() {
            ( $(window).scrollTop() ) > 233 ?  showClip($clip) : hideClip($clip);
        });

        function showName(me) { $(me).css({'z-index':4}).next().show(); }
        function hideName(me) { $(me).css({'z-index':2}).next().hide(); }

        $('.fp_artistFace').hover(
            function() { $(this).animate({ height: '+=22px' }, 150,  showName(this) );  }, //function(){}
            function() { $(this).animate({ height: '-=22px' }, 150, hideName(this) ); //function(){ }
        });
// css hover war..
        $('.fp_musical .fp_card, .fp_libruary .fp_card').hover(function(){
            $(this).addClass('hovered');
            $(this).find('.fp_footLine').show();
        }, function(){
            $(this).removeClass('hovered');
            $(this).find('.fp_footLine').hide();
        });
    })();
</script>
