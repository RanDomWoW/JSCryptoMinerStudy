<script id="TrackTmpl" type="text/template">
    <div class="tnum fl"><%= number %></div>
    <div class="tName fl"><%= title %></div>
    <div class="tAuthor fl"><%= artist %></div>
    <div class="tAlbum fl"><%= album %></div>
    <div class="tTime fl"><% if( length.split(':')[0]+0 > 0) {
                                length = (length.split(':')[0]+0) + length.substr(2); print(length);
                            } else {
                                length = length.substr(3).replace(/^0+/, '');
                                print( length );
                            } %></div>
    <div class="buttonz left <% if (playing) print('playing'); %>">
        <i class="butt playtrack" title="<% playing ? print('пауза') : print('играть') %>"><% playing ? print('U') : print('T') %></i>
    </div>
    <div class="buttonz right">
        <i class="butt fr ty tyfnt" title="благодарю"  who=".pay" data-object-id="<%= id %>" data-object-type="Track" <% if(price){print('style="font-size:18px"');}%>><% if(price){print(price);}%></i><!--<span class="tyfnt"><</span>-->
        <% if(TYGLOBAL.userId){ // если залогонен, показываем лайк.
                var className;
                if(likeInfo.count=='wait') {
                  className = 'LikeWait'
                }else{
                 if(likeInfo.selected) { className='selected'; }
                }
                print('<i class="butt fr TYLike '+className+ '" title="Добавить в любимые">');
                if( 'wait' != likeInfo.count ){ print('<i>u</i> '+likeInfo.count); };
                print('</i>');
            }
        %>

        <i class="butt fr subMenu" title="еще действия">X</i>
        <div class="fr shareTrack"></div>
    </div>
</script>
