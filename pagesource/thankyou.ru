.thPopUpGreyDock>.reg{ display: block;text-align: center; color: #fff;  width: 300px; height: 42px; border-radius: 5px; background-color: #3FA9F5; line-height: 42px; text-decoration: none; margin: auto; cursor: pointer;  }
.thPopUpGreyDock>.reg:hover{ background-color: #1498f6; }
.thPopUpGreyDock a.white{ color:#fff; }
/* Sub menu */
.subPopUp{ position: absolute;    left: 50%;    top: 50%;    z-index: 1000;    background: #fff; display: none; padding: 22px 0; width: 220px; font-size: 15px; color: #272727; font-family: 'PT Sans', sans-serif;border-radius: 10px; box-shadow: 0 0 15px 0 rgba(0, 0, 0, 0.3); -webkit-box-shadow:  0 0 15px 0 rgba(0, 0, 0, 0.3);  }
.subPopUp i{ font-family: thankyou_ru; font-size: 15px; margin-right: 5px; font-style: normal; font-weight: 800; }
.subPopUp i.truba{ font-size: 23px;}
.subEl{ line-height: 42px; padding-left: 31px; cursor: pointer;}
.subEl:hover{ background-color: #E6E6E6; }
/* notify */
#notify{ background-color: #000; opacity: 0.7; color: #fff; text-align: center; position: fixed; left: 50%; top: 50%; width: 660px; min-height: 23px; margin-left: -350px; padding: 20px; border-radius:7px; z-index:999999; display: none;  }
.closeNtf{ position: absolute; top: 10px; right: 10px; cursor: pointer; color: #888; }
.closeNtf:hover{ color: #fff;}
</style>
<!--субменю кнопки ...  -->
<script id="subMenuTempl" type="text/template">
<div class="subPopUp">
    <div class="subEl addTrack"><i>ҿ</i> Добавить...</div>
    <div class="subEl downloadTrack"><i>А</i> Скачать <% if(!isAllowDownload && price){ %>(<%= price %> ₶)<%}%></div>
    <% if(price) { %>
    <div class="subEl songGift" who=".pay" data-object-id="<%= id %>" data-object-type="Track" data-gift="true"><i>ҕ</i> Подарить (<%= price %> ₶) </div>
    <% } %>
    <div class="subEl createRingtone"><i class="truba">ѹ</i> Создать рингтон <% if( !isAllowedRingtone ){ %>(<%= price<=10?10:price %> ₶)<%}%></div>
    <div class="subEl shareTrack"><i>ҷ</i> Поделиться</div>
</div>
</script>
