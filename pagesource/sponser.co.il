    function getNotifycations(t) {
        var url = "http://www.sponser.co.il/Handlers/PushNotificationsGet.ashx?GT=" + t;
        if(window.location.href.toLowerCase().indexOf("finance.sponser.co.il")!=-1) url = "http://finance.sponser.co.il/api/PushNotificationsGet.php?GT=" + t;
        var cnt = 0;
        //$("#AlertsSquare" +t).css("visibility", "hidden");
        $("#AlertsSquare" +t).css("display", "none");
        $.ajax({
            type: "GET",
            contentType: "application/json",
            url: url,
            data: {},
            crossDomain: true,
            dataType: "json",
            success: function (data) {

                $.each(data.Pushes, function (i, obj) {
                    addRowPush(obj, false , t);
                    if (parseInt(obj.d) > parseInt(data.LastTimeClear)) {
                        cnt++;
                    }
                });
                $("#AlertsSquare" +t).html(cnt);

                if (cnt == 0)
                {
                    //$("#AlertsSquare" +t).css("visibility", "hidden");
                    $("#AlertsSquare" +t).css("display", "none");
                }
                else
                {
                    //$("#AlertsSquare"+t).css("visibility", "visible");
                    $("#AlertsSquare"+t).css("display", "block");
                }
            },
            error: function (result) {

             }
        });
    }


    function getMMDDYY(ticks) {
        if (isNaN(ticks)) return;
        ticks = parseInt(ticks);
        var date = new Date(ticks);
        var mm = date.getMonth() + 1;
        var dd = date.getDate();
        var yy = new String(date.getFullYear()).substring(2);
        var hh = date.getHours();
        var mn = date.getMinutes();
        if (mm < 10) mm = "0" + mm;
        if (dd < 10) dd = "0" + dd;
        if (mn < 10) mn = "0" + mn;
        return "" + dd + "." + mm + "." + yy + " " + hh + ":" + mn
    }


    function addRowPush(obj, isFirst , t) {



        var html = '<div class="Row"><a href="{u}" {tr}>' +
                                '<img src="http://www.sponser.co.il/Images/{i}" alt="{a}" border="0" />' +
                                '<div class="PushText">' +
                                '{m}' +
                                '<span style="display: block; padding: 0px; font-size: 11px; color: rgb(153, 153, 153);direction:ltr;width:100%;">{d}</span></div>' +
                            '</a></div>';

        var i = '';
        var a = '';
        var u = '';
        var tr = '';
        switch (obj.t) {
            case "1":
                i = 'notification-like.png';
                a = 'לייק חדש';
                u = 'http://www.sponser.co.il/ForumReadMsg.aspx?' + obj.u + '&from=notifications.alerts';
                break;
            case "2":
                i = 'notification-comment.png';
                a = 'תגובה חדשה';
                u = 'http://www.sponser.co.il/ForumReadMsg.aspx?' + obj.u + '&from=notifications.alerts';
                break;
            case "3":
                i = 'notification-portfolio.png';
                a = 'תיק אישי';
                u = 'http://finance.sponser.co.il/portfolio/?from=notifications.alerts';
                break;
            case "4":
                i = 'notification-alert.png';
                a = 'איתות';
                u = '#';
                break;
            case "5":
                i = 'notification-msg.png';
                a = 'הודעה';
                u = obj.u;
                tr = ' target="_blank" ';
                break;
            case "7":
                i = 'notification-watch.png';
                a = 'עוקב';
                u = 'http://www.sponser.co.il/ForumFollowManager.aspx?from=notifications.alerts';
                break;
           case "8":
                i = 'notification-comment.png';
                a = 'הודעה';
                u = 'http://www.sponser.co.il/ForumReadMsg.aspx?' + obj.u + '&from=notifications.follower';
                break;
        }
        var row = html.replace('{m}', obj.m).replace('{i}', i).replace('{a}', a).replace('{u}', u).replace('{tr}', tr).replace('{d}', getMMDDYY(obj.d));
        if (isFirst)
            $("#PushAlertContent" + t).prepend(row);
        else
            $("#PushAlertContent" + t).append(row);

        
    }
</script>


