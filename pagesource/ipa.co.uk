        // iphone + droid popup message for homescreen pinning
        $(function () {
            // window.navigator.standalone = app launch detection, not supported with android - see android comment below
            var IGNORE = true;
            if (!IGNORE && IPA_MOBILE.isMobileDevice() === true && window.navigator.standalone === false) { // mobile detection 
                // setup for message box
                var msgClose = $("<a href='#' class='pinning_message_close'>X</a>"),
                msgImage = $("<img src='/Framework/Images/pinning_icon.png' class='pinning_icon' />"),
                msg = $("<div />", {
                    id: "pinning_message_wrapper"
                }).append(msgClose)
                  .append(msgImage)
                  .appendTo(".mobile_wrapper");

                // user agent detection - different message to be shown for each OS
                var ua = navigator.userAgent.toLowerCase();
                
                if (ua.indexOf("apple") > -1) { // iphone detection
                    $("<p class='pinning_message' />").append("Install this web app on your phone: tap on the <img src='/Framework/Images/pinning_wrapper_clickme.png' /> icon below and then on 'Add to Home Screen'.")
                                                      .appendTo(msg);
                }
                // not included for android at the moment as hide and show features dont currently work on android
                // - this would mean either the message pops up every time a user views the site or a heavy cookie work around?
                //else if (ua.indexOf("android") > -1) { // android detection
                //    $("<p class='pinning_message' />").append("Install this web app on your phone: press the menu key and then look for 'Add shortcut to Home'.")
                //                                      .appendTo(msg);
                //}
                else {
                    msg.hide(); // hide popup
                }
                msg.append("<div class='clear'></div>");

                // adjust position of popup
                msg.css("top", ($(window).height() - (msg.height() + 80)) + "px");

                // close the popup
                msgClose.click(function () { msg.hide(); });
                $(window).scroll(function () { msg.hide(); });
            }
        });
