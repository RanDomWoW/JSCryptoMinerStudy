    $(document).ready(function () {
        
		$('.section_site').waypoint('sticky', {offset: 0});
		
        /* setup the more panel*/
        $('#panel_nav_more').html($('#more_actual').html());
        
        /* Populate native features */
        $('.feature_item_native').each(function () {
            $('.feature-class-' + $(this).data('feature-class')).html($(this).html());
        });

        /* Fixed header support */
        $('#navster_main').waypoint('sticky', {offset: -30});
        
        /* Setup nice panel soft display/cancelling */
        initSoftOpenClose();

        
        
        /* mobile menu show/hide */
        $('#mobileMenuSwitch').click(function () {
            $('#mobileMenu').toggle();
        });

        /* Search support */
        $('#search_button').on('click', function () {doSearch();});
        $('#search_input').keyup(function (e) {
            if (e.keyCode == 13) {
                doSearch();
            }
            return false;
        });

        // personalisatiob/logged in
        $('#person_icon').on('click', function () {
            if ($('#person_icon').data('loggedin') != 'True') {
                /*
                $('#loginWrapper, #registerIntroWrapper').show();
                $('#registerWizardWrapper, #overlayBlock').hide();
                $('#overlayLoginreg, #overlayBlock').fadeIn();
                */
                window.location.href = 'https://id.ipa.co.uk/connect/authorize?response_type=code&client_id=ipa.co.uk&redirect_uri=http://www.ipa.co.uk/identity/cb.aspx&scope=openid profile ipa_user_profile ipa_cpd_profile ipa_lms&state=af0ifjsldkj&nonce=n-0S6_WzA2Mj';
            }
        });

        // set selected item
        var selectedZoneFromBreadcrumb = $('#hdnSelectedZoneClass').val();
        $('.zone').each(function (index) {
            var zn = $(this).data('zone');
            if (zn != 'more') {
                if (zn == selectedZoneFromBreadcrumb) {
                    $(this).addClass('selected');
                    $('.overlay3').addClass(zn);
                }
            }
        });


        // split long ul's in to two
        // css will then present these are additional cols.
        // Rather than dynamically calculate split locations, we use *hard coded* id list.
        // This will need updating should nav items change and splits no longer corect.
        // Fastest option
        splitLongcols();

        // Now removed to its wn control for speed/cacheability
        // finally update breadcrumb if on parent page
        //var crumb = $('#navcrumbtool').html();
        //$('.breadcrumb').html(crumb);

    });


    function splitLongcols() {
        $('li .splitpoint').each(function (index) {
            // TODO - quirk with 3 cols. - will transpose them
            try {
                var $i = $(this);
                var ul = $i.closest('.coltwo').find('ul')[0];
                var copied = $i.nextAll().addBack().detach();

                var copyUl = $(ul).clone().empty().append(copied);
                if (copied.length > 0) {
                    $(ul).after(copyUl);
                }
            } catch (err) {
                //if (console) console.log(err);
            }
        });
        
    };


    function doSearch() {
        $('#search_input').val( $.trim($('#search_input').val().replace(":", " ").replace("(", " ").replace(")", " ")));
        var term = encodeURIComponent($('#search_input').val());
        var rgx_data = new RegExp('%20', 'gi');
        term = term.replace(rgx_data, '+');
        window.location.href = '/search/?term=' + term;
    };


