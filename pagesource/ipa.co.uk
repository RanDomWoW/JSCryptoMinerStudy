        function logout() {
            // make ajax call to logout user adn simply send them to home page
            jQuery.post("/Framework/Ajax/IpaAjax.ashx",
                    'method=Logout',
                    function (data) {
                    },
                    "text"
            ).always(function() {
                // enure coookis purged
                var removed = deleteAllCookies();
                if (removed < 1) document.cookie = data + '=; expires=Thu, 01-Jan-1970 00:00:01 GMT;';
                
                // now call off to IdP if switched on, otherwise redirect to home
                if ('true' == 'true' )
                    window.location.href = '';
                else 
                    window.location.href = 'http://www.ipa.co.uk';
            });
            return false;//?
            

        }

        function deleteAllCookies() {
            var cookies = document.cookie.split(";");

            for (var i = 0; i < cookies.length; i++) {
                var cookie = cookies[i];
                var eqPos = cookie.indexOf("=");
                var name = eqPos > -1 ? cookie.substr(0, eqPos) : cookie;
                document.cookie = name + "=;expires=Thu, 01 Jan 1970 00:00:00 GMT";
            }
            return i;
        }


