        var navsterSearchHideDelay;
        var navsterPersonMenuDelay;
        var navsterPersonMenuHideDelay;

        var menuItems = $('#nav_inner li.zone');

        $(".panel_nav").mouseenter(function () {
            clearTimeout(navsterPanelDelay);
            clearTimeout(navsterPanelHideDelay);
        });

