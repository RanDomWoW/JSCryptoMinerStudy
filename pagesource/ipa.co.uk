<script type="text/javascript">
    $(document).ready(function() {
        
        // now loads vids from server via ajax request in order not to prevent page from loading if issue occurs
        var req = {
            playlist: ''
        };
        $.ajax({
            async: true,
            data: JSON.stringify(req),
            type: "POST",
            url: "/framework/api/rest.ashx?method=youtube.feed.get&apikey=8bb4275c-e771-4a9e-9346-af56a342af4e",
            dataType: "json"
        }).done(function(data) {
            data = data.hasOwnProperty('d') ? data.d : data;
            LoadVids(JSON.stringify(data));
        }).fail(function() {
            // just hide the youtube section for now
            $('.section_ipatv').hide();
        });
        return false;
    });

    function LoadVids(data) {

        videoObjects = [];
        try {
        
            if (data !== '') {
                videoObjects = JSON.parse(data);
            }
 			var vidList = $('#div_ipa_video').html($('#videoTemplate').render(videoObjects)),
	            vids = vidList.find("figure"),
	            mobileItemCount = 0;
		} catch (err) {
            // dont attempt to render as issue wuth ie8
            // this is because its still in alpha development!
            // will update this when available
        }

        if (IPA_MOBILE.isMobileDisplayMode() != true) { // only cycle on desktop version
            $('#div_ipa_video').cycle({
                slides: 'figure',
                startingSlide: 0
            });
        }
        else
        {
            // how many to show in list for mobile display
            if(mobileItemCount > 0) 
                vids.slice(mobileItemCount, vids.length).remove();
        }

        // Try to set nice url slugs (only in standard mde)
        if ('False' != 'True' ) {
            $(".video_item").each(function() {
                var vidTitleAsSlug = convertToSlug($(this).attr('data-title'));
                var vidID = $(this).attr('data-id');
                var newUrl = '/tv/' + vidTitleAsSlug + '/' + vidID + '?vt=you';
                $(this).attr('href',newUrl);
            });
        }

        // set active slide (for nice highligt effect)
        $('.cycle-slide-active').addClass('active_video');

    };

    function convertToSlug(Text) {
        return Text
        .toLowerCase()
        .replace(/[^\w ]+/g, '')
        .replace(/ +/g, '-')
        ;
    };
</script>

