





<!doctype html>
<html lang="fa" ng-app="app" ng-controller="ctrl as self">
<head>
    

    
    <title>شبکه نو آوری تهران</title>


    <title>تینت</title>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="shortcut icon" href="/themeFiles/images/logo-final-5.png">

    <link rel="stylesheet" href="../../website_assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="../../website_assets/css/angular-csp.css">
    <link rel="stylesheet" href="../../website_assets/css/animate.css">
    <link rel="stylesheet" href="../../website_assets/css/loading-bar.min.css">
    <link rel="stylesheet" href="../../website_assets/css/bricklayer.min.css">
    <link rel="stylesheet" href="../../website_assets/css/font-awesome.css">
    <link rel="stylesheet" href="../../website_assets/css/fontiran.css">
    <link rel="stylesheet" href="../../website_assets/css/main.css">
    <link rel="stylesheet" href="../../website_assets/css/normalize.css">
</head>
<body ng-cloak ng-class="{'newsOnly-body': self.newsOnly_body}">

    <div id="preloader" class="backspinner" ng-show="preloader">
        <img class="to-loader-logo" src="/themeFiles/images/logo-final-5.png" alt="" />
        <div class="to-loader">
            <svg width="60px" height="60px" viewBox="0 0 80 80" xmlns="http://www.w3.org/2000/svg">
                <path class="to-loader-circlebg"
                      fill="none"
                      stroke="#dddddd"
                      stroke-width="4"
                      stroke-linecap="round"
                      d="M40,10C57.351,10,71,23.649,71,40.5S57.351,71,40.5,71 S10,57.351,10,40.5S23.649,10,40.5,10z" />
                <path id='to-loader-circle'
                      fill="none"
                      stroke="#0257ab"
                      stroke-width="4"
                      stroke-linecap="round"
                      stroke-dashoffset="192.61"
                      stroke-dasharray="192.61 192.61"
                      d="M40,10C57.351,10,71,23.649,71,40.5S57.351,71,40.5,71 S10,57.351,10,40.5S23.649,10,40.5,10z" />
            </svg>
        </div>
    </div>
    












<header class="index">
    <nav class="navbar navbar-expand-lg navbar-light justify-content-between">
        <a class="navbar-brand" href="../../">
            <div class="logo">
                <img src="../../website_assets/img/el/logotinet-01-01.svg">
            </div>
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="fa fa-navicon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav ml-auto text-right">
                <li class="nav-item" ng-class="nav.class" ng-repeat="nav in self.package.menu">
                    <a class="nav-link" href="{{nav.href}}">
                        {{nav.name}}
                    </a>

                </li>
        
            </ul>

            <div class="form-inline my-2 my-lg-0">
                <div class="dropdown">
                    <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenuButton"
                            data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        {{self.package.dropdown.text}}
                        <i class="fa fa-user-circle fa-2x" style="font-size: 2.5em;"></i>
                    </button>
                    <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                        <a class="dropdown-item text-right" ng-href="{{self.package.signup.href}}">
                            {{self.package.signup.name}}
                        </a>
                        <a class="dropdown-item text-right" ng-href="{{self.package.signin.href}}">
                            {{self.package.signin.name}}
                        </a>
                    </div>
                </div>
            </div>
        </div>


    </nav>

    <section class="container-fluid">
        <div class="row">
            <div class="col col-12 col-xl-6 col-lg-6 col-sm-12">
                <div class="about float-right">
                    <ul class="list-inline">
                        <li class="list-inline-item">
                            <a ng-href="{{self.package.socials.telegram}}">
                                <i class="fa fa-paper-plane fa-2x fa-size"></i>
                            </a>
                        </li>
                        <li class="list-inline-item">
                            <a ng-href="{{self.package.socials.gplus}}">
                                <i class="fa fa-google-plus fa-2x fa-size"></i>
                            </a>
                        </li>
                        <li class="list-inline-item" style="margin-right: 12px;">
                            <a ng-href="{{self.package.socials.instagram}}">
                                <i class="fa fa-instagram fa-2x fa-size"></i>
                            </a>
                        </li>
                    </ul>
                    <h3 class="text-right mt-3" ng-bind="self.package.about.title"></h3>
                    <p class="text-right mt-3" ng-bind="self.package.about.content"></p>

                    <a ng-href="{{self.package.signup.href}}"
                       class="btn btn-lg float-right btn-white">{{self.package.signup.name}}</a>
                    <a ng-href="{{self.package.signin.href}}"
                       class="btn btn-lg float-right btn-white">{{self.package.signin.name}}</a>
                </div>
            </div>
            <div class="col col-12 col-xl-6 col-lg-6 col-sm-12">
                <div class="star float-left">
                    <img src="../../website_assets/img/star.svg">
                    <small>
                        آشنایی با شبکه تینت
                        <i class="fa fa-angle-double-left"></i>
                    </small>
                </div>
                <div class="play" ng-href="{{self.package.video.href}}" data-toggle="modal" data-target="#video">
                    
                </div>
            </div>
        </div>
        <a class="scroll-down" href="#whoYouAre">
            <i class="fa fa-angle-double-down fa-3x"></i>
        </a>
    </section>

    <div class="modal fade" id="video" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
         aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div id="1520509872178327">
                <script type="text/JavaScript" src="https://www.aparat.com/embed/cXY1I?data[rnddiv]=1520509872178327&data[responsive]=yes"></script></div>
                        </div>
        </div>
    </div>

</header>



<main>
    <div class="container-fluid">
        <div class="clearfix mb-5" style="margin-bottom: 1rem!important;">
            <h1 class="flag-1 float-right" id="whoYouAre">
                {{self.package.whoYouAre}}
                <br>
                <small class="text-muted" ng-bind="self.package.whoYouAreDescription"></small>
            </h1>
        </div>

        <div class="container">
            <div class="row">
                <div class="col col-xl-4 col-md-6 col-12 text-center icons mt-md-5" style="margin-top: 0 !important;"
                     ng-repeat="icon in self.package.icons.partA" ng-mouseenter="icon.hover = true" ng-mouseleave="icon.hover = false">
                    <a href="{{icon.link}}">
                        <img ng-hide="icon.hover" ng-src="../../website_assets/img/svg/{{icon.image}}">
                        <img ng-show="icon.hover" ng-src="../../website_assets/img/svg/{{icon.imageHover}}">
                    </a>
                    <a href="{{icon.link}}" ng-bind="icon.title"></a>
                    <p ng-bind="icon.content"></p>
                </div>
                <div class="col col-xl-4 col-md-6 col-12 text-center icons mt-md-5"
                     ng-repeat="icon in self.package.icons.partB" ng-mouseenter="icon.hover = true" ng-mouseleave="icon.hover = false">
                    <a href="{{icon.link}}">
                        <img ng-hide="icon.hover" ng-src="../../website_assets/img/svg/{{icon.image}}">
                        <img ng-show="icon.hover" ng-src="../../website_assets/img/svg/{{icon.imageHover}}">
                    </a>
                    <a href="{{icon.link}}" ng-bind="icon.title"></a>
                    <p ng-bind="icon.content"></p>
                </div>
            </div>
        </div>
    </div>

    <div class="divider"></div>
    <section>
        <div class="container-fluid pt-xl-2">
            <div class="clearfix">
                <h1 class="flag-2 float-right" ng-bind="self.package.stayUpTodate"></h1>
                <div class="filters float-left">
                    <h5 class="text-left">فیلتر کنید:</h5>
                    <button class="btn mr-3 btnfilter" ng-click="self.filterType(filter.link)"
                            ng-class="filter.link === self.activebtn ? 'btn-primary': 'btn-secondary'"
                            ng-repeat="filter in self.package.filters | orderBy:$index">
                        {{filter.name}}
                    </button>
                </div>
            </div>
            <ul class="boxes boxes-index boxes-updated dynamic-grid" angular-grid="self.contents" grid-width="300" gutter-size="2" angular-grid-id="gallery" refresh-on-img-load="true">
                <li ng-repeat="content in self.contents" class="grid animated fadeIn" style="animation-delay: 0.2s;">
                    <div class="box box-in">
                        <label>{{content.InfoType | label}}</label>
                        <div class="content">
                            <a ng-if="content.InfoType == 'Media'" href="../../media/index?id={{content.Id}}" class="pointer">
                                <img ng-src="../..{{content.Logo}}" class="grid-img img-index" data-actual-width="100"  />
                            </a>
                            <a ng-if="content.InfoType == 'Event'" href="../../events/index?id={{content.Id}}" class="pointer">
                                <img ng-src="../..{{content.Logo}}" class="grid-img img-index EventImgGrid" data-actual-width="100" />
                            </a>
                            <a ng-if="content.InfoType == 'News'" href="../../news/index?id={{content.Id}}" class="pointer">
                                <img ng-src="../..{{content.Logo}}" class="grid-img img-index" data-actual-width="100" />
                            </a>
                            <!-- <img src="{{content.Logo}}" class="grid-img img-index" data-actual-width = "100"  data-actual-height="100" /> -->
                            <h6 ng-if="content.InfoType != 'Event'">{{content.Date | jalaliDate:'jDD jMMMM jYYYY'}}</h6>
                            <a ng-if="content.InfoType == 'Media'" href="../../media/index?id={{content.Id}}" class="text-center SimilarA pointer" ng-bind="content.Title"></a>
                            <a ng-if="content.InfoType == 'Event'" href="../../events/index?id={{content.Id}}" class="text-center SimilarA pointer" ng-bind="content.Title"></a>
                            <a ng-if="content.InfoType == 'News'" href="../../news/index?id={{content.Id}}" class="SimilarA SimNews pointer" ng-bind="content.Title"></a>
                            <p ng-if="content.InfoType == 'News'" ng-bind="content.Desc"></p>
                        </div>
                    </div>
                </li>
            </ul>
            <div class="loading-more-indicator-div" ng-show="self.loadingMore">
                <div class="loading-more-indicator"></div>
            </div>
        </div>
        <div class="text-center all" ng-if="self.page == 2">
            <a href="../../all/index">همه موارد</a>
        </div>
        <div class="text-center all" ng-if="self.page != 2">
            <a ng-href="#" ng-click="self.loadMoreShots()">بیشتر</a>
        </div>
    </section>
</main>



<footer id="foo" ng-class="{'footer-update': self.footer_update}">
    <section class="container-fluid">
        <div class="row">
            <div class="col col-12 col-xl-5 col-lg-6 col-md-8 col-sm-8 text-right">
                <h5 class="mr-5 mb-4" style="font-size: 1.2vw;">آخرین اعضاء</h5>
                <div class="members">
                    <div class="images" ng-class="{'active': group.active}" ng-repeat="group in self.members">
                        <div class="row">
                            <div class="col col-xl-3 col-lg-3 col-md-4 col-xs-12 col-3 text-center memberimg"
                                 ng-repeat="member in group.data">
                                <a target="_blank" ng-href="">
                                    <img class="animated flipInY"
                                         ng-src="http://demo.tinet.ir{{member.Logo}}" title="{{member.Title}}" alt="{{member.Title}}" />
                                </a>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
            <div class="col col-12 col-xl-7 col-lg-6 col-md-4 col-sm-4 pt-5" style="padding-top: 0 !important;">
                <div class="row">
                    <div class="col col-xl-4 col-lg-4 col-md-12 col-sm-12 col-6 text-right mt-md-5">
                        <h5 class="pr-4 title-footer">درباره شبکه نوآوری:</h5>
                        <ul>
                            <li class="li-footer" ng-repeat="item in self.package.aboutLinks">
                                <a ng-href="{{item.link}}">
                                    {{item.name}}
                                </a>
                            </li>
                        </ul>
                    </div>
                    <div class="col col-xl-4 col-lg-4 col-md-6 col-sm-12 col-6 text-right mt-md-5">
                        <h5 class="pr-4 title-footer">سامانه صدف:</h5>
                        <ul>
                            <li class="li-footer" ng-repeat="item in self.package.sadafLinks">
                                <a ng-href="{{item.link}}">
                                    {{item.name}}
                                </a>
                            </li>
                        </ul>
                    </div>
                    <div class="col col-xl-4 col-lg-4 col-md-12 col-sm-12 col-12 text-right mt-md-5">
                        <h5 class="pr-4 title-footer">فن بازار:</h5>
                        <ul>
                            <li class="li-footer" ng-repeat="item in self.package.bazarLinks">
                                <a ng-href="{{item.link}}">
                                    {{item.name}}
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </section>
</footer>





    <script src="../../website_assets/js/vendor/jquery-3.2.1.min.js"></script>
    <script src="../../website_assets/js/bootstrap.bundle.js"></script>
    <script src="../../website_assets/js/angular.min.js"></script>
    <script src="../../website_assets/js/loading-bar.min.js"></script>
    <script src="../../website_assets/js/bricklayer.min.js"></script>
    <script src="../../website_assets/js/angular-bricklayer.js"></script>
    <script src="../../website_assets/js/ng-infinite-scroll.min.js"></script>
    <script src="../../website_assets/js/angulargrid.js"></script>
    <script src="../../website_assets/js/moment/moment.js"></script>
    <script src="../../website_assets/js/moment/moment-jalaali.js"></script>


    

    
    <script src="../../website_assets/js/app/HomeController.js"></script>


</body>
</html>