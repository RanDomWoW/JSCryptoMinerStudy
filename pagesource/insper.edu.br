                <script>
                    var oldLogo = "";
                    function fontSize(){
                        if( jQuery("html").hasClass("font-size") ){
                            jQuery("html").removeClass("font-size");
                            jQuery("#acessibilidade #font-size").html('<span>Aumentar fonte</span>');
                            if( jQuery('#category #posts').length ){
                                jQuery('#posts').isotope({transformsEnabled:false});
                            }
                        } else {
                            jQuery("html").addClass("font-size");
                            jQuery("#acessibilidade #font-size").html('<span>Diminuir fonte</span>');
                            if( jQuery('#category #posts').length ){
                                jQuery('#posts').isotope({transformsEnabled:false});
                            }
                        }                    
                    }
                    
                    function contrast(){
                        if( jQuery("body").hasClass("contrast") ){
                            jQuery("body").removeClass("contrast");
                            jQuery("#acessibilidade #contrast").html("<span>Aumentar contraste</span>");
                            jQuery("#site-logo img").attr("src", oldLogo);
                        } else {
                            oldLogo = jQuery("#site-logo img").attr("src");
                            var newLogo = jQuery("#rodape img").attr("src");
                            jQuery("body").addClass("contrast");
                            jQuery("#acessibilidade #contrast").html("<span>Diminuir contraste</span>");
                            jQuery("#site-logo img").attr("src", newLogo);
                        }
                    }
                </script>
