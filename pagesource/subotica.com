                    data: formData,
                    dataType: 'json',
                    success: function (response) {
                        // console.log(response);

                        if (response.ok == '1') {
                            if (response.redirect_to_url) {
                                window.location.href = response.redirect_to_url;
                            }
                            else {
                                setTimeout(function() { document.location.reload(true); }, 3000);
                            }
                            $('.login-msg').addClass('alert-success').text('Uspešno prijavljivanje. Preusmeravanje...').slideDown(300);
                        }
                        else if (typeof response.ajax_message !== 'undefined' && response.ajax_message !== '') {
                            $('.login-msg').addClass('alert-info').html(response.ajax_message).slideDown(300);
                        }
                    },
                    error: function(x, s, e) {
                        // console.log(x);
                        // console.log(s);
                        // console.log(e);
                    },
                    complete: function() {
                        form.removeClass('disabled');
                    }
                });
            }
        });

        // resend activation
         $('body').on('click', '#resend-activation', function() {
            $('.login-msg .ajax-loader').fadeIn();
            var that = $(this);

            $.post(that.data('link'), {
                'user_id' : that.data('uid'),
                'hash' : that.data('hash'),
                'status' : that.data('status')
            }, function(response) {
                // console.log(response);
                $('.login-msg .ajax-loader').fadeOut();
                $('.login-msg').hide().html('').removeClass('alert-success alert-info alert-danger');
                $('.login-msg').addClass('alert-info').html(response.ajax_message).slideDown(300);

                handle_json_response(response);
            }, 'json');
        });
    });
