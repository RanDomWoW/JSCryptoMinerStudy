            <script type="text/javascript">
                jQuery( function($) {

                    // #predstojeca-desavanja-Carousel carousel
                    $('#predstojeca-desavanja-Carousel').carousel({
                        interval: 10000,
                        pause: 'hover',
                        keyboard: false,
                    });


                    // Gallery carousel
                    $('.date-slider').slick({
                        draggable: false,
                        infinite: false,
                        slidesToShow: 4,
                        dots: false,
                        mobileFirst: true,
                        responsive: [
                            {
                                breakpoint: 768,
                                settings: {
                                    slidesToShow: 8,
                                }
                            },
                            {
                                breakpoint: 440,
                                settings: {
                                    slidesToShow: 6,
                                }
                            },
                        ]
                    });
                    $('.date-slider .slick-active:eq(0)').addClass('selected');

                    $('.date-slider').on('beforeChange', function(event, slick, currentSlide, nextSlide) {
                        var newSlide,
                            selector = '.' + $(this).data('type'),
                            currentIndex = parseInt($('.date-slider' + selector + ' .timeline-date.selected').data('slick-index'));

                        if (currentSlide > nextSlide) {
                            newSlide = currentIndex - 1;
                        }
                        else {
                            newSlide = currentIndex + 1;
                        }

                        photoSlider(selector, newSlide);
                    });

                    $('.date-slider .timeline-date').on('click', function() {
                        var newSlide = $(this).data('slick-index');
                        var selector = '.' + $(this).closest('.date-slider').data('type');

                        photoSlider(selector, newSlide);
                    });

                    $('.photo-slider').slick({
                        draggable: false,
                        infinite: false,
                        slidesToShow: 2,
                        dots: false,
                        mobileFirst: true,
                        responsive: [
                            {
                                breakpoint: 768,
                                settings: {
                                    slidesToShow: 4,
                                }
                            },
                        ]
                    });
                });

                function photoSlider(cssClass, newSlide) {
                    $(cssClass + ' .timeline-date').removeClass('selected');
                    var newSelected = $(cssClass + ' .timeline-date[data-slick-index="' + newSlide + '"]');

                    var id = newSelected.addClass('selected').data('id');
                    // console.log(id);

                    var photo = $('.photo-slider .gallery-item[data-id="' + id + '"]');
                    if (photo.length > 0) {
                        var index = photo.data('slick-index');
                        // console.log(index);
                        var visibleSlides = parseInt($('.photo-slider' + cssClass).slick('slickGetOption', 'slidesToShow'));
                        var allSlides = parseInt($('.photo-slider' + cssClass + ' .gallery-item').length);
                        // console.log(allSlides + ' ' + visibleSlides + ' ' + index);
                        if (index > allSlides - visibleSlides) {
                            index = allSlides - visibleSlides;
                        }
                        // console.log('new: ' + index);
                        $('.photo-slider' + cssClass).slick('slickGoTo', index);
                    }
                }
            </script>        </main>

