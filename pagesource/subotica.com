<script type="text/javascript">
    jQuery( function($) {
        $('#ajax_login_form').on('submit', function(e) {
            e.preventDefault();

            var form = $(this);

            if (!form.hasClass('disabled')) {
                form.addClass('disabled');
                var formData = form.serializeArray();
