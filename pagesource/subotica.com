    <script type="text/javascript">
        $(function () {
            // $('#calendar-holder').datepicker();
            // $('#calendar-holder').on("changeDate", function () {
            //     $('#calendar-date').val(
            //         $('#calendar-holder').datepicker('getFormattedDate')
            //     );
            // });
            $('#calendar-holder')
                .datetimepicker({
                    locale: 'sr',
                    inline: true,
                    showTodayButton: true,
                    useCurrent: false,
                    format: 'YYYY-MM-DD',
                                                            maxDate: moment().format('YYYY-MM-DD'),
                })
                .on('dp.change', function(date) {
                    var link = window.location.href;
                                            var link = link.split('?');
                        link = link[0];
                        link = link.split('/');
                        if (link[link.length - 1] == '') {
                            link.pop();
                        }
                    
                    if (typeof link[link.length - 1] != undefined && link[link.length - 1].match(/p-\d+/) !== null) {
                        link.pop();
                    }
                    link = link.join('/') + '/';

                    var date = $(this).data('DateTimePicker').viewDate();
                    var monthName = date.format('MMM').replace(/\./g, '');
                    monthName = monthName.substr(0, 1).toUpperCase() + monthName.substr(1);
                    link += date.format('YYYY') + '/' + monthName + '/' + date.format('DD');
                    window.location.href = link;
                });
        });
    </script>
