	background:#1e3e52;
	color:#fff;
	width:10%;
	font-weight:bold;}
table.type07 td{
	width:45%;}
table.type07 td strong{
	border-bottom:1px solid #000;}
table.type07 td.good-td{
	background:#ebf0f3;}
table.type07 td.bad-td{
	background:#fbdcdc;}
table.type07 td.good-td h3,table.type07 td.bad-td h3{
