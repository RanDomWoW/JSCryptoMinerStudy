<script type="text/javascript">
    function setOnchange() {
        $$('.layer-search select').each(function(sel) {
            sel.setAttribute('onchange', 'setLayer(this.value)');
        });
        
        $$('#layersearch .currently li a').each(function(ael) {
            ael.observe('click', function(event) {
                Event.stop(event);
                var el = Event.element(event);
                layerParams = el.href.toQueryParams();
                setLayer('https://educationmalaysia.gov.my/');
            });
        });        
    }
    
    function setLayer(url) {
        //setLocation(''+url.substr(url.indexOf('?')));
    	var layerParams = {};
    	
    	$$('.layer-search select').each(function(sel) {
    		Object.extend(layerParams, sel.getValue().toQueryParams());
        });
        
    	$('layer_search_action').href = 'https://educationmalaysia.gov.my/courses-education-institutes.html/'+'?'+Object.toQueryString(layerParams);
        var options = {  
            parameters:   layerParams,
            onLoading:    function (transport) {
                              $('narrow-by-list').update('<img width="16" height="16" src="https://educationmalaysia.gov.my/skin/frontend/emgs/default/images/opc-ajax-loader.gif"/>');
                          },                            
            onSuccess:    function(transport) {
                              if (transport.responseText) {
                                  response = transport.responseText.replace(/\r?\n|\r/g, " ").replace( new RegExp( "\>[ ]+\<" , "g" ) , "><" );

                                  if(response.search('<dt>Institution Type</dt>') !== -1){
                                      updateInstitutionType = response.substring(response.indexOf('<dt>Institution Type</dt><dd><select onchange="setLocation(this.value)">')+71,response.indexOf('</select>', response.search('<dt>Institution Type</dt>')));
                                	  $('institution_type').update(updateInstitutionType.replace('Choose an Option...','Institution Type'));
                                  }
                                  
                                  if(response.search('<dt>Specialisation</dt>') !== -1){
                                      updateSpecialisation = response.substring(response.indexOf('<dt>Specialisation</dt><dd><select onchange="setLocation(this.value)">')+71,response.indexOf('</select>', response.search('<dt>Specialisation</dt>')));
                                	  $('specialisation').update(updateSpecialisation.replace('Choose an Option...','Specialisation'));
                                  }

                                  if(response.search('<dt>Level of Study</dt>') !== -1){
                                      updateSpecialisation = response.substring(response.indexOf('<dt>Level of Study</dt><dd><select onchange="setLocation(this.value)">')+71,response.indexOf('</select>', response.search('<dt>Level of Study</dt>')));
									  $('level_of_study').update(updateSpecialisation.replace('Choose an Option...','Level of Study'));
                                  }
                                  //$('layersearch').update(transport.responseText);
                                  setOnchange();
                              } else {
                                  $('narrow-by-list').update('error');
                                  //setLocation(url); 
                              }                                                   
                          }                 
        };
        new Ajax.Request('https://educationmalaysia.gov.my/emgs/layer/filter/', options);
    }
    
    setOnchange();
</script>
