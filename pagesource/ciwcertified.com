		}
		
		var changeClass = function(nextView) {
			tagline.className = "slide" + (parseInt(nextView) + 1);		
		}
		
		var fadeOutImg = function(view) {
			var lastView = (parseInt(view));
			if (parseInt(view) + 1 < mainImg.length) {
				nextView = parseInt(view) + 1;
			} else {
				nextView = 0;
			}
			
			var t3 = new TimelineLite();
			t3.to(mainImg[nextView], .1, {display: "block", onComplete:renderLargeImg});
			t3.to(tagline, 1, {opacity: 0, ease:Power3.easeIn, onComplete:changeClass, onCompleteParams:[nextView]}, "+=300");
			t3.to(mainImg[lastView], 3, {opacity: 0, zIndex: 89, ease:Power3.easeIn});
			t3.to(mainImg[nextView], 3, {opacity: 1, zIndex: 90, ease:Power3.easeOut}, "-=3");
			t3.to(tagline, 2, {opacity: 1, ease:Power3.easeIn}, "-=.5");
			t3.to(mainImg[lastView], .1, {display: "none", onComplete:fadeOutImg, onCompleteParams:[nextView]});
			t3.play();
		}
				//pick a random number(slide)
		var startNum = random(1,5);		//assign the correct tagline to the class		tagline.className = "slide" + startNum;		//make the correct slide visible		for (i = 0; i < mainImg.length; i++){			if (i == startNum - 1){				var t4 = new TimelineLite();				t4.to(mainImg[i], .1, {display: "block", zIndex: 90, onComplete:renderLargeImg});				t4.to(mainImg[i], 1, {opacity: 1, ease:Power3.easeOut});				t4.to(tagline, 1, {opacity: 1}, "-=.1");				t4.play();			} else {				mainImg[i].style.display = "none";				mainImg[i].style.opacity = "0";				mainImg[i].style.zIndex = "89";			}		}		fadeOutImg(startNum - 1);
	  </script>
	  
