		//create the timeline with an onComplete callback that calls myFunction() when the timeline completes
		var jobsBox = document.getElementById("home_industry_jobs");
		var jobs = jobsBox.getElementsByTagName("a");
		var initialLoad = true;
		
		var jobViews = Math.floor(jobs.length / 3);
		var lastIndexJobs;
		
		var isHidden2 = function (element) {
			return (element.offsetParent === null);
		};
		
		var renderImages2 = function(){
			if (!isHidden2(jobsBox)) {
				echo.render();
			}
		}
		
		var fadeOutJobs = function(view) {
			var startItem = (parseInt(view)) * 3;
			if (parseInt(view) < jobViews) {
				nextView = parseInt(view) + 1;
			} else {
				nextView = 1;
				view = 0;
				/*var t1 = new TimelineLite();
				t1.to(jobs[0], 2, {opacity: 1, ease:Power3.easeIn});
				t1.to(jobs[1], 2, {opacity: 1, ease:Power3.easeIn}, "-=2");
				t1.to(jobs[2], 2, {opacity: 1, onComplete:fadeOutJobs, onCompleteParams:[nextView], ease:Power3.easeIn}, "-=2");
				t1.play();*/
			}
			
			//first items on first page load
			if (parseInt(view) == 0 && initialLoad) {
				var t2 = new TimelineLite();
				t2.to(jobs[0], 1, {opacity: 0, ease:Power3.easeOut});
				t2.to(jobs[1], 1, {opacity: 0, ease:Power3.easeOut}, "-=1");
				t2.to(jobs[2], 1, {opacity: 0, ease:Power3.easeOut}, "-=1");
				t2.to(jobs[0], .1, {display: "none", ease:Power3.easeOut});
				t2.to(jobs[1], .1, {display: "none", ease:Power3.easeOut}, "-=.1");
				t2.to(jobs[2], .1, {display: "none", onComplete:fadeOutJobs, onCompleteParams:[nextView], ease:Power3.easeOut}, "-=.1");
				t2.play();
				initialLoad = false;
			//first items on repeat
			} else if (parseInt(view) == 0 && !initialLoad) {
				var t2a = new TimelineLite();
				t2a.to(jobs[0], .1, {display: "inline-block", ease:Power3.easeIn});
				t2a.to(jobs[1], .1, {display: "inline-block", ease:Power3.easeIn}, "-=.1");
				t2a.to(jobs[2], .1, {display: "inline-block", onComplete:renderImages2, ease:Power3.easeIn}, "-=.1");
				t2a.to(jobs[0], 1, {opacity: 1, ease:Power3.easeIn});
				t2a.to(jobs[1], 1, {opacity: 1, ease:Power3.easeIn}, "-=1");
				t2a.to(jobs[2], 1, {opacity: 1, ease:Power3.easeIn}, "-=1");
				t2a.to(jobs[0], 1, {opacity: 0, ease:Power3.easeOut}, "+=10");
				t2a.to(jobs[1], 1, {opacity: 0, ease:Power3.easeOut}, "-=1");
				t2a.to(jobs[2], 1, {opacity: 0, ease:Power3.easeOut}, "-=1");
				t2a.to(jobs[0], .1, {display: "none", ease:Power3.easeOut});
				t2a.to(jobs[1], .1, {display: "none", ease:Power3.easeOut}, "-=.1");
				t2a.to(jobs[2], .1, {display: "none", onComplete:fadeOutJobs, onCompleteParams:[nextView], ease:Power3.easeOut}, "-=.1");
				t2a.play();
			//all other items
			} else {
				var t3 = new TimelineLite();
				t3.to(jobs[startItem], .1, {display: "inline-block", ease:Power3.easeIn});
				if (startItem + 1 != undefined) {
					t3.to(jobs[startItem + 1], .1, {display: "inline-block", ease:Power3.easeIn}, "-=.1");
				}
				if (startItem + 2 != undefined) {
					t3.to(jobs[startItem + 2], .1, {display: "inline-block", onComplete:renderImages2, ease:Power3.easeIn}, "-=.1");
				}
				
				t3.to(jobs[startItem], 1, {opacity: 1, ease:Power3.easeIn});
				if (startItem + 1 != undefined) {
					t3.to(jobs[startItem + 1], 1, {opacity: 1, ease:Power3.easeIn}, "-=1");
				}
				if (startItem + 3 != undefined) {
					t3.to(jobs[startItem + 2], 1, {opacity: 1, ease:Power3.easeIn}, "-=1");
				}
				
				t3.to(jobs[startItem], 1, {opacity: 0, ease:Power3.easeOut}, "+=10");
				if (startItem + 1 != undefined) {
					t3.to(jobs[startItem + 1], 1, {opacity: 0, ease:Power3.easeOut}, "-=1");
				}
				if (startItem + 2 != undefined) {
					t3.to(jobs[startItem + 2], 1, {opacity: 0, ease:Power3.easeOut}, "-=1");
				}
				
				t3.to(jobs[startItem], .1, {display: "none", ease:Power3.easeOut});
				if (startItem + 1 != undefined) {
					t3.to(jobs[startItem + 1], .1, {display: "none", ease:Power3.easeOut}, "-=.1");
				}
				if (startItem + 2 != undefined) {
					t3.to(jobs[startItem + 2], .1, {display: "none", onComplete:fadeOutJobs, onCompleteParams:[nextView], ease:Power3.easeOut}, "-=.1");
				}
				t3.play();
			}
		}
		
		if(window.innerWidth > 1000){
			setTimeout(function(){
				fadeOutJobs(0);
				echo.render();
			}, 10000);
		}
	  </script>	  
