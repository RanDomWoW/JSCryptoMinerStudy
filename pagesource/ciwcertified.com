	  <script type="text/javascript">
		var custBox = document.getElementById("home_schools");
		var cust = custBox.getElementsByTagName("div");
		
		var custViews = Math.ceil(cust.length / 5) - 1;
		var lastIndexCust;
		
		var isHidden = function (element) {
			return (element.offsetParent === null);
		};
		
		var renderImages = function(){
			if (!isHidden(custBox)) {
				echo.render();
			}
		}
		
		var fadeOutCust = function(view) {
			var lastView = (parseInt(view)) * 5;
			if (parseInt(view) + 1 <= custViews) {
				nextView = parseInt(view) + 1;
			} else {
				nextView = 1;
				view = 0;
			}
			
			if (parseInt(view) == 0) {
				var t2 = new TimelineLite();
				t2.to(cust[0], 1, {opacity: 0, ease:Power3.easeOut});
				t2.to(cust[1], 1, {opacity: 0, ease:Power3.easeOut}, "-=1");
				t2.to(cust[2], 1, {opacity: 0, ease:Power3.easeOut}, "-=1");
				t2.to(cust[3], 1, {opacity: 0, ease:Power3.easeOut}, "-=1");
				t2.to(cust[4], 1, {opacity: 0, ease:Power3.easeOut}, "-=1");
				t2.to(cust[0], .1, {display: "none", ease:Power3.easeOut});
				t2.to(cust[1], .1, {display: "none", ease:Power3.easeOut}, "-=.1");
				t2.to(cust[2], .1, {display: "none", ease:Power3.easeOut}, "-=.1");
				t2.to(cust[3], .1, {display: "none", ease:Power3.easeOut}, "-=.1");
				t2.to(cust[4], .1, {display: "none", onComplete:fadeOutCust, onCompleteParams:[nextView], ease:Power3.easeOut}, "-=.1");
				t2.play();
			} else {
				var t3 = new TimelineLite();
				t3.to(cust[lastView + 1], .1, {display: "inline-block", ease:Power3.easeIn});
				if (lastView + 2 != undefined) {
					t3.to(cust[lastView + 2], .1, {display: "inline-block", ease:Power3.easeIn}, "-=.1");
				}
				if (lastView + 3 != undefined) {
					t3.to(cust[lastView + 3], .1, {display: "inline-block", ease:Power3.easeIn}, "-=.1");
				}
				if (lastView + 4 != undefined) {
					t3.to(cust[lastView + 4], .1, {display: "inline-block", ease:Power3.easeIn}, "-=.1");
				}
				if (lastView + 5 != undefined) {
					t3.to(cust[lastView + 5], .1, {display: "inline-block", onComplete:renderImages, ease:Power3.easeIn}, "-=.1");
				}
				
				t3.to(cust[lastView + 1], 1, {opacity: 1, ease:Power3.easeIn});
				if (lastView + 2 != undefined) {
					t3.to(cust[lastView + 2], 1, {opacity: 1, ease:Power3.easeIn}, "-=1");
				}
				if (lastView + 3 != undefined) {
					t3.to(cust[lastView + 3], 1, {opacity: 1, ease:Power3.easeIn}, "-=1");
				}
				if (lastView + 4 != undefined) {
					t3.to(cust[lastView + 4], 1, {opacity: 1, ease:Power3.easeIn}, "-=1");
				}
				if (lastView + 5 != undefined) {
					t3.to(cust[lastView + 5], 1, {opacity: 1, ease:Power3.easeIn}, "-=1");
				}
				
				t3.to(cust[lastView + 1], 1, {opacity: 0, ease:Power3.easeOut}, "+=8");
				if (lastView + 2 != undefined) {
					t3.to(cust[lastView + 2], 1, {opacity: 0, ease:Power3.easeOut}, "-=1");
				}
				if (lastView + 3 != undefined) {
					t3.to(cust[lastView + 3], 1, {opacity: 0, ease:Power3.easeOut}, "-=1");
				}
				if (lastView + 4 != undefined) {
					t3.to(cust[lastView + 4], 1, {opacity: 0, ease:Power3.easeOut}, "-=1");
				}
				if (lastView + 5 != undefined) {
					t3.to(cust[lastView + 5], 1, {opacity: 0, ease:Power3.easeOut}, "-=1");
				}
				
				t3.to(cust[lastView + 1], .1, {display: "none", ease:Power3.easeOut});
				if (lastView + 2 != undefined) {
					t3.to(cust[lastView + 2], .1, {display: "none", ease:Power3.easeOut}, "-=.1");
				}
				if (lastView + 3 != undefined) {
					t3.to(cust[lastView + 3], .1, {display: "none", ease:Power3.easeOut}, "-=.1");
				}
				if (lastView + 4 != undefined) {
					t3.to(cust[lastView + 4], .1, {display: "none", ease:Power3.easeOut}, "-=.1");
				}
				if (lastView + 5 != undefined) {
					t3.to(cust[lastView + 5], .1, {display: "none", onComplete:fadeOutCust, onCompleteParams:[nextView], ease:Power3.easeOut}, "-=.1");
				}

				t3.play();
			}
		}
		
