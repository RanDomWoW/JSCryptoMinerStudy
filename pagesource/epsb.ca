	<!-- Website homepage events: share.epsb.ca_tekt9nvqqbu3obvhgav41r8qvg@group.calendar.google.com -->
	
	<script>
	var calendar_url = "https://www.googleapis.com/calendar/v3/calendars/share.epsb.ca_tekt9nvqqbu3obvhgav41r8qvg@group.calendar.google.com/events"
	var key = "AIzaSyDd-mj8dHW9AoEFqiByaFs9COsb3qpFqIc"; //api key
	var alt = "json";//json-in-script
	var orderby = "startTime";
	var maxresults = 30;  //max-results
	var singleevents = false; //Indicates whether recurring events should be expanded or represented as a single event.
	var d = new Date();
	//calculate year range (Aug-June) < 7
        //Modified Aug-July
	if (d.getMonth() < 6)  {
		startYear = d.getFullYear()-1;
		endYear = d.getFullYear();
	} else {
		startYear = d.getFullYear();
		endYear = d.getFullYear()+1;
	}	
	//var startMin = (moment(startYear+"-08-01").format()); //start-min
        var startMin = moment().subtract(1, 'days').format(); 
	var startMax = (moment(endYear+"-07-31").format()); //start-max

	var calendar_json_url  = calendar_url + "?alt="+alt+"&orderby="+orderby+"&maxResults="+maxresults+"&singleEvents="+singleevents+"&timeMin="+startMin+"&timeMax="+startMax+"&fields=items(summary,description,start,end,htmlLink)" + "&key="+ key + "&ie=" +(new Date()).getTime();
	
	$(document).ready(function() {
		// events
		$('#upcoming-events').getCalendarEvents({
			jsonURL: calendar_json_url,
			showPerPage: 3
		});
	});

	(function($) {
		$.fn.getCalendarEvents = function(options){
		var defaults = {
			jsonURL: '',
			showPerPage: 3
		};
		var options = $.extend(defaults, options);
		var container = $(this);
		$.ajax({  url: calendar_json_url, dataType: 'jsonp',success: function(data){
			//console.log(data);
			data.items.sort(function(a, b) {
				if ("date" in a.start)
					a = new Date(a.start["date"]);
				else
					a = new Date(a.start["dateTime"]);
				
				if ("date" in b.start)
					b = new Date(b.start["date"]);
				 else
					b = new Date(b.start["dateTime"]);
			    return a<b ? -1 : a>b ? 1 : 0;
			});		
			var x = 1;
                        var numberOfItems  = $.each(data.items, function(i, item){
				if ("date" in item.start)
					var startDate = moment(item.start["date"]);
				else
					var startDate = moment(item.start["dateTime"]);				
				var startDay = startDate.format("DD");
				var calStartDate = startDate.format("DD/MM/YYYY");
				var title = item.summary;
				var url = item.htmlLink; //link to google
				var teaser = $.trim(item.description) + "<br>";
				var fullLink = '<li><p class="green bold date">' + calStartDate + '</p><p style="position: absolute; left: 0; border: 4px solid #edebeb; width: 50px; height: 38px; font-weight: bold; font-size: 1.2em; vertical-align: middle; line-height: 38px; text-align: center;">' + startDay + '</p><p><a class="bold blue ucase" href="' + url + '" + id="showevent"  >' + title + '</a> <span class="blue">&raquo;</span><br /><span class="teaser">' + teaser + '</span></p></li>';
                // event location - if not null, surround with parens
                var event_loc = item.location;
                if(event_loc!="") {
	                event_loc = " (" + event_loc + ")";
	            };
				$('#events').append(fullLink); //works in ie
				var eventDetail = '<div id="event_' + x + '" style="display:none;">' + title + '<br>' +  teaser + '</div>';
				$('#eventdetails').append(eventDetail);
				x++;			}).length//jQuery.each
						
			//pagination
			var numberOfPages = Math.ceil(numberOfItems / options.showPerPage);
			//console.log(numberOfPages);
			var currentPage = 0;
	
			$("<a href='#' class='pager-prev'>&lt;</a>").click(previous).appendTo($(".page_navigation", container));
			while (numberOfPages > currentPage) {
				$("<a href='#' class='page_link'>&nbsp;</a>").text(++currentPage).click(goToPage).appendTo($(".page_navigation", container));
			}
			$("<a href='#' class='pager-next'>&gt;</a>").click(next).appendTo($(".page_navigation", container));
			$('.page_navigation .page_link:first', container).addClass('pager-current');
            $('.content', container).children(':nth-child(' + options.showPerPage + 'n+1)').addClass('no-border-top');
			$('.content', container).children().hide().slice(0, options.showPerPage).show();
			}
		});	

		
		//*************** PAGINATION FUNCTIONS **********************

		function previous(e) {
			e.preventDefault();
			$(this).parent().find('.pager-current').prev(".page_link").click();
		}

		function next(e) {
			e.preventDefault();
			$(this).parent().find('.pager-current').next(".page_link").click();
		}

		function goToPage(e) {
			e.preventDefault();
			
			//Get the zero-based index instead of using an attribute
			var page_num = $(this).index() - 1;

			//get the number of items shown per page
			var show_per_page = options.showPerPage;

			//get the element number where to start the slice from
			start_from = page_num * show_per_page;

			//get the element number where to end the slice
			end_on = start_from + show_per_page;

			//hide all children elements of content div, get specific items and show them
			$('.content', container).children().hide().slice(start_from, end_on).show();

			//Since this always points to the page link, use that instead of looking for it
			$(this).addClass("pager-current").siblings(".pager-current").removeClass("pager-current");
			
			$('.content', container).attr('start', start_from+1);
				
		}//goToPage
		
	}
	})(jQuery);
	</script>
