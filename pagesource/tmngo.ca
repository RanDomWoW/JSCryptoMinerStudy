					<script language=javascript>
	(function(){
		var urls = ['/activate', '/about\/contact-us'];
		var urlsRegExp = new RegExp(urls.join('|'), 'i');
		
		var devices = {
				'ios': ['iPhone', 'iPod', 'iPad'],
				'android': ['Android'],
				'windows_mobile': ['IEMobile'],
				'blackberry': ['BlackBerry', 'bb\\d+']
		};
		var devicesRegExp = '';

		var setCookie = function(name, value){
			var days = 90;
			var date = new Date();
      date.setTime(date.getTime()+(days*24*60*60*1000));
            
			document.cookie = [
				encodeURIComponent(name) + "=" + encodeURIComponent(value || ""),
				"expires="+date.toGMTString(),
				"path=/"
			].join("; ");
		};

		window.isMobileDevice = false;
		for(var prop in devices){
			devicesRegExp = new RegExp(devices[prop].join('|'), 'i');
			if (navigator.userAgent.match(devicesRegExp)) {
				window.isMobileDevice = true;
				window.mobileDeviceType = prop;
				/**
				Some device can switch their device mode between mobile and desktop
				We do not register device when the device is mobile.
				Switching device mode causes 'popping up device dialogue'.
				If someone played with swithcing mode, then we force them to sign out
				in authentication.js using this cookie. 
				**/
				setCookie('wasMobileDevice', true);
			}
		}

		if(window.isMobileDevice == true){
			
			
if (!document.URL.match(urlsRegExp)){
       var curr_url = window.location.href;
       if(curr_url.indexOf('?free=1') != -1) {
       location.replace("/activate?free=1");
        }
        else {
	location.replace("/activate");
        }
}
			
		}
		
		
		
	})();
	
</script>

        
