<script type="text/javascript">
	$(function(){
		var i=0;
		var t;
		var bg_color;
		/*anniu*/
		$("#ban_btn").find("li").eq("0").addClass("ban_btn_select");
		/*给middle颜色*/
		/*bg_color=$(".ban").eq(i).attr("bg_color");
		$("#banner").css("background",bg_color);*/
		/*第一次先将第一个图给显示出来*/
		/*$(".ban").eq(i).fadeIn("slow");*/
		$(".ban").eq(i).find(".ban_name").animate({
			opacity: 'show',
			left:'+0px'
		},1000);
		$(".ban").eq(i).find(".ban_pic").animate({
			opacity: 'show',
			right:'+0px'
		},1000);
		/*轮切图按钮*/
		$("#ban_btn").find("li").mouseover(function(){
			var anniu=$(this).index();
			if(i==anniu){
				return;
			}
			/*让三个动画都停止*/
			$(".ban").eq(i).find(".ban_name").stop(true,true);
			$(".ban").eq(i).find(".ban_pic").stop(true,true);
			$(".ban").eq(i).stop(true,true);
			i=anniu;
			qiehuan();
			clearInterval(t);
			t=setInterval(ban_zidong,5000);
		});
		/*点击图片跳转*/
		$("#banner").click(function(){
            if(i==0){
				window.location.href="html/product/usrp_b210.html";
			}
			if(i==1){
				window.location.href="html/product/usrp_n210.html";
			}
			if(i==2){
				window.location.href="html/product/usrp_x310.html";
			}
			if(i==3){
				window.location.href="html/product/usrp1.html";
			}
		});
		
		function ban_zidong(){
			i++;
			var len=$(".ban").length;
			if(i==len){i=0;}
			qiehuan();
		};
		
		function qiehuan(){
			/*让图片回到原点，并隐藏*/
			$(".ban").find("div").removeAttr("style").hide();
			/*anniu*/
			$("#ban_btn").find("li").eq(i).addClass("ban_btn_select").siblings().removeClass("ban_btn_select");
			/*middle颜色切换*/
			/*bg_color=$(".ban").eq(i).attr("bg_color");
			$("#banner").css("background",bg_color);*/
			/*让背景图轮切*/
			$(".ban").eq(i).fadeIn("slow").siblings(".ban").hide();
			/*将需要显示的图片移动*/
			$(".ban").eq(i).find(".ban_name").animate({
				opacity: 'show',
				left:'+0px'
			},1000);
			$(".ban").eq(i).find(".ban_pic").animate({
				opacity: 'show',
				right:'+0px'
			},1000);
		};
		
		t=setInterval(ban_zidong,5000);
	});
</script>
