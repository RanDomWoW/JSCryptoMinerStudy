<script id="results-list-template" type="text/x-tmpl">
    <ul>
    {{#each results}}
        <li>
            <section class="search-result {{#if this.sitecode}}{{this.sitecode}}{{/if}}" {{#if this.id}} data-listing-id='{{this.id}}' {{/if}}>
                <div class="result-wrap">
                    <a href='{{this.url}}' class='listing-link' rel='nofollow' target='_blank'>
                        <div class="image" style="background-image: url('{{this.img}}');"></div>
                    </a>
                    <div class="description-wrap">
                        <h2>
                            <a href='{{this.url}}' class='listing-link' rel='nofollow' target='_blank'>{{this.title}}
                                {{#if this.ll_hyundai_cpo}}
                                    <img class='hyundai-cpo' src='{{../hyundai_cpo_img}}'>
                                {{/if}}
                            </a>
                        </h2>

                        <div class="price-wrap">
                            {{#if this.price}}
                                <div class='price'>{{this.price}}</div>
                            {{/if}}
                            {{#if this.current_bid}}
                                <span class='current-bid'>Current Bid: <span class='current-price'>${{this.current_bid}}</span></span>
                            {{/if}}
                            {{#if this.misc}}
                                <div class='misc'>
                                    <span class='bids'>Bids: {{this.bids}}</span>
                                    {{#if this.time_left}}
                                        <span class='time-left {{#if this.urg}}{{{this.urg}}}-urgent{{/if}}'>Time Left: {{this.time_left}}</span>
                                    {{/if}}
                                </div>
                            {{/if}}
                        </div>

                        <div class="info-wrap">
                            {{#if this.mileage}}
                                <span class="info mileage"><i class="icon-gauge"></i>{{this.mileage}} mi.</span>
                            {{/if}}
                            {{#if this.date}}
                                <span class="info date"><i class="icon-clock"></i>{{this.date}}</span>
                            {{/if}}
                            {{#if this.location}}
                                <span class="info location {{#if this.distance}}has-distance{{/if}}"><i class="icon-direction"></i><span class="location-info-wrap">{{this.location}}{{#if this.distance}}<span class="distance">({{round this.distance}} mi. from {{getUserLocation}})</span>{{/if}}</span></span>
                            {{/if}}
                        </div>

                        {{#if this.details}}
                            {{!--
                                Note the use of triple {}, thus our code must
                                sanitize all html tags in
                                this.detailsShort/this.detailsMid/this.details
                            --}}
                            <div class="details"><span class="short">{{{this.detailsShort}}}</span><span class="mid">{{{this.detailsMid}}}</span><span class="long">{{{this.detailsLong}}}</span>... <span class="more"><a href='{{this.url}}' class='listing-link' rel='nofollow' target='_blank'>Read More</a></span></div>
                        {{/if}}

                        <div class='inline-tools'>
                            <a href='{{this.url}}' rel='nofollow' target='_blank' class='full-listing listing-link'><i class='icon-link-ext-alt'></i>Open {{{this.link_site}}} Listing</a>
                            <div class='tools'>
                                <div class='vehicle-history inline-tool' {{#if this.vin}} data-vin='{{this.vin}}' {{/if}}>
                                    <i class='icon-ok-circled'></i><span>Vehicle History Report</span>
                                </div>
                                <div class='ship-quote inline-tool' {{#if this.distance}} data-distance='{{this.distance}}'{{/if}}>
                                    <i class='icon-truck'></i><span>Shipping Quotes</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </li>
    {{/each}}
</ul>
</script>
<script id="sh-lists-index-template" type="text/x-tmpl">
    {{#each sections}}
    {{#with (lookup ../results this)}}
        <div class="col span_6_of_12">
            <section class='{{../this}}-searches searches-list widget'>
                <div class="widget-content">
                    <h3 class="sh-title">{{this.name}} Searches</h3>
                    <ul id='{{../this}}-searches'>{{{this.html}}}</ul>
                    <div class='buttons'>
                        <img class='loader hide' src='//static.autotempest.com/www/bundles/app/static/img/loaders/loader.gif' alt='Loading' />
                        <button class='more-searches'>Load More</button>
                        <button class='clear-searches hide'><i class='icon-{{this.icon}}'></i>{{this.button}}</button>
                    </div>
                </div>
            </section>
        </div>
    {{/with}}
{{/each}}
</script>
<script id="sh-items-template" type="text/x-tmpl">
    {{#each searches}}
    <li class='sh-item-container'>
        <section class='saved-search' data-searchnum='{{this.searchnum}}'>
            <a href="{{this.run_url}}">
                {{!-- this.title is unescaped as the title is already escaped by the backend, and in particular we want to see the quotes when a keyword search is rendered --}}
                <h2>{{#if this.current}}<span class='current'>Current: </span>{{/if}}{{{this.title}}}</h2>
                <span class='details'><h3>Details:</h3> {{this.details}}.</span>
            </a>
            <div class="controls">
                <button class='{{this.class}}'><i class='icon-star'></i><span>{{this.save_text}}</span></button>
                <a class='search' href='{{this.run_url}}'><i class='icon-search'></i>Search</a>
                <button class="edit"><i class='icon-pencil'></i>Edit</button>
            </div>
        </section>
    </li>
{{/each}}
</script>
