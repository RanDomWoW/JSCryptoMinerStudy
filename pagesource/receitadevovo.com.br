			enviar_formulario_cadastrar(form,tipo_formulario);
	}
	function entrar_perfil(form){
		var email,senha,login;

		email = document.getElementById('usuarios_email_entrar').value;
		senha = document.getElementById('usuarios_senha_entrar').value;
		if($('#lembrar_login').attr('checked')=='checked')
			login = 1;
		else
			login = 0

		if(email.length>0 && senha.length>0){
			$(form).find('button').html('Acessando...').css({'padding':'5px 10px 3px 6px'});
			$(form).find('button').prepend('<img id="img_carregando" src="https://www.receitadevovo.com.br/imagens/cadastrando.gif" />');
			$(form).find('#img_carregando').css({'width':'20px','height':'20px','float':'left','margin':'-2px 3px 0 0'});
			setTimeout(function(){
				$.ajax({
					url: "https://www.receitadevovo.com.br/ajax/fazer_login.ajax.php",
					type: "POST",
					dataType: "text",
					data: {
						 "email":email,"senha":senha,"login":login
					},
					success: function(resposta){
						var ret = resposta;

						if(resposta==0){
							$(form).find('button').html('Entrar').css({'padding':'0'});
							$(form).find('button').removeAttr('img');
							$(form).find('button').css({'padding':'8px'});

							$('#dados_salvos_perfil').find('p').html('Login inválido!');
							$('#dados_salvos_perfil').fadeIn(900);

							setTimeout(function(){
								$('#dados_salvos_perfil').fadeOut(900);
							},3000);
						}
						else{
							location.reload();
						}
					}
				});
			},1500);
		}
	}
	function entrar_perfil_enter(tecla,form){
		if(tecla==13)
			entrar_perfil(form);
	}
	function esqueci_senha(form){
		var email;

		email = document.getElementById('email_esqueci_senha').value;

		if(email.length>0){
			$(form).find('button').html('Enviando...').css({'padding':'5px 10px 3px 6px'});
			$(form).find('button').prepend('<img id="img_carregando" src="https://www.receitadevovo.com.br/imagens/cadastrando.gif" />');
			$(form).find('#img_carregando').css({'width':'20px','height':'20px','float':'left','margin':'-2px 3px 0 0'});
			setTimeout(function(){
				$.ajax({
					url: "https://www.receitadevovo.com.br/ajax/esqueci_senha.ajax.php",
					type: "POST",
					dataType: "text",
					data: {
						 "email":email
					},
					success: function(resposta){
						$(form).find('button').html('Enviar').css({'padding':'0'});
						$(form).find('button').removeAttr('img');
						$(form).find('button').css({'padding':'8px'});

						if(resposta==1){
							$('#dados_salvos_perfil').find('p').html('Uma nova senha foi enviada para seu e-mail!');
							$('#dados_salvos_perfil').fadeIn(900);

							setTimeout(function(){
								$('#dados_salvos_perfil').fadeOut(900);
								document.getElementById('email_esqueci_senha').value='';
								fechar_cadastre('div_esqueci_senha');
							},3000);
						}
						else{
							$('#dados_salvos_perfil').find('p').html('E-mail não encontrado!');
							$('#dados_salvos_perfil').fadeIn(900);

							setTimeout(function(){
								$('#dados_salvos_perfil').fadeOut(900);
							},3000);
						}
					}
				});
			},1500);
		}
	}
	function esqueci_senha_enter(tecla,form){
		if(tecla==13)
			esqueci_senha(form);
	}
	function imprimir_receita(pagina){
		window.open(pagina,'Imprimir Receita','toolbar=no,location=no,status=no,menubar=no,scrollbars=yes,resizable=no,width=1000');
	}
	function mostrar_mensagem(texto){
		$('#escurecer').fadeIn(900);
		$('#dados_salvos_perfil').find('p').html(texto);
		$('#dados_salvos_perfil').fadeIn(900);

		setTimeout(function(){
			$('#escurecer').fadeOut(900);
			$('#dados_salvos_perfil').fadeOut(900);
		},4000);
	}
	function mudar_botao_busca(tipo){
		if(tipo=='buscar'){
			var valor = $('#formbusca_topo2').find('input').val();
			if(valor.length>0)
				$('#formbusca_topo2').submit();
			else
				$('#formbusca_topo2').find('div').css('display','block');
		}
		else{
			$('#formbusca_topo2').find('div').css('display','none');
			$('#formbusca_topo2').find('input').val('');
		}
	}
