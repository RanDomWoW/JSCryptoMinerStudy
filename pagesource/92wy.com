<script>

	var checklogined = function(){
		var uid = jQuery("#tb-wy-login").attr('data-uid');
		return uid && parseInt(uid) > 0;
	}
	var wyLoginBox = {
		loginHover: function() {
			var login = jQuery("#tb-wy-login");
			var loginBox = jQuery("#tb-user-login-box");
			var close =  jQuery("#tb-closebox");
			login.click(function() {
				if(!checklogined()) {
					loginBox.addClass("loginboxon");
				}
			});
			close.click(function() {
				loginBox.removeClass("loginboxon");
			});
		},
		changeyzm: function(imgtag){
			var imgtag_src = 'http://passport.92wy.com/login.php?action=image';
			var imgtag_src_n = imgtag_src + '&r=' + Math.random();
			jQuery(imgtag).attr('src', imgtag_src_n);
		},
		login: function(){
			var from = jQuery('#tb-loginform');
			var loginurl = 'http://passport.92wy.com/?action=loginok&jp=1';
			jQuery.ajax({
				url: from.attr('action'),
				type: from.attr('method'),
				dataType: 'jsonp',
				data: from.serialize(),
				success: function(d){
					if(d && d.state && d.state > 0){
						if(d.uid > 0){
							wyLoginBox.logined(d.uid, d.passport, d.username);
							jQuery("#tb-user-login-box").removeClass("loginboxon");
							jQuery("#u-home").attr('href', 'http://bbs.92wy.com/space-uid-' + d.uid + '.html');
						}else{
							location.reload();
						}
					}else{
						fadeToggle(jQuery('#tb-error-info'));
						jQuery('#tb-error-info').html(d.msg || '服务器正忙，请稍后重试').show();
					}
				}
			});
		},
		logined: function(uid, passport, username){
			if(uid && uid > 0){
				jQuery("#tb-wy-login").text(username).attr({'data-uid': uid, 'data-passport': passport, 'title': username});
				jQuery(".userinfobox").show();
				jQuery("#logoff").text("退出").attr('href', 'javascript:;').click(function() {
					wyLoginBox.logout();
				});
				getmessage();
			}else{
				jQuery("#tb-wy-login").text('登录').removeAttr('data-uid data-passport title');
				jQuery(".userinfobox").hide();
				jQuery("#logoff").text("注册");
			}
		},
		notice: function(t) {
			if(t == 'msg'){
				jQuery(".infoimg-msg").children("i").show();
			}else if(t == 'friend'){
				jQuery(".infoimg-friend").children("i").show();
			}
		},
		rnotice: function(t) {
			if(t == 'msg'){
				jQuery(".infoimg-msg").children("i").hide();
			}else if(t == 'friend'){
				jQuery(".infoimg-friend").children("i").hide();
			}
		},
		addnotice: function(d, t){
			jQuery('#game-box-ul').empty();
			if (t == 'notice') {
				if(toType(d) != 'array'){
					wyLoginBox.rnotice('msg');
					return ;
				}else{
					for (var i = 0, len = d.length; i < len; i++) {
						jQuery('#game-box-ul').append('<li class="game-item"><a target="_blank" title="' + d[i].message + '" href="' + d[i].url + '">' + d[i].subject + '</a></li>');
					}
					var clearmsg = jQuery('<li class="game-item clearinfo"><a href="javascript:;">标记全部消息为已读</a></li>');
					clearmsg.click(function() {
						jQuery.ajax({
							url: 'http://passport.92wy.com/msg.php?action=read',
							type: 'get',
							dataType: 'jsonp',
							success: function(d){
								if(d && d.state && d.state == 1){
									wyLoginBox.rnotice('msg');
									jQuery('#game-box-ul').html('');
								}
							}
						});
					});
					jQuery('#game-box-ul').append(clearmsg);
				}
				wyLoginBox.notice('msg');
			} else if (t == 'friend') {
				if(toType(d) != 'array'){
					wyLoginBox.rnotice('friend');
					return ;
				}else{
					for (var i = 0, len = d.length; i < len; i++) {
						jQuery('#game-box-ul-frd').append("<li class='game-item'><a target=\"_blank\"  title='" + d[i].message +"' href='" + d[i].url + "'>" + d[i].subject + "</a></li>");
					}
				}
			}
		},
		logout: function(){
			jQuery.ajax({
				url: 'http://passport.92wy.com/login.php?action=exit&jp=1',
				type: 'get',
				dataType: 'jsonp',
				success: function(d){
					if(d && d.state && d.state == 1){
						location.reload();
					}
				}
			});
		}
	}
	var getmessage = function(){
		
		if(checklogined()){
			jQuery.ajax({
				url: 'http://passport.92wy.com/msg.php?action=get',
				type: 'get',
				dataType: 'jsonp',
				success: function(d){
					if(d && d.state && d.state == 1){
						wyLoginBox.addnotice(d.d, 'notice');
					}
					setTimeout(getmessage, 20000);
				}
			});
		}
	};

	var fadeToggle = function(obj) {
		obj.fadeIn('slow', function() {
			var e = jQuery(this);
			setTimeout(function(){
				e.fadeOut('slow');
			},1000);
		});
	};

	var toType = function(obj) {
	 return ({}).toString.call(obj).match(/\s([a-zA-Z]+)/)[1].toLowerCase()
	}
	jQuery('#tb-yanzhengimg').click(function(){
		wyLoginBox.changeyzm(this);
	});
	jQuery(document).ready(function(){
		wyLoginBox.loginHover();
		wyLoginBox.changeyzm(jQuery('#tb-yanzhengimg'));
		wyLoginBox.login();
	});
	jQuery('#tb-loginsubmit').click(function(){
		wyLoginBox.login();
	});
</script>
