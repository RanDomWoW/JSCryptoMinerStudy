<script type="text/javascript">
var mod_fwg_carousel_pos_854947563 = 0;
function fwg_fade(elem, start, end, time) {
	elem.style.opacity = start;
	(function fwg_go() {
		if (start < end) {
			var opacity = elem.style.opacity * 1 + 0.1;
			elem.style.opacity = opacity;
			elem.style.filter = 'alpha(opacity=' + elem.style.opacity * 100 + ')';
			if (elem.style.opacity < end) setTimeout(fwg_go, 50);
		} else {
			var opacity = elem.style.opacity * 1 - 0.1;
			elem.style.opacity = opacity;
			elem.style.filter = 'alpha(opacity=' + elem.style.opacity * 100 + ')';
			if (elem.style.opacity > end) setTimeout(fwg_go, 50);
		}
	})();
}

var fwg_film_eff_854947563 = {
	stop: function() {
		if (this.timer.length) for (var i = 0; i < this.timer.length; i++) if (this.timer[i]) clearTimeout(this.timer[i]);
		this.timer = new Array;
	},
	start: function(to) {
		var curr_pos = fwg_film_eff_854947563.el.style.left?parseInt(fwg_film_eff_854947563.el.style.left):0;
		var step = (curr_pos - to) / 10;
		for (var i = 0; i < 10; i++) {
			this.timer.push(
				setTimeout(function() {
					fwg_film_eff_854947563.el.style.left = (curr_pos + step * i * -1) + 'px';
				}, 50 * i)
			);
		}
	}
}

