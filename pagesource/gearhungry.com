<script>
            jQuery(document).ready(function( $ ) {

                var dynamic_links = function () {
                    $('.product-info').each(function (index, element) {
                        var $this = $(this);
                        if ($this.hasClass('dynamically-linked')) return true;
                        var product_info = $this.data('product-info');
                        var shop_info = $this.data('product-name');

                        if (product_info != undefined) {
                            var link = window.atob(product_info);
                            var $link = $(link);
                            var $element = $(this);
                            var classes = $element.attr('class');
                            var content = $element.html();
                            $link.addClass(classes).html(content);
                            $element.replaceWith($link);
                        }

                        if (shop_info != undefined) {
                            var shop_name = window.atob(shop_info);
                            if($link != undefined){
                                var $element = $link;
                            }else {
                                var $element = $(this);
                            }
                            $element.html(shop_name);
                        }
                        $this.addClass('dynamically-linked');
                    });
                };
                dynamic_links();
            });
        </script>
