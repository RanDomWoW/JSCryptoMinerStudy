							height: 'auto',
							width: 'auto',
							modal: true,
							title: false,
							resizable: false,
							open: function(event, ui){
								$(".ui-dialog-titlebar").hide(); //hide Title Bar
								$(".ui-widget-overlay").click(function () {
									$(event.target).dialog("close");
								}); //automatically assign Click event to overlay
								$('#js-login-popup').dialog('option', 'position', 'center');
							}
						});

						loginPopup.setupOpener();
					});
				},
				setupOpener: function() {
					$('.js-login-opener').unbind('click').click(function(){
						var $this = $(this);

						var href = $this.attr('href');
						var redirectTo = '';

						if(href != '' && href != '#') {
							redirectTo = href;
						}

						loginPopup.open(redirectTo);

						return false;
					});
				},
				open: function(redirectTo) {
					var redirectTo = redirectTo || '';

					if(redirectTo == '') {
						if($('#js-login-popup-redirect-to').data('orig-value')) {
							$('#js-login-popup-redirect-to').val($('#js-login-popup-redirect-to').data('orig-value'));
						}
					} else {
						if(!$('#js-login-popup-redirect-to').data('orig-value')) {
							$('#js-login-popup-redirect-to').data('orig-value', $('#js-login-popup-redirect-to').val());
						}

						$('#js-login-popup-redirect-to').val(redirectTo);
					}

					var $signupPopup = $("#js-signup-popup");

					if($signupPopup.length > 0) {
						$signupPopup.dialog('close');
					}

					$("#js-login-popup").dialog("open");
				}
			};

			loginPopup.init();
		</script>
	</div>

