				<script type="text/javascript">
				(function($){

					$.contri_rotate = function ( el, options ) {
						var s = this, animated = false, timer = false , mouseInOver = false;

						s.el = el;
						s.$el = $(el);
						s.$el.data('contri_rotate', s );

						s.init = function() {

							s.opts = $.extend( {}, $.contri_rotate.defaults , options );
							s.$items = s.$el.children();
							s.itemLength = s.$items.length;
							s.itemWidth = s.$items.eq(0).outerWidth(true);
							s.itemHeight = s.$items.eq(0).height;
							s.totalItemWidth = s.itemWidth * s.itemLength;
							s.currentIndex = 1;
							s.wrapBox();

							if ( s.itemLength <= s.opts.viewCounts ) {
								return false;
							};

							s.cloneAdd();
							s.setPosition();
							s.setEventBind();

							if ( s.opts.showNav ) {
								s.setNav();
							};

							//if ( s.opts.autoStart) {
								//s.play();
							//};
						};

						s.wrapBox = function() {
							s.$container = $("<div class='roate-containers'></div>");
							s.$wrap = $("<div class='roate-wrapper'></div>").css({
								'overflow' : 'hidden',
								'position' : 'relative',
								'margin-left' : '0',
								'width' : s.opts.wrapperWidth,
								'height' : s.opts.wrapperHeight
							});
							s.$container.append( s.$wrap );
							s.$el.wrap( s.$container );
							s.$el.css({'position':'absolute' });
							if ( s.opts.move === 'left' ) {
								s.$items.css('float', 'left');
							}
						};

						s.setNav = function() {
							var box = "", selectedClass = "";
							for ( var i=0, cnt = s.itemLength ; i < cnt ; i++ ) {
								selectedClass = ( i === 0 ) ? "active" : "";
								box += "<li><a href='#' class='" + selectedClass + " roate-num'>" + ( i + 1 ) + "</a></li>";
							}
							if ( s.opts.navContainer ) {
								$(s.opts.navContainer).append(box);
								s.$nav = $(s.opts.navContainer);
							} else {
								s.$nav = $("<ul id='banner'>" + box + "</ul>");
								s.$el.parent().parent().append(s.$nav)
							}

							s.$nav.find(".roate-num").bind("click.roate", function(){
								s.stop();
								s.transition( 'next',  $(this).index() + 1 );
								//if ( s.opts.autoStart ) {
									//s.play();
								//}
							});
						};

						s.cloneAdd = function() {
							s.$el.children('.cloned').remove();
							var lastIndex = s.itemLength - s.opts.viewCounts;

							for ( var i = s.itemLength-1; i >= lastIndex ; i-- ) {
								s.$el.prepend( s.$items.eq(i).clone().addClass('cloned') );
							};

							for ( var i = 0 ; i < s.opts.viewCounts; i++ ) {
								s.$el.append( s.$items.eq(i).clone().addClass('cloned') );
							};

							s.$el.find('.cloned').each(function(){
								$(this).find('a,input,textarea,select,button,area,form').attr({ disabled : 'disabled', name : '' });
								$(this).find('[id]').andSelf().removeAttr('id');
							});
						};

						s.setPosition = function() {
							var totSize = 0;
							s.$el.children().each(function(){
								totSize += ( s.opts.move === 'left' ) ? $(this).outerWidth(true) :  $(this).outerHeight(true) ;
							});
							totSize = totSize * 2;
							if ( s.opts.move === 'left' ) {
								s.$el.css({
									'width' : totSize + 'px',
									'left' : '-' + s.itemWidth  * s.opts.viewCounts + 'px'
								});
							} else {
								s.$el.css({
									'heihgt' : totSize + 'px',
									'top' : '-' +  s.itemHeight  + 'px'
								});
							}
							s.$items.eq(0).addClass( s.opts.activeClass );
						};

						s.displayPlayButton = function() {
							$(s.opts.playButton).css('display', 'inline');
							$(s.opts.stopButton).css('display', 'none');
						};

						s.displayStopButton = function() {
							$(s.opts.playButton).css('display', 'none');
							$(s.opts.stopButton).css('display', 'inline');
						};

						s.setelectNav = function( index ) {
							var idx = ( s.currentIndex < 0 ) ? s.itemLength - 1 : ( s.currentIndex % s.itemLength );
							s.$nav.find(".roate-num").removeClass("roate-num-selected").eq((index || idx)-1).addClass("roate-num-selected");
						};

						s.active = function () {
							var idx = ( s.currentIndex < 0 ) ? s.itemLength - 1 : ( s.currentIndex % s.itemLength );
							s.$items.removeClass( s.opts.activeClass ).eq(idx - 1).addClass( s.opts.activeClass );
						};

						s.setDirection = function ( direction, index ) {
							( direction === 'next' ) ? ++s.currentIndex : --s.currentIndex ;
							if ( index !== undefined ) {
								s.currentIndex = index;
							};
						};

						s.transition = function ( direction, index ) {
							var moving = {}, dest = 0;

							if ( !animated ) {
								animated = true;
								s.setDirection( direction, index );

								if ( s.opts.showNav ) {
									s.active();
									s.setelectNav( index );
								};

								if ( s.opts.move === 'left' ) {
									if ( index ) {
										dest = (s.itemWidth * (s.opts.viewCounts - 1) ) +  index * s.itemWidth;
										moving['left'] =  '-' + dest + 'px';
									} else {
										var coord = ( direction === 'next' ) ? "-" : "+";
										moving['left'] =  coord + '=' + s.itemWidth + 'px';
									}
								} else {
									moving['top'] = '-' + (  ( index || s.currentIndex ) * s.itemHeight ) + 'px'
								}

								s.$el.filter(':not(:animated)').animate( moving,
										s.opts.duration,
										s.opts.motion,
										function(){
											s.endAnimate( index );
										}
								);
							}
						};

						s.endAnimate = function( index ) {
							if ( s.currentIndex <= 0 ) {
								s.currentIndex = s.itemLength;
								if ( s.opts.move === 'left' ) {
