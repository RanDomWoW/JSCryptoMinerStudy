				<script type="text/javascript">
				UI = {
					Load: function(){
						UI.Rolling();
					},

					Rolling: function(){
						var ground = $('.bannerRolling'),
							imgs_wrap = ground.find('.ciList'),
							imgs = imgs_wrap.children(),
							size = imgs.width()+10,
							btn_prev = ground.find('.btn_left'),
							btn_next = ground.find('.btn_right');

						//default
						imgs_wrap.css('left',-size);
						imgs_wrap.children('li:last').prependTo(imgs_wrap);

						var prev = function(){
							imgs_wrap.stop().animate({'left':0}, 1000, 'easeInExpo', function(){
								$(this).children('li:last').prependTo(this);
								$(this).css({'left':-size});
							});
						}

						var next = function(){
							imgs_wrap.stop().animate({'left':-(size*2)}, 1000, 'easeInExpo', function(){
								$(this).children('li:first').appendTo(this);
								$(this).css({'left':-size});
							});
						}

						btn_prev.click(function(){
							prev();
						});

						btn_next.click(function(){
							next();
						});

						var start = function(){
							set_interval = setInterval(function(){
								next();
							}, 3000);//3초 자동롤링
						}

						start();
					}
				}

				$(document).ready(function(){
					UI.Load();
				});
				</script>
