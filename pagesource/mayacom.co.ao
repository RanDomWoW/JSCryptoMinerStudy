    <script type="text/javascript">
        $(window).scroll(function ()
        {
            if ($(this).scrollTop()  > 50)
            {
                $('.top-menu').addClass('Mscrol');
                $('.main-rwd').addClass('Main-black');
                $('.menu-view-display-none').addClass('menu-superior-view');
