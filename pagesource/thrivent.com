<script>
	jQuery(document).ready(function($) {
		//select the items, set the counter to 0
		var $cItems = $('.ca-item');
		var cItemCount = 0;
		
		function cItemSelect() {
			//flush out selected class from all items
			$cItems.removeClass('ca-selected');
			
			//add selected class to the next item in the list
			$cItems.eq(cItemCount).addClass('ca-selected');
			
			if (cItemCount >= ($cItems.length - 1) ) {
				//if the count is at the end of the items in the list, reset to zero
				cItemCount = 0;
			}
			else {
				//otherwise increment
				cItemCount++;
			}
		}
		
		//execute cItemSelect() every 6 seconds
		var cTimer = setInterval(cItemSelect, 6000);
		
		$('.ca-item-name').on('click', function() {
			//when clicking on a 'tab' for the carousel, stop the interval
			clearInterval(cTimer);
			
			//flush out selected class from all items
			$('.ca-item').removeClass('ca-selected');
			
			//add selected class to the chosen item
			$(this).parents('.ca-item').addClass('ca-selected');
		});
	});
</script>

