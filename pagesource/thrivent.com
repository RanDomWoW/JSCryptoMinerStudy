<!-- Carousel Script -->
<script>
$(document).ready(function() {
	var carouselTimeout,
		nextItem = 0,
		carouselAuto = true,
		carouselItems = $('#carouselHome .home-carousel__item').length;
	
	function carouselSelect(cItemNum) {
		//hide all carousel items
		$('#carouselHome .home-carousel__item').removeClass('home-carousel__item--active');
		$('#carouselHome .home-carousel__nav li').removeClass('home-carousel__nav--active');
		
		//show just the carousel item selected and highlight the nav item
		var cItem = $('#carouselHome .home-carousel__item').get(cItemNum);
		var cNavItem = $('#carouselHome .home-carousel__nav li').get(cItemNum);
		$(cItem).addClass('home-carousel__item--active');
		$(cNavItem).addClass('home-carousel__nav--active');
		
		//increment the nextItem unless it's already at the end of the ammount of items
		if (nextItem < carouselItems-1) {
			nextItem = nextItem + 1;
		}
		else {
			nextItem = 0;
		}
	}
	
	function carouselStart() {
		//select the nextItem to display
		carouselSelect(nextItem);
		
		//start a timeout for auto advance of the carousel
		if (carouselAuto === true) {
			carouselTimeout = setTimeout(carouselStart, 5000);
		}
	}
	
	$('#carouselHome .home-carousel__list li').each(function(idx) {
		var itemNum = idx + 1;
		$('#carouselHome .home-carousel__nav ul').append('<li><a href="#"><span class="is-vishidden">Carousel Item '+ itemNum +'</span></a></li>');
	});
	
	
	$('#carouselHome .home-carousel__nav li').each(function(idx) {
		$(this).on('click', function(e) {
			//when a nav item is clicked, prevent the default behavior of the link
			e.preventDefault();
			//also stop the auto advancing
			carouselAuto = false;
			clearTimeout(carouselTimeout);
			//and finally select the clicked item to be displayed
			carouselSelect(idx);
		});
	});
	
	carouselStart();
});
	
</script>
<!-- End Carousel Script -->



