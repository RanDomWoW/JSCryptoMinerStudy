jQuery(document).ready(function($){

    var $mail = $(".subscribeform [id*=edit-mail]");
    var $user = $(".subscribeform [id*=edit-name]");

    $("#email").blur(function() {
        $mail.val( this.value );
    });
    $("#email").blur(function() {
        $user.val( this.value );
    });

    $('.subscribeform [id*=edit-field-user-name-first-und-0-value]').attr("placeholder", "First Name");
    $('.subscribeform [id*=edit-field-user-name-last-und-0-value]').attr("placeholder", "Last Name");
    $('.subscribeform [id*=edit-field-user-job-title-und-0-value]').attr("placeholder", "Job Title");
    $('.subscribeform [id*=edit-field-user-company-und-0-value]').attr("placeholder", "Company");
    $('.subscribeform [id*=edit-field-phone-und-0-value]').attr("placeholder", "(555) 555-5555");
    $('.subscribeform [id*=edit-pass-pass1]').attr("placeholder", "Password");
    $('.subscribeform [id*=edit-pass-pass2]').attr("placeholder", "Password Confirm");
    $('.subscribeform [id*=edit-pass-pass2]').attr("placeholder", "Password Confirm");
    $('.sign_in_box [name=name]').attr("placeholder", "Username or Email");
    $('.sign_in_box [name=pass]').attr("placeholder", "Password");
    $('.forgot_password [name=name]').attr("placeholder", "Username or Email");
    $('.subscribeform select option').each(function() {
      var text = $(this).text();
      text = text.replace('- Select a value -', 'Country');
      $(this).text(text);
    });
    
});
jQuery(".continue_btn").on("click", function(){
  var email = jQuery('.emailfirst input[name=email]').val();

  if( /(.+)@(.+){2,}\.(.+){2,}/.test(email) ){
    jQuery(".processing_btn, .continue_btn").slideToggle(10);
    var postData = jQuery("#email_lead").serializeArray();
    jQuery.ajax({
      url: "https://s893759278.t.eloqua.com/e/f2",
      type: "POST",
      data: postData ,
      success: function(data) {
        console.log(data.result);
      }
    });
    jQuery.ajax({
      url: "/leads/emailonly",
      type: "POST",
      data: postData ,
      success: function(data) {
        if(data.indexOf("Success!") > -1){
          console.log(data.result);
          jQuery(".subscribeform, .emailfirst").slideToggle(100);
        } else {
          var result = data;
          jQuery("#ajaxCall").text(result);
          jQuery(".used_email, .processing_btn, .continue_btn").slideToggle(10);
          jQuery(".sign_in_box, .ajaxCall").show();
          jQuery(".emailfirst, .subscribeform, .invalid_email").hide();
        }
      }
    });
  } else {
    jQuery(".email-field").addClass("has-error");
    jQuery(".invalid_email").slideToggle(10);
    jQuery(".used_email").hide(10);
  }
});

jQuery(".subscribeform .form-submit").on("click", function(){
  jQuery(".subscribeform, .thank_you_box").slideToggle(100);
});
</script>

