            cX.callQueue.push(['sendEvent', 'play', { vid: _cx_videoId }]);
            _cx_curProgress = 0;
        } else if (_cx_curProgress != p) {
            _cx_curProgress = p;
            // report 25, 50, 75
            if (p >= _cx_curBlock && p<100) {
                cX.callQueue.push(['sendEvent', 'progress', { percent: _cx_curBlock + '', vid: _cx_videoId }]);
                _cx_curBlock += 25;
            }
        }
    });
});
</script>
