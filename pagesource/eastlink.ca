<script type="text/javascript">
    //Scripts to deal with Wireless Only areas
    // Currently Moncton only, but add additional community names to the array below as new ones are released
    jQuery(document).ready(function ($) {

        //read a cookie
        function readCookie(name) {
            var nameEQ = name + '=';
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1, c.length);
                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
            }
            return null;
        }

        var wirelessRegionCookie = readJSONCookie('regionalCookie');

        function readJSONCookie(name) {
            var value = readCookie(name);
            if (value == null) return null;
            return JSON.parse(value);
        }

        if (wirelessRegionCookie != null) {
            //read a JSON encoded cookie
            // determine if the community is wireless only, add any new ones to the array
            var wirelessOnlyCommunityList = ["Moncton", "Memramcook", "Shediac", "St. John's", "Conception Bay South", "Mount Pearl", "Middle Cove", "Paradise", "Saint John", "AddNextWirelessOnlyCommunity"];
            var wirelessOnlyCommunity = false;
            var currentCommunity = wirelessRegionCookie.Community;
            if (wirelessOnlyCommunityList.indexOf(currentCommunity) >= 0) {
                wirelessOnlyCommunity = true;
            }

            // determine if on myeastlink
            var currentURL = window.location.host;
            var isMyEastlink = false;
            if (currentURL.search('my') >= 0) {
                isMyEastlink = true;
            }


            // takes care of hiding and some redirecting for wireless only community for eastlink.ca for english and french
            if ((wirelessOnlyCommunity == true) && (isMyEastlink == false)) {
                var currentPathName = window.location.pathname;
                if (currentPathName.search('fr') > 0) {
                    // French conditions
                    // Hide top nav links except Wireless
                    if ((currentPathName.search('wireless') > 0) || (currentPathName.search('apropos') > 0) || (currentPathName.search('trouverunmagasin') > 0)) {
                        $('#nav ul li:not(:contains("Wireless"))').css('display', 'none');
                    }
                    $('#nav ul li a:contains("Wireless")').css('border-right', '0');

                    // Change Residential, Support and Contact Us Links
                    $('ul.global li a:contains("sidentiel")').attr('href', '/fr/wireless.aspx');
                    $('#LogoRow a').attr('href', '/fr/wireless.aspx');

                } else {
                    // English Conditions
                    // Hide top nav links except Wireless
                    $('#nav ul li:not(:contains("Wireless"))').css('display', 'none');
                    $('#nav ul li a:contains("Wireless")').css('border-right', '0');
                    // Redirect My Account in Secondary Nav
                    $('ul#secondary li a:contains("Account")').attr('href', 'https://mywireless.eastlink.ca/login.html');

                    // Change Residential, Support and Contact Us Links
                    $('ul.global li a:contains("Residential")').attr('href', '/wireless.aspx');
                    $('ul.global li a:contains("Support")').attr('href', 'http://my.eastlink.ca/customersupport/wireless.aspx');
                    $('#LogoRow a').attr('href', '/wireless.aspx')

                    // Redirect to /wireless.aspx if pages are not in either Wireless or About sections
                    var currentPathName = window.location.pathname;
                    if ((currentPathName.indexOf('wireless') == -1) && (currentPathName.indexOf('about') == -1) && (currentPathName.indexOf('locator') == -1) && (currentPathName.indexOf('search') == -1)) {
                        window.location = "/wireless.aspx";
                    }

                }


            }

            // takes care of hiding and some redirecting for wireless only community for my.eastlink.ca
            if ((wirelessOnlyCommunity == true) && (isMyEastlink == true)) {
                // Change main nav links
                $('#nav ul li a:contains("Support")').attr('href', '/customersupport/wireless.aspx');
                $('#nav ul li a:contains("Account")').attr('href', 'https://mywireless.eastlink.ca/login.html');
                $('ul.global li a:contains("Support")').attr('href', '/customersupport/wireless.aspx');

                // Hide secondary support nav links for wireline
                $('ul#secondary li:not(:contains("Wireless"))').css('display', 'none');
                $('ul#secondary li:contains("Billing")').css('display', 'none');
                $('ul#secondary li:contains("Billing")').css('display', 'none');
                $('ul#secondary:first li:contains("Wireless")').css('border-left', '0');
                $('ul#secondary li ul li').css('display', 'inline');

                // redirect my account to wireless self serve if wireless only community
                if (window.location.pathname == "/myaccount.aspx") {
                    window.location = "https://mywireless.eastlink.ca/login.html";
                }

            }

            // takes care of hiding and some redirecting for wireless only community for both eastlink.ca and my.eastlink.ca
            if ((wirelessOnlyCommunity == true)) {
                // Footer adjustments
                $('.about_eastlink').css('display', 'none');
            }

        }
    });


</script>
