<script type="text/javascript">
    
        var cookieName = "regionalCookie";
        var isCrawler = 'False';

        // Create or update a JSON encoded cookie
        function createJsonCookie(name, value, days) {
            createCookie(name, JSON.stringify(value), days);
        }

        // Create or update a cookie
        function createCookie(name, value, days) {
            var expires;
            if (days) {
                var date = new Date();
                date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                expires = "; expires=" + date.toGMTString();
            } else expires = "";

            if (window.location.hostname.toLowerCase() == "eastlink.ca" || window.location.hostname.toLowerCase() == "www.eastlink.ca") {
                document.cookie = name + "=" + value + expires + "; path=/; domain=.eastlink.ca";
            } else if (window.location.hostname.toLowerCase() == "my.eastlink.ca" || window.location.hostname.toLowerCase() == "www.my.eastlink.ca") {
                document.cookie = name + "=" + value + expires + "; path=/; domain=.eastlink.ca";
            } else {
                document.cookie = name + "=" + value + expires + "; path=/;";
            }

            if (name == regionalCookieString) {
                createCookie(confirmedCookieString, confirmedCookieValueString, 365);
            }
        }
        
        // Read a cookie
        function readCookie(name) {
            var nameEq = name + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1, c.length);
                if (c.indexOf(nameEq) == 0) return c.substring(nameEq.length, c.length);
            }
            return null;
        }

        // Erase a cookie
        function eraseCookie(name) {
            createCookie(name, "", -1);
        }

        // Store the cookie
        // IR-0030397 ' was breaking the GetRegionalPage webservice call so replaced  cookie.Community with  cookie.Community.replace(/'/g, "\\'") and for region too.
        function storeCookie(cookie,isDefault) {
            if (cookie.Province == "QC" && (window.location.hostname.toLowerCase().indexOf("my.eastlink") == -1 || window.location.hostname.toLowerCase().indexOf("myeastlink") == -1)) {
                location.href = "/quebec.aspx";
                return;
            }

            createJsonCookie(cookieName, cookie, 365);

            if (isDefault) {
                //Better way?
                eraseCookie(confirmedCookieString);
                createCookie(confirmedCookieString, unConfirmedCookieValueString, 365);
            }
            jQuery.ajax({
                type: "POST",
                url: "/DesktopModules/EastLinkRegionalSelector/EastLinkRegionalSelector.asmx/GetRegionalPage",
                data: "{'href':'" + location.pathname + "', 'community':'" + cookie.Community.replace(/'/g, "\\'") + "','region':'" + cookie.Region.replace(/'/g, "\\'") + "','portalId':" + 0 + "}",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: OnGetRegionalPageComplete,
                error: onAjaxError
            });
        }

        // Check to see if the cookie exists
        function cookieExists(name) 
        {
            var allcookies = document.cookie;
            var exists = false;

            // Get all the cookies pairs in an array
            var cookiearray  = allcookies.split(';');
            var currentCookie;

            // Now take key value pair out of this array
            for(var i=0; i<cookiearray.length; i++)
            {
                currentCookie = cookiearray[i].split('=')[0].trim();
                if (name == currentCookie) 
                {
                    exists = true;
                                       
                }
            }

            return exists;
        }
 
        // Check the community dropdown and get the communities
        function checkCommunity() {
            jQuery("#selProvince").removeClass("ui-state-error");
            jQuery("#selCommunity").removeClass("ui-state-error");

            var strCommunity = jQuery.trim(jQuery("#selCommunity").val());


            if (strCommunity == 'Coast Cable Customers')
                location.href = 'http://coastcable.com';
            else if (strCommunity == 'Delta Cable Customers')
                location.href = 'http://deltacable.com';
            else if (strCommunity == 'North Bay')
                location.href = '/ltessential.aspx';
            else {
                if (jQuery("#selCommunity").val() != "") {
                    jQuery.ajax({
                        type: "POST",
                        url: "/DesktopModules/EastLinkRegionalSelector/EastLinkRegionalSelector.asmx/CheckCommunity",
                        data: "{'province':'" + jQuery("#selProvince").val() + "','community':'" + jQuery("#selCommunity").val().replace(/'/g, "\\'") + "'}",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (data) {

                            // Use to calculate the GA bounce rate. If they are here without region cookie then track in GA.
                            if (!(cookieExists("regionalCookie")))
                            {
                                _gaq.push(['_trackEvent', 'Regional Selector', 'Select Province and Community', 'Choose Region', 1, true]);
                            }

                            onCheckPostalCodeComplete(data);},
                        error: onAjaxError
                    });
                } else {
                    if (jQuery("#selProvince").val() == "") {
                        jQuery("#selProvince").addClass("ui-state-error");
                    } else {
                        jQuery("#selCommunity").addClass("ui-state-error");
                    }
                }
            }
        }

        // Check the province dropdown and get the communities
        function checkProvince() {
            jQuery("#selProvince").removeClass("ui-state-error");
            jQuery("#selCommunity").removeClass("ui-state-error");
            if (jQuery("#selProvince").val() != "") {
                jQuery.ajax({
                    type: "POST",
                    url: "/DesktopModules/EastLinkRegionalSelector/EastLinkRegionalSelector.asmx/GetCommunities",
                    data: "{'province':'" + jQuery("#selProvince").val() + "'}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: onSelProvinceChangeComplete,
                    error: onAjaxError
                });
            } else {
                jQuery('#selCommunity').children('option:not(:first)').remove();
            }
        }

        function doPostalCodeLookup(postal,isDefault) {
            jQuery.ajax({
                type: "POST",
                url: "/DesktopModules/EastLinkRegionalSelector/EastLinkRegionalSelector.asmx/CheckPostalCode",
                data: "{'postalCode':'" + postal + "'}",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data) {
                    // Use to calculate the GA bounce rate. If they are here without region cookie then track in GA.
                    if (!regionConfirmed()) {
                        if (isDefault != true) {
                            _gaq.push(['_trackEvent', 'Regional Selector', 'Enter Postal Code', 'Choose Region', 1, true]);
                        }
                    }
                    onCheckPostalCodeComplete(data,isDefault);
                },
                error: onAjaxError
            });
        }

        // Check the validity of the postal code and look for a match
        function checkPostalCode() {
            var regexp = /^[ABCEGHJ-NPRSTVXY]{1}[0-9]{1}[ABCEGHJ-NPRSTV-Z]{1}[ ]?[0-9]{1}[ABCEGHJ-NPRSTV-Z]{1}[0-9]{1}$/;
            var txtPostalCode = jQuery("#txtPostalCode");
            var errorMessage = '* Invalid postal code, e.g. B3K 5M3';
            txtPostalCode.val(txtPostalCode.val().toUpperCase());

            if (!(regexp.test(txtPostalCode.val()))) {
                jQuery("#txtPostalCode").addClass("ui-state-error");
                jQuery("#msgPostalCodeError").addClass("errorMessage").text(errorMessage);
            } else {
                doPostalCodeLookup(jQuery("#txtPostalCode").val());
            }
        }

    
    
    function readJSONCookieTemp(name) {
        var value = readCookie(name);
        if (value == null) return null;
        return JSON.parse(value);
    }

    //callback for redirecting to the appropriate region page
    function OnGetRegionalPageComplete(data, status) {
        if (data != null && data.d != null) {
            var testCookie = readCookie(cookieName);
            var readCommunity= readJSONCookieTemp(cookieName);

            if (testCookie != null) {
                if (location.href.indexOf("SearchResults") != -1 || location.href.indexOf("searchresults") != -1) {
                    location.reload(true);
                } else {
                    if (readCommunity.Community.toLowerCase()=='moncton' || readCommunity.Community.toLowerCase()=='st. john\'s') {
                        location.href = '/wireless.aspx';
                    } else {
                      location.href = data.d;
                    }
                }
            } else {
               
                history.go(-1);
            }
        }
    }

        // Callback for checking for matching postal code
        function onCheckPostalCodeComplete(data,isDefault) {
            var errorMessage;
            jQuery('#txtPostalCode').blur();
            if (data != null && data.d != null) {

                if (data.d.Community == null || data.d.Community.length == 0) 
                {
                    createJsonCookie(cookieName, data.d, 365);
                    jQuery("#msgPostalCodeError").text("");
                    jQuery("#msgPostalCodeError").removeClass();
                    errorMessage = "* Sorry, but we could not find your postal code in our system. Please select your province and community from the list below. If you do not see your community in the list then please select the community nearest to you.";
                    jQuery("#msgPostalCodeMajorError").addClass("errorMessage").text(errorMessage);

                } 
                else 
                {
                    storeCookie(data.d,isDefault);
                }
            } 
            else 
            {
                jQuery("#msgPostalCodeError").text("");
                jQuery("#msgPostalCodeError").removeClass();
                errorMessage = "* Sorry, but we could not find your postal code in our system. Please select your province and community from the list below. If you do not see your community in the list then please select the community nearest to you.";
                jQuery("#msgPostalCodeMajorError").addClass("errorMessage").text(errorMessage);
            }
        }

        // Callback for getting communities for a province
        function onSelProvinceChangeComplete(data) {
            if (data != null && data.d != null && data.d.length > 0) {
                jQuery("#selCommunity>option").remove();
                var output = [];
                output.push('<option value="">and Community</option>');
                jQuery.each(data.d, function(idx) {
                    output.push('<option value="' + data.d[idx].Key + '">' + data.d[idx].Value + '</option>');
                });
                jQuery("#selCommunity").html(output.join(''));
            } else {
                jQuery('#selCommunity').children('option:not(:first)').remove();
            }
        }

        // AJAX error callback
        function onAjaxError(status) {
           // alert(status.status + ' \n\r ' + status.statusText + '\n\r' + status.responseText);
        }

        // Reset the form
        function resetForm() {
            createCookie(cookieName + "_test", "test", 1);
            var testCookie = readCookie(cookieName + "_test");
            if (testCookie == null) {
                jQuery("#msgCookieError").show();
                jQuery("#modal_regional_selector").hide();

            } else {
                jQuery("#modal_regional_selector").show();
                jQuery("#msgCookieError").hide();
            }
            eraseCookie(cookieName + "_test");
            jQuery("#txtPostalCode").removeClass("ui-state-error");
            jQuery("#msgPostalCodeError").text("");
            jQuery("#msgPostalCodeError").removeClass();
            jQuery("#msgPostalCodeMajorError").text("");
            jQuery("#msgPostalCodeMajorError").removeClass();
            jQuery("#selProvince").removeClass("ui-state-error");
            jQuery("#selCommunity").removeClass("ui-state-error");
            jQuery("#txtPostalCode").val("");
            jQuery("#selProvince").val("");
            jQuery('#selCommunity').children('option:not(:first)').remove();

        }

        function trackCommunity(community, province)
        {
        var provinceID;

            switch (province.toLowerCase())
            {
            case 'ns':
                provinceID = 1;
                break;
            case 'nb':
                provinceID = 2;
                break;
            case 'pe':
                provinceID = 3;
                break;
            case 'nl':
                provinceID = 4;
                break;
            case 'on':
                provinceID = 5;
                break;
            case 'ab':
                provinceID = 6;
                break;
            case 'bc':
                provinceID = 7;
                break;
            default:
                provinceID = 8;
                break;
            }
            _gaq.push(['_trackEvent', 'Regional Selector', 'Cookie Info', community, provinceID,true]);
        }

        var regionalCookieString = "regionalCookie";
        var confirmedCookieString = "regionConfirmed";
        var unConfirmedCookieValueString = "0";
        var confirmedCookieValueString = "1";

        //'Level' the cookie playing field
        function validateRegionalInformation() {         
            checkForCookieUpdate();
        }

        function checkForCookieUpdate() {
            //TODO move update routine(s) here
            if (!cookieExists(confirmedCookieString)) { // case for 'old' users before 'confirmed' cookies.
                createCookie(confirmedCookieString, confirmedCookieValueString, 365);
            }

        }
        
        function getCookieValue(cookieName) {
            var retVal = "";
            if (cookieExists(cookieName)) {
                var allcookies = document.cookie;
                var cookiearray = allcookies.split(';');
                var currentCookie;
                for (var i = 0; i < cookiearray.length; i++) {
                    currentCookie = cookiearray[i].split('=')[0].trim();
                    if (currentCookie == cookieName) {
                        retVal = cookiearray[i].split('=')[1].trim();                        
                    }
                }
            }
            return retVal;
        }

        function regionConfirmed() {
            if (!cookieExists(regionalCookieString) || !cookieExists(confirmedCookieString)) {                
                return false;
            } else {
                var confirmed = parseInt(getCookieValue(confirmedCookieString)) || 0;
                if (confirmed == 1) {
                    return true;
                }
            }
            return false;
        }

        function promptUserForRegion() {
            //Variable should be set on a page by page basis. Default to false, then opt in for true
            
                 var regionRequiredForPage = false;  
                   
                        
            if (regionRequiredForPage) {
                //Region required, user has not chosen
                if (!regionConfirmed() && (isCrawler == "False")) {
                    //Do not send No Cookie event when a 'default' cookie exists.
                    if (!cookieExists(confirmedCookieString)) {
                        _gaq.push(['_trackEvent', 'Regional Selector', 'Region Selector No Cookie', 'Choose Region', 1, true]);
                    }
                    return true;
                }
            } else {
                return false;
            }
         }

            // This adds 'placeholder' to the items listed in the jQuery .support object. 
            jQuery(function () {
                jQuery.support.placeholder = false;
                test = document.createElement('input');
                if ('placeholder' in test) jQuery.support.placeholder = true;
            });

            // This adds placeholder support to browsers that wouldn't otherwise support it. 
            jQuery(function () {
                if (!jQuery.support.placeholder) 
                {
                    var active = document.activeElement;
                    jQuery('#txtPostalCode').focus(function () 
                    {
                        if (jQuery(this).attr('placeholder') != '' && jQuery(this).val() == jQuery(this).attr('placeholder')) 
                        {
                            jQuery(this).val('').removeClass('hasPlaceholder');
                        }
