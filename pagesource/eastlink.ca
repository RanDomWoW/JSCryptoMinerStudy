<script type="text/javascript">

    function isValidPostalCode(zipcode) {
        try {
            if (zipcode.length == 6 && zipcode.search(/^[a-zA-Z]\d[a-zA-Z]\d[a-zA-Z]\d$/) != -1) {
                return true;
            } else {
                alert("Please enter a valid postal code");
                return false;
            }
        } catch (e) { }
    }

    function doZap2It2() {

        jQuery('.zipCodeInput2').val(jQuery('.zipCodeInput2').val().replace(/\s*/g, ''));

        if (jQuery(".zipCodeInput2").val() != "" && jQuery(".zipCodeInput2").val() != "Enter your Postal Code") {
            if (isValidPostalCode(jQuery(".zipCodeInput2").val())) {
                _gaq.push(['_trackEvent', 'Whats on TV', 'Click']);
                //location.href = jQuery(".zapAction2").val() + "?zipcode=" + jQuery(".zipCodeInput2").val() + "&aid=" + jQuery(".aid2").val();
                location.href = "http://affiliate.zap2it.com/tvlistings/ZCGrid.do" + "?zipcode=" + jQuery(".zipCodeInput2").val() + "&aid=" + jQuery(".aid2").val();
                setZapCookie(jQuery(".zipCodeInput2").val());
                return false;
            } else {
            return false;
            }
        }
    }

    // Sets additional cookie with Postal Code - Jeff Harrop April 30, 2014
    function setZapCookie(zipCode) {
       var name = "ZapToItPostalCode";
       var expires = new Date();
       expires.setTime(expires.getTime() + 31536000000); //1 year
       if (window.location.hostname.toLowerCase() == "eastlink.ca" || window.location.hostname.toLowerCase() == "www.eastlink.ca") {
           document.cookie = name + "=" + zipCode + "; expires=" + expires.toUTCString() + "; path=/; domain=www.eastlink.ca";
       } else if (window.location.hostname.toLowerCase() == "my.eastlink.ca" || window.location.hostname.toLowerCase() == "www.my.eastlink.ca") {
           document.cookie = name + "=" + zipCode + "; expires=" + expires.toUTCString() + "; path=/; domain=www.eastlink.ca";
       } else {
           document.cookie = name + '=' + zipCode + ';expires=' + expires.toUTCString();
       }
   }

    jQuery(function () {

        jQuery(".zipCodeInput2").keydown(function (e) {
            if (e.keyCode == 13) {
                doZap2It2();
                return false;
            }
        });

        jQuery("#zipCodeButton2").click(doZap2It2);

        jQuery(".zipCodeInput2").click(function () {

            if (jQuery('.zipCodeInput2').val() == ('Postal Code'))
                jQuery('.zipCodeInput2').val('');
        });

    });

