<script type="text/javascript">

    // Search box functionality
    var defaultSearchTerm = "Search Eastlink.ca";

    function doSearch() {

        if (jQuery("#searchInput").val() != "" && jQuery("#searchInput").val() != defaultSearchTerm) {
            location.href = jQuery(".searchAction").val() + "?performSearch=true&q=" + escape(jQuery("#searchInput").val());
            return false;
        }
    }

    jQuery(document).ready(function () {

        jQuery("#searchInput").blur(function () {
            if (jQuery("#searchInput").val() == "") jQuery("#searchInput").val(defaultSearchTerm);
        });

        jQuery("#searchInput").focus(function () {
            if (jQuery("#searchInput").val() == defaultSearchTerm) jQuery("#searchInput").val("");
        });
        jQuery('#searchInput').keydown(function (e) {
            if (e.keyCode == 13) {
                doSearch();
                return false;
            }
        });
        jQuery("#searchButton").click(function() {
                doSearch();
                return false;
        });

        $("#searchInput").autocomplete({

            maxResults: 5,
            delay: 0,
            source: function (request, response) {

                $.post("/desktopmodules/eastlinksearch/api/search/GetSearchSuggest", {searchQuery: request.term, maxReturnedResults: 5 }, function (data) {

                    var suggestions = jQuery.parseJSON(data);
                    response(suggestions[1]);

                })

            },

            select: function (event, ui) {

                jQuery("#searchInput").val(ui.item.value);
                doSearch();
                return false;

            },

            messages: { noResults: '', results: function () { } }

        });

    });

</script>

