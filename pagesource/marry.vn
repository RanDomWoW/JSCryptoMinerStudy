<script>
    var $ = jQuery.noConflict();
    $(document).ready(function () {
        var view_popup = $.cookie('view_popup');

        if ( typeof view_popup == 'undefined' && OA_output[447] != '') {
            $.magnificPopup.open({
                items: {
                    src: '#rvn_popup',
                    type: 'inline'
                }
            });

            $.cookie('view_popup', true, {expires: 7});

            $('.mfp-close').css('margin-right', 0 - ($('.popup-ad__inner img').width() / 2) - 30);
        }
    });
</script>
