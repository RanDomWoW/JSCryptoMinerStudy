<script>
    function show_loading() {
        $('#rvn_loading').fadeIn();
    }

    function hide_loading() {
        $('#rvn_loading').fadeOut();
    }

    var $ = jQuery.noConflict();
    $(document).ready(function () {

        // Update Cart
        $.ajax({
            url: ajaxurl,
            type: "post",
            dataType: 'json',
            data: {
                action: 'rvn_ajax_handler_order',
                method: 'GetCartQuantity'
            },
            success: function (data) {
                if (data.status == 'success') {
                    // Update header quantity
                    $('.header-cart-quantity').html(data.data);
                    if (data.data > 0) {
                        $('.header-cart-quantity').css('display', 'initial');
                    } else {
                        $('.header-cart-quantity').css('display', 'none');
                    }
                }
            }
        });

    });
</script>

