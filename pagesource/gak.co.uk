<script>
    var CookieKey_PersistentCookie_Suffix = "Persistent";
    var CookieKey_COOKIESENABLED = "GAK_CookiesEnabled";
    var EuroPrice_CookieKey = "GAK_EuroPrice";
    var EuroPrice_ConversionRate = 1;
    var EuroPrice_CurrencySymbol = "£";
    var ShoppinBasket_CookieKey = "GAK_ShoppinBasket";
    var ShoppingBasket_AddProductToOrderURL = '/ShoppingBasket/AddProductToOrder';
    var ShoppingBasket_BuyNowWithPayPalURL = '/ShoppingBasket/BuyNowWithPayPal';
    var ShoppingBasket_GoToShoppingBasketURL = '/ShoppingBasket';
    var ShoppingBasket_DestroyOrderItemURL = '/ShoppingBasket/DestroyOrderItem';
    var ShoppingBasket_UpdateCartURL = '/ShoppingBasket/UpdateCart';
    var ShoppingBasket_ApplyVoucherURL = '/ShoppingBasket/ApplyVoucherOnOrder';
    var ShoppingBasket_FreeGiftList = [];
    var ShoppingBasket_AddFreeGiftListToOrderURL = '/ShoppingBasket/AddFreeGiftListToOrder';
    var DeliveryOptions_SelectedDivItem = null;
    var DeliveryOptions_SelectedLastDeliveryDate = null;
    var DeliveryOptions_IndexDeliveryOptionsURL = '/DeliveryOptions';
    var DeliveryOptions_UpdateDeliveryOptionsURL = '/DeliveryOptions/Update';
    var EmailRegistration_ProceedToRegisterURL = '/Home/_EmailRegistration';
    var TargetIsMobile = false;

    // Deferred CSS function.
    function GAK_displayHiddenItems() {
        // Display header-social-container.
        $(".header-social-container").removeClass("hidden");
    }

    $(document).ready(function () {

        // Cookies enabled?
        $.cookie(CookieKey_COOKIESENABLED, cookiesEnabled());

        setTimeout(function () { GAK_displayHiddenItems(); }, 1000);

        // Disable click on Menu LIs
        $("div.dropdown-menu div.nav-grid ul li.title").click(function (e) {
            e.stopPropagation();
        });

        // Menu options. Brands tab
        var displayBrandLinks = $(".displayBrandLink");
        displayBrandLinks.click(function (e) {
            e.stopPropagation();
            $('[class*="displayBrandBlock"]').addClass("hidden");
            $(".displayBrandBlock" + $(this).data("id")).removeClass("hidden");
        });
        // Display "A" Brands.
        if (displayBrandLinks.length > 0) {
            displayBrandLinks[0].click();
        }

        // spinner
        var $loading = $('.spinnerDiv').hide();
        $(document)
            .ajaxStart(function () {
                $loading.show();
            })
            .ajaxStop(function () {
                $loading.hide();
            });

        // Basket popover
        ShoppingBasket_UpdateBasketPopoverContent();
        $('#basketLink').popover();

        // Email Registration
        EmailRegistration_BindEvents();

        Campaigns_BindEvents('4');
    });

    //Top search bar binding
    app.controller("top_banner_search_controller", ['$rootScope', '$scope', 'sharedProperties' , function ($rootScope, $scope, sharedProperties) {
        $scope.topBannerSearchText = "";

        $scope.onKeyPressed = function (keyEvent) {
            if (keyEvent.which === 13) {
                $scope.onSearchPressed();
            }
        };

        //Broadcast-related methods
        $scope.$on('onUpdateSearchQuery', function (event, text) {
            $scope.topBannerSearchText = text;
        });

        $scope.onSearchPressed = function () {
            //Clearing up the local storage filters once the filters model is updated
            sharedProperties.removeCookie("LastSearchFilters");
            sharedProperties.removeCookie("OriginalSearchFilters");

            $rootScope.$broadcast('onSearchQueryChanged', $scope.topBannerSearchText);
        };

            
        $scope.$on('onSearchQueryChanged', function (event, text) {
            document.location = "/Search/#q=" + encodeURIComponent(text);
        });
        
    }]);

</script>


