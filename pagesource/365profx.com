<script>
var has_submit = false;
window.passwords = {};
var tooltipstring = [
	'Password must be at least 6 alphanumeric characters long, it must not be longer than 12 characters, it must contains at least 1 digit and 1 Capital letter and it must NOT contains special characters ( % , $  , # , etc. )' ,
  	'Password is empty' ,
  	'Password is not 6 characters long' ,
  	'Password is longer than 12 characters' ,
  	'Password must contains at least 1 digit' ,
  	'Password must contains at least 1 Capital letter' ,
  	'Password must NOT contains special characters'
];
var formtemplate =

  
function setSelect( countrysel , prefixsel ){
	countries.forEach( function( el ){
        countrysel.append( '<option value="' + el[2].toUpperCase() + '">' + el[0] + '</option>' );
        prefixsel .append( '<option data-img-src="https://static.chameleon.ad/images/flags/' + el[2].toLowerCase() + '.png" value="' + el[1] + '">' + el[1] + '</option>' );
});
  
    var userdata = undefined;
    $.get("https://ipinfo.io/?token=f786e3b0aab215", function(response) {
        userdata = response;
        prefixsel.chosen();
        countrysel.val( response.country );
        countries.forEach( function( el ){
            if( el[2] == response.country ){
                prefixsel.val( el[1] ).trigger('chosen:updated');
            }
        });

    }, "jsonp");  
}
  
function validateEmail( email ){
    var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    return re.test( email.toLowerCase() );
}
  
function validatePassword( pass ){
	var re = /^(?=.*\d)(?=.*[A-Z])[0-9a-zA-Z]{6,12}$/;
  	return re.test( pass );
}
  
function isNormalInteger( str ) {
    return /^\d+$/.test( str );
}
  
function setTooltip( form ){
  window.passwords[form] = form.find( '.tooltip' );
  form.find( '.tooltip' ).tooltipster({
      	maxWidth: 200 ,
      	trigger:"custom",
        triggerOpen: {
          click: true,  // For mouse
          tap: true    // For touch device
        },
        triggerClose: {
          click: true,  // For mouse
          tap: true    // For touch device
        }
    });
}
  
function setModal(){
	$( 'body' ).css( 'overflow' , 'hidden' ).append( '<div id="black"></div>' );
  	$( '#black' ).append( formtemplate );
  	$( '#black .primary' ).append( '<a class="close"></a>' );
  	$( '#black' ).fadeIn( function(){
    	setSelect( $( '#black .campo_country' ) , $( '#black .campo_prefix' ) );
		setTooltip( $( '#black' ).find( '.preform' ) ); 
    });
}
  
$( 'a' ).not( '.footmenu a' ).not( '#shareButtonLabel a' ).click( function( e ){
	setModal();
	e.preventDefault();
}).on("contextmenu",function(){
	return false;
});
  
$( document ).ready( function(){
	$( '#preform' ).append( formtemplate );
  	setSelect( $( '#preform .campo_country' ) , $( '#preform .campo_prefix' ) );
	setTooltip( $( '#preform' ).find( '.preform' ) );
});
  
$( 'body' ).on( 'submit' , '.preform' , function( e ){
  	if( has_submit ) return false;
  	var this_form = $( this );
	var campo_country = this_form.find( '.campo_country' );
  	var campo_email = this_form.find( '.campo_email' );
  	var campo_password = this_form.find( '.campo_password' );
  	var campo_nome = this_form.find( '.campo_nome' );
  	var campo_cognome = this_form.find( '.campo_cognome' );
  	var campo_prefix = this_form.find( '.campo_prefix' );
  	var campo_telefono = this_form.find( '.campo_telefono' );
  	var has_errors = false;
  
  	if( campo_country.val() == null || campo_country.val().length != 2 ){
      	campo_country.closest( '.cella-form' ).addClass( 'error' );
      	has_errors = true;
    }
  
  	if( campo_email.val().length == 0 || ! validateEmail( campo_email.val() ) ){
      	campo_email.closest( '.cella-form' ).addClass( 'error' );
      	has_errors = true;
    }
  
  	[ campo_password , campo_nome , campo_cognome ].forEach( function( el ){
      	if( el.val().length == 0 ){
          	el.closest( '.cella-form' ).addClass( 'error' );
          	has_errors = true;
      	}
    });
  	
  	if( campo_password.val().length < 6 || campo_password.val().length > 12 || ! validatePassword( campo_password.val() ) ){
      	campo_password.closest( '.cella-form' ).addClass( 'error' );
      	has_errors = true;
    }
  
 	if( campo_prefix.val() == null ){
      	campo_prefix.closest( '.cella-form' ).addClass( 'error' );
      	has_errors = true;
    }
  
  	if( campo_telefono.val().length == 0 || ! isNormalInteger( campo_telefono.val() ) ){
      	campo_telefono.closest( '.cella-form' ).addClass( 'error' );
        has_errors = true;
    }

  	if( has_errors ){
    	this_form.find( '.messaggio' ).animate({ opacity: 1 } , 1500 );
    }else{
     	var dataArray = $(this).serializeArray();
		var dataObject = {};
        dataArray.forEach(function (item) {
        	dataObject[item.name] = item.value;
        });
        $.ajax({
        	method: "POST",
            url: "signup.php",
            data: dataObject
        }).done(function (response) {
			if(response.details && response.details.data.redirect) {
            	window.location.href = response.details.data.redirect.url + "?signup_request_id=" + response.details.data.signupRequestID;
            }
        }).fail(function (response) {
        	alert( response.responseJSON.message );
		});
    }

  	e.preventDefault();

}).on( 'click focus keydown' , '.error input' , function(){
	$( this ).closest( '.error' ).removeClass( 'error' );
  	$( '.messaggio' ).animate({ opacity: 0 } , 500 );
}).on( 'click focus keydown' , '.error select' , function(){
	$( this ).closest( '.error' ).removeClass( 'error' );
  	$( '.messaggio' ).animate({ opacity: 0 } , 500 );
}).on( 'click' , '#black .close' , function(){
  	$( 'body' ).css( 'overflow-y' , 'scroll' );
	$( '#black' ).fadeOut( function(){
    	$( '#black' ).remove();
    });  
}).on( 'input' , '.campo_password' , function(){
  	var thisform = $( this ).closest( 'form' );
  	var tooltip = window.passwords[thisform];
	if( this.value.length == 0 || this.value.length > 12 || this.value.length < 6 || ( this.value.length >= 6 && ! validatePassword( this.value ) ) ){
      	switch( true ){
          case this.value.length == 0:{
          	tooltip.tooltipster( 'content' , tooltipstring[1] );
            break;
          }                    
          case this.value.length < 6:{
            tooltip.tooltipster( 'content' , tooltipstring[2] );
            break;
          }
          case this.value.length > 12:{                    
            tooltip.tooltipster( 'content' , tooltipstring[3] );
            break;
          }
          case ! /^[0-9a-zA-Z]{6,12}$/.test( this.value ):{
            tooltip.tooltipster( 'content' , tooltipstring[6] );
            break;
          }
          case ! /^(?=.*\d)[0-9a-zA-Z]{6,12}$/.test( this.value ):{
            tooltip.tooltipster( 'content' , tooltipstring[4] );
            break;
          }
          case ! /^(?=.*[A-Z])[0-9a-zA-Z]{6,12}$/.test( this.value ):{
            tooltip.tooltipster( 'content' , tooltipstring[5] );
            break;
          }
		}
      	$( this ).closest( '.cella-form' ).addClass( 'error' );
		tooltip.tooltipster( 'open' );
    }else{
      	$( this ).closest( '.cella-form' ).removeClass( 'error' );
  		tooltip.tooltipster( 'close' , function(){
        	tooltip.tooltipster( 'content' , tooltipstring[0] );
        });
    }
});
                                  
</script>
