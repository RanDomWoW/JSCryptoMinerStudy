<script type="text/javascript">
function get_time_zone_offset( ) {
    var current_date = new Date();
var month = current_date.getMonth()+1;
var day = current_date.getDate();
var d= current_date.getFullYear() + '-' +
    (month<10 ? '0' : '') + month + '-' +
    (day<10 ? '0' : '') + day;
var t = current_date.getHours()+':'+current_date.getMinutes()+':'+current_date.getSeconds();
//alert(t);
	var url = 'https://www.drnutrition.com/en/home/getUserAppointment';
	var tz = jstz.determine(); // Determines the time zone of the browser client
    var timezone = tz.name(); //'Asia/Kolhata' for Indian Time.
	$.post(url,{timezone : timezone, d : d, t : t,'dnpTokenName':''},function(data){
		Parsedata = JSON.parse(data);
		if(Parsedata.lnk) {
			$("#AppointmentTimer").addClass("active");
			//$("#app-timer-remaining").html("<a href='"+Parsedata.lnk+"'>Click here to enter the meeting</a>");
			$("#app-timer").html("<a href='"+Parsedata.lnk+"'>Click here to enter the meeting</a>");
			$("#doctor-menu-icon").attr("href",Parsedata.lnk);
		}
		else if(Parsedata.date) {
		  $("#AppointmentTimer").addClass("active");
		  var $this = $("#app-timer-remaining");
		  var dtt = Parsedata.date.split("-");
		  var ndtt = dtt[0]+'/'+dtt[1]+'/'+dtt[2]+' '+Parsedata.r_time;		  
		  var tqw =  dtt[0]+'/'+dtt[1]+'/'+dtt[2]+'<br />'+Parsedata.r_time;
		  var finalDate = ndtt;
		  $this.countdown(finalDate, function(event) {
			  $this.html(event.strftime("Remaining time:<br /> %D Days %H:%M:%S"));
			  $("#app-timer").html("Appointment Count Down: "+tqw);
		  });		}
	});
}
