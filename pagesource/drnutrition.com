function submitCart(elem)
{
	setTimeout("doSubmitCart("+elem+")",300);
}
var pushed_ids = [];
function doSubmitCart(elem,id,name,lang,price)
{
	var FormObject = $('#cart-form-'+elem);
	var FormAction = FormObject.attr("action")+'?ajax=1';
	var FormBtn = $('.cart-add-btn-'+elem);
	//$(".shadow-menu-white").fadeIn("fast");
	$('.top-cart-loader').fadeIn("fast");
	if(FormBtn.hasClass("active")) {
		removeFromCart(id,elem);
	}
	else {
		FormBtn.attr("disabled","disabled");
		var FormBtnText = FormBtn.html();
		if(FormBtnText != "")
		FormBtn.html('Loading...');
		if(isInArray(pushed_ids,id)) {
			//alert('pushed');
		}
		else {
			var cart_ids = [];
			cart_ids.push(id);
			pushed_ids.push(id);
			dataLayer.push({'event': 'add_to_cart','id':cart_ids,'name':name,'lang':lang,'price':price,'currency':'AED','productType':'product'});
		}
		$.ajax({
			url:FormAction,
			type:"POST",
			data:FormObject.serialize(),
			success:function(data) {
				var ParseData = JSON.parse(data);
				if(ParseData.result == 1) {
					if(!FormBtn.hasClass("active") && !FormBtn.hasClass("addToCart_1")) {
						FormBtn.addClass("active");
					}
					$('.items-in-cart').fadeIn("fast").addClass("scale").html(ParseData.no_items);
					$("#webItemNumbers").html(ParseData.no_items);
					setTimeout("removeScale()",1000);
					successCartLoader();
				}
				else {
					if(ParseData.redirect_link) {
						window.location = ParseData.redirect_link;
					}
					else {
						alert(ParseData.message);
					}
				}
				setTimeout("hideCartLoader()",1500);
				//$(".shadow-menu-white").fadeOut("fast");
				FormBtn.removeAttr("disabled");

				if(FormBtn.hasClass("addToCartFromCarousel")){
					FormBtn.html("Remove from card");
				}
				else if(FormBtnText != "")
				FormBtn.html(FormBtnText);
			}
		});
	}
}
function isInArray(array, search)
{
    return array.indexOf(search) >= 0;
}

function fadeCartLoader() {
	$('.top-cart-loader').fadeIn("fast");
}
function successCartLoader()
{
	$('.top-cart-loader').find(".inner-loader").html("<img src='https://assets.drnutrition.com/front/images/mobile/menu/storiesA.png' />");
}
function hideCartLoader()
{
	$('.top-cart-loader').fadeOut("fast",function(){
		$('.top-cart-loader').find(".inner-loader").html("<img src='https://assets.drnutrition.com/front/images/325.GIF' />")
	});
}

function removeScale() {
	$('.items-in-cart').removeClass("scale");
}

function removeFromCart(id,elem)
{
	var FormAction = 'https://www.drnutrition.com/en/cart/removeProduct/'+id+'?ajax=1';
	fadeCartLoader();
	var FormBtn = $('.cart-add-btn-'+elem);

	FormBtn.attr("disabled","disabled");
	var FormBtnText = FormBtn.html();
	if(FormBtnText != "")
	FormBtn.html('Loading...');

	$.ajax({
		url:FormAction,
		type:"GET",
		success:function(data) {
			var ParseData = JSON.parse(data);
			if(ParseData.result == 1) {
				if(FormBtn.hasClass("active")) {
					FormBtn.removeClass("active");
						if(FormBtn.hasClass("addToCartFromCarousel")){
							FormBtn.html("Add to cart");
						}
				}
				successCartLoader();
				$('.items-in-cart').fadeIn("fast").addClass("scale").html(ParseData.no_items);
				$("#webItemNumbers").html(ParseData.no_items);
				setTimeout("removeScale()",1000);
			}
			else {
				alert(ParseData.message);
			}
			setTimeout("hideCartLoader()",1500);
		}
	});
}
function deleteFromCart(rid)
{
	var FormAction = 'https://www.drnutrition.com/en/cart/remove/'+rid+'?ajax=1';
	fadeCartLoader();
	$("#cartRow-row-"+rid).remove();
	$.ajax({
		url:FormAction,
		type:"GET",
		success:function(data) {
			var ParseData = JSON.parse(data);
			if(ParseData.result == 1) {
				successCartLoader();
				reloadCart();
				$('.items-in-cart').fadeIn("fast").addClass("scale").html(ParseData.no_items);
				$("#webItemNumbers").html(ParseData.no_items);
				setTimeout("removeScale()",1000);
			}
			else {
				alert(ParseData.message);
			}
			setTimeout("hideCartLoader()",1500);
		}
	});
}

var xhr;
function updateCart()
{
	var FormObject = $('#update_cart_form');
	fadeCartLoader();
	var FormAction = FormObject.attr("action")+'?ajax=1';
	if(xhr && xhr.readyState != 4){
          xhr.abort();
    }
	//alert(FormAction);
	xhr = $.ajax({
		url:FormAction,
		type:"POST",
		data:FormObject.serialize(),
		success:function(data) {
			var ParseData = JSON.parse(data);
			if(ParseData.result == 1) {
				successCartLoader();
				$('.items-in-cart').fadeIn("fast").addClass("scale").html(ParseData.no_items);
				$("#webItemNumbers").html(ParseData.no_items);
				$("#CartLoadingContainer").html(ParseData.content);
				checkCartQtyKeyUp();
				setTimeout("removeScale()",1000);
			}
			else {
				alert(ParseData.message);
			}
		setTimeout("hideCartLoader()",1500);
	}
});
}

function reloadCart()
{
	var FormAction = 'https://www.drnutrition.com/en/cart/reload';
	if(xhr && xhr.readyState != 4){
          xhr.abort();
    }
	fadeCartLoader();
	//alert(FormAction);
	xhr = $.ajax({
		url:FormAction,
		type:"GET",
		success:function(data) {
			var ParseData = JSON.parse(data);
			if(ParseData.result == 1) {
				successCartLoader();;
				$('.items-in-cart').fadeIn("fast").addClass("scale").html(ParseData.no_items);
				$("#webItemNumbers").html(ParseData.no_items);
				$("#CartLoadingContainer").html(ParseData.content);
				checkCartQtyKeyUp();
				setTimeout("removeScale()",1000);
			}
			setTimeout("hideCartLoader()",1500);
		}
	});
}
