<!--判断电脑还是首页显示-->
<script>
var clientType = function(){
    var type = new Array("Android","iPhone","Windows Phone","iPod","BlackBerry","MeeGo","SymbianOS");
    var info=navigator.userAgent;
    for(var i=0; i<type.length; i++){
	if(info.indexOf(type[i])>0){
	    return "Mobile";
	}
    }
    return "PC";
}

var pcHref="http://www.szhrss.gov.cn/";
var moblieHref="http://www.szhrss.gov.cn/ydmh/";


var sClientType=clientType();
if(sClientType=="Mobile"){
    location.href=moblieHref;                  
}
</script>  
