function AcquisitionConditional(){

	$('.branded-card-header__heading').html(function(){
		var acquisition_message = $(this).html();
		var acquisition_subMessage = acquisition_message.match(/\{[^\}]+\}|[^\s]+/g)[0];
		newAcquisition_subMessage = acquisition_subMessage.replace("{", " ").replace("}", " ");
		newAcquisition_message = acquisition_message.replace(acquisition_subMessage, " ");	


		//console.log('is there anything? ' + acquisition_subMessage);
		//console.log('FULL TEXT is:' + acquisition_message );
		//console.log('NEW:' + newAcquisition_message );
		//console.log('On {} is:' + acquisition_subMessage );
