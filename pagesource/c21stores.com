		// CLONE OBJECT FOR MOBILE
		$( ".promo" ).prepend('<div class="jm-shipping-plcc-acquisition-mobile jmrv-mobile"><a href="https://www.c21stores.com/pages/shipping-information">' + newAcquisition_subMessage + '</a></div>');


	    return $(this).html().replace(acquisition_message, newAcquisition_message + '<div class="jm-shipping-plcc jm-shipping-plcc-acquisition"><a href="https://www.c21stores.com/pages/shipping-information">' + newAcquisition_subMessage + '</a></div>');

