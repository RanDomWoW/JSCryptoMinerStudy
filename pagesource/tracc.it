<script>
$(document).ready(function(){
	  var last_index = -1;
	  $(".anchor").click(function(e){
	    e.preventDefault();
	 
	    var id     = $(this).attr("href");
	    var offset = $(id).offset();

		var index = $(this).parent('li').index();
		var margin_top = 105;
		
		
		if(index == 0) margin_top = 65;
	    if(index == 0 && index != last_index) margin_top = 50;
		if(!$('#sub-menu').hasClass('f-nav')){
			  margin_top += 40;
		}
		
		last_index = index;
		console.log(offset.top + ' ' + margin_top);
	    $("html, body").animate({
	      scrollTop: offset.top - margin_top
	    }, 500);
	  });

	  var nav = $('#sub-menu');
	  var nav_offset = nav.offset();
	  
	    $(window).scroll(function () {
	    	
	        if ($(this).scrollTop() + 40 > nav_offset.top) {
	            nav.addClass("f-nav");
	        } else {
	            nav.removeClass("f-nav");
	        }
	    });

	 if($(window).scrollTop() > nav.offset().top){
		 nav.addClass("f-nav");
	 }
});
</script></div>
