    function setSearchManufacturRul() {
        //获取空气搜索text
        var swSearchName = $("#txt_searchManufactur").val();
        if (swSearchName != '') {
            $("#txt_searchManufactur").attr("placeholder", swSearchName);
            $("#a_searchManufactur").attr("href", "http://es.mkc.ckcest.cn/Tool/KeywordCloud?keyword=" + swSearchName);
        } else {
            $("#a_searchManufactur").attr("href", "http://es.mkc.ckcest.cn/Tool/KeywordCloud?keyword=人工智能");
        }
    }

    /**
     * 设置气象搜索链接跳转
     * @param swName
     */
    function setSearchWeatherRul(typeName) {
        //获取空气搜索text
        var swSearchName = $("#txt_searchWeather").val();

        switch (typeName) {
            case 0:
                if (swSearchName == "") {
                    swSearchName = "大气科学";
                }
                $("#txt_searchWeather").attr("placeholder", "大气空气");
                $("#a_searchWeather0").attr("href", "http://meteor.ckcest.cn/analysis/trend/?q=" + swSearchName);
                break;
            case 1:
                var param = swSearchName.split(" ");
                if (param <= 0) {
                    $("#txt_searchWeather").attr("placeholder", "比较分析:至少输入2个词, 空格隔开！");
                    $("#txt_searchWeather").focus();
                } else {
                    $("#txt_searchWeather").attr("placeholder", "大气空气");
                    window.open("http://meteor.ckcest.cn/analysis/diff/?q=" + swSearchName);
                }
        }
    }
</script>
