    /**
     *添加收藏
     * @param source
     * @param title
     * @param url
     * @param callback
     */
    function addUserFav(data, callback) {
        var p = "http://www.ckcest.cn/userfav/add";
        if (data.source == undefined || data.source == "") {
            data.source = "其他";
        }
        if (data.title == undefined && data.title == "") {
            data.title = $("title").text();
        }
        if (data.url == undefined || data.url == "") {
            data.url = window.location.href;
        }

        $.ajax({
            url: p,
            data: data,
            type: "GET",
            dataType: "json",
            success: function (req) {
                if (req.logIn == false) {
                    common_util.windowOpen($("#top-login").attr("href"));
                    return;
                }
                if (callback != null && callback != undefined) {
                    callback(req);
                }
            }
        });
    }

    /**
     *获取收藏
     */
    function getUserFav(data, callback) {
        var p = "http://www.ckcest.cn/userfav/getfavsfromurl";

        if (data == null || data.length == 0) {
            return;
        }
        $.ajax({
            url: p,
            data: {urls: data},
            type: "GET",
            dataType: "json",
            success: function (req) {
                if (req.logIn == false) {
                    return;
                }
                if (callback != null && callback != undefined) {
                    callback(req);
                }
            }
        });
    }

    //注册通用收藏
    function initAddUserFav() {
        $("body a[userfav='true']").click(function () {
            var $a = $(this);

            var source = $a.attr("favsource");
            var title = $a.attr("favtitle");
            var url = $a.attr("favurl");

            var data = {
                source: source,
                title: title,
                url: url
            }
            addUserFav(data, function (e) {
                if (e.obj == "del") {
                    $a.removeClass("collec_on");
                    $a.addClass("collec");
                    $a.attr("title", "收藏");
                }
                if (e.obj == "add") {
                    $a.removeClass("collec");
                    $a.addClass("collec_on");
                    $a.attr("title", "点击取消收藏");
                }
            });
        });
    }

    //注册通用获取当前页面搜索功能
    function initGetUserFav() {
        var $as = $("body a[userfav='true']");
        if ($as == null && $as.length == 0) {
            return;
        }
        var urls = new Array();

        for (var i = 0; i < $as.length; i++) {
            var url = $($as[i]).attr("favurl");
            if (url != null && url != "") {
                urls.push(url);
            }
        }
        if (urls.length == 0) {
            return;
        }
        var _urls = urls.join(",");
        getUserFav(_urls, function (data) {
            var urls = data.obj;
            if (urls == null || urls.length == 0) {
                return;
            }
            for (var i = 0; i < urls.length; i++) {
                for (var k = 0; k < $as.length; k++) {
                    var $a = $($as[k]);
                    if ($a.attr("favurl") == urls[i]) {
                        $a.removeClass("collec");
                        $a.addClass("collec_on");
                        $a.attr("title", "点击取消收藏");
                        break;
                    }
                }
            }
        });
    }
</script>






