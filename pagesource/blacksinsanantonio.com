    <script src="/assets/js/lib/typeahead.bundle.min.js"></script>
    <script src="/assets/js/search/utility.js"></script>
    <script src="/assets/js/search/suggest.js"></script>
    <script src="/assets/js/search/jvectormap/jquery-jvectormap-2.0.3.min.js"></script>
    <script src="/assets/js/modules/event/upcoming/event.upcoming.js"></script>
    <script src="/assets/js/modules/event/upcoming/upcoming.auto.js"></script>
