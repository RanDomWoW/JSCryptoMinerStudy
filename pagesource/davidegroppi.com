<script>

$( ".menu-item-has-children" ).click(function() {

	if($( "#search-box" ).css('display') == 'block'){
		$( "#search-box" ).slideToggle();
		}
	
	$( ".menu-selected" ).slideToggle();
	
	if($(this).find('.menu-selected').length != 0){
		$( ".menu-selected" ).removeClass("menu-selected");
	} else {
		$( ".menu-selected" ).removeClass("menu-selected");
		$(this).find( ".sub-menu" ).slideToggle( "slow");
		$(this).find( ".sub-menu" ).addClass("menu-selected");
	}
});



$( "nav h1" ).first().click(function() {
	$( "#menu-principale" ).slideToggle();
	});



$(".search-menu").click(function () {
	
	$( ".menu-selected" ).slideToggle();
	$( ".menu-selected" ).removeClass("menu-selected");
	$("#search-box").slideToggle( "slow");
	
	});


$(".back").click(function () {
	
	$("#search-box").slideToggle( "slow");
	
	});



jQuery(document).ready(function($){

	$('a[href^="#"]').on('click',function (e) {

	    e.preventDefault();



	    var target = this.hash;

	    $target = $(target);



	    $('html, body').stop().animate({

	        'scrollTop': $target.offset().top

	    }, 900, 'swing', function () {

	        window.location.hash = target;

	    });

	});
	


});

$('#lampadacampo').css('display','none');


