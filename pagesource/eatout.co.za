(function($) {
    wp.hooks.addAction('facetwp/refresh/checkboxes', function($this, facet_name) {
        var selected_values = [];
        $this.find('.facetwp-checkbox.checked').each(function() {
            selected_values.push($(this).attr('data-value'));
        });
        FWP.facets[facet_name] = selected_values;
    });

    wp.hooks.addAction('facetwp/ready', function() {
        $(document).on('click', '.facetwp-facet .facetwp-checkbox', function() {
            $(this).toggleClass('checked');
            var $facet = $(this).closest('.facetwp-facet');

            // Static facet (the current facet uses the "or" operator)
            if ('or' == $facet.attr('data-operator')) {

                // Refresh facet anyways if empty
                if (0 < $facet.find('.facetwp-checkbox.checked').length) {
                    FWP.static_facet = $facet.attr('data-name');
                }
            }

            FWP.refresh();
            FWP.static_facet = null;
        });
    });
})(jQuery);
</script>
<script>
(function($) {
    wp.hooks.addAction('facetwp/refresh/date_range', function($this, facet_name) {
        FWP.facets[facet_name] = [
            $this.find('.facetwp-date-min').val() || '',
            $this.find('.facetwp-date-max').val() || ''
        ];
    });

    wp.hooks.addAction('facetwp/ready', function() {
        $(document).on('facetwp-loaded', function() {
            $('.facetwp-date').datetime();
        });

        $(document).on('datetime-close', function() {
            FWP.refresh();
        });
    });
})(jQuery);
</script>
