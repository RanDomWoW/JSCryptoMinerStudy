<script>
    var context = {};

    jQuery(function() {
        var feedUrl = "https://blog.octanner.com/aem-pull";
        if(feedUrl !== "") {
            jQuery.ajax({
                url: feedUrl,
                success: function(data) {
                    if(data) {
                        context["featured-post-right"] = data;
                        context["featured-post-left"] = data[0];
                        jQuery("#left-post-area").empty();
                        doTemplate("#post-left", context, "#left-post-area");
                        doTemplate("#post-right", context, ".featured-post-right");
                    }
                },
                dataType: "jsonp",
                jsonpCallback: "aemCallback"
            });
        }
    });
    function doTemplate(templateName, data, appendTo) {
        if(!jQuery(templateName) || jQuery(templateName).length > 0) {
            var templateScript = jQuery(templateName).html();
            var template = Handlebars.compile(templateScript);
            jQuery(appendTo).append(template(data));
        }
        return template;
    }
</script>


