<script type="text/javascript">
    function wrapWindowByMask5() {
        //화면의 높이와 너비를 구한다.
        var maskHeight = $(document).height();
        var maskWidth = $(window).width();

        //마스크의 높이와 너비를 화면 것으로 만들어 전체 화면을 채운다.
        $('#mask5').css({ 'width': maskWidth, 'height': maskHeight });

        //애니메이션 효과 - 일단 1초동안 까맣게 됐다가 80% 불투명도로 간다.
        $('#mask5').fadeIn(500);
        $('#mask5').fadeTo("slow", 0.8);

        //윈도우 같은 거 띄운다.
        $('.window5').slideDown("1000");
    }

    $(document).ready(function () {
        //검은 막 띄우기
        $('.openMask5').click(function (e) {
            e.preventDefault();
            wrapWindowByMask5();
        });

        //닫기 버튼을 눌렀을 때
        $('.window5 .close5').click(function (e) {
            //링크 기본동작은 작동하지 않도록 한다.
            e.preventDefault();
            $('#mask5, .window5').slideUp("slow");
        });

        //검은 막을 눌렀을 때
        $('#mask5').click(function () {
            $(this).hide();
            $('.window5').slideUp("slow");
        });

    });
</script>

