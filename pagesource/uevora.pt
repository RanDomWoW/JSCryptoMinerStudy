     <script type="text/javascript">

       var slide_activo = 1;
       var num_slides = 7;
       var intervalID = 0;

       //num_slide = 0 - faz o cycle para o proximo slide
       //num_slide > 0 - faz o cycle para o slide num_slide
       function cycle_slides(num_slide){
       
        //verifica o num_slide		
        if (num_slide==0 && num_slides > 1) {
	   if (slide_activo < num_slides) {
	      num_slide = slide_activo + 1 ;
	   } else {
	      num_slide = 1 ;
	   }
	}
	
	slide_activo = num_slide;

        if (num_slide > 0) {
	   $('.slide_pointer').removeClass('slide_pointer_selected');	
	   $('#slide_pointer_link_' + num_slide).addClass('slide_pointer_selected');

	   $('#slide_' + num_slide).css('top',0);
	   $('#slide_' + num_slide).stop().animate({opacity: 1},2000,function(){});

	   $('.slide').each(function(index,element){
		if ($(this).attr('id') != 'slide_' + num_slide) {
	   	   $('#' + $(this).attr('id')).stop().animate({opacity: 0},1000,function(){
		      $('#' + $(this).attr('id')).stop().css('top',400);
    	   	   });
		}
	   });
	   
	}
       		
       }


       function init_cycle(){
          if (intervalID != 0) {	
	     clearInterval(intervalID);	     
	  }
          intervalID = setInterval(function(){
	  cycle_slides(0);
	  },5000);
       }


       $(document).ready(function(){
       
	  init_cycle();

	  $('.slides').mouseover(function(){
		clearInterval(intervalID);
	  });    
	  $('.slides').mouseout(function(){
		init_cycle();
	  });    
	  $('.slide_pointer').mouseover(function(){
		clearInterval(intervalID);
		var num_slide_pointer = parseInt($(this).attr('href').replace('#slide_',''));
		cycle_slides(num_slide_pointer);
	  });    
	  $('.slide_pointer').mouseout(function(){
		init_cycle();
	  });    
	  
       });

     </script>
     
     
