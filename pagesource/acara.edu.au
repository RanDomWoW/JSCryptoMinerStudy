    <script type="text/javascript">
        // create youtube player
        var player;
        var vStatus = 0;
        function onYouTubeIframeAPIReady() {

            player = new YT.Player('player', {
                height: '390',
                width: '640',
                events: {
                    'onReady': onPlayerReady,
                    'onStateChange': onPlayerStateChange
                }
            });
        }
        // To autoplay video un comment the below line
        function onPlayerReady(event) {
            //event.target.playVideo();
        }
        // when Playes State Changes say Pause, Play or Stop
        function onPlayerStateChange(event) {
            vStatus = event.data; //update video status
            if (vStatus === 1) {
                CarouselStop();
            }
            if (vStatus === 0) {
                $(".carousel").carousel({
                    interval: 10000,
                });
            }
        }
        // To Pause the Video (when the Carousel Indicators are clicked)
        function stop() {
            if (vStatus !== 0) {
                //To Paues the video
                $('#player')[0].contentWindow.postMessage('{"event":"command","func":"' + 'pauseVideo' + '","args":""}', '*');
            }
            // To enable carousel scroll
            $(".carousel").carousel({
                interval: 10000,
            });
            return false;
        }


    </script>

