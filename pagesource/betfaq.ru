        var timer = 0;
        function updateTime ()
        {
            timer++;
        }
        setInterval (updateTime,1000);

        // dont' forget setup time here
        clock.setTime (11986);
        $('#slider').carouFredSel({
            auto: 10000,
            width: 400,
            height: 356,
            items: {
                visible: 1,
                width: 400,
                height: 356
            },
            scroll: {
                items: 1,
                pauseOnHover: true,

                onBefore: function(data) {
                    $('#slider-overlay').fadeIn();
                },
                onAfter: function(data) {
                    var slide = data.items.visible,
                        p = parseInt (slide.attr ('data-time')) - timer,
                        time = p <= 0 ? 0 : p;

                    clock.setTime (time);

                    $('#slider-overlay').fadeOut();


                    $('.overlay .team1').text(slide.attr('data-team-1'));
                    $('.overlay .team2').text(slide.attr('data-team-2'));

                    $('#slider-kind').text(slide.attr('data-kind'));
                    $('.overlay .league').text(slide.attr('data-league'));
                    $('.overlay ins').attr('class',slide.attr('data-css'));

                    $('.overlay .forecast').attr('href', slide.attr('rel'));
                }
            },
            pagination: '#slider-pager'
        });
    });
