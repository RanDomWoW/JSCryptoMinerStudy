<script language="javascript">
	$(document).ready(function() {
		
		var lastScrollTop = 0;
		var scrollFlag = false;
		$(window).scroll(function(event){
			
			if( $(window).scrollTop()>1 ){
				$(".header").addClass('box_shadow');
			}else {
				$(".header").removeClass('box_shadow');
			}
			
			var st = $(this).scrollTop();
			if( st>36 ) scrollFlag = true; else  scrollFlag = false;
			if (scrollFlag && st > lastScrollTop){
				$(".header").addClass('small');
			} else {
				$(".header").removeClass('small');
			}
			lastScrollTop = st;
			
		});
		
		/*-----------------------------------------------*/

		$('.user-name div').click(function(){
			$('.header-user').toggleClass('on');	
		})
		
		/*-----------------------------------------------*/
		
		$('.login-form .form-btn').click(function(){
			$('.login').removeClass('open');	
		});
		$('.login-request').click(function(){
			$('.login').addClass('open');	
		});
		
		/*-----------------------------------------------*/
		
		$('.menu-open-symb').click(function(){
			$('.header-menu').toggleClass('open');	
		});

		/*-----------------------------------------------*/

		$('.reply_set').click(function(){
			var fatherId=$(this).attr('id');
			$('#father').val(fatherId);
			var replyName = $(this).parent(this).parent(this).find('.comment-info h5').text();
			$('#reply_show').html('<span> پاسخ به نظر '+replyName+' </span>');
		});
		$('.comment-reset').click(function(){
			$('#father').val(0);
			$('#reply_show').html('');
		});
		
		/*-----------------------------------------------*/
		
		$('.more-item div').click(function(){
			$(this).parent(this).parent(this).addClass('open');
		});
		
	
	});

