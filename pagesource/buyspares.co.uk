<script>
	$(document).ready(function(){
		var winWidth = $(window).width();

		var mylist = $('.home-brands');
		var listitems = mylist.children('div').get();
		var newItems = listitems;

		if (winWidth < 449) {
			var newItems = listitems.slice(0,9)
			$('.home-brands > div:lt(9)').addClass("active");
		} else if (winWidth >= 450 && winWidth <= 767) {
			var newItems = listitems.slice(0,8)
			$('.home-brands > div:lt(8)').addClass("active");
		} else if (winWidth >= 768 && winWidth <= 991) {
			var newItems = listitems.slice(0,14)
			$('.home-brands > div:lt(14)').addClass("active");
		} else {
			$('.home-brands > div').addClass("active");
		}

		newItems.sort(function(a, b) {
			return $(a).attr('title').toUpperCase().localeCompare($(b).attr('title').toUpperCase());
		})
		$.each(newItems, function(idx, itm) { mylist.append(itm); });
	});
</script>


