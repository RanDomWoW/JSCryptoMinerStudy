        $(document).ready(function () {
            var initiallyVisibleBanners = $('.icampv').filter(function () {
                if ($(this).hasClass('slick-slide') && !$(this).hasClass('slick-active')) {
                    return false;
                } else {
                    return true;
                }
            });

            initiallyVisibleBanners.each(function (i, element) {
                if (typeof $(element).data('viewed') !== 'undefined') {
                    $(element).data('viewed').gtm = true;
                }
            });

            var initiallyVisibleBannerData = initiallyVisibleBanners.map(function () {
                return splitBannerTrackingString($(this).data('icampv'));
            }).get();

            if (initiallyVisibleBannerData.length) {
                var content = Object.keys(initiallyVisibleBannerData).map(function (i) {
                    return getValuesAsArray(initiallyVisibleBannerData[i]).join(':');
                });

                for (var i in content) {
                    dataLayer.push({
                        event: 'ad_view',
                        ad_view: content[i]
                    });
                }
            }

            // view tracking for banners in sliders that aren't initially visible
            $('.slick-slider').on('afterChange', function (e, slick, currentSlide) {
                var laterVisibleBanners = $('.slick-slide.slick-active', e.currentTarget).filter(function () {
                    if (typeof $(this).data('viewed') !== 'undefined') {
                        return $(this).data('viewed').gtm === false;
                    }
                });

                laterVisibleBanners.each(function (i, element) {
                    if (typeof $(element).data('viewed') !== 'undefined') {
                        $(element).data('viewed').gtm = true;
                    }
                });

                var laterVisibleBannersData = laterVisibleBanners.map(function (i, ele) {
                    return splitBannerTrackingString($(ele).data('icampv'));
                }).get();

                if (laterVisibleBannersData.length) {
                    var content = Object.keys(laterVisibleBannersData).map(function (i) {
                        return getValuesAsArray(laterVisibleBannersData[i]).join(':');
                    });

                    dataLayer.push({
                        event: 'ad_view',
                        ad_view: content
                    });
                }
            });

            // Click tracking
            $(document).on('click', '.icampc a', function (e) {
                var trackingString = splitBannerTrackingString($(e.currentTarget).parents('.icampv').data('icampv'));
                var content = getValuesAsArray(trackingString).join(':');

                dataLayer.push({
                    event: 'ad_click',
                    ad_click: content
                });
            });

            function splitBannerTrackingString(_string) {
                var splittedString = _string.split('/');
                var trackingMap = {};
                trackingMap['Account']        = splittedString[0];
                trackingMap['Partner']        = splittedString[1];
                trackingMap['Kampagne']       = splittedString[2];
                trackingMap['Marke']          = splittedString[3];
                trackingMap['Page']           = splittedString[4];
                trackingMap['BannerName']     = splittedString[5];
                trackingMap['AdditionalInfo'] = splittedString[6];

                return trackingMap;
            }

            $(document).on('click', '.track-emcs', function(){
                var $this = $(this);
                var widgetContainer = $this.parents('.recommendation-list:first');
                var widgetData = {
                    emcs0: widgetContainer.data('wid'),
                    emcs1: widgetContainer.data('emcs1'),
                    emcs2: $this.data('emcs2'),
                    emcs3: $this.data('emcs3')
                };
                sessionStorage.setItem('crosssellwidgetdata', JSON.stringify(widgetData));
            });

            if (sessionStorage.getItem('crosssellwidgetdata')) {
                var widgetData = JSON.parse(sessionStorage.getItem('crosssellwidgetdata'));
                    dataLayer.push({
                        "widgetData": widgetData,
                        "event": "widget-click"
                    });
                sessionStorage.removeItem('crosssellwidgetdata');
            }

            function getValuesAsArray(object) {
                var array = [];
                try {
                    Object.keys(object).forEach(function(key) { array.push(object[key]); });
                } catch (e) {

                }

                return array;
            }

            // addToCart tracking
            $(document).on('click', '[data-gtm^="add-"]', function () {
                var productData = GoogleTagManager.createGTMProductData(this);
                if ((/mobi/i).test(navigator.userAgent)) {
                    GoogleTagManager.writeToDataLayerMobile(productData, 'addToCart');
                } else {
                    GoogleTagManager.writeToDataLayer(productData, 'addToCart');
                }
            });

            // addAllFromNotePad tracking
            $(document).on('click', '[data-gtm="addAll"]', function () {
                var productsData = $('*[data-gtm^="add-"]').map( function () {
                    return GoogleTagManager.createGTMProductData(this);
                }).each(function(i) {
                    if ((/mobi/i).test(navigator.userAgent)) {
                        GoogleTagManager.writeToDataLayerMobile(productData, 'addToCart');
                    } else {
                        GoogleTagManager.writeToDataLayer(productData, 'addToCart');
                    }
                });
            });

            // Remove from cart tracking
            $(document).on('click', '[data-gtm^="remove-"]', function () {
                var productData = GoogleTagManager.createGTMProductData(this);
                productData.quantity = null;

                GoogleTagManager.writeToDataLayer(productData, 'removedFromCart');
                updateDataLayerCartObject('remove', productData);
            });

            // Change product quantity tracking
            $(document).on('change', '[data-gtm^="update-"]', function () {
                var productData = GoogleTagManager.createGTMProductData(this);

                if ((/mobi/i).test(navigator.userAgent)) {
                    GoogleTagManager.writeToDataLayerMobile(productData, 'cartModified');
                    updateDataLayerCartObject('update', productData);
                } else {
                    GoogleTagManager.writeToDataLayer(productData, 'cartModified');
                    updateDataLayerCartObject('update', productData);
                }
            })
        });

        function updateDataLayerCartObject(type, productData) {
            var cartEntries = window.dataLayer[0].cart.cartEntries.entries;

            // Update quantity in the dataLayer.cart entry
            $.each(cartEntries, function (index, entry) {
                if (entry.id === productData.id) {
                    if (type === 'remove') {
                        cartEntries.splice(index, 1);
                    } else if (type === 'update') {
                        entry.quantity = productData.quantity;
                    }
                    return false;
                }
            });
        }
