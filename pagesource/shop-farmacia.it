        var initialllyVisibleBanners = $('.icampv').filter(function () {
            if ($(this).hasClass('slick-slide') && !$(this).hasClass('slick-active')) {
                return false;
            } else {
                return true;
            }
        });

        initialllyVisibleBanners.each(function (i, element) {
            if (typeof $(element).data('viewed') !== 'undefined') {
                $(element).data('viewed').econda = true;
            }
        });

        var initialllyVisibleBannerData = initialllyVisibleBanners.map(function () {
            return ([[$(this).data('icampv')]]);
        }).get();

        if (initialllyVisibleBannerData.length) {
            emospro.icampv = initialllyVisibleBannerData;
        }

        // Econda view tracking for banners in sliders that aren't initially visible
        $('.slick-slider').on('afterChange', function (e, slick, currentSlide) {
            var laterVisibleBanners = $('.slick-slide.slick-active', e.currentTarget).filter(function () {
                if (typeof $(this).data('viewed') !== 'undefined') {
                    return $(this).data('viewed').econda === false;
                }
            });

            laterVisibleBanners.each(function (i, element) {
                if (typeof $(element).data('viewed') !== 'undefined') {
                    $(element).data('viewed').econda = true;
                }
            });

            var laterVisibleBannersData = laterVisibleBanners.map(function (i, ele) {
                return ([[$(ele).data('icampv')]]);
            }).get();

            if (laterVisibleBannersData.length) {
                emospro.icampv = laterVisibleBannersData;
                window.emosPropertiesEvent(emospro);
            } else {
                delete emospro.icampv;
            }
        });

        // Econda click tracking
        $(document).on('click', '.icampc a', function (e) {
            emospro.icampv = [[$(e.currentTarget).parents('.icampv').data('icampv')]];
            emospro.icampc = [[$(e.currentTarget).parents('.icampc').data('icampc')]];

            var url = new URL($(e.currentTarget).attr('href'));
            var marker;

            if (url.searchParams.get('i') !== null && url.searchParams.get('i') === '1') {
                marker = 'Interne Suche/';
            }

            window.emosPropertiesEvent(emospro);
        });
    }

    // Video tracking
    var videoTracker;

    $('video').on('play', function() {
        var video = this;
        var source = $($(video).children()[0]).attr('src')

        videoTracker = new econda.media.helper.HtmlVideoTracker({
            player: video,
            tracker: {
                content: source
            }
        });
    });

