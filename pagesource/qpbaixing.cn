(function() {
	function b64EncodeUnicode(str) {
		return btoa(encodeURIComponent(str).replace(/%([0-9A-F]{2})/g, function(match, p1) {
			return String.fromCharCode('0x' + p1);
		}));
	}

	$('.ex_re').on('click', function(e) {
		var $el = $(e.target)
		var link = $el.attr('href')

		if(!$el.data('url')) $el.data('url', link)

		var extra = { url: $el.data('url'), href: window.location.href }
		var encodeLink = b64EncodeUnicode($.param({url: link, eventType: 'external_redirect', eventValue: extra})).toString()
		var redirectUrl = 'http://www.baixing.com/redirect/index?r=' + encodeLink.replace('+', '-').replace('_', '/').replace('=', ',')

		$el.attr('href', redirectUrl)
	})

	// 站内广告位打点相关逻辑
	function b64DecodeUnicode(str) {
		return decodeURIComponent(atob(str).split('').map(function(c) {
			return '%' + ('00' + c.charCodeAt(0).toString(16)).slice(-2);
		}).join(''));
	}

	function decodeRedirectUrl(str) {
		str = str.replace(/\,/g, '')
		str = decodeURIComponent(b64DecodeUnicode(str))

		return str.split('&')
	}

	function getParamsFromHref(href, params) {
		function getVal(str, key) {
			var prefix = key + '='
			if (str.indexOf(prefix) === 0) {
				return str.slice(prefix.length)
			}

			return ''
		}

		if (href.indexOf('/redirect/index?r=') !== -1) {
			var str = href.split('/redirect/index?r=').pop()
			var arr = []
			try {
				arr = decodeRedirectUrl(str)
			} catch (err) {}
			for (var i = 0; i < arr.length; i++) {
				var v = arr[i]
				if (getVal(v, 'url')) {
					params.target_url = getVal(v, 'url')
				}
				if (getVal(v, 'eventValue[platform]')) {
					params.platform = getVal(v, 'eventValue[platform]')
				}
				if (!params.ad_slot_id && getVal(v, 'eventValue[ad_slot_id]')) {
					params.ad_slot_id = getVal(v, 'eventValue[ad_slot_id]')
				}
			}
		} else {
			params.target_url = href
		}

		return params
	}

	var ads = $('*[data-atype="b-x-a-d-a"]')
	ads.on('click', function onAdClick(e) {
		var params = {
			'action': 'ad_slot_click',
			'tracktype': 'event'
		}

		if ($(this).data('position')) {
			params.ad_slot_id = $(this).data('position')
		}

		var href = $(this).data('href') || $(this).attr('href')
		if (href) {
			params = getParamsFromHref(href, params)
		}

		window.tracker && tracker.evt('bxmainsite_aux', params)
	})

	var lastTop = 0
	var lastLeft = 0

	function imprAd(init) {
		ads.each(function() {
			var ad = $(this)
			var win = $(window)

			var ww = win.width()
			var wh = win.height()
			var top = win.scrollTop()
			var left = win.scrollLeft()

			var adw = Math.max.apply(null, ad.children().map(function() {
				return $(this).width()
			}))
			var adh = Math.max.apply(null, ad.children().map(function() {
				return $(this).height()
			}))
			var adTop = ad.offset().top
			var adLeft = ad.offset().left

			// TODO - 曝光判断优化(先行测试) 多帧及其他
			var adVisiable = (adTop >= top) && (adTop + adh <= top + wh) &&
				(adLeft >= left) && (adLeft + adw <= left + ww)
			var lastVisiable = (adTop >= lastTop) && (adTop + adh <= lastTop + wh) &&
				(adLeft >= lastLeft) && (adLeft + adw <= lastLeft + ww)
			// console.log(init, adVisiable, lastVisiable)
			if (adw && adh && adVisiable) {
				if (init || !lastVisiable) {
					var params = {
						'action': 'ad_slot_impr',
						'tracktype': 'event'
					}

					if ($(this).data('position')) {
						params.ad_slot_id = $(this).data('position')
					}

					var href = ad.data('href') || ad.attr('href')
					if (href) {
						params = getParamsFromHref(href, params)
					}

					window.tracker && tracker.evt('bxmainsite_aux', params)
				}
			}

			lastLeft = left
			lastTop = top
		})
	}

	var timer = null
	imprAd(true)

	if (window.addEventListener) {
		window.addEventListener('scroll', function() {
			if (timer) {
				clearTimeout(timer)
				timer = null
			}

			timer = setTimeout(imprAd, 200)
		})
	}
})();
</script>
<script type='text/javascript'>
(function() {
	var clickedAdsense = []

	$('body').on('adsense_click', function(event, data) {
		if(!data || !data.id) return false

		var id = '#' + data.id
		if(clickedAdsense.indexOf(id) === -1) {
			clickedAdsense.push(id)
		} else {
			setTimeout(function() {
				$(id).remove()
			}, 1000)
		}
	})

	// 暂时检测一下搜索广告点击状况，就不取全量了
	if (Math.random() < 0.1) {
		$('body').on('click', '.media.clearfix.adsense', function(e) {
			var offset = $(e.target).parents('.adsense').offset()
			var posY = offset.top - $(window).scrollTop()
			var posX = offset.left - $(window).scrollLeft()

			var result = {
				x: e.screenX - posX,
				y: e.screenY - posY - 120
			}

			tracker.evt('search_ad_click_position', result)
		})
	}
})();
</script>
