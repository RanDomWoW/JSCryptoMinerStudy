    <div class="container">
        <h2 class="slogan">Search. Find. Recycle.</h2>

        <div class="home-recycle-search">
            <div id="search-bar" class="search-wrap">
                <form action="/find" method="GET">
                    <span>
                        I want to recycle
                    </span>
                    
                    <input type="hidden" name="materials" data-bind="value: materialId" />

                    <div class="autosuggest-materials open" data-bind="css: { 'open': materialsFiltered().length > 0  }">
                        <input type="text" class="home-materials-input" placeholder="Aluminum Cans" data-bind="value: materialFilter, valueUpdate: 'keyup', event: { keydown: keyup, keypress: noPostBack }" />
                        
                        <div class="dropdown-wrapper">
                            <ul class="dropdown-suggestions" role="menu" data-bind="foreach: materialsFiltered">
                                <li data-bind="css: {active: $index() == $root.selectedSearch()}, scrollToMe: $index() == $root.selectedSearch()">
                                    <a href="javascript:void(0)" data-bind="text: description, click: $root.clickMaterial"></a>
                                </li>
                            </ul>
                        </div>
                    </div>

                    <span>in</span>

                    <div class="zip-input-wrap">
                        <input type="number" name="zip" class="zip-input" placeholder="Zip Code" maxlength="5" autocomplete="off">
                    </div>

                    <button type="submit" class="search">
                        <span class="sr-only">Search</span>
                    </button>
                </form>
            </div>
        </div>

