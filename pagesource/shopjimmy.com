    };

    var modeloptions = {
        url: function(phrase) {
            return "brandmodel/index/modelSayt";
        },
        
        getValue: function(element) {
            return element;
        },
        
        ajaxSettings: {
            dataType: "json",
            method: "POST",
            data: {
            dataType: "json"
            }
        },
        
        preparePostData: function(data) {
            data.brand = $("#brand_input").val();
            data.model = $("#model_input").val();
            
            return data;
        },
        
        requestDelay: 400,

        theme: "square"
    };
    
    $("#brand_input").easyAutocomplete(brandoptions);
    $("#model_input").easyAutocomplete(modeloptions);

    $('#model_form_submit').click(function(){
        $('#model_form').submit();
        
        return false;
    })

    $('#model_form').submit(function(){
        if ($('#brand_input').val() == '') {
            $('#brand_input').focus(); 
            
            return false;
        }

        if ($('#model_input').val() == '') {
            $('#model_input').focus(); 
            
            return false;
        }

        $.post(this.action, $(this).serialize(), function(data){
            if (data.url) {
                window.location = data.url;
            } else {
                alert('Hmm, we do not recognize that TV model. Please give it another shot.');
            }
        });

        return false;
    });
