                    }
                    
                    col.append(img);
                    
                    setTimeout(function(){
                        this.css({marginTop: '30px'}).animate({marginTop: '0px', opacity: 1}, 400);
                    }.bind(img), 100*i);
                    
                    img.load(function(){
                        place(++i);
                    }.bind(i));
                }
            };
            
            place(0);
