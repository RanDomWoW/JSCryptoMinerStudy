            var cols = $('.collage-container');
            
            var place = function(i) {
                if (imgs[i] != '') {
                    var img = $('<img/>').attr('src', imgs[i]);
                    
                    var cur = 0;
                    var col = null;
                    for (y=0; y<cols.length; y++) {
                        if (!col || (cur > cols.eq(y).height())) {
                            col = cols.eq(y);
