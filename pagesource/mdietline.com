<script type="text/javascript">
	function setCookie(name, value, expires, path, domain, secure) {
    var cookieStr = name + "=" + escape(value) + "; ";
    if (expires) {
        var expiresDate = new Date(new Date().getTime() + expires * 24 * 60 * 60 * 1000);
        cookieStr += "expires=" + expiresDate.toGMTString() + "; ";
    }
    cookieStr += "path=/" + "; ";
    if (domain) {
        cookieStr += "domain=" + domain + "; ";
    }
    if (secure) {
        cookieStr += "secure; ";
    }
    document.cookie = cookieStr;
    document.location.href= 'https://mdietline.com/';
  }

  $mt(window).load(function() {
    $mt("#sbHolder_countries").on("mouseenter", function(e) {
      e.preventDefault();
      $mt(".header-countries").slideDown('fast');
    });

    $mt(".select-store .sbHolder").on("mouseenter", function(e) {
      e.preventDefault();
      $mt(".header-countries").slideUp('fast');
    });

    $mt(".header-countries").on("mouseenter", function(e) {
      e.preventDefault();
      $mt(this).on("mouseleave", function(e) {
        e.preventDefault();
        $mt(".header-countries").slideUp('fast');
      });
    });

    $mt(".header-countries .country").on("click", function(e) {
      e.preventDefault();
      var store = $mt(this).data("store");
      setCookie("redirect", store, 28);
    });

    $mt("#select-store option").each(function(i,e){
			//var lang = $mt(e).html().toLowerCase().replace(/\s/g, '');
			var lang = $mt(e).val();
			$mt('.form-store .sbOptions li:eq('+i+') a').css({
				"background-image":"url('https://mdietline.com/media/wysiwyg/magenthemes/languages/"+lang+".png')",
				"background-repeat":"no-repeat",
				"background-position":"5px 8px"
			});
		}); 
		$mt('.sbHolder:eq(0)').addClass('lang');
		$mt('.sbHolder:eq(1)').addClass('curs');

		var currentStore = "spain"
		$mt('.form-store .sbSelector').css({
			"background-image":"url('https://mdietline.com/media/wysiwyg/magenthemes/languages/"+currentStore+".png')",
		});

  });
</script>

