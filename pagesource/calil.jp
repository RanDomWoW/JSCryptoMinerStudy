    }else if(calil.header.divider==true &&e.clientY >= 80){
      calil.header.divider = false;
      calil.header.divider_timer = setTimeout(function(){
        $('.cnavs>li').animate({'border-right-color': 'rgba(255, 255, 255, 0)'}, 'fast');
      }, 500);
    }
  });

  $('#query').focus(function(){
    $('#query_wrap').css('background-position', '0 -46px')
  }).blur(function(){
    $('#query_wrap').css('background-position', '0 3px')
  });

  

  
</script>


