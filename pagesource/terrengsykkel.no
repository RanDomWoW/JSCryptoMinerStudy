}


function setBackground(){
    $("body").css('background-image', 'none');
}

function manageWidth(breakFlag){
    var main_window_width = $(window).width();
    var ua = navigator.userAgent.toLowerCase();
    if (ua.indexOf('safari') != -1) {
        if (ua.indexOf('chrome') > -1) {

        } else {
            breakFlag = false;
            console.log("got safari");
        }
    }


    if (breakFlag && main_window_width < 1795)
        return false;

    showAds(false);

    if (main_window_width <= 1024) {
        $('.mainAdManageParent').css('width', "100%");
        return false;
    }

    var MAIN_FIXED_CONTAINER = 1000;


    $('.middleConatiner').attr('style', 'width:' + MAIN_FIXED_CONTAINER + 'px !important;');
    $('.search_div').attr('style', 'width:' + MAIN_FIXED_CONTAINER + 'px !important;');
    $('.terrengsykkel_search_div').attr('style', 'width:' + MAIN_FIXED_CONTAINER + 'px !important;');




    var total_container_width = main_window_width;
    var both_ad_width = total_container_width - MAIN_FIXED_CONTAINER;

    var individual_ad_width = 0;
    if (both_ad_width > 0) {
        individual_ad_width = both_ad_width / 2;
    }


    var manage_final_width = total_container_width - $(document).width();

    if (manage_final_width > 0) {

        var total_container_width = total_container_width - manage_final_width - 1;
        var individual_ad_width = (both_ad_width - manage_final_width - 1) / 2;
    }


    $('.adFinal').css('width', individual_ad_width + "px");
    $('.mainAdManageParent').css('width', total_container_width + "px");

    $('.fs14').each(function (ifs, efs) {
        $(efs).attr('style', "font-size:14px !important;");

    });

}
function showAds(showNow){

    var main_window_width = $(window).width();

    if (main_window_width < 1616) {
        $(".the_left_one").css('visibility', 'visible');
        $(".the_right_one").css('visibility', 'visible');
        return false;
    }
    if (showNow) {
        $(".the_left_one").css('visibility', 'visible');
        $(".the_right_one").css('visibility', 'visible');
    }
}


</script>
</html>