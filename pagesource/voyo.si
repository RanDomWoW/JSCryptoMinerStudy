	/**
	 * FUNCTION adds or removes classes for main navigation
	 *
	 * @return void
	 */
	function mainNavClasses() {
		if($j(".main-nav .panel-primary").is(":visible") && !$j(".main-nav").hasClass("main-nav-promo")) {
			$j(".main-nav").addClass("main-nav-promo");
		}
		if(!$j(".main-nav .panel-primary").is(":visible") && $j(".main-nav").hasClass("main-nav-promo")) {
			$j(".main-nav").removeClass("main-nav-promo");
		}
	}

	/**
	 * FUNCTION submits search
	 *
	 * @return void
	 */
	function poplusSubmitSearch() {
		var query = $j('#skeyword').val();

		if (!query) {
			return false;
		}

		query = "/izberi-in-poglej/query:" + query;

		window.location.href = query;
	}

	/**
	 * FUNCTION opens izberi in poglej dropdown
	 *
	 * @return void
	 */
	function toggleDropdown() {
		if($j(".tv-nav").is(":visible")) {
			toggleTvDropdown();
		}

		if($j(".izberi-nav").is(":visible")) {
			$j(".izberi-nav").hide();
			$j(".main-nav").removeClass("main-nav-open");
			$j("body").removeClass("pickplay-open");
			$j(".izberi-poglej-dropdown").removeClass("open");
			createMainCategories();
		}
		else {
			$j(".izberi-nav").show();
			$j(".main-nav").addClass("main-nav-open");
			$j("body").addClass("pickplay-open");
			$j(".izberi-poglej-dropdown").addClass("open");
		}
	}

	/**
	 * FUNCTION opens tv dropdown
	 *
	 * @return void
	 */
	function toggleTvDropdown() {
		if($j(".izberi-nav").is(":visible")) {
			toggleDropdown();
		}

		if($j(".tv-nav").is(":visible")) {
			$j(".tv-nav").hide();
			$j(".main-nav").removeClass("main-nav-open");
			$j(".tv-dropdown").removeClass("open");
		}
		else {
			$j(".tv-nav").show();
			$j(".main-nav").addClass("main-nav-open");
			$j(".tv-dropdown").addClass("open");
		}
	}

	/**
	 * FUNCTION CREATES SUBSUBCATEGORIES
	 *
	 * @param sectionId (int)
	 * @params parentSectionId (int)
	 * @return void
	 */
	function createSubSubCategories(sectionId, parentSectionId) {
		// save section data
		var sectionData = sections[parentSectionId]['subcategories'][sectionId];
		var html = "";

		// create additional close and show all html
		html += '<div class="izberi-nav-header clearfix">' +
				'<div class="izberi-nav-title clearfix">' +
					'<a href="#" class="izberi-nav-back btn btn-primary btn-back sub" data-section="' + parentSectionId + '"><span class="el-icon-chevron-left"></span></a>'+
					'<h2>' + sectionData.title + '</h2>' +
				'</div>'+
				'<div class="izberi-nav-options clearfix">' +
					'<a class="close-dropdown btn btn-primary btn-xs btn-close" id="show_list_close" href="#">' +
						'<span class="glyphicon glyphicon-remove"></span>' +
					'</a>' +
				'</div>' +
			'</div>' +
			'<div class="row">';

		// loop through subcategories and create html
		$j.each(sectionData['subcategories'], function(index, value) {

			var image;
			if(typeof(value.image) != "undefined" && value.image && value.image != "") {
				image = value.image;
			}
			else {
				image = "/static/slo/microsites/voyo2014/img/izberi-thumb-all.jpg";
			}

			html += '<div class="col-md-2">'+
					'<div class="video-item';
			if(value.expose)
				html += ' exposed';

			html +=		'">'+
						'<a href="' + (value.additionalUrl ? value.additionalUrl : (currentUrl + value.url)) + '/" class="sub ' + (value.subcategories ? "submain" : "") + '" data-section="' + value.id + '">'+
							'<div class="thumb-icon">'+
								'<div></div>'+
							'</div>'+
							'<h3>'+
								value.title +
							'</h3>'+
							'<img src="' + image + '" />' + (value.mojVoyoVisible ? '<div data-custom-id="' + value.mojVoyoCustomId + '" data-type="show" data-link="' + (value.additionalUrl ? value.additionalUrl : (currentUrl + value.url)) + '" data-media-id="' + value.mediaId + '" class="glyphicon glyphicon glyphicon-remove-circle user_display_when_loggedin ' + value.mojVoyoClass + ' moj-voyo-enabled moj-voyo-button moj-voyo-add" aria-hidden="true" data-toggle="tooltip" data-placement="bottom"></div>' : '') +
						'</a>'+
					'</div>'+
				'</div>';
		});

		html += '</div>';


		// change html of the izberi+poglej
		$j(".izberi-nav .types").html(html);

		setMojVoyoButtons();
	}

	/**
	 * FUNCTION CREATES SUBCATEGORIES
	 *
	 * @param sectionId (int)
	 * @return void
	 */
	function createSubCategories(sectionId) {
		// save section data
		var sectionData = sections[sectionId];
		var html = "";
		// create additional close and show all html
		html += '<div class="izberi-nav-header clearfix">' +
				'<div class="izberi-nav-title clearfix">' +
					'<a href="#" class="izberi-nav-back btn btn-primary btn-back"><span class="el-icon-chevron-left"></span></a>'+
					'<h2>' + sectionData.title + '</h2>' +
				'</div>'+
				'<div class="izberi-nav-options clearfix">' +
					'<a class="close-dropdown btn btn-primary btn-xs btn-close" id="show_list_close" href="#">' +
						'<span class="glyphicon glyphicon-remove"></span>' +
					'</a>' +
				'</div>' +
			'</div>' +
			'<div class="row">';


		var unsortedHtml = [];
		var htmlItem;
		// loop through subcategories and create html
		$j.each(sectionData['subcategories'], function(index, value) {
			htmlItem = '';
			var image;
			if(typeof(value.image) != "undefined" && value.image && value.image != "") {
				image = value.image;
			}
			else {
				image = "/static/slo/microsites/voyo2014/img/izberi-thumb-all.jpg";
			}

			htmlItem += '<div class="col-md-2 ddd">'+
					'<div class="video-item';
			if(value.expose)
				htmlItem += ' exposed';

			htmlItem +=		'">'+
						'<a href="' + (value.additionalUrl ? value.additionalUrl : (currentUrl + value.url)) + '/" class="sub ' + (value.subcategories ? "submain" : "") + '" data-section="' + value.id + '" data-parent-section="' + value.parent + '">'+
							'<div class="thumb-icon">'+
								'<div></div>'+
							'</div>'+
							'<h3>'+
								value.title +
							'</h3>'+
							'<img src="' + image + '" />' + (value.mojVoyoVisible ? '<div data-custom-id="' + value.mojVoyoCustomId + '" data-type="show" data-link="' + (value.additionalUrl ? value.additionalUrl : (currentUrl + value.url)) + '" data-media-id="' + value.mediaId + '" class="glyphicon glyphicon glyphicon-remove-circle user_display_when_loggedin ' + value.mojVoyoClass + ' moj-voyo-enabled moj-voyo-button moj-voyo-add" aria-hidden="true" data-toggle="tooltip" data-placement="bottom"></div>' : '') +
						'</a>'+
					'</div>'+
				'</div>';

				unsortedHtml.push(htmlItem);
		});

		unsortedHtml.sort(function (a, b) {
			var aTitle = a.match(/<h3>(.*)<\/h3>/g);
			var bTitle = b.match(/<h3>(.*)<\/h3>/g);

			if (aTitle === null || bTitle === null) {
				return 0;
			}

		  return aTitle[0].localeCompare(bTitle[0], 'sl');
		});

		html += unsortedHtml.join("");

		// add show all button
		html += '<div class="col-md-2">' +
				'<div class="video-item show-all">' +
					'<a href="/izberi-in-poglej/' + sectionData.url + '" class="sub">' +
						'<div class="thumb-icon">' +
							'<div></div>' +
						'</div>' +
						'<h3>' +
							'Prikaži vse' +
						'</h3>' +
						'<img src="/static/slo/microsites/voyo2014/img/izberi-thumb-all.jpg" />' +
					'</a>' +
				'</div>' +
			'</div>';

		html += '</div>';

		// change html of the izberi+poglej
		$j(".izberi-nav .types").html(html);

		setMojVoyoButtons();
	}

	function setMojVoyoButtons() {
			if (mojVoyoList === null) {
					$j(document).find('.moj-voyo-button').css('display', 'none');
			} else {
					$j('.moj-voyo-button')
						.removeClass('moj-voyo-remove')
						.tooltip('hide')
						.addClass('moj-voyo-add')
						.attr('data-original-title', 'Dodaj v Moj VOYO')
						.tooltip('fixTitle');
					$j.each(mojVoyoList, function(key, value) {
							var $el = $j('.moj-voyo-button[data-media-id="' + value.media_id + '"]');
							$el.toggleClass('moj-voyo-remove moj-voyo-add');
							$el.tooltip('hide').attr('data-original-title', 'Odstrani iz Moj VOYO').tooltip('fixTitle');
					});
					$j(document).find('.moj-voyo-button').css('display', 'block');
			}
	}

	/**
	 * FUNCTION CREATES MAIN CATEGORIES
	 *
	 * @return void
	 */
	function createMainCategories() {
		$j(".izberi-nav .types").html(mainCategoriesHtml);
		return;
	}

	function getParameterByName(name) {
		var match = RegExp('[?&]' + name + '=([^&]*)').exec(window.location.search);
		return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
	}

	function crm_registration(specialClass)
	{
		crm_login.show(null, {callback: function() {
			open_login(null, function() {
				$crm.registration_show($j("#crm-btn-register"), $j("#crm-btn-register"));
			});
		}});
	}

	/**
	 * FUNCTION opens login modal window
	 *
	 * @return void
	 */
	var login_opening = false;

	function open_login(logintype, callback, loginSubtype)
	{
		if (login_opening)
			return;

		$j("#loginModal").removeClass("paylife");

		var url = "/lbin/voyo2014/ajax_login_box.php";
		if (logintype) {
			url += "?type=" + logintype;
		}
		if(loginSubtype) {
			if(logintype) {
				url += "&subtype=" + loginSubtype;
			}
			else {
				url += "?subtype=" + loginSubtype;
			}
		}

		if(typeof(buyReferer) != "undefined" && buyReferer && buyReferer != "") {
			url += "&referer=" + buyReferer;
		}

		login_opening = true;
		$j("body").addClass("login-opening");

		$j.ajax({
			url: url,
			success: function(data) {
				login_opening = false;

				$j("#loginModal")
				.one('hidden.bs.modal', {}, function() {
					$crm.setPopupState("closed");
				})
				.modal("show",{
					keyboard: false,
					backdrop: 'static'
				});

				$crm.setPopupState("opened");

				$j("#loginModal .modal-body").html(data);
				$j("body").removeClass("login-opening");

				if (callback)
					callback();
			},
			error: function() {
				login_opening = false;
				$j("body").removeClass("login-opening");
			}
		})
	}
</script>

