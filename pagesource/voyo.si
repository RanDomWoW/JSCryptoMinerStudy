<script language="javascript">
	var clickCarousesIndicator = false;
	var carouselVideo = null;
	var carouselFirst = true;
	
	$j(".carousel-inner").on("click", function() {
		$j('#article-photo-carousel')
			.carousel('pause')
			.addClass("paused");
	});
	
	
	// on change play video if exists
	$j(".carousel").on("slid.bs.carousel", playVideo);
	$j(".carousel").on("slide.bs.carousel", stopVideo);
	
	// on mouse over ideo
	$j(".carousel").find(".carousel-video").on("mouseenter", function() {
		if (carouselVideo) {
			carouselVideo.muted = false;
		}
	});
	$j(".carousel").find(".carousel-video").on("mouseleave", function() {
		if (carouselVideo) {
			carouselVideo.muted = true;
		}
	});
	
	function playVideo()
	{
		if( $j(".carousel .carousel-inner .item.active .carousel-video").length > 0) {
			$j(".carousel").carousel('pause');
			// on first carousel we need to add delay
			if(carouselFirst) {
				setTimeout(function() {
					$j(".carousel").carousel('pause');
				}, 2000);
			}
			// find id
			var id = $j(".carousel .carousel-inner .item.active .carousel-video").data("id");
			// Play
			if(!carouselVideo) {
				carouselVideo = document.getElementById("video-" + id);
			}
			carouselVideo.muted = true;
			carouselVideo.play();
			carouselVideo.addEventListener('ended', videoEnded, false);
			carouselFirst = false;
		}
		else {
			if(carouselFirst) {
				$j(".carousel").carousel('cycle');
				carouselFirst = false;
			}
		}
	}
	
	function videoEnded()
	{
		if ($j(".carousel:hover").length === 0) {
			$j(".carousel").carousel('next');
		}
		$j(".carousel").carousel('cycle');
		stopVideo();
	}

	function stopVideo()
	{
		if(!carouselVideo) {
			return;
		}
		
		carouselVideo.pause();
		carouselVideo.currentTime = 0;
		$j(".carousel").carousel('cycle');
		carouselVideo.removeEventListener('ended', videoEnded);
		carouselVideo = null;
	}
	

	// if user clicks on indicators of controls slider stops
	$j(".carousel-control, .carousel-indicators li").on("click", function() {
		clickCarousesIndicator = true;
		$j(".carousel-inner, .carousel-indicators-frame").off("mouseenter.animate");
		$j(".carousel-inner, .carousel-indicators-frame").off("mouseleave.animate");
		$j('#article-photo-carousel')
			.carousel('pause')
			.addClass("paused");
	});

	// stop carousel progress bar
	$j(".carousel-inner, .carousel-indicators-frame").on("mouseenter.animate", function() {
		var divWidth = $j(".transition-timer-carousel-progress-bar").width();
		$j(".transition-timer-carousel-progress-bar").removeClass("animate").width(divWidth);
	});

	// start carousel progress bar
	$j(".carousel-inner, .carousel-indicators-frame").on("mouseleave.animate", function() {
		$j(".transition-timer-carousel-progress-bar").css("width", "100%").addClass("animate")
	});
	

	$j(".document").ready(function() {
		$j(".main-promo-overlay .close-splash > a").on("click", function(e) {
			e.preventDefault();
			$j(".main-promo-overlay").slideUp(300);
		});
		
		
	});

	$j(".document").ready(function() {
		$j(".main-foreign-overlay .close-btn > a").on("click", function(e) {
			e.preventDefault();
			$j(".main-foreign-overlay").slideUp(300);
		});
	});
	
	$j(window).load(function() {
		playVideo();
	});
</script>



