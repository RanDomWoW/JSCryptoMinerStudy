		$j("document").ready(function () {
			if (document.location.href.indexOf("/programi") > -1) {
				$j("html").addClass("section-tv");
			}
		})

		function getBookmarkClassName(bookmark) {
			if (bookmark.category === 'Glej naprej')
				return 'item-watching';
			if (bookmark.category === 'Že pogledano')
				return 'item-watched';

			return '';
		}

		function seconds_to_hhmm(bookmark) {

			var minutes = Math.floor(bookmark.seconds / 60);
			var hours = 0;

			if (minutes > 60) {
				hours = Math.floor(minutes / 60);
				minutes = minutes - 60*hours;

				return hours + ' h ' + minutes + ' min';
			}

			return minutes + ' min';
		}

		function markBookmarkedVideo(item, bookmarkInfo) {
			var className = getBookmarkClassName(bookmarkInfo);
			item.addClass(className);

			if (className === 'item-watching') {
				var hhmm = seconds_to_hhmm(bookmarkInfo.bookmark);
				item.find('.item-watching-minute').html(hhmm);
			}
		}

		/**
		 * Add css style to videos that were watched or are
		 * being watched.
		 */
		function markBookmarkedVideos() {
			try {
				var items = $j("body").find("[data-video]");

				items.each(function(index, item) {
					var media_id = $j(item).attr('data-video');

					if (!media_id || (media_id === ''))
						return;

					var bookmarkInfo = $visitor.bookmarks().isBookmarked(media_id);

					if (bookmarkInfo) {
						markBookmarkedVideo($j(item), bookmarkInfo);
					}
				});
			} catch (err) {
				console.log(err);
			}
		}

		// load bookmarks
		function loadBookmarks(){
			if (!$visitor.is_logged || !$visitor.bookmarks()) {
				return;
			}
			$visitor.bookmarks().ready(markBookmarkedVideos);
			$visitor.bookmarks().load();
		}

		// display bookmarked data when we have loaded user data
		if ($visitor && $visitor.sso_ready) {
			$visitor.sso_ready(loadBookmarks);
		}

		function bookmark_seekTo(media_id) {
			try {
				var bookmarkInfo = $visitor.bookmarks().isBookmarked(media_id);
				if (!bookmarkInfo)
					return;

				var percent = bookmarkInfo.bookmark.percent;
				var seekTo = percent / 100;


        // Update the video time
        $j('#html5_pop_player')[0].currentTime = $j('#html5_pop_player')[0].duration * seekTo;


			} catch (err) {
				console.log(err);
			}
		}
		</script>


