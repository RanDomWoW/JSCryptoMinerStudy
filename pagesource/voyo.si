		<script type="text/javascript">
	run_after_login="callAll()";

	function callAll() {
		//Call this only on root/front page. This loads box - continue with watching.
		if (window.location.pathname === '/') {
				loadVoyoBoxes();
				loadVoyoBoxesContinue();
		}
		//Call this everywhere.
		loadMojVoyo();
		loadNotifications();
	}

	function loadVoyoBoxesContinue() {
		$j.ajax({
			url: '/lbin/voyo2014/ajax_voyoboxes_continue.php',
			data: 'country=' + country,
			success: function(data) {
				//console.log(data);
				$j(".voyoBoxesContent-continue-watching")
					.html(data)
					.fadeIn("slow");
				$visitor.sso_ready(loadBookmarks);
				carouselOverflow();
			}
		});
	}

	function loadVoyoBoxes() {
		$j.ajax({
			url: '/lbin/voyo2014/ajax_voyoboxes.php',
			data: 'country=' + country,
			success: function(data) {
				if(!data || data == '' || data == ' ') {
					return;
				}

				$j(".voyoBoxesContent")
					.html(data)
					.fadeIn("slow");
				$visitor.sso_ready(loadBookmarks);
				carouselOverflow();
				setMojVoyoButtons();
			}
		});
	}

	$j(document).ready(function(){
		loadVoyoBoxes();
		loadVoyoBoxesContinue();
		loadMojVoyo();
		loadNotifications();
	});

