	var mojVoyoList = [];

	function loadMojVoyo() {
		$j(document).ready(function(){
					$j.post("/lbin/voyo2014/ajax_moj_voyo.php", {
							action: 'load',
					}).success(function(response) {
							response = JSON.parse(response);
							$j.each(sections, function(i, section) {
									$j.each(section.subcategories, function(j, subcategory) {
											subcategory.mojVoyoClass = '';
											if (subcategory.subcategories !== undefined) {
													$j.each(subcategory.subcategories, function(k, subsubcategory) {
															subsubcategory.mojVoyoClass = '';
													});
											}
									});
							});
							getMojVoyoList();
							$j(document).find('#moj-voyo').html(response.data);
					});
			});
	}

	function getMojVoyoList() {
			return $j.get("/lbin/voyo2014/ajax_moj_voyo_list.php", function(response) {
					response = JSON.parse(response);
					mojVoyoList = response.data;
					setMojVoyoButtons();
			});
	}

	function loadNotifications() {
			$j.get("/lbin/voyo2014/ajax_moj_voyo_notifications.php").success(function(response) {
					response = JSON.parse(response);
					if (response.status === 'success') {
							$j('#notification-dropdown').html(response.data);
					}
			});
	}






  var serveTime, nextEnd;

    function newLiveData() {
      // get new data
      $j.ajax({
        url: "/lbin/voyo2014/ajax_front_live_refresh.php",
        success: function(response) {
          updateNext(response);
        }
      });
    }


    function updateNext(response) {
      $j('.tv-live-front .container').html(response);
      nextEnd = $j('.container div[data-next-end]').data('next-end');
    }


  function getServerTime() {
    serverTime = server_time;
    setInterval(function() {
      serverTime += 10;
      if (serverTime > nextEnd) {
        newLiveData();
      }
    }, 10000);
  }



	$j(document).ready(function() {

    nextEnd = $j('.container div[data-next-end]').data('next-end');

		getMojVoyoList();
		searchOffsetOriginal = $j("#skeyword").offset();
		mainCategoriesHtml = $j('.izberi-nav .types').html();

		$j(document).on("click", ".moj-voyo-box .pagination a", function(e) {
				e.preventDefault();
				var page = $j(this).data("page");

				$j.get("/lbin/voyo2014/ajax_moj_voyo_all.php?page=" + page, function(response) {
						response = JSON.parse(response);
						$j('#moj-voyo-listing').replaceWith(response.data);

						$j('html, body').animate({
							scrollTop: 0
						}, 500);

						historyState = History.getState();
						var url = window.location.pathname + "?page=" + page;
						History.replaceState(historyState, 'VOYO - Moj Voyo', url);
				});
		});

		$j(document).on('click', '.moj-voyo-button', function(e) {
				var $el = $j(this);
				e.preventDefault();
				e.stopPropagation();

				if ($el.hasClass('moj-voyo-enabled')) {
						$el.removeClass('moj-voyo-enabled glyphicon-remove-circle').addClass('glyphicon-refresh moj-voyo-button-loader');
						if ($j('#moj-voyo').length > 0) {
								var page = $j('#moj-voyo .item').index($j('#moj-voyo .item.active'))+1;
								toggleMojVoyoMedia($el, 'carousel', page);
						} else if ($j('#moj-voyo-listing').length > 0) {
								var page = getParameter('page');
								if ($j('#moj-voyo-listing .video').length == 1 && $el.hasClass('moj-voyo-remove')) {
										page = page == 1 ? page : (page-1);
										toggleMojVoyoMedia($el, 'listing', page);
										historyState = History.getState();
										var url = window.location.pathname + "?page=" + (page);
										History.replaceState(historyState, 'VOYO - Moj Voyo', url);
								} else {
										toggleMojVoyoMedia($el, 'listing', page);
								}

						} else {
								toggleMojVoyoMedia($el, null, null);
						}
				}
		});

		$j(document).on('click', '.notification-icon', function() {
				var $icon = $j(this);
				if ($icon.hasClass('active')) {
					$j.post("/lbin/voyo2014/ajax_moj_voyo_update_seen_list.php", {})
					.success(function(response) {
							response = JSON.parse(response);
							if (response.status === 'success') {
									$icon.removeClass('active').text('');
							}
					});
				}
		});

		$j(document).on('hide.bs.dropdown', '.notification-dropdown', function() {
				$j(document).find('#notification-dropdown li').removeClass('active');
		});

		function getParameter(name, url) {
		    if (!url) url = window.location.href;
		    name = name.replace(/[\[\]]/g, "\\$&");
		    var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
		        results = regex.exec(url);
		    if (!results) return 1;
		    if (!results[2]) return '';
		    return decodeURIComponent(results[2].replace(/\+/g, " "));
		}

		function toggleMojVoyoMedia($el, responseType, page) {
				var mediaId = $el.data('media-id');
				$j.post("/lbin/voyo2014/ajax_moj_voyo.php", {
						action: $el.hasClass('moj-voyo-remove') ? 'remove' : 'add',
						link: $el.data('link'),
						id: mediaId,
						type: $el.data('type'),
						customId: $el.data('custom-id'),
						responseType: responseType,
						page: page
				}).success(function(response) {
						response = JSON.parse(response);
						if (response.status === 'success') {
								getMojVoyoList().then(function() {
										$el.removeClass('glyphicon-refresh moj-voyo-button-loader').addClass('moj-voyo-enabled glyphicon-remove-circle');
								});
								//Moj Voyo box is visible only at /
								if (responseType === 'carousel') {
										$j('#moj-voyo').html(response.data);
								} else if (responseType === 'listing') {
										$j('#moj-voyo-listing').replaceWith(response.data);
										if ($j('#moj-voyo-listing .video').length) {
												$j('#empty-reminder').addClass('hide');
												if ($j('#notification-reminder').hasClass('activated')) {
														$j('#notification-reminder').removeClass('hide');
												} else {
														$j('#notification-reminder').addClass('hide');
												}
										}  else {
												$j('#empty-reminder').removeClass('hide');
												$j('#notification-reminder').addClass('hide');
										}
								}
						} else {
								alert(response.message);
								$el.removeClass('glyphicon-refresh moj-voyo-button-loader').addClass('moj-voyo-enabled glyphicon-remove-circle');
						}
						getNotifications();
				}).fail(function(error) {
						alert(error.message);
						$el.addClass('moj-voyo-enabled');
						$el.removeClass('glyphicon-refresh moj-voyo-button-loader').addClass('moj-voyo-enabled glyphicon-remove-circle');
				});
		}

		function getNotifications() {
				$j.get("/lbin/voyo2014/ajax_moj_voyo_notifications.php").success(function(response) {
						response = JSON.parse(response);
						if (response.status === 'success') {
								$j(document).find('#notification-dropdown').html(response.data);
						}
				});
		}

    var isSliding = false;
		$j(document).on('slid.bs.carousel', '#carousel-moj-voyo', function(e) {
      isSliding = false;
  		var current = $j(this).find('.active').index() + 1;
			$j('#moj-voyo').find('#moj-voyo-current-active-number').text(current);
		});

    $j(document).on('slide.bs.carousel', '#carousel-moj-voyo', function(e) {
        isSliding = true;
    });

    //This is for toggling vissible/hidden class on carousel to show popup on dynamically loaded content.
