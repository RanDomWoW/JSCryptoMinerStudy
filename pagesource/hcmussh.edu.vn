              $('ul', this).css({ 'width': '400px' });
          if (countLi > 30 && countLi <= 45)
              $('ul', this).css({ 'width': '600px' });
          if ($('ul', this).is(":hidden")) {
              $('ul', this).slideDown(300);
          }
      }
  },
  function () {
      //hide its submenu
      $('ul', this).stop(true, false).slideDown();
      $('ul', this).hide();
  }
 );
    });
