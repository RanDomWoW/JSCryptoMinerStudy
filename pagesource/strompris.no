            body.off('click', close);
        }
    }

    menuButton.click(function() {
        if (!overlayMenu.hasClass('show')) {
            overlayMenu
                .removeClass('fadeOut')
                .addClass("fadeIn")
                .addClass("show");

            menuContent
                .show({ duration: 200 });
            
            window.setTimeout(function() {
                body.on('click', close);
            }, 200)
        }
    });
})(jQuery);
</script>
