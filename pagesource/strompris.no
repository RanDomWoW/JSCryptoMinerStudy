    function close(e) {
        if ($(e.target).closest('#menu-content').length === 0) {
            overlayMenu
                .removeClass('fadeIn')
                .addClass("fadeOut");

            window.setTimeout(function() {
                overlayMenu.removeClass('show')
            }, 200)

            menuContent
