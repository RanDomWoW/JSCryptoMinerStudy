<script id="content-comments-template" type="text/template">
  <div class="ui comments">
    {{#items}}

    {{#comments}}
      <div class="comment">
        <a href="http://mein.dbna.de/kurzsteckbrief.php?mid={{comment_editor.id}}" class="avatar" target="_blank">
          {{#comment_editor.profileAvatar}}<img src="{{comment_editor.profileAvatar}}">{{/comment_editor.profileAvatar}}
          {{^comment_editor.profileAvatar}}<img src="https://mein.dbna.de/images/default_tnb.jpg">{{/comment_editor.profileAvatar}}
        </a>
        <div class="content">
          <a href="http://mein.dbna.de/kurzsteckbrief.php?mid={{comment_editor.id}}" target="_blank" class="author">
            {{#comment_editor.nickname}}{{comment_editor.nickname}}{{/comment_editor.nickname}}{{^comment_editor.nickname}}{{editor_login}}{{/comment_editor.nickname}}
          </a>
          <div class="metadata">
            <span class="date">{{timestamp_cute}}</span>
          </div>
          <div class="text">
            {{{htmlText}}}
          </div>
          {{#allowDel}}
          <div class="actions">
            <a href="http://mein.dbna.de/kurzsteckbrief.php?mid=48946&seite=7&lshowid={{id}}" class="reply">Bearbeiten</a>
          </div>
          {{/allowDel}}
        </div>
      </div>
      {{/comments}}

      <div class="{{^commentsMore}}displaynone{{/commentsMore}}" id="commentsbutton">
        <button class="ui button" onclick="showMoreComments(); return false;">{{{countComments}}}</button>
      </div>

      {{#commentsMore}}
        <div class="comment displaynone js-commentsmore">
          <a href="http://mein.dbna.de/kurzsteckbrief.php?mid={{comment_editor.id}}" class="avatar" target="_blank">
            {{#comment_editor.profileAvatar}}<img src="{{comment_editor.profileAvatar}}">{{/comment_editor.profileAvatar}}
            {{^comment_editor.profileAvatar}}<img src="https://mein.dbna.de/images/default_tnb.jpg">{{/comment_editor.profileAvatar}}
          </a>
          <div class="content">
            <a href="http://mein.dbna.de/kurzsteckbrief.php?mid={{comment_editor.id}}" target="_blank" class="author">
              {{#comment_editor.nickname}}{{comment_editor.nickname}}{{/comment_editor.nickname}}{{^comment_editor.nickname}}{{editor_login}}{{/comment_editor.nickname}}
            </a>
            <div class="metadata">
              <span class="date">{{timestamp_cute}}</span>
            </div>
            <div class="text">
              {{{htmlText}}}
            </div>
            {{#allowDel}}
            <div class="actions">
              <a href="http://mein.dbna.de/kurzsteckbrief.php?mid=48946&seite=7&lshowid={{id}}" class="reply">Bearbeiten</a>
            </div>
            {{/allowDel}}
          </div>
        </div>
        {{/commentsMore}}
        {{^comments}}
        <div class="ui small icon header" style="width: 100%">
          <i class="frown icon"></i>
          <div class="content">
            <div class="sub header">Noch keine Kommentare</div>
          </div>
        </div>
        {{/comments}}
        <div class="ui section divider"></div>
        <div class="textcenter">
          <button onClick="openCommentModal();return false;"class="ui inverted {{^thatsMeId}}disabled{{/thatsMeId}} blue button" {{^thatsMeId}}disabled{{/thatsMeId}}>Schreibe einen Kommentar</button>
        </div>
        {{^thatsMeId}}
        <div class="textcenter logintext">
          <p>Melde dich mit deinem dbna-Account an, um den Artikel zu kommentieren!</p>
        </div>
        {{/thatsMeId}}
        <div class="ui section hidden divider"></div>

    {{/items}}
    </div>

 </script>



