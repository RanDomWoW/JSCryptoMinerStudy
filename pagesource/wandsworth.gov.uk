<script type="text/javascript" src="http://www.google.com/cse/brand?form=cse-search-box&amp;lang=en"></script> 
<script src="/site/javascript/libs/jquery-1.9.1.min.js"></script>
<script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.8/jquery-ui.js"></script>




<script>
    jQuery(function ($) {
        $(document).ready(function () {
            var myArr = [];




            var SearchItem = function (value, title, id, category, urlimage, linkUrl) {

                var ret = new Object();
                ret.value = value;/*NEED THIS TO SEARCH*/
                ret.lable = title;/*NEED THIS TO DISPLAY IN SEARCH BOX*/
                ret.title = title;
                ret.id = id;
                ret.category = category;
                ret.urlimage = '';
                ret.urlpageautocomplete = "";
                ret.imageautocomplete = "image-autocomplete";
                ret.titleautocomplete = "title-autocomplete";
                ret.categoryautocomplete = "category-autocomplete";
                ret.linkurl = linkUrl;
                ret.renderHtml = '<a class="link-auto" href="' + ret.linkurl + '"   ><h3>' + ret.title + '</h3><span>' + ret.category + '</span></a>';
                return ret;
            };

            $.ajax({
                type: "GET",
                url: "/download/downloads/id/13140/promotions.xml", // change to full path of file on server
                dataType: "xml",
                success: parseXml,
                complete: setupAC,
                failure: function (data) {
                    alert("XML File could not be found");
                }
            });

            function parseXml(xml) {
                $(xml).find("Promotion").each(function () {
                    value = $(this).attr("queries");
                    title = $(this).attr("title");
                    id = $(this).attr("id");
                    category = $(this).attr("description");
                    urlimage = "";
                    linkUrl = $(this).attr("url");


                    myArr.push(new SearchItem(value, title, id, category, urlimage, linkUrl));
                });
            }




            function setupAC() {
                $("input#gsc-i-id1").autocomplete({
                    source: myArr,
                    minLength: 2,
                    select: function (event, ui) {
                        //$("input#search").val(ui.item.value);
                        //alert(ui.item.linkurl)
                        window.location = ui.item.linkurl;
                        return false;

                    }
                }).data("autocomplete")._renderItem = function (ul, item) {
                    return $("<li></li>")
                            .data("item.autocomplete", item)

                            .append(item.renderHtml)

                            //.append("<a class='myclass' href='" + item.linkurl + "'>" + item.title + " - <span>"+ item.category+ "</span></a>")
                            .appendTo(ul);
                };
            }




            var _borough = 'Wandsworth Borough';

            function initPage() {



                $("#csearch").click(function (event) {

                    event.preventDefault();
                    searchConsulations($("#srchAny").val());
                }

                );

                function sortByEndDateAsc(a, b) {
                    return  new Date(b.enddate).getTime() - new Date(a.enddate).getTime();
                }



                $.getJSON("https://haveyoursay.citizenspace.com/api/2.3/json_search_results?callback=?",
                        {
                            'in': _borough,
                            'st': 'open'},
                        function (data) {
                           
                           data.sort(sortByEndDateAsc);

                            $.each(data.reverse().slice(-10), function (i, item) {
                                // Create a link to the consultation

                                var endDate = format(item.enddate);
                                var result = $("<tr><td><a href=\"" + item.url + "\">" + item.title + "</a></td><td>" + endDate + "</td></tr>");

                                $('#consultations').append(result);

                            });
                        });

                $.getJSON("https://haveyoursay.citizenspace.com/api/2.3/json_search_results?callback=?",
                        {
                            'in': 'Wandsworth Borough',
                            'st': 'closed'
                        },
                        function (data) {


                            data.sort(sortByEndDateAsc);
                            
                            var hasResults = false;

                            $.each(data.reverse().slice(-10), function (i, item) {

                                // Create a link to the consultation
                                hasResults = true;

                                var endDate = format(item.enddate);
                                var result = $("<tr><td><a href=\"" + item.url + "\">" + item.title + "</a></td><td>" + endDate + "</td></tr>");

                                $('#closedconsultations').append(result);
                                return i < 5;
                            });
                            if (!hasResults)
                            {
                                $('#closedconsultations').hide();
                                $('.noclosedresults').show();
                            }
                        });



                function format(inputDate) {
                    var date = new Date(inputDate);
                    if (!isNaN(date.getTime())) {
                        // Months use 0 index.
                        var month = date.getMonth();
                        month++;
                        return   date.getDate() + '/' + month + '/' + date.getFullYear();
                    }
                }

                function searchConsulations(searchText) {


                    $.getJSON("https://haveyoursay.citizenspace.com/api/2.3/json_search_results?callback=?",
                            {
                                'in': _borough,
                                'st': 'closed',
                                'tx': searchText
                            },
                            function (data) {



                                data.sort(sortByEndDateAsc);

                                var hasResults = false;

                                $.each(data.reverse().slice(-10), function (i, item) {
                                    // Create a link to the consultation
                                    hasResults = true;
                                    var endDate = format(item.enddate);
                                    var result = $("<tr><td><a href=\"" + item.url + "\">" + item.title + "</a></td><td>" + endDate + "</td></tr>");

                                    $('#searchresults').append(result);


                                });
                                if (!hasResults)
                                {
                                    $('#searchresults').hide();
                                    $('.nosearchresults').show();
                                } else {
                                    $('#searchresults').show();
                                    $('.nosearchresults').hide();
                                }
                            });





                }

            }

            if (document.getElementById('csearch')) {
                initPage();
            }

        });
    });


</script>



