<script type="text/javascript-deferred">
    $(document).ready(function(){

        // apply images for searchlove scrolling background. done in js for pageload speed.
        $('.img9').css('background-image', 'url(/uploads/slbg.jpg)')
        var postload_sl_bg_style = '<style>'
        for (img_num in [1,2,3,4,5,6,7,8]) {
            postload_sl_bg_style += '.img' + img_num + '::after { background-image: url(/static/images/searchlove-bg/' + img_num + '.jpg);} ';
        }
        $(postload_sl_bg_style + '</style>').appendTo("head")



        var thumb_links = $('.team-thumb');
        for(var i = 0; i < thumb_links.length; i++) {
            var thumb_link = $(thumb_links[i]);
            thumb_link.html('<img src="' + thumb_link.attr('data-src') + '" width="125" height="126">');
            thumb_link.attr('style', 'background-image:url(' + thumb_link.attr('data-bgimg') + ');');
        }
        $('#home_searchlove_bg').height($('#home_searchlove').outerHeight(true))
        $('#home_searchlove_bg ul li').height($('#home_searchlove').outerHeight(true))
        $('#home_searchlove').css({
            'marginTop' : '-' + $('#home_searchlove').outerHeight(true) + 'px'
        });


        var lastScrollTop = 0;
        var $searchloveBg = $('#home_searchlove_bg');
        var scrollerPos = $searchloveBg.offset().top + $searchloveBg.outerHeight(true);
        var executed = false;

        $(window).on('scroll',function(){
            if (executed) {
                return;
            }
            var st = $(this).scrollTop(),
                movingDown = st > lastScrollTop,
                searchloveVisible = st + $(this).height() > scrollerPos;
            if (movingDown && searchloveVisible){
                setTimer();
                executed = true;
            }
            lastScrollTop = st;
        });

        function setTimer(){
            var count = 1;
            var timer = setInterval(function() {
                if(count < 9) {
                    $('.img'+count).animate({
                        left: '-=' + $(window).width()
                    }, 500, 'swing');
                    count++;
                  }
            }, 4000);
        }
    });

</script>



