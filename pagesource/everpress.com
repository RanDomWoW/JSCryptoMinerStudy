<script id="tplBasketItem" type="text/template">
    <% var bgColor = _.find(selectedProduct.colours, function(colour) { return colour.colourId === colourId; }).code; %>

    <div class="column thumbnail">
        <img src="<%= selectedProduct.image %>" style="background-color: #<%= bgColor %>;" />
    </div>
    <span class="basket__column__label">Product</span>
    <div class="column campaign__basket__products">
        <select tabindex="1">
            <% campaignProducts.forEach(function(product) { %>
                <option value="<%= product.campaignProductId %>" data-html="</span> <span class='brand'><%= product.name %></span>" <% if(campaignProductId === product.campaignProductId) { %>selected<% }; %>><%= product.name %></option>
            <% }); %>
        </select>
    </div>
    <span class="basket__column__label">Colour</span>
    <div class="column campaign__basket__colours">
        <select tabindex="1">
            <% selectedProduct.colours.forEach(function(colour) { %>
                <option value="<%= colour.colourId %>" <% if(colour.colourId === colourId) { %>selected<% }; %>><%= colour.name %></option>
            <% }); %>
        </select>
    </div>
    <span class="basket__column__label">Size</span>
    <div class="column campaign__basket__sizes">
        <select tabindex="1">
            <% selectedProduct.sizes.forEach(function(_size) { %>
                <option value="<%= _size.code %>" <% if(_size.code === size) { %>selected<% }; %>><%= _size.code %></option>
            <% }); %>
        </select>
    </div>
    <span class="basket__column__label">Quantity</span>
    <div class="column campaign__basket__quantity">
        <input class="basket-item-quantity" type="number" pattern="\d*" value="<%= quantity %>" min="1" max="100">
    </div>
    <div class="column row-total"><span class="price"><%= App.currency.format(price * quantity) %></span> <button title="delete product" class="button button--raw icon-cross basket__delete-btn basket-item-delete"></button></div>
</script>

<script id="tplCampaignBasketItemColors" type="text/template">
    <% colours.forEach(function(colour) { %>
        <option value="<%= colour.colourId %>" <% if(colour.colourId === featuredColourId) { %>selected<% } %>><%= colour.name %></option>
    <% }); %>
</script>

<script id="tplCampaignBasketItemSizes" type="text/template">
    <% sizes.forEach(function(size) { %>
        <div class="radio-group"><input id="<%= id %>-<%= size.code %>" type="radio" name="size" value="<%= size.code %>"><label for="<%= id %>-<%= size.code %>"><%= size.code %></label></div>
    <% }); %>
</script>


