    var sliderCount = $(".slider").find(".item").length;
    function slidePlay(i) {
        $(".slider-dot").find("span").eq(i).addClass("active").siblings().removeClass("active");
        $(".slider").find(".item").eq(i).fadeIn(500).siblings().fadeOut(500);
        $(".focus .item-a").find(".item-a-link").eq(i).show().siblings().hide();
    }
    var autoNum = 0;
    function autoPlayF() {
        slidePlay(autoNum);
        autoNum++;
        if (autoNum == sliderCount) {
            autoNum = 0;
        }
    }
    var autoPlay = setInterval("autoPlayF()", 3000);

    $(".focus-bar .item-a").mouseenter(function () {
        clearInterval(autoPlay);
    }).mouseleave(function () {
        autoPlay = setInterval("autoPlayF()", 3000);
    });

    $(".slider-dot").delegate("span", "click", function () {
        var $this = $(this);
        var sliderNum = $this.index();
        slidePlay(sliderNum);
    });
</script>
