<script type="text/javascript">
    $(document).ready(function () {

        $('#divMainBanners').iosSlider({
            desktopClickDrag: true,
            snapToChildren: true,
            infiniteSlider: true,
            autoSlide: true,
            autoSlideTimer: 4000,
            scrollbar: false,
            keyboardControls: true,
            navSlideSelector: '.slider .pager li',
            onSlideChange: slideMainBannerChange,
            onSlideStart: slideMainBannerStart
        });

        function hideMainBannerRibbon() {
            $('.banner_ribbon').css("display", "none");
        }

        function slideMainBannerStart(args) {
            hideMainBannerRibbon();
        }

        function slideMainBannerChange(args) {
            hideMainBannerRibbon();

            if ($(args.currentSlideObject).find('.price').length == 1) {
                $('.banner_ribbon').css("display", "block");
            }

            $('.slider .pager li').removeClass('active');
            $('.slider .pager li:eq(' + (args.currentSlideNumber - 1) + ')').addClass('active');
        }

    });
</script>

