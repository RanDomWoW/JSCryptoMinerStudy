    //   function loadMore(){
    function RemoveloadMore() {
      if ( current > last ) {
        jQuery( '.bigbutton' ).addClass( 'done' );
        return false;
      }
      var search = window.location.search;
      var next = parseInt( current ) + 1;
      var url = window.location.origin + window.location.pathname + '/page/' + next + search;
      /*url  = '/page/'+next+'/?show=popular';*/
      jQuery( '#ajaxbutton i' ).addClass( 'spinner' );
      var categories_data = JSON.parse( jQuery( '#categories_data' ).val() );
      var posts_used_data = JSON.parse( jQuery( '#posts_used_data' ).val() ).join( ',' );
      var ajaxData = {
        categories_data : categories_data,
        posts_used_data : posts_used_data
      };
      var object = {
        data : ajaxData, //JSON.parse(jQuery('#categories_data').val()),
        dataType : 'json'
      };
      jQuery.get( url, object, function ( data ) {
        if ( jQuery( data ).find( ".post__item" ).length === 0 ) {
          return jQuery( '#ajaxbutton' ).hide();
        }
        current++;
        var nextposts = jQuery( data ).find( ".posts" );
        last = jQuery( data ).find( '.pager li.last' ).prev().find( 'a' ).text();
        var new_categories_data = JSON.parse( jQuery( data ).find( '#categories_data' ).val() );
        var new_posts_used_data = JSON.parse( jQuery( data ).find( '#posts_used_data' ).val() );
        jQuery( '#categories_data' ).val( JSON.stringify( mergeObjects( categories_data, new_categories_data ) ) );
        jQuery( '#posts_used_data' ).val( JSON.stringify( new_posts_used_data ) );
        jQuery( '.load-more' ).before( nextposts );
        jQuery( '.load-more' ).before( $( '.money-block__900-90' ).last().clone() );

        ga( 'send', 'pageview', url.replace( '//', '' ).replace( 'pics.ru', '' ).replace( 'www.', '' ) );

        jQuery( '#ajaxbutton i' ).removeClass( 'spinner' );
      } ).fail( function () {
        jQuery( '#ajaxbutton' ).hide();
      } );
    }

    function mergeObjects() {
      var mergedObj = arguments[0] || { };
      for ( var i = 1; i < arguments.length; i++ ) {
        var obj = arguments[i];
        for ( var key in obj ) {
          if ( obj.hasOwnProperty( key ) ) {
            if ( mergedObj[key] ) {
              mergedObj[key] += obj[key];
            } else {
              mergedObj[key] = obj[key];
            }
          }
        }
      }
      return mergedObj;
    }


    $( document ).ready( function () {
      $( '#overlay, .sidr-close' ).click( function () {
        jQuery.sidr( 'close' );
      } )
      $( '#simple-menu' ).sidr( {
        renaming : false,
        displace : false,
        onOpen : function () {
          $( '#overlay' ).css( 'visibility', 'visible' );
        },
        onClose : function () {
          $( '#overlay' ).css( 'visibility', 'hidden' );
        }
      } );

        //add image sharer
        var img_sharer_template = jQuery( '.image-sharer' ).clone();
        jQuery( 'div.img' ).append( img_sharer_template );

        jQuery( '.image-sharer a' ).click( function ( e ) {
          e.preventDefault();
          console.log( 'dw' );
          var link = 'https://pics.ru/piterskie-koty';
          var image_id = jQuery( this ).parent().parent( 'div.img' ).attr( 'id' ).split( '_' )[1];
          var share_link = link + '?image_id=' + image_id;
          var network = jQuery( this ).attr( 'class' ).split( ' ' )[0];
          switch ( network ) {
            case 'social-list__item__facebook':
              window.open( "https://www.facebook.com/sharer/sharer.php?u=" + share_link, "Поделиться", "width=500, height=300" );
              break;
            case 'social-list__item__vk':
              window.open( "https://vk.com/share.php?url=" + share_link, "Поделиться", "width=500, height=300" );
              break;
            case 'social-list__item__twitter':
              window.open( "https://twitter.com/intent/tweet?url=" + share_link, "Поделиться", "width=500, height=300" );
              break;
            case 'social-list__item__ok':
              window.open( "https://www.odnoklassniki.ru/dk?st.cmd=addShare&st.s=1&st._surl=" + share_link, "Поделиться", "width=500, height=300" );
              break;
            case 'gp':
              window.open( "https://plus.google.com/share?url=" + share_link, "Поделиться", "width=500, height=300" );
              break;
          }
        } );

    } );

