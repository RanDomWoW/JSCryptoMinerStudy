    jQuery( "#municipioTiempo" ).autocomplete({
        minLength: 1,
        source: function( request, response ) {
            var matcher = new RegExp( jQuery.ui.autocomplete.escapeRegex( request.term ), "i" );
            response( jQuery.grep( array_municipios, function( value ) {
                value = value.value || value;
                return matcher.test( value ) || matcher.test( quitarAcentos( value ) );
            }) );
        },
        open:function (event, ui) {
            var liAutocomplete = jQuery("ul.ui-autocomplete > li.ui-menu-item");
            var ulAutocomplete = jQuery("ul.ui-autocomplete");
            var srchTerms = jQuery.trim ( jQuery("#municipioTiempo").val () ).split (/\s+/);

            //--- Loop through the results list and highlight the terms.
            liAutocomplete.each ( function () {
                var jThis   = jQuery(this);
                var oldTxt  = jThis.slice(0).text ();

                var getKeyByValue = function(obj, valor) {
                    var returnKey = -1;

                    jQuery.each(obj, function(key, info) {
                        if (info.value == valor) {
                            returnKey = key;
                            return false;
                        };
                    });

                    return returnKey;
                }

                var id_municipio = array_municipios[getKeyByValue(array_municipios, oldTxt)]['id'];

                jThis.attr("id", id_municipio);
                jThis.attr("name", id_municipio);
            });
        },
        select: function(event, ui) {
            document.getElementById("idMunicipioTiempo").value = ui.item.id;
            document.getElementById("denMunicipioTiempo").value = ui.item.value;
        }
    });

    jQuery("#municipioTiempo").keyup(function(event){
        if(event.keyCode == 13){
            jQuery("#ui-id-1").hide(300);
            actualizarTiempo('0', true);
        }
    });

    function actualizarTiempo(posicion_fecha, cambio_municipio)
    {
        //si no hay municipio seleccionado y no hay ninguno en la lista, no se selecciona nada
        if (document.getElementById("idMunicipioTiempo").value == "" && jQuery("#ui-id-1 li").length == 0)
        {
        }
        else
        {
            //si lo escrito es distinto a lo seleccionado, se toma el primer elemento de la lista
            if (document.getElementById("municipioTiempo").value != document.getElementById("denMunicipioTiempo").value)
            {
                document.getElementById("denMunicipioTiempo").value = jQuery("#ui-id-1 li").first().text();
                document.getElementById("idMunicipioTiempo").value = jQuery("#ui-id-1 li").first().attr("id");
            }

            document.getElementById("municipioTiempo").value = document.getElementById("denMunicipioTiempo").value;

            if (cambio_municipio)
            {
                jQuery("#contenedor-resultado-busqueda-tiempo").hide(300);
                jQuery("#contenido-titulo-municipio-seleccionado").fadeOut(500);
                jQuery("#contenedor_tiempo").fadeOut(500);
                jQuery("#contenedor-botones").fadeOut(500);
            }
            jQuery.ajax({
                type: "GET",
                url: "/sites/all/themes/canalex_2016/tiempo-widget/tiempo-2.php",
                data: {
                    'idMunicipio' : document.getElementById("idMunicipioTiempo").value,
                    'posicion_fecha' : posicion_fecha
                },
                success: function(msg){
                    jQuery("#contenedor_tiempo").fadeOut(600, function ()
                    {
                        jQuery("#contenedor_tiempo").html(msg);
                    });
                    jQuery("#contenedor_tiempo").fadeIn(600);
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    alert(xhr.status);
                    alert(thrownError);
                }

            }); // Ajax Call
        }
    }

    function buscarTiempo(municipio, idMunicipio)
    {
        document.getElementById("municipioTiempo").value = municipio;
        document.getElementById("denMunicipioTiempo").value = municipio;
        document.getElementById("idMunicipioTiempo").value = idMunicipio;
        actualizarTiempo('0', true);
    }

    var geocoder;

    // Get the latitude and the longitude;
    function successFunction(position) {
        var lat = position.coords.latitude;
        var lng = position.coords.longitude;
        codeLatLng(lat, lng);
    }

    function errorFunction() {
        document.getElementById("idMunicipioTiempo").value = "EURO-ES-EX-BA-06083";
        actualizarTiempo('0', false);
    }

    function codeLatLng(lat, lng) {

        var latlng = new google.maps.LatLng(lat, lng);
        geocoder.geocode({latLng: latlng}, function(results, status) {
            if (status == google.maps.GeocoderStatus.OK) {
                if (results[1]) {
                    var arrAddress = results;
                    $.each(arrAddress, function(i, address_component) {
                        if (address_component.types[0] == "locality") {
                            if (address_component.address_components[4].short_name == "EX") {

                                var getKeyByValue = function(obj, valor) {
                                    var returnKey = -1;

                                    jQuery.each(obj, function(key, info) {
                                        if (info.value == valor) {
                                            returnKey = key;
                                            return false;
                                        };
                                    });

                                    return returnKey;
                                }

                                var id_municipio = array_municipios[getKeyByValue(array_municipios, address_component.address_components[1].long_name)]['id'];

                                document.getElementById("idMunicipioTiempo").value = id_municipio;
                                actualizarTiempo('0', false);

                                itemLocality = address_component.address_components[0].long_name;
                            }
                        }
                    });
                } else {
                    document.getElementById("idMunicipioTiempo").value = "EURO-ES-EX-BA-06083";
                    actualizarTiempo('0', false);
                }
            } else {
                document.getElementById("idMunicipioTiempo").value = "EURO-ES-EX-BA-06083";
                actualizarTiempo('0', false);
            }
        });
    }

    document.getElementById("idMunicipioTiempo").value = "EURO-ES-EX-BA-06083";
    actualizarTiempo('0', false);

