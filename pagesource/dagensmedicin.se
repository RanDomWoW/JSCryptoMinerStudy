<script>
	$(function () {
		$('#formlogin').keydown(function (e) {
			var code = e.keyCode || e.which;
			
			if (code == 13) {
			    
			    e.preventDefault();

			    var pane = $(this).closest('.formlogin');
			    validating(pane);

			    if ($(pane).valid())
			        $(pane).submit();

				//if ($("#formlogin").valid()) {
				    
				//    $("#formlogin").submit();
				//} else {
				//    alert("Error");
				//}
			}
		});

		$('body').on('click', '.submitterz', function (e) {
			e.preventDefault();

			var pane = $(this).closest('.formlogin');
			validating(pane);

			if ($(pane).valid())
				$(pane).submit();
		});

		function validating(pane) {
			$(pane).validate({

				rules: {
					email: {
						required: true,
						minlength: 2
					},
					pwd: {
						required: true,
						minlength: 2
					}
				},
				messages: {
					email: {
						required: "Måste ange e-post",
						minlength: "För kort!"
					},
					pwd: {
						required: "Måste ange lösenord",
						minlength: "För kort!"
					}
				},
				submitHandler: function (form) {
				    //alert("debug: Valid form, goping to login user");
					login_user(form);
				},
				errorPlacement: function (error, element) {
					error.insertBefore(element);
				}
			});
		}
	});

	function login_user(pane) {
	    
		var email = $(pane).find('.email').val();
		var pwd = $(pane).find('.pwd').val();
		var persist = "false";
	    var pageid = '7';

		if ( $(pane).find('.chkRememberMe').prop("checked"))
			persist = "true";

		var url = "/Services/ajax/proxy.ashx";
		var values = { email: email, pwd: pwd, persist: persist, pageid: pageid };
		
		var mess = "Registrering klar";
		var ok = "true";

		$.ajax({
			type: "GET",
			url: url,
			data: values,
			async: true,
			cache: false,
			beforeSend: function () {
				$(pane).find('#loader').show();
				$(pane).find(".submitterz").attr("disabled", "disabled");
				$(pane).find(".submitterz").attr("style", "background:none;background-color:#005869;color:#666 !important;");
			},
			success: function (html) {
				if (html.indexOf("|")) {
					mess = html.split('|')[1];
					ok = html.split('|')[0];
				}

				if (html.indexOf("true") > -1 && html.indexOf("true") < 10) {
				    location.href = '/';
				} else {
                    // stuck at
                    if (html.indexOf("edir|") > -1) {
                        var uu = html.replace("redir|", "");
                        uu = uu.replace("/true", "/");
				        location.href = uu;
                    } else if (mess.indexOf("ypage") > -1) {
                        location.href = '/min-sida-nya/?fields=required';
                    } else {
                        alertbox("red", mess);
                    }
				}

				$(pane).find('.loader').hide();
			},
			complete: function () {
				
				$(pane).find('.loader').hide();
			},
		});

		
	}
</script>

			  
