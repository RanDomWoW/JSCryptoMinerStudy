    function Voted9fd() {

        var ids = $(".poll-content.currentblockd9fd input:radio:checked").map(function () {
            return $(this).val();
        }).get().join(",");

        if (ids.length < 1) {
            alert("Must select one item");
            return false;
        }

        var url = "/Services/ajax/PollProxy.aspx";
        var values = { action: "poll", voteindex: ids, blockid: '398751' };

		var response = $.ajax({
		    type: "GET",
		    url: url,
		    data: values,
		    async: true,
		    cache: false,
		    beforeSend: function () {
                $('#loaderunod9fd').show();
		    },
		    success: function (html) {
                $('#pollformd9fd').hide();
                $('.poll-content-answered.currentblockd9fd').show();
		    },
		    complete: function () {
                $('#loaderunod9fd').hide();
		    }
		});

		return false;
    }

    function LoadResultd9fd() {

        $('.poll-content-answered.currentblockd9fd').hide();
        

        var url = "/Services/ajax/PollProxy.aspx";

		var response = $.ajax({
		    type: "GET",
		    url: url,
		    data: {blockid: '398751'},
		    async: true,
		    cache: false,
		    beforeSend: function () {
                $('#loaderd9fd').show();
		    },
		    success: function (html) {
                $('#pollformd9fd').html(html);
                $('#pollformd9fd').show();
		    },
		    complete: function () {
                $('#loaderd9fd').hide();
                $('#loaderzd9fd').hide();
		    }
		});

		return false;
    }
</script>

