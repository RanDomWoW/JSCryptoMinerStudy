<script language="javascript">

   function createSticky2(sticky) {
       if (typeof sticky !== "undefined") {
           $(window).on("scroll", function() {
               if($(window).scrollTop() >= ($('.FooterContainr').offset().top - $(window).height())){
                   sticky.addClass("FooterScrollUpNotFixed");
               }else{
                   sticky.removeClass("FooterScrollUpNotFixed");
               }
           });
           $(window).scrollTop() >= $('.FooterContainr').offset().top ? sticky.addClass("FooterScrollUpNotFixed") : sticky.removeClass("FooterScrollUpNotFixed");
       }
   }
   createSticky2($(".FooterScrollUp"));

   $(".FooterScrollUp").click(function() {
       $("html, body").animate({ scrollTop: 0 }, "slow");
       return false;
   });

   if($(document).height()<=$(window).height()){
       $(".FooterScrollUp").hide();
   }
</script>
