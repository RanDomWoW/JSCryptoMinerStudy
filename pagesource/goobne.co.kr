	//쿠키 셋팅
	function setCookie (name,value,expiredays){
	  var exdate = new Date();
	  exdate.setDate(exdate.getDate()+expiredays);
	  
	  var cookie = null;
	  cookie = document.cookie;
	  cookie = name+"="+escape(value)+ "; path=/" + ((expiredays==null)?"":";expires="+exdate.toGMTString());
	  //cookie.Path = "/";
	  document.cookie = cookie;
	}
	
