	//퀵메뉴
	function quick(){
		var elem = $("#quick"),
			elemTop = elem.offset().top,
			elemH = elem.height();
		$(window).bind('scroll resize',function(){
			scrollQuick()
		});
		function scrollQuick(){
			var winTop = $(window).scrollTop(),
				bodyH =$('body').height(),
				bottom = bodyH - (elemH);
			if (winTop > elemTop){
				elem.addClass('fixed');
			}else{
				elem.removeClass('fixed');
			}
		}
	} 
	
