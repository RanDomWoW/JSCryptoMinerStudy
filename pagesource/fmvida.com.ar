		<script>
			
		
			$(document).ready(function() {
				
				$(".open-popin-contacto").click(function(){
					$("#popin-contacto").show();
				});
				$(".contact-form-close").click(function(){
					$("#popin-contacto").hide();
				});
				
				
				$(".enviar").click(function(){
					
					var flagError = "no";
					
					if($("#field1").val()==""){
						flagError = "si";
					}
					if($("#field2").val()==""){
						flagError = "si";
					}
					if($("#field4").val()==""){
						flagError = "si";
					}
					console.log(flagError);
					if(flagError=="no"){
						$.ajax({
							url: "enviar_email.php", 
							data: {field1: $("#field1").val(), field2: $("#field2").val(), field3: $("#field3").val(), field4: $("#field4").val()},
							success: function(result){
								$("#div1").html(result);
								$('.contact-form-fields').hide();
								$('.message-ok').show();
								$('.message-error').hide();
							}
						});
					}else{
						$('.message-error').show();
					}
				});
			});
		</script>
		
