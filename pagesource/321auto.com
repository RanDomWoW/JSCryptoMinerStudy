    <script type="text/javascript">
        $(document).ready(function(){
			
			// au changement de la marque : binding de la ddl des modèles (bloc des véhicules occasion)
    $("#ddlMarquesOccasion").on("change", function () {
        $("#ddlModelesOccasion option").remove();
        $("#ddlModelesOccasion").append("<option value=''>Mod&egrave;le</option>");
        if ($("#ddlMarquesOccasion option:selected").val() != null && $("#ddlMarquesOccasion option:selected").val() != "") {
            $.ajax({
                url: "/modelByAjax?marque=" + $("#ddlMarquesOccasion option:selected").val(),
                timeout: "120000",
                success: function (response) {
                	var mlist = response.list;
                    for (var i = 0; i < mlist.length; i++) {
                    	if(mlist[i].modele != "") {
                    		$("#ddlModelesOccasion").append("<option title='"+mlist[i].nb+" annonces " + $("#ddlMarquesOccasion option:selected").val() + " " + mlist[i].modele + " occasion' value='" + mlist[i].modele + "'>" + mlist[i].modele + "</option>");
                    	}
                    }
                    if ($('#ddlModelesOccasion').is(':disabled')) {
                        $('#ddlModelesOccasion').removeAttr('disabled');
                    }
                }
            });
        }
        else {
            if ($('#ddlModelesOccasion').is(':enabled')) {
                $('#ddlModelesOccasion').attr("disabled", "disabled");
            }
        }
    });
    
    
    // au changement de la marque : binding de la ddl des modèles (bloc des véhicules neufs)
    $("#ddlMarquesNeuf").on("change", function () {
        $("#ddlModelesNeuf option").remove();
        $("#ddlModelesNeuf").append("<option value=''>Mod&egrave;le</option>");

        if ($("#ddlMarquesNeuf option:selected").val() != null && $("#ddlMarquesNeuf option:selected").val() != "") {
            $.ajax({
