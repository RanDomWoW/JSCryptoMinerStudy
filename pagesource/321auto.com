                    }
                    if ($('#ddlModelesNeuf').is(':disabled')) {
                        $('#ddlModelesNeuf').removeAttr('disabled');
                    }
                }
            });
        }
        else {
            if ($('#ddlModelesNeuf').is(':enabled')) {
                $('#ddlModelesNeuf').attr("disabled", "disabled");
