				if(resultado == 22)	enviado = '<span class="sent">Mensaje Enviado! Gracias por Suscribirte.</span>';
				if(resultado == 23)	enviado = '<span class="sent">Mensaje Enviado al Locutor!.</span>';
				form.find('span.response').hide();
				form.find('span.response').html(enviado);
				form.find('span.response').fadeIn("slow");
