		error = '<span class="error">* Debe llenar todos los campos.</span>';
		enviado = '';
		if(resultado == 1){
			form.find('span.response').hide();
			form.find('span.response').html(error);
			form.find('span.response').fadeIn("slow");
		}else{

			if( resultado == 21 || resultado == 22 || resultado == 23){
