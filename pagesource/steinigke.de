        <script type="text/javascript">
            $(document).ready(function () {
                /* enable auto suggest */
                $('#searchparam').focus();         // set the focus to the search input
                $('#test_UsrOpt2_usr').focus();    // set the focus to the Customer Login (login,account,forgotpw)
                $('#artnid').focus();               // set the focus to the Customer Login (basket)
                $('#searchparam').autocomplete({
                    source: function( request, response ) {
                        $.ajax({
                            url: 'http://www.steinigke.de/ajax.php?cl=mfajax&fnc=getAutoSuggest&mode=json&id=searchparam',
                            dataType: "json",
                            data: { data: request.term },
                            error: function( jqXHR, textStatus, errorThrown ) {console.log(jqXHR); console.log("error " + errorThrown + "  " + textStatus);},
                            success: function( data ) { response( data ); }
                        });
                    },
                    minLength: 2,
                    select: function( event, ui ) { if( ui.item.value != " " ) { window.location=ui.item.value; } else { if( ui.item.id == "more" ) { $('#search').submit(); }}return false; },  // goto link
                    focus: function( event, ui ) { return false; }   // do not copy the selected value in the input field
                }).data("ui-autocomplete")._renderItem = function (ul, item) {
                    return $("<li><\/li>")  // '\/li used to avoid w3c errors!
                            .data("item.autocomplete", item)
                            .append(item.label)
                            .appendTo(ul);
                };
                shopNavi = new ShopNavigation("http://www.steinigke.de/", "http://www.steinigke.de/out/steinigke/src/", "Bitte Kategorie auswählen.");
                var $naviObj = $('.categoryItem.category', '#categoryBar');
                /*                    $naviObj.toggle(
                            shopNavi.defaultShow,
                            shopNavi.defaultHide
                    );*/
                                var maxLevel = $('div.result', '#navi-categories-content').size();
                shopNavi.collapseLevel(maxLevel, $('.level' + maxLevel + ' .aktiv a', '#navi-categories-content').html(), 100);
                // basket drop down
                var basket = false;
                if ($('#header-basket').size() == 1) {
                    basket = '#header-basket';
                } else {
                    basket = '#header-basket-filled';
                }
                $(basket).click(function () {
                    if ($(this).hasClass('opened')) {
                        $(this).removeClass('opened');
                        $('#ajaxBasket').slideUp(250);
                        $(document).unbind('click');
                        setContentActive();
                    } else {
                        closeLanguages();
                        $('#ajaxBasketLoader').slideDown(250);
                        $('#ajaxBasket').load('http://www.steinigke.de/ajax.php?cl=mfajax&fnc=getBasket&mode=html', function () {
                            $('#ajaxBasketLoader').slideUp(250);
                            $(this).slideDown(250);
                        });
                        $(this).addClass('opened');
                        setContentInactive(true);
                    }
                    return false;
                });
                $(document).bind('click', function (event) {
                    if ($(event.target).parents('#ajaxBasket').size() == 0 && $(basket).hasClass('opened')) {
                        $(basket).removeClass('opened');
                        $('#ajaxBasket').slideUp(250);
                        $(document).unbind('click');
                        setContentActive();
                    }
                    if ($(event.target).parents('#login-header').size() == 0 && $('#header-account').hasClass('header-account-open')) {
                        $('#header-account').removeClass('header-account-open');
                        $('#login-header').slideUp(250);
                        setContentActive();
                    }
                });
            });
        </script>
