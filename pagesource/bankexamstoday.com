}
.post-meta, .post-header {  margin-bottom: 10px;  width: 100%;  color: #acacac;   font-size: 14px; font-style: italic;  text-transform: initial; text-align: center; }
.post-meta a, .post-header a {  color: #3d3d3d; font-size: 16px; font-weight: bold; }
.post-meta a:hover, .post-header a:hover {  color: #3E3E3E;  }
.post-meta {  padding: 10px 0 0;  }
.rmlink a {  float: right;  font-weight: bold;  }
.post-body h3, .social{ position: relative;
padding: 10px;
background-color: #eeeeee;
