                $("#popup-callback #ostrov_callback_phone").mask('+7 (9##) ###-####');
            } else if ($(this).val() == '0') {
                $("#popup-callback #ostrov_callback_phone").mask('+38 (0##) ###-####');
            } else {
                $("#popup-callback #ostrov_callback_phone").unmask();
            }
        });

