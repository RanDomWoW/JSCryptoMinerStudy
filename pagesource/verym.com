function side_move(){
    var objTop1 = $('#article').offset().top;
    var objTop2 = $('#real_case').offset().top;
    var objTop3 = $('#hospital').offset().top;
    var objTop4 = $('#strategy').offset().top;
    var objTop5 = $('#doctor').offset().top;
    var objTop6 = $('#video').offset().top;
    var objTop7 = $('#power').offset().top;
    $(window).scroll(function(){
        var top = $(document).scrollTop()+200;
        if( top>= objTop1 ){
            $('.side_nav ul').css({'position':'fixed','display':'block'})
            if(top >= objTop7){
                $('.side_nav li').eq(6).addClass('on').siblings().removeClass('on');
            }else if(top >= objTop6){
                $('.side_nav li').eq(5).addClass('on').siblings().removeClass('on');
            }else if(top >= objTop5){
                $('.side_nav li').eq(4).addClass('on').siblings().removeClass('on');
            }else if(top >= objTop4){
                $('.side_nav li').eq(3).addClass('on').siblings().removeClass('on');
            }else if(top >= objTop3){
                $('.side_nav li').eq(2).addClass('on').siblings().removeClass('on');
            }else if(top >= objTop2){
                $('.side_nav li').eq(1).addClass('on').siblings().removeClass('on');
            }else if(top >= objTop1){
                $('.side_nav li').eq(0).addClass('on').siblings().removeClass('on');
            }
        }else{
            $('.side_nav ul').attr('style','');
            $('.side_nav li span').removeClass('on');
        }
    })
    function toMove(obj1,obj2){
        var top = $(obj2).offset().top-30;
        $('body,html').animate({ scrollTop: top },300);
    }
    $('.side_nav li').each(function(){
        $(this).click(function(){
            var id = $(this).attr('data-id');
            toMove($(this),id);
        })
    })
}
