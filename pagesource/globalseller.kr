.ms-overview .tve_table>tbody>tr>td:nth-of-type(1) {width:80px !important}

/* COMMON STYLE */
.clear {clear:both}

.land-font {font-size:10px}

.land-box {margin:0 0 20px;padding:15px;background:#eaeaea}

.land-a {display:inline-block;text-decoration:none !important}
.land-a.small {font-size:1.2em}
.land-a.regular {font-size:1.4em}
.land-a.large {font-size:1.6em}
.con .land-a.small {font-size:0.857em}
.con .land-a.regular {font-size:1em}
.con .land-a.large {font-size:1.143em}
.land-a.box {padding:15px}
.land-a.box.black {background:#4a4a4a;color:#fff}
.land-a.box.gray {background:#f6f6f6;color:#4c4c4c}

.togr {position:relative}
.togr .togr-item {margin:0 0 5px}
.togr .togr-btn {position:relative;margin:0;padding:8px 35px 8px 25px;width:100%;border:1px solid #ccc;background:#fff;color:#777;text-align:left;cursor:pointer;outline:0}
.togr .togr-btn .fa {position:absolute;top:0;right:0;padding:8px 15px}
.togr .togr-btn .on-ico {display:none}
.togr .togr-btn-on {color:inherit;border-color:#666}
.togr .togr-btn-on .on-ico {display:inline-block}
.togr .togr-btn-on .off-ico {display:none}
.togr .togr-tog {display:none;padding:15px 25px}
.togr .togr-tog-on {display:block}

.txt-center {text-align:center}
.txt-right {text-align:right}

.round {border-radius:7px}
.round-large {border-radius:10px}
.round-xlarge {border-radius:15px}

.con.lined {padding:0 0 0 10px;border-left:2px solid #e7e7e7}

.bg-gray {background:#eaeaea !important}

/* FONT AWESOME */
.fa--05x {font-size:0.5em !important}
.fa--075x {font-size:0.75em !important}
.fa--15x {font-size:1.5em !important}
.fa--2x {font-size:2em !important}
.fa--25x {font-size:2.5em !important}
.fa--3x {font-size:3em !important}
.fa--35x {font-size:3.5em !important}
.fa--4x {font-size:4em !important}
.fa--45x {font-size:4.5em !important}
.fa--5x {font-size:5em !important}

/* GRID */
.land-grid {}
.land-grid:after {display:block;visibility:hidden;clear:both;content:''}
.land-grid .item {-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box}

/* LANDING TITLE */
.land-title {position:relative;top:-60px;padding:70px 0}
.land-title:after {display:block;visibility:hidden;clear:both;content:''}
.land-title .tve-page-section-out {background-size:cover !important;background-position:center;background-repeat:no-repeat}
.land-title .big {position:relative;margin:0 0 30px;padding:30px 40px 40px}
.land-title .big:before {display:block;z-index:1;position:absolute;top:0;right:-2000px;left:0;height:100%;background-image:-moz-linear-gradient(-70deg, #4c4c4c 0%, #404040 100%);background-image:-webkit-linear-gradient(-70deg, #4c4c4c 0%, #404040 100%);background-image:-ms-linear-gradient(-70deg, #4c4c4c 0%, #404040 100%);opacity:0.95;content:''}
.land-title .big .title, .land-title .big .copy {z-index:1;position:relative;color:#fff}
.land-title .big .title {padding:40px 0 0;font-size:4.4em;font-weight:bold}
.land-title .big .copy {padding:20px 0 0;line-height:1.4em;font-size:1.8em}
.land-title .sub .pad {padding:30px 40px 40px !important;background-image:-moz-linear-gradient(-70deg, #4c4c4c 0%, #404040 100%);background-image:-webkit-linear-gradient(-70deg, #4c4c4c 0%, #404040 100%);background-image:-ms-linear-gradient(-70deg, #4c4c4c 0%, #404040 100%);color:#fff;opacity:0.95;
-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box
}
.land-title .sub .title {margin:0 0 20px;font-size:2.8em;font-weight:bold}
.land-title .sub .copy {line-height:1.6em;font-size:1.5em}

/* LANDING BUTTON */
.land-button {position:relative;top:-60px;padding:50px 30px;text-align:center}
.land-button.bottom {top:0}
.land-button .desc {margin:0 0 10px;color:#fff;font-size:1.5em}
.land-button .highlight {margin:0 0 20px;color:#ffed00;font-size:1.5em}
.land-button .button {margin:0 0 20px;font-size:1.5em}
.land-button .button .a {padding:15px 5px;color:#000;text-decoration:none;
-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box
}
.land-button .button .a {background:#40362a;color:#fff}
/*.land-button .button .a:nth-child(odd) {background:#2eecb3}*/
/*.land-button .button .a:nth-child(even) {background:#f7e44c}*/
/*.land-button .button-reverse .a:nth-child(odd) {background:#f7e44c}*/
/*.land-button .button-reverse .a:nth-child(even) {background:#2eecb3}*/

/* SECTION */
.land-sect {margin:0 0 70px}
.land-sect .sect-title {position:relative;margin:0 0 15px;font-size:2.2em;font-weight:bold}
.land-sect .sect-title:before {display:block;z-index:-1;position:absolute;bottom:2px;left:0;width:100%;height:1px;background:#000;content:''}
.land-sect .sect-title span {display:inline-block;padding:0 10px 0 0;background:#fff}
.land-sect .sub-title {margin:0 0 10px}
.land-sect .sub-title.small {font-size:1.2em}
.land-sect .sub-title.regular {font-size:1.4em}
.land-sect .sub-title.large {font-size:2em}

.land-sect .table {font-size:1.4em}
.land-sect .table table {width:100%;border:0;border-spacing:0;border-collapse:collapse}
.land-sect .table thead th {padding:15px;border:1px solid #fff;background:#c2b18a;color:#fff}
.land-sect .table tbody th {padding:15px;width:25%;border:1px solid #fff;background:#867b60;color:#fff}
.land-sect .table tbody td {padding:15px;border:1px solid #fff}

.land-sect .con {font-size:1.4em}
.land-sect .question {margin:0 0 10px;font-weight:bold}

.land-sect .quote {position:relative;margin-bottom:10px;padding:40px 0 0;font-size:2em;font-style:normal}
.land-sect .quote .fa {position:absolute;top:15px;left:0}
.land-sect .line-quote {display:block;padding:30px 0;text-align:center}
.land-sect .line-quote .fa-quote-left {margin:0 5px 0 0}
.land-sect .line-quote .fa-quote-right {margin:0 0 0 5px}
.land-sect .line-quote.small {font-size:1.2em}
.land-sect .line-quote.regular {font-size:1.6em}
.land-sect .line-quote.large {font-size:2em}

.land-sect .land-box {font-size:1.6em}

/* OVERVIEW */
.land-overview {position:relative}
.land-overview .thrv_wrapper:after {display:block;visibility:hidden;clear:both;content:''}
.land-overview .title {margin:0 0 30px;font-size:3.5em;font-weight:bold}

.land-overview .ov, .land-overview .price {padding:30px;
-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box
}
.land-overview .ov {}
.land-overview .ov .item {margin:0 0 20px;font-size:1.6em}
.land-overview .ov .item:after {display:block;visibility:hidden;clear:both;content:''}
.land-overview .ov .subject {position:relative;float:left;width:30%}
.land-overview .ov .subject:before {display:block;z-index:-1;position:absolute;top:50%;left:0;margin:-1px 0 0;width:100%;height:1px;background:#000;content:''}
.land-overview .ov .subject span {display:inline-block;padding:0 10px 0 0;background:#fff}
.land-overview .ov .desc {float:left;padding:0 0 0 30px;width:70%;
-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box
}
.land-overview .price {color:#fff}
.land-overview .price .desc {margin:0 0 20px;font-size:1.6em}
.land-overview .price .highlight {margin:0 0 30px}
.land-overview .price .highlight span {background:#fff;color:#ef2048;font-size:1.5em;font-weight:bold}
.land-overview .price strike {color:#888;text-decoration:line-through}
.land-overview .price .small {font-size:0.75em}

/* MEDIAQ */
@media all and (max-width:800px) {
.land-grid .item {margin:0 0 30px}

.land-button .button .a {display:block;margin:0 0 10px}

.land-overview, .land-overview .thrv_wrapper {padding:0 !important}
.land-overview .price {background:#4a4a4a}
}

@media all and (min-width:801px) {
.land-grid .item {float:left}
.land-grid .item .pad {padding:0 5px}
.land-grid-2 .item {width:50%}
.land-grid-3 .item {width:33%}
.land-grid-4 .item {width:25%}
.land-grid-5 .item {width:20%}

.land-title .sub {padding:0 20px 0 0}

.land-button .button:after {display:block;visibility:hidden;clear:both;content:''}
.land-button .button .a {float:left}
.land-button .button-1 .a {width:100%}
.land-button .button-2 .a {width:48%}
.land-button .button-2 .a:first-child {margin:0 4% 0 0}
.land-button .button-3 .a {width:30%}
.land-button .button-3 .a:nth-child(2) {margin:0 5%}

.land-overview:before {display:block;z-index:-1;position:absolute;top:0;right:0;bottom:0;left:50%;background:#4a4a4a;content:''}
.land-overview .ov, .land-overview .price {float:left;width:50%}
}</style>
