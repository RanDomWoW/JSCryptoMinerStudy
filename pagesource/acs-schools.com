d":"\u003cp\u003e\u003ca href=\"/ACS-American-High-School-Diploma\"\u003eUS High School Diploma \u0026 Honours\u003c/a\u003e\u003c/p\u003e\n","MobileAppVisible":null,"AreCommentsAllowed":null,"AreCommentsEnabled":null,"ExpireDate":null,"Tags":null,"Status":"PUBLISHED","CreatedBy":"schoolwebsite","CreatedDate":"\/Date(1415368500000)\/","LastUpdated":"\/Date(1418206938000)\/","LastUpdatedBy":"carlo_jolly@acs-schools.com","TotalCount":8,"IsTwitterPost":false,"IsFacebookPost":false,"EditPost":false,"CreatedDateString":"07/11/2014"}];
