            function CautaLink_v2() {
                var dst = encodeURIComponent($("#destinatie_v2").val());
                var Hotel = "";
                Hotel = encodeURIComponent($("#hotelname_v2").val());
                var Categorie = $("#selCategorie_v2").val();
                var tipunitate = $("#DropDownListTipUnitate_v2").val();
                var os = $("#DropDownListOferteSpeciale_v2").val();
                var Transport = $("#selTransportCategorie_v2").val();
                var pl = $("#selPlecareDin_v2").val();
                if (pl == null || pl == "null") {
                    pl = "";
                }
                var facilitate = $("#selFacilitati_v2").val();
                var Masa = $("#selMasa_v2").val();

                var lnk = "";
                var NrA = $("#CheckBoxListNrAdulti_v2").val();
                var NrC = parseInt($("#CheckBoxListNrCopii_v2").val());
                var ds = $("#startdate_v2").val();
                var NrN = $("#DropDownListNrNopti_v2").val();
                var vc1 = (NrC < 1 ? "0" : $("#DropDownListVC1_v2").val());
                var vc2 = (NrC < 2 ? "0" : $("#DropDownListVC2_v2").val());
                var vc3 = (NrC < 3 ? "0" : $("#DropDownListVC3_v2").val());
                if (NrC > 0 && vc1 == "0") vc1 = "5";
                if (NrC > 1 && vc2 == "0") vc2 = "7";

                var stele = $("#DropDownListStele_v2").val();

                if (stele == "*") {
                    stele = "1";
                }
                if (stele == "**") {
                    stele = "2";
                }
                if (stele == "***") {
                    stele = "3";
                }
                if (stele == "****") {
                    stele = "4";
                }
                if (stele == "*****") {
                    stele = "5";
                }

                if (lnk == "") {
                    if (dst != "" || Transport != "" || Categorie != "" || Hotel != "") {
                        lnk = "dest=" + dst + "&tr=" + Transport + "&cat=" + Categorie + "&start=1&hotel=" + Hotel;
                    }
                    lnk += "&nra=" + NrA + "&nrc=" + NrC + "&ds=" + ds + "&nrn=" + NrN + "&vc1=" + vc1 + "&vc2=" + vc2 + "&vc3=" + vc3 + "&stele=" + stele + "&masa=" + Masa;
                    lnk += "&pl=" + pl;
                    lnk += "&ff=" + facilitate;
                    if (os != "" && os != "0") {
                        lnk += "&os=" + os;
                    }
                    if (tipunitate != "" && tipunitate != "0") {
                        lnk += "&tu=" + tipunitate;
                    }
                    if (Limba == "ro") lnk = "cautare.aspx?" + lnk;
                    if (Limba == "en") lnk = "search.aspx?" + lnk;
                }
                return lnk;
            }

            function homesearch_v2() {
                var dst = encodeURIComponent($("#destinatie_v2").val());
