<script type="text/javascript">
    $("document").ready(function () {

        $('#register_address').click(function(){
            if($("#sq_by_phone_no_address").val().length > 0){
                $(this).val($('#sq_by_phone_no_address').val());
            }
        });

        $("#sq_by_phone_no_address").blur(function(){
            $('#register_address').val($(this).val());
        });

        $('.button-wrap button').click(function(){
            var addressSaved = $("#sq_by_phone_no_address").val();
            $("#modalAddress").html(addressSaved);
        });

        $("#sq_by_phone_no_address").blur(function(){
            var addressSaved = $("#sq_by_phone_no_address").val();
            $("#sq_by_address").val(addressSaved);
            $("#modalAddress").html(addressSaved);
        });


        $(".navbar-toggler").on("click", function () {
            $(this).toggleClass("active");
        });

        // Autocomplete address
        var sq_check_request_obj = null;
        var set_sq = $.cookie('default_sq');
        $("#sq_by_phone_no_address").val($.cookie('default_sq'));
        var set_sq_result = $.cookie('default_sq_result');
        $(".sq_respond_container").html(set_sq_result);
        var cache = {};
        $("#sq_by_phone_no_address").autocomplete({
            minLength: 1,
            source: function (request, response) {
                var mod_term = request.term;
                mod_term = mod_term.replace(/ /g, '+');
                if (mod_term in cache) {
                    response(cache[ mod_term ]);
                    return;
                }
                $.getJSON("../include/address_auto_complete_addressify_proxy.php", {
                    term: mod_term
                }, response);
            },
            search: function (event, ui) {
                $(this).addClass("ui-autocomplete-loading");
            },
            response: function (event, ui) {
                $(this).removeClass("ui-autocomplete-loading");
            }
        });

        $("#register_address").autocomplete({
            minLength: 1,
            source: function (request, response) {
                var mod_term = request.term;
                mod_term = mod_term.replace(/ /g, '+');
                if (mod_term in cache) {
                    response(cache[ mod_term ]);
                    return;
                }
                $.getJSON("../include/address_auto_complete_addressify_proxy.php", {
                    term: mod_term
                }, response);
            },
            search: function (event, ui) {
                $(this).addClass("ui-autocomplete-loading");
            },
            response: function (event, ui) {
                $(this).removeClass("ui-autocomplete-loading");
            }
        });

        // SQ Address
        $("#sq_check_exefone_btn").click(function (event) {
            var test_string_ad = $("#sq_by_phone_no_address").val();
            $("#sq_by_phone_no_address_form").validationEngine({promptPosition: "topLeft", scroll: false});
            var adress_not_valid = !$("#sq_by_phone_no_address_form").validationEngine("validate");
            if (adress_not_valid) {
                event.stopPropagation();
                return false;
            } else {
                sq_check_request_obj = do_sq(test_string_ad);
            }

        });

        // Redo SQ on invalid data
        $(".sq_redo").live('click', function() {
            $("#sq_by_phone_no_address").val($(this).html());
            $("#sq_fail_model").modal("hide");
            $("#sq_check_exefone_btn").click();
        });

        // Cancel SQ
        $("#sq_cancel_btn").click(function () {
            sq_check_request_obj.abort();
            $(this).hide();
            $("#sq_check_exefone_btn").show();
            $("#sq_check_select_type").show();
        });

        // Complete SQ
        $("#continute_to_exefone").on("click", function () {
            $("#sq_success_model").modal("hide");
        });

        var icons = {
            header: "ui-icon-carat-1-e",
            activeHeader: "ui-icon-carat-1-s"
        };
        $('#dropper_my_exetel_help').accordion({
            header: 'div.title',
            heightStyle: "content",
            collapsible: true,
            active: false,
            icons: icons
        });

    });

    function registerSubmit(token) {
        $("#register_form").validationEngine({promptPosition: "topLeft", scroll: false});
        if ($("#register_form").validationEngine('validate')) {
            var data = $('#register_form').serializeArray();
            $.post("../ajax/index_register.php", data)
                .done(function (data) {
                    $('.formUnsubmitted').hide();
                    $('.formSubmitted').show();
                })
                .fail(function (xhr, textStatus, errorThrown) {
                    //alert(xhr.responseText);
                });
        }
        grecaptcha.reset()
    }
    $(document).ready(function() {
        $("#sq_by_phone_no_address").focus();
        $("#sq_by_phone_no_address").typed({
            strings: ["Please enter your address or phone number..."],
            typeSpeed: 30,
            // show cursor
            showCursor: true,
            // character for cursor
            cursorChar: "|",
            attr: 'placeholder',
            backDelay: 500,
            loop: false,
            contentType: 'html', // or text
            // defaults to false for infinite loop
            loopCount: false,
        });
    });
</script>

