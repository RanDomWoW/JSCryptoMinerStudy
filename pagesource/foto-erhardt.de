<script>
    $(document).ready(function(){
        $.ajax({
            url: 'blog.php'
        }).done(function(data){
            var container = $('.content_container.blog'),
                posts = JSON.parse(data),
                postTemplate = 
                    '<div class="row">' +
                    '    <div class="col-xs-12 col-md-4">' +
                    '        <a href="{permalink}" target="blog"><img src="{post_thumbnail}" style="max-width:100%;height:auto" /></a>' +
                    '    </div>' +
                    '    <div class="col-xs-12 col-md-8">' +
                    '        <a href="{permalink}" target="blog"><h3>{post_title}</h3></a>' +
                    '        <p>{post_excerpt}</p>' +
                    '        <a href="{permalink}" target="blog" class="btn btn-primary">Weiterlesen</a>' +
                    '    </div>' +
                    '</div>';

            $(posts).each(function(){
                var post = this,
                    excerpt = post.excerpt.rendered.replace(/\[&hellip;\]/g,'&hellip;'),
                    title = post.title.rendered,
                    permalink = post.link,
                    thumbnail = post._embedded['wp:featuredmedia'][0].media_details.sizes['large'].source_url;

                container
                    .append(
                        postTemplate
                            .replace(/{permalink}/g, permalink)
                            .replace(/{post_title}/g, title)
                            .replace(/{post_excerpt}/g, excerpt)
                            .replace(/{post_thumbnail}/g, thumbnail)
                    );
            });

        });
    });
</script>
