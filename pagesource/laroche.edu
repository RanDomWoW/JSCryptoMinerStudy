<script type="text/javascript">
        // FLEXSLIDER //
(function() {
    // SLIDER //
    // tiny helper function to add breakpoints
    var $window = $(window),
      flexslider, pageCarousel;

    function getGridSize() {
        return (window.innerWidth < 525) ? 1 :
        (window.innerWidth < 770) ? 2 : 3;
    }

    //$('.home-page-slide').css("visibility", "hidden");

    $('.home-page-slide').hide();

    $window.load(function () {
    	//$('.home-page-slide').css("visibility", "visible");
    	$('.home-page-slide').fadeIn("slow");
        // FLEXSLIDER //

        $('.home-page-slide').flexslider({
            controlNav: true,
            controlsContainer: ".controls",
            directionNav: false,
            slideshow: true,
            slideshowSpeed: 15000
        });

        pageCarousel = $('.home-bottom-slide');

        moduleHeader = pageCarousel.siblings('.home-bottom-header');
        nextControl = moduleHeader.children('.module-next'),
        prevControl = moduleHeader.children('.module-prev');

        pageCarousel.flexslider({
            animation: "slide",
            itemWidth: 300,
            itemMargin: 20,
            animationLoop: true,
            minItems: getGridSize(),
            maxItems: getGridSize(),
            controlNav: false,
            directionNav: false,
            slideshow: true,
            start: function (slider) {
                flexslider = slider;
            }
        });

    //    // SET DIRECTIONAL ARROWS //

        nextControl.click(function (e) {
            pageCarousel.flexslider("next");
            e.preventDefault();
        });

        prevControl.click(function (e) {
            pageCarousel.flexslider("prev");
            e.preventDefault();
        });
    })

    $window.resize(function () {
        var gridSize = getGridSize();
        flexslider.vars.minItems = gridSize;
        flexslider.vars.maxItems = gridSize;
    });

    // EQUALIZE ALL DIV HEIGHTS //
    $.fn.equalizeHeights = function () {
        var maxHeight = this.map(function (i, e) {
            return $(e).height();
        }).get();
        return this.height(Math.max.apply(this, maxHeight));
    };

    $(".home-section > .callout .news-container").equalizeHeights();
}());


</script>


