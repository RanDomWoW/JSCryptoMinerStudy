<script type="text/javascript">

    /**
     * Funzioni per l'inizializzazione del carousel (bookGallery) e relativi popover per i link
     * associati alla copertina corrente da eseguire al caricamento della pagina
     */
    $(function() {
        $('#book-gallery').carousel({
                    pause: true,
            interval: false
                }).on('slide.bs.carousel', function () { $( "img[data-toggle='popover']" ).popover('hide') });
        $('#book-gallery').mouseleave(function () { $( "img[data-toggle='popover']" ).popover('hide')});
        $( "img[data-toggle='popover']" ).popover({trigger: 'manual', placement: 'bottom', container: '#book-gallery', html: true, content: function() { return getContent($(this)); }});
        $( "img[data-toggle='popover']" ).on('show.bs.popover', function() { hideOtherBooks($(this)); });
        $( "img[data-toggle='popover']" ).mouseenter(function () {$(this).popover('show');});
    });

    /**
     * Funzione che calcola e restituisce il contenuto del popover associato al nodo in input
     */
    function getContent(node) {
        var ean13 = node.attr('data-volume-ean13');
        var title = node.attr('data-original-title');
        var shopVolumeNode = $('#shop-volume-data-container-' + ean13);
        var shopVolumeButtons = shopVolumeNode.html();
        var shopVolumePrice = shopVolumeNode.attr('data-price');
        var shopVolumeDiscountedPrice = shopVolumeNode.attr('data-discounted');
        var shopVolumeHasDiscount = parseInt(shopVolumeNode.attr('data-has-discount'));
        var shopVolumeHasEbook = parseInt(shopVolumeNode.attr('data-has-ebook'));
        var shopVolumeHasEpub = parseInt(shopVolumeNode.attr('data-has-epub'));
        var shopVolumeIsForSale = parseInt(shopVolumeNode.attr('data-for-sale'));
        var shopVolumeIsOnlyDigital = parseInt(shopVolumeNode.attr('data-is-only-digital'));
        var shopVolumeIsPodReady = parseInt(shopVolumeNode.attr('data-is-pod-ready'));

        if (shopVolumeHasEbook == 1) {
            var shopEbookNode = $('#shop-ebook-data-container-' + ean13);
            var shopEbookButtons = shopEbookNode.html();
            var shopEbookPrice = shopEbookNode.attr('data-price');
            var shopEbookDiscountedPrice = shopEbookNode.attr('data-discounted');
            var shopEbookHasDiscount = shopEbookNode.attr('data-has-discount');
            var shopEbookFiles = jQuery.parseJSON(shopEbookNode.attr('data-ebook-files'));
        }

        var nodeContent = '<div class="icon-chain">';
        if (shopVolumeHasEbook == 1 && shopVolumeHasEpub == 1) {
            nodeContent = nodeContent + '<div class="icon-inline-btn"><a class="scheda-ref" href="/isbn/' + ean13 + '">Leggi scheda</a></div>';
        }
        else {
            nodeContent = nodeContent + '<div class="icon-inline-btn-single"><a class="scheda-ref" href="/isbn/' + ean13 + '">Leggi scheda</a></div>';
        }
        if (shopVolumeHasEbook == 1 && shopVolumeHasEpub == 1) {
            nodeContent = nodeContent + '<div class="icon" title="Sfoglia"><a href="/isbn/' + ean13 + '#sfoglia"><img src="/images/Main/icons/read-black-23x23.png" alt="icon-23x23-read-black" width="23" height="23"/></a></div>';
        }
        nodeContent = nodeContent + '</div>'
        nodeContent = nodeContent + '<div class="book-detail">';

        if (shopVolumeIsForSale == 1) {
            nodeContent = nodeContent + '<div class="book-buy">';
            nodeContent = nodeContent + '<div>';
            nodeContent = nodeContent + '<h2 class="popover-buy-label">Acquista:</h2>';
            if (!shopVolumeIsOnlyDigital) {
                if (shopVolumeHasEbook == 1) {
                    nodeContent = nodeContent + '<div class="book-buy-type clearfix">';
                } else {
                    nodeContent = nodeContent + '<div class="book-buy-type clearfix last">';
                }
                if (shopVolumeHasDiscount) {
                    nodeContent = nodeContent + '<div class="book-buy-type-text">volume a stampa <span class="strike">&euro; ' + shopVolumePrice + '</span> <span class="discouted-price">&euro; ' + shopVolumeDiscountedPrice + '</span>';
                    if (shopVolumeIsPodReady) {
                                                nodeContent = nodeContent + '<div class="pod-ready"><!-- volumi_podready_buymessage tpl --> available in 10-15 days</div>';
                    }
                    nodeContent = nodeContent + '</div>';
                } else {
                    nodeContent = nodeContent + '<div class="book-buy-type-text">volume a stampa &euro; ' + shopVolumePrice;
                    if (shopVolumeIsPodReady) {
                                                nodeContent = nodeContent + '<div class="pod-ready"><!-- volumi_podready_buymessage tpl --> available in 10-15 days</div>';
                    }
                    nodeContent = nodeContent + '</div>';
                }
                nodeContent = nodeContent + '<div class="icon-chain">';
                nodeContent = nodeContent + '<div class="icon">' + shopVolumeButtons + '</div>';
                //nodeContent = nodeContent + '<div class="icon"><a href=""><img src="/images/Main/icons/wishlist-black-23x23.png" alt="icon-23x23-read-black" width="23" height="23"></a></div>';
                nodeContent = nodeContent + '</div>';
                nodeContent = nodeContent + '</div>';
            }

            if (shopVolumeHasEbook == 1) {
                nodeContent = nodeContent + '<div class="book-buy-type clearfix last">';

                if (shopEbookHasDiscount) {
                    nodeContent = nodeContent + '<div class="book-buy-type-text">e-book <span class="strike">&euro; ' + shopEbookPrice + '</span> <span class="discouted-price">&euro; ' + shopEbookDiscountedPrice + '</span>';
                } else {
                    nodeContent = nodeContent + '<div class="book-buy-type-text">e-book &euro; ' + shopEbookPrice;
                }
                nodeContent = nodeContent + '<div class="book-buy-type-format">Formato: ';
                for (var i = 0; i < shopEbookFiles.length; i++) {
                    var file = shopEbookFiles[i];
                    append = ((i+1) == shopEbookFiles.length) ? '' : ', ';
                    nodeContent = nodeContent + file.type + append;
                }
                nodeContent = nodeContent + '</div>';
                nodeContent = nodeContent + '</div>';
                nodeContent = nodeContent + '<div class="icon-chain">';
                nodeContent = nodeContent + '<div class="icon">' + shopEbookButtons + '</div>';
                //nodeContent = nodeContent + '<div class="icon"><a href=""><img src="/images/Main/icons/wishlist-black-23x23.png" alt="icon-23x23-read-black" width="23" height="23"></a></div>';
                nodeContent = nodeContent + '</div>';
                nodeContent = nodeContent + '</div>';
            }

            nodeContent = nodeContent + '</div>';
            nodeContent = nodeContent + '</div>';
        }

        nodeContent = nodeContent + '</div>';
        return nodeContent;
    }

    /**
     * Esegue la redirezione alla scheda volume del libro identificato dal codice ISBN in input
     */
    function goToSchedaVolume(ean13) {
        location.href = '/isbn/' + ean13;
    }

    /**
     * Nasconde i popover diversi da quello selezionato
     */
    function hideOtherBooks(node) {
        var ean13 = node.attr('data-volume-ean13');
        $( "img[data-toggle='popover']" ).each(function(index) { if($(this).attr('data-volume-ean13') != ean13) { $(this).popover('hide') } });
    }

</script>
