/**
 * Gestione operazioni in caso di modifica della selezione dell'argomento di ricerca
 */
function changeArgomentoHeader() {
	var argomento = dojo.byId("select-argomento-header").value;
	if(argomento == "") {
		cleanOptions("select-sottoargomento-header");
		cleanOptions("select-materia-header");
		dojo.byId("select-sottoargomento-header").add(dojo.create("option", { innerHTML:"Subsubject", value:"" }));
		dojo.byId("select-materia-header").add(dojo.create("option", { innerHTML:"Subject", value:"" }));
	}
	else {
		cleanOptions("select-sottoargomento-header");
		cleanOptions("select-materia-header");
		dojo.byId("select-sottoargomento-header").add(dojo.create("option", { innerHTML:"Subsubject", value:"" }));
		$.each(sottoargomentiHeader[argomento], function(key, value){
			dojo.byId("select-sottoargomento-header").add(dojo.create("option", { innerHTML:value, value:key, id:"sottoargomento_header_" + key }));
		});
		dojo.byId("select-materia-header").add(dojo.create("option", { innerHTML:"Subject", value:"" }));
		$.each(materieHeader[argomento], function(key, value){
			dojo.byId("select-materia-header").add(dojo.create("option", { innerHTML:value, value:key, id:"materia_header_" + key  }));
		});
	}
}

/**
 * Rimuove tutte le options da una determinata select
 */
function cleanOptions(selectId) {
	for (var i = dojo.byId(selectId).length - 1; i>=0; i--) {
       dojo.byId(selectId).remove(i);
    }
}

