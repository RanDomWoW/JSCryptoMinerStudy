    <script type="text/javascript">
        $(document).ready(function () {
            var $searchStoreSelect = $('.search-store-select');

            $searchStoreSelect.select2({
                minimumResultsForSearch: Infinity,
                dropdownCssClass: 'search-store-select-dropdown'
            });

            options = {
                minChars: 1,
                noCache: true,
                maxHeight: 400
            };

            autocompleteCategorySlug = $searchStoreSelect.find('option:selected').attr('value');

            if (autocompleteCategorySlug) {
                options.serviceUrl = Routing.generate(
                    'search_autocomplete_by_category',
                    {"categorySlug": autocompleteCategorySlug }
                );

                options.autoSubmit = true;

                var autocompleteObj = $('#search-widget-autocomplete').autocomplete(options);

                $searchStoreSelect.change(function () {
                    autocompleteCategorySlug = $searchStoreSelect.find('option:selected').attr('value');

                    options.serviceUrl = Routing.generate(
                        'search_autocomplete_by_category',
                        {"categorySlug": autocompleteCategorySlug }
                    );

                    autocompleteObj.serviceUrl = options.serviceUrl;
                    autocompleteObj.setOptions(options.serviceUrl);
                });
            }
        });
    </script>


