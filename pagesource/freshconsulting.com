	<script type="text/javascript">
		jQuery(document).ready(function() {

			//Setting cookies for use in adsense ROI

			var cookieOptions = {expires: 90, path: '/'};

			var getURLParameter = function(name) {
				return decodeURIComponent((new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search)||[,""])[1].replace(/\+/g, '%20'))||null
			}

			var urlKeyword = getURLParameter('keyword');
			var urlGclid = getURLParameter('gclid');
			var existingKeyword = Cookies.get('initial_keyword');
			var existingReferrer = Cookies.get('referrer');
			var existingReferralDate = Cookies.get('referral_date');
			var existingInitialGclid = Cookies.get('initial_gclid');

			if(!!urlGclid) {
				//If no existing keyword cookie and a url param keyword is present
				if(!existingInitialGclid) {
					Cookies.get('initial_gclid', urlGclid, cookieOptions);

					var googleReferrer = Cookies.get('inbound_referral_site');
					if(googleReferrer !== false && googleReferrer !== null) {
						//If there is an existing inbound_referral cookie
						Cookies.get('referrer', googleReferrer, cookieOptions);
					} else {
						//Edge case where casperjs and possibly other browsers do not get a referrer on direct traffic
						Cookies.get('referrer', 'Direct Traffic', cookieOptions);
					}

					var referralDate = new Date().toISOString();
					Cookies.get('referral_date', referralDate, cookieOptions);
				}

				if(!existingKeyword && !!urlKeyword) {
					//If no existing referral date cookie
					Cookies.get('initial_keyword', urlKeyword, cookieOptions);
				} else if(!!existingKeyword && !!urlKeyword) {

					//If existing keyword cookie is present and a url param keyword is present
					var existingExtraKeywords = Cookies.get('extra_keywords');
					if(urlKeyword === decodeURIComponent(existingKeyword)) {
						//skip if url keyword is same as existing searchKeyword
					} else if(!!existingExtraKeywords && !!urlKeyword) {
						existingExtraKeywords = decodeURIComponent(existingExtraKeywords);
						Cookies.get('extra_keywords', existingExtraKeywords + ', ' + urlKeyword, cookieOptions);
					} else {
						Cookies.get('extra_keywords', urlKeyword, cookieOptions);
					}
				}
			}
		});
	</script>

