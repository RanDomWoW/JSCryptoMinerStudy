</head>
<body>
<div id="left-panel">
    <div id="left-panel-top">
        <div id="logo">
            <a href="/">
                <img src="/images/mapeia-logo.png" width="121px" height="40px"/>
            </a>
        </div>
    </div>
    <div id="left-panel-main">
        <div id="query">
            <div id="move-waypoints">
                <a href="#" id="swap" disabled="true"><span class="icon icon-swap_vert"></span></a>
            </div>
            <div id="waypoints">
                <div class="waypoint">
                    <input id="origin" type="text" placeholder="Digite a origem">
                </div>
                <div class="waypoint">
                    <input id="destination" type="text" placeholder="Digite o destino">
                </div>
            </div>
            <div id="query-bottom">
                <div>
                    <input id="fuel-price" type="text" placeholder="R$ / litro">
                    <span class="icon icon-gaspump"></span>
                </div>
                <div>
                    <input id="fuel-consumption" type="text" placeholder="Km / litro">
                    <span class="icon icon-directions_car"></span>
                </div>
                <a href="#" id="calc">Mapear <span class="icon icon-arrow-right"></span></a>
            </div>
        </div>

        <div class="results-panel" id="loading"></div>
        <div class="results-panel" id="error"></div>
        <div class="results-panel" id="results">
            <div class="results-row">
                <a class="result" id="time">
                    <span class="result-label"><span class="icon-stopwatch"></span></span>
                    <span class="result-value" id="time-value"></span>
                    <span class="tooltip">Duração</span>
                </a>
                <a class="result" id="dist">
                    <span class="result-label"><span class="icon-road"></span></span>
                    <span class="result-value" id="dist-value"></span>
                    <span class="tooltip">Distância</span>
                </a>
            </div>
            <div class="results-row">
                <a class="result total-result" id="fuel">
                    <span class="result-label"><span class="icon-gaspump"></span></span>
                    <span class="result-value" id="fuel-value"></span>
                    <span class="tooltip">Combustível</span>
                </a>
                <span class="total-result" id="plus-sign">+</span>
                <a class="result" id="tolls">
                    <span class="result-label"><img src="/images/toll-icon-dollar-black-18.png"></span>
                    <span class="result-value" id="toll-value"></span>
                    <span class="tooltip">Pedágios</span>
                </a>
                <span class="total-result" id="equal-sign">=</span>
                <a class="result total-result" id="total">
                    <span class="result-value" id="total-value"></span>
                    <span class="tooltip">Custo total</span>
                </a>
            </div>
        </div>

        <div class="ad-panel result-dependent sponsor" id="sponsor-1">
            <div id='gpt-ad-5' style='height:80px; width:300px;'>
                <script type='text/javascript'>
                    googletag.cmd.push(function () {
                        googletag.display('gpt-ad-5');
                    });
                </script>
            </div>
        </div>

        <div class="ad-panel result-dependent offer" id="hotels">
            <div class="offer-img">
                <img src="/images/hotel.jpg">
            </div>
            <div class="offer-label">
                <div>Encontre <strong>hotéis</strong> em</div>
                <div id="hotels-city"></div>
            </div>
        </div>

        <div class="ad-panel result-dependent offer" id="coach">
            <div class="offer-img">
                <img src="/images/coach.jpg">
            </div>
            <div class="offer-label">
                <div>Encontre passagens</div>
                <div>de <strong>ônibus</strong></div>
            </div>
        </div>

        <div class="ad-panel post-results" id="ad-3">
            <div id='gpt-ad-3' style='height:250px; width:300px;'>
                <script type='text/javascript'>
                    googletag.cmd.push(function () {
                        googletag.display('gpt-ad-3');
                    });
                </script>
            </div>
        </div>

        <div class="ad-panel pre-results" id="ad-1">
            <div id='gpt-ad-1' style='height:250px; width:300px;'>
                <script type='text/javascript'>
                    googletag.cmd.push(function () {
                        googletag.display('gpt-ad-1');
                        googletag.pubads().refresh([gptSlot1]);
                    });
                </script>
            </div>
        </div>

        <div class="ad-panel pre-results" id="ad-2">
            <div id='gpt-ad-2' style='height:250px; width:300px;'>
                <script type='text/javascript'>
                    googletag.cmd.push(function () {
                        googletag.display('gpt-ad-2');
                        googletag.pubads().refresh([gptSlot2]);
                    });
                </script>
            </div>
        </div>

        <div class="map-ad result-dependent" id="ad-4">
            <div id='gpt-ad-4' style='height:90px; width:728px;'>
                <script type='text/javascript'>
                    googletag.cmd.push(function () {
                        googletag.display('gpt-ad-4');
                    });
                </script>
            </div>
        </div>
    </div>
</div>
<div id="map"></div>


<script>
    googletag.cmd.push(function () {
        init({"lat": null, "lng": null, "address": null}, {"lat": null, "lng": null, "address": null});
    });
</script>

</body>
</html>