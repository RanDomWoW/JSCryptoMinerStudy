        googletag.cmd = googletag.cmd || [];
        (function () {
            var gads = document.createElement('script');
            gads.async = true;
            gads.type = 'text/javascript';
            var useSSL = 'https:' == document.location.protocol;
            gads.src = (useSSL ? 'https:' : 'http:') +
                    '//www.googletagservices.com/tag/js/gpt.js';
            var node = document.getElementsByTagName('script')[0];
            node.parentNode.insertBefore(gads, node);
        })();
    </script>
    <script type='text/javascript'>
        var gptSlot1, gptSlot2, gptSlot3, gptSlot4, gptSlot5;
        googletag.cmd.push(function () {
            gptSlot1 = googletag.defineSlot('/85930771/MapeiaHome300x250Top', [300, 250], 'gpt-ad-1').addService(googletag.pubads());
            gptSlot2 = googletag.defineSlot('/85930771/MapeiaHome300x250Bottom', [300, 250], 'gpt-ad-2').addService(googletag.pubads());
            gptSlot3 = googletag.defineSlot('/85930771/MapeiaResults300x250', [300, 250], 'gpt-ad-3').addService(googletag.pubads());
            gptSlot4 = googletag.defineSlot('/85930771/MapeiaMap728x90Bottom', [728, 90], 'gpt-ad-4').addService(googletag.pubads());
            gptSlot5 = googletag.defineSlot('/85930771/MapeiaResults300x80', [300, 80], 'gpt-ad-5').addService(googletag.pubads());
            googletag.pubads().disableInitialLoad();
            googletag.enableServices();
        });
    </script>
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r;
            i[r] = i[r] || function () {
                        (i[r].q = i[r].q || []).push(arguments)
                    }, i[r].l = 1 * new Date();
            a = s.createElement(o),
                    m = s.getElementsByTagName(o)[0];
            a.async = 1;
            a.src = g;
            m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
