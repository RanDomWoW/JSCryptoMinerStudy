function getColumn ( name,pages) {
   // 点击更多以后pagesplus标记点击的链接
   pagesplus=pages-1;
   $("#more_a"+pagesplus).text("正在加载，请稍后");
    $.ajax({
        url: '/index/ajax_get_column/',
        type: 'POST',
        data: {name:name,pages:pages},
        success: function(json){
            json = eval('('+ json +')');
            var html='';
            if ( json.msg == 100 ) {
                for ( var i=0; i<json.data.length; i++ ) {
                    html += '<DIV class="author-box" >';
                    html += '<DIV class="pic"><A href="http://www.ccw.com.cn/space/index/'+json.data[i].userid+'"><IMG title="'+json.data[i].username+'" alt="'+json.data[i].username+'" src="http://www.ccw.com.cn/avatar/big/'+json.data[i].userid+'" width="70" height="70"></A></DIV>';
                    html += '<DIV class="cont">';
                    html += '<H3><A href="http://www.ccw.com.cn/article/view/'+json.data[i].id+'" target="_blank">'+json.data[i].title+'</A></H3>';
                    
                        html += '<DIV class="entry-meta">';
                        html += '    <SPAN class="entry-author"><A href="http://www.ccw.com.cn/space/index/'+json.data[i].userid+'" target="_blank">'+json.data[i].username+'</A></SPAN>';
						html += '    <SPAN class="entry-date">'+json.data[i].dateline+'</SPAN>';
                        html += '    <SPAN class="entry-comments"><A href="http://www.ccw.com.cn/article/view/'+json.data[i].id+'">'+json.data[i].comments+'条评论</A></SPAN>';
                        html += '<SPAN class="entry-terms"><I></I>';
                        for ( var j=0; j<json.data[i].tags.length; j++ )
                        html += '    <A href="http://www.ccw.com.cn/tag/name/'+json.data[i].tags[j]['name']+'" target="_blank">'+json.data[i].tags[j]['name']+'</A>';
                        html += '</SPAN>';
                        html += '</DIV>';
						html += '<P>'+json.data[i].content+'</P>';
						html +='<div class="entry-options">';
                        html += '<a class="btn btn-icon" href="http://www.ccw.com.cn/space/index/'+json.data[i].userid+'" target="_blank">查看原文<span><i>&raquo;</i></span></a>';
                     html +='</div>';
                    html += '</DIV>';
                html += '</DIV>';
                }
                if(json.pages>0){
                    html+='<div class="more-article" id="more_div'+pages+'">';
                    html+='<a href="javascript:;" onclick="getColumn(\''+name+'\','+json.pages+');" id="more_a'+pages+'">更多</a>';
                    html+='</div>';
                }
                
                //alert(json.data.length);
                if(pages==0){
                    //$("#more_div"+pages).remove();
                    $('#leftContent').show();
                    $('#leftContent').html(html);
                    $(".loading").hide();
                }else{
                    $("#more_div"+pagesplus).remove();
                    $('#leftContent').show();
                    $('#leftContent').append(html);
                    $(".loading").hide();
                }
                
            }else if(json.msg == 102){
                alert("此标签暂无内容");
                $('#leftContent').show();
                $(".loading").hide();
            }else{
                alert("暂无此标签");
                $('#leftContent').show();
                $(".loading").hide();
            }
        }
    });
    }

