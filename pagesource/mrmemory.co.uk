<script type="text/javascript">
	// common scripts
	$(function(){

		// set up any dialog boxes that have not already been initialised
		$("a[class^='dialogLink_']").each(function() {
			var ev = $._data(this, 'events');
			if ($(this).attr("onclick") == undefined && !(ev && ev.click))
			{
				$(this).click(function(e) {
					e.preventDefault();
					var c = $(this).attr('class').replace(/(\S+)(.*)/, "$1"); // pull out first class name only
					var n = c.replace(/([^_]+)(_*)(.*)/, "$3");
					showDialog({name: n});
				});
			}
		});

		// animated scrolling - stop if the user scrolls. Defaults on .stop() should be fine
		$("html,body").bind("scroll mousedown DOMMouseScroll mousewheel keyup", function(e){
			if ( e.which > 0 || e.type === "mousedown" || e.type === "mousewheel"){
				$("html,body").stop();
			}
		});

		// default tooltip
		$(".tooltip_default").tooltip({
			tooltipClass: "tooltip_basic",
			position: {
				my: "center top+10",
				at: "center bottom"
			},
			content: function() {
				return tooltipClose($( this ).attr('title'));
			}
		});

		
		$(".switchTemplate").click(function(e) {
			e.preventDefault();
			var loader = new dialogLoader();
			$.ajax({
				url: "ajax.php",
				type: "post",
				data: {ajax:'switchTemplate', path:'mobile'},
				success: function(data){
					location.reload(true);
				},
				error: function() {
					loader.remove();
				}
			});
		});

		$(".dismissTemplate").click(function(e) {
			e.preventDefault();
			$.ajax({
				url: "ajax.php",
				type: "post",
				data: {ajax:'dismissTemplate'},
				success: function(data){
					$('#headerBanner').slideUp(500);
				}
			});
		});

		// back to top link
		var amountScrolled = 600;
		$(window).scroll(function() {
			if ( $(window).scrollTop() > amountScrolled ) {
				$('a.backToTop').fadeIn('slow');
			} else {
				$('a.backToTop').fadeOut('slow');
			}
		});

		$("a.backToTop").off('click').on('click', function(e) {
			e.preventDefault();
			$("html,body").animate({
				scrollTop: $("html").offset().top
			}, 600);
		});

	});
</script>

