				//今日新品轮播
				function banner(){
					var ban = $(".banner-wrap");
					var li = ban.find("li");
					var len = li.length;
					var index = 0;
					var count = 5;
					var staytime = 4000;
					function moveright(num,obj){
						obj.eq(num).addClass("active");
						var last = num - count;
						obj.eq(last++).removeClass("active");		
					}
					function right(){
						for (var i = 0; i < count; i++) {
							if( index >= len ){
								index = 0;
							}			
							moveright(index,li);
							index++;
						}
					}
					function moveleft(num,obj,j){
						obj.eq(num).addClass("active");
						var last = num + count;
						if(last >= len){
							last = last - num - j - 1;
						}
						obj.eq(last--).removeClass("active");
					}
					function left(){
						for (var i = 0; i < count; i++) {
							if(index <= 0 ){
								index = len;
							}
							--index;
							moveleft(index,li,i)
						}
					}
					var timer = setInterval(function(){	
						right();
					},staytime);
					
					ban.hover(function(){
						clearInterval(timer);
					},function(){
						timer = setInterval(function(){
							right();
						},staytime);
					});
					
					ban.find(".next").on("click",function(){						
						right();						
					});
					ban.find(".prev").on("click",function(){
						left();
					});
				}
