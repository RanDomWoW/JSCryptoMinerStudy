	function init() {
   		AddShoppersWidget.API.Event.bind("sign_in", createAccount);		
	};
	function createAccount(params) {	
		if (params.source == "social_login") {	
			var data = AddShoppersWidget.API.User.signed_data();
			var has_email = false;
			var custom_email = "";
			if ("google_picture" in data) {
				data.google_picture = data.google_picture.replace('http', 'h--p');
			}
			for (var key in data) {
			  if (data.hasOwnProperty(key) && key.indexOf('email') > -1) {
				has_email = true;
				break;
			  }
			}
			if (!has_email) {
				var custom_email = prompt("Please enter your email", "");
			};

			postLoginData(JSON.stringify(data),custom_email);
		}
	}
	if (window.addEventListener) {			
	   window.addEventListener("load", init, false); 
	} else {
	   document.onreadystatechange = function() { 
	    if(document.readyState in {loaded: 1, complete: 1}) {
		document.onreadystatechange = null; 
	       init();			
	    } 
	  }					
	} 
	function postLoginData(data,custom_email) {
  		// Send the data using post
  		var posting = jQuery.post( 'https://canna-pet.com/', { action: 'addshoppers_social_login', as_signature: data, custom_email: custom_email } );
  		// Put the results in a div
  		posting.done(function(data) {
    		// refresh page
    		location.reload();
  		});
	}
	</script>
