        $("#OkRegionButton").click(function () {
            var countReg = 0;
            var titleToggle = "";
            var valInpute = "";
            $(".checkboxRegion:checkbox:checked").each(function (indx, element) {
                
                countReg++;

                if (indx == 0) {
