                else {
                    valInpute = valInpute + "," + $(element).val();
                    titleToggle = titleToggle + "," + $(element).parent().text();
                }
            });

            $(".RegInpute").val(valInpute);

            $(".RegP").text("Район [" + countReg + "]");
            $('.RegP').tooltip('destroy');
            $(".RegP").tooltip({
                trigger: "hover",
                placement: "left",
                title: titleToggle
            });
            $('#myModal').modal('hide');
        });

    });
</script>

