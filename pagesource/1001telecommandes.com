<script>
    $(document).ready(function() {

        //get brand ajax
        var category = '70';
        var brandSelect = $(".brand-select").chosen({width: "100%"});;
        var colorSelect = $(".color-select");
        var buttonSelect = $(".button-select");
        initialize();
        
        
         var products = new Bloodhound({
                datumTokenizer: function(datum){
                    cleaned = Bloodhound.tokenizers.nonword(datum.cleaned);
                    brand = Bloodhound.tokenizers.nonword(datum.brand);
                    nameWhitespace = Bloodhound.tokenizers.whitespace(datum.name);
                    nameNonWord = Bloodhound.tokenizers.whitespace(datum.name);
                    named = datum.name ? datum.name.split(/\W+/) : [];
                    concat = brand.concat(datum.brand).concat(cleaned).concat(datum.cleaned).concat(nameWhitespace).concat(nameNonWord).concat(datum.name).concat(named);
//                    console.log(concat);
                    return concat;
                },
                queryTokenizer: function(q){
                    brut = q;
                    qWithespace = Bloodhound.tokenizers.whitespace(q);
                    qnonWord = Bloodhound.tokenizers.nonword(q);
                    qnon = q ? q.split(/\W+/) : [];
                    concat = qnon.concat(qWithespace).concat(qnon).concat(brut).concat(qnonWord);
//                    console.log(concat);
                    return concat;
                },
                limit: 6,
                prefetch: {
                    cache:false,
                    url: '/search/ajax-products-list/'+category,
                    filter: function(list) {
                        return $.map(list, function(product) {
                            return product;
                        });
                    },
                },
                remote: '/search/ajax-products-list/'+category+'?q=%QUERY'
            });
            
            
            products.clearPrefetchCache();
            products.initialize();

            $('.typeahead').typeahead({
                    highlight: true
                }, 
                {
                    name: 'products',
                    displayKey: 'name',
                    source: products.ttAdapter(),
                    templates: {
                        empty: [
                            '<div class="empty-message">',
                            "Cette référence est introuvable!",
                            '</div>'
                        ].join('\n'),
                        suggestion: Handlebars.compile('<p>{{brand}} {{name}}</p>')
                }
            });


         //control zftable
        $('.typeahead').bind('typeahead:selected', function(ev, suggestion) {
            $('#model-form').submit();
        });

        $('#model-form').on('submit', function(e) {
            e.preventDefault();
            resetSearchEngine();
            $('input[name=zff_product_name]').val($('#model').val());
            var e = jQuery.Event("keypress");
            e.which = 13; // # Some key code value
            $('input[name=zff_product_name]').trigger(e);
        });
        

        function initialize() {

            getData("/search/ajax-brands-list/"+category, brandSelect,false);
            getData("/search/ajax-colors-list/"+category, colorSelect, true);
            
            brandSelect.change(function() {
                resetProductName();
                $('input[name=zff_brand_id]').val($(this).val());
                var e = jQuery.Event("keypress");
                e.which = 13; // # Some key code value
                $('input[name=zff_brand_id]').trigger(e);
            });

            colorSelect.change(function() {
                resetProductName();
                $('input[name=zff_buttons_color]').val($(this).val());
                var e = jQuery.Event("keypress");
                e.which = 13; // # Some key code value
                $('input[name=zff_buttons_color]').trigger(e);
            });
            
            buttonSelect.change(function() {
                resetProductName();
                $('input[name=zff_nb_buttons]').val($(this).val());
                var e = jQuery.Event("keypress");
                e.which = 13; // # Some key code value
                $('input[name=zff_nb_buttons]').trigger(e);
            });
        }
        ;
        
//        //input model search
//        $('.btn-model').on('click', function() {
//            $('input[name=zff_product_name]').val($('#model').val());
//            var e = jQuery.Event("keypress");
//            e.which = 13; // # Some key code value
//            $('input[name=zff_product_name]').trigger(e);
//        });
        

        function getData(url, select, lower) {
            $.get(url, function(data) {
                var options = $.parseJSON(data);
                select.empty();
                select.append(new Option('Toutes', ''));
                $.each(options, function(i, item) {
                    if (lower == true) {
                        select.append(new Option(item.name.toLowerCase(), item.id));
                    } else {
                        select.append(new Option(item.name, item.id));
                    }
                });
                select.trigger("chosen:updated");
            });
        }
        
        
        function resetSearchEngine() {
            console.log('test');
            $('input[name=zff_buttons_color]').val("");
            $('input[name=zff_nb_buttons]').val("");
            $('input[name=zff_brand_id]').val("");
            $(".radios-to-slider").trigger('force-change', ["option0"]);
            $(".search-select").val($(".search-select option:first").val());
            brandSelect.trigger('chosen:updated');
        }

        function resetProductName() {
            $('input[name=zff_product_name]').val("");
            $('#model').val("");
        }
        
        


    });

    </script>

