	<img src="//my-chrome.ru/logo.png">
</canvas>
<script language="JavaScript" type="text/javascript">
	(function() {
		var sait = "//my-chrome.ru";
		var angle = 0, maxspeed = -16, speed = maxspeed, accel = maxspeed * maxspeed / (Math.PI * 2 * 3 * 2), lastTime, isSleep = false;
		var elem = document.getElementById('myCanvas');
		var elemWidth = elem.width;
		var elemHeight = elem.height;
		if (elem && elem.getContext) {
			var context = elem.getContext('2d');
			if (context) {
				var bg = new Image(), chromeimg = new Image(), imagesLeft = 2;
				chromeimg.onload = bg.onload = function() {
					if (--imagesLeft == 0) {
						elem.onmousedown = spin;
						lastTime = +new Date();
						update();
					}
				};
				bg.src = sait + "/logoimg/logo.png";
				chromeimg.src = sait + "/logoimg/chromium.png";
			}
		}
		
		function update() {
		
			var curTime = +new Date();
			var dt = (curTime - lastTime) / 1000;
			lastTime = curTime;
		
			angle += speed * dt + accel * dt * dt / 2;
			speed += accel * dt;
			
			if (speed > 0)
				speed = 0;
			if (speed < maxspeed)
				speed = maxspeed;
			
			context.clearRect(0, 0, elemWidth, elemHeight);
			context.drawImage(bg, 0, 0);
			context.save();
			context.translate(113, 25);
			context.rotate(angle);
			context.drawImage(chromeimg, -25, -25, 50, 50);
			context.restore();
			
			if (speed != 0)
				setTimeout(update, 20);
			else
				isSleep = true;
		}
		
		function spin(event) {
			var a = elem.getBoundingClientRect();
			var b = (event.clientX - a.left) / jQuery("#myCanvas").width() * 300;
			var c = (event.clientY - a.top) / jQuery("#myCanvas").height() * 57;
			var d = b - 113;
			var e = c - 25;
			if (d * d + e * e < 25 * 25) {
				speed += maxspeed;
				if (isSleep) {
					lastTime = +new Date();
					update();
				}
			}
		}
	})();
</script>

