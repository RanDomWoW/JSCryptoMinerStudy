function share(type){
    var item = jQuery('#carousel-generic').find('.item.active').attr('data-data');
        item = eval('(' + item + ')');
    var url = encodeURIComponent('http://video.sinovision.net?id=' + item.id),
        title = encodeURIComponent(item.title),
        img = item.image,
        u = '';
    switch(type){
        case 1:
            u = 'https://www.facebook.com/share.php?u='+url+'&t='+title+'&pic='+img;
            break;
        case 2:
            u = 'http://twitter.com/share?url='+url+'&text='+title+'&via=sinovision';
            break;
        case 3:
            u = 'http://service.weibo.com/share/share.php?url='+url+'&title='+title+'&searchPic=true';
            break;
        case 4:
            sharewx();
            break;
        case 5:
            var pr = prompt('复制地址',item.title + 'http://video.sinovision.net?id=' + item.id);
            if(pr != null && pr != ''){
                return false;
            }else{
                return false;
            }
            break;
    }
    if(type !== 4 && type !== 5){
        window.open(u);
    }
};
