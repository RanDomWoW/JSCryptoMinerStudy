function liveTai(){
    jQuery.getJSON('http://public.sinovision.net/livestream.php?func=?', {username:''}, function(ret){
        var str = '<ul>',
            str1 = '',
            data = '',
            istype = '';
        if(typeof(ret.trailer) != "undefined" && ret.trailer.length > 0){
            data = ret.trailer;
            istype = 't';
        }
        if(typeof(ret.live) != "undefined" && ret.live.length > 0){
            data = ret.live;
            istype = 'l';
        }
        var dcount = data.length;
        if(dcount>0){
            var array = [];
            var j = 0;
            for(var i = 0;i < dcount; i++){    
                //判断是否有无banner           
                if(data[i].banner_image && data[i].banner_image!='Array'){
                    if(data[i].url){
                        if(istype == 'l'){
                            str1 = '<a href="' + data[i].url + '" target="_blank"><img src="http://video.sinovision.net/attachments/'+data[i].banner_image+'" style="width:320px;height:50px;"></a>';
                        }else{
                            str1 = '<a href="' + data[i].url + '" target="_blank"><img src="http://video.sinovision.net/attachments/'+data[i].banner_image+'" style="width:320px;height:50px;"></a>';
                        }
                    }else{
                        if(istype == 'l'){
                            str1 = '<a href="javascript:void(0);"><img src="http://video.sinovision.net/attachments/'+data[i].banner_image+'" style="width:320px;height:50px;"></a>';
                        }else{
                            str1 = '<a href="javascript:void(0);"><img src="http://video.sinovision.net/attachments/'+data[i].banner_image+'" style="width:320px;height:50px;"></a>';
                        }
                        // str1 = '<a href="javascript:void(0);">';
                    }
                    //如果有banner，直播优先
                    
                    array[j] = str1;
                    j++;
            
                } else {
                    //如果没有banner
                    if(data[i].url){
                        str += '<li class="rolltitle"><a href="' + data[i].url + '" target="_blank">';
                    }else{
                        str += '<li class="rolltitle"><a href="javascript:void(0);">';
                    }
                    //直播优先
                    if(istype == 'l'){
                        str += '<span style="color:#e50012;">正在直播&nbsp;&nbsp;|&nbsp;&nbsp;</span>';
                    }else{
                        str += '<span style="color:#e50012;">直播预告&nbsp;&nbsp;|&nbsp;&nbsp;</span>';
                    }
                    str += data[i].title + '&nbsp;&nbsp;&nbsp;&nbsp;' + data[i].sdate + '</a></li>';
                }
                
            }
            str += '</ul>';
            // console.log(array)
            // str1 = array.json('-');
            // console.log(str1)
            var img = '<img';
            //判断是否有banner,有banner显示banner,将文字和banner分开
            if(str1.indexOf(img) > -1) {
                var array1='';
                for(var k=0;k<array.length;k++){
                    if(k==0){
                        array1 += '<div class="item active">'+array[k]+'</div>';
                    } else {
                        array1 += '<div class="item">'+array[k]+'</div>';
                    }

                }
                jQuery('.lbimg').html(array1);
                jQuery(".live-tai").css('display','none');
                jQuery(".live-tai-banner").css('display','block');
                jQuery(".live-tai-banner").css('margin-top','5px');
                jQuery(".live-tai-banner").css('height','50px');
            } else {
                //没banner显示文字
                jQuery(".live-tai-banner").css('display','none');
                jQuery(".live-tai").css('display','block');
                jQuery(".live-tai").html(str);
                jQuery(".live-tai").css('height','30px');
            }            
            if(dcount>1){
                setInterval('textScroll(".live-tai",500)',5000);
                // setInterval('textScroll_banner(".live-tai-banner",500)',5000);
            }
        }else{
            jQuery(".live-tai").css('height','auto');
            jQuery(".live-tai-banner").css('height','auto');
        }
    });
}
