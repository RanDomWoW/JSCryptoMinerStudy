<script type="text/javascript">
jQuery(function(){
    jQuery(window).scroll(function(){
        //返回顶部
        if(jQuery(window).scrollTop() > 30){
            jQuery('.starboard>a').css('display','block');
        }else{
            jQuery('.starboard>a').css('display','none');
        }
    });
    //返回顶部
    jQuery('.starboard>a').click(function(){
        jQuery(window).scrollTop();
        jQuery('body,html').animate({scrollTop:0},500);
    });
    jQuery('.starboard>.closes>span').on('click',function(){
        jQuery('.starboard').css('display','none');
    });
    //文字左右滚动
    jQuery(document).on('mouseover','.rolltitle', function(){
        var el = this;
        var itme_to_left = setInterval(function(){
            el.scrollLeft += 3;
        },30);
        jQuery(el).bind('mouseout', function(){
            if(itme_to_left){
                clearInterval(itme_to_left);
                itme_to_left = null;
            }
            el.scrollLeft = 0;
            jQuery(el).unbind('mouseout');
        });
    });
});
</script>
