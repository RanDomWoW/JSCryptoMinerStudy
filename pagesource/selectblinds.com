<script type="text/javascript">
    var slideshowSpeedNumber = 6000;
    var shoppingFavoritesShown = false;

    $('document').ready(function () {
        $('#mainSlider .flexslider').flexslider({
            selector: ".slides > .slide",
                    slideshow: isSlideShow(),
            slideshowSpeed: slideshowSpeedNumber,
            animationSpeed: 0,
            fadeFirstSlide: false,
            customDirectionNav: "#sliderArrows a",
            controlsContainer: "#sliderControl .container",
            start: function (slider) {
                $('#mainSlider .slide').each(function () {
                    var curIndex = $('#mainSlider .slide').index($(this));
                    $('#mainSlider .flex-control-nav li:eq(' + curIndex + ') a').html($(this).children('.slideThumb').html());
                });
            }
        });

        $('#discoverSlider').flexslider({
            selector: ".slides > .slide",
            slideshow: false,
            animation: "slide",
            animationLoop: false,
            itemWidth: 300,
            minItems: 1,
            maxItems: 3,
            itemMargin: 0,
            move: (winWidth > 769 ? 3 : winWidth < 440 ? 1 : 2)
        });

        $('#favoritesSlider').flexslider({
            selector: ".slides > .slide",
            slideshow: false,
            animation: "slide",
            animationLoop: false,
            itemWidth: 300,
            minItems: 1,
            maxItems: 4,
            itemMargin: 0,
            move: (winWidth > 769 ? 3 : winWidth < 440 ? 1 : 2),
            start: function (slider) {
                initializeMultipleRatingStars($('.productRatingStars'));
            }
        });

        $('body').on('click', '#mainSlider .slide a', function(e){
            if (window['google_tag_manager']){
                e.preventDefault();
                var curLink = $(this);

                dataLayer.push({
                    'event': 'mainBannerClick',
                    'eventLabel': 'Banner' + (parseInt($('#mainSlider .slide').index(curLink.parents('.slide')) + 1)),
                    'eventCallback': function () {
                        window.location = curLink.attr('href');
                    },
                    'eventTimeout': 1000
                });
            }
        });

        $('body').on('click', '.browseCatBtn', function () {
            $('.discoverBrowse').toggleClass('open');
        });

        $('body').on('click', '.flexslider .slide', function () {
            var slide = $(this);
            if (window['google_tag_manager'] && slide.find('.slideLink').length > 0 && slide.parents('#favoritesSlider').length > 0) {
                // pushes click event to Google analytics
                dataLayer.push({
                    'event': 'productClick',
                    'ecommerce': {
                        'click': {
                            'actionField': { 'list': 'Shopping Favorites' },
                            'products': [{
                                'name': slide.find('.slideLink').html().replace("½", "one-half").replace("\"", "").toLowerCase(),
                                'id': slide.attr('data-prodid'),
                                'price': slide.find('.favoritesPrice').children('.blue').html().replace("$", ""),
                                'brand': slide.data('prodbrand'),
                                'category': slide.data('categoryname'),
                                'position': $('#favoritesSlider .slide').index(slide) + 1
                            }]
                        }
                    },
                    'eventCallback': function () {
                        window.location = slide.find('.slideLink').attr('href');
                    },
                    'eventTimeout': 1000
                });
            }
            else if (window['google_tag_manager'] && slide.find('.slideLink').length > 0 && slide.parents('#discoverSlider').length > 0) {
                dataLayer.push({
                    'event': 'discoverClick',
                    'eventLabel': slide.find('.slideLink').text().toLowerCase(),
                    'eventCallback': function () {
                        window.location = slide.find('.slideLink').attr('href');
                    },
                    'eventTimeout': 1000
                });
            }
            else if (!window['google_tag_manager'] || slide.find('.slideLink').length > 0) {
                window.location = slide.find('.slideLink').attr('href');
            }
        });

        $('body').on('click', '#sbAdvantagePanel .col, .styleCols .col', function () {
            if ($(this).find('.advantageLink').length > 0) {
                window.location = $(this).find('.advantageLink').attr('href');
            }

            if ($(this).find('.lgBtn').length > 0) {
                window.location = $(this).find('.lgBtn').attr('href');
            }
        });

        $('body').on('click', '.categoryLinks a', function (e) {
            if (window['google_tag_manager']) {
                e.preventDefault();
                var curLink = $(this);
                var curLabel = curLink.text().toLowerCase();

                dataLayer.push({
                    'event': 'catLinkClick',
                    'eventLabel': curLabel,
                    'eventCallback': function () {
                        window.location = curLink.attr('href');
                    },
                    'eventTimeout': 1000
                });
            }
        });

        $('body').on('click', '.exploreCatTile', function (e) {
            e.preventDefault();
            var curLink = $(this).find('.exploreCatTitle');
            var curLabel = curLink.text().toLowerCase();
            if (window['google_tag_manager']) {
                dataLayer.push({
                    'event': 'exploreCatClick',
                    'eventLabel': curLabel,
                    'eventCallback': function () {
                        window.location = curLink.attr('href');
                    },
                    'eventTimeout': 1000
                });
            }
            else {
                window.location = curLink.attr('href');
            }
        });
    
        $('body').on('click', '.exploreCatModal .orangeBtn', function (e) {
            if (window['google_tag_manager']) {
                e.preventDefault();
                var curLink = $(this);
                var curLabel = $(this).parents('.exploreCatModal').find('.sbModalTitleText').text().toLowerCase();

                dataLayer.push({
                    'event': 'exploreCatBtnClick',
                    'eventLabel': curLabel,
                    'eventCallback': function () {
                        window.location = curLink.attr('href');
                    },
                    'eventTimeout': 1000
                });
            }
        });

        $('body').on('click', '#exploreVideos .exploreVidTile', function () {
            var curLink = $(this).find('.exploreVidTitle');
            dataLayer.push({
                'event': 'exploreVideoClick',
                'eventLabel': curLink.text()
            });
        });

        $('.no-touchevents body').on('mouseenter', '.exploreCatTooltipBtn', function () {
            $(this).parents('.exploreCatImageWrap').find('.exploreCatImage').css({ 'border-color': '#fff' });
            $(this).parents('.exploreCatTile').find('.exploreCatTitle').css({ 'text-decoration': 'none' });

            var tooltipID = $(this).attr('id').split('_');
            if (winWidth > 769) {
                $('#customTT_' + tooltipID[1]).qtip({
                    content: {
                        text: $('#customTTContent_' + tooltipID[1]).clone()
                    },
                    position: {
                        my: 'left top',
                        at: 'right center',
                        viewport: $(window),
                        target: $('#customTT_' + tooltipID[1]),
                        adjust: {
                            y: -10
                        }
                    },
                    style: {
                        classes: 'exploreCatTooltip',
                        tip: {
                            mimic: 'center',
                            width: 15,
                            height: 10,
                            border: 1,
                            offset: 15
                        }
                    },
                    show: { ready: true },
                    events: {
                        hide: function () {
                            $(this).qtip('destroy');
                        }
                    }
                });
            }
        }).on('mouseleave', '.exploreCatTooltipBtn', function(){
            $(this).parents('.exploreCatImageWrap').find('.exploreCatImage').removeAttr('style');
            $(this).parents('.exploreCatTile').find('.exploreCatTitle').removeAttr('style');
        });

        $('body').on('click', '.exploreCatTooltipBtn', function () {
            var tooltipTitle = $(this).parents('.exploreCatTile').find('.exploreCatTitle').html();
            var tooltipID = $(this).attr('id').split('_');
            $('#dialogtemplate').html($('#customTTContent_' + tooltipID[1]).clone().html());
            $("#dialogtemplate").sbModal({
                title: tooltipTitle,
                width: 320,
                modal: true,
                modalClass: "exploreCatModal",
                close: function(){
                    $(this).html('');
                }
            });
            return false;
        });

        $('body').on('change', '.budgetSizes select', function () {
            $(this).parents('form').submit();
        });

        $('body').on('click', '.budgetPriceCircle:not(.disabled)', function(){
            $(this).toggleClass('active');
            checkEnableBudgetTool();
        });

        $('body').on('click', '#budgetToolShopBtn', function(e){
            if($(this).hasClass('disabled')){
                e.preventDefault();
                $('.budgetToolPriceCircles').addClass('incomplete');
            }
        });

        $('body').on('click', '#BudgetTool input[type=submit]', function(e){
            if($(this).hasClass('disabled')){
                e.preventDefault();
            }
        });
    });

    $(window).scroll(function () {
        if ($('#cordlessPanel').length > 0) {
            if (scrollPos > ($('#cordlessPanel').offset().top - 150)) {
                $('.animatedBlind').addClass('animated');
            }
            else {
                $('.animatedBlind').removeClass('animated');
            }
        }

        if($('#favoritesPanel').length > 0 && !shoppingFavoritesShown && ((scrollPos + $(window).height()) > ($('#favoritesPanel').offset().top + 100)) && typeof favProducts != "undefined"){
            shoppingFavoritesShown = true;
            // push shopping favorites products to GA (in wg_ProductSlider view)
            dataLayer.push({
                'event': 'showShoppingFavorites',
                'ecommerce': {
                    'impressions': favProducts
                }
            });
        }
    });

    function isSlideShow() {
        return $('#IsSlideShow').val() == "true" ? true : false;
    }

    function checkEnableBudgetTool(){
        var base = '/shop-by-feature/shop-by-feature.html';
        var width = parseInt($('#SelectedWidth').val()) + (parseInt($('#SelectedWidthEighth').val())/8);
        var height = parseInt($('#SelectedHeight').val()) + (parseInt($('#SelectedHeightEighth').val())/8);
        if($('.budgetPriceCircle.active').length < 1){
            $('#budgetToolShopBtn').addClass('disabled');
        }
        else {
            base += '?';
            $('#budgetToolShopBtn').removeClass('disabled');
            $('.budgetToolPriceCircles').removeClass('incomplete');
            $('.budgetPriceCircle.active').each(function(){
                base = addSeparatorForArgument(base);
                base += $(this).data('budgetqs');
            });
            $('.resetBudgetTool').css({'display':'inline-block'});
        }
        base=addSeparatorForArgument(base)+"width="+width;
        base=addSeparatorForArgument(base)+"height="+height;
        $('#budgetToolShopBtn').attr('href', base);
    }
</script>


