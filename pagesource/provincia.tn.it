	<div id="jquicksearch_shadow" class="png_fix">&nbsp;</div>
	<div id="jquicksearch_shadow_bottom" class="png_fix">&nbsp;</div>
</div>

<div id="jqt_popupwin" class="jq_textip jqt_popupwin png_fix">
	<div class="jqt_pad">
		<div class="jqt_handle">
			<div class="jqt_close jqt_close_button">&nbsp;</div>
			<div class="jqt_title">{{risultati}} <span id="jqt_startnum"></span> - <span id="jqt_endnum"></span> &nbsp;{{di_circa}} <span id="jqt_results"></span>&nbsp; {{per}} <span id="jqt_question" class="jqt_question"></span>
			</div>
		</div>
		<div class="jqt_subtitle">
			<div class="jqt_subtitle_text">
				<div class="jqt_subtitle_text_popupwin">
					{{puoi_provare_anche}}: <span class="jqt_synonyms"></span>
				</div>
			</div>
		</div>
		<div class="jqt_body">
			<div class="jqt_body_pad">
				<div id="jqt_contents" class="jqt_contents"></div>
			</div>
		</div>
		<div class="jqt_footer">
			<div class="jqt_footer_contents">
				<div class="jqt_footer_pad">
					<div class="jqt_footer_order">
						{{ordina_per}} <span class="jqt_order_date">{{ordina_data}}</span>&nbsp;/&nbsp;<span class="jqt_order_relevance jqt_order_selected">{{ordina_rilevanza}}</span>
					</div>
					<div class="jqt_showall">{{visualizza_tutto}}</div>
				</div>
			</div>
		</div>
	</div>
	<div id="jqt_popupwin_shadow" class="png_fix">&nbsp;</div>
	<div id="jqt_popupwin_shadow_bottom" class="png_fix">&nbsp;</div>
</div>
<div id="jqt_tipcall" class="jqt_tipcall"></div>

<!--
stringa di controllo per nagios non togliere--><!--googleon: index-->
</body>
</html>