<script type="text/javascript">
    var mccCode = 'mid=V012965';
    var mccHostnames = ['shireregistration.com$','thecompassproject.net$','udotherest.com$','theydotherest.com$','bed.vyvanse.com$'];

    $('a').each(function () {
        var href = $(this).attr('href').trim();

        if (href.match(/((http:\/\/)|(https:\/\/))/i)) {
            var a = document.createElement("a");
            a.href = href;
            var hostname = a.hostname;

            var appendMcc = false;

            if (mccHostnames != null) {
                $.each(mccHostnames, function (index, name) {
                    var re = new RegExp(name, 'i');
                    if (re.test(hostname))
                        appendMcc = true;
                });
            }

            if (appendMcc) {
                href += (href.match(/\?/) ? '&' : '?') +  mccCode;
                $(this).attr('href', href);
            }
        }
    });
</script>



