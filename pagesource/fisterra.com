	 $("input#txtBuscador").autocomplete({
	 
    source: function( request, response ) {
		 sCategoria = "";
		 bEscrito1=0;
		 bEscrito2=0;
		 bEscrito3=0;
		 bEscrito4=0;
		 bEscrito5=0;
		 bEscrito6=0;
		var str = request.term;
		//alert(str);
			$.ajax({
				url: "https://www.fisterra.com/AjaxBuscar.asp",
				dataType: "json",
			    cache: false,
				data: {texto: str.replaceLatinChar(),idTipoFicha: 0},
				success: function( data ) {
					response( $.map( data, function( item ) {
						return {
							id: item.idFicha,
							value: item.Nombre,
							idTipoFicha: item.idTipoFicha,
							urlseo: item.urlseo
						}
					}));
				}
			});
		},
		minLength: 3,
		select: function (event, ui) {
			var str = ui.item.value;
			if (ui.item.idTipoFicha <= 4){
				//son fichas
				if (ui.item.idTipoFicha==1){
					sURLFicha="guias-clinicas/"+ui.item.urlseo+"/"
				}
				if (ui.item.idTipoFicha==2){
					sURLFicha="ayuda-en-consulta/tecnicas-atencion-primaria/"+ui.item.urlseo+"/"
				}
				if (ui.item.idTipoFicha==3){
					sURLFicha="ayuda-en-consulta/informacion-para-pacientes/"+ui.item.urlseo+"/"
				}
				if (ui.item.idTipoFicha==4){
					sURLFicha="herramientas/medicamentos/"+ui.item.urlseo+"/"
				}
				document.location = "https://www.fisterra.com/"+ sURLFicha
			}
			else{
				if (ui.item.idTipoFicha==6)
				{
					sURLFicha="ayuda-en-consulta/calculos/"+ui.item.urlseo+"/"
					document.location = "https://www.fisterra.com/"+ sURLFicha
				}
				else
				{
				//palabras clave
				//document.location = "https://www.fisterra.com/buscador/?txtBuscador="+ str.replaceLatinChar() +""
				}
			}
		}
		
	});
  $.ui.autocomplete.prototype._renderItem = function( ul, item ) {		  
		  var re = new RegExp("(" + this.term + ")","i") ;
		  var nom_contacto_origen= this.term;
		  var nom_contacto= item.value;
		  nom_contacto = nom_contacto.replace(re,"<span style='font-weight:bold;'>" + nom_contacto_origen + "</span>");
		
		  var idTipoFicha= item.idTipoFicha;
			if (idTipoFicha==1 && bEscrito1==0){
				sCategoria="Gu&iacute;as cl&iacute;nicas";
				bEscrito1=1;
			}
			else{
				if (idTipoFicha==2 && bEscrito2==0){
					sCategoria="T&eacute;cnicas en atenci&oacute;n primaria";
					bEscrito2=1;
				}
				else{			
					if (idTipoFicha==3 && bEscrito3==0){
						sCategoria="Informaci&oacute;n para pacientes";
						bEscrito3=1;
					}
					else{
						if (idTipoFicha==4 && bEscrito4==0){
							sCategoria="Medicamentos";
							bEscrito4=1;
						}
						else{
							if (idTipoFicha==6 && bEscrito5==0){
								sCategoria="C&aacute;lculos";
								bEscrito5=1;
							}
							else{
								/*if ( (idTipoFicha!=1 && idTipoFicha!=2 && idTipoFicha!=3 && idTipoFicha!=4 && idTipoFicha!=6)  && bEscrito6==0 )
								{
									sCategoria="Palabras Clave";
									bEscrito6=1;
								}
								else sCategoria="";
								*/
								sCategoria="";
							}
						}
					}
				}
				
			}
			if (sCategoria == ""){
				sCategoria="";
			}
			else{
				sCategoria="<span class='ui-autocomplete-category'>"+sCategoria+"</span>";
			}
		return $( "<li></li>" )
			.data( "item.autocomplete", item )
			//.append( sCategoria+"<a href='http://www.fisterra.com/fichas/interiorNew.asp?idFicha="+ item.idFicha +"&idCategoria="+ item.idCategoria +"'>" + nom_contacto +"</a>" )
			.append( sCategoria+"<a href='#'>" + nom_contacto +"</a>" )
			.appendTo( ul );
	};
	
