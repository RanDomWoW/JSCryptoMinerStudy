	function seccionOver(seccionActiva){

		//posicionamos submenu
		var elemento = $("#"+seccionActiva.id);
		var posicion = elemento.position();
		$(".nav2").css('left', posicion.left+22+'px');
		//alert( "left: " + posicion.left + ", top: " + posicion.top );
		var str = elemento.html();
		
		var n = str.indexOf("nav1_4_Img");
		if(n>0){
			$(".nav2").css('left', posicion.left-30+'px');
		}
		bIconos=false;
		var n = str.indexOf("nav1_6_Ico1");
		if(n>0){
			//$(".nav2").css('top', '0px');
			$(".nav2").css('left', '0px');
			bIconos=true;
		}
		var n = str.indexOf("nav1_7_Ico2");
		if(n>0){
			//$(".nav2").css('top', '0px');
			$(".nav2").css('left', '0px');
			bIconos=true;
		}
		if(bIconos==false){
			$("#nav1_6_login").hide();
			$("#collapseSearch").hide();
		}
		//alert( "left: " + posicion.left + ", top: " + posicion.top );
		//cambiamos icono menu
		var flecha= $("#"+seccionActiva.id+"_Img");
		$(flecha).attr("src","https://www.fisterra.com/graficos/flechaMenuNaranja.png");
		
		//cambiamos icono menu
		var icono1= $("#"+seccionActiva.id+"_Ico1");
		$(icono1).attr("src","https://www.fisterra.com/graficos/usuarioMenuNaranja.png");
		

		var icono2= $("#"+seccionActiva.id+"_Ico2");
		$(icono2).attr("src","https://www.fisterra.com/graficos/lupaMenuNaranja.png");
		

		//SI NO ES la que esta en el mouseover ocultamos todas las demas
		$(".nav2 ul:not('#"+seccionActiva.id+"_sub')").hide();
		$("#navTexto img:not('#"+seccionActiva.id+"_Img')").attr("src","https://www.fisterra.com/graficos/flechaMenuGris.png");
		$("#nav1_6_a img:not('#"+seccionActiva.id+"_Ico1')").attr("src","https://www.fisterra.com/graficos/usuarioMenuGris.png");
		$("#nav1_7_a img:not('#"+seccionActiva.id+"_Ico2')").attr("src","https://www.fisterra.com/graficos/lupaMenuGris.png");
		//si no es el activo
		if(!$(seccionActiva).hasClass('activo')){
			$(".nav1 ul li").removeClass("activo");

		}
		
		

		//si los items del menu estan ocultos cargamos para cada mouseover, si ya estan cargados no hacemos nada
		if($("#"+seccionActiva.id+"_sub").css('display')=='none'){

			$(seccionActiva).addClass("activo");
			$("#"+seccionActiva.id+"_sub a").hide();
			$("#"+seccionActiva.id+"_sub").show();
			$("#"+seccionActiva.id+"_sub a").fadeIn(); //problematico
		}
	}


