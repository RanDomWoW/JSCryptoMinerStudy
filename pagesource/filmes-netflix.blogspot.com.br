/* --- What About Bob? - Ric - --- */
#BobMovie {display:none;position:absolute;z-index:210;width:360px;text-align:left;font:13px/1.231 arial,helvetica,clean,sans-serif;*font-size:small;*font:x-small}
#BobMovie dt,dl {margin:0;padding:0;width:auto;}
#BobMovie dt {float:left;margin-right:0.5em;display:block;text-align:right;font-weight:bold;}
#BobMovie .clearfix:after {content:".";display:block;font-size:0;line-height:0;height:0;clear:both;visibility:hidden;}
#BobMovie .title {background-color:#B9090B;color:#FFF;padding:6px 0;}
#BobMovie-content {background:#fff;border:1px solid #B8B5B5;-webkit-box-shadow:rgba(0, 0, 0, .8) 0 0 15px;-moz-box-shadow:rgba(0, 0, 0, .8) 0 0 15px;box-shadow:rgba(0, 0, 0, .8) 0 0 15px;margin:0 15px;}
#BobMovie a {text-decoration:none;}
#BobMovie a:hover {text-decoration:underline;color:#dc181e;}
#BobMovie-arrow {display:block;position:absolute;top:60px;background:transparent url(//3.bp.blogspot.com/-mQXnSM-1OZE/VyncfjVtDgI/AAAAAAAAGKQ/yBawy7OMbDgBymKgUEuBQz1l7lTa30ftQCLcB/s1600/arrows.png) no-repeat;width:95px;height:204px;}
#BobMovie .ne {clip:rect(136px 95px 204px 47px);right:-32px;}
#BobMovie .nw {clip:rect(136px 48px 204px 0);left:-32px;}
