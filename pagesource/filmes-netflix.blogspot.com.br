    margin-left: -10px;
    -o-transition: all.3s;
    -ms-transition: all .3s;
    -moz-transition: all .3s;
    -webkit-transition: all .3s;
}

.read-more:hover:after {
    opacity: 1;
    padding-left: 5px;
    margin-left: 6px;
}

