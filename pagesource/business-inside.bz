<script type="text/javascript">
$(document).ready(function(){
	//Открытие попап окна fancybox
    $('.popup').fancybox({
        scrolling: 'visible',
		padding: 0, 
		helpers: {overlay: {locked: true}}
    });
	//плавный скролл
	$('.scrollto').click(function(event){
		event.preventDefault();
		var full_url = this.href;
		var parts = full_url.split("#");
		var trgt = parts[1];
		var fix = 50;
		var target_offset = $("#"+trgt).offset();
		var target_top = target_offset.top - fix;
		var scrTop = $(window).scrollTop();
		if(target_top != scrTop){ $('html, body').animate({scrollTop:target_top}, 500);}
	});
	function rotate() {
		var curEl = $('.js-list li.active');
		$(curEl).removeClass('active');
		if($(curEl).hasClass('last')) $('.js-list li:first').addClass('active');
		else $(curEl).next().addClass('active')
	}
	setInterval(rotate, 3000);
});

//Откроем popup при попадании мыши вверх
if($(window).width() > 768){
	document.onmousemove = handleMouseMove;
	setInterval(getMousePosition, 25); 

	function handleMouseMove(event) {
		var dot, eventDoc, doc, body, pageX, pageY;
		event = event || window.event; 
		if (event.pageX == null && event.clientX != null) {
			eventDoc = (event.target && event.target.ownerDocument) || document;
			doc = eventDoc.documentElement;
			body = eventDoc.body;

			event.pageX = event.clientX +
			  (doc && doc.scrollLeft || body && body.scrollLeft || 0) -
			  (doc && doc.clientLeft || body && body.clientLeft || 0);
			event.pageY = event.clientY +
			  (doc && doc.scrollTop  || body && body.scrollTop  || 0) -
			  (doc && doc.clientTop  || body && body.clientTop  || 0 );
		}
		mousePos = {
			x: event.pageX,
			y: event.pageY
		};
	}
	var mouseIsOut = true;
	var pos;
	var tempY;
	$('html').mouseleave(function(){
		if (mouseIsOut && $('#pop_up_cont').css('display') != 'block'){ 
			$.fancybox.open( {href : '#pop_up_cont'} );
		}
	});
	function getMousePosition(){
		pos = mousePos;
		if (pos) 
		{
			var tepScroll = window.scrollY;
			if (pos.y < (tepScroll + 300)) mouseIsOut = true;
			else mouseIsOut = false;
		}
	}
}
</script>
