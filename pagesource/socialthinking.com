                    fillColor: '#D1D3D3'
                }
            }
            ]
        });
    }
    var nSp_image = 'Content/img/conferences/mapMarkerGreen.png';
    var sp_image = 'Content/img/conferences/mapMarkerBlue.png';


    function createMarker(conference) {
        var delta = 0;

        if (existingLocations.length>0 && existingLocations.indexOf(conference.Location.CityId) > -1) {
            delta = 0.5;
        } else {
            existingLocations.push(conference.Location.CityId);
        }

        var image = conference.IsSponsored ? sp_image : nSp_image;
        var url ='/Conferences/Details' + '?name=' + conference.Link;
        var marker=new google.maps.Marker({
            position:new google.maps.LatLng((parseFloat(conference.Location.Latitude) + delta),(parseFloat(conference.Location.Longitude)+delta)),
             label:name,
             icon: image,
             title:conference.Location.City
         });
        marker.setMap(map);
        marker.addListener('click', function() {

            infowindow.setContent(getInfoContent(conference));
            infowindow.addListener('domready', function() {
                 findParent();
            });
            infowindow.setPosition(marker.getPosition());
             infowindow.open(map, marker);
         });
         marker.id = conference.Id;
         myMarkers.push(marker);

         function getInfoContent(conf) {
           if (conf.IsSponsored) {
                 return "<div class='conf-overlay " + ((conf.IsSoldOut == true) ? "soldout" : "") + "'>" +
                 "<a href='" + url + "'>" +
                 "<span class='conf-title green'>" + conf.Title + "</span>" +
                 "<span class='date green'>" + conf.DisplayDate + "</span>" +
                 "<span class='date sold-out'>SOLD OUT</span>" +
                 "<p>" + conf.ShortDescription + "</p>" +
                 "<span class='conf-speakers-label green'>Speakers</span>" +
                 "<span class='conf-speakers'>" + conf.Speakers + "</span>" +
                 "<span class='register'>Click Here To Register</span>" +
                  "</a></div>";
             } else {
                 return "<div class='conf-overlay " + ((conf.IsSoldOut == true) ? "soldout" : "") + "'>" +
                 "<span class='conf-title green'>" + conf.Title + "</span>" +
                 "<span class='date green'>" + conf.DisplayDate + "</span>" +
                 "<span class='date sold-out'>SOLD OUT</span>" +
                 "<p>" + conf.ShortDescription + "</p>" +
                 "<span class='conf-speakers-label green'>Speakers</span>" +
                 "<span class='conf-speakers'>" + conf.Speakers + "</span>" +

                  (conf.NonSponsoredExternalLink != ""? (
                     "<span class='conf-speakers-label green'>Contact Information</span>" +
                  "<a  target='_blank' class='contact-link' href=http://"+conf.NonSponsoredExternalLink +">"+conf.NonSponsoredExternalLink+"</a>" +
                  "<a target='_blank' class='contact-link' href=mailto:"+conf.NonSponsoredContactEmail +">"+conf.NonSponsoredContactEmail+"</a>" ):"")+
                  "</div>";
             }
         };
    }


    function centerMap(resultsMap, country, state) {
        var e = document.getElementById("ddlCountry");
        var address = e.options[e.selectedIndex].value;
        if(address=="USA")
        {
            map.setZoom(4);
            layer2.setMap();
            layer1.setMap(map);

        } else {
            layer1.setMap();
            layer2.setMap(map);
            map.setZoom(3);
        }
        if(country === 'USA' && state === "AK"){
            address ="Alaska, USA";
        }
        geocoder.geocode({'address': address}, function(results, status) {
            if (status === google.maps.GeocoderStatus.OK) {
                resultsMap.setCenter(results[0].geometry.location);
            }
        });
        detectBrowser();
    }

     function geocodeLocationAndCreateMarker(conference) {
         gecodeAddress(conference);
     }

     function gecodeAddress(conference) {
         geocoder.geocode({'address': conference.Location.State}, function(results, status) {
             if (status == "OVER_QUERY_LIMIT") {
                 console.log("OVER_QUERY_LIMIT");
                 return;
             }
             var result = results[0].geometry.location;
             conference.Location.Latitude = result.lat();
             conference.Location.Longitude = result.lng();
             createMarker(conference);
         });
     }

     function showHideMarkers(selectedConferences) {
         for (var i = 0; i < myMarkers.length; i++) {
             myMarkers[i].setVisible(false);

          //   infoWindows[i].close();
         }
         for (var i = 0; i < myMarkers.length; i++) {
             for (var j = 0; j < selectedConferences.length; j++) {
                 if (myMarkers[i].id == selectedConferences[j].Id) {
                     myMarkers[i].setVisible(true);
                     break;
                 }
             }
         }
     }

     function detectBrowser() {
         var useragent = navigator.userAgent;
         var mapdiv = document.getElementById("map");
         if (useragent.indexOf('iPhone') != -1 || useragent.indexOf('Android') != -1 ) {
             mapdiv.style.width = '100%';
             mapdiv.style.height = '100%';
             mapdiv.style.minHeight = '250px';
             map.setZoom(3);
         } else {
             mapdiv.style.width = '100%';
             mapdiv.style.height = '520px';
         }
     }

     function filterConferences() {
         var confs = conferences.conferences;

         var country = $("#ddlCountry").val();
         var eventType = $("#ddlEventType").val();
         var eventTypeValue = $("#ddlEventType").val() === "true";
         var state = $stateDD.find("option:selected").val().replace("_"+country,"");

         var selectedConferences = new Array();
         for (var i = 0; i < confs.length; i++) {
             var result = false;
             if (state != "") {
                 result = (confs[i].Location.State == state);
                 if(!result)continue;
             }
             result = (confs[i].Location.Country == country);
             if (!result) {
                 continue;
             }
             if (eventType != "") {
                 result = (confs[i].IsSponsored == eventTypeValue);
                 if(!result)continue;
             }
             var date = parseJsonDate(confs[i].StartDate);
             var selectedDate = $("#ddlYear").val();
             if (selectedDate !== '' && selectedDate!=null) {
                 var dateParts = selectedDate.split("_");
                 var selectedYear = dateParts[0];
                 var selectedMonth = dateParts[1];
                 if (selectedMonth == "0") {
                     result = date.getFullYear() == selectedYear;
                     if(!result)continue;
                 } else {
                     result= (date.getFullYear() == selectedYear) && (date.getMonth()+1 == selectedMonth);
                     if(!result)continue;
                 }
             }
             selectedConferences.push(confs[i]);
         }
         showHideMarkers(selectedConferences);
         centerMap(map, country, state);
     }

     function prepareMarkers() {
         var confs = conferences.conferences;
         for (var i = 0; i < confs.length; i++) {
             if (confs[i].Location.Longitude == null || confs[i].Location.Longitude == "") {
                geocodeLocationAndCreateMarker(confs[i]);
             } else {
                 createMarker(confs[i]);
             }
         }
     }

     function buildMap(){
         initMap();
         centerMap(map);
         prepareMarkers();
         detectBrowser();
         $("#ddlCountry").change();
     }

     var $stateDD = $("#ddlState");
     var initialStateDD = $("#ddlState")[0][0].text;
     var currentCountry = "USA";
     $("#ddlCountry").change(function () {
         currentCountry = $(this).val();
         $stateDD.val('');
         if (currentCountry == "USA") {
            $stateDD[0][0].text = "All States";
         } else {
             $stateDD[0][0].text = initialStateDD;
         }

         $stateDD.find('option').each(function () {
             var currentState = $(this);
             if (currentState.val() != '') {
                 if (currentState.val().indexOf(currentCountry) > -1) {
                     if ( currentState.parent().is( "a" ) ) {
                         currentState.unwrap();
                     }
                 } else {
                     if (!currentState.parent().is( "a" ) ) {
                         currentState.wrap("<a></a>");
                     }
                 }
             }
         });
         $stateDD.change();
     });
    function findParent() {
        $(".gm-style-iw").parent("div").addClass("info-parent");
    }

     $("#ddlYear option").each(function(index, item) {
        if (item.value.indexOf("_0")>-1) {
            $(item).addClass("bold-option");
        }

     });

</script>
