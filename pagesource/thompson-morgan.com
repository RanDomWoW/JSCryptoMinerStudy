			}, 3000)		
          }
		});
	}else{
		$('p#BISmsg').html('Invalid Email Address');
	}
}

var windowWidth = $(window).width();

$(window).resize(function() {
  windowWidth = $(window).width();
});

$('.shut, .mask').click(function(){ closeKey(); });
function keyInfo(){
		var m = $('.mask');
		$('.popout').animate({
	        right: '0%'
	    });
	    m.show();
	    return false;
	};
function closeKey(){
		var x = $('.popout');
		var m = $('.mask');
		$(x).animate({
	        right: '-100%'
	    });
	    m.hide();
}

