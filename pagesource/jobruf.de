    $(function () {
        var $jobMenu = $('#job-menu-studentenvermittlung');

        var $landingpageForm =
            $jobMenu
                .parents('.landingpage-form');

        var $alternatSearch =
            $landingpageForm
                .find('.alternative-search');

        var $searchForm =
            $landingpageForm
                .find('[name="job_menu_form[search][search_term]"]');

        var defaultSearchText =
            $landingpageForm
                .find('.selected')
                .text();

        var $zipField = $('#job_menu_form_zip');

        $(window).click(function (ev) {
            // TODO: Fix closing on multiple jobMenus
            if ($(ev.target).parents(".landingpage-form").length === 0) {
                /**
                 * @type {JobMenu}
                 */
                var menuObject = $jobMenu.data('jobMenu');
                if (menuObject.isOpen()) {
                    menuObject.close();
                }
            }
        });

        $jobMenu
            .on(
                'afterOpen.jobMenu',
                /**
                 * @param {jQuery.Event} e
                 * @param {JobMenu} jobMenu
                 */
                function (e, jobMenu) {
                    $('span[data-trigger="menu"]').hide();
                    $alternatSearch.show();
                    var $input = $alternatSearch.find('input');
                    if (!ModuleManager.getModule('style').isMobile) {
                        $input.focus();
                    } else {
                        ModuleManager.getModule('style').scrollToTarget($input)
                    }
                }
            )
            .on(
                'afterClose.jobMenu',
                /**
                 * @param {jQuery.Event} e
                 * @param {JobMenu} jobMenu
                 */
                function (e, jobMenu) {
                    $('span[data-trigger="menu"]').show();
                    $alternatSearch.hide();

                    if ($searchForm.val()) {
                        // Repace Was? with Searchbox content.
                        $landingpageForm.find('.selected').text($searchForm.val());
                        $landingpageForm.find(':checked').prop('checked', false);
                    } else {
                        if ($landingpageForm.find(':checked').length === 0) {
                            $landingpageForm.find('.selected').text(defaultSearchText);
                        }
                    }
                }
            )
            .on(
                'afterSelectJob.jobMenu',
                /**
                 * @param {jQuery.Event} e
                 * @param {JobMenu} jobMenu
                 */
                function (e, jobMenu) {
                    $alternatSearch.find('input').val('');
                    $zipField.focus();
                }
            )
        ;
        $searchForm
            .on(
                'autocompleteselect autcompletechange keypress',
                function (e, ui) {
                    if (e.type === 'keypress' && e.which !== 13 /*RETURN*/) {
                        // Skip everything beside return for the job selection reset.
                        return;
                    }

                    if (typeof ui !== 'undefined') {
                        $searchForm.val(ui.item.value);
