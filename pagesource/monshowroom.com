<script>
  $("#formPopinAlerteStock").on("submit", function (e) {
    e.preventDefault();
    if ($(this).children("input[name=id_prod]").val() !== '' && $(this).children("input[name=id_taille]").val() !== '' && $(this).children("input[name=email]").val() !== '') {
      el = $(this);
      var idProd = el.children("input[name=id_prod]").val(),
              idTaille = el.children("input[name=id_taille]").val(),
              email = el.children("input[name=email]").val(),
              urlInscription = urlInscriptionAlerteStock;
      _Ajax.call({
        url: urlInscription,
        data: {
          idProd: idProd,
          idTaille: idTaille,
          email: email
        },
        success: function (response) {
          if (response.status == 1) {
            _Utils.showNotif(response.message, 'success', 3000);
            localStorage.setItem('email', email);
            $('#icon-alert-close').click();
          } else {
            _Utils.showNotif(response.message, 'error', 3000);
          }
        }
      });
    }
  });
</script>
