            aPcseckill_warp.eq(_tindex).css('left',- iproductCart[_tindex] * $('.Pcseckill_box').eq(_tindex).width())
        }
        
        iproductCart[_tindex]--;

        iPceckill_btn.eq(_tindex).children().removeClass('active');
        iPceckill_btn.eq(_tindex).children().eq(iproductCart[_tindex]).addClass('active');

        aPcseckill_warp.eq(_tindex).stop().animate({'left': - iproductCart[_tindex] * $('.Pcseckill_box').eq(_tindex).width()},600);
    });
    $('.pcsk_right').click(function(event) {
        /* Act on the event */
        var _tindex =   $('.pcsk_right').index($(this));
        if(iproductCart[_tindex] > aPcindex_seckill.eq(_tindex).find('.PCseckill_list').length / 2 -1){
            iproductCart[_tindex] = 0;
            aPcseckill_warp.eq(_tindex).css('left',- iproductCart[_tindex] * $('.Pcseckill_box').eq(_tindex).width())
        }
        iproductCart[_tindex]++;

