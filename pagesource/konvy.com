<script>
function productClick(from, id, product, price){
    dataLayer.push({
        'event': 'productClick',
        'ecommerce': {
            'click': {
                'actionField': {'list': from},      // Optional list property.
                'products': [{
                    'id': id,
                    'name': product,                      // Name or ID is required.
                    'price': price,
                }]
            }
        }
    });
}
function productAddToCart(id, product, price){
    dataLayer.push({
        'event': 'addToCart',
        'ecommerce': {
            'currencyCode': 'THB',
            'add': {
                'products': [{
                    'id': id,
                    'name': product,                      // Name or ID is required.
                    'price': price,
                    'quantity': 1
                }]
            }
        }
    });
}
function productAddToCartId(id){
    dataLayer.push({
        'event': 'addToCart',
        'ecommerce': {
            'currencyCode': 'THB',
            'add': {
                'products': [{
                    'id': id,
                    'quantity': 1
                }]
            }
        }
    });
}
</script>

