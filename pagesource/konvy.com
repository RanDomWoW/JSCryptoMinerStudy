        if(arr != null) return (arr[2]);
        return null;
    }

    function setCookie (name, value, expire) {
        var expireStr = '';

        if (expire || expire > 0) {
            var exp  = new Date();
            exp.setTime(exp.getTime() + expire * 1000);

            expireStr = ';expires=' + exp.toGMTString();
        }

        document.cookie = name + '=' + value +
                          '; path=/;' + expireStr +
                          'domain=.konvy.com';
    }
</script>


