    <script type='text/javascript'>
        $(function(){
            if($('body').hasClass('reply-edit') || $('body').hasClass('topic-edit')){
                var currentAttachments = $('#bbpress-forums .bbp-attachments-form-current');
                if(currentAttachments.length){
                    var attachments = currentAttachments.find('.d4p-bbp-attachment');
                    if(attachments.length){
                        var i = 0, nbAttachments = attachments.length;
                        for(i; i<nbAttachments; i++){
                            if(i > 0){
                                if(attachments.eq(i).find('> a').attr('href') === attachments.eq(i-1).find('> a').attr('href')){
                                    attachments.eq(i-1).css('display', 'none');
                                }
                            }
                        }
                    }
                }
            }
        });
    </script>

    <script type="text/javascript">
        (function(window) {
            var pardotUrl = 'https://go.pardot.com';

            addEvent(window, 'message', function(message) {
                var dataLayer = window.dataLayer || (window.dataLayer = []);
                if(message.origin === pardotUrl && typeof message.data !== 'number'){
                    var myAlert = message.data;
                    var splits = myAlert.split(',');
                    var category = splits[0];
                    var action = splits[1];
                    var label = splits[2];
                    if( label !== '' ){
                        __gaTracker('send', 'event', category, action, label);
                    }
                }       
            });

            function addEvent(el, evt, fn) {
                if (el.addEventListener) {
                    el.addEventListener(evt, fn);
                } else if (el.attachEvent) {
                    el.attachEvent('on' + evt, function(evt) {
                        fn.call(el, evt);
                    });
                } else if (typeof el['on' + evt] === 'undefined' || el['on' + evt] === null) {
                    el['on' + evt] = function(evt) {
                        fn.call(el, evt);
                    };
                }
            }

        })(window);
    </script>

