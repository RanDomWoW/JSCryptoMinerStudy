function a_search(){
	
	$.ajax({
	    url: "./amazon.php", 
	    dataType: 'jsonp',
	    data: {
	        q: query,
	        status: item_status,
	        i: 1
	    },
	    jsonp: 'jsoncallback',
	    success: function( data ) {
		
		var obj = data.Items.Item;
		if(!$.isArray(obj)){obj=[obj]}
		//if(data.Items.TotalResults==="1")obj=[obj];
		
	      $.each(obj, function(i, itm) {
	      	if(itm)
	      	{
	        	try
	        	{
	        		if (itm.ItemAttributes.Title && itm.Offers.Offer) {
		              //var code = itm.Item.itmName;   
		              var name = itm.ItemAttributes.Title;
		              if(name.length>60)
	          				name = name.substring(0,60) + "...";   

		              var code = itm.ItemAttributes.EAN;

					  var categoryName = itm.ItemAttributes.ProductGroup;
					  
					  var mpn = itm.ItemAttributes.MPN;
					  
					  var offers = itm.Offers.Offer;
					  if(!$.isArray(offers)){offers=[offers]}
					  
		              var price = parseInt(itm.OfferSummary.LowestNewPrice.Amount);

		              var availability = itm.Offers.Offer.OfferListing ? itm.Offers.Offer.OfferListing.Availability : '';
		              var imgS = itm.SmallImage.URL;  
		              var imgM = itm.MediumImage.URL;  
		              var url = itm.DetailPageURL;   
					  
					  //var iframe = itm.CustomerReviews.IFrameURL;

		              var hash = new Array();
		              hash.shopName = "アマゾン";
		              hash.jan = "";
		              hash.isbn = "";
		              hash.code = code;
		              hash.mpn = mpn;
		              hash.name = name;
		              hash.price = price;
		              hash.shipping = "";
		              hash.availability = availability==null? "" : availability.replace("。","");
		              hash.imgM = imgM;
		              hash.imgS = imgS;
		              hash.url = url;
		              
		              hash.categoryName = categoryName;
	     			  hash.num = i;
		              
		              //hash.iframe = iframe;
					
					  //all_prices.push(price);
					  original_data['amazon'].push(hash);
				}
			  }
			  catch(e)
			  {
			  		return "error";
			  }
	        }   
	      }); // end of each   

	    },
	    error: function() {
	        //alert('error');
	    },
	    complete: function() {
			$.extend(true, pickup_data['amazon'], original_data['amazon']);
	  		
	  		var html=getHtml('amazon',0,10);
	  		if(html!="")
		  		$('#a_results').html(html);
	  		Finalize();

	    }
	    
	})//ajax

}


