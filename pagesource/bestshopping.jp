function getBidsDiv(bids)
{
	var div="";
	if(bids>=10)
	{
		div = '<div class="bids1">入札 ' + bids + '</div>';
	}
	else if(bids>0)
	{
		div = '<div class="bids2">入札 ' + bids + '</div>';
	}
	else
	{
		div = "入札 " + bids;
	}
	
	return div;

}
function getLeftTime(end_time)
{
	var left_total_seconds = (end_time - new Date())/1000;
	var left_days = Math.floor(left_total_seconds/86400);
	var left_hours = Math.floor((left_total_seconds - left_days*86400)/3600);
	var left_minutes = Math.floor((left_total_seconds - left_days*86400 - left_hours*3600)/60);
	var left_seconds = Math.floor(left_total_seconds - left_days*86400 - left_hours*3600 - left_minutes*60);
	
	var left_time="";
	if(left_days>0)
	{
		left_time = left_days + "日";
	}
	else if(left_hours>0)
	{
		left_time = left_hours + "時間";
	}
	else if(left_minutes>10)
	{
		left_time = left_minutes + "分";
	}
	else if(left_minutes>0)
	{
		left_time = left_minutes + "分 " + left_seconds + "秒";
	}
	else if(left_seconds>0)
	{
		left_time = left_seconds + "秒"
	}
	else
	{
		left_time = "終了";
	}
	

	
	if(left_total_seconds<=10)
	{
		left_time = '終了';
	}
	else if(left_total_seconds>0 && left_total_seconds<3600)
	{
		left_time = '<div class="left_time1">残り ' + left_time + '</div>';
	}
	else if(left_total_seconds<14400)
	{
		left_time = '<div class="left_time2">残り ' + left_time + '</div>';
	}
	else if(left_total_seconds<43200)
	{
		left_time = '<div class="left_time3">残り ' + left_time + '</div>';
	}

	return left_time;
}




function toHankaku(str) {
	return str.replace(/[！-～]/g, function(s) {
	    return String.fromCharCode(s.charCodeAt(0) - 0xFEE0);
	});
}

function toZenkaku(str) {
	return str.replace(/[\!-\~]/g, function(s) {
		return String.fromCharCode(s.charCodeAt(0) + 0xFEE0);
	});
}

