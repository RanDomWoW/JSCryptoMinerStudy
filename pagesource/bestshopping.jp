function specialSortAuc(shop, st)
{
	
	switch(st)
	{

		case "re": special_sort_auc(shop, 'sortValue', false); break;
		case "lo": yo_search("cbids", "a"); break;
		case "hi": yo_search("cbids", "b");
		break;
		case "ev": yo_search("bidorbuy", "a"); break;
		case "bi": yo_search("bids", "a"); break;
		case "ti": yo_search("end", "a"); break;
		default: break;
	}
}


	//ソート用ラジオボタン
	var radios = $("input[name=sort_radio]");

	radios.on("change", function(e){
	    // 一旦全てのlabel要素からselectedクラスを除去
	    radios.closest("label").removeClass("selected");
	    // 選択されたラジオボタンの親のlabelにselectedクラスを設定
	    $(e.target).closest("label").addClass("selected");

//alert(e.target.value);
	    //console.log("Changed to " + e.target.value);
	    specialSortAuc("yafuoku", e.target.value);//とりあえず、ヤフオク専用
	    
	    //console.log("ソート");
	    //getSummary();
	});


	// 選択状態を初期化する
	radios.filter(":checked").trigger("change");

	//IE対策
	radios.closest("label").on("click", function(e){
	    var input = $(this).find("input");
	    if(! input.prop("checked")){
	        input.prop("checked", true).trigger("change");
	    }
	});
