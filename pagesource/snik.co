.breadcrumbs{display:block;font-size: 11px;list-style: none;margin-bottom: 0;padding: 0.3rem 0rem;margin: 0;}.breadcrumbs li{display:inline;margin:0}.breadcrumbs .separator{height: 4px;width: 4px;border: 1px solid #777;border-width: 0px 1px 1px 0;transform: rotate(-45deg);display: inline-block;position: relative;left: 0px;top: -1px;margin-left: 8px;margin-right: 10px;overflow: hidden;text-indent: -90px;}.breadcrumbs li:first-child{margin-left:0} .breadcrumbs h1 {display: inline; font-size: 12px;} .breadcrumbs>span,.breadcrumbs a{color:#777;}.breadcrumbs a:hover{color:#2779ec;}
.card{position:relative;box-sizing:border-box;background:#fff;display:block;float:left;width:25%;height:auto;color:#222;padding-bottom:1.4rem;}.card .card-img-link {display: block;padding: 0.5rem 1.1rem 0;}
.card .image{vertical-align:middle;display:inline-block;}
.card .info{position:relative;margin:0 1.2rem 6px;font-size:13px;display:flex;flex-direction: column-reverse;}.card .info h3 {font-family: Helvetica, sans-serif;font-weight: normal;margin: 0;font-size: 12px;color: #404040;} 
.card .brand-link{display: block;max-height: 40px;font-size: 10px;padding: 0.3rem 0.9rem;padding-top: .33rem;text-align: center;text-transform: uppercase;font-family: Helvetica Neue;font-weight: bold;    top: -22px;position: absolute;z-index: 300;width: 90%;box-sizing: border-box;border: 1px solid transparent;text-shadow: 0px -1px 8px #fff;}
.card .info .promo{font-family:Georgia;font-style:italic;position:absolute;width:100%;height:30px;line-height:30px;top:-30px;background:rgba(255,255,255,0.8);color:#555;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;text-align:center}.card .info .desc{display:inline-block;font-size:12px;line-height:1.2}.card .info .desc h3{font-family:Helvetica, sans-serif;font-weight:normal;margin:0;font-size:1em;}.card .item-description{line-height:1.4;font-size:12px;color:#999;margin:0;margin-bottom:0.3em;}.card .brand{color:#666;text-transform:uppercase;white-space:nowrap;overflow:hidden;text-overflow:ellipsis}.card .prices{font-size:1.1em}.card .price{font-weight:bold;color:#222;margin-right: .8rem;}.card .price,.card .old-price{display:inline-block;white-space:nowrap} .card .actions{position:relative;margin-right: 1.2rem;}.card .actions .icon:before{font-size:20px}.card .list-info {display: none;} .card .actions .show-link{position:absolute;right:0;top:-30px;color:#999;padding:4px 4px 0 4px}.card .card-img{position:relative;height:276px;text-align:center;background:transparent}.card .card-img:after{content: "";position: absolute;bottom: 0;left: 0;width: 100%;height: 20px;background: #fff;background: -moz-linear-gradient(top, rgba(255,255,255,0) 0%, #fff 100%);background: -webkit-gradient(left top, left bottom, color-stop(0%, rgba(255,255,255,0)), color-stop(100%, #fff));background: -webkit-linear-gradient(top, rgba(255,255,255,0) 0%, #fff 100%);background: -o-linear-gradient(top, rgba(255,255,255,0) 0%, #fff 100%);background: -ms-linear-gradient(top, rgba(255,255,255,0) 0%, #fff 100%);background: linear-gradient(to bottom, rgba(255,255,255,0) 0%,#fff 80% ,#fff 100%);z-index: 10;}.card .card-img img{position:absolute;max-width:100%;height: auto;max-height:100%; bottom: 0;left: 0;right: 0; margin: auto auto; -webkit-filter: saturate(1.1);filter: saturate(1.1);}
/*.card:hover img{-webkit-filter: saturate(1.2);filter: saturate(1.2);}*/
.card .old-price{position:relative; color: #777;}.card .old-price::after{content: '';width: 68%;position: absolute;right: 0;top: 7px;left: -4px;border-bottom: 1px solid #666;-webkit-transform: skewY(-18deg);transform: skewY(-18deg);}.card .status{position:absolute;bottom:2px;z-index:200}.card .status .st-label{display:inline-block;vertical-align:middle;font-size:12px;line-height:16px;color:#404040}.card .status .st-icon{display:inline-block;width:8px;height:8px;margin-right:4px;border-radius:50%}.card.is-chip .st-icon{background:#2ecc71}.card.is-bigsale .st-icon{background:#e74c3c}.card .out-stock-label{position:absolute;z-index:20;font-size:.9em;width:100%;font-size:13px;line-height:14px;bottom:0;text-align:right;color:#bbb}.card .out-stock-label .icon-out-stock{position:relative;font-size:19px;padding-right:6px}.card .out-stock-label .icon-out-stock:after{position:absolute;content:'';width:18px;right:2px;top:11px;border-bottom:1px solid #ccc;-webkit-transform:skewY(-7deg);transform:skewY(-45deg)}
@media (hover: hover) {
  .catalog-section h1.title a:hover {color: #404040;}
  /*.card:hover { outline: 2px solid #06befb; z-index: 800;}*/
  .card .brand-link:hover {background: #fbfbfb;    background: #fbfbfb;
    border-color: #222;
    color: #222;}
  .b-list li.cat-item a:hover {border-color: #222;-moz-transition: all .3s;-o-transition: all .3s;-webkit-transition: all .3s;transition: all .3s;}
}

.catalog-section .b-head .sub-title {margin: .6rem 0;font-size: 0.66rem;color: #888;margin-top: 0;margin-bottom: 0.4rem;letter-spacing: 0.08rem;text-transform: uppercase;display: inline-block;margin-right: .25rem;}
.navi-block,.cats-cards{margin-bottom: 4px;}.cats-cards .nav-list{display: inline-block;list-style: none;margin: 0;padding: 0;}
.navi-block.pop{display: inline-block;padding: 8px 1.2rem 4px;background: #f4f4f4;border-radius: 6px;}.pop a.tag{font-weight: bold;}
.cats-cards .nav-item {display: inline-block ;background: #fff;border: 1px solid #e6e6e6;border-radius: 6px;box-shadow: 0px 2px 4px 0px rgba(0,0,0,0.06) !important;margin-right: 0.5rem;margin-bottom: .5rem;vertical-align: top;overflow: hidden;white-space: normal;}
.cats-cards .nav-item .wrap,.cats-cards .nav-item .title-cat {display:block; text-decoration: none;padding: 0.7rem .5rem;font-weight: bold;vertical-align: middle;color: #484848;}
.cats-cards .wrap {position: relative;display:inline-block !important;overflow: hidden;height: 40px;width: 40px;padding: 0 !important;border-right: 1px solid #eee;
}.cats-cards .wrap img {position: absolute;left: 0;bottom: -18%;}
.catalog-section .cats-cards .title-cat {padding:0;font-size: .68rem;width: 132px;margin: 0;display: inline-block;vertical-align: middle;}
.catalog-section .cats-cards .title-cat>span {overflow: hidden !important;text-overflow: ellipsis !important;display: -webkit-box !important;-webkit-line-clamp: 2 !important;-webkit-box-orient: vertical !important;margin: .5rem;padding: 0;}

