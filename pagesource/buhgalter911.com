function setLng( value, expires, path, domain, secure){
var coockieSaveTime = new Date((new Date).getTime() + (365 * 86400000));
var cookieName = "userSettings";
var matches = document.cookie.match(new RegExp(
"(?:^|; )" + cookieName.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)" ));
var cook=(matches!='')&&(matches!=null)?JSON.parse(matches[1]):{};
cook.lng=escape(value);
document.cookie = cookieName + "=" + JSON.stringify(cook) + "; path=/; expires=" + coockieSaveTime;
}
function setCook(cookieName, value, expires,path){

var path=(path!='')?path:'';
var domain='';
var secure='';
var now = new Date();
var time = now.getTime();
time += expires;
now.setTime(time);
var coockieSaveTime = now.toUTCString();

document.cookie = cookieName + '=' + value + '; path='+path+'; expires=' + coockieSaveTime;
}
function getCook(cookieName){
var matches = document.cookie.match(new RegExp(
"(?:^|; )" + cookieName.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)" ));
var cook=(matches!='')&&(matches!=null)?JSON.parse(matches[1]):'';
return cook;
}

