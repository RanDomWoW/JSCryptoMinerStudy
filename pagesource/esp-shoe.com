<script>
$(function(){


// feed読み込み
	entryPerPage = 4;
	maxEntries = 32;
	thisPage = 1;

	var url = location.href;
	var urlParam = location.search;
	var params = url.split("?");
	if (params[1]) {
		params = params[1].split("&");
		var paramArray = [];
		for (i=0; i<params.length; i++) {
			var param = params[i].split("=");
			paramArray[param[0]] = param[1];
		}

		if(paramArray["page"]){
			thisPage = paramArray["page"]
		}
	}


	container = $(".magazine_container");

	loadImg ='<img src="/static/full/images/common/news_loading.gif" class="news_load" />';
	container.prepend(loadImg);
	initialize();

});

function initialize() {
	// パラメーターによるURL振り分け
	var page = "";
	var url = location.href;
	var urlParam = location.search;
	var params = url.split("?");
	if (params[1]) {
		params = params[1].split("&");
		var paramArray = [];
		for (i=0; i<params.length; i++) {
			var param = params[i].split("=");
			paramArray[param[0]] = param[1];
		}

		if(paramArray["p"]){
			page = paramArray["p"]
		}
	}

	feedUrl = "https://esp-shoe.sakura.ne.jp/blog/?feed=rss2";
	$.ajax( {
		type: "get",
		cache: false,
		dataType: "xml",
		crossDomain: true,
		url : feedUrl,
		success : function(xml){
			var items = $(xml).find("item");
			var entryNum = items.size();
			if (entryNum > 0) {
					container=$(".magazine_container");

				for (var i=entryPerPage*(thisPage-1); i<entryPerPage*thisPage && i<entryNum; i++) {
					var item = items.eq(i);

					var html = "";

					var entryLink = $(item).find("link").text();

						// タイトルデータ生成
						var entryTitle = $(item).find("title").text();

						// テキストデータ生成
						var entryText = $(item).find("content\\:encoded, encoded").text();
						// 画像データ生成
						var entryImageBase = $(item).find("description, encoded").text();
						var entryImage ="";
						if(entryImageBase.indexOf('thumb')!= -1){
							var entryImage = $(entryImageBase).find("img").attr("src").replace("https://magazine.esp-shoe.com/","https://esp-shoe.sakura.ne.jp/blog/");
						}
						var entryLink = entryLink.replace("https:","http:");

						var entryCategoryArray =$(item).find("category");
						var entryCategory ="";
						var entryInfomation = "";
						$(entryCategoryArray).each(function(){
							var _category = $(this).text();
							if ( _category.toUpperCase().indexOf("INFORMATION") > -1 ) {
								if ( entryInfomation == "" ) {
									entryInfomation = _category;
								} else {
									entryInfomation = entryInfomation + "," + _category;
								}
							} else {
								if ( entryCategory == "" ) {
									entryCategory = _category;
								} else {
									entryCategory = entryCategory + "," + _category;
								}
							}
						});

						// 日付データ生成
						var entryDateBase = new Date($(item).find('pubDate').text());
						var entryDate     = entryDateBase.getFullYear()+'.'+(entryDateBase.getMonth()+1)+'.'+entryDateBase.getDate();
						if (entryDateBase.getMonth() < 9) {
							entryDateForTimeMonth = '0'+(entryDateBase.getMonth()+1);
						} else {
							entryDateForTimeMonth = entryDateBase.getMonth()+1;
						}
						if (entryDateBase.getDate() < 10) {
							entryDateForTimeDay   = '0'+entryDateBase.getDate();
						} else {
							entryDateForTimeDay   = entryDateBase.getDate();
						}
						var entryDateForTime = entryDateBase.getFullYear()+'.'+entryDateForTimeMonth+'.'+entryDateForTimeDay;

						//Newフラグ
						var newIcon="";
						keep_day = 7; // この日数表示されます
						old_day = new Date(entryDateBase.getFullYear() + "/" + (entryDateBase.getMonth()+1) + "/" +entryDateBase.getDate());
						new_day = new Date();
						d =(new_day - old_day) / (1000 * 24 * 3600);

						if(d <= keep_day) {
							newIcon=" icon_new";
						}

						// html生成
						html += '<li>';
						html += '<a href=\"'+ entryLink +'\" target=\"_blank\">';
						html += '<div class="magazine_photo"><img src="'+entryImage+'" width=\"130\" /></div>';
						html += '<div class="magazine_text">';
						html += '<span class="date">'+entryDateForTime+'</span>';
						html += '<h5>'+entryTitle+'</h5>';
						html += '</div>';
						html += '</a>';
						html += '</li>';

							container.append(html).hide().fadeIn(1000);
							$('.news_load').hide();
					}


			} else {
				container.append('<li class="newslist_noentry">記事はありません</li>');
			}



		},
		error : function (xhr,text,thrown){
			console.log(text);
		}
	});
}
	</script>
							

