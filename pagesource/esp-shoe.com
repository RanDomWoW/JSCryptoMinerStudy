function getBookMarkList() {
    var params = new Array();
    params.push('wt=json');

	var bookmarkCookie = ADSS.EcApp.SimpleBookMark.getCookies();
	//console.log(bookmarkCookie.SimpleBookMark);

	if (bookmarkCookie.SimpleBookMark) {
		var bookmarks = $.parseJSON(bookmarkCookie.SimpleBookMark);
		if ( bookmarks.length > 0 ) {
		  	params.push('rows=50');
		  	params.push('fq=cd:(' + bookmarks.join('+OR+') + ')');
		  	params.push('fq=-bd:EO040');
		    //params.push('fq=-stnum:0');

		    var template = _.template($('#templateClipItems').html());

			var $header_clip_container = $('.header_clip_box');
			$header_clip_container.html("");
	    	$header_clip_container.append('<h5>クリップ</h5>');
			$header_clip_container.append('<ul id="header_clip"></ul>');
			var $header_clip_container = $('#header_clip');

		    $.ajax({
		        url: '/ise/select',
		        data : params.join('&'),
		        dataType : "json",
		    }).done(function(data, status, xhr) {
		    	var numFound = data.response.numFound;
		        var rows = 1 * data.responseHeader.params.rows;
		        var start = 1 * data.responseHeader.params.start;
		    	numFound = numFound || 0;
		        $header_clip_container.html('');
		        if ( numFound > 0 ) {
					var count = 0;
			        var dataArray = new Array(numFound);
					var groupCount = 0;
					var docArray = new Array();
			        _.each(data.response.docs, function(doc) {
	  			        var index = $.inArray(doc.cd, bookmarks);
					    //$header_clip_container.append(template({ doc: doc }));
					    if ( index > -1 ) {
					    	dataArray[index] = doc;
					    }
			        });
				var ret = ADSS.EcApp.SimpleBookMark.data();

			        _.each(dataArray, function(doc) {

						if (doc && (count < 20) ) {
							docArray.push(doc);
							var isClip = false;
							if ( jQuery.inArray(doc.cd, ret) > -1 ) {
								isClip = true;
							}
								$header_clip_container.append(template({ docArray: docArray, isClip: isClip }));
								docArray = new Array();
							count++;
						}

			        });
					if ( groupCount > 0 ) {
						$header_clip_container.append(template({ docArray: docArray }));
					}

					var minus = 0;
					for (var i in bookmarks) {
						if (dataArray[i]) {
							continue;
						}
						ADSS.EcApp.SimpleBookMark.removeStatic(bookmarks[i]);
						minus++;
					}
					if (minus > 0) {
						//alert("販売終了商品が存在しました。");
