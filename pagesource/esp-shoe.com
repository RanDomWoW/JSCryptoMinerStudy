<script type="text/javascript">
	$(function() {
		var params = new Array();
		params.push('wt=json');
		params.push('facet=true');
		params.push('facet.limit=1000');
		params.push('facet.field=cf1');
		params.push('facet.field=cf2');
	   	params.push('facet.field=cf3');
		params.push('facet.sort=false');
		params.push('facet.mincount=1');
		params.push('fq=-bd:EO040');

		var dataParam = params.join('&');
		dataParam = dataParam.replace("&&","&");
		$.ajax({
			'url'      : '/ise/select',
			'data'     : dataParam,
			'dataType' : "json"
		}).done(function(data) {
			var largeCts = data.facet_counts.facet_fields.cf1;
			var middleCts = data.facet_counts.facet_fields.cf2;
			var smallCts = data.facet_counts.facet_fields.cf3;
			var selectedCc1 = "";
			var selectedCc2 = "";

			var cc = "";

			var smallCategoryPossibleList = new Array();
			smallCategoryPossibleList.push("DA");
			smallCategoryPossibleList.push("DB");
			smallCategoryPossibleList.push("DC");

			var footer_holder = new Array();
			for (var i = 0; i < largeCts.length; i+= 2) {
				var _codeName = largeCts[i].split("__");
				var _code = _codeName[0];
				var _name = _codeName[1]
				var _count = largeCts[i+1];

				if ( _count > 0 ) {
					var map = {'code' : _code, 'name' : _name, 'count' : _count};
					if ( selectedCc1 == _code ) {
						map.active = true;
					}
					map.link = "https://www.esp-shoe.com/ap/s/s?sort=fsdt+desc,rk+desc&fq=scls:n_*&fq=cc1:" + _code ;

					var holderM = new Array();
					for (var k = 0; k < middleCts.length; k+= 2) {
						var _McodeName = middleCts[k].split("__");
						var _Mcode = _McodeName[0];
						var _Mname = _McodeName[1];
						var _Mcount = middleCts[k+1];

						if (_code != _Mcode.substr(0, 1)) {
							continue;
						}

						if (parseInt(_Mcount, 10) < 1) {
							continue;
						}

						var mid = {'code' : _Mcode, 'name' : _Mname, 'count' : _Mcount};
						mid.link = "https://www.esp-shoe.com/ap/s/s?sort=fsdt+desc,rk+desc&fq=scls:n_*&fq=cc2:" + _Mcode ;

						var holderS = new Array();
						if ( jQuery.inArray(_Mcode, smallCategoryPossibleList) > -1 ) {
				    		for (var p = 0; p < smallCts.length; p+= 2) {
				       			var _ScodeName = smallCts[p].split("__");
				       			var _Scode = _ScodeName[0];
				       			var _Sname = _ScodeName[1];
				       			var _Scount = smallCts[p+1];

				       			if (_Mcode != _Scode.substr(0, 2)) {
									continue;
								}

				        		if (parseInt(_Scount, 10) < 1) {
				        			continue;
				        		}

				       			var sml = {'code' : _Scode, 'name' : _Sname, 'count' : _Scount};
			       				if ( cc == _Scode ) {
				       				sml.active = true;
				       			}
					          	sml.link = "https://www.esp-shoe.com/ap/s/s?sort=fsdt+desc,rk+desc&fq=scls:n_*&fq=cc3:" + _Scode;


					          	holderS.push(sml);
				          	}
						}
			          	mid.small = holderS;

						holderM.push(mid);
					}
					map.medium = holderM;

					footer_holder.push(map);
				}
			}

			$footer_nav_category_container = $(".footer_nav_category_ul_01");
			$footer_nav_category_container.html("");
			_.each(footer_holder, function(category, index) {
				$footer_nav_category_container.append(_.template(jQuery("#template-footer-category").html())({ "category": category }));
			});

			/* フッター　アコーディオン */
			$('.footer_nav_category_ul_01 > li > a').click(function(){
				if($(this).hasClass('selected')){
					$(this).removeClass('selected');
					$(this).next('ul').slideUp();
				}else{
					$(this).addClass('selected');
					$(this).next('ul').slideDown();
				}
				return false;
		    });
			$('.footer_nav_category_ul_02 > li > a.existunder').click(function(){
				if($(this).hasClass('selected')){
					$(this).removeClass('selected');
					$(this).next('ul').slideUp();
				}else{
					$(this).addClass('selected');
					$(this).next('ul').slideDown();
				}
				return false;
		    });
		});
	});

</script>

