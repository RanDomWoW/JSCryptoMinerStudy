<script type="text/javascript">
	$(function() {
		// 最近見た商品のコンテナ
		var $top_recently_itemContainer = $('#top_recently_itemContainer');
		// 最近見た商品を取得
		var recentlyViewedItems = ADSS.Promotion.getRecentlyViewedItems();
		// 最近見た商品の保存数
		var myLimit = 21;
		// 最近見た商品を描画
		if (recentlyViewedItems.length > 0) {
			var template = _.template($('#templateTopCheckedItems').html());
			var query = 'wt=json&rows=' + (myLimit - 1) + '&dfq=scls:*&fq=';
			var isFirst = true;
			_.each(recentlyViewedItems, function(solrId) {
					query += ((isFirst ? '' : ' OR ') + 'ID:' + solrId);
					isFirst = false;
			});

			$.ajax({
				url: '/ise/select',
				data: query,
				async: true,
				dataType: "json",
			}).done(function(data, status, xhr) {
				var docs = data.response.docs;
				var numFound = data.response.numFound;
				var count = 0;
				var groupCount = 0;
				
				if ( numFound > 0 ) {
					var docArray = new Array();
					$top_recently_itemContainer.html("");
					_.each(recentlyViewedItems, function(solrId) {
						var doc = _.findWhere(docs, { ID: solrId });
						if (doc && (count < 20) ) {
							groupCount++;
							docArray.push(doc);

							if ( groupCount == 8 ) {
								$top_recently_itemContainer.append(template({ docArray: docArray }));
								docArray = new Array();
								groupCount = 0;
							}
							
							count++;
						}
					});

					if ( groupCount > 0 ) {
						$top_recently_itemContainer.append(template({ docArray: docArray }));
					}

					if ( count > 8 ) {
						$('.section_checkitem .itemslider01').flexslider({
							animation: "slide",
							slideshow: true,
							controlNav:false

						});
					}
				} else {
					$(".section_checkitem").remove();
				}
			});
		} else {
			$(".section_checkitem").remove();
		}
	});
</script>
