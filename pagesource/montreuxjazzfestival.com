<script>
    var isMobile = $( ".show-for-small-only" ).is(':visible');

    setVideoAnim(isMobile);

    $(window).resize(function() {
        if (false === isMobile && true === $( ".show-for-small-only" ).is(':visible')) {
            isMobile = true;
            setVideoAnim(isMobile);
        } else if (true === isMobile && false === $( ".show-for-small-only" ).is(':visible')) {
            isMobile = false;
            setVideoAnim(isMobile, true);
        }
    });

    function setVideoAnim(isMobile, destroySlick) {
        $( "[data-slide]" ).slick({
            arrows: true,
            slidesToShow: 4,
            slidesToScroll: 4,
            responsive: [
            {
              breakpoint: 914,
              settings: {
                slidesToShow: 1,
                slidesToScroll: 1
              }
            }
            ]
        });
        if (false === isMobile) {

            if (true === destroySlick) {
                $( "[data-slider]" ).slick('unslick');

                $( "[data-video-section]" ).hide();
                $( "[data-video-section=0]" ).show();   
            }
            
            $( "[data-video-trigger]" ).click(function(e) {
                e.preventDefault();
                var li = $(this).parent( "li" );
                var index = li.index();

                $(this).addClass('is-active');
                li.siblings().find( "a" ).removeClass('is-active');

                var activeSection = $( "[data-video-section="+index+"]" );

                $( "[data-video-section]" ).hide();
                activeSection.show();
                activeSection.find( ".newsbox-contentwrapper" ).css('height', activeSection.find( ".newsbox-content" ).height() + 'px');
                activeSection.find( ".newsbox-contentwrapper2" ).trigger('update');
            });
        } else {
            $( "[data-video-section]" ).show();
            $( "[data-slider]" ).slick({
                arrows: true
            });
        }
    }
</script>    
