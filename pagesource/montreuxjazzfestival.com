<script type="text/javascript">

    $( window ).load( function() {
        $( ".newsbox-contentwrapper2" ).dotdotdot({
            watch: "window"
        });
    });
    $( window ).resize( function() {
        $( ".newsbox-contentwrapper2" ).dotdotdot({
            watch: true
        });
    });

    $( "a.popup" ).click( function( e ){
        e.preventDefault();
        var width = $(this).data('width');
        var height = $(this).data('height');
        window.open( $( this ).attr( "href" ), "popupWindow", "width="+width+", height="+height+", left=0, top=0, scrollbars=no" );
    });

    $( ".toggle-topbar" ).click( function() {
        var header = $(this).parents( "header" );
        var topbar = $(this).parents( "[data-topbar]" );

        if (topbar.hasClass( "expanded" )) {
            header.css( "position", "fixed" );
        } else {
            header.css( "position", "absolute" );
            $( "html, body" ).animate({ scrollTop: 0 }, 1);
        }
    });

    function setPlusnavPos() {
        var windowWidth = $( window ).width();
        var topbarWidth = $( "[data-topbar]" ).width();

        $( "[data-plusnav] > ul" ).css( "right", (windowWidth - topbarWidth) / 2 * -1 + "px" );
    }

    setPlusnavPos();

    $( window ).resize( function() {
        setPlusnavPos();
    });

    $(document).ready(function() {
        var layers = {
            'objectsTop': {
                'object': $( ".bg2015_objects_top" ),
                'top': parseInt($( ".bg2015_objects_top" ).css('top')),
                'ratio': 0.3
            },
            'splashTop': {
                'object': $( ".bg2015_splash_top" ),
                'top': parseInt($( ".bg2015_splash_top" ).css('top')),
                'ratio': 0.2
            },
            'splashBottomLeft': {
                'object': $( ".bg2015_splash_bottom_left" ),
                'top': parseInt($( ".bg2015_splash_bottom_left" ).css('top')),
                'ratio': 0.5,
            },
            'objectsBottomRight': {
                'object': $( ".bg2015_objects_bottom_right" ),
                'top': parseInt($( ".bg2015_objects_bottom_right" ).css('top')),
                'ratio': 0.2
            },
        }

        $(window).scroll(function() {
            if (0 === $( "[data-coverwrapper]" ).length) {
                $.each(layers, function(i,e) {
                    var top = e.top + e.ratio * $(window).scrollTop();
                    e.object.css('top', top);
                });
            }
        });
    });
</script>

