	var isDownload  = new RegExp("\\.(7z|aac|arc|arj|asf|asx|avi|bin|csv|doc|exe|gif|gz|gzip|hqx|jar|jpe?g|js|mp(2|3|4|e?g)|mov(ie)?|msi|msp|pdf|phps|png|ppt|qtm?|ra(m|r)?|rtf|sea|sit|tar|tgz|torrent|txt|wav|wma|wmv|wpd|xls|xml|z|zip)(\\?mod=ajperes)?$", "i");
	var wcmDownload1 = new RegExp("^\/resources\/([a-zA-Z0-9_])*\/([a-zA-Z0-9_])*\/([a-zA-Z0-9_])*\/", "i");
	var wcmDownload2 = new RegExp("^\/wps\/wcm\/connect\/([a-zA-Z0-9_])*\/", "i");

	// Creating custom :external selector
	$.expr[':'].external = function(obj) {
		return !isInternal.test(obj.href) && !isIrd.test(obj.href) && !isDownload.test(obj.href) && !isMailto.test(obj.href) && !obj.href.match(/^javascript\:/);
	};
	// Creating custom :mailto selector
	$.expr[':'].mailto = function(obj) {
		return isMailto.test(obj.href);
	};
	// Create custom :download selector
	$.expr[':'].download = function(obj) {
		return isDownload.test(obj.href);
	};
	// bind to external clicks
	$("a:external").click(function() {
		trackEvent('external-links', 'click', this.href);
		// add small delay to allow event tracking
		pauseJS(400);
	});
	// bind to mailto clicks
	$("a:mailto").click(function() {
		trackEvent('mailto-links', 'click', this.href.substring(7));
	});
	// bind to download clicks
	$("a:download").click(function() {
		var extension = isDownload.exec(this.href);
		var file = this.href.replace(isInternal, '');
		file = file.replace(wcmDownload1, '');
		file = file.replace(wcmDownload2, '');
		trackEvent('downloads', extension[1], file);
		// add small delay to allow event tracking
		pauseJS(400);
	});
	// bind to colorbox onload
	$(document).bind('cbox_complete', function(event, data) {
		trackEvent('overlay', 'open', document.location.href.replace(isInternal, ''));
	});
	// bind to facebox clicks
	$('.facebox').bind('click', function() {
		var pop_name=$(this).attr("href").substring(1);
		trackEvent('facebox', 'popup', pop_name , '');
	});
	// track external bank link virtual pageviews
	$(".banklink").click(function() {
		_gaq.push(['_trackPageview', '/GA-virtual-pages/make-a-payment/pay-from-overseas-external-links']);
	});
	// track new contact us page channel option
	$('.contact_items ul.clearfix li a').click(function(){
		trackEvent('contact_us','contact_channel', $(this).find('span').text());
	});
});
</script> 

