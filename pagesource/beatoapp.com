	<script type="text/javascript">

		$(".progress-bar").loading();
		$('input').on('click', function () {
			 $(".progress-bar").loading();
		});	
		
		function contactUs() {
			logLSEvent('157', 'Contact Us More Info');
			$("#contactUsBtn_Temp").prop("disabled", true);
			 var title = document.forms["saveForm"]["title"].value;
			 var name = document.forms["saveForm"]["name"].value;
			 var phone = document.forms["saveForm"]["phone"].value;
			
			var url= "/contactRequest";
				$.ajax({
					type : "POST",
					url : url,
					
					accepts : {
						text : "application/json"
					},
					data : {
						title  : title,
						name : name,
						phone : phone
						
					},
					success : function(response) {
						
						 $("#thanku").show();
						 $('#message2').text(response.message);
							
					},
					
					error : function(message) {
						
					}
				});
			
		}   
		
		function apply(){
	var coupon = $('#coupon').val();
    var price=$('#amount').val();  
   
	var urlVar = "/applyCoupon" ;
	
			$.ajax({
				type : "POST",
				url : urlVar,
				
				accepts : {
					text : "application/json"
				},
				data : {
					coupon  : coupon,
					price : price
				},
				success : function(response) {
					$('#three').hide();
					$('#one').hide();
					$('#two').show();
					$('#price').html(response.price);
					$('#discountedprice').val(response.price);
					
					var disc = price - $('#discountedprice').val();
					$('#discount').val(disc);
					if(response.value == 'Invalid coupon') {
						$('#three').show();
					}
				},
				
				error : function(message) {
					alert(message)
				}
			});	
		
	}  
		
	</script>
	
	
