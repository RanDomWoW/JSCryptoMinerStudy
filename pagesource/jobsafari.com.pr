    function g_form_password_insert_values() {
        if (jQuery('.edit_candidate_settings .password_fields').length > 0) {
            jQuery('.edit_candidate_settings .password_fields span input').each(function() {
                jQuery(this).val('random');
            });
        }
    }

    function make_json_on_click_and_select() {
        jQuery(document).on('click', '.category_type li, .job_type li', function() {
            make_json_for_all_types();
        });
        jQuery(document).on('change', '.selectRegion', function() {
            make_json_for_all_types();
        });
    }

    function make_json_for_all_types() {

        if (typeof page_type != 'undefined') {
            var page = page_type[0];
            console.log('pageid = ' + page);
        }

        if (page == '0') { // category page
            category_json();
        }
        else if (page == '1') { // all jobs page
            category_json();
        }
        else if (page == '2') { // search page
            category_json();
        }
        else if (page == '3') { // single job page, single company page
            var posttype = post_type[0];
            if (posttype == 'job') {
                single_job_json();
            }
            else if (posttype == 'company') {
                single_company_json();
            }
            console.log('posttype = ' + posttype);
        }


    }

    function sanitize_title_with_dashes(str)
    {
        str = str.replace(/^\s+|\s+$/g, ''); // trim
        str = str.toLowerCase();

        // remove accents, swap ñ for n, etc
        var from = "àáäâèéëêìíïîòóöôùúüûñç·/_,:;";
        var to = "aaaaeeeeiiiioooouuuunc------";

        for (var i = 0, l = from.length; i < l; i++)
        {
            str = str.replace(new RegExp(from.charAt(i), 'g'), to.charAt(i));
        }

        str = str.replace(/[^a-z0-9 -]/g, '') // remove invalid chars
                .replace(/\s+/g, '-') // collapse whitespace and replace by -
                .replace(/-+/g, '-'); // collapse dashes

        return str;
    }

    function category_json() {
        setTimeout(function() {
            var n_category = [];
            var n_type = [];
            var n_location = [];
            var re = new RegExp('"', 'g');

            jQuery('.category_type .jcf-label-active').each(function() {
                n_category.push(sanitize_title_with_dashes(jQuery(this).text()));
            });
            n_category = JSON.stringify(n_category);            
            n_category = n_category.replace(re, "'");
            console.log(n_category);            
            jQuery('.desktopView #n_category, .mobileHide #n_category').val(n_category);
            jQuery('.mobileView #n_category, .desktopHide #n_category').val(n_category);            

            jQuery('.job_type .jcf-label-active').each(function() {
                n_type.push(sanitize_title_with_dashes(jQuery(this).text()));
            });
            n_type = JSON.stringify(n_type);
            n_type = n_type.replace(re, "'");
            console.log(n_type);
            jQuery('.desktopView #n_type, .mobileHide #n_type').val(n_type);
            jQuery('.mobileView #n_type, .desktopHide #n_type').val(n_type);            

            n_location.push(jQuery('.selectRegion').val());
            n_location = JSON.stringify(n_location);
            n_location = n_location.substring(1, n_location.length - 1);
            n_location = n_location.replace(re, "'");
            console.log(n_location);
            jQuery('.desktopView #n_location').val(n_location);
            jQuery('.mobileView #n_location').val(n_location);
            jQuery('.mobileHide #n_location').val(n_location);
            jQuery('.desktopHide #n_location').val(n_location);
            

        }, 500);
    }

    function single_job_json() {
        setTimeout(function() {

            var re = new RegExp('"', 'g');

            if (job_categories) {
                var job_cat = JSON.stringify(job_categories);
                job_cat = job_cat.replace(re, "'");
                console.log(job_cat);
                jQuery('.desktopHide #n_category').val(job_cat);
                jQuery('.mobileHide #n_category').val(job_cat);
            }
            if (job_types) {
                var job_typ = JSON.stringify(job_types);
                job_typ = job_typ.replace(re, "'");
                console.log(job_typ);
                jQuery('.desktopHide #n_type').val(job_typ);
                jQuery('.mobileHide #n_type').val(job_typ);
            }
            if (job_locations) {
                var job_loc = JSON.stringify(job_locations);
                job_loc = job_loc.replace(re, "'");
                console.log(job_loc);
                jQuery('.desktopHide #n_location').val(job_loc);
                jQuery('.mobileHide #n_location').val(job_loc);
            }
            if(company_name){
                var comp_nam = JSON.stringify(company_name);
                comp_nam = comp_nam.replace(re, "'");
                console.log(comp_nam);
                jQuery('.desktopHide #n_company').val(comp_nam);
                jQuery('.mobileHide #n_company').val(comp_nam);
            }


        }, 500);
    }

    function single_company_json() {
        setTimeout(function() {
            console.log(JSON.stringify(company_name));
            console.log(JSON.stringify(company_country));
        }, 500);
    }

    function get_the_search_parameters_for_search_page() {
        var params = getSearchParameters();
        var re = new RegExp('"', 'g');

        if ((params['f'] && params['f'] != '') && (params['jobLoc'] && params['jobLoc'] != '')) {
            var n_keyword = [];
//            n_keyword['f'] = params['f'];
//            n_keyword['jobLoc'] = params['jobLoc'];
            n_keyword.push(params['f']);
            n_keyword.push(params['jobLoc']);
            n_keyword = JSON.stringify(n_keyword);
            n_keyword = n_keyword.replace(re, "'");
            console.log(n_keyword);            
            jQuery('.desktopHide .newsletter_form input#n_keyword').val(n_keyword);
            jQuery('.mobileHide .newsletter_form input#n_keyword').val(n_keyword);
        }
        else if (params['f'] && params['f'] != '') {
            var n_keyword = [];
//            n_keyword['f'] = params['f'];
            n_keyword.push(params['f']);
            n_keyword = JSON.stringify(n_keyword);
            n_keyword = n_keyword.replace(re, "'");
            console.log(n_keyword);
            jQuery('.desktopHide .newsletter_form input#n_keyword').val(n_keyword);
            jQuery('.mobileHide .newsletter_form input#n_keyword').val(n_keyword);
        }
        else if (params['jobLoc'] && params['jobLoc'] != '') {
            var n_keyword = [];
//            n_keyword['jobLoc'] = params['jobLoc'];
            n_keyword.push(params['jobLoc']);
            n_keyword = JSON.stringify(n_keyword);
            n_keyword = n_keyword.replace(re, "'");
            console.log(n_keyword);
            jQuery('.desktopHide .newsletter_form input#n_keyword').val(n_keyword);
            jQuery('.mobileHide .newsletter_form input#n_keyword').val(n_keyword);
        }
    }

    function getSearchParameters() {
        var prmstr = window.location.search.substr(1);
        return prmstr != null && prmstr != "" ? transformToAssocArray(prmstr) : {};
    }

    function transformToAssocArray(prmstr) {
        var params = {};
        var prmarr = prmstr.split("&");
        for (var i = 0; i < prmarr.length; i++) {
            var tmparr = prmarr[i].split("=");
            params[tmparr[0]] = tmparr[1];
        }
        return params;
    }

