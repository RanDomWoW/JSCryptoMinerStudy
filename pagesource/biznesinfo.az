<script type="text/javascript">
    function exchange(obj)
    {
        val=obj.value;
        simv=val.substr(val.length-1,1);
        if(simv==',')
        {
            val=val.replace(',','.');
            obj.value=val;
            simv='.';
        }
        if(simv=='.')
        {
            val=val.concat('0');
        }
        re=/[^0-9\,\.]/ig
        matches=val.match(re);
        if(matches)
        {
            for(i=0;i<matches.length;i++)
                val=val.replace(matches[i],'');
            obj.value=val;
        }
        for(i=0;i<items.length;i++)
        {
            if(obj.id==items[i]) continue;
            tobj=window.document.getElementById(items[i]);
            tobj.value = (Math.round( val*getCurs(obj.id)/getCurs(items[i]) * 100) / 100).toFixed(2) ;
            //tobj.value = tobj.value.toFixed(2)
            //tobj.value = Math.ceil(tobj.value);
        }
    }
    
    var items=Array('azn','rub','usd','try','eur','aed','gbp');
    function getCurs(id)
    {
        switch (id)
        {
            case 'azn': return 1;
            case 'rub': return 0.0296;
                case 'usd': return 1.7000;
                    case 'try': return 0.4360;
                        case 'eur': return 2.0928;
                            case 'aed': return 0.4628;
                                case 'gbp': return 2.3683;
                            }
                        }
                        $(function(){
                            exchange(document.getElementById('azn'));
                        });

