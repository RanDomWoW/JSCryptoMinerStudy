		    function populateRecommendations(data) {
			    var self = ilRR;
			    var message = self.data.strategyMessage;
			    var placementName = self.data.placement;
			    var items = self.data.items;
			    var elementId;
			    //console.log("populate recs 0: ",self);
			
			    // PLACEMENT STRATEGY MESSAGES AND CONTAINERS
			    var meganavPlacementTitleHTMLContent = '<div class="container"><div class="message"><h3 class="ellipsis">' + message + '</h3></div>';
			    var meganavPlacementHTMLContent = '<div class="meganav-products">';
			    var carouselPlacementTitleHTMLContent = '<h3 class="recommend-header-title ellipsis">' + message + '</h3>';
			    var carouselPlacementHTMLContent = '<div id="recommend-carousel" class="everslider"><ul class="es-slides">';
			    //console.log("populate recs 1: ",self);
			    // GENERATE MEGANAV PLACEMENTS
			    if (placementName.indexOf('jsonmega') > -1) {
			    				
			      elementId = placementName;
			      self.el = document.getElementById(elementId);
			      if (self.el) {
			        for (var i = 0; i < items.length; i++) {
			          var meganavPlacementItem = items[i];
			          var meganavPlacementImageURL = meganavPlacementItem.imageURL;
			          var meganavPlacementLinkURL = meganavPlacementItem.linkURL;
			          var meganavPlacementName = meganavPlacementItem.name;
			          var meganavPlacementPrice = meganavPlacementItem.price; // RRP
			          var meganavPlacementSaleprice = meganavPlacementItem.salePrice; // SALE PRICE
			          var meganavPlacementSavingPrice = meganavPlacementItem.savingPrice; // SAVING PRICE
			
			          // MEGANAV PLACEMENT HTML ELEMENTS
			          var meganavPlacementImageHTML = '<a href="' + meganavPlacementLinkURL + '" title="' + meganavPlacementName + '"><img src="' + meganavPlacementImageURL + '" width="186" height="186" alt="' + meganavPlacementName + '" /></a>';
			          var meganavPlacementTitleHTML = '<div class="main-product-title ellipsis"><a href="' + meganavPlacementLinkURL + '">' + meganavPlacementName + '</a></div>';
			
			          // CHECK IF RRP PRICE IS NOT £0.00 AND ALSO NOT EQUAL TO SALE PRICE
			          var meganavPlacementRrpHTML;
			          if ((meganavPlacementPrice > 0) && (meganavPlacementPrice != meganavPlacementSaleprice)) {
			            meganavPlacementRrpHTML = '<div class="main-product-rrp ellipsis">RRP:&nbsp;<span class="text-line-through">&pound;' + meganavPlacementPrice + '</span><span class="main-product-save">SAVE:&nbsp;&pound;' + meganavPlacementSavingPrice.toFixed(2) + '</span></div>';
			          } else {
			            meganavPlacementRrpHTML = '<div class="main-product-rrp">&nbsp;</div>';
			          }
			
			          // CHECK IF SALE PRICE EXISTS
			          var meganavPlacementPriceHTML;
			          if (meganavPlacementSaleprice !== '') {
			            meganavPlacementPriceHTML = '<div class="main-product-price">&pound;' + meganavPlacementSaleprice + '</div>';
			          } else {
			            meganavPlacementPriceHTML = '<div class="main-product-price">&nbsp;</div>';
			          }
			
			          var meganavPlacementViewHTML = '<div class="main-product-buy"><a href="' + meganavPlacementLinkURL + '">VIEW</a></div>';
			
			          // ADD EACH PLACEMENT TO MEGANAV CONTAINER
			          meganavPlacementHTMLContent += '<div class="main-product-container">' + meganavPlacementImageHTML + meganavPlacementTitleHTML + meganavPlacementRrpHTML + meganavPlacementPriceHTML + meganavPlacementViewHTML + '</div>';
			        }
			
			        // CLOSE MEGANAV CONTAINER
			        meganavPlacementHTMLContent += '</div></div>';
			
			        // APPEND TITLE AND MEGANAV HTML CONTENT TO RECOMMENDATIONS ELEMENTS
			        self.el.innerHTML = meganavPlacementTitleHTMLContent + meganavPlacementHTMLContent;
			      }
			
			    } else {
			    		
			      // GENERATE CAROUSEL PLACEMENTS
			      elementId = placementName.substr(placementName.indexOf('.') + 1);
			      self.el = document.getElementById(elementId);
			      if (self.el) {
				    for (var j = 0; j < items.length; j++) {
			          var carouselPlacementItem = items[j];
			          var carouselPlacementImageURL = carouselPlacementItem.imageURL;
			          var carouselPlacementLinkURL = carouselPlacementItem.linkURL;
			          var carouselPlacementName = carouselPlacementItem.name;
			          var carouselPlacementPrice = carouselPlacementItem.price; // RRP
			          var carouselPlacementSaleprice = carouselPlacementItem.salePrice; // SALE PRICE
			          var carouselPlacementSavingPrice = carouselPlacementItem.savingPrice; // SAVING PRICE
			          var carouselPlacementSavingPercentage = carouselPlacementItem.savingPercentage; // SAVING PERCENTAGE
			
			          // CAROUSEL PLACEMENT HTML ELEMENTS
			          var carouselPlacementImageHTML = '<a href="' + carouselPlacementLinkURL + '" title="' + carouselPlacementName + '"><img src="' + carouselPlacementImageURL + '" width="186" height="186" alt="' + carouselPlacementName + '" /></a>';
			          var carouselPlacementTitleHTML = '<div class="recommend-product-title ellipsis"><a href="' + carouselPlacementLinkURL + '">' + carouselPlacementName + '</a></div>';
			
			          // CHECK IF RRP PRICE IS NOT £0.00 AND ALSO NOT EQUAL TO SALE PRICE
			          var carouselPlacementRrpHTML;
			          if ((carouselPlacementPrice > 0) && (carouselPlacementPrice != carouselPlacementSaleprice)) {
			            carouselPlacementRrpHTML = '<div class="recommend-product-rrp ellipsis">RRP:&nbsp;<span class="text-line-through">&pound;' + carouselPlacementPrice + '</span><span class="recommend-product-save">SAVE:&nbsp;&pound;' + carouselPlacementSavingPrice.toFixed(2) + '<span class="recommend-product-save-percentage">&nbsp;(' + carouselPlacementSavingPercentage + ')</span></span></div>';
			          } else {
			            carouselPlacementRrpHTML = '<div class="recommend-product-rrp">&nbsp;</div>';
			          }
			
			          // CHECK IF SALE PRICE EXISTS
			          var carouselPlacementPriceHTML;
			          if (carouselPlacementSaleprice !== '') {
			            carouselPlacementPriceHTML = '<div class="recommend-product-price">&pound;' + carouselPlacementSaleprice + '</div>';
			          } else {
			            carouselPlacementPriceHTML = '<div class="recommend-product-price">&nbsp;</div>';
			          }
			
			          // ADD EACH PLACEMENT TO CAROUSEL CONTAINER
			          carouselPlacementHTMLContent += '<li>' + carouselPlacementImageHTML + carouselPlacementTitleHTML + carouselPlacementRrpHTML + carouselPlacementPriceHTML + '</li>';
			        }
			
			        // CLOSE CAROUSEL CONTAINER
			        carouselPlacementHTMLContent += '</ul></div>';
			        // APPEND TITLE AND CAROUSEL HTML CONTENT TO RECOMMENDATIONS ELEMENTS
			        self.el.innerHTML = carouselPlacementTitleHTMLContent + carouselPlacementHTMLContent;
					//show as ready
					var carousel = document.getElementById("recommend-carousel");
					if(carousel){
						carousel.className += ' es-slides-ready';
					}
					//delay the plugin till jquery is ready
					if(typeof jQuery !== 'undefined' && typeof jQuery('#recommend-carousel').everslider !== 'undefined'){
						(function ($) {
							//console.log("build ever slider: ",$('#recommend-carousel'));
				    		
				    		$('#recommend-carousel').everslider();
				    	})(jQuery);
					} else {
						//cache the function untill jquery ready
						postJSload.push(function(){		
							(function ($) {
								//console.log("build ever slider: ",$('#recommend-carousel'));
					    		$('#recommend-carousel').everslider();
					    	})(jQuery);
						});
					}
					
					
			       }
			    }
			    
			    //set render hash data
			    recRenderHash[placementName] = true;
			    
		    }


		    //engagements
		    function populateEngagePlacements() {
			    
		    	var self = ilENG;
			    var engagePlacementName = self.data.placement;
			    var engageElementId;
			    console.log("populateEngagePlacements...: ",self); 
			    if (engagePlacementName.indexOf('global_header') > -1) {
			      // GENERATE ENGAGE GLOBAL HEADER BANNER ADVERTS
			      engageElementId = engagePlacementName;
			      self.el = document.getElementById(engageElementId);
			      if (self.el) {
			        var globalHeaderItem = self.data;
			        var globalHeaderAltText = globalHeaderItem.alt_text;
			        var globalHeaderImageURL = globalHeaderItem.imageURL;
			        var globalHeaderMobileImageURL = globalHeaderItem.mobileImageURL;
			        var globalHeaderLinkURL = globalHeaderItem.linkURL;
			
			        // GLOBAL HEADER PLACEMENT HTML ELEMENTS
			        var globalHeaderImageHTML = '<div class="globalheader-desktop"><a href="' + globalHeaderLinkURL + '" title="' + globalHeaderAltText + '"><img src="' + globalHeaderImageURL + '" alt="' + globalHeaderAltText + '" width="987" height="50" /></a></div><div class="globalheader-mobile"><a href="' + globalHeaderLinkURL + '" title="' + globalHeaderAltText + '"><img src="' + globalHeaderMobileImageURL + '" alt="' + globalHeaderAltText + '" width="376" height="50" /></a></div>';
			
			        // APPEND IMAGE
			        self.el.innerHTML = globalHeaderImageHTML;
			      }
			    } else if (engagePlacementName.indexOf('slideshow') > -1) {
			      // GENERATE ENGAGE HOMEPAGE SLIDESHOW
			      engageElementId = engagePlacementName;
			      self.el = document.getElementById(engageElementId);
			      if (self.el) {
			        var slideshowItem = self.data;
			        var slideshowAltText = slideshowItem.alt_text;
			        var slideshowLargeImageURL = slideshowItem.largeImageURL;
			        var slideshowMediumImageURL = slideshowItem.mediumImageURL;
			        var slideshowSmallImageURL = slideshowItem.smallImageURL;
			        var slideshowLinkURL = slideshowItem.linkURL;
			
			        // SLIDESHOW IMAGE PLACEMENT HTML ELEMENTS
			        var slideshowImageHTML = '<a href="' + slideshowLinkURL + '" title="' + slideshowAltText + '"><figure class="responsive" data-media="' + slideshowSmallImageURL + '" data-media320="' + slideshowMediumImageURL + '" data-media530="' + slideshowLargeImageURL + '" title="' + slideshowAltText + '"></figure><!--[if lt IE 9]><img src="' + slideshowLargeImageURL + '" width="732" height="284" alt="' + slideshowAltText + '" /><![endif]--></a>';
			
			        // APPEND IMAGE
			        self.el.innerHTML = slideshowImageHTML;
			
			        // ADD LINK TO SLIDESHOW TAB
			        //document.getElementById('slideshow1-pager1').innerHTML = '<a href="'+ slideshowLinkURL +'">' + slideshowAltText + '</a>';
			
			        // INITIALISE JQUERY PICTURE PLUGIN
			        //delay the plugin till jquery is ready
					if(typeof jQuery !== 'undefined' && typeof jQuery('figure.responsive').picture !== 'undefined'){
						(function ($) {
				          $('figure.responsive').picture();
				        })(jQuery);;
					} else {
						//cache the function untill jquery ready
						postJSload.push(function(){		
							(function ($) {
					          $('figure.responsive').picture();
					        })(jQuery);
						});
					}
					
			        
			
			      }
			    } else if (engagePlacementName.indexOf('brandmsg') > -1) {
			      // GENERATE ENGAGE HOMEPAGE SLIDESHOW BRAND MESSAGE
			      engageElementId = engagePlacementName;
			      self.el = document.getElementById(engageElementId);
			      if (self.el) {
			        var brandMessageItem = self.data;
			        var brandMessageAltText = brandMessageItem.alt_text;
			        var brandMessageImageURL = brandMessageItem.imageURL;
			        var brandMessageLinkURL = brandMessageItem.linkURL;
			
			        // BRAND MESSAGE PLACEMENT HTML ELEMENTS
			        var brandMessageImageHTML = '<a href="' + brandMessageLinkURL + '" title="' + brandMessageAltText + '"><img src="' + brandMessageImageURL + '" width="243" height="98" alt="' + brandMessageAltText + '" /></a>';
			
			        // APPEND IMAGE
			        self.el.innerHTML = brandMessageImageHTML;
			      }
			    } else if (engagePlacementName.indexOf('block') > -1) {
			      // GENERATE ENGAGE HOMEPAGE BLOCK ADVERTS
			      engageElementId = engagePlacementName;
			      self.el = document.getElementById(engageElementId);
			      if (self.el) {
			        var blockItem = self.data;
			        var blockAltText = blockItem.alt_text;
			        var blockImageURL = blockItem.imageURL;
			        var blockLinkURL = blockItem.linkURL;
			
			        // BLOCK ADVERT PLACEMENT HTML ELEMENTS
			        var blockImageHTML = '<a href="' + blockLinkURL + '" title="' + blockAltText + '"><img src="' + blockImageURL + '" width="320" height="260" alt="' + blockAltText + '" /></a>';
			
			        var blockTitleHTML = '<div class="cat-ad-title"><a href="' + blockLinkURL + '">' + blockAltText + '</a></div>';
			
			        // APPEND IMAGE
			        self.el.innerHTML = blockImageHTML + blockTitleHTML;
			      }
			    } else if (engagePlacementName.indexOf('brandmessagingbottom') > -1) {
			      // GENERATE ENGAGE GLOBAL FOOTER BANNER ADVERTS
			      engageElementId = engagePlacementName;
			      self.el = document.getElementById(engageElementId);
			      if (self.el) {
			        var brandMessagingBottomItem = self.data;
			        var brandMessagingBottomAltText = brandMessagingBottomItem.alt_text;
			        var brandMessagingBottomImageURL = brandMessagingBottomItem.imageURL;
			        var brandMessagingBottomLinkURL = brandMessagingBottomItem.linkURL;
			
			        // BANNER ADVERT PLACEMENT HTML ELEMENTS
			        var brandMessagingBottomImageHTML = '<a href="' + brandMessagingBottomLinkURL + '" title="' + brandMessagingBottomAltText + '"><img src="' + brandMessagingBottomImageURL + '" width="987" height="50" alt="' + brandMessagingBottomAltText + '" /></a>';
			
			        // APPEND IMAGE
			        self.el.innerHTML = brandMessagingBottomImageHTML;
			      }
			    }
				
			  	//set render hash data
			    engRenderHash[engagePlacementName] = true;
			    console.log("engRenderHash: ",engRenderHash);
			  }
		     
		</script> 
