        function scrollnews( spazio, tempo ){
            $strip.animate({left: '-='+ spazio}, tempo, "linear", function(){$strip.css("left", 0); scrollnews(totalTravel, defTiming);});
        }
        scrollnews(totalTravel, defTiming);
