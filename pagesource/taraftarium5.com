	<script>
		/* Current Time */
		$(function(){
			$.hour = function(){
				var date = new Date(),
				hour = date.getHours(),
				min = date.getMinutes(),
				sec = date.getSeconds();

				var twoDigitMonth = ((date.getMonth().length+1) === 1)? (date.getMonth()+1) : '0' + (date.getMonth()+1);

				if ( hour < 10 ) hour = "0" + hour;
				if ( min < 10 ) min = "0" + min;
				if ( sec < 10 ) sec = "0" + sec;

				$(".calendar").text( hour + ':' + min + ' / ' + date.getDate() + '.' + twoDigitMonth + '.' + date.getFullYear() );
			}
			setInterval("$.hour()",1000);
		});

		var current_channel;
		$(document).ready(function() {
			/* Chat Panel - List Panel */
			$('#cr-header .cr-chat-panel').click(function(event) {
				event.preventDefault();
				if ( !$(this).hasClass('cr-chat-opened') ) {
					$('#cr-broadcast-list').hide();
					$('#cr-chat').show();
					$(this).html('YAYIN AKIŞINI AÇ');
				} else {
					$('#cr-broadcast-list').show();
					$('#cr-chat').hide();
					$(this).html('<i class="fa fa-comment-o"></i> SOHBETİ AÇ');
				}
				$(this).toggleClass('cr-chat-opened');
			});

			/* Copy Iframe Code */
			$('a.copy-code').click(function(event){
				event.preventDefault();
				$('textarea[name="iframe-code"]').select();
				document.execCommand('copy');
				$('#copied-code').fadeIn();
				setTimeout( function(){
					$('#copied-code').fadeOut();
				}, 2000 );
			});

			/* Refresh Iframe */
			$('a.refresh-iframe').click(function(event){
				event.preventDefault();
				current_channel = $('iframe#cr-broadcast-list').contents().find('#current_channel').val();
				console.log(current_channel);
				$('iframe#macth-video').attr('src', current_channel);
			});

			/* Iframe Kod Kontrol */
			$('iframe#cr-broadcast-list').contents().find('a.crcl-link').click(function(){
				iframe_link = $(this).attr('href');
				console.log(iframe_link);
				iframe_code = '<iframe width="650" height="445" scrolling="no" frameborder="0" src="' + iframe_link + '" allowfullscreen=""></iframe>';
				$('textarea[name="iframe-code"]').html(iframe_code);
			});
