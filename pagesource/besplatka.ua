<script type="text/javascript">
    Vue.component('category-modal-vue', {
        template: '#vue_category_modal_template',
        props: [
            'opened',
            'second_open'
        ],
        data: function(){
            return {
                categoryModalHtml: ''
            };
        },
        methods: {
            openModal: function(need_redirect){

                var category_modal = $('#header-category-modal'),
                    is_modal_empty = category_modal.html().trim() === '';

                if(is_modal_empty){
                    var el_to_copy = $('#main_categories_top').clone();

                    el_to_copy.children('.hidden').removeClass('hidden');

                    el_to_copy.removeAttr('id')
                        .removeClass('no-styles') //case with only top modal
                        .find('#sub_categories_modal')
                        .remove()
                    ;

                    this.categoryModalHtml = $('<div>').append(el_to_copy).html();
                }

                if(typeof need_redirect === 'undefined')
                    $(document).trigger('vue:change-redirect-false');
//                this.open = false;
                this.$emit('update:opened', false);
            }
        },
        watch: {
            opened: function(){
                if(this.opened === true){
                    this.openModal();
                }
            },
            second_open: function(){
                if(this.second_open === true){
                    this.openModal(true);
                }
            }
        }
    });

    $(document).on('category-modal-vue:open', function(){

    });
</script>
