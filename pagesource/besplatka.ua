<script type="text/javascript">

    var only_cities = 0;
    var ajax_params = only_cities ? {only_cities: only_cities} : {};
    /**
     * @this containerSearcherVue
     */
    var initRegionsForModal = function(){
        if(this.regionsInMemory.length === 0){
            var vue = this;

            $.get(BASE_LANG + '/ajax/all-geografy', ajax_params, function(res){
                vue.regionsInMemory = res;
            })
        }
    };
    /**
     * @this containerSearcherVue
     * @param event
     */
    var showCitiesByRegion = function(event){
        if(event) event.preventDefault();

        var _this = $(event.target);

        if(_this.is('a')){
            _this = _this.closest('li');
        }

        var link = _this.children('a'),
            href = link.attr('href'),
            name = link.text().trim(),
            id = link.attr('data-id')
        ;

        var vue = this;

        vue.cities = [];

        var tmp = [];
        _this.find('ul.hidden > li > a').each(function(i, el){
            el = $(el);
            tmp.push({
                href: el.attr('href'),
                name: el.text().trim(),
                id: el.attr('data-id'),
                region_id: el.attr('data-region-id'),
                reg_center: el.attr('data-region-center')
            });
        });
        vue.cities = tmp;

        this.showCityPart = true;
        this.showStaticRegions = false;

        this.selectedRegion = {
            href: href,
            name: name,
            id: id
        };

    };

    Vue.component('region-modal', {
        template: '#region-modal-vue-template',
        props: {
            'opened': {
                'default': false
            },
            'id': {
                'default': 'region_modal'
            }
        },
        data: function(){
            return {
                searchRegionTerm: '',
                showStaticRegions: true,
                showSearchBlock: false,
                showCityPart: false,
                selectedRegion: {
                    href: ''
                },
                selectedGeografy: {
                    href: '',
                    name: '',
                    id: ''
                },
                searchResultRegions: [],
                listSearchClass: '',
                staticRegionClass: 'regions-list list',
                regionsInMemory: [],
                cities: [],
                citiesHtml: '',

                only_cities: only_cities
            };
        },
        methods: {
            initRegions: initRegionsForModal,
            clickRegion: showCitiesByRegion,
            selectRegion: function(e, model){
                if(e) e.preventDefault();
                $(e.target).closest('.modal-content').click();

                var href = model.href || model.url,
                    name = model.name,
                    id = model.id
                ;

                var city_id = model.id, region_id = model.region_id;

                if(typeof region_id === 'undefined'){
                    region_id = city_id;
                    city_id = undefined;
                }

                this.$emit('select', {
                    name: name,
                    url: href,
                    id: id,
                    city_id: city_id,
                    region_id: region_id
                });


                $(document).trigger('vue:select-region', {
                    region_id: region_id,
                    city_id: city_id
                });

                this.closeModal();

                this.$emit('update:opened', false);
            },
            clearModal: function(){
                this.showStaticRegions = true;
                this.showSearchBlock = false;
                this.showCityPart = false;
                this.searchRegionTerm = '';
                this.cities = [];
            },
            closeModal: function(){
                $('#' + this.id).modal('hide');
            },
            clearSearch: function(){
                this.searchRegionTerm = '';
            }
        },
        watch: {
            searchRegionTerm: function (term) {
                term = term.trim();
                var caseShow = term.length > 0;

                this.showStaticRegions = !caseShow;
                this.showSearchBlock = caseShow;

                this.showCityPart = false;

                if (this.regionsInMemory.length === 0) {
                    var vue = this;

                    $.get(BASE_LANG + '/ajax/all-geografy', ajax_params, function (res) {
                        vue.regionsInMemory = res;
                        vue.searchResultRegions = res.filter(function (value) {
                            return value.name.search(new RegExp('^' + term, 'gi')) !== -1
                        });

                    })
                }
                else {
                    this.searchResultRegions = this.regionsInMemory.filter(function (value) {
                        return value.name.search(new RegExp('^' + term, 'gi')) !== -1
                    });
                }
            },
            opened: function(){
                if(this.opened === true){
                    this.initRegions();
                }
                else{
                    this.clearModal();
                }
            },
            cities: function(){
                if(this.cities.length > 0)
                    recalculateCitiesContainerHeight();
            },
            searchResultRegions: function(){
                if(this.searchResultRegions.length > 0)
                    recalculateSearchContainerHeight();
            }
        }
    });

    var getColumnsCount = function(){
        var width = $(window).width();
        if(width > 991){
            return 4;
        }
        else if(width >= 768 && width < 992){
            return 3;
        }
        else{
            return 2;
        }
    };

    var reContainerHeight = function(block){
        var lis = block.find('li');
        var cols_cnt = getColumnsCount();
        console.log('cols_cnt', cols_cnt);
        block.height(lis.eq(0).outerHeight(true) * Math.ceil(lis.length / cols_cnt));
    };

    var recalculateSearchContainerHeight = function(){
        setTimeout(function(){
            reContainerHeight($('#searchRegion'));
        }, 20)
    };

    var recalculateCitiesContainerHeight = function(){
        setTimeout(function(){
            reContainerHeight($('#blockAppendCities'));
        }, 20)
    };

    $(document).on('device-changed', function(){
        recalculateSearchContainerHeight();
        recalculateCitiesContainerHeight();
    })
</script>
