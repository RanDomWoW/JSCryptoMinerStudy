            }



            var id        ='';
            var page_type ='';
            var price     = 0;
            window.onload = function()
            {
                        if(window.location.pathname == '/') //home page
                        {
                                   page_type = "home";
                        }
                        else if(jQuery('.productImage').length != 0) // product page
                        {
                                   id                    = jQuery('span[itemprop="productID"]').text();
                                   page_type       = "product";
                                   price                = parseFloat(jQuery('span[itemprop="price"]').text().replace(',','.'));
                        }
                        else if(jQuery('.flexSearchConfigWide').length != 0)    // Category page
                        {
                                   page_type = "category";
                        }
                        else if(window.location.href.indexOf('index.php?page=cart') !=-1) // Cart page
                        {
                                   len         = jQuery('[name="cartUpdateSuccessContainer"]').length;
								
                                   id          = new Array();
                                   page_type   = "cart";
                                   var pric     = jQuery('.remarketing_komplett').text().replace(',','.');
								   
                                   price = parseFloat(pric);
								 
                                   for(i=0;i<len;i++){
								    if(jQuery('.lastProductItem')[i] !== undefined){
                                     id.push(jQuery('.lastProductItem .productImageSmall')[i].src.split('-kl.jpg')[0].split('products/')[1]);
									}
                                   }
                        }
                        else
                        {
                                   page_type = "category";
                        }
                        setTimeout(function(){remartketingFuction(id,page_type,price);},2000);
            }
