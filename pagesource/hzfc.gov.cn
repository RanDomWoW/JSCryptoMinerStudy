}
	function check_key(){
	var keyword=document.searchs.keyword.value;
	var stArray=new Array("\"","\'","\\","\/");
	var length=stArray.length;
	var falge=true;
	for(var i=0;i<length;i++){
		var test=stArray[i];
		var index=keyword.indexOf(test);
		if(index!=-1){
			falge=false;
			alert("关键字中包含了非法字符，请重试!");
			break;
		}
	}
	return falge;
}

