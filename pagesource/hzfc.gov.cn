function check_form() {
  if (document.form1.user_name.value == "") {
    alert('请输入您的用户名...');
    document.form1.user_name.focus();
    return false;
  }  if (document.form1.pass_wd.value == "") {
    alert('请输入您的密码...');
    document.form1.pass_wd.focus();
    return false;
  }
  document.form1.passwd.value=document.form1.pass_wd.value;
  document.form1.Mname.value=document.form1.user_name.value;
  document.form1.Mpassword.value=document.form1.pass_wd.value;
  
  document.form1.USER_NAME.value=document.form1.user_name.value;
  document.form1.PASSWORD.value=document.form1.pass_wd.value;
  document.form1.rand.value=document.form1.Maddword.value;
  
  return true;
}

function get_check_code(who)
{
	var myid = document.getElementById("imgcheckcode");
	var myid1 = document.getElementById("imgcheckcode1");
	var myid2 = document.getElementById("imgcheckcode2");

	switch(who)
	{
				case "1" : 
			var imgid = document.getElementById("codeimg");
			imgid.src="/public/getcode.php?" + Math.random();
			myid.style.display = "";
			myid1.style.display = "";
			myid2.style.display = "";
			document.form1.action = "/index.php?ty=dl";
			document.form1.subreg.disabled=false;
			break;	
		case "2" :
			myid.style.display = "none";
			myid1.style.display = "none";
			myid2.style.display = "none";
			document.form1.action = "/index.php?ty=dl&go=%2Fwsbs%2Fywbl%2Fwssl-gr.php";
			document.form1.subreg.disabled=false;
			break;
		case "3" :
			var imgid = document.getElementById("codeimg");
			imgid.src="/office/getcode.php?" + Math.random();
			myid.style.display = "";
			myid1.style.display = "";
			myid2.style.display = "";
			document.form1.action = "/office/index.php";
			document.form1.subreg.disabled=true;
			break;
		case "4" :
			var imgid = document.getElementById("codeimg");
			imgid.src="/wsba/getcode.php?" + Math.random();
			myid.style.display = "";
			myid1.style.display = "";
			myid2.style.display = "";
			document.form1.action = "/wsba/index.php";
			document.form1.subreg.disabled=false;
			break;
					case "5" :
			var imgid = document.getElementById("codeimg");
			imgid.src="/public/getcode.php?" + Math.random();
			myid.style.display = "";
			myid1.style.display = "";
			myid2.style.display = "";
			document.form1.action = "/wsbs/ywbl/wyglcj/index.php";
			document.form1.subreg.disabled=true;
			break;
		case "6" :
			var imgid = document.getElementById("codeimg");
			var sRand="";
			for(var i=0;i<4;i++){
				sRand+=Math.floor(Math.random()*10);
			}
			//imgid.src="http://www.tmsf.com/admin/image.jsp?sRand=" + sRand;
  		document.form1.sRand.value=sRand;
			myid.style.display = "";
			myid1.style.display = "";
			myid2.style.display = "";
			document.form1.action = "http://www.tmsf.com/admin/login.jsp";
			document.form1.subreg.disabled=false;

			break;
	}
}
function go_reg_url()
{
	if(document.form1.usertype.value == "1" || document.form1.usertype.value == "2")
	{
		window.document.location="/wsbs/ywbl/wssl_register_next.php?register_kind=50901";
	}
	else if(document.form1.usertype.value == "4")
	{
		window.document.location="http://www.hzfgj.gov.cn/wsba/web_user_regist.php";
	}
	else if(document.form1.usertype.value == "6")
	{
		window.document.location="http://www.tmsf.com/admin/register.html";
	}
}

