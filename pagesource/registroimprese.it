					elem.find(".rowDidascalia").show()
				}else{
					if (esterno){
					    dataLayer.push({'event':'serviziperleimprese','typepage':link});
						window.open(link);
					}else{
					    dataLayer.push({'event':'serviziperleimprese','typepage':link});
						window.location.href = link;
					}
				}
			}
		}
		
		else {
		    if (!elem.hasClass("spanServizioDisabilitato")){
    			if (esterno){
    			    dataLayer.push({'event':'serviziperleimprese','typepage':link});
    				window.open(link);
