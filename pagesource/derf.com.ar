      /* AGREGAMOS EL OVERLAY */

      jq(document).on('click tap', '.nav-overlay', function() {
        jq('#header-navigation').removeClass('active');
        jq(this).removeClass('active');
      });

      /* DESPLEGABLE EN MENU MOBILE */

      jq(document).on('click tap', '.subnav-action', function() {
        jq(this).toggleClass('active');
        jq(this).children().toggleClass('fa-angle-up fa-angle-down');
        var brother = jq(this).next();
        jq(brother).toggleClass('active');
      });

      /* BOTON DE BUSCAR */

      var focus = true;

      jq(document).on('click', '.search-btn', function() {
        if( focus ) {
          jq('#search-form input').addClass('active').focus();
          focus = false;
        } else {
          jq('#search-form input').removeClass('active').focusout();
          focus = true;
        }
        jq(this).toggleClass('active');
        if (used) {
          jq('.sections-collapse ul.nav').animate({height: 'toggle'});
          jq('.sections-collapse').removeClass('active');
          used = false;
        } else {
          return;
        }
      });

      /* DESPLEGAMOS BOTON MENU DESKTOP */

      var used = false;

      jq(document).on('click tap hover', '.sections-collapse', function() {

        //if (jq(window).width() < 992) {
          jq('.sections-collapse ul.nav').animate({height: 'toggle'});
          jq(this).toggleClass('active');
