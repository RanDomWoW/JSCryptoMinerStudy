      });

      /* HACEMOS EL NAV STICKY */

      jq(document).ready(function() {
        var stickyNavTop = jq('#header-navigation').offset().top;
        var stickyMobileBrandTop = jq('#header-branding').offset().top; //mobile

        var stickyNav = function() {
          var scrollTop = jq(window).scrollTop();

          if (scrollTop > stickyNavTop) {
            jq('#header-navigation').addClass('sticky');
            jq('.sticky-effect').addClass('active');
            jq('.logo-w-sticky').addClass('active');
            jq('.banner-side-left').addClass('banner-sticky');
            jq('.banner-side-right').addClass('banner-sticky');
            jq('.nav li.home').css('display', 'none');
            jq('#header-navigation .header-buttons').css('display', 'block');
          } else {
            jq('#header-navigation').removeClass('sticky');
            jq('.sticky-effect').removeClass('active');
            jq('.logo-w-sticky').removeClass('active');
            jq('.banner-side-left').removeClass('banner-sticky');
            jq('.banner-side-right').removeClass('banner-sticky');
            jq('.nav li.home').css('display', 'block');
            jq('#header-navigation .header-buttons').css('display', 'none');
          }
        };

        var stickyMobileBrand = function() {
          var scrollTop = jq(window).scrollTop();

          if (scrollTop > stickyMobileBrandTop) {
              jq('#header-branding').addClass('sticky-mobile');
              jq('#header-navigation').addClass('sticky-mobile-menu');


            } else {
              jq('#header-branding').removeClass('sticky-mobile');
              jq('#header-navigation').removeClass('sticky-mobile-menu');
            }
        };

        stickyNav();
        stickyMobileBrand();  //mobile

        jq(window).scroll(function() {
          if (jq(window).width() < 840) {
            stickyMobileBrand();
          } else {
            stickyNav();
          }
        });
      });

      jq('#textoBuscar').bind("enterKey", function(e) {
        window.location = "/resultado/" + encodeURIComponent(jq('#textoBuscar').val());
