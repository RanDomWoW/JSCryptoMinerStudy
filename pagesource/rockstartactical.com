        $('.MobileTabs .Mtab').removeClass('Active480');
        $('#Content1,#Content2,#Content3').hide();
        
        $(this).addClass('Active480');
        var mmresult=$(this).attr("content");
        $('#'+mmresult).show();
       //console.log(mmresult);
       

    });

});
</script>   
