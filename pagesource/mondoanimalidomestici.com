                                        fullscreen_btn.innerHTML='<i class="hugeicons hugeicons-expand"></i>';
                                        jQuery.when(cFullScreen).done(function(){
                                            setTimeout(function(){
                                                video_width=video.offsetWidth;
                                                timeupdate();
                                                progressHandler();
                                                saveVolume();
                                            },500);
                                        });
