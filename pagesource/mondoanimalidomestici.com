                                        }
                                    },false);
                                    window.addEventListener("mouseup",function(){
                                        if(volume_dragging==1){
                                            volume_dragging="";
                                        }
                                    },false);

                                    /* ONLOAD STUFF */
                                    /* *** */
                                    if(autoplay=="true"){
                                        video.autoplay = true;
                                        addClass("playing",container);
                                        removeClass("poster",container);
                                        thumb_box.style.display="none";
