	});
	
	montasub = "";
	$('#sel_unidades option').each(function(){
		if($(this).attr('class')!='divisa'&&$(this).val()!=''&&$(this).attr('class')!='nomecidade' && ($(this).attr('tipo')=='unidade' || $(this).attr('tipo')=='balcao'  || $(this).attr('tipo')=='faculdade')){
			if($(this).attr('tipo')=='unidade' || $(this).attr('tipo')=='faculdade'){
				montasub = "<div class='menumn2'><a href='unidades.asp?unidade="+$(this).val()+"'>"+$(this).text()+"</a></div>";
			}
			if($(this).attr('tipo')=='balcao'){
				//montasub = "<div class='menumn2'><a href='unidades.asp?unidade="+$(this).val()+"'>"+$(this).text()+"</a></div>";
				montasub = "<div class='menumn2'><a href='balcoes.asp?codigobalcao="+$(this).val()+"&unidade="+$(this).attr("idunidade")+"'>"+$(this).text()+"</a></div>";
			}
			$('.submenuescolas').append(montasub);	
		}
		if($(this).attr('class')=='nomecidade'){
			montasub = "<div class='menumn2 menunomecid'>"+$(this).text()+"</a></div>";
			$('.submenuescolas').append(montasub);	
		}
		/*if($(this).val()=='7' && $(this).attr('tipo')=='unidade'){
			montasub = "<div class='menumn2 menunomecid'>DEMAIS CIDADES</a></div>";
			
			$('.submenuescolas').append(montasub);	
		}*/
	});
	
	$('.ulmenuinst .itemsub').each(function(){
		montasub = "<div class='menumn2'>"+$(this).html()+"</div>";
		$('.submenuintitucional').append(montasub);
	});
	$('.ulmenuserv .itemsub').each(function(){
		montasub = "<div class='menumn2'>"+$(this).html()+"</div>";
		$('.submenuservicos').append(montasub);
	});
	$('.ulmenucontato .itemsub').each(function(){
		montasub = "<div class='menumn2'>"+$(this).html()+"</div>";
		$('.submenucontato').append(montasub);
	});
	$('.menumn1').click(function(){
		$('.menumn1').attr('ativo',0);
		$(this).attr('ativo',1);	
		$('.menumn1').each(function(){
			if($(this).next().css('display')=='block' && $(this).attr('ativo') != 1){
				$(this).next().slideToggle(300);		
			}
		});
		$(this).next().slideToggle(300);
	});
	
	$('.menumn2,.qnivelhome').click(function(){
		reflink = $(this).children('a').attr('href');
		window.location.href = reflink;
	});
	
	$('.lupamobi').click(function(){
		$('.mascarabranca').slideToggle(100);
		$('.buscamobi').slideToggle(100);
		$('.inputbuscamobi').focus();
	});
	$('.mascarabranca').click(function(){
		$('.mascarabranca').hide(100);
		$('.buscamobi').hide(100);
	});
	$('.btbuscarmobi').click(function(){
		$('.inputbusca').val($('.inputbuscamobi').val());
		$('.lupa').click();
	});
	
	$('.inputbuscamobi').keypress(function(e) {
    	if(e.which == 13){ 
			$('.lupa').click();
		}
	});
	
	
	function ajustaMenuM(){
		alturamenumobi = $(window).height() - 60;
		$('.montamenumobi').css({'height':alturamenumobi + 'px'});	
	}
	
	$(window).resize(function(){
		ajustaMenuM();
   	});
	ajustaMenuM();
	
});
</script>

