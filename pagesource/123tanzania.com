<script>
	function subscribe(){
		email=$('#subscribe').val();
		
		if (validateEmail(email) && (email!='')){
		$.get('?module=home&action=subscribe&format=json&email='+email,function(d){
			CC = JSON.parse(d);
			
			if (CC[0].msg) {
				$("#thanksSide").slideDown(1000);
			}
		});
		}
	}
	
	function validateEmail($email) {
	  var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
	  return emailReg.test( $email );
	}
	
	function showLive(){
	$("#liveStream").slideDown();
	}
	</script>
