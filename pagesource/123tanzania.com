<script>
	
	function fillCompanies() {
		var comp = $('#compsearch').val();
		t = $('#pcompid').get(0);
		t.options.length=0;
		t.options[0]=new Option('Searching...','0');
		
		if (comp.length >= 4) {
			$.get("?module=home&action=getCompanies&format=json&name="+comp, null,function(d) {
				CC = eval(d);
				t.options.length=0;
				if (CC.length==0){
				} 
				else {
					for(i=0;i<CC.length;i++) {
						t.options[i]=new Option(CC[i].name,CC[i].id);
					 }
				}
			})
		}
		else t.options[0]=new Option('Please enter  atleast 4 Characters...','');
	}
		
	
	function verifyPremiumRegister() {
		$('#premiumad-submit').html('Saving...');
		
		formdata = $('#premiumad-form').serialize();	
		$.get("?module=home&action=premium_ad&format=json&"+formdata, null,function(d){
		console.log("?module=home&action=premium_ad&format=json&"+formdata);
			CC = eval(d);

			if (CC[0]['error']) {
			
			    var timeout;
			
				if (CC[0]['error'] == 'formfill') {
					$('#premiumad-submit').html(CC[0]['msg']);
					clearTimeout(timeout);
					timeout = setTimeout(function() {
						$("#premiumad-submit").html('Sign Up');
					}, 3000);
				}
				
			}
			else {
				$('#premiumad-submit').html(CC[0]['msg']);
			}
		});
	}
