            replace: function(url, uriEncodedQuery) {
                    a = window.a;
                    t = window.t;
                    return url + '?query=' + uriEncodedQuery + '&t=' + t + '&a=' + a 
                },
                filter: function (suggests) {
                    return $.map(suggests.results, function (suggest) {
                        return {
                            value: suggest.value, 
                            idtype: suggest.idtype,
                            id: suggest.id,
                            type: window.t,
                            action:  window.a
                        };
                    });
                }
            }
        });

        suggests.initialize();

        $('#searchId .typeahead').typeahead(
        {
            minLength: 3,
        }, 
        {
            source: suggests.ttAdapter()
        });

        var numSelectedHandler = function (eventObject, suggestionObject, suggestionDataset) {
            $('#search_key').val(JSON.stringify(suggestionObject, null, 2));
        };

        $('.typeahead').on('typeahead:selected', numSelectedHandler);
    });
    $('.SeeMore').click(function(){
        var $this = $(this);
        $this.toggleClass('SeeMore');
        if($this.hasClass('SeeMore')){
                $this.text('Більше...');			
        } else {
                $this.text('Менше...');
        }
    });
 </script>

