    <script type="text/javascript">
    $j(document).ready(function() {
        var tabletWidth = 1024;
        var timer;
        if ($j(window).width() > tabletWidth) {
            $j(document).on('mouseover click', '.menu-item', function (e) {
                $this = $j(this);
                $j($this).parent().find('li').removeClass('is-selected');
                $j($this).addClass('is-selected');

                if (!$j(this).hasClass('is-open')) {
                    timer = setTimeout(function () {
                        if ($j('.menu-item:hover').length > 0) {
                            menuhover(e);
                        }
                    }, 200/* <--- the delay */);
                } else {
                    menuhover(e);
                }
            });
        }else if($j(window).width() <= tabletWidth){
            $j('#main-menu li a').each(function() {
                $j(this).attr("href", "#");
            });
            $j(document).on('click', '.menu-item', function (e) {

                $this = $j(this);
                $j($this).parent().find('li').removeClass('is-selected');
                $j($this).addClass('is-selected');

                if (!$j(this).hasClass('is-open')) {
                    timer = setTimeout(function () {
                        if ($j('.menu-item:hover').length > 0) {
                            menuhover(e);
                        }
                    }, 200/* <--- the delay */);
                } else {
                    menuhover(e);
                }
            });
        }
        $j(document).on('mouseleave', '.sub-menu-item', function () {
            $j('#main-menu').find('li.menu-item.is-selected').removeClass('is-selected');
        });
        $j(document).on('mouseover', '.header-top-wrapper', function () {
            $j('#main-menu').find('li.menu-item.is-selected').removeClass('is-selected');
            $j('.sub-menu-item,.header-overlay').hide();
            $j('.sub-menu').removeClass('is-open');
        });

        function menuhover(e) {
            var index = $this.index('.menu-item');
            if ($j(window).width() > tabletWidth) {
                $j('.header-overlay').show().css('display', 'fixed');
            } else if ($j(window).width() <= tabletWidth) {
                if (e.type === 'click') {
                    e.preventDefault();
                }
                $j('nav').css('width', '100%');
            }
            $j('.sub-menu').show(0, function () {
                $j(this).addClass('is-open');
            });
            $j('.sub-menu-item').hide();
            $j('.sub-menu-item').eq(index).show();
            var thumb = $j('.sub-menu-item').eq(index).find('.sub-menu-block-item').data('thumb');
            var $thumbItem = $j('.sub-menu-item').eq(index).closest('.sub-menu-item').find('.sub-menu-thumbnail');
            $thumbItem.css({
                'background-image': 'url(' + thumb + ')'
            });
        }


        $j(document).on('mouseleave', 'header', function () {
            if ($j(window).width() > tabletWidth) {
                $j('.sub-menu-item').hide();
                $j('.header-overlay').hide();
            }
        });

        $j(document).on('mouseover', '.header-overlay', function () {
            if ($j(window).width() > tabletWidth) {
                $j('.header-overlay').hide();
            }
        });
        $j(document).on('click', '.sub-menu .sub-menu-back', function () {
            if ($j(window).width() <= tabletWidth) {
                var animationDuration = $j('.sub-menu').css('transition-duration');
                animationDuration = cssTimeToMs(animationDuration);
                setTimeout(function () {
                    $j('.sub-menu').hide();
                }, animationDuration);
            }
        });
        /*
        $j(document).on('click', '.header-overlay-tablet, .sub-menu .icon-cross-light', function () {
            if ($j(window).width() <= tabletWidth) {
                var animationDuration = $j('.sub-menu').css('transition-duration');
                animationDuration = cssTimeToMs(animationDuration);

                $j('.header-overlay-tablet').hide();
                $j('.sub-menu').removeClass('is-open');
                $j('body').removeClass('nav-open');

                $j('nav').css('width', 'auto');

                setTimeout(function () {
                    $j('.sub-menu').hide();
                }, animationDuration);
            }
        });
        */
        $j(document).on('mouseover', '.sub-menu-block-item', function () {
            var thumb = $j(this).data('thumb');
            var $thumbItem = $j(this).closest('.sub-menu-item').find('.sub-menu-thumbnail');

            $thumbItem.css({
                'background-image': 'url(' + thumb + ')'
            });
        });

        $j(document).on('click', '#btn-nav-burger', function () {
            if ($j(window).width() <= tabletWidth) {
                $j('body').toggleClass('nav-open');
                //$j('.header-overlay-tablet').show().css('display', 'fixed');
            }
        });

        $j(window).resize(function () {
            if ($j(window).width() > tabletWidth) {
                $j('.sub-menu').removeClass('is-open');
                $j('.sub-menu').hide();
                $j('body').removeClass('nav-open');
                // $j('.header-overlay,.header-overlay-tablet').hide();
            }
        });
        $j('.catalog-category-view .slick-slider-top-wrapper').css("margin-top",$j('header.sticky').height());
        $j(window).on('resize', function () {
            $j('.catalog-category-view .slick-slider-top-wrapper').css("margin-top",$j('header.sticky').height());
        });

        function cssTimeToMs(time_string) {
            var num = parseFloat(time_string, 10),
                unit = time_string.match(/m?s/),
                milliseconds;

            if (unit) {
                unit = unit[0];
            }

            switch (unit) {
                case "s": // seconds
                    milliseconds = num * 1000;
                    break;
                case "ms": // milliseconds
                    milliseconds = num;
                    break;
                default:
                    milliseconds = 0;
                    break;
            }

            return milliseconds;
        }

    });
</script>

