<script type="text/javascript" defer="defer">
    function throttle(callback, delay) {
        var last;
        var timer;

        return function () {
            var context = this;
            var now = +new Date();
            var args = arguments;

            if (last && now < last + delay) {
                // le délai n'est pas écoulé on reset le timer
                clearTimeout(timer);

                timer = setTimeout(function () {
                    last = now;
                    callback.apply(context, args);
                }, delay);
            } else {
                last = now;
                callback.apply(context, args);
            }
        };
    }

    $j(document).ready(function() {
        $j.expr.filters.offscreen = function(el) {
            var rect = el.getBoundingClientRect();

            return (
                (rect.left + rect.width) < 0
                || (rect.top + rect.height) < 0
                || ((rect.left + rect.width) > window.innerWidth || (rect.top + rect.height) > window.innerHeight)
            );
        };

        // Manage cta in the header
        var ctaTimer;

        $j(document).delegate('.cta', 'mouseenter', function() {
            $j('.hover').removeClass('hover');
        });

        $j(document).delegate('.is-dropdown-menu', 'mouseenter' , function() {
            if (screen.width > 860) {
                var $this = $j(this);
                var $dropdownMenu = $this.find('.deroulant-data');

                clearTimeout(ctaTimer);

                $this.addClass('hover');
                $j('.header-overlay').show().css('display', 'fixed');
                $j('.sub-menu-item').hide();

                var offscreen = $dropdownMenu.is(':offscreen');

                // If bloc is out of the screen, it move to the indoor of the window
                if (typeof $dropdownMenu !== 'undefined' && $dropdownMenu.is(':offscreen')) {
                    var rect = $dropdownMenu.get(0).getBoundingClientRect();
                    var offset = (rect.left + rect.width) - window.innerWidth;
                    var initialMarginValue = parseInt($dropdownMenu.css('margin-left'));
                    var finalValue = initialMarginValue - offset - 20;

                    $dropdownMenu.css('margin-left', finalValue + 'px');

                }

                // Move the arrow of dropdown menu in order to align it with cta
                var $dropdownWrapper = $this.find('.dropdown-wrapper');
                var offsetXDropdownWrapper = $dropdownWrapper.offset().left;
                var offsetXDropdownMenu = $dropdownMenu.offset().left;

                $dropdownMenu.find('.arrow').css({
                    left: (offsetXDropdownWrapper - offsetXDropdownMenu) + 'px'
                });
            }
        });

        $j(document).delegate('.is-dropdown-menu', 'mouseleave', function() {
            var $this = $j(this);

            ctaTimer = setTimeout(function() {
                $this.removeClass('hover');
                $j('.header-overlay').hide();
            }, 500);
        });

        $j("#menu_hamburger").on('click', function() {
            $j("#storenavigationnew").toggle();
        });
    });
