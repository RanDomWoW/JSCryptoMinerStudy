        });

        window.addEventListener("scroll", function () {fixedMenu(); });
        window.addEventListener("resize", function () {fixedMenu(); });
        window.addEventListener("touchmove", function () {fixedMenu(); });
        function fixedMenu() {
            var w, top;
            var topBreackpoint = 100;
            w = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
            top = scrolltop();
            if (w > 991) {
                if (top > topBreackpoint) {
                    $('.container.body-wrapper').css('padding-top', 48);
                    $('.main-menu')
                        .css('position', 'fixed')
                        .css('box-shadow', '0 2px 3px 0 rgba(143, 126, 118, 0.45)')
                        .css('top', 48);
                    $('.hide-on-scroll').hide();
                    $('.top-fixed')
                    //                        .css('background','url(//) no-repeat 50% 50%')
                        .css('background', '#FFF')
                        .css('z-index', '9999')
                        .css('height', '48px');
                    $('.minilogo').fadeIn();
                } else {
                    $('.container.body-wrapper').css('padding-top', 0);
                    $('.main-menu')
                        .css('position', 'relative')
                        .css('top', 0)
                        .css('margin-left','')
                        .css('box-shadow','');
                    $('.top-fixed')
                        .css('background','')
                        .css('z-index', '100')
                        .css('height', '48px');
                    $('.minilogo').hide();
                    $('.hide-on-scroll').show();
                }
            }
        }
        function scrolltop() {
            var top = 0;
            if (typeof(window.pageYOffset) == "number") {
                top = window.pageYOffset;
            } else if (document.body && document.body.scrollTop) {
                top = document.body.scrollTop;
            } else if (document.documentElement && document.documentElement.scrollTop) {
                top = document.documentElement.scrollTop;
            }
            return top;
        }
    })(jQuery);
</script>
