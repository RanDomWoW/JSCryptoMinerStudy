<script type="text/javascript">
    (function($){
        $(document).ready(function(){
            mainSliderManager();
            $('.homeslider').slick({
                autoplay: true,
                arrows: true,
                dots: true,
                fade: false,
                infinite: true,
                prevArrow: '<span class="glyphicon prevArrow glyphicon-chevron-left" aria-hidden="true"></span>',
                nextArrow: '<span class="glyphicon nextArrow glyphicon-chevron-right" aria-hidden="true"></span>'
            });
        });
        function mainSliderManager() {
            var slider = $('.homeslider');
            var ww = $(window).width();
            if(ww < 768) {
                $(slider).removeClass('container');
            } else if (ww >= 768) {
                $(slider).addClass('container');
            }
        }
        $(window).onResizeEnd({
            doThis: function(){
                mainSliderManager();
            },
            wait: 600
        });
    })(jQuery);
</script>
