<!-- EG: Adding albertsons solution to global footer, this script checks for
     j4uIcon and redirects to Coming Soon page and vice versa     -->
<script type="text/javascript">
    // redirect to Coming-Soon page if conditions met
    var redirectJ4UCallBack = function(data) {
        if ( data ){
            if ( typeof data.j4uIcon !== 'undefined' && data.j4uIcon == '1' ) {
                goURL('/ShopStores/Justforu-Coming-Soon.page' + window.location.search);
            }
            else if (window.location.href.indexOf("Offers-Landing-IMG.page") > -1 && loggedOn != '' ) {
                goURL('/ShopStores/Justforu-Coupons.page' + window.location.search);
            }
        }
        else if (window.location.href.indexOf("Offers-Landing-IMG.page") > -1 && loggedOn != '' ) {
            goURL('/ShopStores/Justforu-Coupons.page' + window.location.search);
        }

    }
    var toRedirect = window.location.href.match(/\/ShopStores\/(Justforu-Coupons|Justforu-YourClubSpecials|Offers-Landing-IMG)\.page/);
    if ( toRedirect ) {
        GeoAPI.getStoreData(redirectJ4UCallBack);
    }

    // Revisit this once cookie issue resolved to see if still needed.
    var redirectCSCallBack = function (data) {
        if ( data ){
            if ( !(typeof data.j4uIcon !== 'undefined' && data.j4uIcon == '1') ) {
                goURL('/ShopStores/Justforu-Coupons.page' + window.location.search);
            }
        }

    }

    var isComingSoon = window.location.href.match(/\/ShopStores\/(Justforu-Coming-Soon)\.page/);

    if ( isComingSoon ) {
        GeoAPI.getStoreData(redirectCSCallBack);
    }

    var storeRefreshRedirect = window.location.href.match(/\/ShopStores\/(grocery-rewards|RewardPoints)\.page/);
    if ( storeRefreshRedirect) {
        GeoAPI.syncCurrentStoreData();
    }
    


</script><script language="javascript">
			
