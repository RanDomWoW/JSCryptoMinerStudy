<!-- Do not display Safeway header/footer on the Forgot Password page when banner=albertsons passed as query param -->
<!-- Remove this script when the Forgot Password form has been implemented in native app code -->
<script language="JavaScript" type="text/javascript">
    var tryHidingClickHereLinkCount = 0;

    function hideTheHeaderAndFooter() {
        document.getElementsByClassName('fal-header')[0].style.display = "none";
        document.getElementsByClassName('footer_content')[0].style.display = "none";
    }

    function tryHidingClickHereLink() {

        if (tryHidingClickHereLinkCount < 10) {
            setTimeout(function() {
                var theHtml = $('.error_txt').html();
                if (theHtml && theHtml.indexOf('Click here') != -1) {
                    $('.error_txt').html("Please try again or contact customer care at 1-877-276-9637");
                } else {
                    tryHidingClickHereLink();
                }
                tryHidingClickHereLinkCount++;
            }, 1000);
        }

    }

    // added a condition for  B-21553 to call isQueryParamGrocery() from commun_utilities
    if ((window.location.href.match(/RSS-Forms-UserChangePasswordForm.page/i) || window.location.href.match(/Subscription-Center.page/i)) && (window.location.href.match(/banner=justforu/i)) || isQueryParamGrocery()) {

        if (typeof(Storage) !== "undefined") {
            sessionStorage.banner = 'justforu';
        }

        hideTheHeaderAndFooter();

        // Remove strings "Safeway" and " - " from title
        document.title = document.title.replace(/Safeway/ig, "").replace(/^ - /, "");

        if (window.location.href.match(/Subscription-Center.page/i)) {
            document.getElementsByClassName('t6_pagenav_relatedlink')[0].style.display = "none";
            document.getElementsByClassName('emailOpt-edit-item')[0].style.display = "none";
            var unsubscribeAllDiv = document.getElementById("unsubscribeAllBlock");
            unsubscribeAllDiv.innerHTML = unsubscribeAllDiv.innerHTML.replace(/Safeway/g, "");
        } else {
            //$('.signin_btns').hide();
            $('.rssForm').css('background-color', 'white');
            $('.a720_main_box').css('background-color', 'white');
            $('#ls-row-4').hide();
            $('#t6a_content').css('min-width', '0');
            // $('#ls-gen5-ls-fxr').css('width', '100%'); $('#ls-row-3-area-1').css('width', '100%'); $('#ls-gen6-ls-area-body').css('width', '100%'); $('#t6a_inner_content').css('width', '100%'); $('#t6_third_party_content').css('width', '100%');
            // $('.a720_main_container').css('width', '100%'); $('.a720_main').css('width', '100%'); $('.a720_main_box').css('width', '100%'); $('.cmm_title').css('width', '100%'); $('.userChangePasswordFormTitle').css('width', '100%');
            // $('.inputPanelEmail').css('width', '100%');
            $('.a720_subtitle_box').hide();
            $('.rssFormBox').css('border', '0');
            $('.rss_header_h1').css('font-family', 'Tahoma, Arial, sans-serif');
            $('.cancel_btn').hide();
            $('.pswd_userchange_update_235').css('height', '100%');
            $('.pswd_userchange').css('height', '100%');
            $('a.button_primary_arrow').hide();
            $('.rss_header_h1').css('padding-left', "20px");

            var width100Elements = [
                '#ls-gen5-ls-fxr',
                '#ls-row-3-area-1',
                '#ls-gen6-ls-area-body',
                '#t6a_inner_content',
                '#t6_third_party_content',
                '.a720_main_container',
                '.a720_main',
                '.a720_main_box',
                '.cmm_title',
                '.userChangePasswordFormTitle',
                '.inputPanelEmail',
                '.help_txt2',
                '.help_txt'
            ];

            for (var i = 0; i < width100Elements.length; i++) {
                $(width100Elements[i]).css('width', '100%');
            }

            $('.next_btn').click(function() {
                tryHidingClickHereLinkCount = 0;
                tryHidingClickHereLink();
            });
            $('.help_txt2').html('Please contact customer support at 1-877-276-9637 or try entering your email address again.');

            $(function() {
                if ($("#userId")) {
                    $("#userId").focus();
                }

                //$('.rss_header_h1').html($('.rss_header_h1').html() + ' ' + navigator.userAgent);

                if (/(iPhone|iPod|iPad)/i.test(navigator.userAgent) === false) {
                    $('body').css('zoom', "250%");
                }
            });
        }
    }

    if (window.location.href.match(/RSS-Forms-AccountDisabled.page/i)) {

        if (typeof(Storage) !== "undefined" && sessionStorage.banner && sessionStorage.banner === 'justforu') {
            hideTheHeaderAndFooter();
            $('.a720_main_box').css('background-color', 'white');
            $('.rss_header_h2').hide();
            $('.a720_top_box').css('background-image', 'none');
            $('.regBottomBox').css('background-image', 'none');
            $('.a720_left_box').css('background-image', 'none');
            $('.a720_right_box').css('background-image', 'none');
            $('.help_txt2').html($('.help_txt2').html().replace(/877\-258\-2799/g, "877-276-9637"));

        }
