    <script type="text/javascript">
      $(document).ready(function () {
        window.moment.tz.setDefault('Europe/Ljubljana');

        $.validator.setDefaults({
            errorPlacement: function(err, el) {
              return true;
            }
          });

        $(document).ready(function () {
          if (!isCookieSet()) {
            $("#scc-consent").show()
          }
        })

        window.c = Cookies

        $(".scc-btn-confirm").on('click', function (e) {
          e.preventDefault()
          setCookie()
          $("#scc-consent").hide()
        })

        $(".scc-btn-decline").on('click', function (e) {
          e.preventDefault()
          rmCookie()
          $(".scc-text").hide()
          $(".scc-controls").hide()
          $(".scc-text.scc-declined").show()
        })

        // Prijava kršitve modal
        $("[href='#prijava-krsitve']").attr('rel', 'modal:open');

        // Sidebar ajax
        $(".change-sidebar-links").on('click', function (e) {
          e.preventDefault();
          var clickedDom = $(this);
          var url = clickedDom.attr('ajax-url');
          var appendClass = clickedDom.attr('class-active');

          $(".change-sidebar-links").each(function (item) {
            if(this !== clickedDom[0]) {

              $(this).removeClass("side-menu__navigation--business-active")
              $(this).removeClass("side-menu__navigation--personal-active")
            } else {
              $(this).addClass(appendClass);
            }
          });

          $.ajax({
            url: url
          }).done(function (response) {
            var contentDom = $("#sidebar-content");
            contentDom.fadeOut(200, function() {
              $(this).html(response);
              $(this).fadeIn(200);
            })
          })
        });
      })
    </script>
