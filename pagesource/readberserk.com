            img.before('<div><span class="tag is-success zoom__image" button data-zoom="+">+ Larger Image</span></div>');
        }
    }).each(function() {
        if(this.complete || $(this).height() > 0) {
            $(this).load();
        }
    });
    $('.img_container').on('click', '.zoom__image', function() {
        var img = $(this).parent().next();
        var zoom = $(this);
        if(zoom.attr('data-zoom') == '+') {
            zoom.html('- Smaller Image')
            zoom.attr('data-zoom', '-');
            var size = 1200;
            var originalSize = img.attr('data-width');
            img.attr('width',  originalSize + 'px');
            img.css('max-width', 'none');
        } else {
            zoom.html('+ Larger Image')
            zoom.attr('data-zoom', '+');
            img.attr('width', 'none');
            img.css('max-width', '100%');
        }
    });

    if( window.canRunAds === undefined ) {
        $(".pages__ad").each(function(div) {
            $(this).html('<img src="https://i.imgur.com/XWnLr5m.jpg"/>');
        });
    }

</script>
