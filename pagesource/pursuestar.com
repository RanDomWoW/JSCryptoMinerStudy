<script type="text/javascript">
  $(function(){
    var items = $('.video-tabnav ul li');
    var wp = $('.video-tabnav ul');
    var conitems = $('.video-tabcon>div');
    $('.prev').on('click',function(){
      var n = $('.video-tabnav ul li.active').index();
      n--;
      if(n < -1) {
        n = items.length+1;
      }
      var nextitem = items.eq(n);
      nextitem.addClass('active').siblings(items).removeClass('active');
      conitems.addClass('active').eq(n).siblings().removeClass('active');
      nextitem.velocity("scroll", { 
        container:wp,
        axis: "x",
        offset: 0,
        duration: 500
      });
    })

    $('.next').on('click',function(){
      var n = $('.video-tabnav ul li.active').index();
      n++;
      if(n>items.length-1) {
        n = 0;
      }
      var nextitem = items.eq(n);
      nextitem.addClass('active').siblings().removeClass('active');
      conitems.addClass('active').eq(n).siblings().removeClass('active');
      nextitem.velocity("scroll", { 
        container:wp,
        axis: "x",
        offset: 0,
        duration: 500
      });
    })
  })
</script>
