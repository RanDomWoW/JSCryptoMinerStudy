             var $hideshow = $(this);
             var $placeholder = $hideshow.attr("title");
             if ($hideshow.val() === "" || $hideshow.val() === $placeholder) {
                 $hideshow.addClass("placeholder").css("color", "#666666");
                 $hideshow.val($placeholder);
              }
         };

         // hide function
         function hidePlaceholder() {
             var $hideshow = $(this);
             if ($hideshow.val() === $hideshow.attr("title")) {
                 $hideshow.removeClass("placeholder").css("color", "#666666");
                 $hideshow.val("");
             }
         }
    });

    $('#tabs form').submit(function(e) {
        form = $(e.target);

        form.find('input[type=text][title],textarea[title]').each(function(i, el) {
