			$(this).toggleClass('active').next('div').toggle().toggleClass('active');
			$(this).children('i').toggleClass('fa-chevron-down');
		});

		$('.info-menu a').click(function(e){
			e.preventDefault();

			var tab = $(this).data('tab');

			$('.mobile-tab-head.active').children('i').toggleClass('fa-chevron-down');

			$('.info-menu a.active, .mobile-tab-head.active').removeClass('active');
			$(this).addClass('active');
			$('.mobile-tab-'+tab).addClass('active');

			$('.mobile-tab-head.active').children('i').toggleClass('fa-chevron-down');

			$('.tab.active').removeClass('active');
			$('.tab-'+tab).addClass('active');
		});

					$('.timeline-end').click(function(){
				$('.timeline-end').hide();
				$('.hidden-timeline').show();
			});

			$('.timeline-box').mouseover(function(){
				$(this).children('.timeline-year').css('background-color', '#214e8b').children().css('color', '#f1f1f2');
			});

			$('.timeline-box').mouseleave(function(){
				$(this).children('.timeline-year').css('background-color', '#f1f1f2').children().css('color', '#214e8b');;
			});
		
			</script>
</body>
</html>