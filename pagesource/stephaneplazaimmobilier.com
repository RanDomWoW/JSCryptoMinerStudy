    <script type="text/javascript">
    jQuery(function($) {

        $('.civilite-choice').on('click', function(e) {
            $element = $(this);
            $value = $element.data('value');
            $('#civilite-value').val($value);
        });

        $('.btn-submit-create-account').on('click', function(e) {
            e.preventDefault();
            e.stopPropagation();

            // if(grecaptcha.getResponse() != ''){$val_recaptcha = "1";}
            // else{$val_recaptcha = "";}
            if ($('#confirm_label').is(':checked') && $('[data-field="confirm_label"]').val() == '') {
                $val_recaptcha = "1";
            }
            else{
                $val_recaptcha = "";
            }

            $('<input />').attr('type', 'hidden')
                .attr('name', "recaptcha")
                .attr('value', $val_recaptcha)
                .appendTo('#create_account');

            if ($('input[name="newsletter"]:checked').length <= 0) {
                $('<input />').attr('type', 'hidden')
                    .attr('name', "newsletter")
                    .attr('value', "")
                    .attr('id', "temp_check")
                    .appendTo('#create_account');
            }

            $element = $(this);
            $form = $('#create_account');
            $method = $element.data('method');

            $.ajax({
                url: $form.attr('action'),
                method: $form.attr('method'),
                dataType: 'json',
                data: {
                    type: 'create_account',
                    method: $method,
                    data: $form.serialize()
                },
                beforeSend: function() {
                    $element.addClass('btn-loader');
                },
                success: function(data) {
                    $element.removeClass('btn-loader');
                    clearErrors($form);
                    if (!data.error) {
                        $('#modal-account').modal('hide');
                        //$('#redirect_success').submit();
                        modal_thx();
                                                    $('#account .dropdown-toggle').remove();
                            $('#account').append('<a title="Mon compte" href="account.php" class="">Mon compte</a>');
                                            }
                    else {
                        $.each(data.errors, function(index, value) {
                            message = '';
                            if(index == 'captcha') {
                                $element_form = $("#recaptcha");
                            }
                            else {
                                if(value['field'] == 'email_address' && value['exists'] == true){
                                    message = 'Un compte a déjà été créé avec cette adresse Email';
                                }
                                $element_form = $form.find('[name="' + index + '"]');
                            }
                            addError($element_form, message);
                        });
                    }
                }
            });

            $( "input[name$='recaptcha']").remove();
            $( "#temp_check").remove();
        });

        function clearErrors(element) {
            element.find('.help-text-error').remove();
            if (element.is('select')) {
                element.find('.bootstrap-select').removeClass('error');
            }
            element.find('.error').removeClass('error');
        }

        function addError(element, message) {
            $parent = element.parent();

            element.addClass('error');
            $parent.find('.bootstrap-select').addClass('error');

            if (!$parent.find('.help-text-error').text()) {
                $parent.append('<span class="help-text-error">' + ((message) ? message : element.data('message-error')) + '</span>');
                // element.after('<span class="help-text-error">' + ((message) ? message : element.data('message-error')) + '</span>');
            }
        }
    });

</script>
