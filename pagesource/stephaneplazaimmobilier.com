<script type="text/javascript">
    jQuery(function($) {

        $('.js-radio-project').on('click', function() {
            $element = $(this);
            $group = $element.data('group');
            $target = $element.data('target');
            $value = $element.data('value');

            if ($target == '#project-value') {
                if ($value == 'none' || $element.hasClass('active')) {
                    $('#campagne-agence').hide();
                    $('#campagne-manufacturers_id').selectpicker('val', '');
                    $('#campagne-manufacturers_id').prop('disabled', true).selectpicker('refresh');
                }
                else {
                    $('#campagne-agence').show();
                    $('#campagne-manufacturers_id').prop('disabled', false).selectpicker('refresh');
                }
            }

            if ($element.hasClass('active')) {
                $value = '';
                $element.addClass('btn-gray').removeClass('btn-magenta active');
            }
            else {
                $($group).find('.js-radio-project').each(function() {
                    $(this).removeClass('btn-magenta active').addClass('btn-gray');
                });
                $element.removeClass('btn-gray').addClass('btn-magenta active');
            }

            $($target).val($value);
        });

        $('#modal-pub').on('click', '#display_logger', function() {
            $.ajax({
                method: 'GET',
                url: $('#form-campagne-pub-part1').attr('action'),
                dataType: 'html',
                data: {
                    ajax: true,
                    session: $('input[name="session"]').val(),
                    callback: 'init_campagne',
                    logger: true,
                    mail: $('#modal-pub').find('#customers_email_address').val()
                },
                beforeSend: function() {
                    $('#modal-loader').show();
                },
                success: function(data) {
                    $('#modal-loader').hide();
                    $('#modal-pub .modal-body').empty().html(data);
                }
            });
        });

                    var force = false;
        
                    /** Pattern pour la date de naissance */
            $('#date_naissance').mask("00-00-0000", {
                placeholder: "JJ-MM-AAAA"
            });
        
                    /** Spinner */
            $('.js-ui-spinner').spinner({
                max: 24,
                min: 0
            });
        
        // Fermer la modal
        $('#modal-pub').on('hide.bs.modal', function () {
            var close = true;
            $('#form-campagne-pub-part1').find('.check-entry').each(function() {
                if ($(this).val() != '') {
                    close = false;
                }
            });

            if (!close) {
                if (confirm('Vous allez perdre les données renseignées, voulez-vous continuer ?')) {
                    return true;
                }

                return false;
            }

            return true;
        });

        // Submit form part 1
        $('#modal-pub').on('submit', '#form-campagne-pub-part1', function(e) {
            e.preventDefault();

            $button = $('#validation');
            $button.attr('disabled', 'disabled').html('<i class="fa fa-circle-o-notch fa-spin"></i>');
            $.ajax({
                method: 'POST',
                url: $(this).attr('action'),
                dataType: 'json',
                data: {
                    ajax: true,
                    session: $('input[name="session"]').val(),
                    callback: 'init_campagne',
                    form: $(this).attr('id'),
                    data: $(this).serialize()
                }
            }).done(function(data) {
                $customers_localisation = $('#customers_localisation').val();
                if (!data.status) {
                    if (data.html)
                        $('#message').html('<div class="alert alert-danger">'+data.html+'</div>');
                    else
                        $('#message').html('<div class="alert alert-danger">An error occured</div>');
                    scrollToErrors();
                }
                else {
                    if (typeof dataLayer != 'undefined') {
                        dataLayer.push({
                            'typeform': 'campagne-pub'
                        });
                    }
                    $.ajax({
                        method: 'GET',
                        url: $(this).attr('action'),
                        dataType: 'html',
                        data: {
                            ajax: true,
                            step: 'step2',
                            cid: data.cid,
                            callback: 'init_campagne'
                        }
                    }).done(function(data) {
                        // Changement du bouton account.php
                        $('#account > a').removeClass('dropdown-toggle').removeAttr('data-toggle').attr('href', 'account.php');
                        $('#account-mobile > a').attr('href', 'account.php');

                        $('#modal-container').empty().html(data);
                        setTimeout(function() {
                            $('#customers_address_cp').val($customers_localisation);
                            $('.selectpicker').selectpicker();
                                                            /** Tooltip */
                                $('#date_naissance').tooltip();
                                                                                        /** Spinner */
                                $('.js-ui-spinner').spinner({
                                    max: 24,
                                    min: 0
                                });
                                                    }, 300);

                        $("#modal-pub").animate({
                            scrollTop: 0
                        }, 1250);
                    });
                }
                $button.attr('disabled', false).html('Valider');
            });
        });

        // Submit form part 2
        $('#modal-pub').on('submit', '#form-campagne-pub-part2', function(e) {
            e.preventDefault();

            $button = $('#validation');
            $button.attr('disabled', 'disabled').html('<i class="fa fa-circle-o-notch fa-spin"></i>');

            $.ajax({
                method: 'POST',
                url: $(this).attr('action'),
                dataType: 'json',
                data: {
                    ajax: true,
                    session: $('input[name="session"]').val(),
                    step: 'part2',
                    callback: 'init_campagne',
                    form: $(this).attr('id'),
                    data: $(this).serialize()
                }
            }).done(function(data) {
                if (data.status) {
                    $('#message').empty();
                    $('#message').html('<div class="alert alert-success">Vos informations ont bien été enregistrées</div>');
                    $('#form-campagne-pub-part2').remove();
                }
                else {
                    if (data.html)
                        $('#message').html('<div class="alert alert-danger">'+data.html+'</div>');
                    else
                        $('#message').html('<div class="alert alert-danger">An error occured</div>');
                    scrollToErrors();
                }
                $button.attr('disabled', false).html('Valider');
            });
        });

        $('.cp-civilite-choice').on('click', function(e) {
            e.preventDefault();

            $element = $(this);
            $value = $element.data('value');
            $target = $($element.data('target'));

            $('.cp-civilite-choice').removeClass('active');
            $target.addClass('active');

            $('#cp-civilite-value').val($value);
        });

        $('#modal-pub').on('hide.bs.modal', function (e) {
            var src = $('#modal-video-presentation').find('iframe').attr('src');
            $('#modal-video-presentation').find('iframe').attr('src', '');
            $('#modal-video-presentation').find('iframe').attr('src', src);
        });

        // Affichage d'une fois tout les 45 jours
        // à l'aide d'un cookie
        var display = true;
        if ((display && !getCookie('campagne_pub')) || force) {
            setTimeout(function() {
                if (env == 'production') {
					setCookiePub();
                }
                $('#modal-pub').modal('show');
            }, 1500);
        }

        $('#modal-pub').on('submit', '[name="logger"]', function(e) {
            e.preventDefault();

            $button = $(this).find('button');
            $button.prop('disabled', true).html('<i class="fa fa-circle-o-notch fa-spin"></i>');
            agent.call('', 'ajax_light_login', 'ajax_light_login_return', $(this).find('#email_address').val(), $(this).find('#password').val());
        });

    });

    function ajax_light_login_return(str) {
        if (str == 'ok') {
            window.location.href = '/catalog/account.php';
        }
        else {
            $('[name="logger"]').find('button').prop('disabled', false).html('Connexion');
            $('[name="logger"]').prev().html(str);
        }
    }

    function setCookiePub() {
        var cookieTimeout = 3888000000; // Le nombre de millisecondes qui font 45 jours
        var date = new Date();
        date.setTime(date.getTime() + cookieTimeout);
        document.cookie = "campagne_pub=true;expires=" + date.toGMTString();
    }

    function scrollToErrors() {
        $("#modal-pub").animate({
            scrollTop: ($("#modal-pub").find('.modal-header').outerHeight() + $("#modal-pub").find('.part1-intro').outerHeight())
        }, 750);
    }
</script>
