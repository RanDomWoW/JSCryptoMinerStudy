<script type="text/javascript">
  $(function () {
    $('.z-signup-form').each(function (index, item) {
      new SignupForm('/account/signup/', $(item))
    });

    $(".z-restore-form").each(function (index, item) {
      new RestoreForm('/account/restore/', $(item));
    })
  });

  var loginForm;
  $(function () {
    var $loginForm = $('.z-login-form'),
      $selectPhoneForm = $('.z-login-form-select-number'),
      $enterCodeForm = $('.z-login-form-send-code'),
      $title = $('.form-title'),
      $subtitle = $('.form-subtitle'),
      loginForm,selectPhoneForm,enterCodeForm;
    function onSelectPhone(data) {
      $loginForm.hide();
      selectPhoneForm = new TFA_SelectPhoneForm('/account/2f-phone/', $selectPhoneForm, data.phones);
      selectPhoneForm.whenEnterCode(onEnterCode);
      selectPhoneForm.whenFail(onFail);
      $selectPhoneForm.show();
      $title.text("2-Step Verification");
      $subtitle.text("Select one of your phones");
    }
    function onEnterCode(data) {
      $loginForm.hide();
      $selectPhoneForm.hide();
      $enterCodeForm.show();
      $title.text("Confirmation");
      $subtitle.text(data.phone);
    }
    function onFail(data) {
      $loginForm.show();
      $selectPhoneForm.hide();
      $enterCodeForm.hide();
      loginForm.showServerError(data.responseJSON.error.message);
      loginForm.reset();
      enterCodeForm.reset();
      $title.text("Log in");
      $subtitle.text("Please enter your email and password");
    }
    $title.text("Log in");
    $subtitle.text("Please enter your email and password");
    loginForm = new TFA_LoginForm('/account/session/', $loginForm);
    loginForm.whenSelectPhone(onSelectPhone);
    loginForm.whenEnterCode(onEnterCode);
    loginForm.whenFail(onFail);
    enterCodeForm = new TFA_EnterCodeForm('/account/2f-confirm/', $enterCodeForm);
    enterCodeForm.whenFail(onFail);
  });

  $(window).on('load', function () {
    (function () {
      var widgetItems = '.whr-items';
      var $jobTab = $('.js-team-tab');
      var $jobShowBlock = $('.js-show-block');
      var $jobContent = $jobShowBlock.find(widgetItems);
      var $jobContentItem = $jobContent.children();
      var defaultShowBlockContent = 'all';

      var showTabContent = function (name) {
        $jobShowBlock.find(widgetItems).hide();
        $jobShowBlock.find(widgetItems + '[data-tab-id="' + name + '"]').show();
      };

      var createAllItemsFakeTag = function () {
        if ($(widgetItems).length) {
          $('<ul></ul>').addClass('whr-items all-items')
                        .prependTo($jobShowBlock);
          $jobContentItem.clone().appendTo('.all-items');
        }
      };

      var addDataIdAttr = function () {
        $jobShowBlock.find(widgetItems).each(function (index) {
          if (index === 0) {
            $(this).attr('data-tab-id', defaultShowBlockContent);
          } else {
            $(this).attr('data-tab-id', $(this).prev().text().toLowerCase());
          }
        });
      };

      var showPlaceholder = function () {
          var $placeholder = $('.js-workable-empty');

          if ($(widgetItems).is(':visible')) {
            $placeholder.hide();
          } else {
            $placeholder.show();
          }
      };

      var handleActiveTab = function () {
        $jobTab.on('click', function () {
          var $this = $(this);
          var currentTabName = $this.data('tab');

          $jobTab.removeClass('is-active');
          $this.addClass('is-active');

          showTabContent(currentTabName);
          showPlaceholder();
        });
      };

      createAllItemsFakeTag();
      addDataIdAttr();
      showTabContent(defaultShowBlockContent);
      handleActiveTab();
      showPlaceholder();
    })();
  });

  // TOS tabs
  (function showBlock() {
    var section = document.documentElement.querySelector('.js-privacy');

    var handler = function (context, elements) {
      var section = elements.section,
          blocks = elements.blocks,
          tabs = elements.tabs,
          block,
          tab = context,
          data = tab.getAttribute('data-val');

      Array.prototype.forEach.call(blocks, function (el) {
        if (el.getAttribute('data-val') == data) {
          block = el;
        }
      });

      if (!tab.classList.contains('is-active')) {
        section.querySelector('.js-tab.is-active').classList.remove('is-active');
        section.querySelector('.js-block.is-active').classList.remove('is-active');
        tab.classList.add('is-active');
        block.classList.add('is-active');
      }
    };

    var init = function (section) {

      var blocks = section.querySelectorAll('.js-block'),
          tabs = section.querySelectorAll('.js-tab'),
          elements = {
            section: section,
            blocks: blocks,
            tabs: tabs
          };

      for (var i = 0; i < tabs.length; i++) {
        tabs[i].addEventListener(
          'click',
          function(event) {
            handler(this, elements);
          }
        );
      }
    };

    section !== null && init(section);

  })();

