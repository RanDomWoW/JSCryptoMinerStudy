    var renderCallToAction = function (id, title, btnTitle, secStart, secEnd, video, inputs=false) {
        var popoverId = '#' + id + '_popover';
        var mainVideoContent = $(popoverId);
        var popupCloseButton = popoverId + '_popover_close_button, ' + popoverId + '_overlay';
