<script type="text/javascript">

	var timevar = 7000; //таймер смены слайдов
	var goslide = true; //разрешение на смену слайдов
$(document).ready(function() {

	/*
	setInterval(function(){
  	$(".slider .nav span").trigger("click");
	}, 5000);
*/
 $(".slider").each(function () { // обрабатываем каждый слайдер
  var obj = $(this);
  $(obj).append("<div class='nav'></div>");
  $(obj).find(".item-li").each(function () {
   $(obj).find(".nav").append("<span class='item-span' rel='"+$(this).index()+"'></span>"); // добавляем блок навигации
   $(this).addClass("slider"+$(this).index());
  });
  $(obj).find("span.item-span").first().addClass("on"); // делаем активным первый элемент меню
 });

});
function sliderJS (obj, sl) { // slider function
 var ul = $(sl).find("ul"); // находим блок
 var bl = $(sl).find("li.slider"+obj); // находим любой из элементов блока
 var step = $(bl).width(); // ширина объекта
 $(ul).animate({marginLeft: "-"+step*obj}, 800); // 1000 это скорость перемотки
}
	//остановка прокрутки слайдера при наведении мышки
	$(".slider").mouseover(function(){
	goslide = false;
	});
	$(".slider").mouseout(function(){    
	goslide = true;    
	});


window.setInterval(function(){
	var n = $(".slider .nav span.on").next(); 
	if(n.length==0)n=$(".slider .nav span").first(); 
	goslide && n.click();},
	timevar);
$(document).on("click", ".slider .nav span", function() { // slider click navigate
 var sl = $(this).closest(".slider"); // находим, в каком блоке был клик
 $(sl).find("span.item-span").removeClass("on"); // убираем активный элемент
 $(this).addClass("on"); // делаем активным текущий
 var obj = $(this).attr("rel"); // узнаем его номер
	sliderJS(obj, sl); // слайдим
	//setInterval(sliderJS(obj, sl),10000);//пролистывание по интервалу
 return false;
});
</script>



