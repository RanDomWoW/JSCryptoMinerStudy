                                                        console.log("Error in marking as read");
                                                    }
                                                }
                                            });

                                        });
                                    });
                                    $.ajax({
                                        type: 'GET',
                                        url: "/bin/calix/servlets/notification?action=count",
                                        success: function (response) {
                                            if (response) {
                                                console.log("new unread count: " + response);
                                                updateCount(response);
                                            }
                                        },
                                        error: function () {
                                            console.log("Error getting counts");
                                        }
                                    });
                                    hideWaitingBlock();
                                },
                                error: function () {
                                    hideWaitingBlock();
                                    console.log("Error with Mark all read");


                                }
                            });
                        });

                        $('span.navbar-note [data-toggle="popover"]').first().attr('data-isLoad', 'true');
                    },
                    error: function (x, t, m) {
                        hideWaitingBlock();
                    }
                });
            }
        });

        function generateNotificationList(data) {
            data = jQuery.parseJSON(data);
            var notificationHtmlList = [];
            var listHtmlGenerator = function (item, buttonText) {
                var htmlOutput = "";
                //item.subject = item.subject.replace(/[^\x00-\x7F]/g, "");

                htmlOutput = '<div class="listItem clearfix"><div class="icoNote">';
                if (!item.isRead) {
                    htmlOutput = htmlOutput + '<i class="fa fa-circle"></i>';
                    htmlOutput = htmlOutput + '</div><div class="noteContent" style="font-weight:600" data-isLoad="false" data-otherInfo="' + item.otherInfo + '"  data-read="' + item.isRead + '" data-id="' + item.recId + '" data-type="' + item.notificationType + '" data-info="' + item.otherInfo + '">';
                } else {
                    htmlOutput = htmlOutput + '</div><div class="noteContent" style="font-weight:400" data-isLoad="false" data-otherInfo="' + item.otherInfo + '"  data-read="' + item.isRead + '" data-id="' + item.recId + '" data-type="' + item.notificationType + '" data-info="' + item.otherInfo + '">';
                }
                htmlOutput = htmlOutput + '<span>' + item.subject + '</span><span class="createDate">' + item.createdDate + '</span></div><div class="viewClose hideCtrl">';
                htmlOutput = htmlOutput + '<a class="btn txtBtn" style="display: block !important" data-read="' + item.isRead + '" data-id="' + item.recId + '" data-type="' + item.notificationType + '" data-info="' + item.otherInfo + '" href="' + (item.description || "#") + '">' + buttonText + '</a>';
                htmlOutput = htmlOutput + '<a class="closeNote" data-read="' + item.isRead + '" data-id="' + item.recId + '"><i class="fa fa-trash-o"></i></a></div></div>';
                return htmlOutput;

            }
            if (data.records !== null) {


                $.each(data.records, function (i, item) {
                    if (i < 10) {
                        notificationHtmlList.push(listHtmlGenerator(item, getNotificationLabel(item.notificationType)));
                    }
                });
            }
            $('.notificationsMenu .noteList').html(notificationHtmlList.join(""));

            bindDeleteEvent();
        }

        //bind delete item event
        $('span.navbar-note [data-toggle="popover"]').on('click', function () {
            // bindDeleteEvent();
        });
        function bindDeleteEvent() {
            $('.viewClose a.closeNote').each(function (index) {
                $(this).bind("click", function () {
                    var id = $(this).get(0).getAttribute('data-id');
                    var isRead = $(this).get(0).getAttribute('data-read');
                    var data = 'action=delete&recId=' + id;
                    deleteButtons = $('.viewClose a.closeNote[data-id=' + id + ']');
                    // call delete
                    displayWaitingBlock('notificationsMenu');
                    ajaxPost(data, notificationServiceUrl, 'deleteSuccessCallBack', 'notificationsMenu');
                });
            });
        }

        //bind view detail item event
        $('span.navbar-note [data-toggle="popover"]').on('click', function () {
            bindViewDetailEvent();
        });

        function bindViewDetailEvent() {
            $('.viewClose a.btn').each(function () {
                $(this).bind('click', function () {
                    var id = $(this).get(0).getAttribute('data-id');
                    var isRead = $(this).get(0).getAttribute('data-read');
                    var data = 'action=read&recId=' + id;
                    viewDetailCTAs = $('.viewClose a.btn[data-id=' + id + ']');
                });
            });
        }

        //bind get notification detail event
        $('span.navbar-note [data-toggle="popover"]').on('click', function () {
            bindGetNotificationDetailEvent();
        });
        function bindGetNotificationDetailEvent() {
            $('.noteList .noteContent').each(function (index) {
                $(this).bind('click', function () {
                    var id = $(this).get(0).getAttribute('data-id');
                    var isLoad = $(this).get(0).getAttribute('data-isLoad');
                    var data = 'action=getDetail&recId=' + id
                    viewDetailCTAs = $('.viewClose a.btn[data-id=' + id + ']');
                    noteContentNodes = $('.noteList .noteContent[data-id=' + id + ']');
                    if (isLoad == 'false') {
                        //call mark as read
                        $(viewDetailCTAs).each(function () {
                            $(this).hide();
                        });
                    }
                });
            });
        }
    });


