<script type="text/javascript">
    function doDesktopSearch() {
        console.log("Desktop search triggered");
        var searchPageUrl = $("#searchResultsHeader").html();
        var searchKeyword = "";
        if ($("input#headerSearch").val() == $("input#headerSearch").attr('placeholder')) {
            console.log("placeholder and input value EQUAL! Enter a real search term");
            console.log("saerch keyword " + searchKeyword);
            return false;
        } else {
            console.log("placeholder unequal");
            searchKeyword = $("input#headerSearch").val();
            var fullUrl = (searchPageUrl.trim() + "?searchKeyword=" + searchKeyword);
            window.location = fullUrl;
        }
    }

    function doMobileSearch() {
        console.log("Mobile search triggered");
        var searchPageUrl = $("#searchResultsHeader").html();
        console.log("SEarch url - " + searchPageUrl);
        var searchKeyword = "";
        if ($("input#mobileSearchInput").val() == $("input#mobileSearchInput").attr('placeholder')) {
            console.log("placeholder and input value EQUAL! Enter a real search term");
            console.log("search keyword " + searchKeyword);
            return false;
        } else {
            console.log("placeholder unequal");
            searchKeyword = $("input#mobileSearchInput").val();
            var fullUrl = (searchPageUrl.trim() + "?searchKeyword=" + searchKeyword);
            window.location = fullUrl;
        }
    }

    $(document).ready(function () {

        $('#msearchboxholder i').click(function () {
            doMobileSearch();
            event.preventDefault();
            return false;
        });

        $('input#headerSearch').on('keypress', function (event) {
            if (event.keyCode == 13) {
                doDesktopSearch();
                event.preventDefault();
                return false;
            }
        });

        $('input#mobileSearchInput').on('keypress', function (event) {
            if (event.keyCode == 13) {
                doMobileSearch();
                event.preventDefault();
                return false;
            }
        });

    });
</script>
