<script>

    function convertTimeZone() {
        var dateFormatted = [];
        var dtval = [];

        $('.noteList .listItem div.noteContent .createDate').each(function (i) {
            dateFormatted.push($(this).html());
            dtval.push(formatDate(new Date(dateFormatted[i]).toString()));
            $(this).html(dtval[i]);
        });

    }

    function showDateInClientSideFormat(dateValue) {
        // in notification flag component
        var modifyDate = dateValue.replace('Z', '');
        var d = new Date();
        var n = d.getTimezoneOffset();
        var dateClientSide = new Date(modifyDate + n);
        var dateClient = formatDate(dateClientSide);
        return dateClient;
    }

    function convertUTCDateToLocalDate(date) {
        date = new Date(date);
        var newDate = new Date(date.getTime() + date.getTimezoneOffset() * 60 * 1000);

        var offset = date.getTimezoneOffset() / 60;
        var hours = date.getHours();

        newDate.setHours(hours - offset);

        return newDate;
    }

    function formatDate(dateClientSide) {
        var formattedDate = new Date(dateClientSide);
        var hours = formattedDate.getHours();
        var minutes = formattedDate.getMinutes();
        var ampm = hours >= 12 ? 'PM' : 'AM';
        hours = hours % 12;
        hours = hours ? hours : 12;
        minutes = minutes < 10 ? '0' + minutes : minutes;
        var strTime = hours + ':' + minutes + ' ' + ampm;
        var d = formattedDate.getDate();
        var m = formattedDate.getMonth();
        m += 1;  // JavaScript months are 0-11
        var y = formattedDate.getFullYear();
        var dateFormats = m + "/" + d + "/" + y + " " + 'at' + " " + strTime;
        return dateFormats;
    }

    var viewDetailCTAs;
    var deleteButtons;
    var noteContentNodes;

    function updateCount(response) {
        if (response > 0) {
            $(".notification .messageCount span").html(response);
        } else {
            $(".notification .messageCount").hide();
        }
    }

    function viewDetailSucessCallBack(data) {
        if (data == 'true') {
            $(viewDetailCTAs).each(function () {
                $(this).attr('data-read', 'true');//view detail button
                $(this).next().attr('data-read', 'true');//delete button
                $(this).parents('.listItem').first().find('.icoNote i.fa-circle').first().remove();
            });
            decreaseNumberOfNotification();
        }
    }

    function deleteSuccessCallBack(data) {
        if (data == 'true') {
            var isRead;
            $(deleteButtons).each(function () {
                isRead = $(this).get(0).getAttribute('data-read');
                $(this).parents('.listItem').first().remove();
            });

        }
    }

    /**
     *    Returns notification label based on notification type
     */
    function getNotificationLabel(notificationType) {
        var buttonText;
        switch (notificationType) {
            case "Pardot":
                buttonText = 'VIEW MESSAGE';
                break;
            case "MonthlyUpdates":
                buttonText = 'VIEW UPDATE';
                break;
            case "Generic":
                buttonText = 'VIEW';
                break;    
            case "ServiceStation":
                buttonText = 'VIEW CASE UPDATE';
                break;
            case "DocRecommended":
            case "FavoriteDocUpdated":
            case "RecommendedDocRead":
                buttonText = 'VIEW DOCUMENT';
                break;
            case "E-Commerce" :
            	buttonText = 'VIEW UPDATE';
                break;
            default:
                /**
                 *    If new any new notification type is added which is not listed in above switch case statements
                 *    that notification type will be added as text.
                 *    If notification type is undefined or null we are returing empty string
                 */
                buttonText = notificationType || "";
        }

        return buttonText;
    }
    function getNotificationDetailSucessCallBack(data) {
        if (data != '') {
            data = jQuery.parseJSON(data);
            $(viewDetailCTAs).each(function () {
                var buttonText, _this = $(this);
                _this.attr('data-read', 'true');//view detail button
                _this.next().attr('data-read', 'true');//delete button
                _this.parents('.listItem').first().find('.icoNote i.fa-circle').first().remove();

                // If description is not avalable as a place holder we are placing "#" (hash symbol)
                _this.attr('href', data.description || "#")
                      //  .attr('target', '_blank')
                        .text(getNotificationLabel(data.notificationType))
                        .show();
            });
            var isRead;
            var id;
            $(noteContentNodes).each(function () {
                isRead = $(this).get(0).getAttribute('data-read');
                id = $(this).get(0).getAttribute('data-id');

                $(this).attr('data-read', 'true');
                $(this).attr('data-isLoad', 'true');
                if (data.notificationType == 'pardot') {
                    $(this).html($(this).html() + '<span>' + data.description + '</span>');
                }
            });
        }
    }

    function decreaseNumberOfNotification() {
        var number = parseInt($('.navbar-note .notification .messageCount span').text()) - 1;
        if (number <= 0) {
            $('.navbar-note .notification .messageCount').remove();
        } else {
            $('.navbar-note .notification .messageCount span').text(number);
        }
