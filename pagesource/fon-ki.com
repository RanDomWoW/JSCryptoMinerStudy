 apohider = 0; 
 $('#puzadpn').attr('style', 'height:24px;'); 
 $(this).attr('style', 'margin-top:24px;'); 
 $('.cuzadpn').attr('style', 'margin-top:0px;z-index:' + _uWnd.getTopZ()); 
 eraseCookie('hideapoadmin'); 
