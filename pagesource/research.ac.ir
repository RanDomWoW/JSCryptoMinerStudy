	<script>

		$(function () {

			onResized();
			$('map').imageMapResize();

			setTip();
			activateNavScroll();

			ARSHEET.ajaxStat('WebsiteStat', 60000, '3');
		});

		$(window).resize(function(event) {

			onResized();
		});

		function activateNavScroll() {

			$('ul.nav li a').on('click', function () {

				ARSHEET.scrollToElement($($(this).attr('href')));
			});
		}

		function onResized() {

			$('.home').height($(window).height());
			ARSHEET.onWindowResized();
		}

		var index = 0;
		var tips = null;
		var timer = null;

		function setTip() {

			if (!tips || !tips.length) {

				$('.home-content').hide();
				return;
			}

			$('.tip-button').hide();
			$('#tip-handle').slideUp(500);

			if (index < 0) {

				index += tips.length;
			}

			var tip = tips[index % tips.length];

			var titleClass = IsFarsi(tip.title) ? 'text' : 'zarEn';
			var descriptionClass = IsFarsi(tip.description) ? 'zar' : 'zarEn';

			setTimeout(function () {

				var style = tip.url ? 'cursor: pointer;' : '';
				var click = tip.url ? 'onclick="window.open(\'' + tip.url + '\')"' : '';
				var html = '<div id="tip-handle" style="display: none;' + style + '" ' + click + '>' +
					'<h1 class="' + titleClass + '">' + tip.title + '</h1>' +
					'<p class="' + descriptionClass + '">' + tip.description + '</p>' +
					'</div>';
				$('#tip').html(html);

				$('#tip-handle').show(500);
				$('.tip-button').fadeIn(500);

			}, 500);

			timer = setTimeout(function () {

				index++;
				setTip();
			}, 50000);
		}

		function tipNext() {

			clearTimeout(timer);
			index++;
			setTip();
		}

		function tipPrev() {

			clearTimeout(timer);
			index--;
			setTip();
		}

	</script>
