	<script type="text/javascript">
		// 상세보기
		function goDiscuss(BTIdx, BIdx) {
			if (validLoginGo() == true)
				location.href = StringFormat("/Board/BoardDiscussView.aspx?BTIdx={0}&BIdx={1}&SearchType={2}&SearchText={3}&PageNo={4}", BTIdx, BIdx, 0, '', 1);
		}
		
		// 팝업 닫기
		function closePopup(vBannerIdx, vBlindDay){
			// 체크박스 확인
			var vChecked = $("#chkNotView"+vBannerIdx).prop("checked");
			if(vChecked == true){
				$.cookie("layerPopup"+vBannerIdx, "hidden", {path: "/", domain: document.domain, expires: vBlindDay });
			}
			$("div#divPopup" + vBannerIdx).hide();
		}

		$(document).ready(function(){

			// 팝업 정렬
			var vTotalWith = 0;
			var vMaxHeight = 0;
			var vMinWidth = 200;
			$("div.ld_popup_wrap").each(function(){
				// 쿠키값 확인
				var vBannerIndex = $(this).data("banner-index");
				if($.cookie("layerPopup"+vBannerIndex) != "hidden"){
					var vItemWidth = $(this).data("width");
					var vItemHeight = $(this).data("height");
					vTotalWith = vTotalWith + (vItemWidth < vMinWidth ? vMinWidth : vItemWidth);
					vMaxHeight = (vMaxHeight < vItemHeight ? vItemHeight : vMaxHeight);
				}
			});

			var vLeftMargin;
			var vTopMargin;
			if($("div.ld_popup_wrap").eq(0).data("align") == "Center"){
				vLeftMargin = $(window).width() / 2 - vTotalWith / 2;
				vTopMargin = $(window).height() / 2 - vMaxHeight / 2;
			}else{
				vLeftMargin = 100;
				vTopMargin = 100;
			}
			
			var vLeft = 0;
			$("div.ld_popup_wrap").each(function(index, item){
				// 쿠키값 확인
				vBannerIndex = $(item).data("banner-index");
				if($.cookie("layerPopup" + vBannerIndex) != "hidden"){
					var vItemWidth = $(item).data("width");
					$(item).css("margin-left", vLeftMargin + "px");
					$(item).css("left", vLeft + "px");
					$(item).css("top", vTopMargin + "px");
					vLeft = vLeft + (vItemWidth < vMinWidth ? vMinWidth : vItemWidth);
				}
			});

			// 팝업 보이기
			$("div.ld_popup_wrap").each(function(){
				vBannerIndex = $(this).data("banner-index");
				if($.cookie("layerPopup" + vBannerIndex) != "hidden"){
					$("#divPopup" + vBannerIndex).show();
				}
			});

			if(1 > 0){
				$("ul.premium div").slick({
					arrows: false
					, autoplay: true
				}).show();
			}

			if(1 > 0){
				$("div.quick_banner2").show();
			}

			if(1 > 0){
				$("div.quick_banner").show();
			}
		});
	</script>
