<Script>
/* 메인 롤링 */
var phroll1 = 0;
var phroll2 = 0;
var phwait = 5000;	/* 5초 */
var phmove = true;	/* 시작 */
var phmap = 0;		/* position 설정 */
var phwidth = 273;	/* 이미지 사이즈 */
var phsize = 0;		/* 해당 롤에 이미지 수*/

$(function(){
	phsize = $("img[class^=photo_i]").size();
	phsizeR = phsize + 1;

	
	for(var n=1; n < phsizeR; n++){
		$('.photo_i'+n).css('left',phmap);
		phmap = phmap + phwidth;
	}

	if (phsize > 1){	window.setTimeout("phrolling()",phwait);}

});


function phAction(){
	phroll1 = 0;
	for(var i=1; i <= phsize; i++){
		if(i==1){
			
		phroll2 = phwidth * (phsize - 1);
		$('.photo_i1').animate({'left':(phwidth*-1)},1000); 
		$('.photo_i1').animate({'left':phroll2},0);

		}else{
			phroll2 = phroll1 * phwidth;
			phroll1 = phroll1 + 1;
			$('.photo_i'+i).animate({'left':phroll2},1000);
		}
	}

	for(var j=1; j <= phsizeR; j++){
		if(j == 1){
			$('.photo_i1').attr('class','photo_i'+phsizeR);
		}else{
			k = j - 1;
			$('.photo_i'+j).attr('class','photo_i'+k);
		}
	}
}
function photoA(){


 for(var j=1; j <= 4; j++){
		if(j == 1){
			$('.photo_j1').attr('class','photo_j4');
		}else{
			k = j - 1;
			$('.photo_j'+j).attr('class','photo_j'+k);
		}
	}
}

function phrolling(){
 if(phmove == true){ phAction();  photoA();} window.setTimeout("phrolling()",phwait);
}

</script>

