<script type="text/javascript">
$(document).ready(function() {
	
	if (typeof $.cookie('youviewe_count') !== "undefined") {
		jQuery('#col_viewed').text('Вы смотрели (' + $.cookie('youviewe_count') + ')');
	}
	else {
		jQuery('#col_viewed').text('Вы смотрели (0)');
	}
	
	$('.main-category li').mouseenter(function() {
		var category_id = $(this).data('categoryId');
		if (typeof category_id === "undefined") {
			return false;
		}
		if ($('.horisontal-menu-cat.category-id-'+category_id).length){
			$(".overlay-menu").show();
			$('.horisontal-menu-cat.category-id-'+category_id).show();
			$('.main-category li[data-category-id="'+category_id+'"] a').addClass('active');
		}
		else {
			var category_width = $(this).data('width');
			var category_left = $(this).data('left');
			
			if (typeof category_width === "undefined") {
				var category_width = 345;
			}
			if (typeof category_left === "undefined") {
				var category_left = '0';
			}
			
			var html = $('.popover.category_id_'+category_id).html();
			if (html !== null) {
				$('<div style="display:none;" class="horisontal-menu-cat category-id-'+category_id+'">'+html+'</div>').appendTo(this);
				$('.horisontal-menu-cat.category-id-'+category_id).width(category_width);
				$('.horisontal-menu-cat.category-id-'+category_id).css('left', category_left+'px');

				$(".overlay-menu").show();
				$('.horisontal-menu-cat.category-id-'+category_id).show();
				$('.main-category li[data-category-id="'+category_id+'"] a').addClass('active');
			}
		}
		
	});
	
	$('.main-category li').mouseleave(function(){
		var category_id = $(this).data('categoryId');
		if (typeof category_id === "undefined") {
			return false;
		}
		$('.horisontal-menu-cat.category-id-'+category_id).hide();
		$(".overlay-menu").hide();
		$('.main-category li[data-category-id="'+category_id+'"] a').removeClass('active');
		
		
	});


});

if (typeof getURLVar != 'function') {
	var getURLVar = function(urlVarName) {
		var urlHalves = String(document.location).toLowerCase().split('?');
		var urlVarValue = '';
		
		if (urlHalves[1]) {
			var urlVars = urlHalves[1].split('&');
	
			for (var i = 0; i <= (urlVars.length); i++) {
				if (urlVars[i]) {
					var urlVarPair = urlVars[i].split('=');
					
					if (urlVarPair[0] && urlVarPair[0] == urlVarName.toLowerCase()) {
						urlVarValue = urlVarPair[1];
					}
				}
			}
		}
		
		return urlVarValue;
	} 
}

function pm_update_cart(key, quantity, type) {
	$.ajax({
		url: "index.php?route=module/cart",
		type: "post",
		data: {"key":key,"quantity":quantity},
		success: function(data) {
			var cart = $(data);
			if (getURLVar('route') == 'checkout/cart' || getURLVar('route') == 'checkout/checkout') {
				location.reload();
				return;
			}
			
			$('#cart').html(cart.html());
				
			if ($('#cart-box-list').length) {
				var cart_info = $('.mini-cart-info', cart);
				if (cart_info.length) {
					$('#cart-box-list').html(cart_info.html());
				} else {
					$('#cart-box-list').html($('.content', cart).html());
				}
			}
			if ($('#cart-box-total').length) {
				$('#cart-box-total').html($('.mini-cart-total', cart).html());
			}
			if(typeof type !== "undefined") {
				if (type == 'popupcart') {
					pm_update_cart_popup();
				}
			}
			
			if (typeof load_simplecheckout === 'function') {
				load_simplecheckout();
			}
		}
	});

}

function pm_update_cart_popup() {
		 $.ajax({
			url: 'index.php?route=module/popupcart/load',
			success: function(data) {
				var cart = $(data);

				if (getURLVar('route') == 'checkout/cart' || getURLVar('route') == 'checkout/checkout') {
				   location.reload();
					return;
				}

				$('#cart-popup').html(cart.html());
			}
		});
}
</script>

