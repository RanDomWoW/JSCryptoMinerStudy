  xhttp.onreadystatechange = function () {
    if (xhttp.readyState == 4 && xhttp.status == 200) {

      var store = JSON.parse(xhttp.responseText);

      var codCountries = {};

      for (var i in store.zones) {
        if (store.zones[i].name === "UPS With COD") {
          store.zones[i].countryCodes.forEach(function (countryCode) {
            codCountries[countryCode] = codMax[countryCode] ? codMax[countryCode] : 4500;
