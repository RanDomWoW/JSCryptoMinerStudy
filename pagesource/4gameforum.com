<script>
(function () {

	// fix IE SVG bug
	if (typeof SVGElement.prototype.blur == 'undefined') {
		SVGElement.prototype.blur = function(){};
	}

	var sliderHidden;

	if (getCookie("sliderNavigationHidden") == 'true') {
		toggleNav(false);
		sliderHidden = true;
		$("#navigationToggle").one('click', function() {
			loadSwiper();
		});
	} else {
		loadSwiper();
		toggleNav(true);
		sliderHidden = false;
	}
	
	$("#navigationToggle").on('click', function () {
		var $navigation = $('#navigation');
		var $wrapper = $('#swiper-container').find('> .swiper-wrapper');
		var $extraTabs = $('.extraTabs');
		var height = $wrapper.show(0).outerHeight();
		
		if (sliderHidden) {
			// show
			$navigation.addClass('animationStart');
			$extraTabs.show(0).css('opacity', 0);
			$extraTabs.animate({opacity: 1}, 500, function() {
				$extraTabs.removeAttr('style');
			});
			$wrapper.css('margin-top', -height);
			$wrapper.animate( {marginTop: 0}, 500, function() {
				callback(); 
				$navigation.removeClass('animationStart');
				$wrapper.css('display', '');
			});
		} else {
			// hide
			$navigation.addClass('animationStart');
			$extraTabs.css('opacity', 1);
			$extraTabs.animate({opacity: 0}, 500, function() {
				$extraTabs.removeAttr('style');
			});
			$wrapper.css('margin-top', 0);		
			$wrapper.animate({marginTop: -height}, 500, function() {
				callback(); 
				$navigation.removeClass('animationStart'); 
				$wrapper.css('display', '');
			});
		}
		
		function callback() {
			toggleNav(sliderHidden);
			setCookie("sliderNavigationHidden", sliderHidden, 365);
		}
	});

	var headerSwiper;
	
	$(window).on('resize', function() {
		loadSwiper();
	});

	function loadSwiper() {
		var screenWidth = window.innerWidth;
		
		if ( headerSwiper === undefined && screenWidth > 900 ) {
			headerSwiper = new Swiper ('#swiper-container', {
				simulateTouch: false,
				setWrapperSize: true
			});
			
			$('.extraNavTab').on('click', function(){
				headerSwiper.slideTo($(this).index());
			});
			headerSwiper.on('slideChangeStart', function () {
				$('.extraNavTab.current').removeClass('current');
				$('.extraNavTab').eq(headerSwiper.activeIndex).addClass('current');
			});
			
			if(/MSIE \d|Trident.*rv:/.test(navigator.userAgent)) {
				$(document).on('DOMContentLoaded', function() {
					var evt = window.document.createEvent('UIEvents'); 
					evt.initUIEvent('resize', true, false, window, 0); 
					window.dispatchEvent(evt);
				});
			}
			
		} else if ( headerSwiper !== undefined && screenWidth < 900 ) {
			headerSwiper.destroy(true, true);
			headerSwiper = undefined;
			$('.extraNavTab').off('click');
		}
	}
	
	function setCookie(name, value, exdays) {
		var date = new Date;
		date.setDate(date.getDate() + exdays);
		document.cookie = name + "=" + value + "; path=/; expires=" + date.toUTCString() + ";";
	}
	
	function getCookie(name) {
		var matches = document.cookie.match(new RegExp("(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"));
		return matches ? decodeURIComponent(matches[1]) : undefined;
	}
	
	function toggleNav(state) {
	
		var $navigation = $("#navigation");
	
		if (!state) {
			$navigation.addClass("hidden");
			sliderHidden = true;
		} else {
			$navigation.removeClass("hidden");
			sliderHidden = false;
		}
	}
})();
</script>

