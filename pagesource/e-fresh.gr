                    if(maxOuterWidth > elm_outerWidth){
                        left = (screen_width - elm_outerWidth)/2;
                    } else {
                        left = (screen_width - maxOuterWidth)/2;
                    }
                    if(elm == '#le-ajaxlogin-forgotpass-box'){
