        var left = parseInt(screenX + ((outerWidth - width) / 2), 10);
        var top = parseInt(screenY + ((outerHeight - height) / 2.5), 10);
        var settings = (
            'width=' + width +
                ',height=' + height +
                ',left=' + left +
                ',top=' + top
            );
        var newwindow = window.open(url, '', settings);
        if (window.focus) {
            newwindow.focus()
        }
        return false;
