				var replace_n = new Array('α','Α','β','Β','γ', 'Γ', 'δ','Δ','ε','έ','Ε','Έ','ζ','Ζ','η','Η','θ','Θ','ι','Ι','κ','Κ','λ','Λ','μ','Μ','ν','Ν','χ','Χ','ο','O','π','Π','ρ','Ρ','σ','ς', 'Σ','τ','Τ','υ','Υ','φ','Φ','χ','Χ','ψ','Ψ','ω','Ω','','','ς','ς','ξ','Ξ','ψ','Ψ','θ','Θ');
				for(var i = 0; i < replace.length; i++) {
					string = string.replaceAll(replace[i], replace_n[i]);
				}
				return string;
			}
										
			(function($){
