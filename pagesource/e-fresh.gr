<script type="text/javascript">
	
	var ReferralsWidget = (function(){

		var init = function(){
			var cookie_referralsWidgetState = readCookie("referralsWidgetState");
			if(cookie_referralsWidgetState == "minimized"){
				jQuery(".referrals-minimized").removeClass("hidden");
				jQuery(".referrals-maximized").addClass("hidden");
			}else if(cookie_referralsWidgetState == "maximized"){
				jQuery(".referrals-minimized").addClass("hidden");
				jQuery(".referrals-maximized").removeClass("hidden");
			}
		}
		var showTabDescription = function(tabClass){
			jQuery(".options li").removeClass("active");
			jQuery(".options li."+tabClass).addClass("active");

			jQuery(".options-description li").removeClass("active");
			jQuery(".options-description li."+tabClass).addClass("active");
		}
		var toggleState = function(){
			jQuery(".referrals-minimized").toggleClass("hidden");
			jQuery(".referrals-maximized").toggleClass("hidden");

			if(!jQuery(".referrals-minimized").hasClass("hidden")){
				createCookie("referralsWidgetState","minimized");
			}else if(!jQuery(".referrals-maximized").hasClass("hidden")){
				createCookie("referralsWidgetState","maximized");
			}
		}
		var gotoLogin = function(){
			createCookie("referralsWidgetState","minimized");
			window.location.href = 'https://www.e-fresh.gr/el/customer/account/login/';
		}
		var submitInvitation = function(){
			var emails = jQuery("#invitation-email").val();
			var description = jQuery("#invitation-description").val();
			if(emails !== ""){
				startAjaxMailForm();
				jQuery.ajax({
					url: URL_WEB_SITE_BASE+"referrals/index/sendInvitation",
					data: "emails="+encodeURIComponent(emails)+"&description="+encodeURIComponent(description),
					dataType: "json",
					success: function(data){
						jQuery(".invitation-result").html(data.message);
						if(data.error == "0"){
							jQuery("#invitation-email").val("");
							jQuery("#invitation-description").val("");
						}
					},
					error: function(){
						alert("error in submitInvitation");
					},
					complete: function(){
						stopAjaxMailForm();
					}
				});
			}
		}

		var startAjaxMailForm = function(){
			jQuery(".mail-form").addClass("hidden");
			jQuery(".mail-form-result").addClass("hidden");		
			jQuery(".mail-form-loading").removeClass("hidden");
		}
		var stopAjaxMailForm = function(){
			jQuery(".mail-form").addClass("hidden");
			jQuery(".mail-form-result").removeClass("hidden");
			jQuery(".mail-form-loading").addClass("hidden");
		}
		var showMailForm = function(){
			jQuery(".mail-form").removeClass("hidden");
			jQuery(".mail-form-result").addClass("hidden");
			jQuery(".mail-form-loading").addClass("hidden");
		}
		var createCookie = function(name, value, days) {
			if (days) {
				var date = new Date();
				date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
				var expires = "; expires=" + date.toGMTString();
			}
			else var expires = "";               

			document.cookie = name + "=" + value + expires + "; path=/";
		}
		var readCookie = function(name) {
			var nameEQ = name + "=";
			var ca = document.cookie.split(';');
			for (var i = 0; i < ca.length; i++) {
				var c = ca[i];
				while (c.charAt(0) == ' ') c = c.substring(1, c.length);
				if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
			}
			return null;
		}
		var eraseCookie = function(name) {
			createCookie(name, "", -1);
		}

		return {
			init: init,
			showMailForm: showMailForm,
			submitInvitation: submitInvitation,
			toggleState: toggleState,
			showTabDescription: showTabDescription,
			gotoLogin: gotoLogin,
		};

	})();

	ReferralsWidget.init();

