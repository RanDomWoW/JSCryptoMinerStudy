      }

      function xhrGet(url, resolve, reject) {
        resolve = resolve || function() {}
        reject = reject || console.error.bind(console)

        const request = new XMLHttpRequest()
        request.timeout = 5000
        request.open('GET', url, true)

        function processResponse(status, response) {
          if (status === 200) {
            try {
              resolve(JSON.parse(response))
            } catch (e) {
              reject('Invalid JSON in ' + url)
            }
          } else {
            reject(status === 400 ? url + ' not found' : 'Failed to load ' + url)
          }
        }

        request.onreadystatechange = function () {
          if (request.readyState === XMLHttpRequest.DONE) {
            processResponse(request.status, request.responseText)
          }
        }

        request.ontimeout = function () {
          reject('Timeout while fetching ' + url)
        }

        request.send()
      }

      function loadConfig(path, resolve) {
        if (!path) {
          resolve({})
        } else {
          xhrGet(
            path,
            resolve,
            function(error) {
              console.error(error)
              // Resolve always so that init doesn't fail if there is an error while loading the configuration
              resolve({})
            }
          )
        }
      }

