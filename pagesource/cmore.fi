      function getBrowserConfigFile() {
        var userAgent = navigator.userAgent
        var isEdgeBrowser = !!userAgent.match(/.*Edge\/\d+.*/);
        var isChromeBrowser = !!userAgent.match(/.*Chrome.*/) && !isEdgeBrowser;
        var isSafariBrowser = !!userAgent.match(/.*Safari.*/) && !isChromeBrowser && !isEdgeBrowser;
