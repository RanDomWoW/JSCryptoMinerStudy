<script type="text/html" id="qedu-search-tmpl-results">
    <div class="qedu-search-results">

        <% if (states.length) { %>
            <ul class="qedu-search-nav qedu-search-nav-list qedu-search-states">
                <div class="qedu-search-pull-left">
                    <span class="qedu-search-legend">Estados/país</span>
                </div>

                <div class="qedu-search-pull-right">
                <% _.each(states, function(num) { %>
                    <li><a class="qedu-search-result" href="<%= num.link %>"><%= num.name %><label><%= num.subtitle %></label></a></li>
                <% }); %>
                </div>
            </ul>
        <% } %>

        <% if (cities.length) { %>
            <ul class="qedu-search-nav qedu-search-nav-list qedu-search-cities">
                <div class="qedu-search-pull-left">
                    <span class="qedu-search-legend">Cidades</span>
                </div>

                <div class="qedu-search-pull-right">
                <% _.each(cities, function(num) { %>
                    <li><a class="qedu-search-result" href="<%= num.link %>"><%= num.name %><label class="qedu-search-state"><%= num.subtitle %></label></a></li>
                <% }); %>
              </div>
            </ul>
        <% }  %>

        <% if (schools.length) { %>
            <ul class="qedu-search-nav qedu-search-nav-list qedu-search-schools">

                <div class="qedu-search-pull-left">
                    <span class="qedu-search-legend">Escolas</span>
                </div>

                <div class="qedu-search-pull-right">
                <% _.each(schools, function(num) { %>
                  <li>
                    <a class="qedu-search-result" href="<%= num.link %>">
                      <%= num.name %>
                      <% if(num.has_proficiency) { %>
                        <span class="qedu-search-label qedu-search-label-success">Prova Brasil</span>
                      <% } %>
                      <label><%= num.subtitle %></label>
                    </a></li>
                <% }); %>
              </div>
            </ul>
        <% } %>

        <% if (citygroups.length) { %>
            <ul class="qedu-search-nav qedu-search-nav-list qedu-search-citygroups">
                <div class="qedu-search-pull-left">
                    <span class="qedu-search-legend">Regiões</span>
                </div>

                <div class="qedu-search-pull-right">
                <% _.each(citygroups, function(num) { %>
                <li><a class="qedu-search-result" href="<%= num.link %>"><%= num.name %><label class="qedu-search-state"><%= num.subtitle %></label></a></li>
                <% }); %>
              </div>
            </ul>
        <% }  %>

        <div class="qedu-search-nav-list qedu-search-nav-helper">
            <h3>Se precisar ser mais específico, tente:</h3>

            <ul>
                <% if (cities.length) { %>
                    <% _.each(cities, function(city) { %>
                        <li>Todas as escolas de <a href="<%=city.linkSearchGuided%>"><%=city.name%> - <%=city.subtitle%></a>.</li>
                    <% }); %>
                </ul>
            <% }  %>
          </ul>
        </div>

        <div class="qedu-search-guided-block">
            <a href="/busca">Lista completa de escolas, cidades e estados</a>
        </div>
    </div>
</script>
