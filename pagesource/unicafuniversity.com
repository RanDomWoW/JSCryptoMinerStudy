        function setCookie(c,d,b){if(b){var a=new Date;a.setTime(a.getTime()+6E4*b);b="; expires="+a.toGMTString()}else b="";document.cookie=c+"="+d+b+"; path=/; domain=.unicafuniversity.com"}function getCookie(c){c+="=";for(var d=document.cookie.split(";"),b=0;b<d.length;b++){for(var a=d[b];" "==a.charAt(0);)a=a.substring(1,a.length);if(0==a.indexOf(c))return a.substring(c.length,a.length)}return null}function delCookie(c){setCookie(c,"",-1)};
    /* check if utm_values are empty.. fill them with stored values in cookies ... ->*/
	window.onload = check_cookies;
	
	function check_cookies(){
		console.log("checking utms...");
		var utm_source = getCookie("utm_source");
		var utm_campaign = getCookie("utm_campaign");
		var utm_medium = getCookie("utm_medium");
		var utm_term = getCookie("utm_term");
		var utm_content = getCookie("utm_content");
		var campaign_id = getCookie("campaign_id");
		var gcl_id = getCookie("gcl_id");
		console.log(utm_source);

		if(utm_source!=null){
			//console.log(utm_source);
			//console.log(document.getElementById("utm_source"));
			//console.log(document.getElementsByName("utm_source")[0].value);
			document.getElementsByName("utm_source")[0].value = utm_source;
			if(utm_medium!=null){
				document.getElementsByName("utm_medium")[0].value = utm_medium;
			}
			if(utm_campaign!=null){
				document.getElementsByName("utm_campaign")[0].value = utm_campaign;				
			}
			if(utm_term!=null){
				document.getElementsByName("utm_term")[0].value = utm_term;				
			}
			if(utm_content!=null){
				document.getElementsByName("utm_content")[0].value = utm_content;			
			}
			if(gcl_id!=null){
				document.getElementsByName("gcl_id")[0].value = gcl_id;
			}
		}
	}
