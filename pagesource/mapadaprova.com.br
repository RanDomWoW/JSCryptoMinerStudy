<script type="text/javascript">
	var timerMenu = null;
	function reiniciaTimeout() {
		//console.log('reinicia...');
		
		timerMenu = window.setTimeout(
			function() {
				mataTimeout();
				$('.menu_3_colunas').hide();
			}, 
			1500
		); 
	}

	function mataTimeout() {
		//console.log('mata');
		if(timerMenu) {
		 	window.clearTimeout(timerMenu);
	 		timerMenu = null;
	   }
	}

	$(document).ready(function(){
		$('.menu_3_colunas').mouseout(reiniciaTimeout);
		$('.menu_3_colunas').mouseover(mataTimeout);
		$('.botao_menu').mouseout(reiniciaTimeout);
		$('.botao_menu').mouseover(function() {
			mataTimeout();
			$('.menu_3_colunas').show();
		})

		if(!Modernizr.input.placeholder){

			$('[placeholder]').focus(function() {
				var input = $(this);
				if (input.val() == input.attr('placeholder')) {
					input.val('');
					input.removeClass('placeholder');
				}
			}).blur(function() {
				var input = $(this);
				if (input.val() == '' || input.val() == input.attr('placeholder')) {
					input.addClass('placeholder');
					input.val(input.attr('placeholder'));
				}
			}).blur();

			$('[placeholder]').parents('form').submit(function() {
				$(this).find('[placeholder]').each(function() {
					var input = $(this);
					if (input.val() == input.attr('placeholder')) {
						input.val('');
					}
				})
			});
		}
	});

</script>

