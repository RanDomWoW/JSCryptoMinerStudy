            function SavePositionInCookie(callback) {
                var cookie = Cookies.getJSON("Position");
                if(cookie == undefined && navigator.geolocation) {
                    navigator.geolocation.getCurrentPosition(function (position) {
                        var longitude = position.coords.longitude;
                        var latitude = position.coords.latitude;
                        var data = { Longitude : longitude, Latitude : latitude };
                        Cookies.set('Position', data);
                        callback(longitude,latitude);
                    });
                }
                else {
                    callback(cookie.Longitude,cookie.Latitude);
                }
            }

            function GetEstates(longitude, latitude) {
                $(".loading-spinner").removeClass("loading-spinner--hide");
                var county = $(".js-filter-county").val();
                var objtype = $(".js-filter-estatetype").val();
                var data = {
                    County : county,
                    ObjType : objtype,
                    Longitude : longitude,
                    Latitude : latitude
                };
                $.ajax({
                    async : false,
                    type : "GET",
                    url :
                        "/?",
                    data : data,
                    contentType : "application/json; charset=utf-8",
                    traditional : true,
                    success : function (data) {
                        $(".js-estates-container").find(".boxViewContainer").remove();
                        $(".js-estates-container").append(data.View);
                        $("[data-latest='total']").empty().text(data.Total);
                        $("[data-latest='url']").attr("href", data.Url);
                        $(".loading-spinner").addClass("loading-spinner--hide");
                    },
                    error : function (data) {
                        alert("Gick inte att hämta senaste bostäder");
                        $(".loading-spinner").addClass("loading-spinner--hide");
                    }
                });
            }
        });
    </script>

