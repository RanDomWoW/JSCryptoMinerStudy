<script type="text/javascript">
    function $(id) {
        return document.getElementById(id);
    }

    function hide(id) {
        var element = $(id);
        if (element !== null && element !== undefined) {
            element.classList.add('hide');
        }
    }

    function show(id) {
        var element = $(id);
        if (element !== null && element !== undefined) {
            element.classList.remove('hide');
        }
    }

    function HasStorage() {
        if (typeof (Storage) !== "undefined") {
            return true;
        }
        else {
            return false;
        }
    }

    function PairDevice(sKey) {
        if (HasStorage()) {
            localStorage.setItem("XledgerDevice", sKey);
        }
    }

    function UnpairDevice() {
        if (HasStorage()) {
            localStorage.removeItem("XledgerDevice");
        }
    }

    function StartLogin() {
        if (HasStorage()) {
            if (localStorage.getItem("XledgerDevice") != null) {
                hide("ucLogin_pnlLoginUser");
                show("ucLogin_pnlLoginDevice");
                hide("ucLogin_pnlForgotPassword");
                show("ucLogin_pnlUnpairDevice");
                $("ucLogin_hfDeviceKey").value = localStorage.getItem("XledgerDevice");
                $("ucLogin_txtDevicePassword").focus();
            }
            else {
                show("ucLogin_pnlLoginUser");
                hide("ucLogin_pnlLoginDevice");
                show("ucLogin_pnlForgotPassword");
                hide("ucLogin_pnlUnpairDevice");
                SetUserFocus();
            }
        }
        else {
            hide("ucLogin_btnPairDevice");
            SetUserFocus();
        }
    }

    function SetUserFocus() {
        if ($("ucLogin_txtUser").value == "")
            $("ucLogin_txtUser").focus();
        else
            $("ucLogin_txtPassword").focus();
    }

    document.addEventListener("DOMContentLoaded", function () {
        var $txtUser = $("ucLogin_txtUser");
        if ($txtUser !== null && $txtUser !== undefined) {
            $txtUser.addEventListener('keypress', function (e) {
                return !CheckEnterKey(e.target, e);
            });
        }

        var $UtcOffset = $("ucLogin_UtcOffset");
        $UtcOffset.value = new Date().getTimezoneOffset();
    });
</script>

