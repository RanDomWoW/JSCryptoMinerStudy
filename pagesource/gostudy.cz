/* =============================================================================
   Media queries for different screen sizes
   ========================================================================== */
.owl-theme .owl-controls{
  width: 100%;
  text-align: center;
  position: absolute;
  bottom: 30px;
}
.carousel-item .carousel-item-text {
  position: absolute;
  top: 0;
  left: 0;
  margin: 7% 0 0 10.3rem;
}
/* Styling Next and Prev buttons */

.owl-theme .owl-controls .owl-buttons div{
  color: #FFF;
  display: inline-block;
  zoom: 1;
  *display: inline;/*IE7 life-saver */
  margin: 5px;

  font-size: 12px;

  filter: Alpha(Opacity=50);/*IE7 fix*/
  opacity: 0.5;
  /*
      width: 35px;
      height: 35px;*/
  /*	border: 2px solid rgba(0,0,0,.1);*/
  line-height: 35px;
  color: rgba(0,0,0,.9);
}
/* Clickable class fix problem with hover on touch devices */
/* Use it for non-touch hover action */
.owl-theme .owl-controls.clickable .owl-buttons div:hover{
  filter: Alpha(Opacity=100);/*IE7 fix*/
  opacity: 1;
  text-decoration: none;
}

/* Styling Pagination*/

.owl-theme .owl-controls .owl-page{
  display: inline-block;
  zoom: 1;
  *display: inline;/*IE7 life-saver */
}
.owl-theme .owl-controls .owl-page span{
  display: block;
  width: 12px;
  height: 12px;
  margin: 5px 7px;
  filter: Alpha(Opacity=20);/*IE7 fix*/
  opacity: 0.2;
  -webkit-border-radius: 20px;
  -moz-border-radius: 20px;
  border-radius: 20px;
  background: white;
}
.owl-carousel.js-topCarousel.owl-theme .owl-pagination{display:none;}


a:hover,
a:active,
a:focus,
select {
  outline: 0 none;
  text-decoration: none;
  color: white;
}
h1,
h2,
h3,
h4,
h5,
p {
  margin: 0;
}
.no-margin {
  margin: 0;
}
.no-padding {
  padding: 0;
}
.btn,
a {
  -webkit-transition: background-color 0.2s, color 0.2s, border-color 0.2s;
  -o-transition: background-color 0.2s, color 0.2s, border-color 0.2s;
  transition: background-color 0.2s, color 0.2s, border-color 0.2s;
}
ul {
  list-style: outside none none;
  padding-left: 0;
}
.hover-zoom {
  -webkit-transition: -webkit-transform 0.4s;
  -o-transition: -o-transform 0.4s;
  transition: transform 0.4s;
  -webkit-transform: scale(1, 1);
  -ms-transform: scale(1, 1);
  -o-transform: scale(1, 1);
  transform: scale(1, 1);
}
.hover-zoom:hover {
  -webkit-transition: -webkit-transform 0.4s;
  -o-transition: -o-transform 0.4s;
  transition: transform 0.4s;
  -webkit-transform: scale(1.1, 1.1);
  -ms-transform: scale(1.1, 1.1);
  -o-transform: scale(1.1, 1.1);
  transform: scale(1.1, 1.1);
}
select {
  margin-right: -19px;
  padding-left: 13px;
  margin-left: 0;
  border: none;
  background: none;
  margin-bottom: 1px;
  margin-top: 1px;
}
.btn-default,
.btn-default-alt {
  color: #02b2ed;
  background-color: white;
  border-color: white;
}
.btn-default:hover {
  color: white;
  background-color: #024169;
  border-color: #024169;
}
.btn-default-alt:hover {
  color: white;
  background-color: #02b2ed;
  border-color: #02b2ed;
}
.btn-info {
  color: white;
  background-color: #02b2ed;
  border-color: #02b2ed;
}
.btn-info:hover {
  color: #02b2ed;
  background-color: white;
  border-color: white;
}
.btn-round {
  font-size: 16px;
  font-weight: 600;
  text-transform: uppercase;
  letter-spacing: 0.025em;
  border-radius: 3rem;
  padding: 1.5rem 3rem;
  line-height: 1;
  -webkit-box-shadow: 9px 15px 32px rgba(58, 76, 88, 0.39);
  box-shadow: 9px 15px 32px rgba(58, 76, 88, 0.39);
}
.btn-wide {
  padding: 1.5rem 6rem;
}
.btn-play {
  display: block;
  position: relative;
  top: 50%;
  -webkit-transform: translate(0, -50%);
  -ms-transform: translate(0, -50%);
  -o-transform: translate(0, -50%);
  transform: translate(0, -50%);
  margin: auto;
  width: 7.8rem;
  height: 7.8rem;
  border-radius: 50%;
  background-color: #f22b24;
