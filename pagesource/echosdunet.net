  <script type="text/javascript">

  (function($){

    // Blocks Slider
$(document).ready(function(){
    if( $('.block-slider').length != 0 ){
      $('.block-slider').ednTinySlider();
    }
    if( $('#block-views-recommendations-dp-block').length != 0 ){
      $('#block-views-recommendations-dp-block').ednTinySlider();
    }
    if( $('#block-views-recommendations-breves-block').length != 0 ){
      $('#block-views-recommendations-breves-block').ednTinySlider();
    }

    // Block Slider
    $('.block-slider').each(function(){
      var el = $(this);

      if( el.find('.content').length != 0 ){
        if( el.find('.content').html() == '' ){
          el.remove();
        }
      }
    });
});

    // Submitted
    if( $('.submitted').length != 0 ){
      var sub = $('.submitted');
      var txtSub = sub.html();
      var replaced = txtSub.replace('Soumis', 'Publié');
      sub.html(replaced);
    }

    // Switch Position High<->Low Resolution
    var sidebar = $('#sidebar');
    var sat = $('#sat');

    // Sommaire existant
    if( $('body').hasClass('node-type-book') || $('body').hasClass('node-type-book-page') ){
      if( $('.book-navigation').length != 0 ){}
      $('.book-navigation').prependTo('.node > .content');
      $('.book-navigation').append('<div class="clear"></div>');
    }

    if( $('#block-menu-menu-partenaires').length != 0 ){
      $('#block-menu-menu-partenaires').append('<div class="clear"></div>');
    }

    // Breadcrumb
    if( $('.easy-breadcrumb').length != 0 ){
      $('.easy-breadcrumb').prependTo('#content');
    }

    // Reclame Class
    $('.reclame').find('.info').on('click', function(){
      var block = $(this).parent();
      var pop = block.find('.text-info');

      block.css('position', 'relative');

      if( pop.css('display') == 'none' ){
        pop.css({
          display: 'block',
          top: '100%',
          right: '-1px'
        });
      }
      else{
        pop.hide();
      }
    });

    function structuration(status){
      if( status == true ){
        // High Res
        sidebar.css('display', 'block');
        if( $('#sidebar-min').length == 1 ){
          $('#sidebar-min').remove();
        }
        // footer ul
        $('#footer').find('.pos2').insertAfter( $('#footer').find('.pos1') );
        // footer position
        $('#footer').insertAfter('.footerPrev');
        // Offres
        $('.pres-offre-inclus').insertAfter('.pres-offre-prix');
      }
      else{
        // Low Res
        sidebar.css('display', 'none');
        sat.before('<div id="sidebar-min">' + sidebar.html() + '</div>');
        // footer ul
        $('#footer').find('.pos2').insertAfter( $('#footer').find('.pos3') );
        // footer position
        $('#footer').insertAfter('#sidebar');
        // Offres
        $('.pres-offre-prix').insertAfter('.pres-offre-inclus');
      }
    }

    var currentRes = true; // true = HighRes / false = LowRes
    $(window).resize(function(){
      if( $(window).width() >= 768 ){
        if( currentRes != true ){
          structuration(true);
          currentRes = true;
        }
      }
      else{
        if( currentRes != false ){
          structuration(false);
          currentRes = false;
        }
      }
    });

    if( $(window).width() >= 768 ){
      structuration(true);
      currentRes = true;
    }
    else{
      structuration(false);
      currentRes = false;
    }

  })(jQuery);

  </script>

