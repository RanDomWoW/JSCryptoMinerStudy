					var index = sommaire.find('.onglet-actif').index();

					if( action == 'Précédent' ){
						if( index != 0 ){
							index--;
						}
					}
					else if( action == 'Suivant' ){
						if( index != (countOnglets - 1) ){
							index++;
						}
					}
					else{}

					managePrevNext();

					$('#sommaire .onglet').eq(index).trigger('click');
				});

				function managePrevNext(){

					var sommaire = $('#sommaire');
					var countOnglets = sommaire.find('.onglet').length;
					var index = sommaire.find('.onglet-actif').index();

					if( index == 0 ){
