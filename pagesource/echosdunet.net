<script type="text/javascript">
	(function($){

	// Scroll Nav
		var logo = $('#logo');
		var nav = $('#nav');
		var navT = nav.find('table');
		var td = $('#nav').find('> table > tbody > tr > td');
		var navImg = $('#nav').find('> table > tbody > tr > td > a > img');
		var openav = $('#open-menu-button');
		var bgHeader = $('#bg-header');

		$(document).scroll(function(e){
			if($(this).scrollTop() >= 50 && $(this).width() >= 751){
				logo.css('width', '10%');
				nav.css('width', '88%');
				td.addClass('nav-min-td');
				navImg.height(0);
				bgHeader.css('height', '62px');
			}
			else if($(this).scrollTop() <= 50 && $(this).width() >= 751){
				logo.css('width', '24%');
				nav.css('width', '74%');
				td.removeClass('nav-min-td');
				navImg.height(60);
				bgHeader.css('height', '115px');
			}
		});

	// Click Nav
		openav.click(function(){
			if( nav.hasClass('open') ){
				nav.removeClass('open');
			}
			else{
				nav.addClass('open');
			}
		});
	// Reset Nav
	$(window).resize(function(){
		if( $(this).width() >= 751 ){
			nav.removeClass('open');
		}
	});
	// Hover Navlink
	var navlink = $('#nav .navlink');
	navlink.on('mouseover', function(){
		var src = $(this).find('img').attr('src').split('/')[7];
		var srcModif = src.replace('off', 'on');
		$(this).find('img').attr('src', '/sites/all/themes/edn/images/pictos/' + srcModif);
	});
	navlink.on('mouseleave', function(){
		var src = $(this).find('img').attr('src').split('/')[7];
		var srcModif = src.replace('on', 'off');
		$(this).find('img').attr('src', '/sites/all/themes/edn/images/pictos/' + srcModif);
	});

	})(jQuery);
