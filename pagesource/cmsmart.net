    (function ($) {
        $(document).ready(function () {
            $('form[name="home-cmsmart-filter"]').on('change', function () {
                HomeGetContent();
            });

            function HomeGetContent() {
                var url = $('form[name="home-cmsmart-filter"]').attr('action');
                var typepro = $('input[name="home-type-ajax"]:checked').val();
                var morepro = $('input[name="home-more-ajax"]:checked').val();
                var data = 'search?filter_sort=' + typepro;                
                if(morepro>0){
                  data += '&catid=' + morepro;
                }
                
                var form = $('form[name=home-cmsmart-filter]');                
                $.ajax({
                    type: 'POST',
                    url: url,
                    data: form.serializeArray(),
                    beforeSend: function () {
                        $('.ajax-loader-reset').show();
                    },
                    success: function (html) {
                        $('.ajax-loader-reset').hide(); 
                        document.getElementById("amore-item").href=data; 
                        if(html.trim()==='no product'){
                            $('#more-item').hide();
                            $('#list_product_home').html('<div class="no-product-home">'+html+'</div>');
                        }else{
                            $('#more-item').show();
                            $('#list_product_home').html(html);
                        }
                    }
                });
            }

        });



    })(window.jQuery);
