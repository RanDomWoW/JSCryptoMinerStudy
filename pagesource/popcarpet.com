    $.fn.serializeObject = function() {
        var o = {};
        var a = this.serializeArray();
        $.each(a, function() {
            if (o[this.name] !== undefined) {
                if (!o[this.name].push) {
                    o[this.name] = [o[this.name]];
                }
                o[this.name].push(this.value || '');
            } else {
                o[this.name] = this.value || '';
            }
        });
        return o;
    };
    
    $(function(){
    
        $(window).scroll(function(e) {
            if($(window).scrollTop() > 0 ) {
                if(!$('body').hasClass('fixed-top-menu')) {
                    $('body').addClass('fixed-top-menu');
                }  
            }else{
                if($('body').hasClass('fixed-top-menu')) {
                    $('body').removeClass('fixed-top-menu');
                }  
            }
            
        });
        
        $(document).on('submit', '#subscription-form', function(e) {
            e.preventDefault();
            var formData = $(this).serializeObject();
            var form = $(this);
            if(formData.EMAIL != '') {
                $.ajax({
                    url: 'http://rewardrack.us13.list-manage.com/subscribe/post-json?u=b07017f856a0ff83841ef81c9&id=f580a58e1c&c=?',
                    data: formData,
                    type: 'GET',
                    dataType: 'JSONP',
                    jsonp: 'c',
                    success: function(data){
                        form.trigger('reset');
                        if(data.result == 'error') {
                            $('#subscription-form .help-block').html(data.msg);
                        }else{
                            $('#subscription-form .help-block').html('<p>Almost finished! We have sent you an email with confirmation link</p>');
                        }
                        
                    },
                    error: function (resp, text) {
                        form.trigger('reset');
                        $('#subscription-form .help-block').html(resp.msg);
                    }
                });
            }else{
                $('#subscription-form .help-block').html('<p>Please enter valid email address</p>');
            }
        });
    });
    
</script>
