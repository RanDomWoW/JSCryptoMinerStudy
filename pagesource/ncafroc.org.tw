  opacity: 0;
}
.bannertxt.active .bannerbk{ padding:25px;}
.bannertxt.active .boxcontent{display: block;}

.boxtitle{color:#fff;font-size: 24px;cursor: pointer;}
.boxbtn{margin-left: 10px;}
.boxbtn:before {font-family: FontAwesome;content: '\f078';font-size: 16px;}
.bannertxt.active .boxbtn:before {content: '\f077';}

