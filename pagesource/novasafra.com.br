<script type="text/javascript">
    function TextBoxFalsoLoginCliente() {
        var textFalso = document.getElementById('ptxtSenhaClientePagina');
        textFalso.style.display = 'none';
        document.getElementById('txtSenhaClientePagina').style.display = '';
        document.getElementById('txtSenhaClientePagina').focus();
    }
    function TextBoxVerdadeiroLoginCliente() {
        var textVerdadeiro = document.getElementById('txtSenhaClientePagina');
        if (textVerdadeiro.value == '') {
            textVerdadeiro.style.display = 'none';
            document.getElementById('ptxtSenhaClientePagina').style.display = '';
        }
    }

    function LogarClientePagina() {
        var loginCliente = $("#txtLoginClientePagina").val();
        var senhaCliente = $("#txtSenhaClientePagina").val();

        if (loginCliente.length <= 4 || loginCliente == 'e-mail') {
            $("#lblLoginErrorOuSaudacaoClientePagina").html('Login inválido!');
            return false;
        }
        else {
            if (senhaCliente.length <= 3) {
                $("#lblLoginErrorOuSaudacaoClientePagina").html('E-mail ou Senha inválido(a)!');
                return false;
            }
        }

        $("#lblLoginErrorOuSaudacaoClientePagina").html('');

        $.ajax({
            type: "GET",
            url: "/admin/inbox/integracao/ajax.aspx?ajax=LoginCliente&login=" + loginCliente + "&senha=" + senhaCliente,
            dataType: "text",
            success: function (xml) {
                if (isNaN(xml)) {
					if (xml.indexOf('#RCA#') > -1) {
							xml = xml.replace('#RCA#', '');
	
							if ($(parent.document).find('#cboxLoadedContent').length > 0) {
								$(parent.document).find('#cboxClose').click();
								parent.loadCarrinho('WebStorePage');
								$(parent.document).find('#ver_produtos_carrinho').click();
							}
							else {
								loadCarrinho('WebStorePage');
								try { parent.loadCarrinho('WebStorePage'); } catch (e) { }
								setTimeout(function () { $(document).find('#ver_produtos_carrinho').click(); }, 500);
								setTimeout(function () { $(document).find('#ver_produtos_carrinho').click(); }, 3500);
							}
						}

                    if (xml.indexOf('|||') == 0) {
                        $("#lblLoginCorretoClientePagina").html(xml.replace('|||', ''));
                        $("#LoginInicial").hide();
                        $("#LoginConferido").show();
                        //$(".PrecoAtacado").show();
                        //$("#bemvindo").hide();
                        //LimiteCreditoTempoReal();
                        //try { var f = $('#iframePluginDivulgacao'); f.contents().find(".PrecoAtacado").show(); } catch (e) { }
                    }
                    else {
                        $("#lblLoginCorretoClientePagina").html(xml);
                        $("#LoginInicial").hide();
                        $("#LoginConferido").show();
                        //$(".PrecoAtacado").show();
                        //$("#bemvindo").hide();
                        //LimiteCreditoTempoReal();
                        //try { var f = $('#iframePluginDivulgacao'); f.contents().find(".PrecoAtacado").show(); } catch (e) { }
                    }

                } else {

                    //$(".PrecoAtacado").hide();
                    //$("#bemvindo").show();
                    //try { var f = $('#iframePluginDivulgacao'); f.contents().find(".PrecoAtacado").hide(); } catch (e) { }
                    if (xml == '-1') {
                        $("#lblLoginErrorOuSaudacaoClientePagina").html('Dados incorretos!');
                    }
                    if (xml == '-3') {
                        location.href = 'Inbox/default.htm';
                    }
                    else if (xml == '-10') {
                        $("#lblLoginErrorOuSaudacaoClientePagina").html('Dados incompletos ou usuário inexistente.');
                    }
                    else if (xml == '-99') {
                        location.href = '/novasafra/loja.aspx?cadastro=na';
                    }
                    else if (xml == '1000') {
                        location.href = '/novasafra/loja.aspx?cadastro=na';
                    }
                }
            }
        });
        return true;
    }

    function VerificaLogin(logoff) {
        if (logoff == undefined) {
            $.ajax({
                type: "GET",
                url: "/admin/inbox/integracao/ajax.aspx?ajax=LoginCliente&login=",
                dataType: "text",
                success: function (xml) {
                    if (xml.indexOf('|||') == 0) {
                        $("#lblLoginCorretoClientePagina").html(xml.replace('|||', ''));
                        $("#LoginInicial").hide();
                        $("#LoginConferido").show();
                        //$(".PrecoAtacado").show();
                        //$("#bemvindo").hide();
                        //LimiteCreditoTempoReal();
                        //try { var f = $('#iframePluginDivulgacao'); f.contents().find(".PrecoAtacado").show(); } catch (e) { }
                    }
                    else {
                        $("#lblLoginCorretoClientePagina").html(xml);
                        $("#LoginConferido").hide();
                        $("#LoginInicial").show();
                        //$(".PrecoAtacado").hide();
                        //$("#bemvindo").show();
                        //try { var f = $('#iframePluginDivulgacao'); f.contents().find(".PrecoAtacado").hide(); } catch (e) { }
                    }
                }
            });
        }
        else {
            $.ajax({
                type: "GET",
                url: "/admin/inbox/integracao/ajax.aspx?ajax=LoginCliente&login=sair",
                dataType: "text",
                success: function (xml) {
                    $("#LoginConferido").hide();
                    $("#LoginInicial").show();
                    //$(".PrecoAtacado").hide();
                    $("#txtLoginClientePagina").val('e-mail');
                    $("#txtSenhaClientePagina").val('');
                    TextBoxVerdadeiroLoginCliente();
                    //try { var f = $('#iframePluginDivulgacao'); f.contents().find(".PrecoAtacado").hide(); } catch (e) { }
                }
            });
        }
    }

    function isEnter(e) {
        var e = e || event;
        var k = e.keyCode || e.which;
        if (k == 13) return true
        else return false;
    }

    setTimeout('VerificaLogin();', 500);
</script>

