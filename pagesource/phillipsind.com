<script type="text/javascript">

	function reloadFeedbackPopup()
	{
		var currentBlock = jQuery('#recaptcha_area').parent().parent().html();
		jQuery('.captcha-block').html('');
		jQuery(".feedbackForm .captcha-block").html(currentBlock);
		jQuery(".feedbackForm .form-group input").val('');
		jQuery(".feedbackForm .message").val('');
	}

	function submitFeedbackForm () {
		if ( !jQuery(".feedbackForm .subject").val() || !jQuery(".feedbackForm .message").val()) {
			jQuery( ".feedbackForm .textValid" ).text( "Fill in the required fields." ).show().fadeOut( 5000 );
			return;
		}
		if ( !jQuery(".feedbackForm #recaptcha_response_field").val()) {
			jQuery( ".feedbackForm .textValid" ).text( "Fill in the Captcha field." ).show().fadeOut( 5000 );
			return;
		}

		jQuery.fn.serializeObject = function()
		{
			var o = {};
			var a = this.serializeArray();
			jQuery.each(a, function() {
				if (o[this.name] !== undefined) {
					if (!o[this.name].push) {
						o[this.name] = [o[this.name]];
					}
					o[this.name].push(this.value || '');
				} else {
					o[this.name] = this.value || '';
				}
			});
			return o;
		};
		var data = jQuery("#feedbackForm").serializeObject();

		jQuery.ajax( {
			url : "http://phillipsind.com/phillips/index/sendFeedbackEmail/",
			type: "POST",
			data: data,
			dataType : 'json',
			success : function(data) {
				if (data && data.status == 200) {
					jQuery('#feedbackModal').modal('toggle');
					jQuery('#answerHeader').text('Thank You!');
					jQuery('#answerText').text(data.message);
					jQuery('#answerModal').modal('toggle');
				} else if (data && data.status == 1) {
					Recaptcha.reload();
					jQuery( ".feedbackForm .textValid" ).text(data.message).show().fadeOut( 5000 );
				} else {
					jQuery('#feedbackModal').modal('toggle');
					jQuery('#answerHeader').text('Something Went Wrong!');
					jQuery('#answerText').text(data.message);
					jQuery('#answerModal').modal('toggle');
				}
			}
		})
	}
