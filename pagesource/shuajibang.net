<!--自定义js文件-->

<script>
    $(function () {
        $(".selBrand").change(function () {

            $.post("/product/getProductByBrandId",
                {
                    brandId: $(this).find("option:selected").val(),
                },
                function (data) {
                    $(".selPattern").empty();
                    $(".selPattern").append($("<option>请选择机型</option>"));
                    $.each(data.data, function (i, item) {
                        $(".selPattern").append($("<option>" + item + "</option>"));
                    });
                });
        });

        $("#btnSaveFeedBack").click(function () {
            var pro = $(".selPattern").find("option:selected").text();
            if (pro == "请选择机型") {
                alert("请选择品牌和机型");
                return;
            }

            var con = $(".selBrand").find("option:selected").text() + " " + pro;
            $.post("/product/addFeedBack",
                {
                    cedulevel: "【机型适配】",
                    cNative: "建议增加机型：",
                    MsgContent: con
                },
                function (data) {
                    if (data.success == true) {
                        $("#light.nfinfo p").html("您的机型已提交成功，感谢您的建议！");
                        $(".apply").hide();
                        $(".closebox").show();
                        $(".selBrand").val(-1);
                        $(".selPattern").val(-1);
                    }
                });
        });
    });

    $(function () {
        $("#searchbtn").click(function () {
            loadSearch();
        });

        $("#ajaxLoadChooseKeyword").parent().find("#searchText").keydown(function (e) {
            if (e.keyCode == 13) {
                loadSearch();
            }
        });

        //搜索框输入内容时触发事件
        $("#searchText").keyup(function (e) {
            if (e.keyCode != 13) {
                var keyword = $("#searchText").val();
                if (keyword) {
                    ajaxLoadSearch(keyword);
                } else {
                    $("#ajaxLoadChooseKeyword").hide();
                }
            }
        });

        //失去焦点时触发事件
        $("#searchText").blur(function () {
            $("#ajaxLoadChooseKeyword").fadeOut(200, function () {
                $("#ajaxLoadChooseKeyword").hide();
            });
            var value = $("#searchText").val();
            if (!value) {
                $("#ajaxLoadChooseKeyword").empty();
                $("#ajaxExpanded").empty();
            }
        });

        //获取焦点时触发事件
        $("#searchText").focus(function () {
            var value = $("#searchText").val();
            var val = $("#ajaxLoadChooseKeyword").html();
            if (value && val) {
                $("#ajaxLoadChooseKeyword").fadeIn(200, function () {
                    $("#ajaxLoadChooseKeyword").show();
                });
            } else if (value && !val) {
                ajaxLoadSearch(value);
            }
        });

        $("#comments").click(function () {
            $.ajax({
                type: "post",
                url: "/help/readComments",
                data: { replyid: $(this).attr("replyid") },
                dataType: "json",
                success: function (result) {
                }
            });

        });
    });

    function loadSearch() {
        if ($("#searchText").val() != "") {
            window.location = "/search?brandId=" + '' + "&q=" + $("#searchText").val();
        } else {
            swal({ confirmButtonText: "确定", confirmButtonColor: "#00A8EF", title: "请输入手机品牌、型号或固件号" });
        }
    }

    function ajaxLoadSearch(keyword) {
        $.ajax({
            type: "post",
            url: "/product/SearchPost",
            data: { q: keyword, brandId: '' },
            dataType: "json",
            success: function (result) {
                $("#ajaxLoadChooseKeyword").empty();
                $("#ajaxExpanded").empty();
                if (result.data.length > 0) {
                    $.each(result.data, function (i, data) {
                        var html = '<li><a href="/rom/detail/' + data.id + '"><span class="search_type">【' + data.type + '】</span><span class="search_word">' + data.name + '</span><span class="search_count" id="searchCount' + data.id + '"></span></a></li>';
                        $("#ajaxLoadChooseKeyword").append(html);
                        if (data.countType) {
                            $("#searchCount" + data.id).append('共<font>' + data.countType + '</font>款机型');
                        }
                        if (data.countRom) {
                            $("#searchCount" + data.id).append('共<font>' + data.countRom + '</font>个ROM');
                        }
                    });
                    $("#ajaxLoadChooseKeyword").fadeIn(200, function () {
                        $("#ajaxLoadChooseKeyword").show();
                    });
                }
            }
        });
    }
</script>

