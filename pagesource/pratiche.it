			if(lh_inst.isOnline) {
				$(".chat-footer,#chat-up-header,#btn_chat,#btn_assistenza_via_chat").css("display","block");
				$("#chat-help-timed").css("display","inline");
			} else {
				$(".chat-footer-disabled").css("display","block");
			}
		}
	}

	get_lhchat_instance_onload();
</script>

