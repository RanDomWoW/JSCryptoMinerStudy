    <script>
(function(){
  document.querySelector('.teaser__videobutton').addEventListener('click', function(e){
    e.preventDefault();

    var width = Math.min(window.innerWidth * 0.9, window.innerHeight * 1.6);
    var height = width * 0.5625;

    var iframe = document.createElement('iframe');
    iframe.setAttribute('src', e.currentTarget.dataset.video);
    iframe.setAttribute('frameborder', 0);
    iframe.setAttribute('allowfullscreen', 'allowfullscreen');
    var div = document.createElement('div');
    div.classList.add('fullscreen-video');
    div.style.width = width + "px";
    div.style.height = height + "px";
    div.appendChild(iframe);
    var closeButton = document.createElement('button');
    closeButton.classList.add('fullscreen-video__close');
    div.appendChild(closeButton);
    document.body.appendChild(div);

    closeButton.addEventListener('click', function(e) {
      document.body.removeChild(div);
    });
    closeButton.addEventListener('keydown', function(e) {
      if(e.keyCode === 27) {
        document.body.removeChild(div);
      }
    });
    closeButton.focus();
  });
})();
    </script>
    
