<script>
    // Picture element HTML5 shiv
    //---------------------------
    document.createElement("picture");


    // Toggle off-canvas-menu
    //-----------------------
    $(function() {
        $('.js-mobile-hamburger-button').click(function() {
            toggleMenu();
        });
    });

    function toggleMenu() {
        if ($('.menu-canvas').hasClass('show-menu')) {
            // Nav close stuff
            $('.menu-canvas').removeClass('show-menu');
            $('.overlay').removeClass('open');
        } else {
            // Nav open stuff
            $('.menu-canvas').addClass('show-menu');
            $('.overlay').addClass('open');
        }
    }

    // Location of header modules & sticky toggle
    //-------------------------------------------
    $(window).on("load resize", function (e) {
        // MOBILE media query check
        if (Modernizr.mq(mediaQueries.mobile)) {//'only all and (max-width: 767px)'

            // move postal code and store location nodes
            //$(".location-information").insertAfter($(".navigation-container"));

            // hide and show same nodes on scroll
            $(window).scroll(function () {
                if ($(document).scrollTop() > 100) {
                    $('.location-information').hide();
                } else {
                    $('.location-information').show();
                }
            });
        }


            // DESKTOP media query check
        else if (Modernizr.mq(mediaQueries.desktop)) {//'only all and (min-width: 768px)'
            // return nodes to original position
            //$(".location-information").insertAfter($(".brand"));

            // toggle class for sticky bar
            $(window).scroll(function () {
                if ($(document).scrollTop() > 100) {
                    $('.navigation-container').addClass('stick');
                } else {
                    $('.navigation-container').removeClass('stick');
                }
            });

        }
    });



    // Search box
    //-----------

    // MOBILE
    $(function () {
        // toggle search input
        if (Modernizr.mq(mediaQueries.mobile)) {//'only all and (max-width: 767px)'
            $('.search-toggle').click(function () {
                $('.header-search-form').toggleClass('show');
            });
        }

    });

    // DESKTOP
    $(document).ready(function () {
        var submitIcon = $('.searchbox-icon');
        var inputBox = $('.searchbox-input');
        var searchBox = $('.searchbox');
        var isOpen = false;

        // expand search field
        submitIcon.click(function () {
            if (isOpen == false) {
                searchBox.addClass('searchbox-open');
                inputBox.focus();
                isOpen = true;
            } else {
                searchBox.removeClass('searchbox-open');
                inputBox.focusout();
                isOpen = false;
            }
        });

        submitIcon.mouseup(function () {
            return false;
        });
        searchBox.mouseup(function () {
            return false;
        });

        $(document).mouseup(function () {
            if (isOpen == true) {
                $('.searchbox-icon').css('display', 'block');
                submitIcon.click();
            }
        });
    });

    function buttonUp() {
        var inputVal = $('.searchbox-input').val();
        inputVal = $.trim(inputVal).length;

        // when type anything in search field, hide toggle button, to show submit button
        if (inputVal !== 0) {
            $('.searchbox-icon').css('display', 'none');
        } else {
            $('.searchbox-input').val('');
            $('.searchbox-icon').css('display', 'block');
        }
    }

</script>


