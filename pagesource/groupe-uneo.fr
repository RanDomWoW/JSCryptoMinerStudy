                let submitbtn = jQuery('.login-btn > button');

                function disableButton(inputs) {
                    submitbtn.html('PATIENTEZ...').prop('disabled', true);
                    tryConnecting(inputs);
                }

                function tryConnecting(inputs) {
                    jQuery.ajax('/api/connection/connection', {
                        'data': JSON.stringify({
                            email: inputs[0].value,
                            password: inputs[1].value
                        }),
                        'type': 'POST',
                        'processData': false,
                        'contentType': 'application/json',
                        'success': function (data) {
                            if (data.type == 'ok') {
                                window.location.href = '/adherent/dashboard';
                            } else if (data.type == 'ko') {
                                window.location.replace('/connexion/connexion?err=' + data.err);
                            } else {
                                submitbtn.html('ME CONNECTER').prop('disabled', false);
                                jQuery('.login-wrapper > .error-message').show().html('Une erreur est survenue veuillez réessayer ultérieurement.');
                            }
                        },
                        'error': function (request, error) {
                            if (request.status == 503) {
                                window.location.href = '/503';
                            }
                        }
                    });
                }

                jQuery('.login-wrapper > form').on('submit', function (e) {
                    e.preventDefault();

                    let inputs = jQuery(this).serializeArray();
                    disableButton(inputs);
                });

                jQuery('.login-pop-form').click(function (event) {
                    event.stopPropagation();
                });

                jQuery(window).click(function () {
                    let wrapper = jQuery('.login-wrapper');
                    if (wrapper.hasClass('in')) {
                        wrapper.collapse('hide');
                    }
                });
