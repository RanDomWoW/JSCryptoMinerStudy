<script>

    var width = (window.innerWidth > 0) ? window.innerWidth : screen.width;

    var videoWidth;
    var videoHeight;
    var isTablet=false;
    var isSmartphone=false;
	var streamingWidth;


    if(width > 768){
    	streamingWidth = 550;
        streamingHeight = 309;
        videoWidth=640;
        videoHeight=360;
    }
    if(width <= 768 && width > 457){
		streamingWidth = 550;
        videoWidth=457;
        streamingHeight = 254;
        videoHeight=254;
        isTablet=true;
    }
    if(width <= 457){
		streamingWidth = 300;
         streamingHeight = 167;
        videoWidth=300;
        videoHeight=167;
        isSmartphone=true;
    }
    if(width == 768){
    	streamingWidth = 420;
        streamingHeight = 310;
    }
</script>


