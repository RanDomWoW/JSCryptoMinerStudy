    <script>
     (function($) {
         // 固定ヘッダ
         $(window).on('scroll', function() {
             var shouldVisible = function() {
                 return $(window).scrollTop() > $('header').height();
             }

             if (shouldVisible()) {
                 $('#fix, #searcharea').addClass('fixed');
                 $('#fix').addClass('is-visible');
                 if ($('#global-nav').hasClass('is-visible')) {
                     $('#global-nav').removeClass('is-visible');
                     $('#nav-toggle').removeClass('is-close');
                     $('#fix .subnav_box').addClass('is-visible');
                 }
                 // #searchareaは.fixedを適用してから.is-visibleを適用
                 $('#searcharea').delay(250).queue(function(){
                     if (shouldVisible()) {
                         $(this).addClass('is-visible').dequeue();
                     } else {
                         $(this).dequeue();
                     }
                 });
             } else {
                 $('#fix, #searcharea, #fix .subnav_box, #fix_search').removeClass('is-visible');
                 $('#searcharea').removeClass('fixed');
                 // #fixは.is-visibleを解除してから.fixedを解除
                 $('#fix').delay(250).queue(function(){
                     if (!shouldVisible()) {
                         $(this).removeClass('fixed').dequeue();
                     } else {
                         $(this).dequeue();
                     }
                 });
             }
         });
         $(function() {
             // 検索フォームの表示/非表示
             $('#search_s').on('click', function(){
                 $('#fix_search').addClass('is-visible');
                 $('#fix .subnav_box').removeClass('is-visible');
                 return false;
             });
             $('#fix_search .close').on('click', function(){
                 $('#fix_search').removeClass('is-visible');
                 return false;
             });

             // ハンバーガーメニューの表示/非表示
             var $header = $('#global-nav');
             // Nav Style
             $(window).on('load resize', function(){
                 if (!onPC()) {
                     $header.addClass('open');
                 } else {
                     $header.removeClass('open');
                 }
             });
             // Nav Toggle Button
             $('#nav-toggle').click(function(){
                 $header.toggleClass('is-visible');
                 $(this).toggleClass('is-close');
             });

             // Topへ戻る
             var topBtn = $('.backToTop');
             topBtn.hide();
             $(window).scroll(function () {
                 if ($(this).scrollTop() > 600) {
                     topBtn.fadeIn();
                 } else {
                     topBtn.fadeOut();
                 }
             });
             topBtn.click(function () {
                 $('body,html').animate({
                     scrollTop: 0
                 }, 500);
                 return false;
             });

             // カテゴリメニューの表示/非表示
             $('#fix .subnav').children('a').on('click', function(){
                 $('#fix .subnav_box').toggleClass('is-visible');
                 return false;
             });

             // ランキングのタブ切り替え
             $('#ranking .side-tab li a').on('click', function(){
                 var tab = {'ranking-tab-daily': 'ranking-tabcontent-daily',
                            'ranking-tab-weekly': 'ranking-tabcontent-weekly',
                            'ranking-tab-monthly': 'ranking-tabcontent-monthly'};

                 $(this).parent().siblings().removeClass('side-tab-current');
                 $(this).parent().addClass('side-tab-current');

                 $('#ranking .side-tabs_content').css('display', 'none');
                 $('#'+tab[$(this).attr('id')]).css('display', 'block');

                 return false;
             });

             // PC幅の場合、タイトルの長さによって記事リスト要素の高さが
             // 変化するのに合わせてアイキャッチ画像の高さを合わせる。
             $(window).on('load resize', function(){
                 alignEyecatchHeight();
             });
         });

              })(jQuery);

     function onPC() {
         return !onSP() && !onTablet();
     }
     function onTablet() {
         return !onSP() && $(window).width() <= 1100;
     }
     function onSP() {
         return $(window).width() <= 767;
     }

     function alignEyecatchHeight() {
         if (onPC()) {
             $('.col1 ul li').each(function(){
                 $('figure', this).innerHeight($(this).innerHeight());
             });
         }
     }
    </script>
