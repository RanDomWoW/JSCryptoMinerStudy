<script>
$(function(){
	$(".line2").css("border-bottom","0px");//去除下底部的線

	var total_images_count = 9;
	var original_index = 1;
	var current_index = 1;

	var focus_width = $(".focus_image_wrap").width();
	var focus_height = focus_width * 416 / 1024;

	$(".focus_wrap").css("height", focus_height + "px");

	$(window).resize(function() {
		focus_width = $(".focus_image_wrap").width();
		focus_height = focus_width * 416 / 1024;
		$(".focus_wrap").css("height", focus_height + "px");
	});

	//點擊首焦slide
	$("#top_focus .slide_wrap span").click(function(){
		$("#top_focus .slide_wrap span").removeClass("active");
		$(this).addClass("active");
		var focus_index = $(this).attr("data-focus-index");
		if(focus_index == current_index) return false;
		clearInterval(auto_play_focus_images);
		current_index = focus_index;
		$("#focus_"+original_index).fadeOut(1500);
		$("#focus_"+current_index).fadeIn(1500);
		original_index = current_index;
		auto_play_focus_images = setInterval(play_focus_images,3000);
		//alert(current_index+'.'+original_index);
	});

	var play_focus_images = function(){
		$("#focus_"+original_index).fadeOut(1500);
		current_index = parseInt(current_index) + 1;
		if(current_index > total_images_count) current_index = 1;
		$("#focus_"+current_index).fadeIn(1500);
		$("#top_focus .slide_wrap span").removeClass("active");
		$("#slide_"+current_index).addClass("active");
		original_index = current_index;	
	};
	
	var auto_play_focus_images = setInterval(play_focus_images,3000);

	$(".channel_item").click(function(){
		window.location.href = "/offical/index/channel?type="+$(this).attr("data-type");
	});

	$(".focus_image_wrap").click(function(){
		window.open($(this).attr("data-link"));
	});

	$(".videos_list div").click(function(){
		window.location.href = $(this).attr("data-link");
	});
});
</script>  </div>
