	function switchDip_363825(num,pause){
		clearTimeout(slideshowTimer_363825);
		var timer;
		var nums = num;
		var slideshowMotion = slideshowMotion_363825();
		var speed = slideshowMotionSpeed_363825();
		var currentNum = document.getElementById('currentNum_363825').value;
		var totalDips = document.getElementById('totalDips_363825').value;
		var video = document.getElementById('video_363825').value;
		if(speed == '' || speed == null){
			speed = 800;
		}
		
		var direction = directions_363825(slideshowMotion);
		var directionOn = direction.directionOn;
		var directionOff = direction.directionOff;
		
		if(totalDips > 1 && video == 0){
		
			if(nums < 1){nums = 1;}
			if(nums > totalDips){nums = totalDips;}
			
			if($('#nav_363825' + nums).length){
				$('#nav_363825' + nums).removeClass('on');
			}
			
			if($('#nav_li_363825' + nums).length){
				$('#nav_li_363825' + nums).removeClass('on');
			}
			
			if(slideshowMotion != 'f'){
				if($('#videoShell_363825' + nums).length){
					$('#videoShell_363825' + nums).hide();
				}
				$('#dipImage_363825' + nums).hide("slide", { direction: directionOff }, speed);
			}else{
				$('#dipImage_363825' + nums).fadeOut(speed);
			}
			
			if($('#videoShell_363825' + nums).length){
				$('#videoShell_363825' + nums).fadeOut(speed);
			}
			if($('#dipContent_363825' + nums).length){
				$('#dipContent_363825' + nums).fadeOut(speed);
			}
			
			if(nums >= totalDips){nums = 1;}else{nums = nums + 1;}
			
			if($('#nav_363825' + nums).length){
				$('#nav_363825' + nums).addClass('on');
			}
			
			if($('#nav_li_363825' + nums).length){
				$('#nav_li_363825' + nums).addClass('on');
			}
			
			if(slideshowMotion != 'f'){
				$('#dipImage_363825' + nums).show("slide", { direction: directionOn }, speed, function(){
				if($('#videoShell_363825' + nums).length){
					$('#videoShell_363825' + nums).fadeIn(speed);
					}	
				});
			}else{
				$('#dipImage_363825' + (nums)).fadeIn(speed);
			}
			
			if(slideshowMotion == 'f'){
				if($('#videoShell_363825' + nums).length){
					$('#videoShell_363825' + nums).fadeIn(speed);
				}
			}
			if($('#dipContent_363825' + nums).length){
				$('#dipContent_363825' + (nums)).fadeIn(speed);
			}
				
			if(num < totalDips){timer = num + 1;}else{timer = 1;}
			rotateDip_363825(timer);
			document.getElementById('currentNum_363825').value = nums;
		}
	}
	
	function slideshowMotion_363825(){
