    <script type="text/javascript">
        $('#drpState').html('');
        $("#drpState").append('<option value="" selected="selected">State</option>');
        $.ajax({
            type: "POST",
            url: "cars/states.ashx",
            contentType: "application/json; charset=utf-8",
            datatype: "json",
            async: "true",
            success: function (response) {
                var i;
                for (i = 0; i < response.states.items.length; i++) {
                    $("#drpState").append('<option value="' + response.states.items[i].id + '">' + response.states.items[i].name + '</option>');
                }
                $('#drpState').selectric('refresh');
            },
            error: function (response) {
            }
        });
        $("#drpState").change(function () {
            $('#drpCity').html('');
            $("#drpCity").append('<option value="" selected="selected">City</option>');
            $.ajax({
                type: "POST",
                url: "cars/cities.ashx?code=" + $("#drpState").val(),
                contentType: "application/json; charset=utf-8",
                datatype: "json",
                async: "true",
                success: function (response) {
                    var i;
                    for (i = 0; i < response.cities.items.length; i++) {
                        $("#drpCity").append('<option value="' + response.cities.items[i].id + '">' + response.cities.items[i].name + '</option>');
                    }
                    $('#drpCity').selectric('refresh');
                },
                error: function (response) {
                }
            });
        });
        $("#drpCity").change(function () {
            $('#drpDealer').html('');
            $("#drpDealer").append('<option value="" selected="selected">Dealer</option>');
            $.ajax({
                type: "POST",
                url: "cars/dealers.ashx?code=" + $("#drpCity").val(),
                contentType: "application/json; charset=utf-8",
                datatype: "json",
                async: "true",
                success: function (response) {
                    var i;
                    for (i = 0; i < response.dealers.items.length; i++) {
                        $("#drpDealer").append('<option value="' + response.dealers.items[i].id + '">' + response.dealers.items[i].name + '</option>');
                    }
                    $('#drpDealer').selectric('refresh');
                },
                error: function (response) {
                }
            });
        });
        $('#drpCar').html('');
        $("#drpCar").append('<option value="" selected="selected">Model</option>');
        $.ajax({
            type: "POST",
            url: "cars/cars.ashx",
            contentType: "application/json; charset=utf-8",
            datatype: "json",
            async: "true",
            success: function (response) {
                var i;
                for (i = 0; i < response.cars.items.length; i++) {
                    $("#drpCar").append('<option value="' + response.cars.items[i].id + '">' + response.cars.items[i].name + '</option>');
                    if (response.cars.items[i].name.toLowerCase() == "baleno") {
                        $("#drpCar").append('<option value="' + response.cars.items[i].id + '"> BalenoRS</option>');
                    }
                    if (response.cars.items[i].name.toLowerCase() == "ciaz") {
                        $("#drpCar").append('<option value="' + response.cars.items[i].id + '"> Ciaz S</option>');
                    }
                }
                $('#drpCar').selectric('refresh');
            },
            error: function (response) {
            }
        });
        $("#drpCar").change(function () {
            $('#drpVariant').html('');
            $("#drpVariant").append('<option value="" selected="">Variant</option>');
            if ($("#drpCar option:selected").text().toLowerCase().indexOf("balenors") >= 0) {
                $('#drpVariant').append('<option value="47">BalenoRS</option>');
                $('#drpVariant').selectric('refresh');
                return;
            }
            if ($("#drpCar option:selected").text().toLowerCase().indexOf("ciaz s") >= 0) {
                $('#drpVariant').append('<option value="143">Ciaz S Petrol</option>');
                $('#drpVariant').append('<option value="144">Ciaz S Smart Hybrid</option>');
                $('#drpVariant').selectric('refresh');
                return;
            }
            $.ajax({
                type: "POST",
                url: "cars/variants.ashx?car=" + $("#drpCar").val(),
                contentType: "application/json; charset=utf-8",
                datatype: "json",
                async: "true",
                success: function (response) {
                    var i;
                    for (i = 0; i < response.variants.items.length; i++) {
                        $("#drpVariant").append('<option value="' + response.variants.items[i].id + '">' + response.variants.items[i].name + '</option>');
                    }
                    $('#drpVariant').selectric('refresh');
                },
                error: function (response) {
                }
            });
        });
        function ReSndOTP() {
                if (document.getElementById("btnVOTP").outerHTML.toLowerCase().indexOf("disabled") > 0) {
                   return;
                }
                Timer = 30;
                $(".otp-time-message").html("OTP expiring in " + Timer + " seconds.");
                $.ajax({
                   type: "POST",
                   url: "/user/generateotp.ashx?mobile=" + encodeURIComponent($.trim(form1.mobile.value))
                       + "&type=" + encodeURIComponent("Form"),
                   async: "true",
                   success: function (response) {
                       form1.otp.value = '';
                   },
                   error: function (response) {
                   }
                });
        }
        function SaveData() {
            var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;

            if (form1.drpCar.value == "")
            { document.getElementById('errm').style.display = 'block'; }
            else { document.getElementById('errm').style.display = 'none'; }

            if (form1.drpVariant.value == "")
            { document.getElementById('errv').style.display = 'block'; }
            else { document.getElementById('errv').style.display = 'none'; }

            if ($.trim(form1.name.value).length == 0 || $.trim(form1.name.value.toLowerCase().replace("name", "")).length == 0)
            { document.getElementById('errn').style.display = 'block'; }
            else { document.getElementById('errn').style.display = 'none'; }

            if ($.trim(form1.email.value).length == 0)
            { document.getElementById('erre').style.display = 'block'; }
            else { document.getElementById('erre').style.display = 'none'; }

            if (reg.test(form1.email.value) == false)
            { document.getElementById('erre').style.display = 'block'; }
            else { document.getElementById('erre').style.display = 'none'; }

            if ($.trim(form1.mobile.value).length == 0)
            { document.getElementById('errp').style.display = 'block'; }
            else { document.getElementById('errp').style.display = 'none'; }

            if ($.trim(form1.mobile.value).length != 10)
            { document.getElementById('errp').style.display = 'block'; }
            else { document.getElementById('errp').style.display = 'none'; }

            if ($.trim(form1.mobile.value).length == 10) {
                if (isNaN(form1.mobile.value) == true)
                { document.getElementById('errp').style.display = 'block'; }
                else
                { document.getElementById('errp').style.display = 'none'; }
            }

            if (form1.drpState.value == "")
            { document.getElementById('errs').style.display = 'block'; }
            else { document.getElementById('errs').style.display = 'none'; }

            if (form1.drpCity.value == "")
            { document.getElementById('errc').style.display = 'block'; }
            else { document.getElementById('errc').style.display = 'none'; }

            if (form1.drpDealer.value == "")
            { document.getElementById('errd').style.display = 'block'; }
            else { document.getElementById('errd').style.display = 'none'; }

            if (form1.check.checked == false)
            { document.getElementById('errchk').style.display = 'block'; }
            else { document.getElementById('errchk').style.display = 'none'; }

            if (document.getElementById('errm').style.display == "none" && document.getElementById('errn').style.display == "none"
                && document.getElementById('erre').style.display == "none" && document.getElementById('errd').style.display == "none"
                && document.getElementById('errs').style.display == "none" && document.getElementById('errc').style.display == "none"
                && document.getElementById('errp').style.display == "none"
                && document.getElementById('errchk').style.display == "none" && document.getElementById('errv').style.display == "none") {
                if (document.getElementById("btnVOTP").outerHTML.toLowerCase().indexOf("disabled") > 0) {
                   return;
                }
                $('#progressfinal').css('display', 'block');
                Timer = 30;
                $(".otp-time-message").html("OTP expiring in " + Timer + " seconds.");
                window.setTimeout(function () { DoTiming(); }, 1000);
                $.ajax({
                   type: "POST",
                   url: "/user/generateotp.ashx?mobile=" + encodeURIComponent($.trim(form1.mobile.value))
                       + "&type=" + encodeURIComponent("Form"),
                   async: "true",
                   success: function (response) {
                       $('#progressfinal').css('display', 'none');
                       form1.otp.value = '';
                       $('.opt-form-popup').show();
                   },
                   error: function (response) {
                   }
                });
            }
        }
        function VerifyOTP() {
           if (document.getElementById("btnVOTP").outerHTML.toLowerCase().indexOf("disabled") > 0) {
               return;
           }
           if ($.trim(form1.otp.value).length == 0 || $.trim(form1.otp.value.toLowerCase().replace("----", "")).length == 0)
           { document.getElementById('errorotp').style.display = 'block'; }
           else { document.getElementById('errorotp').style.display = 'none'; }
           if (document.getElementById('errorotp').style.display == "none") {
               $('#progressfinal').css('display', 'block');
               $("#btnVOTP").attr("disabled", "disabled");
               $.ajax({
                   type: "POST",
                   url: "/user/verifyotp.ashx?mobile=" + encodeURIComponent($.trim(form1.mobile.value))
                       + "&code=" + encodeURIComponent($.trim(form1.otp.value)),
                   async: "true",
                   success: function (response) {
                       $('#progressfinal').css('display', 'none');
                       if (response.result == "1") {
                           $('input[name="mobile"]').attr("readonly", "readonly");
                           $('input[name="otp"]').attr("readonly", "readonly");
                            $('#progressfinal').css('display', 'block');
                            $.ajax({
                                type: "POST",
                                url: "forms/savequotes.ashx?car=" + encodeURIComponent($.trim(form1.drpCar.value)) +
                                    "&model=" + encodeURIComponent($.trim(form1.drpVariant.value)) +
                                    "&title=" + encodeURIComponent("NA") +
                                    "&name=" + encodeURIComponent($.trim(form1.name.value)) +
                                    "&email=" + encodeURIComponent($.trim(form1.email.value)) +
                                    "&cc=" + encodeURIComponent("+91") +
                                    "&mobile=" + encodeURIComponent($.trim(form1.mobile.value)) +
                                    "&city=" + encodeURIComponent($.trim(form1.drpCity.value)) +
                                    "&dealer=" + encodeURIComponent($.trim(form1.drpDealer.value)) +
                                    "&captcha=" + encodeURIComponent($.trim(form1.otp.value)),
                                async: "true",
                                success: function (response) {
                                    if (response == "0") {
                                    }
                                    else {
                                        if (form1.drpCar.value == "1") {
                                            ga('send', 'event', 'Homepage', 'Model', 'S-Cross');
                                            window.location.href = "s-cross-thank-you.html";
                                        }
                                        else {
                                            if (form1.drpCar.value == "10") {
                                                ga('send', 'event', 'Homepage', 'Model', 'Ignis');
                                                window.location.href = "ignis-thank-you.html";
                                            }
                                            else {
                                                if (form1.drpCar.value == "15") {
                                                    if (form1.drpVariant.value == "143" || form1.drpVariant.value == "144") {
                                                        ga('send', 'event', 'Homepage', 'Model', 'Ciaz S');
                                                        window.location.href = "ciaz-s-thank-you.html";
                                                    }
                                                    else {
                                                        ga('send', 'event', 'Homepage', 'Model', 'Ciaz');
                                                        window.location.href = "ciaz-thank-you.html";
                                                    }
                                                }
                                                else {
                                                    if (form1.drpVariant.value == "47") {
                                                        ga('send', 'event', 'Homepage', 'Model', 'BalenoRS');
                                                        window.location.href = "baleno-rs-thank-you.html";
                                                    }
                                                    else {
                                                        ga('send', 'event', 'Homepage', 'Model', 'Baleno');
                                                        window.location.href = "baleno-thank-you.html";
                                                    }
                                                }
                                            }
                                        }
                                    }
                                    $('#progressfinal').css('display', 'none');
                                },
                                error: function (response) {
                                }
                            });
                       }
                       else {
                           document.getElementById('errorotp').style.display = 'block';
                           $("#btnVOTP").removeAttr("disabled");
                       }
                   },
                   error: function (response) {
                   }
               });
           }
        }
    </script>





