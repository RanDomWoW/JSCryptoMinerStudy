    <script>
        $(document).ready(function () {
            $(".navigation-bar__hamburger-menu-htx").click(function () {
                $(this).toggleClass("navigation-bar__hamburger-menu-htx--is-active");
                var $navigation = $(".navigation-bar__menu-items");
                $navigation.toggleClass("navigation-bar__menu-items--visible");
                if ($navigation.css("flex-wrap") == "nowrap") {
                    setTimeout(function() {
                        $navigation.css("flex-wrap", "wrap");
                    }, 1000);
                } else {
                    $navigation.css("flex-wrap", "nowrap");
                }

            });
            var currentSlider = 0;
            var shouldScroll = true;
            setInterval(function() {
                if (!shouldScroll) {
                    return;
                }
                var $els = $(".jumbotron__tab-right");
                $els.eq(0).trigger("click");
            }, 4500);
            function changeTab(index) {
                $(".slider__block--active").removeClass("slider__block--active");
                $(".slider__block").eq(index).addClass("slider__block--active");
                $(".slider__news-block-active-arrow").css("display", "none");
                $(".slider__news-block").eq(index).find(".slider__news-block-active-arrow").css("display", "block");

            }
            $(".jumbotron__tab-left").click(function(e) {
                currentSlider--;
                if (currentSlider < 0) {
                    currentSlider = $(".slider__block").length - 1;
                }
                changeTab(currentSlider);
            });
            $(".jumbotron__tab-right").click(function(e) {
                currentSlider++;
                if (currentSlider > $(".slider__block").length - 1) {
                    currentSlider = 0;
                }
                changeTab(currentSlider);
                if (typeof e.isTrigger == 'undefined') {
                    shouldScroll = false;
                }
            });
            $('#searchButton').click(function() {
                var $input = $(".navigation-bar__search-input");
                var $inputContainer = $(".navigation-bar__search-container");
                var $searchButton = $(this);
                var $navItems = $(".navigation-bar__menu-items");
                $input.toggleClass('navigation-bar__search-input--active');
                $searchButton.toggleClass('navigation-bar__search--active');
                $navItems.toggleClass('navigation-bar__menu-items--search-active');
                $inputContainer.toggleClass('navigation-bar__search-container--active');
            });
            $('.mg-list').magnificPopup({
                delegate: 'a',
                type: 'image',
                tLoading: 'Loading image #%curr%...',
                mainClass: 'mfp-img-mobile',
                gallery: {
                    enabled: false,
                    navigateByImgClick: false,
                    preload: [0,1] // Will preload 0 - before current, and 1 after the current image
                },
                image: {
                    tError: '<a href="%url%">The image #%curr%</a> could not be loaded.',
                    titleSrc: function(item) {
                        return item.el.data('title');
                    }
                },
                zoom: {
                    enabled: true,
                    duration: 300 // don't foget to change the duration also in CSS
                }
            });
            $(".section__menu-mobile").click(function() {
                $(".section__menu--category").toggleClass("section__menu--category-active");
                $(".section__menu-mobile").toggleClass("section__menu-mobile--active");
            });
        });
    </script>
