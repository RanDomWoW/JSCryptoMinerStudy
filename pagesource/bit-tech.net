    <script>
        window.BITTECH.Urls = {
            top_stories: "/api/functions/get_top_stories/",
            suggested_articles: "/api/functions/get_suggestions/"
        };

        (function($) {
            setTimeout(function(){
                $('#skin').css('height', $(document).height());
                $('style').each(function(){
                    $(this).text(
                        $(this).text().replace('body, body#bit-gamer', '#blurBackground')
                    );
                });
            });
            $("img[src*='featured_double']").each(function(index, value) {
                $(this).error(function() {
                    var img = $(this).clone();
                    var img_container = $(this).closest('.image');
                    var new_src = img.attr("src").replace("featured_double", "index_img");
                    img.attr("src", new_src);
                    $(this).replaceWith(img);
                    if (img_container.css('background-image')){
                        img_container.css('background-image', 'url(' + new_src + ')');
                    }
                });
                $(this).attr("src", function() {
                    return this.src;
                });
            });

            var resManager = new window.BITTECH.ResolutionSwitcher();
            resManager.checkResolution();

            var resizeTimer;

            $(window).resize(function(){
                clearTimeout(resizeTimer);
                resizeTimer = setTimeout(function(){
                    resManager.checkResolution();
                }, 500);
            });

            // short signup script
            function short_signup_redirect(){
                var email = $('#shortSignup input[type="email"]').val();
                window.top.location = '/';
            }

            $('span.send').on('click', function(){
                short_signup_redirect();
            });

            $('#email').keypress(function(event){
                var keycode = (event.keyCode ? event.keyCode : event.which);
                if(keycode == '13'){
                    short_signup_redirect();
