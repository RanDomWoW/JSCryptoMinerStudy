            // function to handle the user being subscribed
            function subscribed(){

                // hide the subscribe to newsletter
                $('#shortSignup').empty().append('<h3 class="headingBar">YOU ARE SUBSCRIBED TO OUR NEWSLETTER</h3>');
            };

            // sign me up click
            $('#signMeUp').click(function(){

                // post to subscribe
