<script type="text/javascript">
	jQuery(document).ready(function($){
		var menuNvUl = $('.bmc-scroll-nav').find('li');
		var totalWidth = 0;
		var maxLi = 0;
		var menuScrolled = 0;
		menuNvUl.each(function(){
			var curLi = $(this).outerWidth();
			totalWidth += curLi;
			if(maxLi < curLi){maxLi = curLi;}
		});
		$('.menu-scroll-nav.btn-prev').on('click',function(e){
			e.preventDefault();
			var current = $(this);
			if(current.hasClass('btn-disable')){return false;}
			menuScrolled -= maxLi;
			if(menuScrolled <= 0){current.addClass('btn-disable');}
			$('.bmc-scroll-nav ul').animate({'margin-left':-menuScrolled},200);
			$('.menu-scroll-nav.btn-nxt').removeClass('btn-disable');
		});
		$('.menu-scroll-nav.btn-nxt').on('click',function(e){
			e.preventDefault();
			var current = $(this);
			if(current.hasClass('btn-disable')){return false;}
			menuScrolled += maxLi;
			var scrollWidth = (totalWidth - $(window).outerWidth());
			if(menuScrolled >= scrollWidth){current.addClass('btn-disable');}
			$('.bmc-scroll-nav ul').animate({'margin-left':-menuScrolled},200);
			$('.menu-scroll-nav.btn-prev').removeClass('btn-disable');
		});
	});
</script>

