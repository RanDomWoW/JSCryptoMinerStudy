	function login(){
			var isV=Validate(TextValidate("uid","帳號必需填寫")
						,TextValidate("pwd","密碼必需填寫")
						,TextValidate("code","驗證碼必需填寫")
						);					   
			if (!isV){return false;}	
			$.post("_adminx/member/action/login.asp",$("#form1").serialize(),function(data){
			if ((data.status == "1" || data.status == "3" || data.status == "4") && data.msg != "成功") {
			    alert(data.msg);
			}
		    if (data.status == "1") {
						parent.location=data.url;
					}else if(data.status=="3"){
						$('#form2').submit();
					}else if(data.status=="4"){
						$('#form2').submit();
					}else{
						var t=Math.random();
						alert(data.msg); $('#uid').val(''); $('#pwd').val(''); $('#code').val(''); $('#imgcode img').attr('src','_adminx/contact/skin/code.asp?t='+t);
						};														 
			},"json");
		
		}
</script>

