<script>
  $(document).ready(function(){
    $("#subscribe").click(function(){
      var email=$("#newsletter-email").val();
      $.ajax({
        url:"https://erasmusplusols.eu/wp/wp-admin/admin-ajax.php",
        type:"POST",
        dataType:"JSON",
        data:{
          action:"wysija_ajax",
          controller:"subscribers",
          ajaxurl:"https://erasmusplusols.eu/wp/wp-admin/admin-ajax.php",
