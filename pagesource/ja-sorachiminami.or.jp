<script type="text/javascript">
$(function(){
	//農産品画像切替
	$(".viewer").each(function () {
		$(this).children("div").hide();
		$(this).children("div:first").stop().fadeIn(2000);
		var that = this;
		setInterval(function(){
            $(that).children("div:first-child").fadeOut(2000)
            .next("div").fadeIn(2000)
            .end().appendTo(that);
		}, 5000);
	});
	$(".viewer2").each(function () {
		$(this).children("div").hide();
		$(this).children("div:first").stop().fadeIn(2000);
		var that = this;
		setInterval(function(){
            $(that).children("div:first-child").fadeOut(4000)
            .next("div").fadeIn(4000)
            .end().appendTo(that);
		}, 7000);
	});
　　function adjust(){
          var height= $('.photo > ul >li:first-child .viewer div:first-child img').height(); //他要素の高さ
          $('.viewer').css('height', height-3);
          $('.viewer2').css('height', height-3); //可変部分の高さを適用
     }
     adjust();
     $(window).on('resize', function(){
	     var win = $(window).width();
		 var p = 768;
		 if(win < p){
	        adjust();
         } else {
	        $('.viewer').css('height', 108);
			$('.viewer2').css('height', 108);
         }
     })

	//スライダー画像読み込み
	$(window).resize(function(){
		var win = $(window).width();
		var p = 768;
		if(win < p){
			$('.slider_sp').fadeIn(1500);
			$('.slider').hide();
		} else {
			$('.slider').fadeIn(1500);
			$('.slider_sp').hide();
		}
	});
	//トップスライダー
	$('.slider').slick({
	  dots: true,
	  autoplay: true,
	  autoplaySpeed: 3000,
	  speed: 2000,
	  fade: true,
	  centerMode: true,
	  infinite: true,
	});
	$('.slider_sp').slick({
	  dots: false,
	  autoplay: true,
	  infinite: true,
	  slidesToShow: 1,
	});
	//タブ切り替え
	$('ul.tabs li').click(function(){
        var tab_id = $(this).attr('data-tab');
        $('ul.tabs li').removeClass('current');
        $('.tab_content').removeClass('current');
        $(this).addClass('current');
        $("#"+tab_id).addClass('current');
    })
    //クリッカブルマップ
    $('img[usemap]').rwdImageMaps();
});
</script>


