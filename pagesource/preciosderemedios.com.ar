	<script type="text/javascript">
		
		$(document).ready(function(){
			
			$('input[name="pattern"]').autocomplete('search_pattern_autocomplete.php', {
				width: 325,
				delay: 800,
				multiple: false,
				max: 20,
				minLength: 3,
				matchContains: true,
				formatItem: formatItem,
				formatResult: formatResult,
				extraParams: {item: function(){ return($('input[name="item"]:checked').val())} }
			});
			
			$('input[name="pattern"]').result(function(event, data, formatted) {
				
				// En data tengo lo siguiente
				// [0] - La descripcion
				// [1] - Id del patron seleccionado
				var hidden= $('input[name="id_pattern_select"]') ;
				// Esto si se busca solo una clave
				hidden.val(data[1]) ;
				
				// Esto va si es multiple
				// hidden.val( (hidden.val() ? hidden.val() + ";" : hidden.val()) + data[1]);
			});
			
		})
		
		function formatItem(row) {
			/*return row[0] + " (<strong>id: " + row[1] + "</strong>)" ;*/
			return row[0];
		}
		function formatResult(row) {
			return row[0].replace(/(<.+?>)/gi, '');
		}
		
		function check(sTagInput) {
		
			var sPattern= $('input[name="'+sTagInput+'"]').val() ;
			
			if ( sPattern=='' ) {
				alert('Ingresar un patron de b&uacute;squeda') ;
				return(false) ;
			}
			else {
				// sPattern= urlencode(sPattern) ;
				// $('input[name="'+sTagInput+'"]').val(sPattern) ;
			}
			return(true) ;
		}
		
	</script>
	
