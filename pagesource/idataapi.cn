<script>
	$(function(){
		var count = 1;
		var liArr = $(".slider-ul li");
		var spanArr = $(".carousel-indicators li")
		var timer = setInterval(function () {
				if(count % 3 == 0){
					count = 0;
				}
				liArr.eq(count).fadeIn().siblings().fadeOut();
				spanArr.eq(count).addClass("on").siblings().removeClass("on");
				count++
				},5000);
		$(".carousel-indicators li").on("click",function(){
				var index = $(this).index();
				$(this).addClass("on").siblings().removeClass("on");
				liArr.eq(index).fadeIn().siblings().fadeOut();
				count = index;
		});
		$(".bd").on("mouseover",function(){
			clearInterval(timer);
		});
		$(".bd").on("mouseout",function(){
			timer = setInterval(function () {
				if(count % 3 == 0){
					count = 0;
				}
				liArr.eq(count).fadeIn().siblings().fadeOut();
				spanArr.eq(count).addClass("on").siblings().removeClass("on");
				count++
				},5000);
		});
		var flag = true;
		var index = 0;
        var item_arr = $(".slider_item");
        $(".partners_next").on("click",function(){
            if(flag){
                flag = false
                $(item_arr[index]).animate({'left':'-100%'},800);
                if(index >= 4){
                    index = 0;
                    $(item_arr[index]).css({'left':'100%'});
                    $(item_arr[index]).animate({'left':0},800,function(){
                        flag = true;
                    });

                }else{
                    index++;
                    $(item_arr[index]).css({'left':'100%'});
                    $(item_arr[index]).animate({'left':0},800,function(){
                            flag = true;
                    });
                    $(this).attr("index",index);
                }
            }
        });
        $(".partners_prev").on("click",function(){
                if(flag){
                    flag = false
                    $(item_arr[index]).animate({'left':'100%'},800);
                    if(index <= 0){
                        index = 4;
                        $(item_arr[index]).css({'left':'-100%'});
                        $(item_arr[index]).animate({'left':0},800,function(){
                            flag = true;
                        });
                    }else{
                        index--;
                        $(item_arr[index]).css({'left':'-100%'});
                        $(item_arr[index]).animate({'left':0},800,function(){
                                flag = true;
                        });
                    }
                }
            });

	});

</script>
