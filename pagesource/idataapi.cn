        function next_guide(obj,lastguide,status,callback){
            if(status == 1){
                obj.html("");
                $(".guide_bg").addClass("hidden");
            }else{
                $('body,html').animate({scrollTop:0},500);
                $(".guide_bg").removeClass("hidden");
                obj.hide();
                //obj.eq(0).show();
                obj.each(function(i,dom){
                    if($(dom).attr("rel") == 1){
                        $(dom).show();
                    }
                });
                obj.click(function(){
                    var rel = $(this).attr("rel");
                    rel++;
                    $(this).html("");
                    //obj.eq(rel).show();
                    obj.each(function(i,dom){
                        if($(dom).attr("rel") == rel){
                            $(dom).show();
                        }
                    })
                    if( rel == obj.length+1 ){
                        send_notice(lastguide);
                        if(callback){
                            callback();
                        }
                    }
                });
            };
            obj.find("i").click(function(){
                obj.html("");
                send_notice(lastguide);
                if(callback){
                    callback();
                }
            });

        };
        function send_notice(lastguide){
            $.ajax({
                url:"/center/data",
                type:"post",
                dataType:"json",
                data:{act:"operate_notice",lastguide:lastguide},
                success : function(data){
                    $(".guide_bg").addClass("hidden");
                    $('body,html').animate({scrollTop:0},500);
                }
            });
        }
	</script>

