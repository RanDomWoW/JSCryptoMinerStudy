<script>
			
			var oldUrl =  '';
			if(typeof dynurl === 'undefined'){
				var oldUrl =  window.location.href;
			}else{
				oldUrl = dynurl;
			}
			
            var myApp = angular.module('myApp', ['infinite-scroll']);
            myApp.controller('myCtrl', function ($scope, News) {
                $scope.newsModel = new News();
            });
            // Reddit constructor function to encapsulate HTTP and pagination logic
            myApp.factory('News', function ($http) {
                var News = function () {
                    this.items = [];
                    this.busy = false;
                    this.loading = false;
                    this.empty = false;
                    this.page = 1;
                };
                News.prototype.nextPage = function () {
					
                    if (this.busy){
							return;
					}
											
					// do a reset if the dynamic url has been changed
					if(typeof dynurl !== 'undefined' && dynurl != oldUrl){

						this.page = 1;
						oldUrl = dynurl;
					}
                        
                    this.busy = true;
                    this.loading = true;
					
                    // $http.post(url, {page: this.page})
                    $http({
                        method: 'POST',
													url: window.location.href,
						                        data: $.param({page: this.page, offset:0, float_index:1}),
                        headers: {'Content-Type': 'application/x-www-form-urlencoded'},
                    }).success(function (data) {

                        this.loading = false;
                        if (data == '') {
                            if (this.page == 1) {
                                this.empty = true;
                            }
                        } else {

                            $('.news-block').append(data);
                            this.page++;
                            this.busy = false;
                        }
                    }.bind(this));
                };
                return News;
            });


</script>
