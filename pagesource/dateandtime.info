	$(document).ready(function(){			
		// Продлеваем разделительную полосу до конца левого поля (#east_side)		
		var west_side_height = $("#west_side").height();
		// 256 - высота социального плагина Фейсбука (не учитывается в обычной высоте, так как подгружается асинхронно)
		var east_side_height = $("#east_side").height() + 256;
		if (west_side_height > east_side_height) {
			$("#east_side").height(west_side_height);
		}				
         // On entering and leaving any search field set and reset background color
		$(".search input[type='text']").focus(function(){  
			$(this).css ("background-color", "#f0f0f0");
		});
		$(".search input[type='text']").blur(function(){  
			$(this).css ("background-color", "#ffffff");
		});
		// После выбора типа поиска показываем соответствующую форму, остальные прячем
		$("#search_select select").change(function () {
			searchtype = $(this).val();		
			switch (searchtype) {
				case "citytime":
					$("#search_city").show();
					$("#search_country").hide();
					$("#search_difference").hide();
					$("#search_city_coord").hide();
					$("#search_distance").hide();
					$("#search_driving").hide();
					break;
				case "countrytime":
					$("#search_city").hide();
					$("#search_country").show();
					$("#search_difference").hide();
					$("#search_city_coord").hide();
					$("#search_distance").hide();
					$("#search_driving").hide();
					break;
				case "timedifference":
					$("#search_city").hide();
					$("#search_country").hide();
					$("#search_difference").show();
					$("#search_city_coord").hide();
					$("#search_distance").hide();
					$("#search_driving").hide();
					break;
				case "coordinates":
					$("#search_city").hide();
					$("#search_country").hide();
					$("#search_difference").hide();
					$("#search_city_coord").show();
					$("#search_distance").hide();
					$("#search_driving").hide();
					break;
				case "distance":
					$("#search_city").hide();
					$("#search_country").hide();
					$("#search_difference").hide();
					$("#search_city_coord").hide();
					$("#search_distance").show();
					$("#search_driving").hide();
					break;
				case "directions":
					$("#search_city").hide();
					$("#search_country").hide();
					$("#search_difference").hide();
					$("#search_city_coord").hide();
					$("#search_distance").hide();
					$("#search_driving").show();
					break;
			}
		});
	});
