		$(".right").stop();
		$("#btn").stop();
		$("#btn-end").stop();
		$(".left").animate({"left" : "337px"}, 1000);
		$("#btn").animate({"opacity" : "0"}, 1000);
		$(".right").animate({"right" : "337px"}, 1000);
	}

	function out(){
		$(".left").stop();
		$(".right").stop();
		$("#btn").stop();
		$("#btn-end").stop();
		$("#btn").animate({"opacity" : "1"}, 1000);
		$(".left").animate({"left" : "2px"}, 1000);
		$(".right").animate({"right" : "2px"}, 1000);
	}

	$(document).ready(function(){
		$("#arrows").on("mouseover", hover);
		$("#arrows").on("mouseout", out);
	});
