for ( var d = 0; d < depoimentos.length; d++ ){
	var a = document.createElement("a");
	a.setAttribute("href", d);
	a.setAttribute("data-ativo", (d == 0) ? 1 : 0);
	a.innerHTML = "&nbsp;";
	a.addEventListener("click", function(e){
		e.preventDefault();
		this.blur();
		var depoimentos = document.querySelectorAll("#avaliacoes_lista li");
		for ( var d = 0; d < depoimentos.length; d++ ){
			if ( d != this.getAttribute("href")){
				document.querySelector("#depoimentos_controle a[href='"+d+"']").setAttribute("data-ativo", 0);
				depoimentos[d].style.display = "none";
			}
			else {
				document.querySelector("#depoimentos_controle a[href='"+d+"']").setAttribute("data-ativo", 1);
				depoimentos[d].style.display = "list-item";
			}
		}
	});
	document.getElementById("depoimentos_controle").appendChild(a);
}
</script>
