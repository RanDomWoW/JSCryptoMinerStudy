<script type="text/javascript">
	function toTitleCase(str){
	return str.replace(/\w\S*/g, function(txt){return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase();});
}

function sugerirBusca(formId){
	var camposBusca = document.querySelectorAll(formId+" input[type='search']");
	if ( ! camposBusca ){
		return;
	}
	for ( var c = 0; c < camposBusca.length; c++ ){

		camposBusca[c].addEventListener("blur", function(event){
			var list = this.getAttribute("data-list");
			setTimeout(function(list){
				var datalist = document.getElementById(list);
				while (datalist.firstChild) {
					datalist.removeChild(datalist.firstChild);
				}
			}, 500, list);
		});

        var searchTimeout = null;
		camposBusca[c].addEventListener("input", function(event){
			var list = this.getAttribute("data-list");
			var datalist = document.getElementById(list);
			if ( this.value == "" ) {
				while (datalist.firstChild) {
					datalist.removeChild(datalist.firstChild);
				}
				return;
			}
			switch ( this.getAttribute("name") ){
				case "escola":
					var campo = 0;
				break;
				case "bairro":
					var campo = 1;
				break;
				case "municipio":
					var campo = 2;
				break;
				default:
					return;
				break;
			}

            if (searchTimeout != null) {
                clearTimeout(searchTimeout);
            }

            var that = this;

			searchTimeout = setTimeout(function(){
                if (that.value.length > 1){
                    $.get("/escola/xhr_sugerir_busca", {'campo': campo, q: that.value}, function(data){
        				if ( data.done ){
        					while (datalist.firstChild) {
        						datalist.removeChild(datalist.firstChild);
        					}
        					if ( campo == 0 ){
        						for ( var i = 0; i < data.datalist.length; i++ ){
        							var a = document.createElement("a");
        							a.setAttribute("href", "/escola/"+data.datalist[i][1]);
        							a.appendChild(document.createTextNode(toTitleCase(data.datalist[i][0])));
        							var li = document.createElement("li");
        							li.appendChild(a);
        							datalist.appendChild(li);
        						}
        					}
        					else {
        						for ( var i = 0; i < data.datalist.length; i++ ){
        							var a = document.createElement("a");
        							a.setAttribute("href", toTitleCase(data.datalist[i]));
        							a.appendChild(document.createTextNode(toTitleCase(data.datalist[i])));
        							a.addEventListener("click", function(event){
        								event.preventDefault();
        								document.querySelector("input[type='search'][data-list='"+list+"']").value = this.getAttribute("href");
        							});
        							var li = document.createElement("li");
        							li.appendChild(a);
        							datalist.appendChild(li);
        						}
        					}
        				}
        			}, "json");
                }
            },700);

		});
	}
}

sugerirBusca("#escola_busca_interno");
sugerirBusca("#escola_busca");
</script>

