		$("#nav").slideToggle(200);
		$(this).toggleClass("active");
	});



	$(".tab-item").css("display","none");
	$("#nav li").hover(function(){
		var is_full_size = $(this).find("img:visible").length;
		if(is_full_size){
			$(this).find(".norm").css("display","none");
			$(this).find(".hov").css("display","block");
			var name="#"+$(this).attr('id')+"2";
			$(".tab-item").css("display","none");
			if ($(name).length > 0 ) {
				$(name).css("display","block");
				$("#navopen").css("display","block");
			}
		}
	},function(){
	  $(this).find(".hov").css("display","none");
	  $(this).find(".norm").css("display","block");
	  $("#navopen").css("display","none");
	});

	$("#navopen").hover(function(){
	  $(this).css("display","block");
	},function(){
	  $(".tab-item").css("display","none");
	  $(this).css("display","none");
	});




	$('#headsup').flexslider({
		animation: "slide",
