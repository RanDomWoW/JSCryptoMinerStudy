        function BeforeAjaxFormValidationQC(form, options)
            {
                 if (window.console){
                     
                     
                     $("#formnl,#formenquiry,#formbook").bind("jqv.field.result", function(event, field, errorFound, prompText){ if(errorFound) return false;});
                    
                    if(form.attr('id')=='formnl')
					{
					form.find(":submit").html("Subscribe<i class='fa fa-spinner'></i>").attr('disabled','disabled');
					}
					if(form.attr('id')=='formbook')
					{
					form.find(":submit").html("<i class='fa fa-spinner'></i>  Send Message").attr('disabled','disabled');					
					}
					if(form.attr('id')=='formenquiry')
					{
					form.find(":submit").html("<i class='fa fa-spinner'></i>  Send Message").attr('disabled','disabled');					}
                    
                    
                    

                
        //alert("Right before the AJAX form validation call");
                 
                 }
     
    return true;
            }
            function ajaxValidationCallbackQC(status, form, json, options) {
                



            if(status == true) 
            {
                
				if(form.attr('id')=='formnl')
				{
					form.find(":submit").html("Subscribe").removeAttr('disabled');	
				}
                    
                if(form.attr('id')=='formenquiry')
				{
					form.find(":submit").html("<i class='fa fa-paper-plane'></i>  Send Message").removeAttr('disabled');				}
				if(form.attr('id')=='formbook')
				{
					form.find(":submit").html("<i class='fa fa-paper-plane'></i>  Send Message").removeAttr('disabled');				}
                    
                 
            
                if(json.code=='1')
                {
							$('div#myModal,div#myModal1').find('button.close').click();
                        
                            $(':input', "#"+form.attr('id')).not(':button, :submit, :reset, :hidden, :radio').val('').removeAttr('checked').removeAttr('selected');
            
                        
                }
                alert(json.msg);
                
                 
                
              }
              
              
             }
            $("#formnl,#formenquiry,#formbook").validationEngine({
                onBeforeAjaxFormValidation : BeforeAjaxFormValidationQC,        
                ajaxFormValidation : true,
                ajaxFormValidationMethod : 'post',
                ajaxFormValidationURL : 'php/',
                onAjaxFormComplete : ajaxValidationCallbackQC,
                scroll : false
            }); 
        
        </script>

    

