);

$("#datepickerDateFrom, #datepickerDateTo").focus(function() {
    removeSearchFormErrorMessage("#checkin");
});

$('#homeSearchForm').submit(
    function() {

        var formIsValid = true;

        removeSearchFormErrorMessage("#city");

        if ($("#guests_select").val() > registry.get('forceGroupWhenGuestCountMoreThan') && !validateGroupsFields()) {
            formIsValid = false;
        }

        //Show error messages below elements if the country search is active, otherwise above
        if ($("#country_input").hasClass("search-active")) {
            var whereToShowErrors = 'bottom';
        } else {
            var whereToShowErrors = 'above';
        }

        if ($("#datepickerDateFrom").val() != "" && $("#datepickerDateTo").val() == "") {
            // must select both dates
            addSearchFormErrorMessage("Please select check out date.", $("#checkin"), whereToShowErrors);
            formIsValid = false;
        } else if ($("#datepickerDateTo").val() != "" && $("#datepickerDateFrom").val() == "") {
            // must select both dates
            addSearchFormErrorMessage("Please select check in date.", $("#checkin"), whereToShowErrors);
            formIsValid = false;
        } else {
            removeSearchFormErrorMessage("#checkin");
        }

        // if none of country, city or free text has been used
        // then show an error message about locations
        if ($("#country_input").hasClass("search-active") && $("#country_input").val() == "") {
            formIsValid = false;
            addSearchFormErrorMessage("To view prices and availability, please select a destination.", $("#country"), "bottom");
        } else if (!$("#country_input").hasClass("search-active") && ($("#freesearch_input").val() == "" || $("#freesearch_input").val() == $("#freesearch_input").attr("defaultValue"))) {
            formIsValid = false;
            addSearchFormErrorMessage("To view prices and availability, please select a destination.", $("#country"), "bottom");
        } else {
            removeSearchFormErrorMessage("#country");
        }

        // if the form is valid and we're about to submit it then replace the real
        // date picker fields with new form elements containing non-localised dates
        // and put in some divs that look like the previous inputs so that the date
        // format doesn't change and scare US visitors
        if (formIsValid) {
            var dateFrom = $.datepicker.formatDate("yy-mm-dd", $("#datepickerDateFrom").datepicker("getDate"));
            var dateTo = $.datepicker.formatDate("yy-mm-dd", $("#datepickerDateTo").datepicker("getDate"));

            var fromName = $("#datepickerDateFrom").attr("name");
            var toName = $("#datepickerDateTo").attr("name");

            $("#datepickerDateFrom").after(
                "<input type=\"hidden\" name=\"" + $("#datepickerDateFrom").attr("name") + "\" value=\"" + dateFrom + "\"/>"
            );
            $("#datepickerDateTo").after(
                "<input type=\"hidden\" name=\"" + $("#datepickerDateTo").attr("name") + "\" value=\"" + dateTo + "\"/>"
            );
            $("#datepickerDateFrom").replaceWith("<div class=\"fakeDateSelector\">" + $("#datepickerDateFrom").val() + "</div>");
            $("#datepickerDateTo").replaceWith("<div class=\"fakeDateSelector\">" + $("#datepickerDateTo").val() + "</div>");
        }

        if (formIsValid && $(this).attr('selectedTarget')) {

            var dateFrom = $('input[name=dateFrom]').val();
            var dateTo = $('input[name=dateTo]').val();
            var guestCount = $('#guests_select').val();
            var homepageSearchType = $("#homepageSearchType").val();


            if (dateFrom != '' && dateTo != '') {
                params = '?dateFrom=' + dateFrom + '&dateTo=' + dateTo + '&guestCount=' + guestCount;
            } else {
                params = '?guestCount=' + guestCount;
            }

            params += '&homepageSearchType=' + homepageSearchType;

            if (guestCount > registry.get('forceGroupWhenGuestCountMoreThan')) {
            	var ageRanges = [];
                $('.ageRange:checked').each(function(){
                    ageRanges.push('ageRanges[]=' + $(this).val());
                });
                ageRanges = ageRanges.length > 0 ? '&' + ageRanges.join('&') : '';

                params += '&groupType=' + $('#groupTypesSelect').val() + ageRanges;
            }

            window.location = $(this).attr('selectedTarget') + params;
            return false;
        }
        return formIsValid;
    }
);

