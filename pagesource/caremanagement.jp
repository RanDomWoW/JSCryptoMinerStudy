var marker_index;
function map_marker() {
	marker_index = new Array();

	ymap.clearFeatures();
	for (var i=0; i<contents.length; i++){
		marker_index[contents[i].lat+','+contents[i].lng] = i;
		var marker = new Y.Marker(new Y.LatLng(contents[i].lat, contents[i].lng), {title:contents[i].name});

				marker.bind('click', function(){
			is_maker_click = true;

			var index = marker_index[this.getLatLng()];
			if (contents[index].cbp_id > 0) {
				location.href = '/?action_snavi_detail=true&cat='+contents[index].servicecd+'&id='+contents[index].business_id+'&plan=g';
			} else if (contents[index].carep > 0) {
				location.href = '/?action_snavi_detail=true&cat='+contents[index].servicecd+'&id='+contents[index].business_id+'&plan=c';
			} else {
				location.href = '/?action_snavi_detail=true&cat='+contents[index].servicecd+'&id='+contents[index].business_id+'&plan=f';
			}
		});
				ymap.addFeature(marker);

				marker_index[marker.getLatLng()] = i;
	}
}
</script>


