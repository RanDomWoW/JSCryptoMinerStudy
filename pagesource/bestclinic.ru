        if ( $(window).scrollTop() <= 32 ){                   // ставим условие
            $('html.landscape.mobile .footer_device_mobile').css('display','none');         // определяем действие
            $('html.portrait.mobile .footer_device_mobile').css('display','none');         // определяем действие
        }
    });
</script>
</body>
</html>
