<script type="text/javascript">
$(function() {
  oldPage = 1;
  function getList( page ) {
    if(page == null) {
      page = 1;
    }
    $.ajax({
      type: 'GET',
      dataType: 'json',
      url: "/talent/happybirthday/"+page,
      success: function(data)
      {
        $('#birthdaylist').html('');
        var list = [];
        for(var i = 0; i < data.rows.length; i++) {
          var html = [];
          html.push('<li><span class="font_L">');
          html.push('<a href="/search/talent.html?id='+data.rows[i].id+'">'+data.rows[i].name+'</a>');
          html.push('</span>');
          //html.push('（'+data.rows[i].nameKana+'）');
          html.push('</li>');
          list.push(html.join(''));
        }
        $('#birthdaylist').html(list.join(''));
        oldPage = Number(data.page);
        if(data.page > 1) {
          $('#birthday-show-prev').css('display', 'block');
        }
        else {
          $('#birthday-show-prev').css('display', 'none');
        }
        if(data.page < data.total) {
          $('#birthday-show-next').css('display', 'block');
        }
        else {
          $('#birthday-show-next').css('display', 'none');
        }
      }
    });
  }
  getList();

  $('#birthday-show-prev a').click(function() {
    getList(oldPage-1);
  });
  $('#birthday-show-next a').click(function() {
    // getList(oldPage+1);
    location.href="/talent/birthday";
  });

});
</script>
