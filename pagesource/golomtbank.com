<script type="text/javascript">
#var set1= new session();
function    setCookie(c_name,value,exdays) {
    var exdate=new Date();
    exdate.setDate(exdate.getDate() + exdays);
    var expires = exdate.toUTCString();
    var isIE8 = (document.documentMode !== undefined);
    if (exdays == 0) {
        expires = (isIE8 == true) ? "" : "0";
    }
    var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+expires);
    document.cookie=c_name + "=" + c_value;
}
function session() {
    if (document.cookie.indexOf("visited") >= 0) {
        window.location= "https://www.golomtbank.com/home/citizen";
    } else { window.location= "https://www.golomtbank.com";
        setCookie('visited','true',999);
    }
}

</script>

