<script>
    $(function() {
        var _width = $(window).width();
        var _body  = _width+250+'px';

        $(".left_menu_btn").click(function(){
            $('.black_bg').css({'display':'block'});
            $('.m_nav_box').css({'display':'block', 'z-index':'102'});
            $('.page').css({'transform':'translate3d(250px,0,0)',
                '-wibkit-transform':'translate3d(250px,0,0)',
                '-moz-transform':'translate3d(250px,0,0)', 'position':'fixed'});
            $('body').css({'overflow':'hidden'});
            $('.black_bg').unbind();
            $('.black_bg').on('click', function(){
                $('.black_bg').css({'display':'none'});
                $('.m_nav_box').css({'display':'none', 'z-index':'0'});
                $('.page').css({'transform':'none', '-wibkit-transform':'none', '-moz-transform':'none', 'position':'relative'});
                $('body').css({'overflow':'auto'});
                $('.black_bg').unbind();
            });
        });

        // 登入
        var login_panel = $('#m_login_panel');
        login_panel.css('left', _width);

        $('.m_button_show_login_panel').on('click', function() {
            prepare_dialog_bg();

            login_panel.show();
            login_panel.animate({
                left: "0"
            }, 1000, function() {
                // Animation complete.
            });
        });

        // 選擇語言
        var language_panel = $('#m_language_panel');
        language_panel.css('left', _width);

        $('.m_button_show_language_panel').on('click', function() {
            prepare_dialog_bg();

            language_panel.show();
            language_panel.animate({
                left: "0"
            }, 1000, function() {
                // Animation complete.
            });
        });

        // 檢查訂閱電子報 E-mail 格式
        var emailInput      = $('#mobileOrderEpaperForm input[name="orderEmail"]');
        var emailExpression = new RegExp(/^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);
        var submitButton    = $('#mobileOrderEpaperForm button');

        emailInput.on('keyup paste', function(evt) {

            if (evt.target.value.match(emailExpression) == null) {
                submitButton.hide();
            } else {
                submitButton.show();
            }
        });

        // (輪播)連結..Mobile 中顯示列表
        var m_link_area = $('#m_linkArea');
        m_link_area.css('left', _width);

        $('.m_button_show_links_panel').on('click', function() {
            prepare_dialog_bg();

            m_link_area.show();

            m_link_area.animate({
                left: "0"
            }, 1000, function() {
                // Animation complete.
            });
        });

        // 關閉開啟的 panel / dialog
        $('.button_close_panel').on('click', function(evt) {
            var target = $(evt.target).parent();

            $('body').css('overflow','auto');

            target.animate({
                left: _width
            }, 1000, function() {
                target.hide();
            });
        });

        // 全部能展開的 menu_item, 點下去能定住
        $('.m_nav_box').find('.slide_item.open').on('click', function() {
            $(this).toggleClass('now');
            if(!$(this).hasClass('now')){
                $(this).find('.layer_2').hide();
            }
            else{
                $(this).find('.layer_2').show();
            }
        });
    });

    function prepare_dialog_bg() {
        $('.black_bg').css({'display':'none'});
        $('.m_nav_box').css({'display':'none', 'z-index':'0'});
        $('.page').css({'transform':'none', '-wibkit-transform':'none', '-moz-transform':'none', 'position':'relative'});
        $('body').css({'overflow':'hidden'});
        $('.black_bg').unbind();
    }

</script>

    
    
