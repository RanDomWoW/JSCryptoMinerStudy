                                prevArrow: '<div class="lbtt" title="向左輪播"></div>',
                                nextArrow: '<div class="rbtt" title="向右輪播"></div>',
                                slidesToShow: 3,
                                slidesToScroll: 1
                            });
                        }
                    }
                });

            });

            
            var bLazy;
            $(document).queue("ajaxRequests", function() {
                bLazy = new Blazy({
//                    loadInvisible: true
                });
                $(document).dequeue("ajaxRequests");
            });

            
            $(document).dequeue("ajaxRequests");

//            view_slick();

//            $(window).resize(view_slick);

        });
        var scitechVistaData;
        
        function view_slick(){
            var window_width = $(window).width();
            $('#scitech_view .arr_btn').html("");
            //科技大觀園
            if( is_mobile() || window_width <= 900) {
                $('#scitech_view').find('.rd_content').slick({
                    lazyLoad: 'ondemand',
                    appendArrows: $('#scitech_view .arr_btn'),
                    prevArrow: '<div class="aerl" title="向左輪播"></div>',
                    nextArrow: '<div class="aerr" title="向右輪播"></div>',
                    slidesToShow: 1,
                    slidesToScroll: 1
                });
            }
            else{
                $('#scitech_view').find('.rd_content').slick({
                    lazyLoad: 'ondemand',
                    appendArrows: $('#scitech_view .arr_btn'),
                    prevArrow: '<div class="aerl" title="向左輪播"></div>',
                    nextArrow: '<div class="aerr" title="向右輪播"></div>',
                    slidesToShow: 3,
                    slidesToScroll: 1
                });
            }
        }

    function generateScitechVistaData(){
		$("#scitech_view .ten_tabar button").removeClass("now");
		$("#scitech_view .ten_tabar button:first-child").addClass("now");

		$("#scitech_view .artar div").html('<ul class="clearfix rd_content" style="width: 100%;"></ul>');
    	for (var key in scitechVistaData){
            if (key != "熱門文章") {
        		resetScitechVistaData(scitechVistaData[key], key);
    		}
    	}
		view_slick();
	}
	function resetScitechVistaData(data, filter){
			for(var key in data){
				var value = data[key];

                var $row = $($("#reContentTemplate").html());
                $row.find(".picarr").attr("href", value.url);
//                $row.find(".picarr").children("img").attr("src",value.image_url);
                $row.find(".picarr").children("img").attr("data-lazy",value.image_url);
                $row.find(".picarr").children("img").attr("alt",value.title);
                $row.find(".catag").html(filter);
                $row.find(".dattt").html(value.activity_date);
                $row.find(".tie").attr("href",value.url);
                $row.find(".tie").html(value.title);
                $("#scitech_view .rd_content").append($row);
			}
	}

	function filterScitechVista(obj, nowcatag){
	    if(!$(obj).hasClass("now")){
			$("#scitech_view .ten_tabar button").removeClass("now");
			$(obj).addClass("now");

			$("#scitech_view .artar div").html('<ul class="clearfix rd_content" style="width: 100%;"></ul>');
	        resetScitechVistaData(scitechVistaData[nowcatag], nowcatag);
			view_slick();
        }
	}

        
        
        function is_mobile() {
            if ( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
                if ($(window).width() <= 768) {
                    return true;
                }
            }

            return false;
        }
        function getCookie(w){
            cName = "";
            pCOOKIES = new Array();
            pCOOKIES = document.cookie.split('; ');
            for(bb = 0; bb < pCOOKIES.length; bb++){
                NmeVal  = new Array();
                NmeVal  = pCOOKIES[bb].split('=');
                if(NmeVal[0] == w){
                    cName = unescape(NmeVal[1]);
                }
            }
            return cName;
        }
    </script>
