                                var uid      = content.articleUid;
                                var link_url;

                                if (content.articleContentType == 'U') {
                                    var link_root = content.articleLinks[0].link;

                                    if (link_root.isOpenNewWindow == 'Y') {
                                        link_url = ' target="_blank" href="'+link_root.linkUrl+'"';
