                    var root_url = $('.rd_news_container').data('root');
                    var get_size = ($('#wacomic:visible').length > 0 )?4:3;

                    // 發送 ajax
                    $.ajax({
                        url     : now_url,
                        data    : 'size='+get_size,
                        type    : "GET",
                        dataType: 'json',
                        success : function(response) {
                            var show_content = '';
                            var data_arr     = response.result.taxonomyArticles.result;
                            var i            = 0;
                            var menu_id      = now_dom.data('menu_id'       );

                            $.each(data_arr, function (index, content){
                                if(i > get_size) {return false;}

                                var actions = (content.articleContentType == "P")?'detail' : 'list';
                                var date_string;
                                var taxonomy ;

                                $.each(content.articleTaxonomies, function(i, con){

                                    if(con.taxonomy.isVisible == "Y" && con.taxonomy.taxonomyType == "T") {
                                        taxonomy = con.taxonomy;
                                        return false;
                                    }
