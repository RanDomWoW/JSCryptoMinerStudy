if(($('#calc_params').val()=='Количество страниц: 20; Способ скрепления: скрепка; Цвет обложки: белая;') && (!(/pokaz_nzurn/.test(document.cookie))))
{

	$("#nuzhiy").show(300);
}
}

function nz_close()
{
	
	var date = new Date(); 
	date.setTime(date.getTime() +86400000); 
    document.cookie = "pokaz_nzurn" + "=no; expires=" + date.toUTCString(); //d.toUTCString()
	$("#nuzhiy").hide(300);
}



