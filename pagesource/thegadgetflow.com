            var gf_registration = jQuery(this).serialize();
            jQuery('.gfl-popup-form-spinner').fadeIn();
            jQuery.ajax({
                type: "POST",
                url: "/wp-admin/admin-ajax.php",
                data: gf_registration,
                success: function (results) {
