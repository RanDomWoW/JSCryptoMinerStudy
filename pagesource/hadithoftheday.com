/* Bottom Bar CSS */
#bottomBar{
	display:none;
	z-index:1000;
}

.site-message {
    background-color: #ffce4b;
    display: block;
    color: #ffffff;
	position:fixed;
	bottom:0;
}

.gs-container{
	position: relative;
    margin: 0 auto;	
}

.site-message__inner {
    display: table;
    padding: 0 $gs-gutter/2;
    width: 100%;
	padding-top: 0.3125rem;       
    padding-bottom: 0.625rem;
}

.site-message__message{	
	font-weight:900;
	color: #333333;
    fill: #333333;
	font-family: 'Merriweather', serif;
	margin-right: 0.625rem;	
}

.site-message__media img{
	width: 36px;
	max-width:none !important;
}

.site-message__copy {
    position: relative;
    display: table-cell;
    vertical-align: middle;
}


.membership__support-button{	
	background-color: #ff9b0b;
	border-radius: 62.5rem;
    border: 0;
    display: inline-block;
    width: auto;    
    position: relative;
    cursor: pointer;
    font-weight: bold;
	vertical-align:text-top;
}

.membership-prominent-yellow
{
	font-family: 'PT Sans', sans-serif;
	color: #333333;
    fill: #333333;
    border-color: #333333;
	text-decoration:none;
	line-height: 2rem;	
	font-size: 0.875rem;
    height: 2.25rem;       	
	position:relative;
	font-weight:100;
}

.inline-arrow-black-right{
    display: block;
    float: right;
    height: 1.875rem;
	position: relative;
}

.site-message__close{	
	display: table-cell;
    vertical-align: top;
    padding-bottom: 0.3125rem;
    padding-top: 0.3125rem;
}

.site-message__close-btn{
	cursor:pointer;
	font-size: 0.8125rem;
    line-height: 1.125rem;
    font-family: 'PT Sans', sans-serif;
    -webkit-border-radius: 62.5rem;
    border-radius: 62.5rem;
    display: block;
    padding: 0;
    height: 2rem;
    width: 2rem;
    background: transparent;
    border: 0.0625rem solid rgb(0,0,0);
    color: #333333;
    float: right;
}

@media screen and (min-width:961px)
{
	.gs-container{
		max-width:90%;
	}
	
	.site-message__media{
		display:table-cell;
		padding-top:0.0625rem;
	}

	.site-message__message{
		padding-left: 0.625rem;
	}

	.site-message__inner {
		padding-left: 0.3125rem;
		padding-right: 0.3125rem;
	}

	.site-message__close{
		padding-right: 1.25rem;
	}
	
	.site-message__message{	
		font-size: 1.5rem;
		line-height: 2.5rem;
	}	
	
	.membership__support-button{
		padding-right: 0.625rem;
	}
	
	.membership-prominent-yellow {
		top: -0.3rem;
	}
	
	.inline-arrow-black-right{
		margin-top: 0.2rem;
	}
	
	#message_content{
		margin-right:30px;
	}
	
	.membership__paypal-container{
		display:inline-block;
	}
	
	.membership-prominent-yellow
	{
		padding-left: 0.9375rem;
		padding-right: 0.6375rem;
	}
}

@media screen and (max-width:960px)
{
	.gs-container{
		max-width:95%;
	}
	
	.site-message__media{
		display:none;
	}
	
	.site-message__message{	
		font-size: 0.9rem;
		line-height: 1.4rem;
	}
	
	.membership-prominent-yellow {
		top:-0.3rem;
	}
	
	.inline-arrow-black-right{
		margin-top: -4px;
	}
	
