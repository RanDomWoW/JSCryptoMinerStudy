				$("#FindZoneContainer").show();
				lookupGrowingZone(zipcode, false);
			 }            
			 else
			 {
				$("#ZoneFoundContainer").hide();
				$("#FindZoneContainer").show();
			 }	
		}
		
		function getZipCode()
		{
			jQuery.ajax(
			{	
				url: "https://www.naturehills.com/ZoneFinder/index/zipcode/", 
				type: 'get',
				success: getZipCodeSuccessCallBack
			});
		}
		
        function lookupGrowingZone(zipcode, isManual)
        {
			// Check for empty zipcode just incase the geo plugin call fails
			if(!zipcode.trim())
			{
				eraseCookie('growing_zone');
                eraseCookie('growing_zone_zipcode');
				eraseCookie('growing_zone_letter');
                $("#ZoneNotFoundContainer").hide();
                $("#ZoneFoundContainer").hide();
                $("#FindZoneContainer").show();
				return;
			}
			
            jQuery.ajax({url: "https://www.naturehills.com/ZoneFinder/index/index/?zipcode=" + zipcode, success: function(result){
                var foundZone = result.Zone;

                if(foundZone)
                {
                    $("#customerZone").html(result.ZoneLetter);
                    createCookie("growing_zone", result.Zone, 365);
                    createCookie("growing_zone_zipcode", zipcode, 365);
					createCookie("growing_zone_letter", result.ZoneLetter, 365);
                    $("#FindZoneContainer").hide();
                    $("#ZoneNotFoundContainer").hide();
                    $("#ZoneFoundContainer").show();
                }
                else
                {
                    eraseCookie('growing_zone');
                    eraseCookie('growing_zone_zipcode');
					eraseCookie('growing_zone_letter');
                    $("#ZoneNotFoundContainer").show();
                    $("#ZoneFoundContainer").hide();
                    $("#FindZoneContainer").show();
                }

				if(isManual)
                {
					location.reload();
				}
            }});
        }
        function createCookie(name, value, days) {
            var expires;

            if (days) {
                var date = new Date();
                date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                expires = "; expires=" + date.toGMTString();
            } else {
                expires = "";
            }
            document.cookie = encodeURIComponent(name) + "=" + encodeURIComponent(value) + expires + "; path=/";
        }

        function readCookie(name) {
            var nameEQ = encodeURIComponent(name) + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) === ' ') c = c.substring(1, c.length);
                if (c.indexOf(nameEQ) === 0) return decodeURIComponent(c.substring(nameEQ.length, c.length));
            }
            return null;
        }

        function eraseCookie(name) {
            createCookie(name, "", -1);
        }
    });
</script>
