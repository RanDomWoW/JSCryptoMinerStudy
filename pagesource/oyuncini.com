<script>
	head.ready("script.js",function(){
		var globalSlider = {
			id 		: "main-slider",
			width	: "472",
			height  : "149",
			nav     : function() {
				var direction 	= null;
				var active 		= null;
				var imageWidth 	= null;
				var counter;

				$(".global-slider__nav__arrow").click(function(){
					direction = $(this).attr("direction");
					if ( direction == "left" ) {
						active = $(".global-slider > ul").find(".active").prev();
					} else {
						active = $(".global-slider > ul").find(".active").next();
					}

					$("#" + globalSlider.id + " > ul > li").removeClass("active");

					if ( active.attr("class")  ) {
						active.addClass("active");
					} else if ( direction == "right" ) {  
						$(".global-slider > ul > li").first().addClass("active");
					} else if ( direction == "left" ) {
						$(".global-slider > ul > li").last().addClass("active");
					}
				
				});
			},
			list     : function() {
				var imageWidth 	= null;
				var counter 	= 0;

				$("#" + globalSlider.id + " > ul").css({
				   "width" 	: globalSlider.width 	+ "px",
				   "height"	: globalSlider.height 	+ "px"
				});

				$("#" + globalSlider.id + " > ul > li").first().addClass("active");

				console.log("id: " + globalSlider.id + " - > width: " + globalSlider.width + " - > " + " height: " + globalSlider.height);
			},
			get 	: function() {
				globalSlider.list();
				globalSlider.nav();
			}
		}
		$(document).ready(function(){
			globalSlider.get();
		}); 
	});
</script>					
