				if(!mail_rule.test($("#email").val()) || $("#email").val() == ""){
					$("#email").css("border-color","red");
					valid = false;
				}else {
				   $("#email").css("border-color","");
					valid=true;
				}
				if($("#nom").val() == ""){
					$("#nom").css("border-color","red");
					valid = false;
				}else {
				   $("#nom").css("border-color","");
					valid=true;
				}
				if($("#prenom").val() == ""){
					$("#prenom").css("border-color","red");
					valid = false;
				}else {
				   $("#prenom").css("border-color","");
					valid=true;
				}
			  return valid;
				});
});
</script>
