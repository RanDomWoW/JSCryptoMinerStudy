                                                    filtro_proyecto(lnk);
                                                }

                                                function filtroProyectos(tipo, dificultad, duracion, orden, link, especial) {
                                                    var lnk = getLink(link) + "load=1&tmpl=component";
                                                    filter_tipo = tipo;
                                                    filter_dificultad = dificultad;
                                                    filter_tiempo = duracion;
                                                    filter_especial = especial;

                                                    orden_listado = orden;

                                                    filtro_proyecto(lnk);
                                                }

                                                function filtro_proyecto(linkk) {
                                                    //console.log(linkk+"&tmpl=component&load=1&mcatid="+filter_tp_proyect);
                                                    jQuery('#CONTAINER_AJAX').html("<div style='height:200px;margin: 0 auto;text-align:center;padding-top:180px;'><img src=\'/images/loading.gif\'></div>");
                                                    var url = "";

                                                    if (filter_tipo.length > 0)
                                                        url += "&claid=" + filter_tipo;
                                                    if (filter_especial && filter_especial.length > 0)
                                                        url += "&espid=" + filter_especial;
                                                    if (filter_tp_proyect.length > 0)
                                                        url += "&mcatid=" + filter_tp_proyect;
                                                    if (filter_dificultad.length > 0)
                                                        url += "&mdifid=" + filter_dificultad;
                                                    if (filter_tiempo.length > 0)
                                                        url += "&rango=" + filter_tiempo;
                                                    if (orden_listado.length > 0)
                                                        url += "&orden=" + orden_listado;
                                                    url = linkk + url;

                                                    console.log(url);
                                                    jQuery.ajax({
                                                        url: url,
                                                        cache: false,
                                                        success: function (data) {
                                                            //console.log(data);
                                                            jQuery('#CONTAINER_AJAX').html(data);
                                                        }
                                                    }).fail(function (jqXHR, textStatus) {
                                                        alert('No se pudo completar la operación' + textStatus);
                                                    });
                                                }

