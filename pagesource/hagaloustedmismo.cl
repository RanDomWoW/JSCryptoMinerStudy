                                                function getLink(url) {
                                                    return url + ((url.indexOf('?') >= 0) ? '&' : '?');
                                                }
                                                function rangoTiempoEjecucion(rango, link) {
                                                    var lnk = getLink(link) + "load=1&tmpl=component&claid=";
