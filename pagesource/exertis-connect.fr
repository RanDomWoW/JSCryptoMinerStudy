    jQuery(document).ready(function () {
        var crumb = jQuery("ul.fil-ariane li.ariane_principal");

        var crumb_global = jQuery("#main-content ul.fil-ariane");
        var width_crumb = 0;

        var count_li_crumb_total = crumb.length;
        var count_li_crumb = 1;
        crumb_global.find("li.ariane_principal").each(function(){
            // fil d'ariane = 958px
            width_crumb += jQuery(this).innerWidth();
            if(width_crumb > 948){
                var title_crumb = jQuery(this).find("a").first().html();
                if(count_li_crumb_total == count_li_crumb){
                    console.log(title_crumb);
                    var count_li_crumb_busy = 1;
                    var totalWidthlibusy = 0;
                    crumb_global.find("li.ariane_principal").each(function(){
                        if(count_li_crumb_busy != count_li_crumb_total) {
                            totalWidthlibusy += jQuery(this).innerWidth();
                        }
                        count_li_crumb_busy++;
                    });
                    jQuery(this).css("width",958 - totalWidthlibusy - 40); // les "40px" correspond aux marges
                    jQuery(this).find(".texte-fleche").css("overflow","hidden");
                    jQuery(this).find(".texte-fleche").css("text-overflow","ellipsis");
                    jQuery(this).find(".texte-fleche").css("white-space","nowrap");

                    //jQuery(this).find("img").css("left","-11px");

                }else {
                    jQuery(this).find("a").first().html(jQuery(this).find("a").first().html().trim(title_crumb).substring(0, 4).slice(0, -1) + "...");
                }
            }
            count_li_crumb++;
        });


        crumb.mouseover(function() {
            jQuery(this).find("div.ariane_subnav").show();
            jQuery(this).find(".arrow-crumb.top").show();
            jQuery(this).find(".arrow-crumb.bottom").hide();
        });

        crumb.mouseout(function() {
            jQuery("div.ariane_subnav").hide();
            jQuery(this).find(".arrow-crumb.top").hide();
            jQuery(this).find(".arrow-crumb.bottom").show();
        });

        jQuery("ul.fil-ariane li ul li.more").click(function() {
            jQuery("ul.fil-ariane li ul li.category_more").show();
            jQuery("ul.fil-ariane li ul li.more").hide();
            jQuery("ul.fil-ariane li ul li.less").show();
        });

        jQuery("ul.fil-ariane li ul li.less").click(function() {
            jQuery("ul.fil-ariane li ul li.category_more").hide();
            jQuery("ul.fil-ariane li ul li.less").hide();
            jQuery("ul.fil-ariane li ul li.more").show();
        });
    });
