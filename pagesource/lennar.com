        function getDomain() {
            var parts = window.location.host.split('.');
            parts.splice(0, 1);
            return "." + parts.join(".");
        }

        function replaceURLHost(url, newHost) {
            newHost = newHost.replace("//", "");
            return url.replace(window.location.host, newHost);
        }

        function replaceURLParameter(url, parameter, value) {
            var urlparts = url.split('?');
            if (urlparts.length >= 2) {
                var prefix = encodeURIComponent(parameter) + '=';
                var pars = urlparts[1].split(/[&;]/g);

                for (var i = pars.length; i-- > 0;) {
                    if (pars[i].lastIndexOf(prefix, 0) !== -1) {
                        pars.splice(i, 1);
                    }
                }
                pars.push(encodeURIComponent(parameter) + '=' + encodeURIComponent(value));

                url = urlparts[0] + (pars.length > 0 ? '?' + pars.join('&') : "");
                return url;
            } else {
                return url + '?' + encodeURIComponent(parameter) + '=' + encodeURIComponent(value);
            }
        }
