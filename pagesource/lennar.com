</style><script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/jquery-ui.min.js"></script><script type="text/javascript">var countryList;
	var stateList;
	var rfiRegion = "Us";

	jQuery().ready(function () {
		jQuery("#txtMoveInDate").datepicker();

		jQuery('.ziplink').on("click", function (e) {
			e.stopPropagation();
			e.preventDefault();
			jQuery('#whyZip').show();
		});

		if (typeof (_satellite) !== "undefined") {
			jQuery(".referrerdomain").val(_satellite.getVar("referringDomain"));
			jQuery(".campaign").val(_satellite.getVar("campaignName"));
		}

		GetStates();
		GetCountries();
		PrePopHomeRFIData();

		jQuery('#ddlMarketState').change(function () {
			GetStateCities();
		});

		jQuery('#ddlMarketCity').change(function () {
		    GetMarketCommunities();
		});

		jQuery('#tabUS').click(function () {
			jQuery("#liUS").addClass("selected");
			jQuery("#liInter").removeClass("selected")
			jQuery("#lblZip").text("Zip code*");
			jQuery(".ziplink").show();
			jQuery("#divUSState").show();
			jQuery("#divIntState").hide();
			jQuery("#phCountry").hide();
			rfiRegion = "Us";
		});

		jQuery('#tabInter').click(function () {
			jQuery("#liInter").addClass("selected");
			jQuery("#liUS").removeClass("selected");
			jQuery("#divUSState").hide();
			jQuery("#divIntState").show();
			jQuery("#lblZip").text("Postal code");
			jQuery(".ziplink").hide();
			jQuery("#phCountry").show();
			rfiRegion = "Intl";
		});
	});

	function btnRFINext_Click() {
		if (ValidateHomeRFIForm()) {
			jQuery("#phUserInfo").hide();
			jQuery("#phRequestInfo").show();
			jQuery("#hHeader").text("Request Info - Step 2 of 2");
		}
	}

	function btnRFISubmit_Click() {
	    if (jQuery("#test_email").val() == "") {
	        SubmitHomeRFI();
	    } else {
	        jQuery("#rfiContent").parent().hide();
	    }
	}

	function btnRFIPrevious_Click() {
		jQuery("#phUserInfo").show();
		jQuery("#phRequestInfo").hide();
		jQuery("#hHeader").text("Request Info - Step 1 of 2");
	}

	function GetStates() {
		if (stateList == undefined) {
			var ddlStates = jQuery('#ddlMarketState');
			ddlStates.empty();
			jQuery.ajax({
				url: "/Services/lookups.asmx/GetStates",
				data: "{}",
				type: "POST",
				contentType: "application/json; charset=utf-8",
				dataType: "json",
				success: function (msg) {
					var stateList = JSON.parse(msg.d);
					for (var i = 0; i < stateList.length; i++) {
						var state = stateList[i];
						jQuery('#ddlMarketState').append('<option value="' + state.StateCode + '">' + state.StateName + '</option>');
					}
				},
				error: function (XMLHttpRequest, textStatus, errorThrown) {
					alert(textStatus);
				}
			})
		}
	}

	function GetCountries() {
		if (countryList == undefined) {
			var ddlCountries = jQuery('#ddlCountry');
			ddlCountries.empty();
			jQuery.ajax({
				url: "/Services/lookups.asmx/GetCountries",
				data: "{}",
				type: "POST",
				contentType: "application/json; charset=utf-8",
				dataType: "json",
				success: function (msg) {
					var countryList = JSON.parse(msg.d);
					for (var i = 0; i < countryList.length; i++) {
						var country = countryList[i];
						jQuery('#ddlCountry').append('<option value="' + country.CountryCode + '">' + country.CountryName + '</option>');
					}
				},
				error: function (XMLHttpRequest, textStatus, errorThrown) {
					alert(textStatus);
				}
			})
		}
	}

	function GetStateCities() {
		var stateCode = jQuery('#ddlMarketState').val();
		var ddlCities = jQuery('#ddlMarketCity');
		ddlCities.empty();
		if (stateCode == '') {
			jQuery(ddlCities).attr('disabled', 'disabled');
		}
		else {
			jQuery('#ddlMarketCity').removeAttr('disabled');
			jQuery.ajax({
				url: "/Services/lookups.asmx/GetMarkets",
				data: "{ 'stateCode' : '" + stateCode + "'}",
				type: "POST",
				contentType: "application/json; charset=utf-8",
				dataType: "json",
				success: function (msg) {
					var marketList = JSON.parse(msg.d);
					for (var i = 0; i < marketList.length; i++) {
						var market = marketList[i];
						jQuery('#ddlMarketCity').append('<option value="' + market.MarketCode + '">' + market.MarketName + '</option>');
					}
				},
				error: function (XMLHttpRequest, textStatus, errorThrown) {
					//alert(textStatus);
				}
			})
		}
	}

	function GetMarketCommunities(call_back)
    {
        var marketCode = $j('#ddlMarketCity').val();
        if (marketCode != null && marketCode.length > 0) {
            $j.ajax({
                url: "/Services/Rest/SearchMethods.svc/GetMarketCommunitiesMultipleDivisions/" + marketCode,
                type: "GET",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (e) {
                    // populate the grid
                    var communities = e;
                    if (communities.length > 0) {
                        $j('#CommunityOfInterest').html('');
                        $j('#CommunityOfInterest').append('<option value="">Please Select</option>');
                        for (var i = 0; i < communities.length; i++) {
                            var community = communities[i];
                            $j('#CommunityOfInterest').append('<option value="' + community.CommunityID + '">' + community.CommunityName + '</option>');
                        }
                        $j('.communityInterest').show();
                    }
                    else
                    {
                        $j('.communityInterest').hide();
                    }
                    if (typeof call_back == 'function')
                        call_back();
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    //alert(textStatus);
                }
            });
        }
    }

	function PrePopHomeRFIData() {
		jQuery('#txtFirstName').val('');
    	jQuery('#txtLastName').val('');
    	jQuery('#txtEmail').val('');
    	jQuery('#txtConfirmEmail').val('');
    	jQuery('#txtAddressUS').val('');
    	jQuery('#txtCity').val('');
    	jQuery('#ddlState').val('');
    	jQuery('#txtZipCode').val('');

    	jQuery('#txtPhone').val('');
    	jQuery('#ddlCountry').val('');
    }

    function ValidateHomeRFIForm() {
        var formIsValid = true;
    	jQuery(".inline-error").hide();
    	//Test First Name
    	if (jQuery("#txtFirstName").val().length == 0) {
    		formIsValid = false;
    		jQuery("#errortxtFirstName").show();
    	}
    	//Test Last Name
    	if (jQuery("#txtLastName").val().length == 0) {
    		formIsValid = false;
    		jQuery("#errortxtLastName").show();
    	}
    	//Test Email
    	if (jQuery("#txtEmail").val().length == 0) {
    		formIsValid = false;
    		jQuery("#errorReqtxtEmail").show();
    	} else if (!/^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/.test(jQuery("#txtEmail").val())) {
    		formIsValid = false;
    		jQuery("#errorRegExtxtEmail").show();
    	}
    	//Test Confirm Email
    	if (jQuery("#txtConfirmEmail").val().length == 0) {
    		formIsValid = false;
    		jQuery("#errorReqtxtConfirmEmail").show();
    	} else if (jQuery("#txtConfirmEmail").val() != jQuery("#txtEmail").val()) {
    		formIsValid = false;
    		jQuery("#errorComptxtConfirmEmail").show();
    	} else if (!/^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/.test(jQuery("#txtConfirmEmail").val())) {
    		formIsValid = false;
    		jQuery("#errorRegExtxtConfirmEmail").show();
    	}
    	//Test Zip/Country
    	if (rfiRegion == "Us") {
    		if (jQuery("#txtZipCode").val().length == 0) {
    			formIsValid = false;
    			jQuery("#errorReqtxtZipCode").show();
    		} else if (!/^\d{5}(?:[\s-]\d{4})?$/.test(jQuery("#txtZipCode").val())) {
    			formIsValid = false;
    			jQuery("#errorRegExtxtZipCode").show();
    		}
    	} else if (rfiRegion == "Intl" && jQuery("#ddlCountry")[0].selectedIndex == 0) {
    		formIsValid = false;
    		jQuery("#errorReqddlCountry").show();
    	}
    	//Test State
    	if (jQuery("#ddlMarketState")[0].selectedIndex == 0) {
    		formIsValid = false;
    		jQuery("#errorReqddlMarketState").show();
    	}
    	//Test City
    	if (jQuery("#ddlMarketCity")[0].selectedIndex == 0) {
    		formIsValid = false;
    		jQuery("#errorReqddlMarketCity").show();
    	}
    	if (jQuery('.communityInterest').is(':visible') && jQuery("#CommunityOfInterest")[0].selectedIndex == 0) {
    	    formIsValid = false;
    	    jQuery("#CommunityOfInterestReq").show();
    	}
    	if (jQuery("#txtMoveInDate").val().length > 0 && !/\d{2}\/\d{2}\/\d{4}/.test(jQuery("#txtMoveInDate").val())) {
    	    formIsValid = false;
    	    jQuery("#errorRegExtxtMoveInDate").show();
    	}
    	if (!formIsValid) {
    	    Lennar.Global.scrollToElement(jQuery(".inline-error:visible"));
    	}
    	return formIsValid;
    }

    var callingRfiService = false;
    function SubmitHomeRFI() {
        Lennar.Global.CleanInputFromXss();
        if (ValidateHomeRFIForm()) {
            if (callingRfiService) {
                return;
            }
            callingRfiService = true;

    		var region, stateCode, cityCode, firstName, lastName, emailAddress, zipCode, comments, cntStreet, cntCity, cntState, cntPhone, cntExt, cntMoveIndate, intlCountry, intlStreet, intlCity, intlState, intlPostalCode, referrerDomain, campaign, commID;
    		region = rfiRegion;
    		stateCode = jQuery('#ddlMarketState').val();
    		cityCode = jQuery('#ddlMarketCity').val();
    		firstName = jQuery('#txtFirstName').val();
    		lastName = jQuery('#txtLastName').val();
    		emailAddress = jQuery('#txtEmail').val();

    		// US
    		zipCode = region == 'Us' ? jQuery('#txtZipCode').val() : '';
    		cntStreet = region == 'Us' ? jQuery('#txtAddressUS').val() : '';
    		cntCity = region == 'Us' ? jQuery('#txtCity').val() : '';
    		cntState = region == 'Us' ? jQuery('#ddlState').val() : '';
    		// Intl
    		intlCountry = region == 'Intl' ? jQuery('#ddlCountry option:selected').text() : '';
    		intlStreet = region == 'Intl' ? jQuery('#txtAddressUS').val() : '';
    		intlCity = region == 'Intl' ? jQuery('#txtCity').val() : '';
    		intlState = region == 'Intl' ? jQuery('#txtIntState').val() : '';
    		intlPostalCode = region == 'Intl' ? jQuery('#txtZipCode').val() : '';

    		//optional info
    		comments = jQuery('#txtComments').val();
    		cntPhone = jQuery('#txtPhone').val();
    		cntMoveIndate = jQuery('#txtMoveInDate').val();

    		commID = jQuery('#CommunityOfInterest').is(':visible') ? jQuery('#CommunityOfInterest').val() : '';

    		referrerDomain = _satellite.getVar("referringDomain");
    		campaign = _satellite.getVar("campaignName");

    		// ajax post, reload data
    		var dataString = "{'messageSource':'RFIHomePage'";

    		// profile information
    		dataString += " , 'region':'" + rfiRegion + "'";
    		dataString += " , 'stateCode':'" + stateCode + "'";
    		dataString += " , 'cityCode':'" + cityCode + "'";
    		dataString += " , 'firstName':'" + firstName + "'";
    		dataString += " , 'lastName':'" + lastName + "'";
    		dataString += " , 'emailAddress':'" + emailAddress + "'";
    		dataString += " , 'zipCode':'" + zipCode + "'";
    		dataString += " , 'comments':'" + comments + "'";
    		dataString += " , 'cntStreet':'" + cntStreet + "'";
    		dataString += " , 'cntCity':'" + cntCity + "'";
    		dataString += " , 'cntState':'" + cntState + "'";
    		dataString += " , 'cntPhone':'" + cntPhone + "'";
    		dataString += " , 'cntExt':''";
    		dataString += " , 'cntMoveIndate':'" + cntMoveIndate + "'";
    		dataString += " , 'intlCountry':'" + intlCountry + "'";
    		dataString += " , 'intlStreet':'" + intlStreet + "'";
    		dataString += " , 'intlCity':'" + intlCity + "'";
    		dataString += " , 'intlState':'" + intlState + "'";
    		dataString += " , 'intlPostalCode':'" + intlPostalCode + "'";
    		dataString += " , 'type':'RFI'";
    		dataString += " , 'reasonForMove':''";
    		dataString += " , 'timeSpentLooking':''";
    		dataString += " , 'lwcomid':'" + commID + "'";
    		dataString += " , 'lwplanid':''";
    		dataString += " , 'lwinvid':''";
    		dataString += " , 'referrerDomain':'" + referrerDomain + "'";
    		dataString += " , 'campaign':'" + campaign + "'";
    		dataString += " , 'leadSource':'Lennar.com'";
    		dataString += " , 'leadSourceCode':'LEN'";
    		dataString += " , 'lwmpcid':''";
    		dataString += "}";

    		jQuery.ajax({
    			url: "/Services/ContactMethods.asmx/SendGeneralInterestMessage",
    			data: dataString,
    			type: "POST",
    			contentType: "application/json; charset=utf-8",
    			dataType: "json",
    			success: function () {
    			    callingRfiService = false;
    			    jQuery("#phRequestInfo").hide();
    			    jQuery("#phUserInfo").hide();
    				jQuery("#hHeader").text("Thank you, " + firstName);
    				jQuery("#hText").text("Thank you for your interest in Lennar homes in " + jQuery('#ddlMarketCity option:selected').text() + ". Your request has been submitted and a Lennar Associate will contact you shortly.")
    				jQuery(".js-leadForm").attr("data-form-type", "requestFormThankYou");
				    
    				//Pardot RFi implementation
    				Lennar.Global.PardotRfi('content_0_homepagerfi_0_hidPardotiframe', emailAddress);
    			},
    			error: function (XMLHttpRequest, textStatus, errorThrown) {
    			    callingRfiService = false;
    				alert(errorThrown);
    			}
    		});
    	}
    }
