    display: flex;
  }

  .hpupdate--secondary .secondaryblock--item {
    padding-bottom: 91px;
  }

  .hpupdate--secondary .secondaryblock--item:nth-child(n+2) {
    margin-left: 20px;
  }

  .hpupdate--secondary .secondaryblock--item--btn {
    bottom: 20px;
