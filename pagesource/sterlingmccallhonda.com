<script>
var bannerShim=0;

function videoCache() {
    $(".af-video-homepage video").each(function() {

    var $video = $(this);
    var $clone = $video.clone();
    $video.after($clone);

    var video = $video[0];
    var clone = $clone[0];

    clone.hidden = true;
    clone.pause();
    clone.currentTime = 0;

    video.ontimeupdate = function() {
        if (video.currentTime >= video.duration - .5) {
            clone.play();
            video.hidden = true;
            clone.hidden = false;
            video.pause();
            video.currentTime = 0;
        }
    }

    clone.ontimeupdate = function() {
        if (clone.currentTime >= clone.duration - .5) {
            video.play();
            clone.hidden = true;
            video.hidden = false;
            clone.pause();
            clone.currentTime = 0;
        }
    }
    });
}

function fullDivVideo() {
        var videoHeight=false;
        if ($(".af-video-homepage").height() > $(".af-video-homepage video").height()) {
            $(".af-video-homepage video").css({"height":"100%"});
            $(".af-video-homepage video").css({"width":"auto"});
            videoHeight=true;
        }
        if ($(".af-video-homepage").width() > $(".af-video-homepage video").width()) {
            $(".af-video-homepage video").css({"width":"100%"});
            $(".af-video-homepage video").css({"height":"auto"});
        }
}

function resizeHomepageVideo() {
    var navHeight=$("#af-main-header").height();
    var bannerHeight=$("#af-homepage-banner").height(); 
    var windowHeight=$(window).height();
    var videoHeight=windowHeight - navHeight - bannerHeight;
	if (window.innerHeight < 850) {
		videoHeight=windowHeight-navHeight;
	}
    if (0!=0 && videoHeight > 0) {
        videoHeight=0;
    }
	if (570!=0 && videoHeight < 570) {
        videoHeight=570;
	}
    videoHeight=videoHeight-bannerShim;
    $(".af-video-homepage").css({"height": videoHeight+"px"});
    $("#af-homepage-introvideo-background").css({"height":videoHeight+"px"});
    $("#af-homepage-introvideo-overlay").css({"height":videoHeight+"px"});
    var marginTop=videoHeight+bannerShim;
    $("#af-homepage-introvideo-background").css({"margin-top":"-"+marginTop+"px"});
    $("#af-homepage-introvideo-overlay").css({"margin-top":"-"+marginTop+"px"});
    if (bannerShim!=0) {
        $(".af-video-homepage").css({"margin-bottom": bannerShim+"px"});
    }
}

$(window).resize(function(){
    if (window.innerWidth >= 768) {
		if (!$("#af-homepage-introvideo video").length) {
			$("#af-homepage-introvideo").html("<div class='af-video-homepage'><video preload='none' autoplay='' loop='' id='hpvid'><source src='//player.vimeo.com/external/187902943.sd.mp4?s=61c15bcb4a3d8932f407925597e6bf9769703085&profile_id=165' type='video/mp4'><img style='width:100%;height:auto;' src='/images/localized/herobox-video-poster.jpg'></video></div>");
			setTimeout(function() {
				videoCache();
			},15000); // Start cache after 15 seconds
		}
		var adjustmentTime=1000;
		if ($("#af-homepage-introvideo").is(":in-viewport")) { adjustmentTime=600; }
		setTimeout(function() {
