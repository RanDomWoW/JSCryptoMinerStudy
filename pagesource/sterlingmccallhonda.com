	})
	.autocomplete({
		source: function(request, callback){
			var type = $('#af-fts-type').val().toLowerCase();
			if (type == 'new') {
				years = new_years;
				makes = new_makes;
				models = new_models;
			} else {
				years = used_years;
				makes = used_makes;
				models = used_models;
			}
			var max = 20;
			var j = 0;
			var tags = []
			var word = extractLast( request.term );
			if (word == '' || word ==  ' ') {
			} else if (!isNaN(parseFloat(word))) {
				var header = 0;
				$.map(years, function(n) {
					var n2 = __highlight(n, word);
					if (j < max && n != n2) {
						if (!header) {
							tags.push({
								label : 'Year',
								value : 'Header'
							});
							header = 1;
						}
						tags.push({
							label : n2,
							value : n
						});
						j++;
					}
				});
			} else {
				var header = 0;
				$.map(makes, function(n) {
					var n2 = __highlight(n, word);
					if (j < max && n != n2) {
						if (!header) {
							tags.push({
								label : 'Make',
								value : 'Header'
							});
							header = 1;
						}
						tags.push({
							label : n2,
							value : n
						});
						j++;
					}
				});
				header = 0;
				$.map(models, function(n) {
					var n2 = __highlight(n, word);
					var n3 = n2.substring(n2.indexOf('  ')+2);
					var n4 = n.substring(n.indexOf('  ')+2);
					if (j < max && n != n2) {
						if (!header) {
							tags.push({
								label : 'Model',
								value : 'Header'
							});
							header = 1;
						}
						tags.push({
							label : n3,
							value : n4
						});
						j++;
					}
				});
			}
			callback(tags);
		},
		focus: function() {
