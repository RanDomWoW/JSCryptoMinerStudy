<script>

function check_form_unlock(form) {
	if (check_form(form)) {
		// set cookie
		var name = 'UNLOCKPRICE';
		var value = 'unlocked';
		var days = '7';

       	var date = new Date();
       	date.setTime(date.getTime()+(days*24*60*60*1000));
       	var expires = "; expires="+date.toGMTString();

    	document.cookie = name+"="+value+expires+"; path=/";	

    	var url = "/cgi-bin/quick_quote.cgi";  // post to script

    	$.ajax({
           type: "POST",
           url: url,
           data: $("#form_modal_unlock").serialize(),
		   async: false
         });

    	$.ajax({
           type: "GET",
           url: '/track_unlock.html'
         });

		var hostname = window.location.hostname;
		var pathname = window.location.pathname;
		if (hostname.indexOf('toyotarockhill') !== -1 && pathname.indexOf('new-specials') === -1) {
			var inst = $('[data-remodal-id=modal-video-after]').remodal();
			inst.open();
			var vid = document.getElementById("vd-after");
			vid.play();
		} else {
			location.reload();
		}

		return false;

	} else {
		return false;
	}

}

</script>


