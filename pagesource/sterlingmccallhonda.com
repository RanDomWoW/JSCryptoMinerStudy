							var currentSlide = $(this).children('.af-specials-carousel').slickCurrentSlide();
							var adjustedCurrentSlide = currentSlide - 1;
							$(this).children('.af-specials-carousel').slickGoTo(adjustedCurrentSlide, true);
						});
					}
					mobileSize = true;
				} else if($('.af-specials').first().find('.slick-active').length > 1) {
					if(mobileSize) {
						$('.af-specials').each(function() {
							var currentSlide = $(this).children('.af-specials-carousel').slickCurrentSlide();
							var adjustedCurrentSlide = currentSlide + 1;
							$(this).children('.af-specials-carousel').slickGoTo(adjustedCurrentSlide, true);
						});
					}
					mobileSize = false;
				}
			}, 500);
		});

		$('.af-vehicles-section.af-specials-showing:odd').css('background-color', '#fff');
		$('.af-vehicles-section.af-specials-showing:even').css('background-color', '#ececec');	});	
