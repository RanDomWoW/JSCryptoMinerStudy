<script>
	$(document).ready(function() {
		// set bg image and show 1st offer (hide others)
		$('.af-rin-incentive-container').each(function() {
			showIncentive($(this),1);
		});

		var windowWidth = $(window).width();

		$(window).on('resize', function() {
			if ($(window).width() != windowWidth) { // check to make sure that the window size really did change
				recalculateIncBox();
				windowWidth = $(window).width();
			}
		});

		$(window).on('orientationchange', function() {
			recalculateIncBox();
		});
	});

	function recalculateIncBox(elem) {
		if(typeof elem !== 'undefined') {
			// recalculate and adjust the height of a specified incentives box
			if(elem.hasClass('af-rin-expanded')) {
				elem.css('height', '');
				var incModelBoxHeightRecal = elem.height();
				if(incModelBoxHeightRecal != 0) {
					elem.css('height', incModelBoxHeightRecal + 'px');
				} 
			}
		} else {
			// recalculate and adjust the height of all opened incentives boxes
			$('.af-rin-model-box').each(function() {
				if($(this).hasClass('af-rin-expanded')) {
					var incModelBoxElem = $(this);
					incModelBoxElem.css('height', '');
					incModelBoxElem.css('transition', 'none');
					var incModelBoxHeightRecal = incModelBoxElem.height();
					if(incModelBoxHeightRecal != 0) {
						incModelBoxElem.css('height', incModelBoxHeightRecal + 'px');
					} 
					setTimeout(function() {
						incModelBoxElem.css('transition', '');
					}, 500);
				}
			});
		}
	}

	function showIncentive(e,next) {

		var counter = 1;

		$(e).parent().find('.af-rin-incentive-container').find('.af-rin-incentive').each(function() {
			if (next == counter) {
				// show this one - remove af-rin-hidden
				var attr =  $(this).attr('class');
				attr = attr.replace(" af-rin-hidden",'');


				$(this).attr('class',attr);
				var imageUrl = $(this).attr('background-img');

				$(e).parent().css('background-image', 'url(' + imageUrl + ')');

			} else {
				// hide all others - append af-rin-hidden if not in class already
				var attr =  $(this).attr('class');

				if (attr.indexOf("af-rin-hidden") < 0) {
					attr += ' af-rin-hidden';
					$(this).attr('class',attr);
				} 

			}
			counter += 1;
		});

	}
</script>
<script>
	$(document).ready(function(){
		// Set the first incentives button as active
		$('#af-rin-bodystyle-group .af-rin-bodystyle:first').addClass('af-brand-text');
		
		var bodystyleCount = $('.af-rin-bodystyle').length;
		var bodystyleWidth = 100 / bodystyleCount;
		$('#af-rin-bodystyle-group .af-rin-bodystyle').css('width', bodystyleWidth + '%');

		$('#af-rin-bodystyle-group .af-rin-bodystyle').click(function(){
			var bodystyle = $(this).attr('data-category');
			$('#af-rin-bodystyle-group .af-rin-bodystyle').each(function() {
				$(this).removeClass('af-brand-text');
			});
			$(this).addClass('af-brand-text');
			$('.af-rin-model-box').each(function(){
				if($(this).attr('data-category') != bodystyle){
					$(this).parent().addClass('af-rin-hidden');
				} else {
					$(this).parent().removeClass('af-rin-hidden');
				}
			});

			recalculateIncBox();
		});

		var incModelBoxHeightCollapsed;
		var incModelBoxHeightExpanded;
		$('.af-rin-expand').click(function() {
			$('#af-rin-bodystyle-group .af-rin-bodystyle').css('pointer-events', 'none')
			$(this).children().css('pointer-events', 'none');
			$(this).css('pointer-events', 'none');
			$(this).parent().css('pointer-events', 'none');
			if(!incModelBoxHeightCollapsed) {
				incModelBoxHeightCollapsed = $(this).parent().height();
			}
			recalculateIncBox($(this).parent());
			if($(this).parent().hasClass('af-rin-collapsed')) {
				$(this).parent().css('height', '');
				var boxHeight = $(this).parent().find('.af-rin-incentive-container').height() + parseInt($(this).parent().find('.af-rin-incentive-container').css('padding-bottom'));
				$(this).parent().css('height', boxHeight + 'px');

				$(this).parent().children('.af-rin-expand').children('.af-rin-expand-text').html('Hide Details');
				$(this).parent().children('.af-rin-expand').find('.af-svg-plus-icon').css('display', 'none');
				$(this).parent().children('.af-rin-expand').find('.af-svg-minus-icon').css('display', 'inline');
			} else {
				$(this).parent().children('.af-rin-expand').children('.af-rin-expand-text').html('Show Details');
				$(this).parent().children('.af-rin-expand').find('.af-svg-plus-icon').css('display', 'inline');
				$(this).parent().children('.af-rin-expand').find('.af-svg-minus-icon').css('display', 'none');
			}
			$(this).parent().toggleClass('af-rin-collapsed af-rin-expanded');
			$(this).parent().children('.af-rin-incentive-container').toggleClass('af-rin-collapsed af-rin-expanded');
			$(this).parent().one('transitionend webkitTransitionEnd oTransitionEnd MSTransitionEnd', function(){
				if(incModelBoxHeightExpanded != $(this).find('.af-rin-incentive-container').height() + parseInt($(this).parent().find('.af-rin-incentive-container').css('padding-bottom'))) {
					incModelBoxHeightExpanded = $(this).find('.af-rin-incentive-container').height() + parseInt($(this).parent().find('.af-rin-incentive-container').css('padding-bottom'));
				}
				if ($(this).hasClass('af-rin-expanded')) {
					$(this).css('height',  '');
					$(this).css('height',  incModelBoxHeightExpanded + 'px');
				} else {
					$(this).children('.af-rin-expand').find('.af-svg-plus-icon').css('display', 'inline');
					$(this).children('.af-rin-expand').find('.af-svg-minus-icon').css('display', 'none');
				}
				$('#af-rin-bodystyle-group .af-rin-bodystyle').css('pointer-events', 'auto')
				$('.af-rin-expand').children().css('pointer-events', 'auto');
				$('.af-rin-expand').css('pointer-events', '');
				$('.af-rin-expand').parent().css('pointer-events', '');
			});
		});

		$('.af-rin-arrow-left').click(function(){
			showPreviousOffer($(this));
		});

		$('.af-rin-arrow-right').click(function(){
			showNextOffer($(this));
		});

		// enable swipe for the incentives boxes
		$(".af-rin-incentive-container").swipe( {
			swipeLeft:function(event, direction, distance, duration, fingerCount, fingerData) {
				showNextOffer($(this));
			},
			swipeRight:function(event, direction, distance, duration, fingerCount, fingerData) {
				showPreviousOffer($(this));
			}
		});

		/* Required for clicking incentives through to search page. */
		$('.af-rin-arrow-left').click(function(e) {
			e.stopPropagation();
		});
		$('.af-rin-arrow-right').click(function(e) {
			e.stopPropagation();
		});
		$('.af-rin-expand').click(function(e) {
			e.stopPropagation();
		});
		$('.af-rin-model-box').click(function(event){
			var year = $(this).parent().parent().attr('data-year');
			var make = $(this).parent().parent().attr('data-make');
			var model = $(this).parent().parent().attr('data-model');
            var confid = $(this).parent().parent().attr('data-confid');
			var invCount = $(this).parent().parent().attr('data-count');
            var offerid = $(this).parent().parent().attr('data-offerid');

			if(invCount == '0') {
				// carfinder
				if (1) {
					window.location = '/car-finder.html?make=' + make + '&model=' + model + '&year=' + year;
				} else {
            		var family = $(this).parent().parent().attr('data-family');
					window.location = '/search/New+' + make + '+' + family + '+tmF';
				}
			} else {
				window.location = '/search/New+' + make + '+' + model + '+tmM';                
			}
		});
	});

	function findCurrent(e) {

		var current = 1;
		var counter = 0;

		$(e).parent().find('.af-rin-incentive-container').find('.af-rin-incentive').each(function() {
			counter += 1;
			if ($(this).attr('class').indexOf("af-rin-hidden") < 0) {
				current = counter;
			}
		});

		return {
			'current' : current,
			'total'	  : counter
		};

	}

	function showPreviousOffer(incElement) {
		var r = findCurrent(incElement);

		var next = r.current-1;	

		// loop
		if (next < 1) { next = r.total; }

		showIncentive(incElement,next);
		if(incElement.parent().hasClass('af-rin-expanded')) {
			incElement.parent().css('height', '');
			incElement.parent().css('transition', 'none');
			incModelBoxHeightExpanded = incElement.parent().height();
			incElement.parent().css('height', incModelBoxHeightExpanded + 'px');
			setTimeout(function() {
				incElement.parent().css('transition', '');
			}, 500);
		}
	}

	function showNextOffer(incElement) {
		var r = findCurrent(incElement);

		var next = r.current+1;	

		// loop
		if (next > r.total) { next = 1; }

		showIncentive(incElement,next);
		if(incElement.parent().hasClass('af-rin-expanded')) {
			incElement.parent().css('height', '');
			incElement.parent().css('transition', 'none');
			incModelBoxHeightExpanded = incElement.parent().height();
			incElement.parent().css('height', incModelBoxHeightExpanded + 'px');
			setTimeout(function() {
				incElement.parent().css('transition', '');
			}, 500);
		}
	}
</script>

