	$(function($)
	{
			// Regular datepicker
			$( '.datePick' ).datepicker({
				dateFormat: 'dd.mm.yy',
				prevText: '<i class="icon-chevron-left"></i>',
				nextText: '<i class="icon-chevron-right"></i>'
			});

			$('.fileplaceholder').change(function(){
				filename = $(this).val();
				$(this).parent().next().val(filename.split("\\").pop());
			});

			//console.log();
			$(".deldata").click(function(e){
			    link =  $(this).attr("href");
		    	e.preventDefault();
				$( "#dialog-confirm" ).dialog({
			      resizable: false,
			      height:250,
			      modal: true,
			      buttons: {
			        "Delete" : function() {
			           window.location = link;
			        },
			        Cancel: function() {
			          $( this ).dialog( "close" );
			        }
			      }
			    });
			});
			$( "#image-warning" ).dialog({
			      resizable: false,
				  width:'auto',
				  autoOpen: true,		     
			      modal: true,			    
			  });

		

	});

</script>

              		
