            <script>
            function choosecurrency(curr) {
                var current_url = window.location.href;
                if (current_url.indexOf('goods/search/index') != -1) {
                    window.location.href = current_url + '&currency=' + curr;
                } else {
                    window.location.href = '?currency=' + curr;
                }
            }
              function ajaxDropGoods(id) {
                  var current_url = window.location.href;
                  if (current_url.indexOf('cart/index/index') != -1) {
                      window.location.href = "/cart/ajax/drop_goods?id=" + id
                  } else {
                      $.ajax({
                          type: 'get',
                          url: '/cart/ajax/drop_goods?ac=left&id=' + id,
                          success: function(datas) {
                              if (datas.indexOf('history')) {
                                  $.ajax({
                                      type: "post",
                                      dataType: "json",
                                      url: "/cart/index/reloadcard",
                                      success: function(data) {
                                          $("#menucarlist .goods_num").html(data.goods_num);
                                          $("#menucarlist .shopPrice").html(data.goods_amount);
                                          if (data.goods_num) {
                                              $("#menucarlist #productlist-panel").html(data.product_list);
                                              $("#shopBag .goods_num").addClass('active').html(data.goods_num);
                                              $(".shopShow").removeClass("empty");
                                          } else {
                                              $("#menucarlist #productlist-panel").html('<p class="cart-empty">Shopping Cart is Empty </p>');
                                              $("#shopBag .goods_num").removeClass('active').html(data.goods_num);
                                              $(".shopShow").addClass("empty");
                                          }
                                          $(".quickCloseTxt").trigger("click");
                                          $('html,body').animate({
                                              scrollTop: 0
                                          },
                                          10);
                                          $(".shopShow").slideDown(200);
                                          //add by zhangqiang
                                          if($('.cart-more-info.bonus_reach').length > 0){
                                              var tmp = data.goods_amount.match(/([^\d]*)(\d*.\d*)/);
                                              if(tmp.length == 3){
                                                  var currency = tmp[1];
                                                  var amount = parseFloat(tmp[2]);
                                                  var bonus_min_amount = $('.bonus_not_reach').data('min_goods_amount');
                                                  var minus = ( bonus_min_amount - amount).toFixed(2);
                                                  if( minus > 0){
                                                      $('.cart-more-info').css('display', 'none');
                                                      $('.cart-more-info.bonus_not_reach').css('display', 'block');
                                                      $('.cart-more-info.bonus_not_reach .need_price').text(currency + minus);
                                                  } else {
