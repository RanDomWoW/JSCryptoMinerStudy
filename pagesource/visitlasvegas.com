		var section = $(location).attr('pathname');
		section.indexOf(1);
		section.toLowerCase();
		section = section.split("/")[1];	

		var checkLines = function(){
			var lines = root.find('.line');

			if (lines.length > 0) {
				setGrid();
			} else {
				setTimeout(function() {
					checkLines();
				}, 500);
			}
		}

		var setGrid = function(){

			var largeGrid = root.find('.largeGrid');
			var smallGrid = root.find('.smallGrid');
			var items = root.find('.largeGrid .col');

			if (items.length > slides_per_page) {
				var loadMore = '<div class="loadMoreWrap"><div class="loadMore goldButton">Load More</div></div>';
				smallGrid.append(loadMore);
				largeGrid.append(loadMore);
			} else {
				$('.largeGrid').css('margin-bottom','20%');
				$('.largeGrid').addClass('space');
			}
			var lines = root.find('.line');
			// add alternate lines designation
			for (var i = 0; i < lines.length; i+=slides_per_page) {
				var sliced = lines.slice(i,i+slides_per_page);
				$(sliced[2]).addClass('alt');
				$(sliced[3]).addClass('alt');
			};

			for (var i = 0; i < lines.length; i++) {
				if (section != 'alteryourreality') {
					if (i < slides_per_page) {
						root.find('.largeGrid .line .col').eq(i).removeClass('hid');
						root.find('.smallGrid .line .col').eq(i).removeClass('hid');
					}					
				} else {
					root.find('.largeGrid .line .col').eq(i).removeClass('hid');
					root.find('.smallGrid .line .col').eq(i).removeClass('hid');
					$('.loadMore').hide();
					$('.largeGrid').addClass('space');
				}
			};

			animations.init();
			animations.altgridAnimations();
			root.show();

			root.find('.loadMore').click(function(){

				var smallShowLines = root.find('.smallGrid .line .col.hid').slice(0,slides_per_page);
				var largeShowLines = root.find('.largeGrid .line .col.hid').slice(0,slides_per_page);

				smallShowLines.removeClass('hid');
				largeShowLines.removeClass('hid');

				var remain = $('.line .col.hid').length;
				if (remain < 1 ) {
					$('.largeGrid').css('margin-bottom','20%');
					$('.loadMore').hide();
					$('.largeGrid').addClass('space');
				}

			});
		}

		$(document).ready(function() {
			checkLines();
		});

	});
