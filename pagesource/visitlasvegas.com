				: searchQuery.currentQuery.startDate;

			updateDateButtons(buttonDate,true);
		}

		var updateDateButtons = function(input,reload) {
			reload = reload || false;
			var dateCount = (window.innerWidth > 640) ? 7 : 1; // only show one date at mobile size
			var dateToday = moment();
			var dateChosen = clientMoment(input, queryDateFormat);
			dateChosen = (dateChosen.isBefore(dateToday, 'day')) ? dateToday : dateChosen;

			eventDates.dateMonth = dateChosen.format('MMMM') + ' ' + dateChosen.format('YYYY');

			// have to init dateFirst as a string and then convert to moment, or else it alters the value of dateChosen
			var dateFirst = dateChosen.format(queryDateFormat);
			dateFirst = clientMoment(dateFirst, queryDateFormat);
			if (dateCount > 1) {
				dateFirst = dateFirst.subtract(dateFirst.weekday(),'days').format(queryDateFormat);
			}

			eventDates.dateList = [];
			for (var i=0; i<dateCount; i++) {
				var dateCurrent = clientMoment(dateFirst, queryDateFormat).add(i, 'day');

				eventDates.dateList.push({
					dateName : dateCurrent.format('ddd'),
					dateNum : dateCurrent.format('D'),
					dateButton : dateCurrent.format(queryDateFormat),
					dateEnabled : (dateCurrent.isSame(dateToday, 'day') || dateCurrent.isAfter(dateToday, 'day')) ? 'enabled' : '',
					dateActive : (dateCurrent.format(queryDateFormat) == dateChosen.format(queryDateFormat)) ? 'active' : ''
				});
			}

			var firstDayLastWeek = clientMoment(dateFirst, queryDateFormat).subtract(dateCount, 'day'); // first day of previous week
			var lastDayLastWeek = clientMoment(dateFirst, queryDateFormat).subtract(1, 'day'); // last day of previous week
			var firstDayNextWeek = clientMoment(eventDates.dateList[dateCount-1].dateButton, queryDateFormat).add(1, 'day'); // first day of next week
			eventDates.dateArrowDisabled = (lastDayLastWeek.isBefore(dateToday)) ? 'disabled' : ''; // enable left arrow if any day from prior week is after today
			eventDates.dateArrowLeft = firstDayLastWeek.format(queryDateFormat);
			eventDates.dateArrowRight = firstDayNextWeek.format(queryDateFormat);

			$datesContainer.html(goatee.fill(eventDatesTmpl, eventDates));

			$startDate.datepicker('setDate',dateChosen.format(queryDateFormat));

			if (reload) {
				newPage = true;
				$formContainer.find('input[type="submit"]').trigger('submit');
			}

			$('[data-button-date].enabled').on('click', function() {
				updateDateButtons($(this).attr('data-button-date'),true);
			});

			$('[data-arrow-left]').not('.disabled').on('click', function() {
				updateDateButtons($(this).attr('data-arrow-left'),true);
			});

			$('[data-arrow-right]').on('click', function() {
				updateDateButtons($(this).attr('data-arrow-right'),true);
			});
		}

		$(window).on('resize', lodash.debounce(function() {
			var newSize = (window.innerWidth > 640) ? 'large' : 'small';
			if (newSize !== oldSize) {
				var qs = getQueryStringQuery();
				var dateSave = (qs.startDate !== undefined) ? qs.startDate : '';
				updateDateButtons(dateSave,false);
			}
			oldSize = (window.innerWidth > 640) ? 'large' : 'small';
		}, 500));

		var createAnimationHandlers = function() {
			if (eventsController !== undefined) {
				eventsController = eventsController.destroy();
				eventsController = null;
			}

			eventsController = new ScrollMagic.Controller({
				globalSceneOptions: {
					triggerHook: "onEnter"
				}
			});

			$('.dailyEventsWrapper .items.loaded .eventItem').each(function() {
				var thisE = this;
				new ScrollMagic.Scene({triggerElement: thisE, offset: 150 })
					.on('enter', function () {
						$(thisE).addClass('animated');
					})
					.addTo(eventsController)
			});
		}

		function search() {
			searchQuery.currentQuery = getQueryStringQuery();
			var query = searchQuery.getFullQuery();

			/************ Update Form Values *************/
			$form.find(".startDate").val(query.startDate);
			/*********************************************/

			formValidator.validateForm();

			// Translate to query
			var filter = {
				solrOptions : {},
				sites : site.site,
				featured : true,
				"udfs_object.2405.value" : { $exists : true }
			};

			var options = {
				skip : (parseInt(query.page) - 1) * parseInt(query.pageSize),
				limit : parseInt(query.pageSize),
				hooks : ["afterFind_listing", "afterFind_host"],
				sort : {},
				fields : {
					categories : 1,
					endDate : 1,
					host_id : 1,
					"host.recid" : 1,
					"host.title" : 1,
					"host.detailURL" : 1,
					latitude : 1,
					listing_id : 1,
					"listing.recid" : 1,
					"listing.title" : 1,
					"listing.detailURL" : 1,
					"listing.address1" : 1,
					address1 : 1,
					location : 1,
					longitude : 1,
					media_raw : 1,
					nextDate : 1,
					rank : 1,
					recId : 1,
					recid : 1,
					recurType : 1,
					recurrence : 1,
					startDate : 1,
					startTime : 1,
					times : 1,
					title : 1,
					typeName : 1,
					loc : 1,
					url : 1,
					date : 1,
					linkUrl : 1,
					udfs_object : true,
				}
			};

			var method = "findEvents";

			// handle sorting behaviors
			options.sort.rank = 1;
			options.sort.title = 1;
			filter["dates.eventDate"] = { $gte : query.startDate, $lte : query.startDate};

			if (xhr) { xhr.abort(); }
			xhr = eventsApi[method](cleanFilter(filter), options, handleSearchResults);
		}

		function handleSearchResults(err, data) {
			if (newPage) {
				eventCount = 0;
			}
			if (data.events !== undefined && data.events.length > 0) {
				for (var i=0; i<data.events.length; i++) {
					if (data.events[i].nextDate !== undefined) {
						data.events[i].nextDate = moment(data.events[i].nextDate);
					}

					data.events[i].startTimeFormatted = (data.events[i].startTime !== undefined)
						? clientMoment(data.events[i].startTime, queryTimeFormat).format('h:mm a')
						: '';

					eventCount ++;

					// don't use the built-in even/odd designation because it goes by row num, which resets on pagination
					data.events[i].designation = (eventCount % 2 == 0) ? 'even' : 'odd';
				}
			}

			data.searchQuery = searchQuery;
			data.showSearchBox = true;
			data.geoResult = geoResult;

			if (newPage) {
				$eventsList.html(goatee.fill(eventsListTmpl, data, {
					eventListItem : eventListItemTmpl,
					pager : pagerTmpl
				}));
			} else {
				$eventsList.append(goatee.fill(eventsListTmpl, data, {
					eventListItem : eventListItemTmpl,
					pager : pagerTmpl
				}));
			}

			$eventsList.addClass('loaded');

			// each page load requires adding animation handlers to the new elements
			createAnimationHandlers();

		}

		function getQueryStringQuery() {
			var url = window.location.search;
			var query = qs.parse(urlLib.parse(url).query);

			return query;
		}

		function setQuery(href) {
			if (href !== window.location.href & href !== "?") {
				history.pushState(null, "", href);
				search();
			}
		}

		function cleanFilter(filter) {
			for (var key in filter) {
				if (filter.hasOwnProperty(key)) {
					if (filter[key] === undefined || (typeof filter[key] === "string" && filter[key] === 0 || filter[key] === "0" || filter[key].length === 0)) {
						delete filter[key];
					} else if (typeof filter[key] === "object" ) {
						if ($.isEmptyObject(cleanFilter(filter[key]))) {
							delete filter[key];
						}
					}
				}
			}
			return filter;
		}

		$root.on("click", ".ui-datepicker-trigger", function() {
			$(this).closest("div").find(".datepicker").select();
		});
	});
</script>
