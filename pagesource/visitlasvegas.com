<script>
	require(["jquery", "goatee", "sv_urlLib", "sv_site", "sv_asyncLib"], function($, goatee, urlLib, sv_site, asyncLib) {
		var root = $("[data-sv-weatherWidget]");
		var template = $("[data-weatherwidget-template]").html();

        var flow = new asyncLib.Flow();
        flow.series({
			state : function(cb) {
				var state = {
					latitude : 36.175,
					longitude : -115.136,
					path : '/plugins/core/svapi/',
					service : 'weatherv2',
					endpoint : 'latlng'
				};

				cb(null, state);
			},
			celcius : function(cb) {
				var url = urlLib.build({
					path : flow.data.state.path,
					get : {
						service : flow.data.state.service,
						endpoint : flow.data.state.endpoint,
						serviceArgs : JSON.stringify({
							lat : flow.data.state.latitude,
							lng : flow.data.state.longitude,
							unit : 'c'
						})
					}
				});

				$.get(url, function(res) {
					if (res.success !== true) { cb(null); }
					res.data.forecast.splice(1);
					cb(null, res.data);
				});
			},
			fahrenheit : function(cb) {
				var url = urlLib.build({
					path : flow.data.state.path,
					get : {
						service : flow.data.state.service,
						endpoint : flow.data.state.endpoint,
						serviceArgs : JSON.stringify({
							lat : flow.data.state.latitude,
							lng : flow.data.state.longitude,
							unit : 'f'
						})
					}
				});

				$.get(url, function(res) {
					if (res.success !== true) { cb(null); }
					res.data.forecast.splice(1);
					
					// add the celcius high and low to the fahrenheit struct
					res.data.forecast[0].highc = flow.data.celcius.forecast[0].high;
					res.data.forecast[0].lowc = flow.data.celcius.forecast[0].low;
					cb(null, res.data);
				});
			},
			render : function(cb) {
				var html = goatee.fill(template, flow.data.fahrenheit);
				root.html(html);

				cb(null);
			}
        }, function(err) {
            if (err) { throw err; }
        });
	});
</script></div>
