		// attach handler to form's submit event 
		$('#user_stats').submit(function() {
			var errorTrigger = 0;
			
			if (ajaxrequestRunning) { // don't do anything if an AJAX request is pending
				return;
			}
			
			$("#user_stats div.error").html("");
			// Check for wrong input
			if($('#stats-ft').val().match(/[a-z]/i) || $('#stats-in').val().match(/[a-z]/i) || !$('#stats-ft').val()){
				$("#user_stats div.error").append("Please input correct height.<br/>");
				errorTrigger = 1;
			}else{
				if(!$('#stats-in').val()){
						$('#stats-in').val('0');
				}
			}

			if($('#stats-weight').val().match(/[a-z]/i) || !$('#stats-weight').val()){
				$("#user_stats div.error").append("Please input correct weight.<br/>");
				errorTrigger = 1;
			}
			
			if($('#stats-bodyfat').val().match(/[a-z]/i)){
				$("#user_stats div.error").append("Please input correct bodyfat %.<br/>");
