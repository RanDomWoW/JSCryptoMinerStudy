			}
			
			if(!$('#stats-goal').val()){
				$("#user_stats div.error").append("Please  select your goal.<br/>");
				errorTrigger = 1;
			}
			
			if(errorTrigger == 1){
				// return false to prevent normal browser submit and page navigation 
				return false; 
			}else{
				$('#div-userstat').hide();
				$('#div-ajaxload').show();
					
