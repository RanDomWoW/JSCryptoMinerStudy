	// SPINNER FUNCTIONS - START
	function randomIntFromRange(min,max) {
		return Math.floor(Math.random()*(max-min+1)+min);
	}
	
	function resetWorkoutLength(){
		var workoutGoal = $('#goalValue').val();
		if(workoutGoal == 2){
			d.setHours(0, 20,0, 0);
		}else{
			d.setHours(0, 5,0, 0);
		}
		DisplayTime('lengthValue', 'lengthDisplay');
	}
	
	function UpdateTimeSpinnerByDiet(subgoal) {
		var subgoal = parseInt(subgoal);
		var excludeMinutes = [];
		
		if (subgoal == 0) {
			excludeMinutes = [23,29];
			minMinute = 20;
			maxMinute = 30;
			maxHour = 0;
		} else if (subgoal == 1){
			minMinute = 5;
			maxMinute = 15;
			maxHour = 0;
		} else if (subgoal == 2){
			excludeMinutes = [23,29,31,37,38,41,43];
			minMinute = 25;
			maxMinute = 45;
			maxHour = 0;
		} else if (subgoal == 3){
			excludeMinutes = [17,19,20,22,23,25,26,27,29];
			minMinute = 16;
			maxMinute = 30;
			maxHour = 0;
		} else if (subgoal == 4){
			minMinute = 15;
			maxMinute = 15;
			maxHour = 0;
		} else if (subgoal == 5){
			minMinute = 20;
			maxMinute = 25;
			maxHour = 0;
		} else if (subgoal == 6){
			excludeMinutes = [15,16,17,18,19,20,21,22];
			minMinute = 14;
			maxMinute = 23;
			maxHour = 0;
		} else if (subgoal == 7){
			excludeMinutes = [11,13];
			minMinute = 5;
			maxMinute = 15;
			maxHour = 0;
		}
		
		if(document.getElementById("isAutoShred").value == "true"){
			for(x = autoShredMin; x <= maxMinute; x++){ // check if minute excluded
				if(excludeMinutes.indexOf(x) == -1 && x >= minMinute){ minMinute = x; break; }
			}
			d.setHours(0, minMinute,0, 0);
		}else{
			var randomMinute = randomIntFromRange(minMinute, maxMinute);
			while(excludeMinutes.indexOf(randomMinute) != -1){
				randomMinute = randomIntFromRange(minMinute, maxMinute);
			}
			d.setHours(0, randomMinute,0, 0);
		}
		
		DisplayTime('lengthValue', 'lengthDisplay');
	}
	
	function UpdateMinMinuteForTimeSpinner(goalValue){
		var goalVal = parseInt(goalValue);
		
		if (goalVal == 2 || (goalVal == 5 && autoShredGoal == 2)) {
			minMinute = 20;
			maxMinute = 30;
			maxHour = 1;
		}else if(goalVal == 3 || (goalVal == 5 && autoShredGoal == 3)){
			minMinute = 3;
			maxMinute = 20;
			maxHour = 0;
		}else {
			minMinute = 5;
			maxMinute = 30;
			maxHour = 1;
		}
		
		if(goalVal == 5){			
			d.setHours(0, autoShredMin,0, 0);
		}else{
			d.setHours(0, minMinute,0, 0);
		}
		
		DisplayTime('lengthValue', 'lengthDisplay');
	}
	
	function TimeSpinner(valueHolder, valueDisplayer, up) {
		var minutes = d.getMinutes();
		var seconds = d.getSeconds();
		
		var maxDate = new Date();			
		maxDate.setHours(maxHour, maxMinute, maxSecond, 0);
		
		var minDate = new Date();
		minDate.setHours(0, minMinute, minSecond, 0);
		
		if (up)
		{
			if (d.getTime() < maxDate.getTime())
				d.setMinutes(minutes + 1);
		}
		else
		{
			if (d.getTime() > minDate.getTime()) {
				d.setMinutes(minutes - 1)
			} else {
				var goalValue = parseInt(document.getElementById("goalValue").value);
				if (goalValue == 2 || (goalValue == 5 && autoShredGoal == 2))
					alert("Dr. Kareem says that you should commit at least 20 minutes to any muscle building workout if you truly want to get great results.");
					if (typeof spinnerHold !== 'undefined') { clearTimeout(spinnerHold); }
			}
		}
		
		DisplayTime(valueHolder, valueDisplayer);
	}
	
	function DisplayTime(valueHolder, valueDisplayer){
		var mDisplay = "00";
		
		if (d.getHours() > 0)
			mDisplay = String((d.getHours() * 60) + d.getMinutes());
		else 
		{
			mDisplay = (d.getMinutes() < 10) ? "0" + d.getMinutes() : d.getMinutes();
			mDisplay = (d.getMinutes() == 0) ? "00" : mDisplay;
		}
		
		var sDisplay = (d.getSeconds() < 10) ? "0" + d.getSeconds() : d.getSeconds();
		sDisplay = (d.getSeconds() == 0) ? "00" : sDisplay;

		document.getElementById(valueDisplayer).value = mDisplay + ":" + sDisplay;
		document.getElementById(valueHolder).value = mDisplay + ":" + sDisplay;
		document.getElementById(valueDisplayer).innerHTML = mDisplay + ":" + sDisplay;
		saveGuestOptions("workoutLength", mDisplay + ":" + sDisplay);
	}
	
	function numInput(evt){
		var regex = /[0-9]/;
		var theEvent = evt || window.event;
		var key = theEvent.keyCode || theEvent.which;
		key = String.fromCharCode( key );
		
		if( !regex.test(key) ) {
			theEvent.returnValue = false;
			if(theEvent.preventDefault) theEvent.preventDefault();
		}
	}
	
	function setSlideTo(slideWrapper, slideNum){
		var thumbWidth = Math.round(((parseFloat($(slideWrapper).children(".workoutOption").not('a.selectedOption').css("width")) / parseFloat($(slideWrapper).css("width")))*100)+1);
		var selectedThumbWidth = Math.round(((parseFloat($(slideWrapper).children(".selectedOption").css("width")) / parseFloat($(slideWrapper).css("width")))*100)+1);
		var afterSelectedMargin = 0;
		
		$(slideWrapper).children(".workoutOption").each(function(index, elem) {			
			$(elem).css("left", (thumbWidth*index)-(thumbWidth*slideNum)+thumbWidth+afterSelectedMargin+"%" );
			
			if(index == slideNum){
				$(slideWrapper).children(".selectedOption").removeClass("selectedOption");
				$(elem).addClass("selectedOption");
				afterSelectedMargin = selectedThumbWidth-thumbWidth;
			}
		});
	}
