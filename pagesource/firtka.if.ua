        $.get( "blog/ajax/posts/1/"+60, function( data ) {
            $( "#IndexNews" ).html( data );
            getPosts();
        });

        function getPosts(){
            $("#index-posts a").on('click', function(e) {
                e.preventDefault();
                var href = $(this).attr("href");
                var href_array = href.split("/");
                var page = href_array[href_array.length-1];
                $.get("blog/ajax/posts/" + page + "/"+60, function (data) {
                    $("#IndexNews").html(data);
                    getPosts();
                })
            })
        }

        if($('#vote_question_id').val()){
            if($.cookie('vote') && $.cookie('vote') != $('#vote_question_id').val()){
                $.cookie('vote', null);
            }
            if($.cookie('vote') == $('#vote_question_id').val()){
                $.ajax({
                    type        : 'POST',
                    url         : '/app_dev.php/voting/result/'+$('#vote_question_id').val(),
                    error       : function(){ console.log('error'); },
                    success     : function(data){ $('#vote_body').html(data); }
                });
            }
            $('#voteForm').on('submit', function(e) {
                e.preventDefault();
                var values = {};
                values['answer'] = $('#voteForm input:radio:checked').val();
                $.ajax({
                    type        : 'POST',
                    url         : '/voting/vote',
                    timeout     : 500,
                    contentType : 'application/json',
                    data        : JSON.stringify(values),
                    error       : function(){ console.log('error'); },
