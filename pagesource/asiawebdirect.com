    <textarea id="quick_jumps_tmpl">
    <% if (destinations.length > 0) { %>
      <div id="popDest_dest_container" class="continent_popular-container group-1">
      <h2>Hotels in Asia</h2>
      <p>Most popular destinations.</p>
        <% var maxshow = 12; %>
        <% var maxDisplay = 999; %>
        <% if (destination.DestinationID == "139") { maxDisplay = 12; } %>
        <% var displayGroup = 1; %>
        <% var objStyle = ''; %>
        <% var img_func = get_dest_img_homepage; %>
        
        <% if (destinations.length-maxshow < 4) { maxshow = destinations.length } %>

        <% for (var i = 0; i < destinations.length; i++ ) { %>
            <% var tmpimg = img_func(destinations[i]); %>
            <div id="popDest_uni_<%=displayGroup%>_<%= i + 1 %>" class="popular-dest" style="display:<%=objStyle%>">
                <img src="<%=tmpimg%>" border="0"/>
                <div class="dest-name-overlay"><%=destinations[i].name%></div>
	        </div>
            <% if ((i+1) >= maxDisplay) { break; } %>
            <% if ((i+1) >= maxshow) { objStyle = 'none'; } %>
            <% if ((i+1) % maxshow == 0) { displayGroup++; } %>
        <% } %> <!-- end destination loop -->
        <br style="clear: both;" />
        <% if (destinations.length > maxshow) { %>
        <a id="show_all_dest">Show all destinations</a>
        <% } %>
        </div>
    <% } %> <!-- end destination length check -->
