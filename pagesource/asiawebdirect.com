(function() {
    function hostContains(val) {
        return document.location.hostname.toLowerCase().indexOf(val) != -1;
    }
    function pathContains(val) {
        return document.location.pathname.toLowerCase().indexOf(val) != -1;
    }
    function pathStartsWith(val) {
        return document.location.pathname.toLowerCase().startsWith(val);
    }
    function queryContains(val) {
        return document.location.search.toLowerCase().indexOf(val) != -1;
    }
    function referrerContains(val) {
        return document.referrer.toLowerCase().indexOf(val) != -1;
    }

    channels = [
        {
            'name' : 'DPS',
            'predicate' : function(ref, loc) {
                if (window.partner && (pathStartsWith('/partner/') || pathStartsWith('/comingsoon.html'))) {
                    return [true, partner.PartnerID];
                }
                return [false, ''];
            },
            'page' : function(ref, loc, current_page, kwrd) {
                return partner.PartnerID;
            }
        },
        {
            'name' : 'SEM',
            'predicate' : function(ref, loc) {
                var isSEM = referrerContains('/aclk') && queryContains('cid=sem');
                if (isSEM) {
                    return [true, queryParam('s_kwcid')];
                } else {
                    return [false, ''];
                }
            }
        },
        {
            'name' : 'EDM',
            'predicate' : function(ref, loc) {
                return [queryContains('cid=edm'), queryParam('cid')];
            },
            'page' : function(ref, loc, current_page, kwrd) {
                var details = kwrd.split(':');
                if (details[details.length -1].match(/\d+/) != null) {
                    return details[details.length - 1];
                }
                return current_page;
            }
        },
        {
            'name' : 'SMO',
            'predicate' : function(ref, loc) {
                return [queryContains('cid=social'), queryParam('cid')];
            },
            'page' : function(ref, loc, current_page, kwrd) {
                var details = kwrd.split(':');
                if (details[details.length -1].match(/\d+/) != null) {
                    return details[details.length - 1];
                }
                return current_page;
            }
        },
        {
            'name' : 'TRP',
            'predicate' : function(ref, loc) {
                return [queryContains('cid=trpadv'), queryParam('cid')];
            }
        },
        {
            'name' : 'SEO',
            'predicate' : function(ref, loc) {
                bots = [['google', []], ['yahoo', []], ['bing', ['q']], ['ask.com', ['q']], ['aol.com', ['q','query']], ['wow.com', []], ['webcrawler', []], ['duckduckgo', []], ['info.com', []], ['yandex', ['text']], ['baidu', ['wd']]];
                for (var i = 0; i < bots.length; i++) {
                    var bot = bots[i];
                    if (referrerContains(bot[0])) {
                        if (bot[1].length == 0) return [true, bot[0]]
                        for (var j = 0; j < bot[1].length; j++) {
                            if (referrerContains(bot[1][j] + '=')) return [true, bot[0]];
                        }
                    }
                }
                return [false, ''];
            }
        }
    ];

    var ref = document.referrer.toLowerCase();
    var loc = document.location.href.toLowerCase();
    for (var i = 0; i < channels.length; i++) {
        var channel = channels[i];
        var matched = channel.predicate(ref, loc);
        if (matched[0]) {
            var name = channel.name;
            var pid = '000';
            var kwrd = matched[1];
            if (channel.page) {
                pid = channel.page(ref, loc, pid, kwrd);
            }

            var overrides = window.a_rch || {};
            if (overrides.channel) { name = overrides.channel; }
            if (overrides.page) { pid = overrides.page; }
            if (overrides.kwrd) { kwrd = overrides.kwrd; }

            var value = name + '|' + pid + '|' + kwrd;
            var expires = new Date();
            expires.setTime(expires.getTime() + 30 * 86400000);
            document.cookie = "a_rch=" + value + '; expires=' + expires + '; domain=.asiawebdirect.com; path=/';
            break;
        }
    }

})();

jQuery(document).ready(function () {
    var tpix = window.tpix || {};
    var ch   = 'DTI';
    var kw   = '';
    var rch  = getCookie('a_rch');
    var page = '000';

    if (rch != null && rch.indexOf('|') != -1) { // new cookie format
        var details = rch.split('|');
        ch = details[0];
        page = details[1];
        kw = details[2];
    } else if (rch != null && rch.indexOf(':') != -1) { // old cookie format
        var details = rch.split(':');
        ch = details[0];
        kw = details.slice(1).join(":");
    }

    var exPxDomains = [['latestays.com', 'APLSUS'],['latestays.com.my', 'APLSMY'],['latestays.co.th', 'APLSTH'],['latestays.ru', 'APLSRU'],['latestays.co.id', 'APLSID'],['latestays.co.kr', 'APLSKR']];
    var pxDomain = 'www.asiawebdirect.com';
    var pId  = 'APAW1';
    var host = document.location.hostname;
    for (var i = 0; i < exPxDomains.length; i++) {
        var domain = exPxDomains[i];
        if (host.endsWith(domain[0])) {
            pxDomain = 'awd.' + domain[0];
            pId = domain[1];
        }
    }

    var aRef = encodeURIComponent('awd.' + page + '.' + ch + '.kwrd=' + kw);
    var hId  = encodeURIComponent(tpix.hId || '');
    var lcat = encodeURIComponent(tpix.lcat || '5');
    var pr   = encodeURIComponent(document.referrer || '');
    var qs   = 'pId=' + pId + '&aRef=' + aRef + '&hId=' + hId + '&lcat=' + lcat + '&pr=' + pr;
    (new Image()).src = '//' + pxDomain + '/partner/rt/b.gif?' + qs;
});
