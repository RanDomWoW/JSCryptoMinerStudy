<html>
<head>
  <title>Horde System</title>
</head>

<frameset rows="50,*" frameborder="no" border=0 name="horde" framespacing=0>
  
  <frameset cols="250,*" frameborder="no" border=0 name="upper">
      <frame name="impfolder"
         src="menu.php3?menu=999"
         scrolling="no"
	 frameborder="no"
      >
      <frame name="hordestatus"
         src="status.php3"
         scrolling="no"
	 frameborder="no"
      >
   </frameset>
    
  
    <frameset rows="100%" cols="130,*" frameborder="no" border=0 name="lower">

      <frameset rows="50%,*" cols="100%" frameborder="no" border=0 name="sidebar">
        <frame name="menu"
          src="menu.php3"
        >
        <frame name="modules"
          src="modules.php3"
        >
      </frameset>
    
      <frame name="main"
        src="./imp/login.php3"
      >
    </frameset>

  </frameset>

</html>