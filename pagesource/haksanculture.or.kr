<script type="text/javascript">
//<![CDATA[
jQuery(document).ready(function(){
	var _stopTime  = 4000; // 멈춰있는 시간
	var _currDiv = 1; // 선택된 엘리먼트 (0번부터 시작)
	var _eventCnt = jQuery("#popzone ul li").length;
	var load_currdiv = true;

	jQuery("#popzone ul li").each(function(index){
		jQuery(this).click(function(){
			var _chkDiv = index + 1;
			var _total_Div = _eventCnt+1;
			for(var i = 1 ; i < _total_Div ; i++){			
				if(_chkDiv == i){
					if(i<10){
						jQuery("#pop_cate0"+i).addClass("over");
						jQuery("#pop_visual0"+i).show();
					}else{
						jQuery("#pop_cate"+i).addClass("over");
						jQuery("#pop_visual"+i).show();
					}
				}else{
					if(i<10){				
						jQuery("#pop_cate0"+i).removeClass("over");
						jQuery("#pop_visual0"+i).hide();
					}else{
						jQuery("#pop_cate"+i).removeClass("over");
						jQuery("#pop_visual"+i).hide();
					}
				}
			}
		});		
	});
	
	var run_popZone_roll = function(){
		jQuery("#popzone ul li").each(function(index){ 
			var i = index + 1;
			if(_currDiv == i){
				if(i<10){
					jQuery("#pop_cate0"+i).addClass("over");  
					jQuery("#pop_visual0"+i).fadeIn(2000);
				}else{
					jQuery("#pop_cate"+i).addClass("over");  
					jQuery("#pop_visual"+i).fadeIn(2000);
				}		
				//2013 가족음악회 팝업존 시간 늘리기 
				if(jQuery("#pop_visual0"+i).find("a").attr('href')=='/board/view.php?code=notice&cat=&sq=363'){		
					//alert("aaa");
					//true 면 false로,false면 true로 					
					if(load_currdiv){load_currdiv = false;
					}else{load_currdiv = true;}
				}
			}else{
				if(i<10){   
					jQuery("#pop_cate0"+i).removeClass("over");
					jQuery("#pop_visual0"+i).fadeOut(1000);
				}else{
					jQuery("#pop_cate"+i).removeClass("over");
					jQuery("#pop_visual"+i).fadeOut(1000);
				}
			}
		});
		
		if(load_currdiv){
		_currDiv++;
		}

		if(_currDiv > _eventCnt){
			_currDiv = 1;
		}
	}	
	timer = setInterval(run_popZone_roll, _stopTime);

	jQuery("#popzone ul li").hover(
		function(){
			clearInterval(timer);	
		},function(){
			timer = setInterval(run_popZone_roll, _stopTime);
	});	
});
//]]>
</script>
