    <script type="text/javascript">
        var _redirect = '/default.aspx';
        var leave = Math.round(1);
        $(document).ready(function () {
            /*if (_redirect.indexOf("default.aspx") >= 0) {
                _redirect = 'http://truykich.vn/noel';
            }*/
            if (leave == 0) {
                document.location.href = _redirect;
                return;
            } else {
                SANAjax();
                intervalId = setInterval("SANAjax();", 100);
                $.ajaxSetup({ cache: false });
            }
        });

        function SANAjax() {
            if (_redirect.length <= 0) {
                clearInterval(intervalId);
                return;
            }
            if (leave < 0) {
                clearInterval(intervalId);
                document.location.href = _redirect;
                return;
            }

            var day = Math.floor(leave / (60 * 60 * 24))
            var hour = Math.floor(leave / 3600) - (day * 24)
            var minute = Math.floor(leave / 60) - (day * 24 * 60) - (hour * 60)
            var second = Math.floor(leave) - (day * 24 * 60 * 60) - (hour * 60 * 60) - (minute * 60)

            hour = hour < 10 ? "0" + hour : hour;
            minute = minute < 10 ? "0" + minute : minute;
            second = second < 10 ? "0" + second : second;
            $('#sdelay').html(second);
            leave = leave - 1;
        }
    </script>
