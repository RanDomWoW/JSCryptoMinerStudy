var timeout = '10000'
if (typeof siteExclude != "undefined"){
var allExclusions = exclusions.concat(siteExclude);
} else if (typeof exclusions !== 'undefined'){
var allExclusions = exclusions;
}
if (typeof timeout == "undefined"){
var timeout = 20000
}
var checkString = location.search.toLowerCase(),
fromEmail = 'false',
showSignupModal = true,
customPop = "";
fromEmail = sessionStorage.getItem('setEmail');
if (fromEmail == 'false' || fromEmail == null){
if (checkString.indexOf("email") >= 0) {
sessionStorage.setItem('setEmail', 'true');
fromEmail = sessionStorage.getItem('setEmail');
} else {
sessionStorage.setItem('setEmail', 'false');
fromEmail = sessionStorage.getItem('setEmail');
}
}
if (typeof allExclusions !== "undefined") {
$.each(allExclusions, function(key, value) {
var data = value.split(',');
var checkpage = data[0];
var showpage = data[1];
if (showpage == null){
showpage = 'none';
}
if (window.location.pathname.match(checkpage)) {
if (showpage == 'none'){
//don't show the popup on this page
showSignupModal = false;
}else{
//set the custompop CID to the selected CID
customPop = showpage;
}
return false;
}
});
}
if (fromEmail == 'false'){
var start_time = new Date().getTime();
var orig_start = sessionStorage.getItem('initialload');
if (!orig_start){
sessionStorage.setItem('initialload', start_time);
var orig_start = sessionStorage.getItem('initialload');
}
var timer_test = setInterval(function(){
var curr_time = new Date().getTime();
var diff = Math.abs((orig_start-curr_time));
if (diff > timeout){
app.marketingCloud.init(showSignupModal, customPop);
clearInterval(timer_test);
timer_test=0;
}
}, 1000);
}
