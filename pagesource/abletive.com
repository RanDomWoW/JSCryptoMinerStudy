<script type="text/javascript">
function submitRegister(){
    if ($('input#user_pass2').val() != $('input#user_pass').val()){
        $('p.status').text('密码不一致');
        return;
    }
    if (!$('input#user_name').val() || !$('input#user_email').val() || !$('input#user_pass').val()){
        $('p.status').text('请填写空白项');
        return;
    }
    $('p.status').text('正在注册中...请稍等(如果长时间无响应请重试，有可能已注册成功)');
    $('input.submit').fadeOut();
    jQuery.ajax({
        url: "http://abletive.com/api/get_nonce/?controller=User&method=register",
        type: "GET",
        dataType: 'json',
        success: function(results){
            nonce = results['nonce'];
            doRegister(nonce);
        }
    });
}
    
function doRegister(nonce){
    var data = new Array();
    data['username'] = $('input#user_name').val();
    data['display_name'] = $('input#user_name').val();
    data['email'] = $('input#user_email').val();
    data['user_pass'] = $('input#user_pass').val();
    data['nonce'] = nonce;
    
    jQuery.ajax({
        url: "http://abletive.com/api/user/register/?username="+data['username']+"&user_pass="+data['user_pass']+"&email="+data['email']+"&display_name="+data['display_name']+"&nonce="+data['nonce'],
        type: "GET",
        dataType: 'json',
        success: function(results){
            if(results['status'] == 'ok'){
                $('p.status').text('注册成功！');
                setTimeout(function(){
                    $('#login-active').click();
                    $('.loginPart input#username').val(data['username']);
                    $('.loginPart input#password').val(data['user_pass']);
                    $('form#login').submit();
                },600);
            } else {
                $('input.submit').fadeIn();
                if (results['error'].indexOf('username')){
                    $('p.status').text('用户名或邮箱已被注册？');
					console.log(results['error'].indexOf('username'));

                } else if (results['error'].indexOf('email')){
                    $('p.status').text('邮箱错误，是否已经被注册？');
                } else {
                    $('p.status').text('出错了 请重试');
                }
                
            }
        }
    });
}
