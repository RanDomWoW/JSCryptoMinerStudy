    <script>
        jQuery(document).ready(function () {

            function cargoSlider(sliderid,sectorid,selectid){
                /*cargo el sliderid con el sectorid y inicializo el carousel*/
                $("#"+sliderid).load("/Item/Related",{sector : sectorid, max : 48, random : false, complete : false, view:"Related"},function(){
                    $("#"+sliderid).owlCarousel({
                        paginationSpeed: 400,
                        lazyLoad: true,
                        
                        autoplay: false,
                        autoplayHoverPause: true,
                        
                        loop: true,
                        nav: true,
                        margin: 20,
                        dots: false,
                        responsive: {
                            0: {items: 2},
                            380: {items: 2, slideBy:2},
                            500: {items: 3, slideBy:3},
                            992: {items: 4, slideBy:4},
                            1199: {items: 5, slideBy:5}
                        }
                    });
                    /*busco en las opciones de plataforma, y las que no etan en el sector las oculto*/
                    $("#"+selectid+" option").each(function () {
                        if($(this).val()!=0)
                        if (!$("#"+ sliderid +" article[data-platformid='" + $(this).val() + "']").length) {
                            $(this).hide();
                        }
                    });
                });

                /*si selecciono otra plataforma hace la consulta de ajax del sector, si es sucess: destruyo el carousel, me trae el html de la plataforma, 
                    y vuelvo a inicializar el carouel en el sector
                */
                $("#"+selectid).change(function () {
                    var idplataforma = $(this).val();
                    $.ajax({
                        url: '/Item/Related/?sector='+sectorid+'&max=48&random=false&complete=false&view=relatedp&plataforma='+idplataforma,
                        type: "GET",
                        success: function (response) {
                            $("#"+sliderid).data('owlCarousel').destroy();
                            $("#"+sliderid).html(response);
                            $("#"+sliderid).owlCarousel({
                                paginationSpeed: 400,
                                lazyLoad: true,
                                
                                autoplay: false,
                                autoplayHoverPause: true,
                                
                                loop: true,
                                nav: true,
                                margin: 20,
                                dots: false,
                                responsive: {
                                    0: {items: 2},
                                    380: {items: 2, slideBy:2},
                                    500: {items: 3, slideBy:3},
                                    992: {items: 4, slideBy:4},
                                    1199: {items: 5, slideBy:5}
                                }
                            });
                        }
                    });
                });
            }

            /*inicializo el slider con el siguiente formato*/
            /*function cargoSlider(sliderid,sectorid,selectid)*/

$.ajax({
    url: "/Banner/Index",
    type: "POST",
    data: {id : "home-slide-1920", view : "IndexSlide"},
