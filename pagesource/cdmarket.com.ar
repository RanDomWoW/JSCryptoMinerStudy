            	if(($(this).attr('data-parent') == 999) && ($(this).attr('data-sb-filtervalue') == 999)){
		                form.find("input[name='Filters']").val("");
               			form.find(".dropdown-toggle").html($(this).html()+'<span class="caret"></span>');
               			return;
            	}
                
                
                form.find("input[name='Filters']").val($(this).attr('data-parent')+',|'+ $(this).attr('data-sb-filtervalue')+'|');

                form.find(".dropdown-toggle").html($(this).html()+'<span class="caret"></span>');
            });    

            $('select#_sb_setplatform').change(function(){
            		//alert($('#_sb_setplatform option:checked').val())
            		var form = $(this).parents('form');
	            	if(($('#_sb_setplatform option:checked').attr('data-parent') == 999) && ($('#_sb_setplatform option:checked').attr('data-sb-filtervalue') == 999)){
			                form.find("input[name='Filters']").val("");
	               			return;
	            	}
	            	form.find("input[name='Filters']").val($('#_sb_setplatform option:checked').attr('data-parent')+',|'+ $('#_sb_setplatform option:checked').attr('data-sb-filtervalue')+'|');
            })

            $("#contactId").click(function () { 
            $.ajax({
                type: 'POST',
                url: '/Contact/Form',
                data: { id: 3 },
                error: function () {
                    alert("error");
                }
            }).done(function (result) {               
                $("._contact_holder").empty();
                $("._contact_holder").html(result);
            });
            });
        });

    </script>
    

