                    if(geoIPLat && geoIPLong) {
                        geolocationFindFromLatLng(geoIPLat, geoIPLong);
                    }

                }
            });
        }

        function geolocationFindFromLatLng(latitude, longitude) {

            if(typeof google === 'undefined') return;
            var latlng = new google.maps.LatLng(latitude, longitude);
            var google_geocoder = new google.maps.Geocoder();

            google_geocoder.geocode({'latLng': latlng}, function(results, status) {
                if(status === google.maps.GeocoderStatus.OK) {
                    if(results[0]) {
                        var components = results[0].address_components;
                        for(var i = 0; i < components.length; i++) {
                            for(var j = 0; j < components[i].types.length; j++) {
                                if(components[i].types[j] === 'postal_code') {
                                    var zip_code = components[i].long_name;
                                }
                            }
                        }
                        if(zip_code) {
                            jQuery.ajax({
                                url: '/locator/locator/getStoresCollection/',
                                type: 'GET',
                                data: {zipcode: zip_code},
                                success: function(stores) {
                                    var storesList = jQuery.parseJSON(stores);
                                    if(storesList['nearstore'][0]) {
                                        var store_code = storesList['nearstore'][0]['id'];
                                        DndLocatorPopin.editCookie(zip_code, store_code);
                                    }
                                }
                            });
                        }
                    }
                }
            });

        }

        var DndLocatorPopin = new Dnd.LocatorPopin({"timeLife":"0","showCount":"0","timeShow":"0"});
        if(DndLocatorPopin.checkCookie()) {

            DndLocatorPopin.editCookie();

            if(navigator.geolocation) {

                navigator.geolocation.getCurrentPosition(function(position) {
                    if(position.coords.latitude && position.coords.longitude) {
                        geolocationFindFromLatLng(position.coords.latitude, position.coords.longitude);

                        Mage.Cookies.set('customer_latitude', position.coords.latitude, null, '/');
                        Mage.Cookies.set('customer_longitude', position.coords.longitude, null, '/');
                        Mage.Cookies.set('customer_geolocation', 'accept', null, '/');

                        if($$('body')[0].hasClassName('sqli-stores-stores-view')) {
                            customerLatLng = new google.maps.LatLng(position.coords.latitude, position.coords.longitude);
                            createLine(customerLatLng);
                        }

                    }
                }, geolocationFallback);

            }
            else {
                geolocationFallback();
            }
        }
        else {
            //console.log('cookie already exists');
        }

    };
    //]]>
</script>
