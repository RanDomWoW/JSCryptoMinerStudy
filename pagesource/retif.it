<script type="text/javascript">
//<![CDATA[

var RetifMenu = {
    default_sector: '',
    current_sector: Mage.Cookies.get('current_sector'),

    init: function() {
        if ( ! this.current_sector) this.current_sector = '0';

        var categories_menu_height = 505;
        var categories_menu_count = jQuery('#categories-vertical-menu .main-category').length;
        var category_line_height = Math.floor(categories_menu_height / categories_menu_count);

        jQuery('#categories-vertical-menu .main-category a').css('height', category_line_height+'px');
        jQuery('#categories-vertical-menu .main-category a').css('line-height', category_line_height+'px');

        RetifMenu.bindEvents();

        RetifMenu.sort();
    },

    bindEvents: function() {

        jQuery("#categories-vertical-menu .main-categories").menuAim({
             activate: RetifMenu.openMainCategory,
             deactivate: function() { },
             exitMenu: function() { return true; },
             rowSelector: '.main-category'
         });

        jQuery('#menu_products').bind('mouseenter', function () {
            jQuery('#categories-vertical-menu').addClass('show'); 
            jQuery('#menu_products').addClass('open'); 
        });

        jQuery('#menu-top').bind('mouseleave', function () { RetifMenu.closeMainMenu(); });
        jQuery('#menu_back, #menu_food, #menu_devis, #menu_jobs').bind('mouseenter', function () { RetifMenu.closeMainMenu(); });
    },

    closeMainMenu: function() {
        if ( ! jQuery('#categories-vertical-menu').hasClass('fixed')) {
            jQuery('#categories-vertical-menu').removeClass('show');
            jQuery('#menu_products').removeClass('open'); 
        }
    },

    openMainCategory: function(e) {
        RetifMenu.highlightMainCategory(e);
        jQuery('#categories-vertical-menu .category-details').css('display', 'none'); 
        var details = jQuery('#categories-vertical-menu .category-details[data-category="'+jQuery(e).attr('data-category')+'"]'); 
        details.css('display', 'block'); 
        details.find('.category-image-right').attr('src', details.find('.category-image-right').attr('data-original')).css('display', 'block');
        RetifMenu.formatColumns(details);
    },

    highlightMainCategory: function(e) {
       jQuery(e).parent().children().removeClass('current');
       jQuery(e).addClass('current');
    },

    sort: function() {
        jQuery('.main-categories, .sub-categories .column1, .sub-sub-categories').each(function( index ) {
            jQuery(this).children().sort(function (a, b) {
                return +a.getAttribute('data-position') - +b.getAttribute('data-position');
            }).appendTo(jQuery(this));
        });
    },

    formatColumns: function($block) {
        var current_column = 1;
        var column_size = 0;
        var max_size = 480;
        $block.find('.column1 .sub-category').each(function( index ) {
            var block_size = jQuery(this).height();
            column_size += block_size + 10;
            if (column_size > max_size || current_column > 1) {
                if (column_size > max_size) {
                    current_column++;
                    column_size = block_size + 10;
                }
                $block.find('.column'+current_column).append(this);
            }
        });
    },

    cut: function(str, max) {
        if (str.length > max) {
            var pieces = str.split(' ');
            str = '';
            for (var i = 0; i < pieces.length; i++) {
                if ((str + pieces[i]).length > max) break;
                str += pieces[i] + ' ';
            }
