function searchActivities(page) {
    var source   = $("#activities-template").html();
    var template = Handlebars.compile(source);
    var url = "/activities/get.json?limit=15&page=" + page + '&filter=' + $("input#search").val();
    $("#load-more-activities").show();
    $.getJSON(url, function(json) {
        var html = {};
        maxPages = parseInt(json.maxPage);
        html = template(json);
        $("#activities-container").children().remove();
        $("#activities-container").append(html);
        if (maxPages <= 1) {
            $("#load-more-activities").hide();
        }
    });
}

function appendActivities(page) {
    var source   = $("#activities-template").html();
    var template = Handlebars.compile(source);

    var url = "/activities/get.json?limit=15&page=" + page;
    $("#activities-container").parent().layerLoader();

    $.getJSON(url, function(json) {
        var html = {};
        maxPages = parseInt(json.maxPage);
        html = template(json);
        $("#activities-container").append(html);
        $("#activities-container").parent().layerLoader("stop");
    });
}
