jQuery(function ($) {
    var image = "/img/icon-map.png", cities, location, url;
    var markers = [], info = [];
    var spain = {lat: 40.27952566881291, lng: -4.2626953125};
    var map = new google.maps.Map(document.getElementById('map-canvas'), {
        zoom: 6,
        center: spain,
        styles: mapTheme,
        scrollwheel: false,
        disableDefaultUI: true
    });
    var bounds = new google.maps.LatLngBounds();
    $.ajax({
        'async': false,
        'global': false,
        'url': "/cities/mapTerritoriesList.json",
        'dataType': "json",
        'success': function (data) {
            $.each(data.result, function(i, territories) {
                var arr = [];
                arr = $.map(territories, function(city) {
                  return city.id;
                });
                $(".selector-cities").append('<li> <a href="/map" data-city = ' + JSON.stringify(arr) +'>'+ i + '</a> </li>');
                $(territories).each(function(index, item) {
                    $(item.centers).each(function(i, center) {
                        if (center.place) {
                            location= {lat: center.place.latitude, lng: center.place.longitude}
                            var marker = new google.maps.Marker({
                                position: location,
                                map: map,
                                icon: image,
                                type: item.id,
                                set_center: true,
                            });
                            markers.push(marker);
                            url = '/gimnasio/' + center.slug;

                            var contentString = '<a href ='+ url +'><div class="map-content"><h5>'+ center.name +'</h4><p>' + center.place.address +'</p> <p>' + center.phone +'</p></div></a>';

                            info[center.id] = new InfoBubble({
                                map: map,
                                content: contentString,
                                backgroundColor: '#262626',
                                hideCloseButton: true,
                                borderRadius: 0,
                                borderColor: '#262626',
                                minHeight: 180,
                                maxWidth: 300
                            });

                            google.maps.event.addListener(marker, "click", function() {
                                closeInfo();
                                info[center.id].open(map, marker);
                            });
                        }
                    });
                });
            });
        }
    });

    function closeInfo() {
        for (var i=0; i < info.length; i++) {
            if (info[i]) {
               info[i].close();
           }
        }
    }
    function fitBoundsToVisibleMarkers() {
        var bounds = new google.maps.LatLngBounds();
        for (var i=0; i < markers.length; i++) {
            if(markers[i].getVisible()) {
                bounds.extend( markers[i].getPosition() );
            }
        }
        map.fitBounds(bounds);
    }
    $("a[href='/map']").on('click', function(e) {
        e.preventDefault();
        var id = $(this).data("city");
        var countVisible = 0, position;
        closeInfo();
        $(".selector-cities li").removeClass("selected");
        $(this).parent().addClass("selected");
        for (var i = 0; i < markers.length; i++) {
            markers[i].setVisible(false);
        }
        for (var i = 0; i < markers.length; i++) {
            $.each(id, function(key, item) {
                if (parseInt(markers[i].type) == parseInt(item)) {
                    markers[i].setVisible(true);
                    countVisible ++;
                    position = markers[i].getPosition();
                }
            });
        }
        if (countVisible > 1){
            fitBoundsToVisibleMarkers();
        }
        else {
            map.setCenter(position);
            map.setZoom(11);
        }
    });
});

