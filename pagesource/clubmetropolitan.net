//<![CDATA[
jQuery(function ($) {
    function redirectToActivityByCity (city, activity, schedule) {
        let link = document.createElement('a');
        link.href = "/actividad/ACTIVITY_SLUG/CITY_SLUG"
            .replace('ACTIVITY_SLUG', activity)
            .replace('CITY_SLUG', city)
        ;
        if (schedule) {
            link.search = 'schedule=' + schedule;
        }

        document.location.href = link;
    }
    $('#schedule-activity-city-selector').find('select').chosen();

    let layerloader = $('#schedule-activity-city-selector').layerLoader('Cargando').data('api');
    $('#schedule-activity-city-selector').on('submit', function (e) {
        let _this = $(this);
        let mode = _this.find('input[name=mode]').val();
        let city = _this.find('#city').val();
        let activity = _this.find('#activity').val();
        let range = _this.find('#schedule').val();

        if (!city.length || !activity.length) {
            $('.schedules-container').html('Seleccione ciudad y actividad para mostrar los horarios');
            return false;
        }

        if (mode === 'redirect') {
            redirectToActivityByCity(city, activity, range);
            e.preventDefault();
        }

        if (activity !== "") {
            redirectToActivityByCity(city, activity, range);
            e.preventDefault();
        }

        // inline mode does an ajax call to retrieve all the centers for the city
        layerloader.start();
        let call = $.ajax({
            type: 'get',
            dataType: 'json',
            url: "/centers/getListByCity/CITY_SLUG.json".replace('CITY_SLUG', city)
        });
        call.done(function (data) {
            $('.schedules-container').empty();
            let total = 0;
            $.each(data.centers, function (center, name) {
                let schedule = $('<div>', {
                    id: 'schedule-' + center,
                    class: 'schedule-multiple'
                });
                let settings = {
                    center: center,
                    activityFilter: activity,
                    printActivityGroups: false,
                    centerName: name
                };
                if (range) {
                    settings.range = range;
                }
                $('.schedules-container').append(schedule);
                schedule.timeschedule(settings);
                if (++total >= Object.keys(data.centers).length) {
                    layerloader.stop();
                    // A bit dirty, but I don't care nigga
                    setTimeout(function () {
                        if (!$('.schedules-container').text().length) {
                            let empty = $('<p>').text('La actividad escogida no está disponible en esta ciudad.');
                            $('.schedules-container').append(empty);
                        }
                    }, 1000);
                }
            });
        });
        call.fail(function (xhr, status, error) {
            alert('Error. Vuelva a intentarlo más tarde por favor.');
            layerloader.stop();
        });

        e.preventDefault();
    });

    if (false) {
        $('#schedule-activity-city-selector').trigger('submit');
    }
});
