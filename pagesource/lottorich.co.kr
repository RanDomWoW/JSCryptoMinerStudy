	function nav_smenu(_type, _id, _num){
		if(_type == "block" && _num != "06"){
			document.getElementById("tmenu_"+_num).style.backgroundImage	= "url('//image.lottorich.co.kr/lottorich/main_new/navi_mn"+_num+"_on.png?v=20170310')";
		}else{
			document.getElementById("tmenu_"+_num).style.backgroundImage	= "url('//image.lottorich.co.kr/lottorich/main_new/navi_mn"+_num+".png?v=20170310')";
		}
		document.getElementById("tmenu_"+_num).style.backgroundRepeat	= "no-repeat";

		for(var i = 1; i <= 6; i++){
			if(i == _num * 1){
				if(i == 1){
					if(_type == "block"){
						document.getElementById("nav_smenu_s"+i).className	= "first on";
					}else{
						document.getElementById("nav_smenu_s"+i).className	= "first";
					}
				}else{
					if(_type == "block"){
						document.getElementById("nav_smenu_s"+i).className	= "on";
					}else{
						document.getElementById("nav_smenu_s"+i).className	= "";
					}
				}
			}else{
				if(i == 1){
					document.getElementById("nav_smenu_s"+i).className	= "first";
				}else{
					document.getElementById("nav_smenu_s"+i).className	= "";
				}
			}
		}
	}

	function nav_smenu_all(_type){
		document.getElementById("nav_2dep_layer").style.display		= _type;
	}

	function siteMap(_type){
		document.getElementById("sitemap_bg").style.display		= _type;
		if(checkUA() == "ie7"){
			document.getElementById("sitemap_bg").style.height	= document.documentElement.scrollHeight;
			document.getElementById("gnbsitemap").style.zIndex	= "1100";
		}else{
			document.getElementById("sitemap_bg").style.height	= document.body.scrollHeight;
		}
		document.getElementById("sitemap").style.display			= _type;

		if(_type == "none"){
			$('html').css('overflow','auto');
		}else if(_type == "block"){
			$('html').css('overflow','hidden');
		}
	}

