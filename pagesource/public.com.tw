<script type="text/javascript">
    $.imgReload = function () {
        var loaded = 1;
        var retries = 0;
        $("img").each(function () {
            if (!this.complete || typeof this.naturalWidth == "undefined" || this.naturalWidth == 0) {
                var src = $(this).attr("src");
                var date = new Date();
                $(this).attr("src", src + "?v=" + date.getTime());
                loaded = 0;
            }
        });
        retries += 1;
        if (retries < 1) {
            if (loaded == 0) {
                setTimeout('$.imgReload()', 3000); // I think 4 seconds is enough to load a small image (<50k) from a slow server
            }
        }
        else {
            $('img').each(function () {
                if (!this.complete || typeof this.naturalWidth == "undefined" || this.naturalWidth == 0) {
                    this.src = 'images/nophoto.jpg?v=' + date.getTime();
                }
            });
        }
    }
    $(document).ready(function () {

        if ($.browser.msie) {//IE
            document.body.oncopy = function () {
                setTimeout(
                    function () {
                        var text = clipboardData.getData("text");
                        if (text) {
                            text = text + "文章來源：志光 文章位置：" + location.href; clipboardData.setData("text", text);
                        }
                    }, 100)
            }
        } else {
            document.oncopy = addLink;
        }
        setTimeout('$.imgReload()', 3000);
    });

    function ShowBanner() {
        $("#myLogin").show();
        $.colorbox({
            href: "#myLogin",
            inline: true,
            width: "50%",
            overlayClose: false,
            onClosed: function () { $("#myLogin").hide(); }
        });
    }

    function login() {
        var $path_base = "/Member/";
        var id = $("#id").val();
        var pw = $("#pw").val();
        var keep = $("#keep").prop('checked') == true ? "true" : "false";

        if (id == '' || pw == '') {
            alert('請輸入帳號或密碼！！');
            return;
        }

        $.ajax({
            url: $path_base + 'getMember_Info',
            type: "POST",
            data: {
                id: id,
                pw: pw,
                keep: keep
            },
            success: function (states) {
                var person = states;
                if (person.Status == "1") {
                    location.reload();
                }
                else if (person.Status == "2") {
                    alert(person.msg);
                }
                else {
                    alert("資料有誤！！");
                }

            },
            error: function (jqXHR, textStatus, errorThrown) {
                alert("資料有誤！！");
                $.colorbox.close();
            }
        });
    }

    function logout() {
        var $path_base = "/Member/";
        var r = confirm("確定登出嗎?");
        if (r == true) {

            $.ajax({
                url: $path_base + 'logout',
                type: "POST",
                data: {

                },
                success: function (states) {
                    //$(".logout_2").html("會員登入").prop("href", "#myLogin").attr("class", "ColorLogin");
                    $(".ColorLogin").colorbox({ inline: true, width: "50%", overlayClose: false });
                    location.href = "/";
                },
                error: function (jqXHR, textStatus, errorThrown) {

                }
            });

        } else {
            return false;
        }
    }

    window.onload = function () {
        this.focus();
    }
    function addLink() {
        var body_element = document.getElementsByTagName('body')[0];
        var selection;
        selection = window.getSelection();
        var pagelink = "文章來源：志光 文章位置： <a href='" + document.location.href + "'>" + document.location.href + "</a>";
        var copytext = selection + pagelink;
        var newdiv = document.createElement('div');
        newdiv.style.position = 'absolute';
        newdiv.style.left = '-99999px';
        body_element.appendChild(newdiv);
        newdiv.innerHTML = copytext;
        selection.selectAllChildren(newdiv);
        window.setTimeout(function () {
            body_element.removeChild(newdiv);
        }, 0);

    }

    //半形
    function calculate(id, value) {

        result = "";
        if (value != '') {
            for (i = 0; i < value.length; i++) {

                if (value.charCodeAt(i) == 12288) {
                    result += " ";
                } else {

                    if (value.charCodeAt(i) > 65280 && value.charCodeAt(i) < 65375) {

                        result += String.fromCharCode(value.charCodeAt(i) - 65248);

                    } else {

                        result += String.fromCharCode(value.charCodeAt(i));

                    }
                }

            }
            $("#" + id).val(result);
        }
    }

    function getInfo() {
        var $path_base = "/Member/";
        var formURL = $path_base + "getMemberInfo";
        var posting = $.post(formURL);
        posting.done(function (data) {
            if (data.status == "1") {
                var t = $("#form1").children();
                $.each(data, function (i, item) {
                    t.find('label').each(function () {
                        getKey = $(this).attr("id");
                        if (getKey == i) {
                            $(this).text(item);
                        }
                    });

                    t.find('input[type=text],input[type=hidden]').each(function () {
                        getKey = $(this).attr("id");
                        if (getKey == i) {
                            $(this).val(item);
                        }
                    });

                    t.find('select').each(function () {
                        getKey = $(this).attr("id");
                        if (getKey == i) {
                            $("#" + getKey + " option").filter(function () { return this.value == item; }).prop('selected', true);
                        }

                        if (i == "ddlCounty") {
                            district($("#ddlCounty").val());
                        }
                    });


                    t.find('input:radio').each(function () {
                        getKey = $(this).attr("id");
                        if (getKey == i + item) {
                            $("#" + getKey).prop('checked', true)
                        }
                    });

                    t.find('input[type=checkbox]').each(function () {
                        getKey = $(this).attr("id");
                        var values = "";
                        values = item;
                        $.each(values.split(","), function (k, e) {
                            if (getKey == i + "_" + e) {
                                $("#" + getKey).prop('checked', true);
                            }
                        });
                    });

                });
                //alert(data.msg);
            }
            else {
                alert(data.msg);
                ShowBanner();
            };

        });

        return false;
    }
</script>