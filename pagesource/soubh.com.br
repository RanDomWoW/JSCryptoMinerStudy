            $(document).ready(function(){
                $('#enviar-report').on('click', function(e){
                    e.preventDefault();
                    if($('.form').parsley().validate()){
                        var $assuntoSelectedIndex = $('#assunto').selectedIndex
                        $.ajax({
                            url : "/send-email/",
                            type: "POST",
                            data : {
                                nome: document.getElementsByName('nome')[0].value,
                                email: document.getElementsByName('email')[0].value,
                                url: document.getElementsByName('url')[0].value,
                                mensagem: document.getElementsByName('mensagem')[0].value,
                                form_type: document.getElementsByName('form_type')[0].value
                            },
                            dataType : "json",
                            beforeSend: function(xhr, settings) {
                                 function getCookie(name) {
                                     var cookieValue = null;
                                     if (document.cookie && document.cookie != '') {
                                         var cookies = document.cookie.split(';');
                                         for (var i = 0; i < cookies.length; i++) {
                                             var cookie = jQuery.trim(cookies[i]);
                                         if (cookie.substring(0, name.length + 1) == (name + '=')) {
                                             cookieValue = decodeURIComponent(cookie.substring(name.length + 1));
                                             break;
                                         }
                                     }
                                 }
                                 return cookieValue;
                                 }
                                 if (!(/^http:.*/.test(settings.url) || /^https:.*/.test(settings.url))) {
                                     xhr.setRequestHeader("X-CSRFToken", getCookie('csrftoken'));
                                 }
                            },
                            success: function( data ){
                                if(data.message == 'sucesso'){
                                    $('#retorno').next('form').fadeOut();
                                    $('#retorno').html('<h1>Sua mensagem foi enviada!</h1> Aguarde que em breve entraremos em contato...');
                                    $('#retorno').fadeIn();
                                }else{
                                    $('#retorno').html('<h1>Ops...</h1>Algo inesperado aconteceu... Tente enviar sua mensagem novamente.');
                                    $('#retorno').fadeIn();
                                }
                            },
                            error : function(e) {
                                console.log(e);
                            }
                        });
                    }
                });
            });

        </script>
        
    

