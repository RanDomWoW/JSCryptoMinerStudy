    <script type="text/javascript">
        function popupWindow(url, name, width, height, scrollbars) {
            //  if (arguments.length == 4)
            scrollbars = "yes"
            height = 600;
            var mywin = window.open(url, name, "width=" + width + ",height=" + height + ", location=0, scrollbars=1");
            mywin.focus();
            return false;
        }

        function popup_player(message) {
            var lit_text = "<iframe scrolling='no' style='height:440px; border:none; background-color:#FFF; width:240px; overflow:hidden;' id='playerframe' src='" + message + "'></iframe><a style='float:left;margin-left: 100px;' href='#' id='cancel_button' class='blue-button'> Close</a>";
            $('.dialog-content').html(lit_text);
            popupLogout("", false);
        }

        function popupLogout(message, logout) {

            //var thevar = $('#playerframe', window.parent.document);
            //	alert(thevar);
            //var anothervar = document.getElementById("playerframe").src;
            //alert(anothervar );


            // get the screen height and width  
            var maskHeight = $(document).height();
            var maskWidth = $(window).width();

            // calculate the values for center alignment
            var dialogTop = $(document).scrollTop() + 400;
            var dialogLeft = (maskWidth / 2) - ($('#dialog-box').width() / 2);

            // assign values to the overlay and dialog box
            $('#dialog-overlay').css({ height: maskHeight, width: maskWidth }).show();
            $('#dialog-box').css({ top: dialogTop, left: dialogLeft }).show();

            // display the message
            // $('#dialog-message').html(message);
            //if (!logout) {
            //    $('#dialog-confirm').html('<a style="float:left;margin-left: 28px;" href="#" id="cancel_button" class="blue-button">Close</a>');
            //}
        }
        






