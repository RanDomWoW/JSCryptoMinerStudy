<script>
function calculateBMI(b_article_id){
	//if(!isNumberKey(event)) return false;	
	
	//$('#bmi_result_'+b_article_id).hide();
	$('#bmi_result_'+b_article_id).html('');
	$('#results_desc_'+b_article_id).html('');
	$('#bmi_result_table_'+b_article_id).hide();
	$('#error_'+b_article_id).html('');

	
	var weight_opt = $('#weight_opt_'+b_article_id).val();
	var weight = $('#weight_'+b_article_id).val();
	var height_opt = $('#height_opt_'+b_article_id).val();
	var height = $('#height_'+b_article_id).val();
	const re = /^(?![0.]+$)\d+(\.\d{1,2})?$/;
	
	
	if(weight!=""){
		//alert("Please fill all the fields");
		//$('#weight_'+b_article_id).focus();
		if(re.test(weight)==false){
		//alert('Please enter numeric value of weight');
		$('#error_'+b_article_id).html('Please enter numeric value of weight');
		$('#weight_'+b_article_id).focus();
		return false;
		}
		
	}
	if(height!=""){
		//alert("Please fill all the fields");
		//$('#height_'+b_article_id).focus();
		if(re.test(height)==false){
		//alert('Please enter numeric value of height');
		$('#error_'+b_article_id).html('Please enter numeric value of height');
		$('#height_'+b_article_id).focus();
		return false;
	}
	}
	if(weight=="" || height==""){
		//$('#error_'+b_article_id).html('Weight & Height can not be empty ');
		return false;
	}
	
	if(height_opt=="" || weight_opt=="" ){
		//alert("Please choose options");
		$('#error_'+b_article_id).html('Please choose options');
		return false;
	}
	
	
	
	if(weight_opt=='lbs'){
		var weight_kg = (weight * 0.45);
		var app_w_s = weight+'Lbs';
	}else{
		var weight_kg = weight;
		var app_w_s = weight+'Kg';
	}
	if(height_opt=='cm'){
		//var height_in_inches = (height * 0.39370); //convert into inch
		//var height_in_metre = (height_in_inches * 0.025);
		var height_in_metre = (height/100);
		var app_h_s = height+'Cm';
	}else if(height_opt=='inch'){
		var height_in_metre = (height/39.370);
		var app_h_s = height+'Inch';
	}else if(height_opt=='feet'){
		var height_in_metre = (0.3048*height);		
		var app_h_s = height+'Feet';
	}
	height_in_metre = height_in_metre*height_in_metre;
	var bmi = weight_kg/height_in_metre;
	//var display_result = "Weight: "+app_w_s+", Height: "+app_h_s+" and BMI = "+bmi.toFixed(2);
	if(bmi>0 && bmi<18.50){
			//console.log('Underweight');
			var display_result = "BMI is  "+bmi.toFixed(2)+ " " + "Underweight";
			var result_desc = "Careful! You might get blown away by the wind! You are way below your required body weight, and chances are you have immunity issues too. A more balanced diet coupled with exercise to build up stamina is what you need.";
		}else if(bmi>=18.50 && bmi<=24.99){
			var display_result = "BMI is  "+bmi.toFixed(2)+ " " + "Normal"; 
			var result_desc = "Congratulations! You are in the pink of health. You are obviously eating right. Add a regular fitness routine and you will soon find the key to eternal youth, or atleast to looking and feeling young at heart always. Keep doing what you are doing. The good news? You have our blessings to go on and indulge once in a while";
		}
		else if(bmi>=25 && bmi<=29.99){
			var display_result = "BMI is  "+bmi.toFixed(2)+ " " + "Overweight"; 
			var result_desc = "Uh-oh. You know this is not good news. You are overweight. Your body protests loudly everytime you go for a walk or take the stairs. Ignore the protest and plod on. You need a lifestyle change urgently. Follow a better diet and find a fitness regimen that works for you if you want to get back on track.";
