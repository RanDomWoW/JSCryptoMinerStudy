		else if(bmi>=30){
			var display_result = "BMI is  "+bmi.toFixed(2)+ " " + "Obese"; 
			var result_desc = "Listen closely and you will hear the sound of alarm bells going off! It's your body crying for attention. Wake up now, and head straight to your doctor. Start with a complete health check-up. Listen to your heart, we mean literally. It's probably working overtime! It's never too late to make a healthy lifestyle change. Do it NOW!";
		}
		
	//$('#bmi_result_'+b_article_id).hide();
	//$('#results_desc_'+b_article_id).html('');
	//$('#results-desc_'+b_article_id).html('');
	//$('#bmi_result_table_'+b_article_id).hide('');	
	
	$('#bmi_result_'+b_article_id).html(display_result);
	$('#results_desc_'+b_article_id).html(result_desc);
	$('#bmi_result_table_'+b_article_id).show();
	
	//alert(Math.round(bmi));
}
