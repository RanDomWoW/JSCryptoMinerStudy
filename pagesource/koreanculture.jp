		//--------------------------------------------------------------------
		//--------------------------- HEADER LINKS(FOOTER MENU) -------------------------------
		$(".footerbar-holder").mouseenter(function() { 
			var index = $(this).attr("index");
			var menulist = "#footer" + index + "list";
			var image = "#footer" + index;

			$("#footerbar").css('z-index', 9999); //footer 2012-03-30
			$("#footerall").css('z-index', 9999); //because IE7 layer back
			$("#right-column").css('z-index', 0); //2012-04-03

			$(menulist).show("slide", { direction: "down" }, 50); //開く速度
			
			var original_src = $(image).attr("src");
			if ( original_src.indexOf("-w.png") > 0 ) return;

			var src = $(image).attr("src").match(/[^\.]+/) + "-w.png";	//-w のイメージに srcを変える
			$(image).attr("src", src);
		})
		.mouseleave(function() {
			var index = $(this).attr("index");
			var menulist = "#footer" + index + "list";
			var image = "#footer" + index;

			$("#footerbar").css('z-index', 0);  //footer 2012-03-30
			//$("#footerall").css('z-index', 0); //footer 2012-04-18
			$("#right-column").css('z-index', 1); //2012-04-03

			$(menulist).hide("slide", { direction: "down" }, 25); //折りたたみ速度
			
			var src = $(image).attr("src").replace("-w", "");				//元のイメージに srcを変える
			$(image).attr("src", src);
        });
		//--------------------------------------------------------------------
		$(".subpage-menu").mouseenter(function(){
			var targetX = $(this).data("top");
			$("#subpage-arrow").stop().animate( {top : targetX }, 100 );
		});
		$("#subpage-menu-holder").mouseleave(function(){
			var targetX = $(this).data("top");
			$("#subpage-arrow").stop().animate( {top : targetX }, 100 );
		});

		//---------- BASIC Mouse Over Image Change ----------//
		 $(".over-btn")
        .mouseover(function() {
			if ( $(this).attr("src").indexOf("-over") > 0 ) return;

			var src = $(this).attr("src").match(/[^\.]+/) + "-over.png";	//-over のイメージに srcを変える
			$(this).attr("src", src);
        })
        .mouseout(function() {
			var src = $(this).attr("src").replace("-over", "");				//元のイメージに srcを変える
			$(this).attr("src", src);
        });
        
        //----------------- MAIN NEWS WINDOW ----------------//
		// SAVE ACTIVE Container (always starting with container1)
		var activeElement = $("#main-container1");
		
		// ICONS Mouse Actions
		$("img.main-icon").mouseover(function() { 
			$(this).fadeTo(fade_in_speed,1); // 썸네일 선명하게 하는 속도
		})
		.mouseout(function() {
			var checkActive = $(this).is(".active");	// アクティブなら無視する。アクティブ以外は半透明にする。
			if( !checkActive ){
				$(this).fadeTo(fade_out_speed,0.4); //썸네일 흐릿하게 하는 속도
			}
        })
        .click(function() {		// ICON CLICK ACTION
			var targetElementID = $(this).attr("target");
			
			$("img.main-icon").each(function() {	// get "target='----' "
				var targetAttr = $(this).attr("target");
				if( targetElementID != targetAttr )	$(this).removeClass("active").fadeTo(fade_out_speed,0.4);
			});
			$(this).addClass("active");
			
			$("div.main-container").each(function() {
				$(this).css("z-index",0).removeClass( "active" );	// bring all contents to BACK
			});
			
			activeElement.fadeTo(fade_out_speed,0); // fade out the PREVIOUS active element
			
			activeElement = $( targetElementID );
			activeElement.delay("0").css("z-index",1).fadeTo(fade_in_speed,1).addClass("active");	// bring clicked content to FRONT and fadeIn
			if( activeElement.is(".singleimage") )
			{
				$("#shousai").fadeOut(fade_out_speed);		// hide SHOUSAI Btn (SINGLE IMAGE CONTENT)
			}
			else
			{
				$("#shousai").fadeIn(fade_in_speed);		// show SHOUSAI Btn (TEXT & IMAGE IMAGE CONTENT)
			}
			
		});
		
		 //-------------- BOTTOM WINDOW ( WADAI <=> COMMUNITY ) -------------//
		$("#wadai-btn").mouseover(function() { 
			var checkActive = $(this).is(".active");	// アクティブなら無視する。アクティブ以外は半透明にする。
			if( !checkActive ){
				$(this).fadeTo(400, 1);
				
				$("#community-content").css("z-index", 9998).fadeTo(100, 0);
				$("#wadai-content").delay("80").css("z-index",9999).fadeTo(300, 1); // bring clicked content to FRONT and fadeIn
				$("#community-btn").removeClass("active").fadeTo(400, 0.5);
        		$(this).addClass("active");
			}
		})
		.mouseout(function() {
			var checkActive = $(this).is(".active");	// アクティブなら無視する。アクティブ以外は半透明にする。
			if( !checkActive ){
				$(this).fadeTo(400, 0.5);
			}
        });
		
		$("#community-btn").mouseover(function() { 
			var checkActive = $(this).is(".active");	// アクティブなら無視する。アクティブ以外は半透明にする。
			if( !checkActive ){
				$(this).fadeTo(400, 1);
				
				$("#wadai-content").css("z-index", 9998).fadeTo(100, 0);
				$("#community-content").delay("80").css("z-index",9999).fadeTo(300, 1); // bring clicked content to FRONT and fadeIn
				$("#wadai-btn").removeClass("active").fadeTo(400, 0.5);
				$(this).addClass("active");
			}
		})
		.mouseout(function() {
			var checkActive = $(this).is(".active");	// アクティブなら無視する。アクティブ以外は半透明にする。
			if( !checkActive ){
				$(this).fadeTo(400, 0.5);
			}
        });
	}
	);
    //]]>
</script>
</body>
