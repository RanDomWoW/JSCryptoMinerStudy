		if(navigator.cookieEnabled) 
		{
	
				document.observe('keydown', function (evt) {
				if (evt.keyCode == 13) {
					win.okCallback();
				}
			});

			var cookieStored = emailCookieExists();

			if(!cookieStored)
			{
				var win = Dialog.confirm($('email').innerHTML, {
					className: "popup",
					id: "popupDialog",
					width: 356,
					zIndex: 2001,
					okLabel: 'Confirm',
										cancelLabel: 'Close',
					onCancel: function (win) {
												createCookie('email=none', "7");
