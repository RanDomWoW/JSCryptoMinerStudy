<style>
	div#yell {position: relative;overflow: hidden;width: 90%;height: 25px;margin-left:10px;}
	div#yell ul, div#yell li {	margin:0;padding:0;}
	div#yell ul {position: absolute;}	
	div#yell ul li {display: block;overflow: hidden;font-size:11pt;font-family:微軟正黑體;height: 25px;line-height:25px;}	
</style>

<script type="text/javascript">
    $(function () {
        var $marqueeUl = $('div#yell ul'),
			_marqueeUlHtml = $marqueeUl.html(),
			_height = $('div#yell').height() * -1,
			scrollSpeed = 600,
			timer,
			speed = 3000 + scrollSpeed,
			direction = 0,
			_lock = false,
			_showItems = 2,
			_moveItems = 2;

        $("div#yell").css("height", _showItems * _height * -1);

        if (_showItems >= $marqueeUl.children('li').length) return;

        var $marqueeli = $marqueeUl.append(_marqueeUlHtml + _marqueeUlHtml).children();


        $marqueeUl.css('top', $marqueeli.length / 3 * _height);

        $marqueeli.hover(function () {
            clearTimeout(timer);
        }, function () {
            timer = setTimeout(showad, speed);
        });


        function showad() {
            _lock = !_lock;
            var _now = $marqueeUl.position().top / _height;
            _now = (direction ? _now - _moveItems + $marqueeli.length : _now + _moveItems) % $marqueeli.length;


            $marqueeUl.animate({
                top: _now * _height
            }, scrollSpeed, function () {


                if (_now + _moveItems >= $marqueeli.length / 3 * 2) {
                    $marqueeUl.css('top', $marqueeli.length / 3 * _height - _height * ($marqueeli.length / 3 * 2 - _now));
                } else if (_now < $marqueeli.length / 3) {

                    $marqueeUl.css('top', $marqueeli.length / 3 * _height + (_height * _now));
                }
                _lock = !_lock;
            });

            timer = setTimeout(showad, speed);
        }

        timer = setTimeout(showad, speed);

        $('a').focus(function () {
            this.blur();
        });
    });
</script>


