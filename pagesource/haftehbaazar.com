                keyboardNav: true
            });
            $('#slider-widget .flexslider').flexslider({
                animation: "fade",
                slideshowSpeed: 3000,
                animationSpeed: 1300,
                directionNav: true,
                controlNav: false,
                keyboardNav: true
            });
            $("#defaultmenu ul li,#defaultmenu ul li ul li").removeClass("active");
            $("#defaultmenu ul.nav li:first").addClass("active");
        });


    </script>
















