<script>
  // index-ad-carousel
  $(".index-ad-carousel").each(function(){
    $(this).append('<ul class="carousel-ad-menu"></ul>');
    $(this).find("li a").each(function(){
      $(this).parents(".index-ad-carousel").find(".carousel-ad-menu").append('<li><a href="#">' + $(this).attr("title") + '</a></li>')
    });
    $(this).find(".carousel-ad-menu li:first-child").addClass("active");
    $(this).find(".carousel-ad-menu a").width($(this).width()/$(this).find("ul.index-ad-list li").length);
    var self = $(this);
    var setAdSwitch = function(){
      var currentActive = self.find("ul.index-ad-list li.active").index();
      self.find("ul.index-ad-list li, ul.carousel-ad-menu li").removeClass("active");
      currentActive = currentActive++ == self.find("ul.index-ad-list li").length - 1 ? 0 : currentActive++;
      self.find("ul.index-ad-list li").eq(currentActive).addClass("active");
      self.find("ul.carousel-ad-menu li").eq(currentActive).addClass("active");
    };
    var setSwitchTimer = window.setInterval(setAdSwitch, 3000);
    $(this).find(".carousel-ad-menu a").on("click", function(e){
      e.preventDefault();
      $(this).parent().addClass("active")
             .siblings("li").removeClass("active")
             .parents(".index-ad-carousel").find("ul.index-ad-list li").removeClass("active")
             .parent().find("li").eq($(this).parent().index()).addClass("active");
      window.clearInterval(setSwitchTimer);
      setSwitchTimer = window.setInterval(setAdSwitch, 3000);
    });
  });
</script>

