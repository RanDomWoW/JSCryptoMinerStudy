/* --- Ordered Square Blue List --- */
.list-ordered-square-blue {
/*counter-reset: li;*/
/* This should allow for nested lists */
counter-reset: section;
margin: 30px 0 20px 0;
color: #666;
}

.list-ordered-square-blue > li {
position: relative;
display: block;
*display: block;
*zoom: 1;
margin-bottom: 30px;
padding-left: 55px;
counter-increment: section;
line-height:1.5em;
}

.list-ordered-square-blue > li:before {
position: absolute;
top: 5px;
left: 0;
color: #ffffff;
background-color: #2bb8c2;
padding: 18px 3px 0px 0px;
margin: 0px 15px 0 0;
line-height: 22px;
font-size: 36px;
font-weight: bold;
text-align: right;
width: 40px;
/*content: counter(li);
counter-increment: li;*/
/* This should make it possible to use nested lists */
content: counter(section);
}

/* --- Ordered Square Blue List - List within this list - Still a work in progress--- */

.list-ordered-square-blue li ul {
    margin: 0;
    padding: 0;
    list-style-type: none;
    position: relative;
}
.list-ordered-square-blue li ul li:before {
    background-color: #fff;
    content: "";
    position: relative;
    padding: 0;
    margin: 0;
}

/* --- Basic Ordered list -- */

.list-ordered-basic li {
padding:0 0 15px 0;
}


/* --- Simple Ordered list -- */

.list-ordered-simple > li::before {
    color: #2bb8c2;
    content: counter(section, decimal);
    font-size: 1em;
    font-weight: bold;
    left: 0;
    line-height: 22px;
    margin: 0 15px 0 0;
    position: absolute;
    text-align: right;
    width: 10px;
}

.list-ordered-simple > li {
    counter-increment: section;
    display: block;
    margin-bottom: 20px;
    padding-left: 25px;
    position: relative;
}

.list-ordered-simple {
margin: 20px 0 20px 10px;
