	display: block;
}

table.table-stacked tr:nth-of-type(even) {
	background: transparent;
}

table.table-stacked tr {
	border: none;
}



/* Hide table headers (but not display: none;, for accessibility) */

table.table-stacked thead tr {
	position: absolute;
	top: -9999px;
	left: -9999px;
}

table.table-stacked td:nth-of-type(1) {
	padding-right: 30px;
	border: none;
}

table.table-stacked td:nth-last-of-type(1) {
	border-bottom: none;
	padding-bottom: 50px;
}

caption.table-caption {
	caption-side: bottom;
	display: block;
}

table.table-stacked:nth-column(1) {
width: 50%;
}

table.table-stacked td {
	/* Behave  like a "row" */
	border: none;
	border-bottom: 1px solid #d8d8d8;
	position: relative;
	padding-left: 75%;
}

table.table-stacked td:before {
	/* Now like a table header */
	position: absolute;
	/* Top/left values mimic padding */
	left: 6px;
	width: 50%;
	padding-right: 10px;
	white-space: nowrap;
}
}
}



