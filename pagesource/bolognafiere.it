<script type="text/javascript"> 
	
$(document).ready(function(){
//$(".calendario-drop-down").show() ;	
	$("#show-calendario").click(function() {
		$(".calendario-drop-down").toggle() ;
	}) ;
	
	// Selezione di default: cronologico
	$(".calendariofiere .calendario-drop-down ul.ordine-alfabetico").hide() ;

	$(".calendariofiere .calendario-drop-down ul.liste-fiere-dx li.citta").hide() ;
	$(".calendariofiere .calendario-drop-down li.lista-fiere-dx-bologna").show() ;
	$("ul.ordinamento-luoghi li").removeClass("current") ;
	$("ul.ordinamento-luoghi li.bologna").addClass("current") ;
	$(".calendariofiere .calendario-drop-down ul.ordine-cronolgico").show() ;
	
	$("ul.ordinamento-tipologia li.cronologico").addClass("current") ;

	// Cambio selezione fiere
	$("ul.ordinamento-luoghi li a").click(function() {
		var citta = $(this).attr("rel") ;
		
		if(citta == "tutti") {
			$(".calendariofiere .calendario-drop-down ul.liste-fiere-dx li.citta").show() ;
		} else {
			$(".calendariofiere .calendario-drop-down ul.liste-fiere-dx li.citta").hide() ;
			$(".calendariofiere .calendario-drop-down li.lista-fiere-dx-"+citta).show() ;
		}
		$("ul.ordinamento-luoghi li").show() ;
		
		$("ul.ordinamento-luoghi li").removeClass("current") ;
		$("ul.ordinamento-luoghi li."+citta).addClass("current") ;
	}) ;
	
	
	// Cambia da cronologico ad alfabetico e viceversa
	$("ul.ordinamento-tipologia a").click(function() {
		var rel = $(this).attr("rel") ;
		
		if(rel == "alfabetico") {
			$("ul.ordine-cronologico").hide() ;
			$("ul.ordine-alfabetico").show() ;

			$("ul.ordinamento-tipologia li.alfabetico").addClass("current") ;
			$("ul.ordinamento-tipologia li.cronologico").removeClass("current") ;
		} else {
			$("ul.ordine-alfabetico").hide() ;
			$("ul.ordine-cronologico").show() ;

			$("ul.ordinamento-tipologia li.cronologico").addClass("current") ;
			$("ul.ordinamento-tipologia li.alfabetico").removeClass("current") ;
		}
	}) ;
	
});
    		
</script>

