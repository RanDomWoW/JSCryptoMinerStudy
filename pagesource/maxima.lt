<style>
#canvas_link{
   display: none;
cursor: pointer;
}

#canvas_link.show {
   display: block;
}

#canvas {
   display: none;
pointer-events: none;
}

#canvas.show {
   display: block;
   cursor: pointer;
}
</style>


<script>
   var deviceWidth;

    $(document).ready(function() {
         if ($('#swiper1').length !== 0) {

         $('#swiper1').prepend('<a id="canvas_link" href="/aciu/mano-aciu"></a><canvas id="canvas" onclick="window.location = \'/aciu/mano-aciu\';" style="width: 100%; position: absolute; height: 900px; z-index: 1;"></canvas></a>');


         setInterval(function () {
            var show = false;

            if ($('#swiper1 .swiper-wrapper .swiper-slide.swiper-slide-active').find('a[href="/aciu/mano-aciu"]').length !== 0) {
               $('#canvas').attr('onclick', '');
                $("#canvas_link").addClass('show');
                $("#canvas").addClass('show').attr("onclick", "window.location = \'/aciu/mano-aciu\';");
                var show = true;
            } else if ($('#swiper1 .swiper-wrapper .swiper-slide.swiper-slide-active').find('a[href="/temines-akcijos/kaledu-kaledos"]').length !== 0) {
$('#canvas').attr('onclick', '');
                 $("#canvas_link").addClass('show')
                $("#canvas").addClass('show').attr("onclick", "window.location = \'/temines-akcijos/kaledu-kaledos\';");
                var show = true;
            }

            if (!show) {
                $("#canvas_link").removeClass('show');
                $("#canvas").removeClass('show');
            }
         }, 1000);

        deviceWidth = window.screen.width;
        if (deviceWidth > $(window).width() && $(window).width() > 0) {
            deviceWidth = $(window).width();
        }

        if (typeof document.body.clientWidth !== "undefined") {
            if (deviceWidth > document.body.clientWidth && document.body.clientWidth > 0) {
                deviceWidth = document.body.clientWidth;
            }
        }

        if (navigator.userAgent.indexOf("Android") >= 0 && window.devicePixelRatio) {
            deviceWidth = deviceWidth / window.devicePixelRatio;
        }

        if (deviceWidth < 1024) {
            window.globalWidth = window.innerWidth;
            window.globalHeight = 900;
            window.globalSnowSizeRadius = 2 + 1;
            window.globalSnowCount = 200;
        } else {
            window.globalWidth = window.innerWidth;
            window.globalHeight = 900;
            window.globalSnowSizeRadius = 3 + 1;
            window.globalSnowCount = 400;
        }

        window.globalCanvasWidth = window.globalWidth;
        window.globalCanvasHeight = window.globalHeight;

            //canvas init
            var canvas = document.getElementById("canvas");
            var ctx = canvas.getContext("2d");

            //canvas dimensions
            var W = window.globalWidth;
            var H = window.globalHeight;
            canvas.width = window.globalCanvasWidth;
            canvas.height = window.globalCanvasHeight;

            //snowflake particles
            var mp = window.globalSnowCount; //max particles
            var particles = [];
            for (var i = 0; i < mp; i++) {
                particles.push({
                    x: Math.random() * W, //x-coordinate
                    y: Math.random() * H, //y-coordinate
                    r: Math.random() * window.globalSnowSizeRadius, //radius
                    d: Math.random() * mp //density
                })
            }

            //Lets draw the flakes
            function draw() {
                ctx.clearRect(0, 0, W, H);

                ctx.fillStyle = "rgba(255, 255, 255, 0.8)";
                ctx.beginPath();
                for (var i = 0; i < mp; i++) {
                    var p = particles[i];
                    ctx.moveTo(p.x, p.y);
                    ctx.arc(p.x, p.y, p.r, 0, Math.PI * 2, true);
                }
                ctx.fill();
                update();
            }

            //Function to move the snowflakes
            //angle will be an ongoing incremental flag. Sin and Cos functions will be applied to it to create vertical and horizontal movements of the flakes
            var angle = 0;

            function update() {
                angle += 0.01;
                for (var i = 0; i < mp; i++) {
                    var p = particles[i];
                    //Updating X and Y coordinates
                    //We will add 1 to the cos function to prevent negative values which will lead flakes to move upwards
                    //Every particle has its own density which can be used to make the downward movement different for each flake
                    //Lets make it more random by adding in the radius
                    p.y += Math.cos(angle + p.d) + 1 + p.r / 2;
                    p.x += Math.sin(angle) * 2;

                    //Sending flakes back from the top when it exits
                    //Lets make it a bit more organic and let flakes enter from the left and right also.
                    if (p.x > W + 5 || p.x < -5 || p.y > H) {
                        if (i % 3 > 0) //66.67% of the flakes
                        {
                            particles[i] = {x: Math.random() * W, y: -10, r: p.r, d: p.d};
                        }
                        else {
                            //If the flake is exitting from the right
                            if (Math.sin(angle) > 0) {
                                //Enter from the left
                                particles[i] = {x: -5, y: Math.random() * H, r: p.r, d: p.d};
                            }
                            else {
                                //Enter from the right
                                particles[i] = {x: W + 5, y: Math.random() * H, r: p.r, d: p.d};
                            }
                        }
                    }
                }
            }

            //animation loop
            setInterval(draw, 33);


        }


        app_.addModule('header');

        app_.addModule('offers');

        var $container = $('.mpblock');
        var oldIE = $('html').hasClass("oldie") ? true : false;

        /*resizeMasonryContainer($container);
        $(window).resize(function(){
            resizeMasonryContainer($container);
        });

        $container.masonry({
            //isInitLayout: !oldIE,
            itemSelector: '.col',
            columnWidth: function( containerWidth ) {
                console.log(containerWidth)
                return containerWidth / 5;
            }
        });

        function resizeMasonryContainer($container){
            $container.css({ width : '' });
            if( app_.w_w > app_.break_point )
            {
                var col2_w = $container.find(".col-2").eq(0).width();
                var col4_w = $container.find(".col-4").eq(0).width();
                $container.css({ width : ( col2_w + col4_w * 2 ) });
//                $container.css({ width : ( col2_w * 5 ) });
            }
        };*/
        $('.mpblock').isotope({
            layoutMode: 'packery',
//            layoutMode: 'masonryHorizontal',
            itemSelector: '.col',
            packery: {
                gutter: 0
            },
            /*masonryHorizontal: {
                rowHeight: 50,
                gutter: 15
            }*/
            getSortData: {
                number: function (itemElem) {
                    var number = $(itemElem).find('.number').text();
                    return parseInt(number);
                }
            },
            sortBy: ['number'],
            sortAscending: true
        });
    });
</script>



