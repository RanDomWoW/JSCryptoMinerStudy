    <script language="javascript" type="text/javascript">
        function CheckAtleastOne() {
            var txtFrmDt = document.getElementById('txtFrmDt').value.length;
            var txtToDt = document.getElementById('txtToDt').value.length;
          //  alert('run1');
            if (document.getElementById('txtIdentityNo').value == "" && document.getElementById('TextBox1').value == "")
           // if(document.getElementById('txtIdentityNo').value == 'Your ID Number' && document.getElementById('TextBox1').value == 'Mobile No.')
            // || (document.getElementById('txtIdentityNo').value == 'Your ID Number' || document.getElementById('TextBox1').value == 'Mobile No.')) {
                if (document.getElementById('txtIdentityNo').value == "" || document.getElementById('txtFrmDt').value == "" || document.getElementById('txtToDt').value == "" || document.getElementById('txtFrmDt').value == "DD/MM/YYYY" || document.getElementById('txtToDt').value == "DD/MM/YYYY" || (document.getElementById('txtIdentityNo').value == 'Your ID Number')) {
                    //            if (document.getElementById('txtFrmDt').value == "" || document.getElementById('txtToDt').value == "" || document.getElementById('txtFrmDt').value == "DD/MM/YYYY" || document.getElementById('txtToDt').value == "DD/MM/YYYY") {
                  //  alert('run2');
                    //                if (((document.getElementById('txtIdentityNo').value == "") && (document.getElementById('TextBox1').value == "")) || ((document.getElementById('txtIdentityNo').value == 'Your ID Number') && (document.getElementById('TextBox1').value == 'Mobile No.'))) {
                    alert('Please Fill all Fields !');
                    return false;
                    //                }
                }
            }
            else {
                alert('RunElse');
                if (txtFrmDt == "1" || txtToDt == "1") {
                    if (txtFrmDt == "1") {
                        alert('Please Select Correct Date !');
                        document.getElementById('txtFrmDt').focus();
                        return false;
                    }
                    if (txtToDt == "1") {
                        alert('Please Select Correct Date !');
                        document.getElementById('txtToDt').focus();
                        return false;
                    }

                    //       if(document.getElementById('txtIdentityNo').value!="" && document.getElementById('txtFrmDt').value!="" && document.getElementById('txtToDt').value!="" && document.getElementById('txtFrmDt').value!="DD/MM/YYYY" && document.getElementById('txtToDt').value!="DD/MM/YYYY" && document.getElementById('txtIdentityNo').value!='Your ID Number')
                }
                if ((document.getElementById('txtFrmDt').value == "") && (document.getElementById('txtToDt').value != "")) {
                    alert('FromDate Can not be Blank !');
                    return false;
                }
                if ((document.getElementById('txtFrmDt').value != "") && (document.getElementById('txtToDt').value == "")) {
                    alert('ToDate Can not be Blank !');
                    return false;
                }

                var frm = document.getElementById('txtFrmDt').value;
                var a1 = new Array();
                a1 = frm.split("/");
                var d = a1[0];
                var m = a1[1];
                var y = a1[2];
                var date_frm = m + '/' + d + '/' + y;


                var to = document.getElementById('txtToDt').value;
                var a1 = new Array();
                a1 = to.split("/");
                var d = a1[0];
                var m = a1[1];
                var y = a1[2];
                var date_to = m + '/' + d + '/' + y;
                if (new Date(Date.parse(date_frm)) > new Date(Date.parse(date_to))) {
                    alert("Invalid Date Range!\nFrom Date can not be after To Date!")
                    document.getElementById('txtToDt').focus();
                    return false;
                }
                else {
                    return true;
                }
            }
        } 
     
    </script>

    <script type="text/javascript">
        function cancel() {
            window.parent.document.getElementById('btnCancel').click();
            var AcceptID = document.getElementById('GVGetBr_Id');
            if (AcceptID != null) {
                document.getElementById('GVGetBr_Id').style.display = 'none';
            }
            else {
                document.getElementById('txtFrmDt').value = 'DD/MM/YYYY';
                document.getElementById('txtToDt').value = 'DD/MM/YYYY';
                document.getElementById('txtIdentityNo').value = 'Your ID Number';
                document.getElementById('txtIdentityNo').className = "watermark";
            }

        }
        function checkTextBox() {
            document.getElementById('txtFrmDt').value = 'DD/MM/YYYY';
            document.getElementById('txtToDt').value = 'DD/MM/YYYY';
            document.getElementById('txtFrmDt').className = "watermark";
            document.getElementById('txtToDt').className = "watermark";
            document.getElementById('txtIdentityNo').value = 'Your ID Number';
            document.getElementById('txtIdentityNo').className = "watermark";
        }
    </script>

