<script>

function home_search_property(formid){

var col_name = jQuery.trim(jQuery("#"+formid+" #col_name").val());  
var type = jQuery.trim(jQuery("#"+formid+" #type").val());  
var cntry1 = jQuery.trim(jQuery("#"+formid+" #cntry1").val()); 
var search_inp1 = jQuery.trim(jQuery("#"+formid+" .search_inp").val());
var orderby = jQuery.trim(jQuery("#orderby").val());
var sortby = jQuery.trim(jQuery("#"+formid+" #sortby").val());    


var url = "show-search-result";


if(col_name != ''){ url += '-'+col_name; }



if(type == 'newhomes'){ url += '-'+type; }
if(cntry1 != ''){

var res12 = cntry1.split(', ').join(',');


var rep_charec = res12.split(' ').join('-');

url += '-'+rep_charec; 



}
if(search_inp1 != ''){ 


var res12 = search_inp1.split(', ').join(',');


var rep_charec = res12.split(' ').join('-');



url += '-'+rep_charec.replace(/\s+/g, '+');

} 


if(sortby != ''){ url += '-'+sortby; }  
// if(orderby != ''){ url += '-'+orderby; }  

var url = url.toLowerCase();
url +='.html';



var theForm=document.getElementById(formid);
theForm.action = url;
theForm.submit();
}
function home_search_review(formid){


var cntry1 = jQuery.trim(jQuery("#"+formid+" #cntry4").val()); 
var search_inp1 = jQuery.trim(jQuery("#"+formid+" .search_inp").val());

var url = "find-reviews-by-location";

var res12 = cntry1.split(', ').join(',');


var rep_charec = res12.split(' ').join('-');

var loc_country = rep_charec.toLowerCase();


var res12 = search_inp1.split(', ').join(',');


var rep_charec = res12.split(' ').join('-');

var loc_new_rep = rep_charec.toLowerCase();   
// alert(loc_country);

if(loc_country != ''){ url += '-'+loc_country; }
if(loc_new_rep != ''){ url += '~'+loc_new_rep; } 

url +='.html';

var theForm=document.getElementById(formid);
theForm.action = url;
theForm.submit();
}



