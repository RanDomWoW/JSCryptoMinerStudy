function getreloaddata(data){
	var arr = '<div class="h"><ul class="tabs"><li><span>昨日赛果</span></li><li class="hover"><span>本日赛事</span></li><li><span>明日预告</span></li></ul></div>';
	for(var i in data){
		if(i == 'today'){
			arr += '<div id="nba_side_today" class="c" style="display: block;">';
		}
		else{
			arr += '<div class="c" style="display:none;">';
		}
		arr += '<div class="date">'+data[i].date+'</div>';
		if(typeof(data[i].games) != 'undefined'){
			for(var j in data[i].games){
				if(1 == data[i].games[j]['baseinfo']['gamestate']){
					if(1 == j%2){
						arr += '<div class="game tr1">';
					}
					else{
						arr += '<div class="game tr2">';
					}
					arr += '<div class="line"><div class="team">';
					arr += '<a href="http://www.bbboo.com/nba/team/'+data[i].games[j]['baseinfo']['game_hteam'].teamid+'/">'+data[i].games[j]['baseinfo']['game_hteam'].teamname+'</a></div>';
					arr += '<div class="score"><a href="http://www.bbboo.com/nba/game/live_'+data[i].games[j]['baseinfo']['gameid']+'.html">'+data[i].games[j]['baseinfo'].gametime+'</a></div>';
					arr += '<div class="team"><a href="http://www.bbboo.com/nba/team/'+data[i].games[j]['baseinfo']['game_vteam'].teamid+'/">'+data[i].games[j]['baseinfo']['game_vteam'].teamname+'</a></div>';
					arr += '<div class="col2"><a href="http://www.bbboo.com/nba/game/live_'+data[i].games[j]['baseinfo']['gameid']+'.html">直播室</a></div>';
					if('' != data[i].games[j]['preview']){
						arr += '<div class="col3"><a href="">前瞻</a></div>';
					}
					else{
						arr += '<div class="col2"><a href="http://www.bbboo.com/nba/game/conversation_'+data[i].games[j]['baseinfo']['gameid']+'.html">网友讨论</a></div>';
					}
					arr +='</div></div>';
				}
				else if(2 == data[i].games[j]['baseinfo']['gamestate']){
					if(1 == j%2){
						arr += '<div class="game tr1">';
					}
					else{
						arr += '<div class="game tr2">';
					}
					arr += '<div class="line"><div class="team">';
					arr += '<a href="http://www.bbboo.com/nba/team/'+data[i].games[j]['baseinfo']['game_hteam'].teamid+'/">'+data[i].games[j]['baseinfo']['game_hteam'].teamname+'</a></div>';
					arr += '<div class="score"><a href="http://www.bbboo.com/nba/game/live_'+data[i].games[j]['baseinfo']['gameid']+'.html">'+data[i].games[j]['baseinfo']['game_hpt']+'-'+data[i].games[j]['baseinfo']['game_vpt']+'</a></div>';
					arr += '<div class="team"><a href="http://www.bbboo.com/nba/team/'+data[i].games[j]['baseinfo']['game_vteam'].teamid+'/">'+data[i].games[j]['baseinfo']['game_vteam'].teamname+'</a></div>';
					//节数，剩余时间
					var qurarray = Array('第一节','第二节','第三节','第四节','加时一','加时二','加时三','加时四','加时五');
					if('0:00' == data[i].games[j]['baseinfo']['qurtime'] || '0:0' == data[i].games[j]['baseinfo']['qurtime']){
						if(2 == data[i].games[j]['baseinfo']['qurnow']){
							arr += '<div class="col1"><a href="http://www.bbboo.com/nba/game/live_'+data[i].games[j]['baseinfo']['gameid']+'.html">中场休息</a></div>';
						}
						else{
							arr += '<div class="col1"><a href="http://www.bbboo.com/nba/game/live_'+data[i].games[j]['baseinfo']['gameid']+'.html">'+qurarray[data[i].games[j]['baseinfo']['qurnow']-1]+'结束</a></div>';
						}	
					}
					else{
						arr += '<div class="col1"><a href="http://www.bbboo.com/nba/game/live_'+data[i].games[j]['baseinfo']['gameid']+'.html">'+qurarray[data[i].games[j]['baseinfo']['qurnow']-1]+' 剩余'+data[i].games[j]['baseinfo']['qurtime']+'</a></div>';
					}
					arr += '</div></div>';
				}
				else{
					if(1 == j%2){
						arr += '<div class="game tr1">';
					}
					else{
						arr += '<div class="game tr2">';
					}
					arr += '<div class="line"><div class="team">';
					arr += '<a href="http://www.bbboo.com/nba/team/'+data[i].games[j]['baseinfo']['game_hteam'].teamid+'/">'+data[i].games[j]['baseinfo']['game_hteam'].teamname+'</a></div>';
					arr += '<div class="score"><a href="http://www.bbboo.com/nba/game/boxscore_'+data[i].games[j]['baseinfo']['gameid']+'.html">'+data[i].games[j]['baseinfo']['game_hpt']+'-'+data[i].games[j]['baseinfo']['game_vpt']+'</a></div>';
					arr += '<div class="team"><a href="http://www.bbboo.com/nba/team/'+data[i].games[j]['baseinfo']['game_vteam'].teamid+'/">'+data[i].games[j]['baseinfo']['game_vteam'].teamname+'</a></div>';
					arr += '<div class="col3"><a href="http://www.bbboo.com/nba/game/boxscore_'+data[i].games[j]['baseinfo']['gameid']+'.html">统计</a></div>';
					if('' == data[i].games[j]['heighlights']){
						arr += '<div class="col3"></div>';
					}
					else{
						arr += '<div class="col3"><a href="http://www.bbboo.com/program/highlights/'+data[i].games[j]['heighlights']+'">集锦</a></div>';
					}
					if('' != data[i].games[j]['recap']){
						arr += '<div class="col3"><a href="http://www.bbboo.com/nba/game/recap_'+data[i].games[j]['baseinfo']['gameid']+'.html">战报</a></div>';
					}
					else{
						arr += '<div class="col3"><a href="http://www.bbboo.com/nba/game/live_'+data[i].games[j]['baseinfo']['gameid']+'.html">实录</div>';
					}
					arr += '</div></div>';
				}
			}
		}
		else{
			arr += '<div class="game"><div class="line">本日没有比赛</div></div>';
			arr += '<div class="clear3"></div>';
		}
		if(i == 'today'){
			arr += '<div class="game imp"><div class="line"><a href="http://www.bbboo.com/nba/schedule/">NBA赛程</a> | <a href="http://www.bbboo.com/nba/rockets/schedule/">火箭赛程</a> | <a href="http://www.bbboo.com/nba/nets/schedule/">篮网赛程</a> | <a href="http://www.bbboo.com/nba/lakers/schedule/">湖人赛程</a></div></div><div class="clear"></div>';
		}
		arr +='</div>';
	}
	arr += '<div class="f"><div></div></div>';
		$('#index_nba_recently').html(arr);
	zbb_areaTabs('index_nba_recently');
	}

function zbb_areaTabs(node){
	var tab_div = $("#"+node);
	var tabs = $('.tabs li', tab_div);
	var contents = $('.c', tab_div);
	var l=tabs.length;
	if (l != contents.length){
		return;
	}
	tabs.each(function(i){
		$(this).hover(function(){
			var c=i;
			for(var j=0;j<l;j++){
				if (j==c){
					tabs[j].className='hover';
					contents[j].style.display='block';
				}
				else{
					tabs[j].className='';
					contents[j].style.display='none';
				}
			}
		});
	});
}
</script>

