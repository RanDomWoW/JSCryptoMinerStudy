<script>
    window.isGuest = '1';
    //全局函数登录
    window.zbbLogin = function(callback, logOrReg){
        $window = $(window);
        scrollTop = $window.scrollTop();
        windowHeight = $window.width();
        $uwindow = $('#uwindow');
        $uwindowbody = $('#uwindow_body');
        $uwindow.show();
        $uwindow.css('top', (scrollTop+85)+'px');
        $uwindow.css('left', (windowHeight/2-400)+'px');
        $uwindow.css('index', '30000');
        $uwindowbody.css('width', '800px');
        $uwindowbody.css('height', '410px');
        if (typeof logOrReg == 'undefined' || logOrReg == 0) {
            $uwindowbody.html('<iframe src="http://www.bbboo.com/user/register?parent_url='+encodeURIComponent(window.location.href)+'" id="uwindow_iframe" name="uwindow_iframe" scrolling="no" frameborder="0"></iframe>');
        }
        else {
            $uwindowbody.html('<iframe src="http://www.bbboo.com/user/register?parent_url='+encodeURIComponent(window.location.href)+'&lor='+logOrReg+'" id="uwindow_iframe" name="uwindow_iframe" scrolling="no" frameborder="0"></iframe>');
        }
        if ($.browser.msie && ($.browser.version == "6.0")) {
            setTimeout(function(){
                $("#uwindow_iframe").attr('src', "http://www.bbboo.com/user/register");
            },10);
        }
        return;
        if(!zbbLogin.init){
            $("#login-dialog-form").dialog({
                autoOpen:false,
                modal:true,
                width:500,
                resizable:false,
                buttons:{
                    "登录": function(){
                        var username = $("#name").val(),
                                password = $("#password").val(),
                                rememberMe = $("#rememberMe").val(),
                                verifyCode = $("#verifyCode").val();

                        $.post( 'http://www.bbboo.com/User/login', {username:username,password:password,rememberMe:rememberMe,verifyCode:verifyCode}, function(result){
                            if(result.status == 0) {
                                alert(result.msg);
                                $("#login-dialog-form").dialog("close");
                                $(document.body).trigger("userlogined.zhiboba",{username:username,id:result.id});
                                callback && callback({username:username});
                            } else {
                                alert(result.msg);
                                $("#gNewCodeButton").click();
                            }

                        }, 'json');
                    }
                }

            }).bind("keypress.ui-dialog",function(event){
                        if (event.keyCode == $.ui.keyCode.ENTER) {
                            $("#login-dialog-form").data("dialog").options.buttons["登录"]();
                        }
                    });
            $("#gNewCodeButton").click(function(){
                $.ajax({
                    url:"http://www.bbboo.com/user/captcha?refresh",
                    dataType:'json',
                    cache:false,
                    success:function(data) {
                        $('#codeUrl').attr('src', data['url']);
                        $('body').data('captcha.hash', [data['hash1'], data['hash2']]);
                    }
                });
                return false;
            });

            zbbLogin.init = true;
        }
        $("#login-dialog-form").dialog("open");
    }
    window.checkLogin = function(callback){
        if (parseInt(isGuest)) {
            zbbLogin();
            return false;
        }
        else{
            callback();
            return true;
        }
    }
</script>


