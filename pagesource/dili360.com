	<script>
    //fill thumbs full

    function fullIt(imgObj){
        var imgWidth = parseInt(imgObj.width()),
             imgHeight = parseInt(imgObj.height()),
             pHeight = parseInt(imgObj.parent().parent().height()),
             pWidth = parseInt(imgObj.parent().parent().width());

        if(imgHeight / imgWidth >= pHeight / pWidth){

            imgObj.width(pWidth);

        }else{
            imgObj.height(pHeight);

        }

        imgHeight = parseInt(imgObj.height());
        imgObj.css('margin-top', -(imgHeight - pHeight) / 2);
    }

    $(function(){
        $('.community-top ').find('a img').load(function(){
            fullIt($(this));
        })

        $('.community-top').find('a img').each(function(){
            fullIt($(this));
        })
    })

</script>
