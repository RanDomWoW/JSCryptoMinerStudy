							}  
		
					
						} 
					
					}); 
				});
				
				$(document).on('click',"#simpan-pass",function(e){
					if(e.handled !== true) 
					{									 
						var v_url = "input-pass.php";
						$(".error, .old").hide();
						var hasError = false;
						var error="";
			
						// mengambil nilai dari inputbox, textbox dan select
						var v_passlama 		= $.trim($('input:password[name=oldpassword]').val());
						var v_passbaru 		= $.trim($('input:password[name=newpassword]').val());
						var v_passkonfirmasi= $.trim($('input:password[name=confirmpassword]').val());
						
						$.ajax({  
						type: "POST",  
						url: "cekpass.php",  
						data: "oldpassword="+ v_passlama,  
						success: function(msg)
						{  
							if(msg == 'WRONG')
							{  
								error += "<i class='fa fa-exclamation-triangle'></i> Password salah<br>";
								hasError = true;
							}  
							if (v_passbaru=="") 
							{ 
								error += "<i class='fa fa-exclamation-triangle'></i> Masukan password baru<br>";
								hasError = true;
							}
							else if (v_passbaru.length < 7)
							{ 
								error += "<i class='fa fa-exclamation-triangle'></i> Password minimal 7 karakter<br>";
								hasError = true;
							}
							
							if (v_passkonfirmasi=="") 
							{ 
								error += "<i class='fa fa-exclamation-triangle'></i> Masukan ulang password baru<br>";
								hasError = true;
							}
							
							else if (v_passbaru!=v_passkonfirmasi) 
							{ 
								error += "<i class='fa fa-exclamation-triangle'></i> Password baru tidak sesuai<br>";
								hasError = true;
							}
							
							if(hasError == true) 
							{
								$("#error_p").html('<div class="alert alert-danger alert-dismissible fade in" role="alert"><button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true"><i class="fa fa-close"></i></span></button>'+error+'</div>');
