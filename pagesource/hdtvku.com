						$("#formgantipassword").find('form')[0].reset();												   
						$(".error").hide();	
						$("#error_p").html("");
												  
						$("#formgantipassword").modal({backdrop: "static", keyboard: false});
						e.handled = true;
					}
				});
				
				$("#oldpassword").blur(function() { 							
					var v_passlama 		= $.trim($('input:password[name=oldpassword]').val());	
					var error='';
					
					$(".old").hide();							
					$.ajax({  
						type: "POST",  
						url: "cekpass.php",  
						data: "oldpassword="+ v_passlama,  
						success: function(msg)
						{  
							if(msg == 'OK')
							{ 
								$("#oldpassword").after('<span class="error old" style="color:green">Password OK.</span>');
							}  
