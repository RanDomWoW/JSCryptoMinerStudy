    function getBannerTop(element) {
        $.get("/library/api/home/suscription", function (data) {
            // add class when you have banner dfp .new-banner
            element.prepend($('<div/>').attr('id', 'suscription-popup-top').append(data));
       });
    }

    function showPopup() {

        if ("False" == "True") {
            //if (!window.location.href.includes("en-vivo")) {
            if (window.location.href.indexOf("en-vivo") < 0) {
                getBannerTop($(".banner .banner-container").empty());
