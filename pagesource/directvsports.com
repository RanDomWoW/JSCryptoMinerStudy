        }

    $(function () {

        loadPopupContent();
        var mySetTimeOut;
        activeBanners();



        $("#suscription-button").click(function (e) {
            e.preventDefault();
            if (!$("#suscription-button").is('[disabled]')) {
                $("#suscription-button").attr("disabled", "disabled");
                $("#suscription-button").text("");
                $("#suscription-button").append("<div class='loaderCss' style='display: none'></div>");
