        function CountryBasedRedirect(url) {
            var countryCode = getCountryCode();
            window.location = "/" + countryCode + url;
        }

        function getCountryCode(codeCountryCode){
            var countryCode = "ar";
            if(codeCountryCode===undefined){
                codeCountryCode=ReadCookie("dtvsports_country");
            }
            switch (codeCountryCode.toUpperCase()) {
                case "AR":
                    countryCode = "ar";
                    break;
                case "CL":
                    countryCode = "cl";
                    break;
                case "CO":
                    countryCode = "co";
                    break;
                case "EC":
                    countryCode = "ec";
                    break;
                case "PE":
                    countryCode = "pe";
                    break;
                case "UY":
                    countryCode = "uy";
                    break;
                case "VE":
                    countryCode = "ve";
                    break;
                default:
                    countryCode = countryCode;
                    break;
            }
            return countryCode;
        }

        function CountryReplaceBasedRedirect(url) {
            var countryCode = getCountryCode();
            var urlComplete = url.replace("{countryCode}", countryCode);
            window.location =  urlComplete;
        }

