    function activeBanners() {
        var obj = ReadCookie("anonymousUser") != null ? ReadCookie("anonymousUser").split("#") : "";
        //show dfp top allways
        //dfp("DTVSP3_header_top1");
        //if (!window.location.href.includes("en-vivo")) {
        if (window.location.href.indexOf("en-vivo")<0) {
            //getBannerTop();
            if ("False" != "True") {
                dfp("DTVSP3_rightsidebar_top1");
            }
            else if (obj == "" && ReadCookie("dtvsports_c") == "true" || obj == "" && ReadCookie("dtvsports_c") != "true") {
                $(".box-suscription").slideToggle("slow");
            }
            // client + not register newsletters
            else if (ReadCookie("dtvsports_c") == "true" && obj[2] == "false") {
                //show newsletter box
                $(".box-suscription").slideToggle("slow");
                getBannerTop($(".banner .banner-container").empty());
            }
            // client + register newsletters
            else if (ReadCookie("dtvsports_c") == "true" && obj[2] == "true") {
                getBannerTop($(".banner .banner-container").empty());
                // close newsletters box
                $(".box-suscription").slideUp("slow");
                // show dfp right
                dfp("DTVSP3_rightsidebar_top1");
            }
                // popup showing + not register newsletters
            else if (ReadCookie("dtvsports_c") != "true" && obj[1] == "true" && obj[2] == "false") {
                getBannerTop($(".banner .banner-container").empty());
                // show newsletter box
                $(".box-suscription").slideToggle("slow");
                // show banner not dfp top
                //  getBannerTop();
                //dfp("DTVSP3_rightsidebar_top1");
            }
                // popup showing + register newsletters
            else if (ReadCookie("dtvsports_c") != "true" && obj[1] == "true" && obj[2] == "true") {
                getBannerTop($(".banner .banner-container").empty());
                // show newsletter box
                $(".box-suscription").slideUp("slow");
                // show banner not dfp top
                //  getBannerTop();
                dfp("DTVSP3_rightsidebar_top1");

            }
        }
        else {
            var objLive = ReadCookie("popUpUserLive") != null ? ReadCookie("popUpUserLive") : "";
