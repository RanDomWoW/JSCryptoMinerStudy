<script type="text/javascript">
$(function() {
	countDown(0);
});
function countDown(c) {
	var setD,setH,setM,setS;
	var data = { date : c };
	$.ajax({
		type:      'POST',
		url:       'https://homare.asia-u.ac.jp/api/date.php?callback=?',
		dataType : 'jsonp',
		jsonp:     'callBack',
		data:      data,
		success:   function(msg) {
			nowImg  = msg.img;
			$('.main_visual').css({'background-image':'url(/common/images/top/countdown/'+nowImg+'.png)'});

			nowDate = msg.html;
			strDate = new Date(nowDate);
			endDate = new Date("2016/11/04 00:00:00");
			setDiff = endDate - strDate;
			if(setDiff>0) {
				d3=null; d2=null; d1=null;
				countMethod();
				setTimer = setInterval(countMethod, 1000);
			} else {
				$('.count_down').css('display', 'none');
				$('.count_down_finish').css('display', '');
			}
			$('.count_down').stop().animate({opacity:1}, 300);
		},
		complete:  function() {
		}
	});
}
function countMethod() {
	setDiff = setDiff - 1000;
	var setTime = 24 * 60 * 60 * 1000;
	var setD = Math.ceil(setDiff / setTime);
	var setH = Math.floor((setDiff % setTime) / (60 * 60 * 1000));
	var setM = Math.floor((setDiff % setTime) / (60 * 1000)) % 60;
	var setS = Math.floor((setDiff % setTime) / 1000) % 60 % 60;
	var dig  = ('00'+setD).slice(-3);
	if(d3 != dig.substr(0,1)) {
		d3 = dig.substr(0,1);
		if($('.main_date img').hasClass('dig3')) {
			$('.main_date img.dig3').attr('src', '/common/images/top/countdown/num'+d3+'.png');
		} else {
			$('.main_date').append('<img src="/common/images/top/countdown/num'+d3+'.png" class="dig3">');
		}
	}
	if(d2 != dig.substr(1,1)) {
		d2 = dig.substr(1,1);
		if($('.main_date img').hasClass('dig2')) {
			$('.main_date img.dig2').attr('src', '/common/images/top/countdown/num'+d2+'.png');
		} else {
			$('.main_date').append('<img src="/common/images/top/countdown/num'+d2+'.png" class="dig2">');
		}
	}
	if(d1 != dig.substr(2,1)) {
		d1 = dig.substr(2,1);
		if($('.main_date img').hasClass('dig1')) {
			$('.main_date img.dig1').attr('src', '/common/images/top/countdown/num'+d1+'.png');
		} else {
			$('.main_date').append('<img src="/common/images/top/countdown/num'+d1+'.png" class="dig1">');
		}
	}
	//$('.main_time').text(setD + '日' + ('0'+setH).slice(-2) + '時間' + ('0'+setM).slice(-2) + '分' + ('0'+setS).slice(-2) + '秒');
	if(setH==0 && setM==0 && setS==0) {
		clearInterval(setTimer);
		countDown(setD);
	}
}
</script>
