<script async>
	//<![CDATA[
		var homepageSubscribeForm = new VarienForm('homepageSubscribeForm', false);
	//]]>

	function addCustomSubscribers(ev) {
		if (!homepageSubscribeForm.validator.validate()) {
			setTimeout(jQuery('.newsletterOpen').colorbox.resize, 100);
			return false;
		}
		jQuery('#newsletterSubscriberLoaderHomepage').show();
		jQuery('.newsletterOpen').colorbox.resize();
		jQuery('#newsletterSubscriberLoaderHomepage').hide();
		jQuery('#newsletterSubscriberLoaderHomepage').fadeIn('slow');
		var email = jQuery('#newsletterEmailHomepage').val();
		jQuery.ajax({
			url: 'https://www.orangecenter.bg/newsletter/subscriber/guestsubscribe/',
			type: 'POST',
			data: {email: email },
		}).done(function() {
			jQuery('#newsletterSubscriberLoaderHomepage').show();
			jQuery('.newsletterOpen').colorbox.resize();
			jQuery('#newsletterSubscriberLoaderHomepage').hide();
			jQuery('#newsletterSubscriberLoaderHomepage').fadeOut('slow');
		}).success(function(result) {
			jQuery('#resultOfAddSubscribersHomepage').hide().html(result);
			jQuery('#resultOfAddSubscribersHomepage').show();
			jQuery('.newsletterOpen').colorbox.resize();
			jQuery('#resultOfAddSubscribersHomepage').hide().fadeIn('slow');
		});
		Event.stop(ev);
		ev.preventDefault();
		return false;
	}

	Event.observe(homepageSubscribeForm.form,'submit', addCustomSubscribers.bind(homepageSubscribeForm.validator),false);

	jQuery(function(){
		jQuery('#newsletterEmailHomepage').data('holder',jQuery('#newsletterEmailHomepage').attr('placeholder'));
		jQuery('#newsletterEmailHomepage').focusin(function(){
			jQuery(this).attr('placeholder','');
		});
		jQuery('#newsletterEmailHomepage').focusout(function(){
			jQuery(this).attr('placeholder',jQuery(this).data('holder'));
		});
	});
</script>
