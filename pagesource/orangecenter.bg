<script async>
	//<![CDATA[
		var popupSubscribeForm = new VarienForm('popupSubscribeForm', true);
	//]]>

	function addCustomSubscribers(ev) {
		if (!popupSubscribeForm.validator.validate()) {
			setTimeout(jQuery('.newsletterOpen').colorbox.resize, 100);
			return false;
		}
		jQuery('#newsletterSubscriberLoader').show();
		jQuery('.newsletterOpen').colorbox.resize();
		jQuery('#newsletterSubscriberLoader').hide();
		jQuery('#newsletterSubscriberLoader').fadeIn('slow');
		var email = jQuery('#newsletterEmail').val();
		jQuery.ajax({
			url: 'https://www.orangecenter.bg/newsletter/subscriber/guestsubscribe/',
			type: 'POST',
			data: {email: email },
		}).done(function() {
			jQuery('#newsletterSubscriberLoader').show();
			jQuery('.newsletterOpen').colorbox.resize();
			jQuery('#newsletterSubscriberLoader').hide();
			jQuery('#newsletterSubscriberLoader').fadeOut('slow');
		}).success(function(result) {
			jQuery('#resultOfAddSubscribers').hide().html(result);
			jQuery('#resultOfAddSubscribers').show();
			jQuery('.newsletterOpen').colorbox.resize();
			jQuery('#resultOfAddSubscribers').hide().fadeIn('slow');
		});
		Event.stop(ev);
		ev.preventDefault();
		return false;
	}

	Event.observe(popupSubscribeForm.form,'submit', addCustomSubscribers.bind(popupSubscribeForm.validator),false);

	jQuery(function(){
		jQuery('#newsletterEmail').data('holder',jQuery('#newsletterEmail').attr('placeholder'));
		jQuery('#newsletterEmail').focusin(function(){
			jQuery(this).attr('placeholder','');
		});
		jQuery('#newsletterEmail').focusout(function(){
			jQuery(this).attr('placeholder',jQuery(this).data('holder'));
		});
	});
</script>            </div>
