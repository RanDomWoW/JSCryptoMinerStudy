                    <script type="text/javascript">
                    // v3.1.0
                      //Docs at http://simpleweatherjs.com
                      $(document).ready(function() {
                        $.get("http://freegeoip.net/json/", function (response) {
                        var lat = response.latitude;
                        var long = response.longitude;
                        weatherInfo(lat,long);
                        console.log(lat,long);                  
                    }, "jsonp");
                        
                        
                        
                        function weatherInfo(lat,long){
                          $.simpleWeather({
                            location: lat +","+long,
                            woeid: '',
                            unit: 'c',
                            success: function(weather) {
                              html = '<h2><i class="icon-'+weather.code+'"></i> '+weather.temp+'&deg;'+weather.units.temp+'</h2>';
                              html += '<ul><li>'+weather.city+', '+weather.country+'</li>';
                              html += '<li class="currently">'+weather.currently+'</li></ul>';
                              // html += '<li>'+weather.wind.direction+' '+weather.wind.speed+' '+weather.units.speed+'</li>';
                          
                              $("#weather").html(html);                            
                            },
                            error: function(error) {
                              $("#weather").html(error);
                            }
                          });
                        }
                        
                        $(".epaper-top > span").click(function(event) {
                          $(this).parent().toggleClass('et-active');
                        });
                        
                        $(document).click(function(e) {
                          var container = $(".epaper-top");
                          if (!container.is(e.target) 
                              && container.has(e.target).length === 0) 
                          {
                            $(".epaper-top").removeClass('et-active');
                          }
                          
                        });
                        
                      
                      });
                    </script>
                    
