<script type="text/javascript">
    $(function() {
        $('#ZiShuWx003').hDialog({ title: '关注紫数网微信公众号', width: 420, height: 462, 'box': '#ZishuWx01','boxBg':'#fff', modalHide: true, closeBg: '#6d1c7b' });
        $('#emailrssclick').hDialog({ title: '', width: 350, height: 200, box: '#EmailRss', boxBg: '#fff', modalHide: true, closeBg: '#6B6B6B' });
    });
    function ZiShuWx() {
        $('#ZiShuWx003').click();
    }
    function EmailRss() {
        var currentcookie = $.cookie('zishucurrentuser');
        if (currentcookie == null) {
            $('#emailrssclick').click();
            $('#EmailRss').css('background', '#fff');
        } else {//直接发送ajax请求
            $.post('/Default/CheckEmailRss', { 'email': null }, function (result) {
                switch (result) {
                    case '1':
                        alert('您已订阅！');
                        break;
                    case '-2': //弹出填写邮箱层
                        $('#EmailRss').window('open');
                        break;
                    case "0":
                        alert('已发送邮件到当前登录的账户邮箱，请查收！');
                        break;
                    default:
                        alert('订阅失败，请稍后再试！' + result);
                        break;
                }
            });
        }
    }
    function CheckEmail() {
        var val = $('#email_input').val().replace(/[ ]/g, '');
        if (val.length > 0) {
            var myreg = /^[_a-zA-Z0-9_-_\._-]+@([_a-zA-Z0-9_-]+\.)+[a-zA-Z]{2,3}$/;
            if (myreg.test(val)) {
                $.post('/Default/CheckEmailRss', { 'email': val }, function (result) {
                    switch (result) {
                        case '1':
                            alert('当前邮箱已订阅！');
                            break;
                        case '0':
                            alert('已发送邮件到此邮箱：' + val + " 请查收并激活！");
                            break;
                        default:
                            alert('订阅失败，请稍后再试！' + result);
                            break;
                    }
                });
            } else {
                alert('请填写正确的订阅邮箱!');
                return false;
            }
        } else {
            alert('请填写订阅邮箱!');
            return false;
        }
        return true;
    }
</script>

    
    
