<script type="text/javascript">
    $(function() {
        $('#zctiaokuan').hDialog({ title: '紫数-注册条款', width: 506, height: 433, 'box': '#ShowXy', modalHide: true, closeBg: '#f2f2f2' });
        $(".zs-login-register ul li").on('click', function() {
            $(this).addClass("zs-border-bottom").siblings().removeClass("zs-border-bottom");
            var name = $(this).attr('name');
            switch(name) {
                case 'login':
                    $('.zs-login-register .login-areas').show();
                    $('.zs-login-register .register-area').hide();
                    $('#checkcodeimg').attr("src", "/z/createcode.html?id=" + Math.random());
                    break;
                case 'register':
                    $('.zs-login-register .login-areas').hide();
                    $('.zs-login-register .register-area').show();
                    $('#phonecheckcodeimg').attr("src", "/z/createcode.html?id=" + Math.random());
                    break;
            }
        }); 
        $(".zs-close").on('click', function () {
            $(".zs-cover").hide();
            $(".zs-login-register").hide();
        });
        //登录成功
        $(".login-buton").on('click', function () {
            $("#HOverlay").hide();
            $("#loginbody").hide();
            $(".zbl-synopsis3-login").hide();
            $(".zs_synopsis3_write").show();
        });
    });

    function ShowLoginDiv() {
        $('#errorshow').html('');
        if (!CheckUser()) { 
            $(".zs-cover").show();
            $(".zs-login-register").show();
            $("#checkcodeimg").attr("src", "/z/createcode.html?id=" + Math.random());
            return false;
        } else {
            return true;
        }
    }

    //检查当前用户是否登录了
    function CheckUser() {
        var currentcookie = $.cookie('zishucurrentuser');
        if (currentcookie == null) {
            return false;
        }
        return true;
    }
</script> 

