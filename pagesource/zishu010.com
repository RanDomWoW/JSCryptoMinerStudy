<input type="hidden" id="currentPageIndex" value="1"/>
<input type="hidden" id="currentPageSize" value="15"/>
<input type="hidden" value="1" id="currentIsBest"/>
<input type="hidden" id="currentClassId" value="999" />
<input type="hidden" value="30" id="currentBestBday" />
<input type="hidden" id="isLoadFlag" value="0"/>
<script type="text/javascript">
    var currentLoadingPage = 1;
    $(function () {
        $(window).scroll(function () {
            var totalHeight = parseFloat($(window).height()) + parseFloat($(window).scrollTop());
            if ($(document).height() - $(document).height() * 1 / 4 <= totalHeight && currentLoadingPage <= 3) {
                var loadFlag = $('#isLoadFlag').val();
                if (loadFlag === '0') {
                    $('#isLoadFlag').val('1');
                    LoadArticleList();
                }
            }
        });
        $('.load-more-a').click(function() {
            LoadArticleList();
            currentLoadingPage = 0;
            $('#isLoadFlag').val('0');
        });
        /*判断当前页有没有更多数据*/
        var alldata = $('#currentTotal').val();
        var psize = $('#currentPageSize').val();
        if (parseInt(alldata) < parseInt(psize)) {
            $('#isLoadFlag').val('1');
            $('div.load-more-btn').html('没有更多的数据'); 
        }

        /*最热、最新、最优*/
        var currentBest = $('#currentIsBest').val();
        switch (currentBest) {
        case '0':
            $('.left-content-menu .bestHot').addClass('on');
            break;
        case '1':
            $('.left-content-menu .bestNew').addClass('on');
            break;
        case '2':
            $('.left-content-menu .bestBest').addClass('on');
            break;
        }

        /*最热、最新、最优 点击事件*/
        $('.left-content-menu .selectedBest li a').click(function() {
            var cb = $(this).attr('name');
            window.location.href = '/z/'+$('#currentClassId').val()+'/' + cb + '.html';
        });
    });
    var LoadArticleList = function () {
        var pageIndex = $('#currentPageIndex').val();
        var classId = $('#currentClassId').val();
        var pd = parseInt(pageIndex)+1;
        var alldata = $('#currentTotal').val();
        if (parseInt(alldata) < (pd-1) * 15) {
            $('#isLoadFlag').val('1');
            $('div.load-more-btn').html('没有更多的数据');
            return;
        }
        var data = {
            pageIndex: pd,
            pageSize: 15,
            classId:classId
        }
        $.post('/Default/LoadArticleList', data, function (result) {
            if (result === '') {
                $('#isLoadFlag').val('1');
                $('div.load-more-btn').html('没有更多的数据');
            } else {
                $('#currentPageIndex').val(pd);
                $('#mainlist').append(result);
                //图片延迟加载
                $('#mainlist' + pd+' img.lazy').lazyload({ effect: "fadeIn" });
                currentLoadingPage = currentLoadingPage + 1;
                $('#isLoadFlag').val('0');
            }
        });
    };
</script>

