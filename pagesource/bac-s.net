    jQuery(document).ready(function(){
        jQuery('#ajaxnews').click(function(){
            var email = jQuery('#blocmail input[name="mail"]').val();
            ajaxdata = {
                email: email,
            };
            jQuery.ajax({
                type: 'post',
                url: '/ajax/newsletter.php',
                data: ajaxdata,
                context: this,
                success: function(data){
                    data = jQuery.parseJSON(data);
                    jQuery(this).add('#blocmail input[name="mail"]').hide();
                    if(data.error)
                        jQuery('#blocmail').append('<span class="newsresponse erreur">'+data.reponse+'</span>');
                    else{                       
                        jQuery('#blocmail').append('<span class="newsresponse valid">'+data.reponse+'</span>');
                    }

                }
            });
            return false;
        });
    });
</script>

        

