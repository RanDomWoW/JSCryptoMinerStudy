<script>

    $(function() {

        // input for auto-posting to a social site
        $('.profile-authentication-button').click(function(e) {
            var type = $(this).data('profile-authentication-type');
            var $icon =  $(this).find('i').first();
            var $input = $(this).find('input[type=hidden]');
            if ($input.val() === '1') {
                $icon.removeClass('fb-icon-' + type + '-square');
                $icon.addClass('fb-icon-' + type + '-square-o');
                $(this).find('.fa-check').addClass('blue');
                $input.val('0');
            } else {
                $icon.removeClass('fb-icon-' + type + '-square-o');
                $icon.addClass('fb-icon-' + type + '-square');
                $(this).find('.fa-check').removeClass('blue');
                $input.val('1');
            }
        });

        // set the share text
        $('#social_share_share_text').val('Les invisibles, Antoine Albertini : découvrir les premières pages ! #NetGalleyFrance');

        // function to submit the review form after validation or confirmation
        NetGalley.submitSocialShareForm = function($socialShareForm) {
            var socialShareFormData = $socialShareForm.serializeObject();

            $.ajax({
                type: 'POST',
                url: "//api.netgalley.fr/public/excerpts/book/128861",
                xhrFields: {
                    withCredentials: true
                },
                data: socialShareFormData,
                dataType: 'json'
            })
            .always(function () {
                $('#social_share_submit').prop('disabled', false);
            })
            .fail(function (data) {
                $('#social_share_submit i').remove();
                if (data.responseJSON && data.responseJSON.errors) {
                    var $errorContainer = $('#formErrorsModal .modal-body-content').first();
                    $errorContainer.text('');

                    for (var i = 0; i < data.responseJSON.errors.length; i++) {
                        $('<p>')
                            .text(data.responseJSON.errors[i])
                            .appendTo($errorContainer)
                        ;
                    }

                    $('#formErrorsModal')
                        .modal('show')
                    ;
                }
            })
            .done(function(data) {
                $('#socialShareForm').hide();
                $('#socialShareSuccess').show();
                $('#social_share_submit i').remove();
            });
        };

        // handling of the social share form
        $('body').on('submit', '#socialShareForm', function (e) {
            e.preventDefault();

            // add the spinner and disable
            $('#social_share_submit').append('<i class="fa fa-spinner fa-spin"></i>');
            $('#social_share_submit').prop('disabled', true);

            NetGalley.submitSocialShareForm($(this));
        });
    });

</script>

