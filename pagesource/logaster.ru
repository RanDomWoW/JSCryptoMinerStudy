            start: function()
            {
                return $(this).data('score');
            }
        });
    });

    var logoCounter = {
        run: function(realLogoCount)
        {
            logoCounter.logoCount = realLogoCount - 40;
            logoCounter.realLogoCount = realLogoCount;
            logoCounter.animate();
        },
        animate: function()
        {
            logoCounter.setLogoCount(logoCounter.logoCount++);
            var timeout = 100;

            if (logoCounter.logoCount >= logoCounter.realLogoCount) return;

            if (logoCounter.logoCount < logoCounter.realLogoCount - 20)
            {
                timeout = (logoCounter.realLogoCount - logoCounter.logoCount - 20);
                timeout = 40 * (20-timeout);
            }
            else
            {
                timeout = (Math.floor(Math.random() * (7)) + 2)*1000;
            }

            //var timeout = logoCounter.logoCount < logoCounter.realLogoCount - 20?100:Math.floor(Math.random() * (7)) + 2*1000;
            window.setTimeout(logoCounter.animate, timeout);
        },

        setLogoCount: function(value)
        {
            $("#logo-count").text(logoCounter.addCommas(value));
        },
        addCommas: function(nStr)
        {
            nStr += '';
            x = nStr.split('.');
            x1 = x[0];
            x2 = x.length > 1 ? '.' + x[1] : '';
            var rgx = /(\d+)(\d{3})/;
            while (rgx.test(x1))
            {
                x1 = x1.replace(rgx, '$1' + ',' + '$2');
            }
            return x1 + x2;
        }
    };

    logoCounter.run(7556524);

