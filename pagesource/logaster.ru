            {
                var ratyContainer = $(this);
                var galleryLogotypeId = ratyContainer.data('id');
                if ($.cookie('rated_' + galleryLogotypeId) != 'true')
                {
                    $.post(
                        'https://www.logaster.ru/gallery/set_gallery_score/',
                        {'galleryLogotypeId': galleryLogotypeId, 'score': score},
