currentOffset=0;
function toggleFeaturedNav(toggle){
	$('featuredNavRight').set('tween', {duration: 500});
	if(toggle){
		if(!$('featuredRowContent').style.marginLeft || $('featuredRowContent').style.marginLeft=="0px")
			$('featuredNavRight').fade('in');
		else
			$('featuredNavLeft').fade('in');
		}
	else{
		if(!$('featuredRowContent').style.marginLeft || $('featuredRowContent').style.marginLeft=="0px")
			$('featuredNavRight').fade('out');
		else
			$('featuredNavLeft').fade('out');
		}
	}
function scrollFeatured(scrollAction){
// 	rowwidth = 630;
//	rowwidth = 972;
	rowwidth = $('featuredRow').offsetWidth+22;
	setMl=0;
	ml=$('featuredRowContent').style.marginLeft.replace("px", "");
	if((ml>(0-rowwidth) && scrollAction=="right") || (ml<(0-rowwidth) && scrollAction=="left"))
		setMl=0-(rowwidth);
	else if((ml>(0-(2*rowwidth)) && scrollAction=="right"))
		setMl=0-(2*rowwidth);
	else if(scrollAction=="right")
		setMl=ml;
	
	$('featuredRowContent').tween('margin-left', setMl);
	if(scrollAction=="right"){
		(function() { 
			$('featuredNavRight').fade('out');
			$('featuredNavLeft').fade('in');
			}).delay(1);
		}
	else{
		(function() { 
			$('featuredNavRight').fade('in');
			$('featuredNavLeft').fade('out');
			}).delay(1);
		}

}
