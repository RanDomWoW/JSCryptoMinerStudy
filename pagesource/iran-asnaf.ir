    <script>
        $(document).ready(function () {
            function datetim() {

                var now = $(".thistime").text();

                var hours = now.substring(0, 2);
                //alert(hours);
                // var tm = " AM";

                //if (hours == 12) { tm = " PM"; }

                //if (hours < 12) { tm = " AM"; }

                //if (hours > 12) { hours = hours - 12; tm = " عصر"; }

                //if (hours == 0) { tm = " صبح"; hours = 12; }

                var minutes = now.substring(3, 5);

                var seconds = now.substring(6, 8);
                seconds++;
                if (seconds >= 60) {
                    minutes++;
                    if (minutes <= 9)
                        minutes = "0" + minutes;
                    seconds = "00";
                }
                else
                    if (seconds <= 9)
                        seconds = "0" + seconds;
                var timeValue = hours + ":" + minutes + ":" + seconds;
                //timeValue =  minutes;
                //timeValue +=  seconds;

                $(".thistime").text(timeValue);

                if (minutes >= 59)
                { date(); }
                else
                    setTimeout(datetim, 1000);

            }
            function date() {

                $.ajax({
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    url: "/WebService/date.asmx/datetime",
                    data: "{}",
                    dataType: "json",
                    success: function (data) {
                        var myArr = JSON.parse(data.d);
                        // alert(myArr[0].time);
                        $(".thistime").text(myArr[0].time);
                        $(".thisDate").text(myArr[0].date);

                        datetim();
                        // setTimeout(date, 500);

                    },
                    error: function (result) {
                        //  alert("Error");
                    }
                });
            }
            date();
            $('.box_div_Desc marquee').css('width', '190px');

            $('.nav2 ul li').find('img').css('opacity', '0.6');


            $('.nav2 ul li').hover(function () {

                $(this).find('img').css('opacity', '1');
            });

            $('.nav2 ul li').mouseleave(function () {

                $(this).find('img').css('opacity', '0.6');
            });

            if ($("#categories  div").hasClass("LiteralDivRight")) {
                $(".LiteralDivLeft").css('width', '76%');
            }
            else {
                $(".LiteralDivLeft").css('width', '98%');
                $(".LiteralDivLeft #ListPro .product").css('margin', '10px 22px');
            }
        });
    </script>
