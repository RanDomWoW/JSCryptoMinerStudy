<script>
/**
 * お気に入りアイコン押下時の処理
 *
 * お気に入りの追加/削除を行い
 * アイコンを切り返る
 *
 * @param url 非同期通信先URL
 */
function addFavorite(url, itemId) {

    /* Class名接頭語 */
    const CLASS_NAME_PREFIX = "favorite_";

   /* Class名接尾語 on */
    const CLASS_NAME_SUFFIX_ON = "on";

     /* Class名接尾語 off */
    const CLASS_NAME_SUFFIX_OFF = "off";

    // 現在ページのurl
    var currentUrl = location.href ;

    /* クリックした商品のお気に入りアイコンclass名 */
    var targetTag = ".favorite_icon_product_" + itemId;

    console.log(targetTag);

    /* アイコン押下前のクラスを取得 */
    var beforeIconClass = $(targetTag).attr('class');

    $.ajax({
	type: 'POST',
	url: url,
        data: {
            returnUrl: currentUrl
        },
        dataType: "json"
    }).done(function(result) {

        // ログイン状態で分岐
        if (result.login) {

            // お気に入りアイコン状態初期化
            $(targetTag).removeClass(CLASS_NAME_PREFIX + CLASS_NAME_SUFFIX_OFF);
            $(targetTag).removeClass(CLASS_NAME_PREFIX + CLASS_NAME_SUFFIX_ON);

            $(targetTag).addClass(CLASS_NAME_PREFIX + result.value);

        } else {
            window.location.href = result.value;
        }

    }).fail(function(e) {
        console.log(e);
    });
}
</script>

