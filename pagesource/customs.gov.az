	<script type="text/javascript">
		$(function(){
		
			$('#btnSearch').click(function(e) { 
				var txt_search = $("#txt_search").val();
					
					if(txt_search.length <= 3) 
						return false;
					window.location.href = "http://customs.gov.az/az/search/?keywords=" + txt_search;
					return false;

		});
	
			$("#txt_search").autocomplete({
				position: { my: "left top+5", at: "left bottom", collision: "none" },
				source: function(request, response) {
					var _this = this.element;
					var url = _this.data('href');
					
					var val = _this.val().trim();
					if(val.length <= 3) 
						return false;
					$.ajax( {
						type: 'POST',
						url: url,
						data: 'livesearch=1&'+_this.serialize(),
						dataType: 'json',
						success: function( data ) {
							response(data);
							
						},
						error: function(XMLHttpRequest, textStatus, errorThrown) {
						}
						});
					
				},
				open: function(event, ui) {
					var txt_search = $("#txt_search").val();
					$('.ui-autocomplete').css('max-width', $(event.target).outerWidth());
					if($('.ui-autocomplete li').length > 3) {
						$('.ui-autocomplete').append('<li class="view-all">    <a href="http://customs.gov.az/az/search/?keywords='+txt_search+'">Bütün nəticə</a></li>'); //See all results
					}
				},	
				select: function(event, ui) {
					var txt_search = $("#txt_search").val();
					$("#txt_search").val($txt_search);
				},
				search: function( event, ui ) {
					var txt_search = $("#txt_search").val();
					if($('.ui-autocomplete li').length > 3) {
						$('.ui-autocomplete').append('<li class="view-all">   <a href="http://customs.gov.az/az/search/?keywords='+txt_search+'">Bütün nəticə</a></li>'); //See all results
					} else {
						$('.ui-autocomplete li.view-all').remove();
					}
				}
			})
			.autocomplete( "instance" )._renderItem = function( ul, item ) {
				var term = this.element.val(),
					regex = new RegExp( '(' + term + ')', 'gi' );
				t = item.label.replace( regex , "<b>$&</b>" );
				return $( "<li></li>" ).data("item.autocomplete", item)
					.append( t )
					.appendTo( ul );
			}
		});
	</script>
	
	<script type="text/javascript">
	$(function () {

			$('#subsrbStep1').click(function(e) { 
				var email = $("#subscr_email").val();
				$("#subscr_email_in").val(email);
			});	


		$('#btn_subscr_submit').click(function(e) { 
				var email = $("#subscr_email_in").val();
				var fullname = $("#subscr_fullname").val();
				var v_g_recaptcha_response = $("#g-recaptcha-response").val();
					if($.trim(fullname) == "")
						return false;
					
					e.preventDefault();
					$("#SubcrResponseMessage").html('');
					$.ajax({
						type: 'POST',
						url: "http://customs.gov.az/az/controller/",
						data: {
						  processSubscription: 1,
						  subscr_email: email,
						  subscr_fullname: fullname,
						  g_recaptcha_response: v_g_recaptcha_response,
						},
						 success:function(data, textStatus, jqXHR) 
						{
							if(data.trim() == "0")
								$("#SubcrResponseMessage").html('Abunə olduğunuz üçün təşəkkür edirik');
							else
								$("#SubcrResponseMessage").html('Səhv baş verdi');
						},
						error: function(jqXHR, textStatus, errorThrown) 
						{
							$("#SubcrResponseMessage").html('Səhv baş verdi');
						}
					});
			

		});

	
 
	/* BEGIN for Budget */
	$('#budget_month, #budget_year').change(function()
	{
		var v_budget_month = $("#budget_month").val();
		var v_budget_year = $("#budget_year").val();
		
		var data = {
			"processGetBudget": 1,
			"budget_month": v_budget_month,
			"budget_year": v_budget_year
		};
		
		data =  $.param(data);
		$.ajax({
			type: "POST",
			dataType: "json",
			url: 'http://customs.gov.az/az/controller/',
			data: data,
			success: function(data) {
		
			$("#gomruk_rusumu_uzre").html(data.gomruk_rusumu_uzre.toFixed(2));
			$("#edv_uzre").html(data.edv_uzre.toFixed(2));
			$("#aksizler_uzre").html(data.aksizler_uzre.toFixed(2));
			$("#yol_vergisi_uzre").html(data.yol_vergisi_uzre.toFixed(2));
			
			$("#budget_total").html((parseFloat(data.gomruk_rusumu_uzre.toFixed(2)) 
			+ parseFloat(data.edv_uzre.toFixed(2)) 
			+ parseFloat(data.aksizler_uzre.toFixed(2)) 
			+ parseFloat(data.yol_vergisi_uzre.toFixed(2))).toFixed(2));
			}
			
		});
		return false;
	
	
	});
	/* END for Budget */
	
	
	});
	
	function round(value, decimals) {
  return Number(Math.round(value+'e'+decimals)+'e-'+decimals);
}

	</script> 

	
	
