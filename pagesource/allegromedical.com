				return '<li><a href="#">' + (idx+1) + '</a></li>' +
						'<img id="header-pause-play" class="hand-cursor" src="/images/icons/rotationalbanners/pause-b-off.gif">'
			return '<li><a href="#">' + (idx+1) + '</a></li>';
	    }
	});
	
	jQuery("#header-pause-play").on( 'click', function() {
		jQuery('#promoHeaderRotatorDiv').cycle('toggle');
		if ( jQuery("#header-pause-play").attr('src').indexOf("pause") > 0 ) {
			jQuery("#header-pause-play").attr('src','/images/icons/rotationalbanners/play-b-on.gif');
		}
		else {
			jQuery("#header-pause-play").attr('src','/images/icons/rotationalbanners/pause-b-off.gif');
		}
	}).on( 'mouseenter', function () {
		if ( jQuery("#header-pause-play").attr('src').indexOf("pause") > 0 ) {
			jQuery("#header-pause-play").attr('src','/images/icons/rotationalbanners/pause-b-on.gif');
		}
		else {
			jQuery("#header-pause-play").attr('src','/images/icons/rotationalbanners/play-b-off.gif');
		}
	}).on( 'mouseleave', function () {
		if ( jQuery("#header-pause-play").attr('src').indexOf("pause") > 0 ) {
			jQuery("#header-pause-play").attr('src','/images/icons/rotationalbanners/pause-b-off.gif');
		}
		else {
			jQuery("#header-pause-play").attr('src','/images/icons/rotationalbanners/play-b-on.gif');
		}
	});
	
});

