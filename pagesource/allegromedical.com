<script type="text/javascript">
	function SetMCCookie(cookieName,cookieValue,nDays)
	{
		try{
			var today = new Date();
			var expire = new Date();
			if (nDays==null || nDays==0) nDays=1;
			expire.setTime(today.getTime() + 3600000*24*nDays);
			document.cookie = cookieName + "=" + escape(cookieValue) + "; expires=" + expire.toGMTString() + "; path=/";
		}
		catch(e){}
	}


try{
	//Set the number of days before your cookie should expire
	var mcExpireDays = 90;
	//Do not change anything below this line
	qstr = document.location.search;
	qstr = qstr.substring(1,qstr.length);
	
	thevars = qstr.split("&");
	for(i=0;i<thevars.length;i++)
	{
		cookiecase = thevars[i].split("=");
		switch(cookiecase[0])
		{
			case "e":
				e = cookiecase[1];
				SetMCCookie("EmailAddr",e,mcExpireDays);
				break;
			case "j":
			    j = cookiecase[1];
			    SetMCCookie("JobID",j,mcExpireDays);
			    break;
			case "l":
			    l = cookiecase[1];
			    SetMCCookie("ListID",l,mcExpireDays);
			    break
			case "jb":
			    jb = cookiecase[1];
			    SetMCCookie("BatchID",jb,mcExpireDays);
			    break;
			case "u":
			    u = cookiecase[1];
			    SetMCCookie("UrlID",u,mcExpireDays);
			    break;
			case "mid":
			    mid = cookiecase[1];
			    SetMCCookie("MemberID",mid,mcExpireDays);
			    break;
			case "sub_id":
			case "sfmc_sub":
				var subId = cookiecase[1];
				SetMCCookie("SubID",subId,mcExpireDays);
			    break;
			default:
				break;
        }
    }
}
catch(e){}
</script>


