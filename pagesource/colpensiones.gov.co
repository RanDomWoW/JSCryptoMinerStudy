	<script type="text/javascript">

		$(document).ready(function () {

			// Item Desplazamiento Anclas
			$('.scroll a').click(function(e){				
				var strAncla=$(this).attr('href'); //id del ancla
				idAncla = strAncla.split('#');
				if (idAncla.length >= 2 )
				{
					e.preventDefault();		//evitar el eventos del enlace normal	
					$('body,html').stop(true,true).animate({				
						scrollTop: $("#"+idAncla[1]).offset().top
					},1000);
				}
			});
			
			//Función Registrar eventos
			
			function registrarClick($this, categoria, tipoLabel){
				var label = $this.attr(tipoLabel);
				console.log(label+' / '+categoria);
				ga('send', 'event', {
					eventCategory: categoria,
					eventAction: 'click',
					eventLabel: label,
					transport: 'beacon'
				  });
				
			}
			
			//Eventos
			$(".listVirtual ul.enlace1 li a, .tablaBloque1004311 .btn.col-md-5.col-md-offset-4").on("click", function(){registrarClick($(this),"Menu Oficina Virtual - RPM", "title")} ); //Se mantiene
			
			$(".megamenu ul.enlace1 li a").on("click", function(){registrarClick($(this),"Menu principal - RPM", "title")} );
			
			$(document).on("click",".bxslider100151 a.bxSliderHref", function(){registrarClick($(this),"Portada - RPM", "data-titulo")} );
			
			$(document).on("click",".tablaBloque1004403 .enlace1 li a", function(){registrarClick($(this),"Menu lateral - RPM", "title")} ); //Se mantiene

			$(document).on("click","#responsive100328 a.slick-slide", function(){registrarClick($(this),"Accesos destacados - RPM", "title")} );
			
			$(document).on("click","#responsive1004168 a.thumbnail", function(){registrarClick($(this),"Menu Segmentos", "title")} ); //Se mantiene
			
			$(document).on("click",".noticias .historicoPubli a, .noticias a.enlacePub", function(){registrarClick($(this),"Ultimas noticias - RPM", "href")} );
			
			$(document).on("click","#responsive1004199 a.enlacePub", function(){registrarClick($(this),"Slider secundario - RPM", "href")} );
			
			$(".tablaBloque100346 .contenido1 h3 a").on("click", function(){registrarClick($(this),"Footer destacados - RPM", "title")} );
			
			$("footer ul.enlace1 li a").on("click", function(){registrarClick($(this),"Menu Footer - RPM", "title")} ); //Se mantiene
			
			
			

		});
