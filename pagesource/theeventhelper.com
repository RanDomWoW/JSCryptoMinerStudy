	<div class="stage"><style>.ie8-hate {display/*\**/: none\9;}</style>

<div class="home-page-content" ng-controller="homePageCtrl as home">

	<!-- blue rater section -->
	<div class="blue section rater" id="a-rater">
		<div class="tbl constrain">
			<div class="td w1">
				<div class="forms" ng-switch on="home.chosenRaterForm">

					<!-- rater form: rater -->
					<form validate-form
						name="home.getQuoteForm"
						class="r-form get-quote fade-slide"
						ng-submit="home.getQuoteForm.$valid && home.submitGetQuote(home.getQuoteFields)"
						ng-switch-when="getQuote">

						<div class="fade-slide" ng-if=" ! home.formError">
							<h1 class="form-title">Event Insurance</h1>

							<div class="clearfix">

								<!-- event location field -->
								<div
									class="field half pull-left"
									ng-hide="global.organization.scheme.criteria.state.hidden">

									<label for="get-quote-state">Event Location</label>
									<select required
										class="form-control"
										id="get-quote-state"
										name="get-quote-state"
										ng-model="home.getQuoteFields.state"
										ng-disabled="global.organization.scheme.criteria.state.disabled"
										ng-options="state.short as state.long for state in home.stateOpts | orderBy : 'long'"></select>
								</div>

								<!-- coverage length field -->
								<div
									class="field half pull-left"
									ng-hide="global.organization.scheme.criteria.length.hidden">

									<label for="get-quote-length">Coverage Length</label>
									<select required
										class="form-control"
										id="get-quote-length"
										name="get-quote-length"
										ng-model="home.getQuoteFields.length"
										ng-disabled="global.organization.scheme.criteria.length.disabled"
										ng-options="opt.value as opt.label for opt in home.lengthSelectOpts | orderBy : 'value'"></select>
								</div>
							</div>

							<!-- attendance field -->
							<div
								class="field half"
								style="width: 55%;"
								ng-hide="global.organization.scheme.criteria.attendance.hidden">

								<label for="get-quote-attendance">Estimated total attendance?</label>
								<div class="input-group">
									<input required
										min="1"
										max="1000000"
										type="text"
										number="int"
										class="form-control"
										id="get-quote-attendance"
										name="get-quote-attendance"
										ng-model="home.getQuoteFields.attendance"
										ng-disabled="global.organization.scheme.criteria.attendance.disabled">

									<span class="input-group-addon">people</span>
								</div>
							</div>

							<!-- event type field -->
							<div ng-cloak
								class="field clearfix"
								style="margin-bottom: 14px;"
								ng-switch="home.eventTypeSearch"
								ng-hide="global.organization.scheme.criteria.ccg.hidden">

								<label class="truncate" for="get-quote-ccg">What type of event is it?</label>

								<div class="tbl">
									<div class="td">
										<select required
											class="form-control pull-left event-type"
											id="get-quote-ccg"
											name="get-quote-ccg"
											ng-switch-when="false"
											ng-model="home.getQuoteFields.ccg"
											ng-disabled="global.organization.scheme.criteria.ccg.disabled"
											ng-options="code.ccg as code.label for code in home.classCodesOpts | orderBy : 'label'"></select>

										<input required search-types
											valid="codeSearch"
											class="form-control pull-left event-type"
											id="get-quote-ccg"
											name="get-quote-ccg"
											placeholder="Search event types..."
											ng-switch-when="true"
											ng-model="home.searchTerm"
											ng-disabled="global.organization.scheme.criteria.ccg.disabled">
									</div>

									<div class="td" style="width: 8px;">&nbsp;</div>

									<div
										tooltip="top"
										class="td"
										style="width: 1px;"
										title="{{home.eventTypeSearch ? 'List event types' : 'Search event types'}}">

										<a
											class="btn btn-default pull-right switch-btn"
											ng-click="home.eventTypeSearch = ! home.eventTypeSearch">

											<i class="fa fa-list-alt" ng-show="home.eventTypeSearch"></i>
											<i class="fa fa-search" ng-hide="home.eventTypeSearch"></i>
										</a>
									</div>
								</div>
							</div>

							<!-- form submit -->
							<button
								ng-disabled="home.cantBuyThis || home.submittingForm"
								class="btn btn-warning">Continue</button>

							<span ng-hide="home.submittingForm || home.cantBuyThis" class="price" ng-cloak>
								{{home.calculatedPrice | currency}}
							</span>
							<span ng-show=" ! home.submittingForm && home.cantBuyThis" class="price">
								N/A
							</span>
							<i ng-if="home.submittingForm" class="fa fa-spin fa-refresh"></i>
						</div>

						<div ng-cloak
