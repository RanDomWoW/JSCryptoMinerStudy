						$("#focus_8373 ul").stop(true,false).animate({"left":nowLeft},300);
						$("#focus_8373 .btn span").stop(true,false).animate({"opacity":"0.3"},300).eq(index).stop(true,false).animate({"opacity":"0.7"},300); //为当前的按钮切换到选中的效果
					}
				});
				
