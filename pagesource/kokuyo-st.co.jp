function smartRollover() {
	if(document.getElementsByTagName) {
		var images = document.getElementsByTagName("img");
		for(var i=0; i < images.length; i++) {
			if(images[i].getAttribute("src").match("_off."))
			{
				images[i].onmouseover = function() {
					this.setAttribute("src", this.getAttribute("src").replace("_off.", "_on."));
				}
				images[i].onmouseout = function() {
					this.setAttribute("src", this.getAttribute("src").replace("_on.", "_off."));
				}
			}
		}
	}
}
if(window.addEventListener) {
	window.addEventListener("load", smartRollover, false);
}
else if(window.attachEvent) {
	window.attachEvent("onload", smartRollover);
}
function loadCarendar(flag) {
	if (jQuery164(".bx-wrapper").size() > 0) {
		jQuery164(".bx-wrapper").remove();
		jQuery164(".slide-wrap").append('<ul id="slider1" class="multiple"></ul>');

		jQuery164('#cal_nav img:eq(0)').attr('src',jQuery164("#cal_nav img:eq(0)").attr("src").replace("_st", "_off"));
		jQuery164('#cal_nav img:eq(1)').attr('src',jQuery164("#cal_nav img:eq(1)").attr("src").replace("_st", "_off"));
		jQuery164('#cal_nav img:eq(2)').attr('src',jQuery164("#cal_nav img:eq(2)").attr("src").replace("_st", "_off"));
		jQuery164('#cal_nav img:eq(3)').attr('src',jQuery164("#cal_nav img:eq(3)").attr("src").replace("_st", "_off"));

		if (flag) {
			if (flag == 1) {
				jQuery164('#cal_nav img:eq(0)').attr('src',jQuery164("#cal_nav img:eq(0)").attr("src").replace("_on", "_st"));
			} else if (flag == 2) {
				jQuery164('#cal_nav img:eq(1)').attr('src',jQuery164("#cal_nav img:eq(1)").attr("src").replace("_on", "_st"));
			} else if (flag == 3) {
				jQuery164('#cal_nav img:eq(2)').attr('src',jQuery164("#cal_nav img:eq(2)").attr("src").replace("_on", "_st"));
			} else if (flag == 4) {
				jQuery164('#cal_nav img:eq(3)').attr('src',jQuery164("#cal_nav img:eq(3)").attr("src").replace("_on", "_st"));
			}
		}
	}

	jQuery164.ajax({
		type: "GET",
   		 scriptCharset: 'utf-8',

		url: "http://www.kokuyo.co.jp/calendar/calendar.json",
		dataType: "jsonp",
		jsonpCallback: "setCalendar",
		success: function(data2) {
			var sumcount = 0;
			jQuery164.each(data2, function(index, itemData) {
			 	var V_CATEGORY 		= itemData.CATEGORY;
			 	var V_START_YEAR		= itemData.START_YEAR;
			 	var V_START_DATE		= itemData.START_DATE;
			 	var V_START_WEEK		= itemData.START_WEEK;
			 	var V_END_DATE		= itemData.END_DATE;
			 	var V_END_WEEK		= itemData.END_WEEK;
			 	var V_NEWITEM_IMG_PATH	= itemData.NEWITEM_IMG_PATH;
			 	var V_TEXT_S 			= itemData.TEXT_S;
			 	var V_TEXT_L	 		= itemData.TEXT_L;
			 	var V_URL	 		= itemData.URL;
			 	var V_CO_FLAG 		= itemData.CO_FLAG;

			 	if ((!flag) || (flag==1 && V_CATEGORY == 1) || (flag==2 && V_CATEGORY == 2) || (flag==3 && V_CATEGORY == 3) || (flag==4 && V_CO_FLAG == 1)) {
			 		sumcount = sumcount + 1;
			 	}
			});


			var count = 0;
			var countFlag = '';
			var startSlide = 0;
			jQuery164.each(data2, function(index, itemData) {
			 	var V_CATEGORY 		= itemData.CATEGORY;
			 	var V_START_YEAR		= itemData.START_YEAR;
			 	var V_START_DATE		= itemData.START_DATE;
			 	var V_START_WEEK		= itemData.START_WEEK;
			 	var V_END_DATE		= itemData.END_DATE;
			 	var V_END_WEEK		= itemData.END_WEEK;
			 	var V_NEWITEM_IMG_PATH	= itemData.NEWITEM_IMG_PATH;
			 	var V_TEXT_S 			= itemData.TEXT_S;
			 	var V_TEXT_L	 		= itemData.TEXT_L;
			 	var V_URL	 		= itemData.URL;
			 	var V_CO_FLAG 		= itemData.CO_FLAG;

				var V_START_ARRAY = V_START_DATE.split("/");
				var today = new Date();
				var myDate=new Date();
				myDate.setFullYear(V_START_YEAR,V_START_ARRAY[0]-1,V_START_ARRAY[1]);



			 	if ((!flag) || (flag==1 && V_CATEGORY == 1) || (flag==2 && V_CATEGORY == 2) || (flag==3 && V_CATEGORY == 3) || (flag==4 && V_CO_FLAG == 1)) {

			 		if (!countFlag) {
				 		if (myDate<today) {
				 			countFlag = 1;

				 			if ((sumcount - 4)  <= (sumcount - count - 1)) {
				 				startSlide = sumcount - 4;
								if (startSlide < 0) {
									startSlide = 0;
								}
				 			} else {
				 				startSlide = sumcount - count - 1
				 			}

				 		} else {
				 			count = count+1;
				 		}
			 		}

					var HTML = '<li id="cal_item' + index + '" class="cal_category' + V_CATEGORY +'">';
					HTML += '<div class="cal-wrap">';
					HTML += '<div class="cal_ittl"><img src="/image/cal_ttl' + V_CATEGORY + '.gif"></div>';
					if (V_END_DATE) {
						HTML += '<div class="cal_date">' + V_START_DATE + '～' + V_END_DATE + '</div>';
					} else {
						HTML += '<div class="cal_date">' + V_START_DATE + '</div>';
					}
					if(V_URL){
						HTML += '<div class="cal_stext"><a href="' + encodeURI(V_URL) + '" target="_blank">' + V_TEXT_S + '</a></div>';
					}else{
						HTML += '<div class="cal_stext">' + V_TEXT_S + '</div>';
					}
					HTML += '</div>';
					HTML += '</li>';

					jQuery164("#slider1").prepend(HTML);


					switch (V_CATEGORY){
						case "1":
							// 新商品
							if (V_NEWITEM_IMG_PATH) {
								var V_contents = '<div class="balloon_wrap2"><div class="cal_l"><div class="cal_ico"><img src="/image/cal_ico' + V_CATEGORY + '.gif"></div>';
								V_contents += '<div class="cal_img"><img src="' + encodeURI(V_NEWITEM_IMG_PATH) + '"></div></div>';
								if (V_END_DATE) {
									V_contents += '<div class="cal_kikan"><span>' + V_START_DATE + '</span>（' + V_START_WEEK + '）～<span>' + V_END_DATE + '</span>（' + V_END_WEEK + '）</div>';
								} else {
									V_contents += '<div class="cal_kikan">発売日：<span>' + V_START_DATE + '</span>（' + V_START_WEEK + '）</div>';
								}
								if (V_TEXT_L) {
									V_contents += '<div class="cal_stext2">' + V_TEXT_L + '</div>';
								} else {
									V_contents += '<div class="cal_stext2">' + V_TEXT_S + '</div>';
								}
								if (V_URL) {
									V_contents += '<div class="cal_btn"><a href="' + encodeURI(V_URL) + '" target="_blank"><img src="/image/cal_btn' + V_CATEGORY + '.gif"></a></div>';
								}
								V_contents += '</div>';
							} else {
								var V_contents = '<div class="balloon_wrap"><div class="cal_ico"><img src="/image/cal_ico' + V_CATEGORY + '.gif"></div>';
								if (V_END_DATE) {
									V_contents += '<div class="cal_kikan"><span>' + V_START_DATE + '</span>（' + V_START_WEEK + '）～<span>' + V_END_DATE + '</span>（' + V_END_WEEK + '）</div>';
								} else {
									V_contents += '<div class="cal_kikan">発売日：<span>' + V_START_DATE + '</span>（' + V_START_WEEK + '）</div>';
								}
								if (V_TEXT_L) {
									V_contents += '<div class="cal_stext2">' + V_TEXT_L + '</div>';
								} else {
									V_contents += '<div class="cal_stext2">' + V_TEXT_S + '</div>';
								}
								if (V_URL) {
									V_contents += '<div class="cal_btn"><a href="' + encodeURI(V_URL) + '" target="_blank"><img src="/image/cal_btn' + V_CATEGORY + '.gif"></a></div>';
								}
								V_contents += '</div>';

							}

							jQuery164("#cal_item" + index + " .cal-wrap").balloon({ 
								classname: "balloon",
								contents: V_contents,
								css: {
									border:'solid 5px #FC7D81',
									backgroundColor: '#FC7D81'
								}
							});
							break;
						case "2":
							// イベント
							var V_contents = '<div class="balloon_wrap"><div class="cal_ico"><img src="/image/cal_ico' + V_CATEGORY + '.gif"></div>';
							if (V_END_DATE) {
								V_contents += '<div class="cal_kikan"><span>' + V_START_DATE + '</span>（' + V_START_WEEK + '）～<span>' + V_END_DATE + '</span>（' + V_END_WEEK + '）</div>';
							} else {
								V_contents += '<div class="cal_kikan"><span>' + V_START_DATE + '</span>（' + V_START_WEEK + '）</div>';
							}
							if (V_TEXT_L) {
								V_contents += '<div class="cal_stext2">' + V_TEXT_L + '</div>';
							} else {
								V_contents += '<div class="cal_stext2">' + V_TEXT_S + '</div>';
							}
							if (V_URL) {
								V_contents += '<div class="cal_btn"><a href="' + encodeURI(V_URL) + '" target="_blank"><img src="/image/cal_btn' + V_CATEGORY + '.gif"></a></div>';
							}
							V_contents += '</div>';

							jQuery164("#cal_item" + index + " .cal-wrap").balloon({ 
								classname: "balloon",
								contents: V_contents,
								css: {
									border:'solid 5px #85D50A',
									backgroundColor: '#85D50A'
								}
							});
							break;
						case "3":
							// キャンペーン
							var V_contents = '<div class="balloon_wrap"><div class="cal_ico"><img src="/image/cal_ico' + V_CATEGORY + '.gif"></div>';
							if (V_END_DATE) {
								V_contents += '<div class="cal_kikan">期間：<span>' + V_START_DATE + '</span>（' + V_START_WEEK + '）～<span>' + V_END_DATE + '</span>（' + V_END_WEEK + '）</div>';
							} else {
								V_contents += '<div class="cal_kikan">期間：<span>' + V_START_DATE + '</span>（' + V_START_WEEK + '）</div>';
							}
							if (V_TEXT_L) {
								V_contents += '<div class="cal_stext2">' + V_TEXT_L + '</div>';
							} else {
								V_contents += '<div class="cal_stext2">' + V_TEXT_S + '</div>';
							}
							if (V_URL) {
								V_contents += '<div class="cal_btn"><a href="' + encodeURI(V_URL) + '" target="_blank"><img src="/image/cal_btn' + V_CATEGORY + '.gif"></a></div>';
							}
							V_contents += '</div>';
							
							jQuery164("#cal_item" + index + " .cal-wrap").balloon({ 
								classname: "balloon",
								contents: V_contents,
								css: {
									border:'solid 5px #34AAF3',
									backgroundColor: '#34AAF3'
								}
							});
							break;
					}
				}
			 });

			myslider = jQuery164('#slider1').bxSlider({
				displaySlideQty: 4,
				moveSlideQty: 1,
				infiniteLoop: false,
				hideControlOnEnd: true
			});
			myslider.goToSlide(startSlide);
		},
		error: function() {
		}
	});
}
