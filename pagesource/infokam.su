var anons_current = -1;
var anons_length = 0;
$(document).ready(function(){
	$('#anons_right').hover(function(){
		$('#anons_right ul').fadeIn();
	},function(){

	});
	$('#anons_right ul').hover(function(){
	},function(){
		$(this).fadeOut();
