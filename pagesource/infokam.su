	anons_length = $('#anons_right ul li a').length;
	$('#scrl').bind('marquee', function() {
		if (anons_current>=0) $('#anons_right ul li a:eq('+(anons_current)+')').removeClass('current');
		anons_current = (anons_current+1)%anons_length;
		$('#anons_right ul li a:eq('+(anons_current)+')').addClass('current')
		var ar = $('#anons_right ul li a:eq('+(anons_current)+')');
		
		$('#scrl').attr('href',ar.attr('href')).text(ar.text());
		var sc = ar.parent('li').find('i').attr('class');
		$('#anons_right').removeClass('photo').removeClass('video')
		if (sc=='video') $('#anons_right').addClass('video');
		if (sc=='photo') $('#anons_right').addClass('photo');
		var ob = $(this);
		var tw = ob.width();
		var ww = ob.parent().width();
		ob.css({ left: ww });
		ob.animate({ left: -tw }, 20000, 'linear', function() {
			ob.trigger('marquee');
		});
	}).trigger('marquee');

	$('#scrlm').bind('marqueeleft', function() {
		var ob = $(this);
		var tw = ob.width();
		var ww = ob.parent().width();
		if (tw>ww) {
			ob.css({ left: 20 });
			ob.animate({ left: ww-tw-20 }, 7000, 'linear', function() {
				ob.trigger('marqueeright');
			});
		}
	}).bind('marqueeright', function() {
		var ob = $(this);
		var tw = ob.width();
		var ww = ob.parent().width();
		if ((parseInt(ob.css('left'))<0)) {
			ob.css({ left: ww-tw-20 });
			ob.animate({ left: 20 }, 7000, 'linear', function() {
				ob.trigger('marqueeleft');
			});
		}
	}).trigger('marqueeleft');
});
$(window).resize(function(){
	var ob = $('#scrlm');
	var tw = ob.width();
	var ww = ob.parent().width();
	if (tw>ww) ob.trigger('marqueeleft');
	var ob = $('#scrl');
	var tw = ob.width();
	var ww = ob.parent().width();
	if (tw>ww) ob.trigger('marqueeleft');

});
</script>
