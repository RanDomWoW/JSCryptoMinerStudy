<script>/*global  jQuery, setTimeout*/
'use strict';
var ss = ss || {};

ss.notificationBar = (function($) {
  var barSelector = '.ss-notification-bar',
    messageSelector = '.ss-notification-bar__message',
    isAnimating = false,
    isHovering = false;

  var init = function() {
    // determine enter and leave
    $(barSelector).on('mouseenter mouseleave', function(e) {
      // mouse has entered
      if (e.type === 'mouseenter') {
        // user is hovering over the notification
        isHovering = true;
        // return
        return;
      } else if (e.type === 'mouseleave') {
        // user has hovered away from the notification
        isHovering = false;
        // hide the notification bar
        if (!isAnimating) {
          // hide the notification bar
          hide();
        }
      }
    });
  };

  var show = function(message, delay) {
    // set the message
    $(messageSelector).html(message);
    // only animate if not animating
    if (!isAnimating) {
      isAnimating = true;
      // fade in notification
      $(barSelector).stop().fadeIn('slow', function() {
        // display the notification bar
        $(barSelector).removeClass('ss-notification-bar--hide').addClass('ss-notification-bar--show');

        isAnimating = false;
        // delay
        setTimeout(function() {
          if (!isHovering) {
            hide();
          }
        }, delay);
      });
    }
  };

  var hide = function() {
    // fade out
    $(barSelector).stop().fadeOut('slow', function() {
      isAnimating = false;
      // hide the notification bar
      $(this).removeClass('ss-notification-bar--show').addClass('ss-notification-bar--hide');
    });
  };

  // An object for getting and setting
  return {
    init: init,
    show: show,
    hide: hide
  };
})(jQuery);
</script>
<script>/*global  jQuery, window, SpecsaversFavourites, console*/
'use strict';
var ss = ss || {};

ss.favouritesIcon = (function($) {
  var favouritesIconSelector = '.ss-favourites-widget__icon',
    favoutitesIconSelectedStateSelector = 'ss-favourites-widget--selected',
    toolTipSelector = '.ss-favourites-widget__icon [data-toggle="tooltip"]',
    $favouriteIcons;

  var init = function() {

    if (!SpecsaversFavourites) {
      console.warn('ss.favouritesIcon: no SpecsaversFavourites global');
      return;
    }

    $favouriteIcons = $(favouritesIconSelector);

    $favouriteIcons.each(function() {
      updateIcons(this);
    });

    // click handler
    $favouriteIcons.click(function(e) {
      e.preventDefault();
      e.stopImmediatePropagation();
      selectFavourite($(this));
    });

    // breakpoints
    if ($(window).width() > 1023) {
      $(toolTipSelector).tooltip();
    }
  };

  function updateIcons(iconEl) {
    var $icon = $(iconEl);

    var skuI = getSku($icon);
    if (!skuI) return;

    var isFavourite = SpecsaversFavourites.isFavourite(skuI);

    renderIcon($icon, isFavourite);
    renderToolTip($icon, isFavourite);
  }

  function renderIcon($icon, isFavourite) {
    if (isFavourite) {
      $icon.stop().addClass(favoutitesIconSelectedStateSelector);
    } else {
      $icon.stop().removeClass(favoutitesIconSelectedStateSelector);
    }
  }

  function renderToolTip($icon, isFavourite) {
    var title;
    if (isFavourite) {
      title = $('.ss-favourites-helper__remove').text();
    } else {
      title = $('.ss-favourites-helper__add').text();
    }

    $icon
      .attr('data-original-title', title)
      .attr('title', title)
      .tooltip('fixTitle');
  }

  function showNotification(isFavourite) {
    if (isFavourite) {
      ss.notificationBar.show($('.ss-favourites-helper__add-notification').html(), 2000);
    } else {
      ss.notificationBar.show($('.ss-favourites-helper__remove-notification').html(), 2000);
    }
  }

  function getSku($icon) {
    var skuI = parseInt($icon.attr('data-sku'), 10);
    if (isNaN(skuI) || skuI === 0) {
      console.warn('ss.favouritesIcon: sku error');
      return false;
    } else
      return skuI;
  }

  function selectFavourite($icon) {
    var skuI = getSku($icon);
    if (!skuI) return;

    var isFavourite = SpecsaversFavourites.toggleFavourite(skuI);

    renderIcon($icon, isFavourite);
    renderToolTip($icon, isFavourite);
    showNotification(isFavourite);

    $icon.tooltip('show');
  }

  // public props
  return {
    init: init,
    updateIcons: updateIcons
  };
})(jQuery);
</script>
