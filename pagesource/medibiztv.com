    /************news letter subscription************/
    function newssubscribe() {
        var email = $.trim($("#email").val());
        // alert(email);
        if (email == "") {
            inlineMsg('email', 'Enter email.', 2);
            return false;
        }
        else {
            var atpos = email.indexOf("@");
            var dotpos = email.lastIndexOf(".");
            if (atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= email.length)
            {
                inlineMsg('email', 'Please enter a valid email address.', 2);
                return false;
            }
        }
        var data = "email=" + email;
        result = $.ajax({
            url: livesite + "content/newsletter",
            type: "POST",
            data: data,
            async: false
        }).responseText;//alert(result);
        if (result == 'existing')
        {
            inlineMsg('email', 'Already Registered User.', 2);
            $("#email").val('');
//             $('#email').jAlert('noo', "failed");
            return false;
        }
        if (result == 'success')
        {

            ///$("#submsg" ).html('Thanks For Subscribe Our NewsLetter');
            //inlineMsg('submsg','Thnks 4 contacting us.',2);
            $("#email").val('');

            $('#email').jAlert('Thanks for Subscribing Our NewsLetter', "success");
            // jAlert('', 'Thanks for Subscribing Our NewsLetter');
            return false;
        }

    }

    /****************news letter subscription end******************/
    function submitenter(myfield, e)
    {
        var keycode;

        if (window.event)
            keycode = window.event.keyCode;
        else if (e)
            keycode = e.which;
        else
            return true;

        if (keycode == 13)
        {
            loginValidate();
            return false;
        }
        else
            return true;
    }

