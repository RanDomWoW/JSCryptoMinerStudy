    function afterRegister(data){
        if (data.done) {
            alert('Для подтверждения регистрации, проверьте почту');
            window.location.href='/profile';
        } else {
            $(".input-description").hide();
			$(".input-wrapper.error").removeClass("error");
			$.each(data, function(key, value) {
				var div = "#"+key+"_em_";
				$(div).text(value);
				$(div).closest(".input-wrapper").addClass("error");
				$(div).show();
			});
        }
    }
