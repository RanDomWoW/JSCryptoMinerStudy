				'ecommerce': {'impressions': impressions}
			});
			// 送信データの初期化
			impressions = [];
		} else {
			// ga_impressionにデータ格納
			var addData ={
				'id': item[0].replace( /\'/g , "" ),
				'name': item[1].replace( /\'/g , "" ).substring(0, 15),
				'list': page_name.replace( /\'/g , "" ),
				'position':i+1};
			impressions.push(addData);
		}
	}
	// ga_impressionの発火(未発火のデータがある場合のみ)
	if (impressions.length > 0) {
		dataLayer.push({
			event: 'ga_impression',
			'ecommerce': {'impressions': impressions}
		});
	}
});

// 追記読みの処理(20単位毎で処理する)
function ga_search_send(){
	setTimeout(function(){
		var num = Math.ceil($('[name=ga_item]').length/20);
		if(num!=1){
			var impressions = [];
			for(var i=$('[name=ga_item]').length; i>$('[name=ga_item]').length-20; i--){
				var item = $('[name=ga_item]')[i-1].value.split(",");
				// ga_impressionにデータ格納
				var addData ={
					'id': item[0].replace( /\'/g , "" ),
					'name': item[1].replace( /\'/g , "" ).substring(0, 30),
					'list': page_name.replace( /\'/g , "" ),
					'position':i};
				impressions.push(addData);
			}
			// ga_impressionの発火
			dataLayer.push({
				event: 'ga_impression',
				'ecommerce': {'impressions': impressions}
			});
		}
	},1000);
}

// 商品クリックをGAに送信する
$(document).on('click', 'div a', function() {
	val = $(this).attr("href");
	if(val!=null && val.match(/\/i\/vv_/)){
		for(var i=0; i<$('[name=ga_item]').length; i++){
			if(val.split("/i/vv_")[1].split("/")[0]==$('[name=ga_item]')[i].id){
				ga_product_click(i)
				i = $('[name=ga_item]').length;
			}
		}
	}else if(val!=null && val.match(/\?skuId=/)){
		for(var i=0; i<$('[name=ga_item]').length; i++){
			if(val.split("?skuId=")[1]==$('[name=ga_item]')[i].id){
				ga_product_click(i)
				i = $('[name=ga_item]').length;
			}
		}
	}
});

//ga_product_click データ格納 & 発火
function ga_product_click(i){
	var item = $('[name=ga_item]')[i].value.split(",");
	dataLayer.push({
		'event': 'ga_product_click',
		'ecommerce': {
			'click': {
				'actionField': {'list': page_name.replace( /\'/g , "" )},
				'products': [{
					'id': item[0].replace( /\'/g , "" ),
					'name': item[1].replace( /\'/g , "" ),
					'position':i+1 
				}]
			}
		}
	});
}

// SPtopページ商品クリック用GAデータ格納 & 発火（/smartphone/js/top_recommend.js）
function top_item_ga_product_click(item,i){
	dataLayer.push({
		'event': 'ga_product_click',
		'ecommerce': {
			'click': {
				'actionField': {'list': page_name.replace( /\'/g , "" )},
				'products': [{
					'id': item[0].replace( /\'/g , "" ),
					'name': item[1].replace( /\'/g , "" ),
					'position':i+1 
				}]
			}
		}
	});
}

