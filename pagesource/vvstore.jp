<script>TLAB.UsertrackTrackingRecord.create()</script>
<script>
$(function(){
	var searchArea = $("#searchArea");
	searchArea.find('.js-input-form').submit(function(e){
		e.preventDefault();
		gotoSearch();
	});
	var suggestlist = searchArea.find('.suggestlist');
	var suggestDiv = searchArea.find('.js-suggest');
	var recentKeyword = searchArea.find('.js-recent');
	var needRecommendRecentKeyword = true;
	searchArea.find('.js-input-form .formarea')
	.keyup(function(){
		var word = this.value;
		if(word){
			$.ajax({
				url: TLAB.apiBaseUrl + "/suggest.json?user_id="+encodeURIComponent("")+"&tracking_id="+TLAB.UsertrackTrackingRecord.create()+"&word="+encodeURIComponent(word)+"&sf=item_name&sf=keyword&method=both&rf=a7d&limit=5&mode=partial&fq=+ds:1&lf=item_name&approx=true",
				success:function(x){
					if(x && x.result){
						var dom = [];
						suggestlist.html("");
						for(var i=0;i<x.result.words.length;i++){
							var li = $('<li><a href=""><span class="word"></span></a></li>');
							li.find(".word").text(x.result.words[i].value);
							li.find("a").attr("href", "/s/search/q="+encodeURIComponent(safeWord(x.result.words[i].value))+"/?from=suggest");
							suggestlist.append(li);
						}
						delete recent_keydown;
						delete suggest_keydown;
						suggestDiv.show("fast" ,
						function(){
							if(suggestDiv.find('ul > li > a').length>0){
								suggest_keydown = key_decision(suggestDiv.find('ul > li > a'));
							}
						});
						recentKeyword.hide();
					}
				}
			});
		}else{
			
			(new TLAB.Recommend("recommend-recent-keyword", "", "", 29, {
			    param : "utm_medium=recommend" 
			})).sendRequest();
			delete recent_keydown;
			delete suggest_keydown;
			suggestDiv.hide();
			recentKeyword.show("fast" ,
				function(){
					if(recentKeyword.find('ul > li > a').length>0){
						recent_keydown = key_decision(recentKeyword.find('ul > li > a'));
					}
				}
			)
		}
	}).click(function(){
		
		(new TLAB.Recommend("recommend-recent-keyword", "", "", 29, {
		    param : "utm_medium=recommend" 
		})).sendRequest();
		delete recent_keydown;
		delete suggest_keydown;
		suggestDiv.hide();
		recentKeyword.show("fast" ,
			function(){
				if(recentKeyword.find('ul > li > a').length>0){
					recent_keydown = key_decision(recentKeyword.find('ul > li > a'));
				}
			}
		)
	});
	
	function safeWord(word){
		return word.trim().replace(/\//g, "／").replace(/\\/g, " ");
	}
	function gotoSearch(){
		var word = safeWord(searchArea.find('.js-input-form').find("input[type=text]").val());
		if(word == ""){
			return;
		}
		var cate = searchArea.find(".js-searchArea-cate").val();
		url="/s/search/q="+encodeURIComponent(word)+(cate && ("/c"+cate))+"/";
		window.location = url;
	}
});
var key_decision = function(elm){
	var win = $("#searchArea"),elm_arr = [],fcs_num = 0,fcs_flg = [];
	elm.each(function(indx){
		var num = indx;
		elm_arr[indx] = $(this);
		fcs_flg[indx] = false;
		$(this).focusin(function(){
			fcs_num = num;
			if( fcs_flg[indx] === false ){fcs_flg[indx] = true;}
			fcs_flg[indx] = true;
		}).blur(function(){
			fcs_flg[indx] = false;
			if( fcs_flg.indexOf(true) < 0 ) {fcs_num = 0;}
		});
	});
	win.keydown(function(evnt){
		if( evnt.which == 38 ){
			if( fcs_flg.indexOf(true) < 0 ) {
				elm_arr[0].focus();
			} else if( fcs_num > 0 ){
				elm_arr[fcs_num - 1].focus();
			}
			evnt.preventDefault();
			evnt.stopPropagation();
		}else if( evnt.which == 40 ){
			if( fcs_flg.indexOf(true) < 0 ) {
				elm_arr[0].focus();
			} else if( fcs_num < elm_arr.length - 1 ){
				elm_arr[fcs_num + 1].focus();
			}
			evnt.preventDefault();
			evnt.stopPropagation();
		}else if( evnt.which == 13 ){
			
		}else{
			win.find('.formarea').focus();
		}
	});
};
$(window).click(function(){
	if(document.activeElement.tagName!="INPUT"){
		setTimeout(function(){
			delete recent_keydown;
			delete suggest_keydown;
			$('.js-suggest').hide();
			$('.js-recent').hide();
		},300);
	}
})

</script>
