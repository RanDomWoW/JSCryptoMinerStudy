<script type="text/javascript">
	var domainParameterCart = "https://ajinomoto-direct.shop";

	$(function() {
		// ログイン情報
		$.ajax({
			url : domainParameterCart + "/ajax/userInfoAjax.jsp",
			type : "GET",
			dataType : "jsonp",
			jsonpCallback : "callbackUserInfo"
		}).done(function(json, status, xhr) {
			if (json.status == "SUCCESS") {
				$("#userKanjinamesei").text(json.kanjinamesei);
				$("#userKanjinamemei").text(json.kanjinamemei);
				$("#sessionId").text(json.sessionId);
			} else if (json.status == "ERROR") {
			}
			displayLoginArea();
		}).fail(function(xhr, status, error) {
		});

		// カート情報
		$
				.ajax({
					url : domainParameterCart + "/ajax/headCartJsonpAjax.jsp",
					type : "GET",
					dataType : "jsonp",
					jsonpCallback : "callbackHeadCart"
				})
				.done(
						function(json, status, xhr) {
							if (json.status == "SUCCESS") {
								$("#head_number").append(json.numberString);
								$("#head_total").append(json.totalString);
								$("#curCart").val(json.curCart);
								if (document.getElementById("cartUrlLogedIn") != null) {
									document.getElementById("cartUrlLogedIn").href = domainParameterCart
											+ json.cartUrlLogedIn;
								}
								if (document
										.getElementById("cartUrlNotLogedIn") != null) {
									document
											.getElementById("cartUrlNotLogedIn").href = domainParameterCart
											+ json.cartUrlNotLogedIn;
								}
							} else if (json.status == "FAILED") {
							}
						}).fail(function(xhr, status, error) {
				});
	});
	function displayLoginArea() {
		// 姓が無い時は未ログインとする
		if ($("#userKanjinamesei").text() == '') {
			displayLoginWrap();
		} else {
			displayLoginWrapAfter();
		}
	}

	function displayLoginWrapAfter() {
		$("div.loginWrap").hide();
		$("div.loginWrapAfter").show();
	}
	function displayLoginWrap() {
		$("div.loginWrapAfter").hide();
		$("div.loginWrap").show();
	}
</script>



