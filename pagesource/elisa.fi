    <script type="text/javascript">
      var ElisaNavi = {
        activeSegment: 'yksityisille',
        showSearch: true,
        showGlobalFooter:  true, 
        enableOldGlobalFooter: false,
        showLocalFooter:false
      }
      
      $(document).ready(function() {
          var hashLocation = window.location.hash;
          if(hashLocation) {
              var locationPath = hashLocation.replace(/#/, '')
              var multipleTags = locationPath.split('/');
              multipleTags.forEach(adjustPagePositionToTag)
          }
          function adjustPagePositionToTag(tag){
              var $target = $('#'+tag + ', [name = ' + tag + ']')
              if ($target.offset() !== undefined) {
                  window.setTimeout(function () {
                      var targetOffset = (!$('html').hasClass('ie9')) ? ($target.offset().top - 65) : ($target.offset().top - 70);
                      $('body, html').animate({scrollTop: targetOffset})
                  }, 100)
              }
          }
      })
    </script>
    
