  <script type="text/javascript" src="//static.criteo.net/js/ld/ld.js" async="true"></script>
  <script type="text/javascript">
  var label_ville = "Ville, département, code postal";
  $("#fh5co-preparer").backstretch("/img/homepage-photo-2.jpg"),
  $("#fh5co-trouver").backstretch("/img/homepage-photo-3.jpg"),
  $("#fh5co-realiser").backstretch("/img/homepage-photo-4.jpg"),
  $("#fh5co-accompagner").backstretch("/img/homepage-photo-5.jpg");

  window.criteo_q = window.criteo_q || [];

  $(document).ready(function () {

    $('.fake-select').fakeSelect({
      btnSize: 'btn-lg',
      btnStyle: 'btn-clean'
    });

    $('input').keypress(function (event) {
      if (event.keyCode == 13) {
        if (datas != null && datas[0] != null) {
          $('#search_id').val(datas[0].value);
          $('#search').val(datas[0].label);
          label_ville = datas[0].label;
        }
      }
    });

    $('#search').focus(function () {
      if ($('#search').val() == label_ville)
        $('#search').val('');
    });

    $('#search').blur(function () {
      $('#search').val(label_ville);
    });

    $('#search').autocomplete({
      delay: 200,
      minLength: 2,
      source: function (request, response) {
        $.ajax({
          url: '/geolocalisation-bien-agence.ashx',
          dataType: 'json',
          data: { str: $('#search').val() },
          success: function (data) {
            datas = data;
            if (datas[0] != null) {
              $('#search_id').val(datas[0].value);
              label_ville = datas[0].label;
            }
            else {
              $('#search_id').val("");
              label_ville = "Ville, département, code postal";
            }
            response(data);
          }
        });
      },
      select: function (event, ui) {
        if (event.keyCode == 13) {
          $(this).next("input").focus().select();
        }

        $('#search_id').val(ui.item.value);
        $('#search').val(ui.item.label);
        label_ville = ui.item.label;
        $('#search').autocomplete('close').blur();

        return false;
      },
      focus: function (event, ui) {
        $('#search').val(ui.item.label);
        //event.preventDefault();
        return false;
      }
