            $(this).html($('.form-wrap>span:nth-of-type(1)>span:nth-of-type(1)').html()).attr('data-name', $('.form-wrap>span:nth-of-type(1)>span:nth-of-type(1)').attr('data-name')).attr('data-placeholder', $('.form-wrap>span:nth-of-type(1)>span:nth-of-type(1)').attr('data-placeholder'))
            $('.form-wrap>span:nth-of-type(1)>span:nth-of-type(1)').html(tempH).attr('data-name', tempN).attr('data-placeholder', tempP)
            $('.form-wrap>span:nth-of-type(3) input').attr('placeholder', tempP).attr('name', tempN).val('').focus()
        })
        $(".form-wrap>span:nth-of-type(3) input").on('input', function () {
            if ($(this).attr('name') == 'barcode') {
                if (!/^\d*$/.test($(this).val())) {
                    alert('条形码为13位纯数字，请输入正确的条形码！')
                    $(this).val('')
                }
            }
        })

        function initTextAlign() {
            $('.book li p span').each(function () {
                if ($(this).height() === 16) {
                    $(this).parent('p').css('text-align', 'center');
                } else {
                    $(this).parent('p').css('text-align', 'left');
                }
            })
        }

        $(function () {
            $(".form-wrap>span:nth-of-type(3) input").keypress(function (e) {
                if ($(this).attr('name') == 'barcode') {
                    if (e.keyCode == 13) {
                        if (($(".form-wrap>span:nth-of-type(3) input").val()) != '') {
                            if (!/^\d{13}$/.test($(".form-wrap>span:nth-of-type(3) input").val())) {
                                alert('条形码为13位纯数字，请输入正确的条形码！')
                                $(".form-wrap>span:nth-of-type(3) input").focus()
                                return false;
                            }
                        } else {
                            alert('条形码为13位纯数字，请输入正确的条形码！')
                        }
                    }
                }
            });
            initTextAlign();
            showAd();
        });

        $(".form-wrap>span:nth-of-type(4)").click(function (e) {
            e.preventDefault()
            if ($(".form-wrap>span:nth-of-type(3) input").attr('name') == 'barcode') {
                if (($(".form-wrap>span:nth-of-type(3) input").val()) != '') {
                    if (!/^\d{13}$/.test($(".form-wrap>span:nth-of-type(3) input").val())) {
                        alert('条形码为13位纯数字，请输入正确的条形码！')
                        $(".form-wrap>span:nth-of-type(3) input").focus()
                        return false;
                    } else {
                        window.location.href = 'http://www.hdzuoye.com/booklist?barcode=' + $(".form-wrap>span:nth-of-type(3) input").val()
                    }
                } else {
                    alert('条形码为13位纯数字，请输入正确的条形码！')
                }
            } else {
                if (($(".form-wrap>span:nth-of-type(3) input").val()) == '') {
                    alert('关键字不能为空！')
                } else {
                    window.location.href = 'http://www.hdzuoye.com/booklist?keyword=' + $(".form-wrap>span:nth-of-type(3) input").val()
                }
            }
        });

        function stop() {
            return false;
        }

        document.oncontextmenu = stop;

        function log(target) {
            $.ajax({
                url: 'down.php?from=app',
                success: function (data) {
                    // alert(data);
                }
            });
        }

        function showAd() {
            $(".advertisement").show();
        }

    </script>
</body>

</html>