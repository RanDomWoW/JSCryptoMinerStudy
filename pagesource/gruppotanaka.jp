<script>
$(function(){
	$(window).on('load resize orientationchange',function(){
		if($(window).width()>641){	
			var dh = $(document).height();
			$('#wrapper').css({'height':dh,'overflow':'hidden'});

			$('.imageSlider ul').slick({
  			dots: true,
  			infinite: true,
  			speed: 500,
  			fade: true,
  			cssEase: 'linear',
				slidesToShow: 1,
				adaptiveHeight: true,
				autoplay: true,
				arrows:false
			});
			
			var wh = $(window).height();
			$('section.about div').wait(3000).animate({opacity:1},1000);
			$('.mask').wait(1000).animate({top:wh},1000);	
			$(window).on('scroll load',function (){
				var scrlH = $(window).scrollTop();
				var winH = $(window).height();
				var dh = $(document).height();
				$('.mask').animate({'top':winH+scrlH},20);
				if(scrlH > dh-1400){
					$('.mask').remove();
				}
			});		
			$(window).on('scroll',function (){
				$('.curtain').each(function(){
					var elmPos = $(this).offset().top;
					var elmH = $(this).height();
					var scrlH = $(window).scrollTop();
					var winH = $(window).height();
					if (scrlH > elmPos - winH + 350){
						$(this).wait(700).animate({'opacity':1},2100);
					}
				});
			});
			$('.brand figure').perfectScrollbar('destroy');
	    $('html,body').animate({ scrollTop: 0 }, '1');
		}else{
			$('.brand figure ul').each(function(){
				var count = $(this).children('li').length; 
				var itemW = $(this).find('img').width()+15;
				$(this).css('width',count*itemW+15) 
      });
			$('.brand figure').perfectScrollbar({
				suppressScrollY: true	
			});
		}
		$('.imageSlider > ul > li').matchHeight();		
	});
});
</script>
