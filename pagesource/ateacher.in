.cutelabel{
margin:0;
padding:0;
position:relative;
}
.cutelabel a{
float:left;
height:24px;
line-height:24px;
position:relative;
font-size:12px;
margin-bottom: 1px;
margin-left:20px;
padding:0 10px 0 12px;
background:#A6BE8E;
color:#000;
text-decoration:none;
-moz-border-radius-bottomright:4px;
-webkit-border-bottom-right-radius:4px;
border-bottom-right-radius:4px;
-moz-border-radius-topright:4px;
-webkit-border-top-right-radius:4px;
border-top-right-radius:4px;
}
.cutelabel a:before{
content:"";
float:left;
position:absolute;
top:0;
left:-12px;
width:0;
height:0;
border-color:transparent #A6BE8E transparent transparent;
border-style:solid;
border-width:12px 12px 12px 0;
}
.cutelabel a:after{
content:"";
position:absolute;
top:10px;left:0;
float:left;
width:4px;
height:4px;
-moz-border-radius:2px;
-webkit-border-radius:2px; border-radius:2px;
background:#fff;
-moz-box-shadow:-1px -1px 2px #004977;
-webkit-box-shadow:-1px -1px 2px #004977;
box-shadow:-1px -1px 2px #004977;
}
.cutelabel a:hover{background:#0060AF;}
.cutelabel a:hover:before{border-color:transparent #0060AF transparent transparent;}
