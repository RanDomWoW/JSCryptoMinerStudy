					function linkKeyword(keyword){
						keyword=$.trim(keyword);
						if (keyword!=""){
							queryKeyword(keyword,true);
						}
					}
					
					function queryKeyword(keyword,isTag){
						$.ajax({
						  type: "POST",
						  url: "/module/filter.php",
						  cache: false,
						  data: { 
								  keyword: keyword
								}
						}).done(function( htmlData ) { 
							if (isTag){
							    window.location='/tw/news/index.php?tag=' + encodeURIComponent(keyword);
							}else{
								window.location='/tw/news/index.php?keyword=' + encodeURIComponent(keyword);
							}
						});	
					}
