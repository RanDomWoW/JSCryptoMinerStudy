    var url = "";

    var className = "";

    var reg = /^1(3|4|5|7|8)\d{9}$/;

    var isVerify =false;
    var timedown = function () {
        $('input').css('border', '1px solid #d3d3d3');
        reg = /^1\d{10}$/;
        if ($('.mobilephone').val() == '') {
            $('.mobilephone').next().text('请输入手机号').fadeIn(100);
            $('.mobilephone').parents('.inputElement').addClass('error');
            return;
        } else if (!reg.test($('.mobilephone').val())) {
            $('.mobilephone').next().text('请输入正确的手机号').fadeIn(100);
            $('.mobilephone').parents('.inputElement').addClass('error');
            return;
        } else if ($('input[name=verifyCodeImg]').val() == '' && !isVerify) {
            $('.verify-code').parent().next().text('图形验证码不能为空').fadeIn();
            return;
        } else {
            var temp = 60;
            kaptcha =  $('input[name=verifyCodeImg]').val();
            $('.verofyCode-img').next().fadeOut(100);
            $('.mobilephone').next().fadeOut(100)
            $.ajax({
                url: url,  //放入json地址
                type: 'POST',
                dataType: "json",
                data: {
                    phone: $('.mobilephone').val(),
                    kaptcha: kaptcha,
                    type:"login"
                },
                success: function (data) {
                    console.log(data)
                    if (data.code != 200) {
                        if (data.code == 100674) {
                            $(".kaptcha").click();
                            $('input[name=verifyCodeImg]').val("").focus();
                            $('.verify-code').parent().next().text(data.message).fadeIn(100);
                        } else {
                            $('.mobilephone').next().text(data.message).fadeIn(100);
                        }
                        temp = 0;
                    } else {
                        $('.verofyCode-img').next().fadeOut(10).text('');
                        $("input[name=verifyCode]").attr("placeholder", "请输入短信验证码");
                        $('input[name=verifyCode]').css('border', '1px solid #d3d3d3').val("");
                    }
                }
            });
            /*处理登录,验证码计时*/
            $('input[name=verifyCodeImg]').attr('disabled', false);
            $('.mobile').next().text('').fadeOut(100);
            $('.'+className).css({backgroundColor: '#888888'});
            $('.'+className).text('60秒后重试');
            $('.'+className).unbind();
            var timeline = setInterval(function () {
                if (temp <= 1) {
                    clearInterval(timeline);
                    $('.'+className).css({
                        backgroundColor: '#3a5795',
                    }).text('重新获取');
                    $('.'+className).bind('click', function () {
                        timedown();
                    });
                } else {
                    temp--;
                    $('.'+className).text(temp + '秒后重试');
                }
            }, 1000);
            /*处理登录,验证码计时*/
        }
    }

