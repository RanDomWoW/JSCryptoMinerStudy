        // Quote Changer Code

        var currentQuote = 1;
        var max = $(".quote-content").length + 1;

        $(".quote-content").hide();
        $(".quote-content:nth-child(1)").fadeIn("slow");

        //function to change to next quote
        function changeNext() {
            $(".quote-content").hide();
            currentQuote += 1;
            if (currentQuote === max) {
                currentQuote = 1;
            }
            $(".quote-content:nth-child(" + currentQuote + ")").fadeIn("slow");
        }

        function changeBack() {
            $(".quote-content").hide();
            currentQuote -= 1;

            if (currentQuote === 0) {
                currentQuote = max - 1;
            }

            $(".quote-content:nth-child(" + currentQuote + ")").fadeIn("slow");
        }

        // startChange();

        $("#prevQuote").click(function () {
            // stopChange();
            changeBack();
            // startChange();
        });

        $("#nextQuote").click(function () {
            // stopChange();
            changeNext();
            // startChange();
        });

