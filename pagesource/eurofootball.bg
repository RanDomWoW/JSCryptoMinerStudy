<script>

    $(document).ready(function(){
        getSJP();
    });
		
    function getSJP(){
        $.ajax({
            type: "GET",
            url: "http://www.eurofootball.bg/sjp/new_system_jack_2012.xml",
            dataType: "xml",
            success: function(xml){
              $(xml).find('baner').each(function(){
									link = "";
									link2 = $(this).attr('link');
									link2 = $(this).attr('link').substring(45,51);
									
									
									image_btn = "";
									number = $(this).attr('link').substring(49,51);
									
									if($(this).attr('type') == "sdp13") image_btn="btn_sjp_13-"+number+".png";
									else if($(this).attr('type') == "sdp10") image_btn="btn_sjp_10-"+number+".png";		
											 else if($(this).attr('type') == "sdp15") image_btn="btn_sjp_15-1.png";
									var divButtons = document.getElementById('22');
									if(divButtons != null){
									divButtons.innerHTML = divButtons.innerHTML + '<a href="fixtures.php#c'+link2+'" class="frontNav" id="" style=""><img src="images/buttons/'+image_btn+'" alt=""   />	</a>';
								  }			
									/*
									if($(this).attr('type')=="sdp10") {link="csjp_6";  $('#sjp10_button').css('display', 'block');
									}else if($(this).attr('type')=="sdp13"){ link="csjp_2"; $('#sjp13_button').css('display', 'block');}
									*/
									
                  day_type = $(this).attr('value');
                  $("#carouselHolder").append(
                    '<div class="item">'+
              				'<a href="fixtures.php#c'+link2+'">'+
                				'<div class="image">'+
              				      '<img src="img/'+$(this).attr('type')+'.jpg" alt="" />'+
              				      '<div class="info">'+
                				      '<ul>'+
                  				      '<li class="date" style="margin-right: 0px;">'+
                  				          'Познай всички срещи и спечели '+$(this).attr('value')+' лв.'+
                  				      '</li>'+
                				      '</ul>'+
              				      '</div>'+
                				'</div>'+
              				'</a>'+
            				'</div>'
            			
                  );
              });
              //Carousel
              	$(".owl-carousel").owlCarousel();

               	var owl = $(".owl-carousel");

              	owl.owlCarousel({
              	transitionStyle : "fade"
              	});

            },
            error: function(err) {
                $(".owl-carousel").owlCarousel();

                var owl = $(".owl-carousel");

                owl.owlCarousel({
                transitionStyle : "fade"
                });
                return;
            }
        });
    }
</script>
