<script>

			$(document).ready(function(){
					getEvents();
			});
			function getEvents(){
			    $.ajax({
			        type: "GET",
			        url: "http://www.eurofootball.bg/crons/site_baner/xmls/site_xml_back/site_events.xml",
			        dataType: "xml",
			        success: function(xml){
								var eventId = $(xml).find('event_single:first').attr('event_id');
								var match = $(xml).find('event_single:first').attr('home_team')+'<br>-<br>'+$(xml).find('event_single:first').attr('away_team');

								var date_arr = $(xml).find('event_single:first').attr('event_date').split(".");
								var time = date_arr[2]+'/'+date_arr[1]+'/'+date_arr[0]+' '+$(xml).find('event_single:first').attr('event_time');

								$("#soonCountdownTitle").html(match);
								$("#soonCountdownTime").data( "countdown", time );
								$("#soonCountdownGame").html($(xml).find('event_single:first').attr('kr_izh'));
								$("#soonCountdownOdds").html("1 - <font color='yellow'>"+ $(xml).find('event_single:first').attr('kr1') +"</font>" +
								"   " + "X - <font color='yellow'>"+ $(xml).find('event_single:first').attr('krX') +"</font>"+
								"   "+"2 - <font color='yellow'>"+ $(xml).find('event_single:first').attr('kr2') +"</font>");

								//Countdown
								 $('[data-countdown]').each(function() {
										 var $this = $(this), finalDate = $(this).data('countdown');
										 $this.countdown(finalDate, function(event) {
												 var today = new Date();
												 var dd = today.getDate();
												 var dd2 = today.getHours();

												 var fday = finalDate.substr(8, 2);
												 var fhours = finalDate.substr(11, 2);

												 var diff = fday - dd;
												// alert(fhours);
												 if (diff>1){
												 $this.html(event.strftime(
													'<div>%D<span>Дни</span></div><div>%H<span>часове</span></div><div>%M<span>мин</span></div>'
													));
												 } else if(diff==0 || fhours - dd2 < 0){
																	$this.html(event.strftime(
																		'<div>%H<span>часа</span></div><div>%M<span>мин</span></div>'
																		));
																}else if(diff==1){
																					$this.html(event.strftime(
																						'<div>%D<span>ден</span></div><div>%H<span>часа</span></div><div>%M<span>мин</span></div>'
																						));
																				}
										 });
								});

									$("#soonCountdownStart").show();
									$("#soonCountdown").click(function(){
											window.location.href = 'http://eurofootball.bg/fixtures.php#e'+eventId;
									})
			        },
			        error: function(err) {
								console.log(err)
			            return;
			        }
			    });
			}
	</script>
