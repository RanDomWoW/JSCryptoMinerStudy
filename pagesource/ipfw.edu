  document.forms["grad-chat"].submit();
  return false;
 }else if (ugMajor == null || ugMajor == "") {
  document.forms["grad-chat"].submit();
  return false;
 }else if (gArea == null || gArea == "") {
  document.forms["grad-chat"].submit();
  return false;
 }else{
  pixelSubmit();
    
 }  
  document.forms["grad-chat"].submit();
}

 
