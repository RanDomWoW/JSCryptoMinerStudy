      <script type="text/javascript" async>
          $scrollEasing = 600;
          $scrollEasingType = "linear";
          $rotationSpeed = 7000;
          $bgimg = $("#HMbgimg");
          $img_title = $("#HMimg_title");
          $(window).load(function () {
              var firstPhoto = "https://www.amli.com/AMLIcontent/files/home/PHCourtyard Eve 01.jpg";
              var the1StImg = new Image();
              the1StImg.onload = CreateDelegate(the1StImg, theNewImg_onload);
              the1StImg.src = firstPhoto;
              the1StImg.alt = titleArray[jQuery.inArray(firstPhoto, photoArray)];
              startPhotos();
          });

          function BackgroundLoad($this, imageWidth, imageHeight, imgSrc, imgTitle) {
              $this.fadeOut($scrollEasing, $scrollEasingType, function () {
                  $this.attr("src", "").attr("src", imgSrc); //change image source
                  $this.fadeIn($scrollEasing, $scrollEasingType);
                  if (imgTitle) {
                      $this.attr("alt", altTagArray[jQuery.inArray(imgSrc, photoArray)]);  
                      //If you load a title, it will display a tooltip on hover.  .attr("title", imgTitle);
                      $img_title.fadeOut($scrollEasing, $scrollEasingType, function () {
                          $img_title.html(imgTitle).fadeIn($scrollEasing, $scrollEasingType);
                      });
                  } else {
                      $img_title.fadeOut($scrollEasing, $scrollEasingType, function () {
                          $img_title.html($this.attr("alt")).fadeIn($scrollEasing, $scrollEasingType);
                      });
                  }
              });
          }

          //Array containing the photos to display on the home page.
          var photoArray = ["https://www.amli.com/AMLIcontent/files/home/south-shore-austin.jpg","https://www.amli.com/AMLIcontent/files/home/AMLI_SAWGRASS-00342_Homepage.jpg","https://www.amli.com/AMLIcontent/files/home/AMLI 2121_026.jpg","https://www.amli.com/AMLIcontent/files/home/UptownOrange.jpg","https://www.amli.com/AMLIcontent/files/home/SLU_211_Homepage.jpg","https://www.amli.com/AMLIcontent/files/home/PHCourtyard Eve 01.jpg","https://www.amli.com/AMLIcontent/files/home/AMLIRiverNorthRooftop_Homepage.jpg"];
          var titleArray = ["<a href='https://www.amli.com/community.aspx?regiontag=austin&propertytag=southshore'>Visit AMLI South Shore - Austin</a>","<a href='https://www.amli.com/community.aspx?regiontag=Florida&propertytag=sawgrassvillage'>Visit AMLI Sawgrass Village - Sunrise, FL</a>","<a href='https://www.amli.com/community.aspx?regiontag=houston&propertytag=2121'>Visit AMLI 2121 - Houston</a>","<a href='https://www.amli.com/community.aspx?regiontag=southcal&propertytag=uptownorange'>AMLI Uptown Orange - Southern California </a>","<a href='https://www.amli.com/community.aspx?regiontag=seattle&propertytag=southlakeunion'>Visit AMLI South Lake Union - Seattle</a>","<a href='https://www.amli.com/community.aspx?regiontag=atlanta&propertytag=piedmontheights'>AMLI Piedmont Heights - Atlanta </a>","<a href='https://www.amli.com/community.aspx?regiontag=chicago&propertytag=rivernorth'>Visit AMLI River North - Chicago</a>"];
          var altTagArray = [
              "AMLI South Shore Apartments in Austin, TX on Lady Bird Lake",
              "AMLI at the Ballpark Apartments in Frisco, TX - Resort Style Pool ",
              "AMLI 2121 Apartments in Houston, TX - Overlooking Allen Parkway",
              "AMLI Lex On Orange Apartments in Glendale, CA- Resort Pool with Cabanas",
              "AMLI South Lake Union Apartments - Rooftop Deck with City Views ",
              "AMLI Ponce Park Apartments in Atlanta, GA - Resort Pool ",
              "AMLI River North Apartments - 22,00 Sq Ft Rooftop Amenity Deck with Pool, BBQs, FirePits and Dog Park"
          ];

          function startPhotos() {
              window.setInterval("rotateNextPhoto(photoArray, titleArray)", $rotationSpeed);
          }

          function rotateNextPhoto(photos, titles) {

              //Check for the source of the currently loaded image.
              //Get the index of the currently loaded image
              var firstImg = $bgimg.attr("src");
              var photoIndex = jQuery.inArray(firstImg, photos);
              var nextPhotoIndex;
              if (photoIndex >= photos.length - 1) {
                  nextPhotoIndex = 0;
              }
              else {
                  nextPhotoIndex = photoIndex + 1;
              }

              var photoToUse = photos[nextPhotoIndex];
              var titleToUse = titles[nextPhotoIndex];
              SwitchImage(photoToUse, titleToUse);
          }
         
          function SwitchImage(img, title) {
              var theNewImg = new Image();
              theNewImg.onload = CreateDelegate(theNewImg, theNewImg_onload);
              theNewImg.src = img;
              theNewImg.alt = title;
          }

          function CreateDelegate(contextObject, delegateMethod) {
              return function () {
                  return delegateMethod.apply(contextObject, arguments);
              };
          }

          function theNewImg_onload() {
              $bgimg.data("newImageW", this.width).data("newImageH", this.height);
              BackgroundLoad($bgimg, this.width, this.height, this.src, this.alt);
          }
      </script>
