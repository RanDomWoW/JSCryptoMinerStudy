/*Кастомный стиль написанный для изменения менюшки на новую версию без доступов к исходникам сайта.*/
/*wondermedia*/
.tax-product_cat .page-header{
    margin: 0;
}
.tax-product_cat .header-main {
    background:#ffd14f;
}
.tax-product_cat .page-header--shop .header-exchanges__item,
.tax-product_cat.header-exchanges,
.tax-product_cat .btn-menu{
    color: #000;
}
.tax-product_cat  .symbol svg {
    fill: #000;
}
.alm-reveal {
    display:flex;
}
/*wondermedia – кастомный заголовок для страницы Event, залито 09.февраля.2017*/
h1.categories__page__title {
    font-size: 32px;
    line-height: 32px;
    font-weight: 700;
    text-align: center;
}
/*танцы с бубном над инстаграмом, залито 09.февраля.2017*/
.article__body iframe.instagram-media {
    width: 200% !important;
    max-width: inherit!important;
}
/*Стиль календаря в шаблоне event*/
.filters-popover {
    background: #fff;
    max-width: 265px;
    border-color: #dcdcdc;
}
.filters-calendar {
    max-width: 265px;
    width: 100%;
    margin-top: 25px;
}
.filter-item.active .filter-button {
    background: #fff;
    border-color: #dcdcdc;
    border-top: 1px solid #dcdcdc;
}
.ui-datepicker .ui-datepicker-next, .ui-datepicker .ui-datepicker-prev {
border: none;
}
div.filter-button:last-child {
    display: none;
}
.ui-datepicker td a, .ui-datepicker td span {
    height: 30px;
    line-height: 30px;
}
.type-list.active .filters-popover {
    left: 265px;
}
.ui-datepicker td {
    height: 30px!important;
    line-height: 30px!important;
}
.ui-datepicker .ui-datepicker-title {
    text-align: left;
    margin-bottom: 0;
    width: 0%;
}
.ui-datepicker .ui-datepicker-month {
    position: relative;
    top: 5px;
}
.ui-datepicker .ui-datepicker-header {
    top: 18px;
    width: 207px;
}
p.not-found {
    margin-left: 20px;
    color: inherit;
}
.filters-category__list a {
    color: #3761a3;
    font-family: ApercuPro,Helvetica Neue,Helvetica,Arial,sans-serif;
    font-size: 11px;
    letter-spacing: 1px;
    font-weight: 700;
}
.filters-category__list ul li a {
    color: #8d99ac;
    text-transform: uppercase;
}
.filters-category__list ul li a:hover {
    color: #3761a3!important;
}
div.filters {
    z-index: 10;
}
ul.w-event-ul {
    display: flex;
    justify-content: space-around;
    height: 64px;
    align-items: center;
    font-family: ApercuPro,Helvetica Neue,Helvetica,Arial,sans-serif;
    font-size: 11px;
    letter-spacing: 1px;
    font-weight: 700;
    text-transform: uppercase;
    line-height: 18px;
    color: #8d99ac;
}
ul.w-event-ul a {
    color: #8d99ac!important;
}
ul.w-event-ul a:hover {
    color: #3761a3!important;
}
.ajax-preloader {
    width: 100%;
    margin: 0 auto;
    color: #192949;
}
i.zima-youtube {
    color: #192949;
}
i.zima-youtube:hover {
    color: #ffffff;
}
a.social-item:hover i.zima-youtube {
    color: #ffffff;
}
img.all-devices-banner {
    margin: 0 auto!important;
}
div.banner--isInHeader {
    height: auto!important;
    padding: 0 20px!important;
    max-width: 768px!important;
    margin: 0 auto 20px auto!important;
    width: 100%;
    z-index: 100;
}
@media only screen and (max-width:511px) {
    .header-main {
    position: fixed!important;
    width: 100%!important;
    height: 70px!important;
    top: 0px!important;
	}
	.header .header-nav {
    position: fixed!important;
	top:10px!important;
	}
	.header.isSticky .site-logo {
        text-align: left;
        padding-left: 20px;
        width: 1%;
    }
    .filters-popover {
        max-width: 336px!important;
    }
    .type-list.active .filters-popover {
        left: 0px;
    }
    .ui-datepicker {
        border: none;
    }
}
    h1.categories__page__title {
        margin-top: 20px;
    }
    .ui-datepicker td {
        height: 30px!important;
        line-height: 30px!important;
    }
}

