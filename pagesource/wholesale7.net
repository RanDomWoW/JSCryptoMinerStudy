    */
    function addLinks(){
    if(!document.querySelector('[rel="canonical"]')){

    var pri=document.getElementsByTagName('head')[0];
    var dq_url=document.location.href;

    dq_url=dq_url.replace(/m\.wholesale7\.net/, "www.wholesale7.net");


    if(dq_url.indexOf("?")>0){
    dq_url=dq_url.substring(0,dq_url.indexOf("?"));
    }

    var link_var = document.createElement("link");
    link_var.setAttribute('rel',"canonical");
    link_var.setAttribute('href',dq_url);
    pri.appendChild(link_var);
    }else{
    var link_url=document.querySelector('[rel="canonical"]').href;
    var dq_url=link_url.replace(/m\.wholesale7\.net/, "www.wholesale7.net");
    document.querySelector('[rel="canonical"]').href=dq_url;
    }
    }

