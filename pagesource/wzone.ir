                    <div class="head_text">Arena Reset Timer</div>
                                        <ul class="times">
                        <li><div class="days value">34</div><div class="label">Days</div></li>
                        <li><div class="hours value">10</div><div class="label">Hours</div></li>
                        <li><div class="minutes value">37</div><div class="label">Minutes</div></li>
                        <li class="orange"><div class="seconds value">17</div><div class="label">Seconds</div></li>
                    </ul>
                    <script type="text/javascript">
                        $(function()
                        {
                            var startDate = new Date();
                            startDate.setTime(startDate.getTime() + 34 * 86400000);
                            startDate.setTime(startDate.getTime() + 10 * 3600000);
                            startDate.setTime(startDate.getTime() + 37 * 60000);
                            startDate.setTime(startDate.getTime() + 17 * 1000);
                            
                            setInterval(function()
                            {
                                var nowDate = new Date();
                                var difference = (startDate.getTime() - nowDate.getTime());
                                var lapse = new Date(difference);  
                                var secs = Math.round(difference / 1000);
                                
                                if (secs <= 0)
                                    return;
                                
                                var days = ( secs / 86400 ) >> 0;
                                var hours = ( secs % 86400 / 3600 ) >> 0;
                                var minutes = ( secs % 3600 / 60 ) >> 0;
                                var seconds = Math.round( secs % 60 );    
                                
                                // Update the texts
                                $('.arena_reset_box .seconds').html(seconds);
                                $('.arena_reset_box .minutes').html(minutes);
                                $('.arena_reset_box .hours').html(hours);
                                $('.arena_reset_box .days').html(days);
                            }, 1000);
                        });
                    </script>
