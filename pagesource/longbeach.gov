<script>
    $(document).ready(function () {
        PopulateEventList();
    });

    function PopulateEventList() {
        var eventListServiceUrl = 'http://www.filmlongbeach.com/event-calendar/list-view/week/current?format=feed&amp;type=rss';

        // if the 'date' portion of the url is "current" then substitute in todays date
        var pos = eventListServiceUrl.indexOf("week/current?");
        if (pos > -1)
        {
            var now = new Date();
            var month = (now.getMonth() + 1).toString();
            if (month.length == 1) month = "0" + month;
            var day = now.getDate().toString();
            if (day.length == 1) day = "0" + day;
            var year = now.getFullYear().toString();
            var today = month + "-" + day + "-" + year;
            eventListServiceUrl = eventListServiceUrl.substr(0, pos + 5) + today + eventListServiceUrl.substr(pos + 12);
        }

        $("#eventFeedList").empty();

        $.ajax({
            url: "clb/eventfeed/GetFeedEvents",
            type: "GET",
            contentType: "application/json; charset=utf-8",
            datatype: "json",
            data: { url : eventListServiceUrl },
            success: function (data) {
                var result = data;
                $.each(result, function (index, value) {
                    var li = $("<li/>");
                    var div = $("<div/>");
                    var h4 = $("<h4/>");
                    var decodedDesc = DecodeEntities(value.Description);
                    var p = $('<p>' + decodedDesc + '</p>');
                    h4.append('<a href="' + value.Link + '">' + value.Title + '</a>');
                    div.addClass("box-shadow");
                    div.append(h4);
                    div.append(p);
                    li.append(div);
                    $("#eventFeedList").append(li);
                })
            },
            error: function () {
                // leave the UL empty
            }
        });
    }

    function DecodeEntities(encodedString) {
        var textArea = document.createElement("textarea");
        textArea.innerHTML = encodedString;
        return textArea.value;
    }
</script>


