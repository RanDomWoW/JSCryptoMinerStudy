		var hideTimeout; //t - 261179

		var root = $("[data-guid=a4ea841c-2f48-46f9-8110-b9c6ac613a1f]");		
		var slider = root.find(".header-slider");
		var $slide = slider.find('.slide');
		var slideIndex = 0;
		var videoPlayers = [];
		var slides = root.find("[data-sv-slide]");
		var slideVideos = root.find('[data-slideshow-video]');

		function getActiveSlide(){
			for(var i = 0; i < slides.length; i++){
				if($(slides[i]).hasClass('slick-active')){
					return $(slides[i]);
				}
			}
		}
		
		function getVideoPlayer(videoId){
			for(var i = 0; i < videoPlayers.length; i++){
				if(videoPlayers[i].options.video_id === videoId){
					return videoPlayers[i];
				}
			}
		}

		var setImage = function(activeSlide){
			
			var imgWrapper = activeSlide.find("[data-sv-image-wrapper]");
			var slideImage = activeSlide.find("[data-sv-slide-image]");
			var videoPlayer = getVideoPlayer(activeSlide.attr("data-sv-video-id"));
			var width =  $(window).width();
			if( width < 1025  && width > 640){
				slideImage.attr( 'src', slideImage.attr('data-md-src') );
				// activeSlide.find("[data-td-video]").hide();
				imgWrapper.css('opacity', '1')
				if(typeof videoPlayer !== "undefined" && typeof videoPlayer.player !== "undefined"){
					if(typeof videoPlayer.player.playVideo === "function"){
						videoPlayer.player.pauseVideo();
					}
				}
			}
			else if( width < 641){
				slideImage.attr( 'src', slideImage.attr('data-sm-src') );
				// activeSlide.find("[data-td-video]").hide();
				imgWrapper.css('opacity', '1');
				if(typeof videoPlayer !== "undefined" && typeof videoPlayer.player !== "undefined"){
					if(typeof videoPlayer.player.playVideo === "function"){
						videoPlayer.player.pauseVideo();
					}
				}
			}
			else{
				activeSlide.find("[data-td-video]").show();
				if(typeof videoPlayer !== "undefined" && typeof videoPlayer.player !== "undefined"){
					if(typeof videoPlayer.player.playVideo === "function"){
						videoPlayer.player.playVideo();
					}
				}
			}
		}

		function pauseAllVideos(){
			for(var i = 0; i < videoPlayers.length; i++){
				videoPlayers[i].player.pauseVideo();
			}
		}

		root.find('[data-slideshow-video]')
		slider.on('init', function(event, slick){
			setUpVideos();
		});
		
		slider.on('beforeChange', function(event, slick, currentSlide, nextSlide){
			pauseAllVideos();
		});
		
		slider.on('afterChange', function(event, slick, currentSlide, nextSlide){
			if(hideTimeout){
				clearTimeout(hideTimeout);// t - 261179
			}
			setImage(getActiveSlide());
		});
		slider.slick({
			
			autoplay: true,
			autoplaySpeed : 5000,
			
			infinite: true,
			dots: false,
			arrows: true,
			fade:true,
			cssEase: 'linear',
			nextArrow : "<span class='next arrow'></span>",
			prevArrow : "<span class='prev arrow'></span>",
			appendArrows : "#arrows"
		});

		function setUpVideos(){
			if(slideVideos.length){
				buildVideos($(slideVideos[0]));
			}
		}


		function buildVideos(slideVideo){
			var videoId = slideVideo.attr("data-sv-video-id");
			var videoType =  slideVideo.attr("data-sv-video-type");
			new vp().init({
					options: {
						parent: slideVideo,
						type: videoType,
						video_id : videoId,
						iframe_id: 'video-iframe-' + videoId,
						frameborder: 0,
						scrolling: 'no',
						width: "100%",
						height: "auto",
						volume: 0,
						settings: {
							autoplay: 0,
							loop: 1,
							fullscreen: 1,
							controls : 0,
							modestbranding : 1,
							vq: '720',
			              	rel: 0,
			              	showinfo : 0,
			              	wmode: "transparent",
						},
						events: {
							ready: onReady,
							play: onPlay,
						},
					}
				}, onPlayerInit)
		}


		function onPlayerInit(_videoPlayer){
			videoPlayers.push(_videoPlayer);
			[].shift.call(slideVideos);
			setUpVideos();
		}

		$(window).on('resize', _debounce(function(){
			setImage(getActiveSlide());
		}, 150));


		function onPlay(){
			if($(window).width() > 1024){
				// t 261179
				hideTimeout = setTimeout(function(){
					getActiveSlide().find("[data-sv-image-wrapper]").css('opacity', '0');
				}, 2000)
			}
		}

		function onReady(){
			setImage(getActiveSlide());
		}


		function _debounce(func, wait, immediate) {
			var timeout;
			return function() {
				var context = this, args = arguments;
				var later = function() {
					timeout = null;
					if (!immediate) func.apply(context, args);
				};
				var callNow = immediate && !timeout;
				clearTimeout(timeout);
				timeout = setTimeout(later, wait);
				if (callNow) func.apply(context, args);
			};
		};

	});
