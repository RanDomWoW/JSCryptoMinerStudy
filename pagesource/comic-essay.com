<script type="text/javascript">
$(function() {
    //more
    $(function () {
        $('.js-more-read').prevAll().hide();
        $('.js-more-read').click(function () {
            if ($(this).prevAll().is(':hidden')) {
                $(this).prevAll().slideDown();
                $(this).html('<p>閉じる</p>').addClass('close');
            } else {
                $(this).prevAll().slideUp();
                $(this).html('<p>もっと見る</p>').removeClass('close');
            }
        });
    });
});
</script>

<!-- popup -->
<script type="text/javascript">
$(function() {
    $('.pop-buy').on('click', function() {
        var bookId = $(this).data('id');
        $.ajax({
            type: "GET",
            url: "/api/book",
            data: 'id=' + bookId,
            success: function(data) {
                $('.amazon-link').attr('href', data.url);
                $('.amazon-link').empty();
                if (data.url) {
                    $('.amazon-link').append('<img src="/images/banner/bnr_amazon.jpg" alt="アマゾン"><p>アマゾン</p>');
                }
                
                $('.rakuten-link').attr('href', data.url_2);
                $('.rakuten-link').empty();
                if (data.url_2) {
                    $('.rakuten-link').append('<img src="/images/banner/bnr_rakuten.jpg" alt="楽天ブックス"><p>楽天ブックス</p>');
                }
                $('.warker-link').attr('href', data.url_3);
                $('.warker-link').empty();
                if (data.url_3) {
                    $('.warker-link').append('<img src="/images/banner/bnr_bookwalker.jpg" alt="ブックウォーカー"><p>ブックウォーカー</p>');
