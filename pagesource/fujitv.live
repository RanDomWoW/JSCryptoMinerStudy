	<script>
	(function() {
		var info = parseThisPageFragments();
		if (info.type == 'login' && info.refresh_token && info.access_token && info.account) {
			setCookie('account', info.account);
			setCookie('refresh_token', info.refresh_token);
			setCookie('access_token', info.access_token);
			setCookie('account_confirmed', ''+parseBoolean(info.confirmed));
			if (parseBoolean(info.activate_email_sent)) {
				alert("An activation email has sent to your email box, please check your email box and activate your account.");
			} else if (!parseBoolean(info.confirmed)) {
				alert("Your account is not activated, \nPlease click on your account name and choose Resend Activation Email menu to do activation.");
				location.href="request-account-activation-email";
				return;
			}			
		} else {
			if (parseBoolean(info.activate_email_sent)) {
				alert("An activation email has sent to your email box, please check your email box and activate your account.");
			}  
			if (parseBoolean(info.request_logout)) {
				logout(true);
				alert("You need to logon again to take effects.");
				location.href = "logon";
				return;
			} else if (parseBoolean(info.resetpwd_email_sent)) {
				alert("An email with password reset link has sent to your email box, please check your email box and reset your password.");	
				location.href = "logon";
				return;
			}
		}
		if (info.code && info.code != 'OK') {
			switch(info.code) {
				case 'NOTFOUND':
					alert("The account does not exist.");
					location.replace("https://fujitv.live/logon");
					break;				
				case 'AUTH':
					alert("Password is bad.");
					location.replace("https://fujitv.live/logon");
					break;				
				case 'BANED':
					 alert("You have tried too many times, please tried later.");
					break;
				default:
					alert("Unexpected error occurs, error code: " + info.code + "\nMessage: " + info.msg);
					break;
			}
		}
		if (info.code) {
			location.replace('#');
		}
		var account = getLoginAccount();		
		if (account) {
			$('#logon_wrapper').css('display', 'none');
			$('#account_name').text(account);
			if (parseBoolean(getCookie('account_confirmed'))) {
				$('#menu_resend_activation').css('display', 'none');
			}
			$('#logout_wrapper').css('display', '');
		} else {
			$('#logon_wrapper').css('display', '');
			$('#logout_wrapper').css('display', 'none');
		}
	})();
</script>
	


