		<script type="text/javascript">
		    // function of timed pop up window to ask expert for help
		    $(document).ready(function () {
		        // alert("test js working");
		        // function to create or update cookie
		        function setCookie(cname, cvalue, exdays) {
		            if (exdays) {
		                var d = new Date();
		                d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
		                var expires = "; expires=" + d.toGMTString();
		            }
		            else var expires = "";
		            document.cookie = cname + "=" + cvalue + expires + ";domain=" + window.location.hostname + ";path=/";
		        }

		        // function to get the value of the cookie
		        function getCookie(cname) {
		            var name = cname + "=";
		            var ca = document.cookie.split(';');
		            for (var i = 0; i < ca.length; i++) {
		                var c = ca[i].trim();
		                if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
		            }
		            return "";
		        }

		        // set the flag to indicate that window never showed up
		        if (getCookie("windowShowedCookie") == "") {
		            setCookie("windowShowedCookie", "false");
		            //alert("first initial")
		        }

		        // set window pop up after 30 seconds
		        var showWindowInterval = 30000;

		        // when first scroll, set cookie to save current time
		        $(window).one("scroll", function () {
		            var d = new Date($.now());
		            if (getCookie("firstScrollCookie") == "") {
		                setCookie("firstScrollCookie", d);
		            }
		        });

		        function myTimer() {
		            var cookieValue = getCookie("firstScrollCookie");
		            if (cookieValue != "") {
		                var date = new Date(cookieValue);
		                var difference = new Date($.now()) - date;

		                //check time passed since first scroll time
		                if (difference > showWindowInterval) {
		                    var windowShowedCookieValue = getCookie("windowShowedCookie");

		                    //check if window showed before
		                    if (windowShowedCookieValue == "false") {
		                        //$('#needHelpWindowPop').show();
		                        //$('#resource_overlay').show();
		                        // set flag to remember window already showed up
		                        setCookie("windowShowedCookie", "true");
		                    }
		                }
		            }
		        }

		        // run myTimer function every second
		        window.setInterval(function () { myTimer(); }, 1000);

		        $('#needHelpWindowPopClose').click(function () {
		            $('#needHelpWindowPop').hide();
		            $('#resource_overlay').hide();
		        });

		        $('#resource_overlay').click(function () {
		            $('#needHelpWindowPop').hide();
		            $('#resource_overlay').hide();
		        });

		        // testing
		        /*$(".test1").click(function () {
                    var cookieValue = getCookie("firstScrollCookie");
                    alert("The cookie time is" + cookieValue);
                    var date = new Date(cookieValue);
                    var difference = new Date($.now()) - date;
                    alert("The difference is" + difference);
                });
                $(".test2").click(function () {
                    $('#needHelpWindowPop').show();
                    $('#resource_overlay').show();
                }); */
		    });
        </script>
        
