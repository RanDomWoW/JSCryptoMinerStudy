                    var options = { direction: 'left' };
                    var duration = 500;
                    $('#js-navabierto').show();
                    $('#m_mbl').toggle(effect, options, duration);
                    $('#m_mbl').addClass('menu_visible');
                }
                else if (e.target.id == "click_close" || (($(e.target).parent().attr("id") != "m_mbl") && $('#m_mbl').hasClass('menu_visible'))) {
                    var effect = 'slide';
                    var options = { direction: 'left' };
