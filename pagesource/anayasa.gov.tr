<script type="text/javascript">
function genislikguncelle() {

//css'imizin yüksekliğik değerini alıyoruz

  var winwidth = $("#content4").width(); // css yüksekliği

// Aldığımız değerleri .istediğimiz css'e dinamik olarak atıyoruz
  $(".framebos").css({'width': winwidth-'20'});

}

$(document).ready(function(){
  genislikguncelle();
});

$(".framebos").resize(function() {
  genislikguncelle();
});

</script>


<script type="text/javascript">
function yukseklikguncelle() {

//css'imizin yüksekliğik değerini alıyoruz

  var winwidth = $("#content4").width(); // css yüksekliği

// Aldığımız değerleri .istediğimiz css'e dinamik olarak atıyoruz
  $(".framebos").css({'height': winwidth/'1.25'});

}

$(document).ready(function(){
  yukseklikguncelle();
});

$(".framebos").resize(function() {
  yukseklikguncelle();
});

</script>



