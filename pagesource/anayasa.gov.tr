<script type="text/javascript">
$(document).ready(function() {
	$("#content2 div").hide(); // Initially hide all content
	$("#tabs2 .aktif").attr("id","current"); // Activate first tab
	$("#content2 div#genelkurulkarar").fadeIn(); // Show first tab content <!--Hangi Yayımlanan Son Kararlar alanı aktif hale getirilmek isteniyorsa onun ismi buraya yazılacak. Şöyle; div#bireyselbasvurukarar veya div#genelkurulkarar -->
        $('#tabs2 a').click(function(e) {
        e.preventDefault();
        $("#content2 div").hide(); //Hide all content
        $("#tabs2 li").attr("id",""); //Reset id's
        $(this).parent().attr("id","current"); // Activate this
        $('#' + $(this).attr('title')).fadeIn(); // Show content for current tab
    });
});
</script>




<script type="text/javascript">
    $(document).ready(function () {
        //Butonun ne zaman görüneceğini ayarlıyoruz.
        $(window).scroll(function () {
            if ($(this).scrollTop() > 100) {
                $('.top').fadeIn();
            } else {
                $('.top').fadeOut();
            }
        });
        //tıklama görevi etkileri için
        $('.top').click(function () {
            $('html, body').animate({ scrollTop: 0 }, 500);
            return false;
        });
    });
</script>
