		function resizeRearrange() {
			var rootElement = jQuery("#scoopit-theme_full").filter(function() {return jQuery(this).data("theme-lid") == "1041954"});
			var totalWidth = rootElement.width();
			var col1 = jQuery(".scoopit-fulltheme-scoops .scoopit-fulltheme-col1", rootElement);
			var col2 = jQuery(".scoopit-fulltheme-scoops .scoopit-fulltheme-col2", rootElement);
			if(totalWidth > 580) {
				// TWO COLUMNS
				jQuery(".scoopit-fulltheme-scoop-wrapper", col1).filter(function() {
					return jQuery(this).attr("id").indexOf("scoopit-fulltheme-1041954-scoop-col2-") == 0;
				}).each(function() {
					jQuery(this).appendTo(col2);
				});
				rootElement.removeClass("scoopit-embed-full-theme-one-column");
			} else {
				// ONE COLUMN
				jQuery(".scoopit-fulltheme-scoop-wrapper", col2).each(function() {
					var counter = jQuery(this).data("counter");
					jQuery(this).insertAfter(jQuery("#scoopit-fulltheme-1041954-scoop-col1-"+counter, col1));
				});
				rootElement.addClass("scoopit-embed-full-theme-one-column");
			}
		}
