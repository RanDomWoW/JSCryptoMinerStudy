<script type="text/javascript">
	jQuery('#contactSubmit').click(function(e){
		
		e.preventDefault();
		console.log(12.3);

		var name = jQuery('#name').val();
		var email = jQuery('#email').val();
		var mobile = jQuery('#mobile').val();
		var city = jQuery('#city').val();
		var program = jQuery('#program').val();
		var data = jQuery('#contactDetailsForm').serialize();

		if( name =='' || email == '' || mobile=='' || city=='' || program ==''){
			alert('Please enter all the Details');
		}else{

			jQuery('.modal-header h4').html('<label>Your Contact Information is submitted! A counselor will get back to you soon.<label>');
			jQuery('.modal-body').remove();
			jQuery('.modal-footer').remove();


			jQuery.ajax({
			  type: "POST",
			  url: 'http://umeacademy.com/mbalead/index.php',
			 //url:'https://docs.google.com/forms/d/e/1FAIpQLSeFVvXJDouUjkp8x948PZws0TYT5NkfM2T-_0pnokwx_5Ex0A/formResponse',
			  data: data,
			  success: function(){
			  	alert('success');
			  },

			});
			
		}

	})
</script>

