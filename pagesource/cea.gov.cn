<script>

function subStringLocationLatitude(sourceString){

var index = sourceString.indexOf(".");//先判断是否有小数点

 if(index > 0){//如果有

  var temp = sourceString.substr(index+1);//取得小数点位

  sourceString = sourceString.substr(0, index);

  

  var target = temp.substr(0, 2);//取得小数点后一位的数字

  if(temp.length >= 3){//如果小数点位数超过2位

   var str = temp.substr(1,2);//取得小数2位后的数

   if(str/1000 >= 0.05){

    target = (target/10 + 0.1)*10;

   }

  }

  sourceString = sourceString + "." +target;

 }

 var targetString=sourceString.substr(0,1) ;

 if(targetString=="-"){

  var modleString=sourceString.substring(1,sourceString.length)

    document.write("南纬"+modleString+",");

 }else{

    document.write("北纬"+sourceString+",");

 }

}

function subStringLocationLongitude(sourceString){

var index = sourceString.indexOf(".");//先判断是否有小数点

 if(index > 0){//如果有

  var temp = sourceString.substr(index+1);//取得小数点位

  sourceString = sourceString.substr(0, index);

  

  var target = temp.substr(0, 2);//取得小数点后一位的数字

  if(temp.length >= 3){//如果小数点位数超过2位

   var str = temp.substr(1,2);//取得小数2位后的数

   if(str/1000 >= 0.05){

    target = (target/10 + 0.1)*10;

   }

  }

  sourceString = sourceString + "." +target;

 }

 var targetString=sourceString.substr(0,1) ;

 if(targetString=="-"){

  var modleString=sourceString.substring(1,sourceString.length)

    document.write("西经"+modleString);

 }else{

    document.write("东经"+sourceString);

 }

}</script>
<script language="javascript1.2" >

function EqTab(labels,defaultClassName,activeClassName){

 this.labels = labels;

 this.defaultClassName = defaultClassName;



 this.activeClassName = activeClassName;

 var me = this;

 this.init=function(){

  for(var i=0;i<labels.length;i++){

   divObj = document.getElementById(labels[i]);

   divCon = document.getElementById(labels[i]+"_con");

   divObj.className=me.defaultClassName;

   divCon.style.display="none";

   divObj.onclick=function(obj){

    if(window.event){

     obj = event.srcElement;

    }else{

     obj = obj.target;

    }

    if(obj.tagName =='A'){

     obj=obj.parentElement;

    }

    me.activeTab(obj.id);

   }

  }

  this.activeTab(me.labels[0]);

 }

 this.activeTab = function(label){

  if(me.currentLabel == label){

   return;

  }

  document.getElementById(label).style.borderColor= "#4B7DDA";

  var con = document.getElementById(label+"_con");

  con.style.display="";

  if(con.innerHTML==""){

   con.innerHTML='<img class="map" width="700" src="'+images[label]+'" />';

  }

  if(me.currentLabel){

   document.getElementById(me.currentLabel).style.borderColor = "#e0e0e0"

   document.getElementById(me.currentLabel+"_con").style.display="none";

  }

  me.currentLabel = label;

 }

 

 this.init();

}

</script>
