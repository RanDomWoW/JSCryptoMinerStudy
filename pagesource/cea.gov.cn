<script>


//视频点播

 var nAdLen2 = $('.silder_con li').length;//焦点图个数

 var oSpan2 = '';//圆点数组

 for(var i=0;i<nAdLen2;i++){oSpan2+='<span>'+(i+1)+'</span>';}//创建圆点

 $('.con_show_num').append(oSpan2);//添加圆点

 var nAdSpeed2 = 0;//当前显示焦点图索引

 var timer2 = null;//定时器

 var bClick2 = false;

 $('.silder_con li').eq(0).css({'display':'block'}).siblings().css({'display':'none'});

 $('.con_show_num span').eq(0).addClass('show_num_cur')

 function showFade2()

 {

  clearInterval(timer2);

  if(nAdSpeed2 <= nAdLen2-1){


   $('.silder_con li').eq(nAdSpeed2).stop(true).css({'display':'block'});


   $('.silder_con li').eq(nAdSpeed2).siblings().stop(true).css({'display':'none'});

   $('.con_show_num span').eq(nAdSpeed2).addClass('show_num_cur').siblings().removeClass('show_num_cur');

   nAdSpeed2++;

  }else{

   nAdSpeed2 = 0;

  

  }

 }

 timer2 = setInterval(showFade2,5000);

 $('.con_show_num span').bind('click',function(){

  var ind = $(this).index();


  $('.silder_con li').eq(ind).stop(true).css({'display':'block'});


