function authData(type, kind, auth, id, name){
	if(kind == "phone"){
		var authColumn = "niceid_di";
	}else if(kind == "ipin"){
		var authColumn = "ipin_safekey";
	}else{
		return false;
	}

	$.ajax({
		type: "POST",
		url: "/member/search_id_pop.php",
		data: ({
			act : "search_idpw_new",
			type : type,
			authColumn : authColumn,
			authCode : auth,
			id : id,
			name : name
		}),
		async: true,
		success:function(result){
			var msg = false;
			if(result){
				var datas = JSON.parse(result);
				if(datas.id != "" && datas.mail != "" && datas.pcs != ""){

					if(type == "idlayer"){
						$("#result_txt").html("아이디 찾기 결과");
