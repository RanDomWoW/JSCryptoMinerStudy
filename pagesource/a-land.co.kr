function authProcess(kind, type){
	if(type == "pw"){
		var checkId = $("#find_id_"+kind).val();
		var checkName = $("#find_name_"+kind).val();

		if(checkId != "" && checkName != ""){
			var pwParam = "&id="+checkId+"&name="+checkName;
			$("#find_id_"+kind).closest(".wrap-login").find(".error-txt").css("display", "none");
		}else{
			if(checkId == ""){
				// 아이디를 입력해주세요.
				var errorTxt = "아이디를 입력해 주세요.";
				$("#find_id_"+kind).focus();
			}else if(checkName == ""){
				// 이름을 입력해주세요.
				var errorTxt = "이름을 입력해 주세요. ";
				$("#find_name_"+kind).focus();
			}
			$("#find_id_"+kind).closest(".wrap-login").find(".error-txt").css("display", "block");
			$("#find_id_"+kind).closest(".wrap-login").find(".error-txt").html(errorTxt);
			return false;
		}

		var user_id = $("#find_id_"+kind).val();
		var user_name = $("#find_name_"+kind).val();
		var chk_obj = $("#find_id_"+kind).parent().find("input[name=idcheck]");

		pwFindCheck(chk_obj, user_id, user_name);

		if(chk_obj.val() == 0){
			// 비밀번호 찾기시 아이디와 이름으로 가입된 회원인지 체크
			customizeAlertBasic("가입된 회원정보가 없습니다. <br/>회원가입 후 이용해 주세요.");
			return false;
		}

	}else if(type == "id"){
		var pwParam = "";
	}

	if(kind == "phone"){
		window.open('/member/checkplus/checkplus.php?act='+type+'layer'+pwParam, 'checkplus', 'top=100, left=200, status=0, width=500, height=480');
	}else if(kind == "ipin"){
		window.open("/member/ipin_2.0/ipin_main.php?act="+type+"layer"+pwParam, "ipin", "top=100, left=200, status=0, width=450, height=550");
	}else{
		return false;
	}
}

