function loginFrmCheck(frm, check, type){
	var checkFrm = $(frm);
	var chk = false;
	var txt = "";

	if(type == "login"){
		if(document.login_frmL.login_flag.value == 1){
			return false;	
		}
	}

	if(type == "login" || type == "sns"){
		var id = checkFrm.find("input[name=id]").val();
		var pw = checkFrm.find("input[name=pw]").val();
		if(id == ""){
			txt = "아이디를 입력해 주세요.";
			chk = true;
		}else if(pw == ""){
			txt = "비밀번호를 입력해 주세요.";
			chk = true;
		}
	}

	if(type == "order"){
		var oid = checkFrm.find("input[name=oid]").val();
		var bname = checkFrm.find("input[name=bname]").val();
		if(oid == ""){
			txt = "주문번호를 입력해 주세요.";
			chk = true;
		}else if(bname == ""){
			txt = "이름을 입력해 주세요. ";
			chk = true;
		}
	}

	if(check == "L" || check == "S"){
		txt = "아이디 또는 비밀번호를 확인해 주세요.";
		chk = true;
	}else if(check == "O"){
		txt = "일치하는 주문번호가 없습니다.";
		chk = true;
	}

	if(chk == true){
		$(".wrap-login.order-pop").addClass("error");
		checkFrm.find(".error-txt").html(txt);
		return false;
	}else{
		$(".wrap-login.order-pop").removeClass("error");
	}

	if(type == "login"){
		document.login_frmL.login_flag.value = 1;
	}
}

function findFrmCheck(frm){
	var frmObj = $(frm).find("input[type=text]");
	var prt = $(frm).closest(".wrap-login");
	var msg = "[TITLE] 입력해 주세요.";
	var chk = false;
	var tit = "";

	prt.find(".error-txt").hide();

	frmObj.each(function(){
		if($(this).val() == ""){
			chk = true;
			tit = $(this).attr("placeholder");
			return false;
		}
	});

	if(chk == true){
		prt.find(".error-txt").css("display", "block");
		prt.find(".error-txt").html(msg.replace('[TITLE]', tit));
		return false;
	}
}

