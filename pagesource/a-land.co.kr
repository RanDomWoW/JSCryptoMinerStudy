var loginCheckSetInterval;
function loginPopupProc(link, option)
{
    var winObj; // 팝업창
    var agent = navigator.userAgent.toLowerCase(); // 브라우저 검사
	/*var opener=window.opener.catch(function(){
	    console.log(opener);
	});*/
    //if(!winObj && (typeof window.opener === 'undefined' || window.opener == "" || window.opener == null)){ // 최초 클릭
    if(!winObj) {
        winObj = window.open(link, '_blank', option);
    }
	//}

	clearInterval(loginCheckSetInterval);

    loginCheckSetInterval = setInterval(function()
	{
        if ( (navigator.appName == 'Netscape' && navigator.userAgent.search('Trident') != -1) || (agent.indexOf("msie") != -1) )
        {
            if(!winObj){ // 윈도우 팝업창이 없다면 부모 페이지를 새로고침한다.
                setTimeout(function(){ window.open('','_parent').close(); }, 5000);
            }
        } else {
            if(!winObj && winObj.closed){ // 윈도우 팝업창이 없다면 부모 페이지를 새로고침한다.
                //window.parent.location.reload(); // 부모창 새로고침
                setTimeout(function(){ window.open('','_parent').close(); }, 5000);
            }
        }
	}, 1000);
}
