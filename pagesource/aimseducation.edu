    jQuery(document).ready(function () {


        jQuery(".txtfName").on("keyup", function () {

            if (jQuery(this).val() != "" || jQuery(this).val() != null) {
                jQuery(".fname").removeClass("display").addClass("hidden");
            }
            else {
                jQuery(".fname").removeClass("hidden").addClass("display");
            }
        });

        jQuery(".txtPhone").on("keyup", function () {
            if (jQuery(this).val() != "" || jQuery(this).val() != null) {
                jQuery(".phone").removeClass("display").addClass("hidden");
            }
            else {
                jQuery(".phone").removeClass("hidden").addClass("display");
            }
        });

        jQuery(".txtEmail").on("keyup", function () {
            var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            if (jQuery(this).val() != "" || jQuery(this).val() != null) {
                jQuery(".emailvalidation").removeClass("display").addClass("hidden");
                if (re.test($(this).val())) {
                    jQuery(".emailcheck").removeClass("display").addClass("hidden");
                }
                else {
                    jQuery(".emailcheck").removeClass("hidden").addClass("display");
                    jQuery(".txtEmail").focus;
                }
            }
            else {
                $(".emailvalidation").removeClass("hidden").addClass("display");
                jQuery(".txtEmail").focus;
            }
        });

    });


    $(function () {
        var html = $('html, body'),
            navContainer = $('#cbp-hrmenu'),
            navToggle = $('.toggleMenu'),
            //navDropdownToggle = $('.has-dropdown');
     navDropdownToggle = $('.cbp-hrmenu > ul > li > a');

        // Nav toggle
        navToggle.on('click', function (e) {
            var $this = $(this);
            e.preventDefault();
            $this.toggleClass('is_active');
            navContainer.toggleClass('is-visible');
        });

        navDropdownToggle.on('click', function () {
            var $this = $(this);
            $this.toggleClass('is_active').next('div.has-dropdown').toggleClass('is-visible');
        });

        // Prevent click events from firing on children of navDropdownToggle
        navDropdownToggle.on('click', '*', function (e) {
            e.stopPropagation();
        });
    });


</script>

                
