                    function positionNavUnderline(aTargetJqObj, anUnderlineJqObj) {
                        var theHeight = aTargetJqObj.height();
                        var theLeft = aTargetJqObj.offset().left;
                        var theTop = aTargetJqObj.offset().top + theHeight;
                        var theWidth = aTargetJqObj.width();

                        anUnderlineJqObj.css({
                            left: theLeft,
                            width: theWidth,
                            top: theTop
                        });
                    }

                    function setUpNavBehaviorForStatePointer() {
                        var theUrlPathParts = window.location.pathname.split('/');
                        var theLastIndex = theUrlPathParts.length - 1;

                        var theMenuItem = $('.nav-list a[href$="' + theUrlPathParts[
                            theLastIndex] + '"]').first().parent().addClass('current-page');

                        if (!!theMenuItem.length) {
                            showArrowOverCurrentPageNav(theMenuItem);
                        }
                    }

                    function showArrowOverCurrentPageNav(aMenuElement) {
                        var theLeft = aMenuElement.offset().left;
                        var theTop = aMenuElement.offset().top;
                        var theWidth = aMenuElement.width();

                        var theCurrentPageArrow = $('#current-page-arrow');
                        theCurrentPageArrow.css({
                            left: theLeft + (theWidth / 2) - 6,
                            top: theTop
                        });

                        if (isPageInMobileMode() && isCurrentPageLastListElement(aMenuElement)) {
                            theCurrentPageArrow.css('display', 'none');                          
                        }

                        theCurrentPageArrow.show();
                    }

                    function isPageInMobileMode() {
                        // quick way to determine if the the view is in mobile mode...fragile...
                        return ($(window).width() < 481);
                    }

                    function isCurrentPageLastListElement(aMenuElement) {
                        return aMenuElement.hasClass('last-of-nav-list-li');
                    }

                    function resetupNavBehaviorForStatePointerForIE8() {
                        hideStatePointer();
                        setTimeout(setUpNavBehaviorForStatePointer, 500);
                    }

                    function hideStatePointer() {
                        var theCurrentPageArrow = $('#current-page-arrow');
                        theCurrentPageArrow.hide();
                    }

                    function showStatePointer() {
                        var theCurrentPageArrow = $('#current-page-arrow');
                        theCurrentPageArrow.show();
                    }

                    $(document).ready(function (e) {
                        popupservice.Pop({
                            name: 'graduation'
                        });
                    });
                });
            });

        });
    </script>
    <script type="text/javascript">
        function safeLoadTypeKit() {
            try {
                Typekit.load();
            } catch (e) {}
        }
    </script>
    <script async="true" type="text/javascript" src="//use.typekit.net/gbh2ymn.js" async="" onload="safeLoadTypeKit()"></script>
