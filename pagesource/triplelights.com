<script src="https://cdn.triplelights.com/vendor/autocomplete/auto-complete.min.js"></script> 



<script>
$(document).ready(function () {

  new autoComplete({
      selector: '.search-query',
      minChars: 0,
      source: function(term, suggest){

          term = term.toLowerCase();
          var choices = ['Tokyo','Kyoto','Osaka','Nara','Kamakura','Hiroshima','Hakone','Mount Fuji','Hokkaido','Shizuoka','Nikko','Yokohama','Kanagawa','Nagasaki','Fukuoka','Kanazawa','Nagano','Kobe','Gifu','Tochigi','Hyogo','Kagawa','Aomori','Yamanashi','Takayama','Niigata','Okinawa','Wakayama','Saitama','Narita','Nagoya','Shiga','Aichi','Ishikawa','Akita','Kochi','Chiba','Kagoshima','Okayama','Toyama','Kumamoto','Miyazaki','Saga','Tottori','Mount Koya','Ibaraki','Mie','Fukushima','Shimane','Iwate','Miyagi','Tokushima','Yamagata','Fukui','Gunma','Oita','Yamaguchi','Kumano','Ehime','Sado'];
          var suggestions = [];


          if( term.length < 1 )
          {
              return false;
          }


          for (i=0;i<choices.length;i++)
          {
             if (~(choices[i]).toLowerCase().indexOf(term))
              {
                  suggestions.push(choices[i]);
              }
          }

          if( suggestions.length != 0 && suggestions.length == 1 && suggestions[0].toLowerCase() == term.toLowerCase() )
          {
                return true;
          }


          suggest(suggestions);

      }

  });


  $('.search-query').focus(function() {

   console.log('open');
    $('.section-head').addClass('section-head-open');

  })

  $('.search-query').focusout(function() {

    console.log('close');
   
    setTimeout("$('.section-head').removeClass('section-head-open');",300);

  });

});
</script>

