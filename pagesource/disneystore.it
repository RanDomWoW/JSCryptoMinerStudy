		var promoSpeed=1000;
		$(document).on('mouseenter', '.headerPromo, .qtip', function(){
			promoPause=true;
		}).on('mouseleave', '.headerPromo, .qtip', function(){
			promoPause=false;
		});
		function doPromoRotate(){
			if(!promoPause){
				var promoHeight = $('.headerRotatorPromos').css('height');
				if (promoStatus==0||promoStatus==""||promoStatus==null){
					$('#rotatorContainer').animate({marginTop:"-"+promoHeight},promoSpeed);
					promoStatus=1;
				}
				else {
					$('#rotatorContainer').animate({marginTop:"0px"},promoSpeed);
					promoStatus=0;
				}
			}
		}
		var promoRotate=setInterval(doPromoRotate,3000);
	});
