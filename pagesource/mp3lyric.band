<script type="text/javascript">
    function getPageNumberVal() {
        return parseInt($(document.getElementById("MusicsFilter_PageInfo_PageNumber")).val());
    };

    function initPageNumber() {
        $(document.getElementById("MusicsFilter_PageInfo_PageNumber")).val('1');
    }

    var myPlaylist2;
    jQuery(document).ready(function () {
        "use strict";

        initPageNumber();
        myPlaylist2 = undefined;

        setSeeAllHref();
    });

    function getMusicOrderBy() {
        return $("#MusicOrderBy").val();
    }

    function setSeeAllHref() {
        $("#a_SeeAllMusics").attr("href", "/Global/Musics?musicOrderBy=" + getMusicOrderBy());
    }

    function setActiveClass(elem) {
        var parentLi = $(elem).parent();
        if (parentLi.hasClass("active")) {
            return false;
        }


        setSeeAllHref();
        
        initPageNumber();

        var parentUl = parentLi.parent();
        parentUl.children().removeClass("active");
        parentLi.addClass("active");
        return true;
    }

    var loadMusicUrl = "/Global/Musics/MusicsByDate";

    function loadMoreMusicsBy(elem) {
        if (setActiveClass(elem)) {
            if (myPlaylist2) {
                myPlaylist2.remove();
            }
            loadMoreMusics();
        }
    }
    function loadMoreMusicsByDownload(elem) {
        $("#MusicOrderBy").val("Download");
        loadMusicUrl = "/Global/Musics/MusicsByDownload";
        loadMoreMusicsBy(elem);
    }
    function loadMoreMusicsByView(elem) {
        $("#MusicOrderBy").val("View");
        loadMusicUrl = "/Global/Musics/MusicsByView";
        loadMoreMusicsBy(elem);
    }
    function loadMoreMusicsByLike(elem) {
        $("#MusicOrderBy").val("Like");
        loadMusicUrl = "/Global/Musics/MusicsByLike";
        loadMoreMusicsBy(elem);
    }
    function loadMoreMusicsByDate(elem) {
        $("#MusicOrderBy").val("Date");
        loadMusicUrl = "/Global/Musics/MusicsByDate";
        loadMoreMusicsBy(elem);
    }


    var isReachEndMusics = false;
    var isBusyLoadMusics = false;

    function loadMoreMusics() {
        isBusyLoadMusics = true;
        $(document.getElementById("div_MusicsLoading")).show();

        var pageSize = parseInt('15');
        //$(this).prop("disabled", true);

        if (!myPlaylist2) {
            initPageNumber();
        }

        $.ajax({
            method: "GET",
            url: loadMusicUrl,
            data: $(document.getElementById("form_musicList")).serialize()
        })
            .done(function (result) {
                if (!myPlaylist2) {
                    initJPlayerPlaylist(result.data);
                } else {
                    for (var i = 0; i < result.data.length; i++) {
                        myPlaylist2.add(result.data[i]);
                    }
                    //console.log(myPlaylist2);
                }

                $(document.getElementById("MusicsFilter_PageInfo_PageNumber")).val(getPageNumberVal() + 1);

                if (result.data.length < pageSize) {
                    isReachEndMusics = true;
                    $(document.getElementById("p_endOfMusics")).show();
                    return;
                }
            })
            .fail(function (error) {
                console.log(error);
            })
            .always(function () {
                //$(this).prop("disabled", false);
                isBusyLoadMusics = false;
                $(document.getElementById("div_MusicsLoading")).hide();
            });
    };

    function initJPlayerPlaylist(musics) {
        myPlaylist2 = new jPlayerPlaylist({
            jPlayer: "#jquery_jplayer_2",
            cssSelectorAncestor: "#jp_container_2"
        },
            musics,
            {
                playlistOptions: {
                    enableRemoveControls: true
                },
                swfPath: "/Content/theartist/assets/jplayer/jplayer",
                supplied: "oga, mp3",
                wmode: "window",
                useStateClassSkin: true,
                autoBlur: false,
                smoothPlayBar: true,
                keyEnabled: true,
            });

        // Show The Current Track !!
        $(document.getElementById("jquery_jplayer_2")).on(
            $.jPlayer.event.ready + ' ' + $.jPlayer.event.play,
            function (event) {
                var current = myPlaylist2.current;
                var playlist = myPlaylist2.playlist;
                $.each(playlist, function (index, obj) {
                    if (index == current) {
                        $(document.getElementById("nowPlaying2")).find(".artist-name").html(obj.artist + " - ");
                        $(document.getElementById("nowPlaying2")).find(".track-name").html(obj.title);
                    }
                });
            }
        );
        $(document.getElementById("jquery_jplayer_2")).on($.jPlayer.event.play, function (event) {
            //console.log(event.jPlayer.status.media);
            $(document.getElementById("player-bars")).addClass("animated");
