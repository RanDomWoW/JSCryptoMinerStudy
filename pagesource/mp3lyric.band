<script type="text/javascript">

    //note: use in jplayer.playlist.js

    function likeMusic(elem, musicId) {

        $.ajax({
            method: "POST",
            url: '/Global/Musics/LikeMusic/' + musicId
        })
        .done(function (result) {
            if (!result.data) {
                alert("برای لایک کردن باید وارد سایت شوید");
                return;
            }

            var aElem = $(elem);
            var spanElem = aElem.children("span");
            var likesCount = parseInt(spanElem.text());

            var iElem = aElem.children(".fa");
            var beforeLikeCss = "fa-heart-o";
            var afterLikeCss = "fa-heart";

            if (iElem.hasClass(beforeLikeCss)) {
                spanElem.text(likesCount + 1);
                iElem.removeClass(beforeLikeCss);
                iElem.addClass(afterLikeCss);
            } else {
                spanElem.text(likesCount - 1);
                iElem.removeClass(afterLikeCss);
                iElem.addClass(beforeLikeCss);
            }
        })
        .fail(function (error) {
            console.log(error);
            if (error.status == 401) {
                alert("برای لایک کردن باید وارد سایت شوید");
            } else {
                alert("خطا در انجام عملیات");
            }
        })
        .always(function () {
        });
    }

</script>



<script src="/Scripts/jquery.unobtrusive-ajax.min.js"></script>



<script type="text/javascript">

    jQuery(document).ready(function ($) {
        "use strict";

        $.ajax({
            method: "GET",
            url: '/Global/Musics/RandomMusics'
        })
            .done(function (result) {

                var myPlaylist = new jPlayerPlaylist({
                    jPlayer: "#jquery_jplayer_1",
                    cssSelectorAncestor: "#jp_container_1"
                },
                    result.data,
                    {
                        playlistOptions: {
                            enableRemoveControls: true
                        },
                        swfPath: "/Content/theartist/assets/jplayer/jplayer",
                        supplied: "mp3",
                        wmode: "window",
                        useStateClassSkin: true,
                        autoBlur: false,
                        smoothPlayBar: true,
                        keyEnabled: true,
                    });

                // Show The Current Track !!
                $(document.getElementById("jquery_jplayer_1")).on(
                    $.jPlayer.event.ready + ' ' + $.jPlayer.event.play,
                    function (event) {
                        var current = myPlaylist.current;
                        var playlist = myPlaylist.playlist;
                        $.each(playlist, function (index, obj) {
                            if (index == current) {
                                $(document.getElementById("nowPlaying .artist-name")).html(obj.artist + " - ");
                                $(document.getElementById("nowPlaying .track-name")).html(obj.title);
                            }
                        });
                    }
                );

            })
            .fail(function (error) {
                console.log(error);
            })
            .always(function () {
            });

    });
</script>


<script type="text/javascript">

    //global.js

    function showErrorMessages(messages) {
        var msg = "";

        for (var i = 0; i < messages.length; i++) {
            msg += messages[i] + "\r\n";
        }

        alert(msg);
    }

    function isInt(value) {
        if (isNaN(value)) {
            return false;
        }
        var x = parseFloat(value);
        return (x | 0) === x;
    }

</script>



