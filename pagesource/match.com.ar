<script type="text/javascript">
    $jq('.box-clic-ler-mais').click(function () {
        var txtBox = $jq(this).parent().find('div p');
        if (txtBox.css('height') == '77px') {
            $jq(this).css('background-position', '1195px -470px');
            txtBox.animateToAutoHeight();
        } else {
            txtBox.animateToAutoHeight({ "height": "77px" });
            $jq(this).css('background-position', '1195px -444px');
        }

    });

    jQuery.fn.animateToAutoHeight = function () {
        var curHeight = this.css('height'),
            height = this.css('height', 'auto').height(),
            duration = 200,
            easing = 'swing',
            callback = $jq.noop,
            parameters = { height: height };
        this.css('height', curHeight);
        for (var i in arguments) {
            switch (typeof arguments[i]) {
                case 'object':
                    parameters = arguments[i];
                    //parameters.height = height;
                    break;
                case 'string':
                    if (arguments[i] == 'slow' || arguments[i] == 'fast') duration = arguments[i];
                    else easing = arguments[i];
                    break;
                case 'number':
                    duration = arguments[i];
                    break;
                case 'function':
                    callback = arguments[i];
                    break;
            }
        }
        this.animate(parameters, duration, easing, function () {
            $jq(this).css('height', parameters.height ? parameters.height : 'auto');
            callback.call(this, arguments);
        });
        return this;
    };
</script>    
            
         

