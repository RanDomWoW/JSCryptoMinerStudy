    <script>    
    $('#header-search-input').keyup(function (evt) {
        var me = this, val = this.value;
        val = val.replace(/[^a-zA-Z0-9üäöß]+/g,'');
        
        $.get('/ajax/shop/suche', {'s': val})
            .done(function (response) {  
                
                if ( me.value != val) /** Abfrage wegen eventueller asynchronität */
                    return;
                
                var sugg = $('#header-search-suggestions');
                
                if ( val === '' ) return sugg.html('');
                
                if (response.length == 0) return sugg.html('<div style="padding:10px"><i>Keine Treffer</i></div>');
    
                sugg.html('<div id="suggestions-container">'+$.map(response, function (entry) {
                    if (entry.type == 'product') {
                                                    return '<a href="' + encodeURI(entry.url) + '" class="size12"><p><img src="' + encodeURI(entry.image) + '" class="left">' + entry.name + '</p></a>';
                                            }
                    else if (entry.type == 'more') {
                        return '<a href="' + encodeURI(entry.link) + '" class="size12 moreSearch"><p>Alle Suchergebnisse anzeigen &raquo;</p></a>';
                    }
                    else {
                        // FAQ
                        var question = entry.question;
                        if (question.length > 45) 
                        {
                            question = question.replace(/^(.{45}[^\s]*).*/, "$1") + ' ...';
                        }
                        return '<a href="' + encodeURI(entry.url) + '" class="size12"><p><img src="//imagepool.smartmobil.de/NEU/images/smartmobil_Icon_FAQ.png" class="left">' + question + '</p></a>';
                    }
                }).join("\n") + '</div>');
            });     
    });
    
    
    $('#header-search-suggestions').on( "mouseleave",function (evt) {
        $('#header-search-suggestions').html('');
    });
    
        
</script>

