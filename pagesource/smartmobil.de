<script>


/* A/B Test: body : Seitennamen als CSS-Klasse setzen + extra Klasse für Seiten der Bestellstrecke.
***********************************************/


var stringURL = $(location).attr('pathname').substring(1).replace(/\//g, '_' );

var bodyTarget = $("body");
if(stringURL == ""){
    bodyTarget.addClass("page-home");    
} else {

    if(/basket_view/i.test(stringURL)||/bestellung/i.test(stringURL)){
         bodyTarget.addClass("page-bestellstrecke");
    }
    
    if(/v[A-F]_/.test(stringURL)) {
        // bei AB-Tests den Klassennamen einmal ohne und einmal mit dem Präfix der Variante anhängen
        var replacedStringURL = stringURL.replace(/v[A-F]_/, "");
        bodyTarget.addClass("page-" + replacedStringURL);
        bodyTarget.addClass("page-" + stringURL);
    } else {
        bodyTarget.addClass("page-" + stringURL);            
    }
}

/* ENDE A/B Test
***********************************************/


