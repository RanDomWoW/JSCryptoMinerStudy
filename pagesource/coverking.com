                homedata = data.replace('Please select a vehicle', 'PERFECTLY PROTECT&trade; YOUR VEHICLE');
                $j('.ch-vehicle').replaceWith(homedata);
                //FIXME temporary decision
                $j('.options-wrapper').hide();
            });
        },
        homepageUpdateSelector : function (currentSelect) {
            var currentOption = currentSelect.getAttribute('id').replace('ch-', '');
            var toDisable = this.getOptionsToDisable(currentOption);
            if (currentOption == 'submodel') {
                this.showLoader();
            }
            if (toDisable.length > 0) {
                toDisable.forEach(function (item, i, arr) {
                    if ($j('select#ch-'+item).length) {
                        var disabled = $j('select#ch-'+item).prop('disabled');
                        if (false === disabled) {
                            $j('select#ch-'+item).html('<option>'+item+'</option>');
                            $j('select#ch-'+item).prop('disabled', true);
                            $j('select#ch-'+item).addClass('ch-disabled');
                        }
                    }
                });
            }
            var options = {};
            $j('.ch-select-wrapper select').each(function() {
                var option = $j(this).attr('id').replace('ch-', '');
                if (option != $j(this).val()) {
                    options[option] = $j(this).val();
                } else {
                    options[option] = '';
                }
            });
            $j.ajax({
                url: this.getBaseUrl() + '/vehicle/ajax/options',
                method: 'POST',
                data: {options: options},
                dataType: 'json'
            })
                .done(function (data) {
                    if (data['last']) {
                        // refresh:
                        window.location.href = 'https://www.coverking.com/coverking-all';
                        return false;
                    }
                    if (data['success']) {
                        var html = data['html'];
                        var idPostfix = data['fill'];
                        if ($j('select#ch-'+idPostfix).length) {
                            $j('select#ch-'+idPostfix).html(html);
                            $j('select#ch-'+idPostfix).prop('disabled', false);
                            $j('select#ch-'+idPostfix).removeClass('ch-disabled');
                        }
                    }
                });
        }
    });
</script>
