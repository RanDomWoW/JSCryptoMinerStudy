    jQuery('.language-switcher .current').click(function(e){
        if(jQuery('.ymm-wrapper:not(.ymm-wrapper-product)').is(':visible')){
            jQuery('.ymm-wrapper').slideUp('fast');
        }
        if(jQuery('.dropdown').is(':visible')){
            jQuery('.dropdown').slideUp('fast');
        }

        if(jQuery('.language-dropdown').is(':visible')){
            jQuery('.language-dropdown').slideUp();
        }else{
            jQuery('.language-dropdown').slideDown();
        }

        e.preventDefault();
    });
});

jQuery(document).click(function(e) {
    if(!jQuery(e.target).parent().hasClass('current')){
        if (jQuery(e.target).closest(".language-dropdown").length === 0) {
            jQuery(".language-dropdown").slideUp();
        }
    }
});
