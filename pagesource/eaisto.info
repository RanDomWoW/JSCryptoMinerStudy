<script>
function validateCheckForm(){
if(($("#vin").val() == "")&&($("#registr").val() == "")&&($("#body").val() == "")&&($("#chassis").val() == "")&&($("#eaisto").val() == "")){
$("#card_alert").removeClass("hidden");
$("#card_alert").text("Введите данные в одно из полей.");
$("#card_alert").removeClass("show");
return false;
}
return true;
}

function showAlertModal(){
	$("#myModalAlert").modal("show");
}
</script>
