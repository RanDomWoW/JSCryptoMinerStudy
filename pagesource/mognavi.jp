<script>
	function globalNaviFix() {
		if (!isHeaderFixed()) {
			jQuery('#headTop').css('position', 'relative').css('top', '');
			jQuery('div#container').css('margin', '0 auto 0');
			jQuery('.navbar-fixed-top').css('top', '0');
			return;
		}

		var globalNavi = jQuery('#globalNavi');
		var pankuzu = jQuery('#pathNavi');
		var globalNaviTop = globalNavi.offset().top - jQuery('#container').css('margin-top').substring(0, jQuery('#container').css('margin-top').length - 2);
		var globalNaviLine2 = jQuery('#globalNavi .line2');
		var globalNaviLine2Span = jQuery('#globalNavi .line2 span');
		var wrapperNew = jQuery('#wrapperNew');
		function fixGlobalNavi() {
			if(jQuery(window).scrollTop() > globalNaviTop) {
				globalNavi
					.addClass('fixed')
					.addClass('thin');
				pankuzu.addClass('fixed');
		    	globalNaviLine2Span.hide();
		    	wrapperNew.addClass('fixed');
			} else {
		    	globalNavi
		    		.removeClass('fixed')
					.removeClass('thin');
		    	pankuzu.removeClass('fixed');
		    	globalNaviLine2Span.show();
		    	wrapperNew.removeClass('fixed');
			}
		}

		jQuery(window).scroll(function() {
			fixGlobalNavi();
		});
		fixGlobalNavi();
	}

	if (document.addEventListener) {
		document.addEventListener('DOMContentLoaded', globalNaviFix, false);
	} else if (window.attachEvent) {
		window.attachEvent("onload", globalNaviFix);
	}

	// header_950pxl.jsp
	function showWantToEatNotice() {
		if (isWindowInvisible('tabetai_notice')) {
			return;
		}
		var left = jQuery('#want-action').offset().left - 100;
		jQuery(".tabetai_notice").attr('style', 'left: ' + left + 'px;');
		jQuery(".tabetai_notice").hide().fadeIn();
	}

	function hideWantToEatNotice() {
		jQuery(".tabetai_notice").fadeOut();
	}

	addOnLoadEventListener(function() {
		jQuery('body').click(function(ev) {
			if ("tabetai_notice" != ev.target.className
				&& "block" == jQuery(".tabetai_notice").css("display")) {
				hideWantToEatNotice();
			}
		});

		jQuery('#tabetai_notice_close_check').click(function() {
			addWindowInvisible('tabetai_notice');
		});

		jQuery('.tabetai_notice_close').click(function() {
			hideWantToEatNotice();
		});

		jQuery(".tabetai_notice p").click(function() {
			setTimeout('toggleWantoToEat()', 500);	// 直後に閉じる動作がはいるためインターバルをセット
		});
	});
</script>
