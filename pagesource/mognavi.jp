		return;
	}

	//console.log('[recommend.negative.js.jsp] no foodId not bind');

});

function bindsendnegative() {
	//console.log('[recommend.negative.js.jsp] recommend negative bind timeout is 20000 msec');
	window.onbeforeunload = function(){
		sendnegative();
	}
	begin = new Date();
	negflag = true;
	setTimeout(
		function(){
			//console.log('[recommend.negative.js.jsp] 20000 msec passed recommend negative timeout');
			negflag = false;
		},
		20000
	);
}

function sendnegative() {
	if(!negflag){
		//console.log('[recommend.negative.js.jsp] not negative');
		return;
	}
	duration = new Date() - begin;
	jQuery.ajax({
		type: 'GET',
		url: '/do/api/member/recommend/negative;jsessionid=913756D859051EA70ECBFDF7281CE2E1.mogpc02/param/foodId//duration/'+duration,
		async: false
	});
	//console.log('[recommend.negative.js.jsp] send negative to  duration '+duration);
}

