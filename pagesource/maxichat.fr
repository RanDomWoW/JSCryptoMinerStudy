function set_value() 
{	group_sexe='';
	var element = document.inscription.group_sexe;
	for (i=0;i<element.length;i++)
	{	if (element[i].checked)
		{	group_sexe=element[i].value;
			break;
		}
	}
	pseudo=document.forms.inscription.pseudo.value;
	motdepasse=document.forms.inscription.motdepasse.value;
	email=document.forms.inscription.email.value;
	if (document.forms.inscription.cgu.checked) cgu=true;
	jour=document.forms.inscription.listejour.options[document.forms.inscription.listejour.selectedIndex].value;
	mois=document.forms.inscription.listemois.options[document.forms.inscription.listemois.selectedIndex].value;
	annee=document.forms.inscription.listeannee.options[document.forms.inscription.listeannee.selectedIndex].value;
}
function openPostData(page,data)
{   var form = document.createElement('form');
    form.setAttribute('action', page);
    form.setAttribute('method', 'post');
    for (var n in data)
    {   var inputvar = document.createElement('input');
        inputvar.setAttribute('type', 'hidden');
        inputvar.setAttribute('name', n);
        inputvar.setAttribute('value', data[n]);
        form.appendChild(inputvar);
    }
    document.body.appendChild(form);
    form.submit();
}
function get_pays(new_pays) 
{	set_value();
	var data = {};
	data.listepays      = new_pays;
	data.pseudo         = pseudo;
	data.motdepasse     = motdepasse;
	data.group_sexe     = group_sexe;
	data.email			= email;
	if (cgu) data.cgu	= true;
	data.listejour		= jour;
	data.listemois		= mois;
	data.listeannee		= annee;
	page				= 'maxichat.php';
	openPostData(page,data);
}
