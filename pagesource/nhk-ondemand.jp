<!--content-->
<div id="content">
	<div class="inner">
		<!-- new content -->
		<div class="contents-order js-contents-order">
			<div class="area_block" v-for="cl in contlist" :id="cl.contp2id">
				<div v-if="cl.normal" class="area_title_block"
					 :class="[{mb10:cl.ismb10,area_title_border:cl.isBorder,clear:cl.isClear}]">
					<ul>
						<li class="area_title" :class="cl.colorClass">{{cl.Title}}</li>
						<li class="tBold area_count_program" v-if="cl.subLead">{{cl.subLead}}</li>
						<li class="area_lead" v-if="cl.Lead">{{cl.Lead}}</li>
					</ul>
				</div>
				<div v-if="cl.mycontent" class="spec_area_title_block clear mb10">
					<p class="spec_area_title tBold area_title">{{cl.Title}}</p>
					<a v-if="cl.needMypage" href="/user/availableList/index.html" class="purProgramLink">マイページ</a>
				</div>
				<div class="area_list_program swiper-container" :id="cl.contpid">
					<div :data-index="cl.index" :id="cl.contid" class="thumblist-loading"><img src="/css/ph2017/img/top/loading.gif" alt=""></div>
				</div>
				<div class="slide_backbtn slide_arrow swiper-button-prev swiper-button-disabled"><img src="/css/ph2017/img/top/icn_arr_prev.png" alt=""/></div>
				<div class="slide_nextbtn slide_arrow swiper-button-next swiper-button-disabled"><img src="/css/ph2017/img/top/icn_arr_next.png" alt=""/></div>
			</div>
		</div>
		<div>
			<div class="area_title_block area_title_border mb10">
				<p class="infoTitle">インフォメーション</p>
			</div>
			<ul class="banner_side js-lower-banner">
				<li v-for="bn in banners"><a :href="bn.Uri"><img :src="bn.Image" alt=""/></a></li>
			</ul>
		</div>
	</div>
</div>
