<!--information-->
<div class="information">
	<div class="inner">
		<ul class="infor_group clear">
			<li class="item-notice infor_contain">
				<div class="infor_contain_inner clear">
					<p class="fLeft infor_label newItem-notice-js notice-close">
						<span class="infoHeadTitle">お知らせ</span>
						<span class="icnOpenP">
							<img src="/css/ph2017/img/top/icn_line.png" alt=""/>
						</span>
					</p>
					<div class="fLeft infor_block js-topinfo">
						<div class="list_infor swiper-container">
							<div class="swiper-wrapper">
								<p class="swiper-slide inbox" :class="ti.Class" v-for="ti in topinfos">
									<a v-if="ti.isBlog" target="_blank" :href="ti.Uri" v-html="ti.TitleShort"></a>
									<a v-else class="info_popup" href="#" v-html="ti.TitleShort" :data-title="ti.Title" :data-body="ti.Body"></a>
									<img v-if="ti.isBlog" src="/css/ph2017/img/top/external-link.png" alt="">
								</p>
							</div>
						</div>

						<div class="list_full">
							<ul class="ul_full">
								<li :class="ti.Class" v-for="ti in topinfos">
									<a v-if="ti.isBlog" target="_blank" :href="ti.Uri" v-html="ti.Title"></a>
									<a v-else class="info_popup" href="#" v-html="ti.TitleShort" :data-title="ti.Title" :data-body="ti.Body"></a>
									<img v-if="ti.isBlog" src="/css/ph2017/img/top/external-link.png" alt="">
								</li>
							</ul>
						</div>

						<div class="bottom">
							<div class="item-bottom bottom-close">
								<p class="close-item close-item-js">閉じる</p>
							</div>
							<div class="item-bottom bottom-link">
								<a href="http://blog.nhk-ondemand.jp/">NHKオンデマンドお知らせ一覧</a>
							</div>
						</div>
					</div>
				</div>
			</li>
			<li class="banner">
				<ul class="clear js-upper-banner">
					<li v-for="bn in banners">
						<a :href="bn.Uri">
							<div class="banner_info">
								<p><img :src="bn.Image" height="68" width="68" alt=""/></p>
								<p v-html="bn.Text"></p>
							</div>
						</a>
					</li>
				</ul>
			</li>
		</ul>
	</div>
</div>
