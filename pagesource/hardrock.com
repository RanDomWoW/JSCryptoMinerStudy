<script type="text/javascript" charset="utf-8">
    $(document).ready(function () {
        $(".imgLiquidFill").imgLiquid();
    });
    $(window).load(function () {
        var flexsliderOptions = {
            animation: "fade",
            direction: "horizontal",
            slideshow: true,
            randomize: false,
            animationLoop: true,
            directionNav: true,
            controlNav: "true",
            useCSS: false,
            pauseOnHover: true,
            controlsContainer: ".flex-container",
            initDelay: 0
        };
        //if controlNav was set to a boolean value then make it an actual boolean value
        //instead of the string value
        if (flexsliderOptions.controlNav === "true") {
            flexsliderOptions.controlNav = true;
        }
        if (flexsliderOptions.controlNav === "false") {
            flexsliderOptions.controlNav = false;
        }
        //if showing thumbnails then find the file paths of the slide images and set them
        //as the thumbnail file paths on the lis
        if (flexsliderOptions.controlNav === "thumbnails") {
            $('#topHeroWidget .flexslider li').each(function () {
                var $li = $(this);
                var $image = $li.find("img");
                $li.attr("data-thumb", $image.attr("src"));
            });
        }
        $('#topHeroWidget .flexslider').flexslider(flexsliderOptions);
        $('#topHeroWidget .carousel-caption a').each(function () {
            if ($(this).attr('href') === undefined && $(this).find("h3").length == 0) {
                $(this).remove();
            }
            else if ($(this).attr('href') === undefined && $(this).find('h3').length != 0) {
                $(this).find(".flex-button").remove();
            }
        });
        $("#viewFlexSliderSwipebox").click(function (e) {
            e.preventDefault();
            var images = [];
            $(".flexslider ul.slides li img").each(function () {
                images.push({
                    href: $(this).attr("src"),
                    title: $(this).attr("alt")
                });
            });
            $("#menuCarousel .carousel-inner img").each(function () {
                images.push({
                    href: $(this).attr("src"),
                    title: $(this).attr("alt")
                });
            });

            $.swipebox(images);
        });
    });
</script>


