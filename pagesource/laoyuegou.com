<script type="text/javascript">
    $(function() {
        $(".main-lay-nav").mouseover(function() {
            $(".lay-nav").hide();
            $(this).find(".lay-nav").show();
        });

        $(".tit").click(function() {
            $(".tit").css("color", "#333");
            $(this).css("color", "#209e89");
            var game = $(this).attr('game');
            if (game == "lol") {
                $(".search-input").attr("placeholder", "输入您的召唤师昵称  （支持全球数据）"); //（暂只支持国服）
                $("#game_type").val("lol");
            } else if (game == "wow") {
                $(".search-input").attr("placeholder", "输入您的公会名称或角色名称（支持全球数据）");
                $("#game_type").val("wow");
            } else if (game == "dota2") {
                $(".search-input").attr("placeholder", "输入您的游戏昵称或好友ID（支持全球数据）");
                $("#game_type").val("dota2");
            } else if (game == 'ow') {
                $(".search-input").attr("placeholder", "输入您的战网昵称,PSN或者XBOX昵称");
                $("#game_type").val("ow");
            }
            var child = '<i class="key-arrow"></i>';
            $(this).siblings().children().remove();
            $(this).append(child);
        });

        $(".search-input").click(function() {
            $(this).val("");
        });
    });
</script>
