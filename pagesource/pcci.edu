	$(document).ready(function(){
		$.getJSON( "/embed/lib/craw.ashx?craw=json&sort=DESC&max=3&query=Homepage", function( alumVids ) {
			for(var i=0; i<alumVids.length; i++){
				var b = $('#alumVid'+i).closest('.box-link');
				b.find('[data-ev-title]').text(alumVids[i][3].Title);
				b.find('[data-ev-desc]').text(alumVids[i][3].Description);
				b.find('[data-ev-poster]').attr('src', (alumVids[i][3].PlainPoster || alumVids[i][3].DefaultPoster));
				b.find('[data-ev-video]').EmbedVideo({ 
					videoID: alumVids[i][1], 
					year: alumVids[i][0], 
					id: alumVids[i][1], 
					bgColor: "ffffff", 
					template: "alumniHomepage", 
					poster: "PlainPoster",
					posterSize: "cover"
				});	
			}
			setTimeout(function(){
    			fitVidFames()
			}, 1500);
		});
		onDoneResize(fitVidFames);
	});
