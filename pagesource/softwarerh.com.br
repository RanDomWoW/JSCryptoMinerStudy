<script type="text/javascript">
//exibe os depoimentos dos clientes
var exibindo = 0;
var porTela = 3;
var qtde = 0;

var slideE = function(){ if(exibindo > 0){ exibindo -= 1; goSlide(); } };
var slideD = function(){ if(exibindo < qtde - porTela){ exibindo += 1; }else{ exibindo = 0; } goSlide(); };
function goSlide(){ $(".carousel").stop().animate({scrollLeft: ( exibindo * 320 ) + (15/2) }, 1000); }


function calculaSlides(){
    var w = $(window).width();
    if( w > 1024 ){
        $('.carousel').addClass('slideGrande');
        qtde = $('#cases_inner > div').length;
        var soma = 0;
        $('#cases_inner > div').each(function(index, part){
            $(part).show();
        });
    }else{
        exibindo = 0
        $('.carousel').removeClass('slideGrande');
        $('#cases_inner > div').each(function(index, part){
            if( index > 2){
                $(part).hide();
            }
        });
    }
}

$(document).ready(function(){
    calculaSlides();
    $(window).resize(function(){ calculaSlides(); });
});
</script>

