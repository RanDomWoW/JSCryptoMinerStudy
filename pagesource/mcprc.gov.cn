<script type="text/javascript" language="javascript">
var timetbg = 500;
var htbg = 0;
var adsobj=document.getElementById("ads");
function addCount()
{
    if(timetbg>0)
    {
       timetbg--;
       htbg= htbg+5;
    }
    else
    {
       return;
    }
    if(htbg>300)  //高度
    {
       return;
    }
    adsobj.style.display = "block";
    adsobj.style.height = htbg+"px";
    setTimeout("addCount()",30); 
}
function showAds()
{
     addCount();
     setTimeout("noneAds()",7000); //停留时间自己适当调整
}
var Ttbg = 500;
var Ntbg = 300; //高度
function noneAds()
{
    if(Ttbg>0)
    {
       Ttbg--;
       Ntbg = Ntbg-5;
    }
    else
    {
       return;
    }
    if(Ntbg<0)
    {
       adsobj.style.display = "none";
       return;
    }
    adsobj.style.height = Ntbg+"px";
    setTimeout("noneAds()",30); 
}
if(adsobj){
setTimeout("showAds()",5000)} 
</script>
 
