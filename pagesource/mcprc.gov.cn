       <script>
$(document).ready(function(){
	//首页滚动图片
	//图片比例自适应
	var imgnew_W = $(".imgnew_img_list").width();
	var imgnew_H = $(".imgnew_img_list").height();
	var imgnew_WH = imgnew_W/imgnew_H;
	$(".imgnew_img_list img").each(function(){
		var fimg_W = $(this).width();
		var fimg_H = $(this).height();
		var fimg_WH = fimg_W/fimg_H;
		if(fimg_WH>imgnew_WH){
			$(this).css({width:"480px",height:imgnew_H});
		}else{
			$(this).css({width:imgnew_W,height:"320px"});
		}
	});
	
	var imgnew_i = 0;
	var imgnew_len = $('.imgnew_img_list .imgnew_img').length;
	function fun_imgnew(){
		$('.imgnew_img_list .imgnew_img').eq(imgnew_i).stop(true,true).fadeIn(500).siblings().fadeOut(400);
		$('.imgnew_title_list .imgnew_title').eq(imgnew_i).stop(true,true).fadeIn(500).siblings().hide();
		$('.imgnew_point_list .imgnew_point').eq(imgnew_i).addClass("On").siblings().removeClass("On");
	}
	fun_imgnew();
	function imgnew_auto(){
		imgnew_i++;
		imgnew_i %= imgnew_len;
		fun_imgnew();
	}
	var imgnew_play = setInterval(imgnew_auto,5000);
	
	$('.imgnew_point_list .imgnew_point').mouseover(function(){
		imgnew_i = $(this).index();
		fun_imgnew();
	});
	$(".sc_imgnew").hover(function(){
		clearInterval(imgnew_play);
	},function(){
		imgnew_play = setInterval(imgnew_auto,5000);
	});
	
});
</script> 
