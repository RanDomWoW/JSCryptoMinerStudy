<script>
	// initialize the library with the API keyprès
   	window.fbAsyncInit = function() {
    	FB.init({
    		appId  : '333031783422888',
    		channelUrl : '//www.cybercartes.com/hz/identification/channel.php',
    	    status : true, // check login status
    	    cookie : true, // enable cookies to allow the server to access the session
    	    logging: true,
    	    xfbml  : true  // parse XFBML
    	});
    
		FB.Event.subscribe('auth.login', function(response) {
			window.location.reload();
		});
		FB.getLoginStatus(handleSessionResponse);
	};
	$('#login').bind('click', function() {
		FB.login(handleSessionResponse, {scope:'email,friends_birthday'});
    });


	$("#fb_connect_btn").mouseup(function(){
		//$(this).attr("src", "//static.cybercartes.com/layouts/cyb_default/img/hp/2/birthdayReminder/fb_connect_login.jpg");
	}).mousedown(function(){
	//	$(this).attr("src", "//static.cybercartes.com/layouts/cyb_default/img/hp/2/birthdayReminder/fb_connect_login_off.jpg");
		$(this).attr("id", "fb_connect_btn_clck");
        FB.login(handleSessionResponse, {scope:'email,friends_birthday'});
	});


	$('#logout').bind('click', function() {
    	FB.logout(handleSessionResponse);
    });

	$('#disconnect').bind('click', function() {
		FB.api({ method: 'Auth.revokeAuthorization' }, function(response) {
			clearDisplay();
		});
	});

	// no user, clear display
	function clearDisplay() {
		$('#fb_connect_body').hide('fast');
	}

	//prérequis: b_day > taday
	function formatDay(b_day, today)
	{
		var one_day=1000*60*60*24;
		var diff = Math.ceil((b_day.getTime()-today.getTime())/(one_day));
		if(diff == 0)
			return "Aujourd'hui";
		else if(diff == 1)
			return "Demain";
		else
		{
			var b_day_dd = b_day.getDate();
			var b_day_mm = (b_day.getMonth()+1);
			if(10 > b_day_dd)
				b_day_dd = "0"+b_day_dd;
			b_day_dd = ""+b_day_dd;
  			if(10 > b_day_mm)
  				b_day_mm = "0"+b_day_mm;
  			b_day_mm = ""+b_day_mm;
			
			return b_day_dd+'/'+b_day_mm;
		}
	}

	function displayFriend(response, i, ji, b_day_mm, b_day_dd, today)
	{
		$('#fb_connect_body_overflow').html(
            	$('#fb_connect_body_overflow').html()
            	+ '<li>'
            	+ '<a onclick="javascript: pageTracker._trackEvent(\'HP\', \'fb_anniversaire\', \''+ji+'_btn_ami\');" href="/cartes/anniversaire" class="bingLink">'
				+ '<img src="' + response[i].pic_square + '" alt=""/>'
				+ '	<div>'
				+ '		<span class="date">' + formatDay(new Date(today.getFullYear(), b_day_mm-1, b_day_dd), today) + '</span>'
				+ '		<span class="name">' + response[i].name + '</span>'
				+ '		<a href="/cartes/anniversaire" class="link">Envoyer une carte</a>'
				+ '	</div></a>'
				+ '</li>'
            );
	}

	// handle a session response from any of the auth related calls
	function handleSessionResponse(response) {
		// if we dont have a session, just hide the user info
		if (!response.authResponse) {
			clearDisplay();
			$('#buttons').css("display","block");
			$('#fb_connect_body_not_logged').css("display","block");
			return;
        }
        else
        {
        	$('#buttons').css("display","none");
        }

		gestionCookie();
		
        // if we have a session, query for the user's profile picture and name
        FB.api(
        {
			method: 'fql.query',
			query: 'SELECT uid, name, pic_square, birthday_date FROM user WHERE uid IN (SELECT uid2 FROM friend WHERE uid1 = ' + response.authResponse.userID +') ORDER BY birthday_date'
        },
		function(response) {
            //Déclaration des variables
        	var today = new Date;
  			var dd = today.getDate();
  			var mm = (today.getMonth()+1);
  			var j = 0;
  			var nb_friends_to_show = 5;
            var b_day_mm = "";
            var b_day_dd = "";

            //On teste si le jour ou le mois < 10 si oui, on ajoute un "0"
  			if(10 > dd)
  				dd = "0"+dd;
  			dd = ""+dd;
  			if(10 > mm)
  				mm = "0"+mm;
			mm = ""+mm;
			
			var texte_test = "";

			//Boucle 1 pour afficher les anniversaires:
            for(i = 0; i < response.length; i++)
            {
                //Si le nombre d'amis à afficher est obtenu, on sort de la boucle
                if(j == nb_friends_to_show)
                    break;
                //On teste que l'ami veuille afficher sa date dans son profil
                if(response[i].birthday_date)
                {
                    //On récupère le jour et le mois
                    b_day_mm = response[i].birthday_date.substr(0,2);
                    b_day_dd = response[i].birthday_date.substr(3,2);

					//Test pour savoir si le mois de naissance est == mois courant
                    if(parseFloat(b_day_mm) == parseFloat(mm))
                    {
                        //Si oui on vérifie que le jour soit >= au jour courant
                    	if(parseFloat(b_day_dd) >= parseFloat(dd))
                        {
                            //On ajoute les données 
                    		var ji = j+1;
                    		if(j != 0)
                    			$('#fb_connect_body_overflow').html($('#fb_connect_body_overflow').html()+'<div class="fb_connect_space">&nbsp;</div>');

                    		displayFriend(response, i, ji, b_day_mm, b_day_dd, today);
                    		
                    		j++;
                        }
                    }
                    else
                    {
                        //Si le mois de naissance est > au mois courant
                        if(parseFloat(b_day_mm) > parseFloat(mm))
                        {
                            //On ajoute les données
                        	var ji = j+1;
                        	if(j != 0)
                        		$('#fb_connect_body_overflow').html($('#fb_connect_body_overflow').html()+'<div class="fb_connect_space">&nbsp;</div>');
                    		
                        	displayFriend(response, i, ji, b_day_mm, b_day_dd, today);
                        	
                    		j++;
                        }
                    }
                }
            }
            //Si le nombre d'amis à afficher n'est toujours pas obtenu
            if(j < nb_friends_to_show)
            {
                //Nombre d'amis à afficher - nb amis déja trouvés
            	j_max = nb_friends_to_show-j;
            	j = 0;
            	//On reboucle pour finir l'affichage
            	for(i = 0; i < response.length; i++)
            	{
            		if(j == j_max)
                        break;
            		if(response[i].birthday_date)
            		{
	            		b_day_mm = response[i].birthday_date.substr(0,2);
	                    b_day_dd = response[i].birthday_date.substr(3,2);
	                    var ji = j+1;
               			
	                    displayFriend(response, i, ji, b_day_mm, b_day_dd, today);
	                    
	                    j++;
            		}
            	}
            }
            //On affiche la liste des amis
            $('#fb_connect_body_not_logged').css("display","none");
            $('#fb_connect_body').css('display','block');
          }
        );
      }

	function gestionCookie()
    {
        //eraseCookie('tokenFacebook');    //pour les tests : suppression du cookie.
        
        if(readCookie('tokenFacebook') == null)
        {
            //cookie inexistant
            var date = new Date();
            createCookie('tokenFacebook', date.getFullYear()+'/'+(date.getMonth()+1)+'/'+date.getDate(), 7);
            recupTokenFacebook();
        }
        else
        {
            strDate = readCookie('tokenFacebook');
            tabDate = strDate.split('/');
            dateCookie = new Date(tabDate[0], tabDate[1]-1, tabDate[2]);
            d = new Date();
            date = new Date(d.getFullYear(), d.getMonth(), d.getDate()); //fait en 2 étapes sinon date.getTime() sera + grand que dateCookie.getTime() s'il date du jour même

            if(dateCookie.getTime() < date.getTime())
            {
                //cookie périmé
                createCookie('tokenFacebook', date.getFullYear()+'/'+(date.getMonth()+1)+'/'+date.getDate(), 7);
                //recupTokenFacebook();
            }
            //else : cookie valide, on ne fait rien
        }
    }
    function recupTokenFacebook()
    {
        //appel ajax FacebookController.php => récupération du token
       	$.post('//www.cybercartes.com/hz/identification/ajFacebook.php', { token : FB.getAuthResponse().accessToken }, function(data)
        {
            //$('#info').html(data);    //pour debug
        });
    }
    
