				layui.use('layer',function(){
					layer.load(1, {
                    shade: [0.1,'#fff'] //0.1透明度的白色背景
                });});
            },
            success: function (data) {
                layer.closeAll();
                $("#detailArticleList").html(template.render('ArticleTemplate', data));
            }
        });
    })
</script>
<script>
$('.cooperativePartner li').hover(function () {
	$(this).find('.hover').stop().fadeIn();
},function () {
	$(this).find('.hover').stop().fadeOut();
});

$(".tab>li").on("click",function () {
	$(this).addClass("active").siblings().removeClass("active");
	var dataId=$(this).attr("data-id");
	$(".cooper_Wrapper").each(function (idx,e) {
		if (dataId==$(e).attr("data-id")){
			$(e).find(".cooperativePartner").addClass("show").removeClass("hidden");
			$(e).siblings().find(".cooperativePartner").removeClass("show").addClass("hidden")
		}
	})
})
</script>
