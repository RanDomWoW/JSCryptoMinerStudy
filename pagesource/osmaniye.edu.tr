<script type="text/javascript">
var ajaxNesne;

function isEmail(email)
{ 
return /^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))$/i.test(email);
} 

function iletisim() 
{
var isim=null
var konu=null
var mesaj=null
var eposta=null

if (document.getElementById('isim').value=="" || document.getElementById('konu').value==""  || document.getElementById('mesaj').value==""  || document.getElementById('eposta').value=="" )
{
document.getElementById('iletisim_sonuc').innerHTML='<font color="red">L&uuml;tfen bo&#351; b&#305;rakmay&#305;n&#305;z!</font> <button type="button" class="btn btn-primary" onclick="iletisim();">Gönder</button>';
}
else
{
isim=document.getElementById('isim').value;
konu=document.getElementById('konu').value; 
mesaj=document.getElementById('mesaj').value;
eposta=document.getElementById('eposta').value;
btu_token=document.getElementById('btu_token').value;

// gerçek email hesabı sorgulama
if (isEmail(eposta) == false)
{
document.getElementById('eposta').focus();
document.getElementById('iletisim_sonuc').innerHTML='<font color="red">Geçerli bir e-posta adresi yazınız!</font> <button type="button" class="btn btn-primary" onclick="iletisim();">Gönder</button>';
}
else
{
document.getElementById('iletisim_sonuc').innerHTML="";
nesne = ajaxNesne;
metot = 'POST';
dosya = 'kod_islemler.php';
fonksiyon = ajax_iletisim;

degisken ='iletisim_isim=' + isim + '&eposta=' + eposta + '&konu=' + konu + '&mesaj=' + mesaj + '&btu_token=' + btu_token;

ajaxistek(nesne,metot,dosya,degisken,fonksiyon);
}
}
}

function ajax_iletisim()
{
if ( ajaxNesne.readyState == 4 )
{
if ( ajaxNesne.status == 200 )
{
msg = ajaxNesne.responseText;
}
else
{
msg = "istenmeyen bir hata olustu";
}
}
else
{
msg="<img src='img/ajax-loader.gif'>";
}
document.getElementById('iletisim_sonuc').innerHTML=msg;
}

</script>    

