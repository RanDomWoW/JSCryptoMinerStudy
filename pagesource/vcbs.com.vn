    <script type="text/javascript">
        $(function () {
            $("#search_button").on("click", function (ev) {
                ev.preventDefault();
                if ($('#stock_code') && $('#stock_code')[0].checked)
                    window.location = "/Research/?stocksymbol=" + $('#search_textbox').val();
                if ($('#news') && $('#news')[0].checked)
                    window.location = "https://www.google.com.vn/#q=" + $('#search_textbox')[0].value + "+site%3Avcbs.com.vn";
            });
            $.ajax({
                url: '/DataGen/companies_2013.txt',
                dataType: 'json'
            }).done(function (source) {

                var countriesArray = $.map(source, function (value, key) { return { value: value, data: key }; });
                $("#search_textbox").autocomplete({
                    width: 300,
                    lookup: countriesArray,
                    onSelect: function (suggestion) {
                        $('#search_textbox')[0].value = suggestion.data;
                        if ($('#stock_code') && $('#stock_code')[0].checked)
                            window.location = "/Research/?stocksymbol=" + suggestion.data;
                    }
                });
                if ($('#news') && $('#news')[0].checked)
                    $("#search_textbox").autocomplete().disable();
                $("#news").on("click", function () {
                    if ($('#news') && $('#news')[0].checked)
                        $("#search_textbox").autocomplete().disable();
                });
                $("#stock_code").on("click", function () {
                    if ($('#stock_code') && $('#stock_code')[0].checked)
                        $("#search_textbox").autocomplete().enable();
                });
            });
        });
    </script>
    
