<script type="text/javascript">
    jQuery(function() {
        var site = 'bali-indonesia.com';
        var engine = 'kEPJxp2MhuV27e49im7A';
        var autocompleteTemplate = Hogan.compile('<p class="title">{{#highlight}}{{{title}}}{{/highlight}}</p>');
        var searchResultItemsTemplate = Hogan.compile(jQuery("#searchResultItemsTemplate").html());
        var searchResultTemplate = Hogan.compile(jQuery("#searchResultTemplate").html());
        var $searchBox = jQuery('#gsc-i-id1');

        // Autocomplete
        var autocompleteCustomRenderFunction = function(document_type, item) {
            return autocompleteTemplate.render(item)
        };

        var customResultRenderFunction = function(ctx, results) {
            var $list = ctx.list,
                config = ctx.config;

            jQuery.each(results, function(document_type, items) {
                jQuery.each(items, function(idx, item) {
                    ctx.registerResult(jQuery('<li>' + config.renderFunction(document_type, item) + '</li>').appendTo($list), item);
                });
            });

            
            ctx.registerResult(jQuery('<li><p class="title more-srp"><em>See more results</p></li>').appendTo($list), {url: "/search#stq=" + $searchBox.val()});
            
        };

        $searchBox.swiftype({
            renderFunction: autocompleteCustomRenderFunction,
            resultRenderFunction: customResultRenderFunction,
            engineKey: engine,
            filters: {'page': {'site': site}},
            perPage: 10
        });

        // Search Result
        var paginationInfo = function (currentPage, totalPages) {
            var info = {
                previousPage: null,
                nextPage: null,
                pages: []
            };

            if (currentPage > 1) {
                info.previousPage = currentPage -1;
            }

            if (currentPage < totalPages) {
                info.nextPage = currentPage + 1;
            }

            var start = currentPage - 2;
            if (start < 1) {
                start = 1;
            }
            var end = start + 4;
            if (end > totalPages) {
                end = totalPages;
                start = end - 4;
                if (start < 1) {
                    start = 1;
                }
            }

            for (i = start; i <= end; i++) {
                info.pages.push({
                    pageNumber: i,
                    active: i == currentPage
                });
            }
            return info;
        };

        var customResultsRenderer = function(ctx, data) {
            var $resultContainer = ctx.resultContainer;
            var page = data.info.page;
            var from = ((page.current_page - 1) * page.per_page) + 1;
            var to = ((page.current_page - 1) * page.per_page) + data.record_count;
            $searchBox.val(page.query);
            page.from = from;
            page.to = to;
            page.pagination = paginationInfo(page.current_page, page.num_pages);
            $resultContainer.html(searchResultTemplate.render(page));

            jQuery.each( data.records.page, function( index, value ) {
                value.shortDescription = "";
                if (value.description) {
                    var shortDesc = jQuery.trim(value.description).substring(0, 200).split(" ").slice(0, -1).join(" ") + " ...";
                    value.shortDescription = shortDesc;
                }
            });

            $resultContainer.find(".st-search-results .st-query-present").html(searchResultItemsTemplate.render(data.records));
        };

        $searchBox.swiftypeSearch({
            engineKey: engine,
            resultContainingElement: '.search_result',
            filters: {'page': {'site': site}},
            perPage: 20,
            renderResultsFunction: customResultsRenderer
        });

        var $containingForm = $searchBox.parents('form');
        $containingForm.submit(function(event) {
            event.preventDefault();
            if ($searchBox.val()) {
                window.location = $containingForm.attr("action") + "#stq=" + $searchBox.val();
            }
        });

        
    });
</script>

