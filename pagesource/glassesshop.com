    // 回调函数
    function signinCallback(authResult) {
        if (authResult) {
            // 是否有错
            if (authResult["error"] == undefined) {
                // 加载api
                gapi.client.load("oauth2", "v2", function () {
                    var flag = $(".google_flag").val();
                    var request = gapi.client.oauth2.userinfo.get();
                    request.execute(function (obj) {
                        console.log('obj:', obj);
                        if (flag == 1) {
                            //google登陆
                            $.ajax({
                                url: '/login/google',
                                data: {'data': obj},
                                type: 'post',
                                dataType: 'json',
                                success: function (retdat) {
                                    console.log('login_google:', retdat);
                                    //已经关联账户，刷新当前页面
                                    if (retdat.status == 1 && retdat.code == 200) {
                                        window.location.reload();
                                    }
                                }
                            });
                        }

                        if (flag == 2) {
                            //绑定账号
                            $.ajax({
                                url: '/Customer_Binding/ajax_google',
                                data: {'data': obj},
                                type: 'post',
                                dataType: 'json',
                                success: function (retdat) {
                                    if (retdat.status == 1 && retdat.code == 200) {
