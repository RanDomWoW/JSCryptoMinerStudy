<script type="text/javascript" >
    function autocomplete_searchwords(thiss){
        if ( $(thiss).val().length>=2 ){
            $.ajax({
                url: '/autocomplete_searchwords/auto?words=' +  $(thiss).val() ,
                dataType: 'html',
                success: function(strValue) {
                    var strValue_json = eval('(' + strValue + ')');
                    html_extra_data = strValue_json['search_list']; // 回传数据
                    html_search_list = '';
                    if ( html_extra_data.length>0 ){
                        for ( i=0;i<html_extra_data.length;i++ ) {
                            html_search_list += '<option value="'+html_extra_data[i]['word']+'">' ;
                        }
                        $("#search_list").html(html_search_list);
                    }
                }
            });

        }
    }

</script>
