    $(function () {
        $(".btn-reset-search").click(function () {
            $("#forgotPassword").validate({
                rules: {
                    emailOrPhone: 'required'
                }
            });
            var valid = $("#forgotPassword").valid();
            if(!valid) {
                return false;
            }
            var emailOrPhone = $('#emailOrPhone').val();
            var pattern = /\w[-\w.+]*@([A-Za-z0-9][-A-Za-z0-9]+\.)+[A-Za-z]{2,14}/;
            var flag = 1;   //默认是手机号
            if(pattern.test(emailOrPhone)) {
                flag = 2;   //正则验证是邮箱
            }
            
            $.ajax({
                url: '/customer/forgot_ajax',
                data: {'emailOrPhone':emailOrPhone,'flag':flag},
                type: 'post',
                async:false,
                dataType: 'json',
                beforeSend: function(){
                    $('.forgot').append('<p class="reset-waiting">Wait a minute...</p>');
                },
                success: function(retdat) {
                    $('.reset-waiting').remove();
                    if(retdat.status == 1 && retdat.code == 200) {
                        $('.forgot').hide();
                        if(retdat.email == '') {
                            $('.reset-email').parent().parent().remove();
                        }
                        else {
                            $('.reset-email').html(retdat.email);
                        }

                        if(retdat.phone == '') {
                            $('.dis-phone').parent().parent().remove();
                        }
                        else {
                            $('.reset-phone').val(retdat.phone);
                            $('.dis-phone').html(retdat.dis_phone);
                            $('.country_code').val(retdat.country);
                        }
                        $('.reset-email-radio').prop('checked',true);
                        $('.reset').show();
                    }
                    else {
                        $('.forgot-login-error').show();
                        $('#emailOrPhone').val('');
                    }
                }
            });
        });

        $("#reset-back").click(function () {
            $('.reset').hide();
            $('.forgot').show();
        });

        /**选择email or phone进行验证*/
        $("#reset-continue").click(function () {
            var email = $('.reset-email').html();
            if($('.reset-email-radio').prop('checked')) {
                $.ajax({
                    url: '/customer/reset_email',
                    data: {'email':email},
                    type: 'post',
                    async:false,
                    dataType: 'json',
                    beforeSend: function(){
                        $('.reset').append('<p class="reset-waiting">Wait a minute...</p>');
                    },
                    success: function(retdat) {
                        $('.reset-waiting').remove();
                        if(retdat.status == 1 && retdat.code == 200) {
                            $('.reset-success h2').html(retdat.msg);
                            $('.reset').hide();
                            $('.reset-success').show();
                        }
                    }
                });
            }

            if($('.reset-phone-radio').prop('checked')) {
                var phone = $('.reset-phone').val();
                $.ajax({
                    url: '/customer/check_phone',
                    data: {'phone':phone,'email':email,'country_code':$('.country_code').val()},
                    type: 'post',
                    async:false,
                    dataType: 'json',
                    success: function(retdat) {
                        $('.reset-waiting').remove();
                        if(retdat.status == 1 && retdat.code == 200) {
                            $('.reset').hide();
                            $('#security_email').val(retdat.email);
                            $('#security_phone').val(retdat.phone);
                            $('.security').show();
                        }
                        if(retdat.status == 2 && retdat.code == 404) {
                            alert('Send failed, please try again.');
                        }
                    }
                });
            }
        });

        //手机号验证，输验证码
        $("#security-continue").click(function () {
            var security_code = $('#security_code').val();
            var security_phone = $('#security_phone').val();
            var security_email = $('#security_email').val();
            $.ajax({
                url: '/customer/security_code',
                data: {'security_code':security_code,'security_phone':security_phone,'security_email':security_email},
                type: 'post',
                dataType: 'json',
                success: function(retdat) {
                    if(retdat.status == 1 && retdat.code == 200) {
                        $('.security').hide();
                        $('#reset_token').val(retdat.token);
                        $('.new-password').show();
                    }
                    else {
                        alert('Your code is wrong.');
                    }
                }
            });
        });

        //没收到验证码
        $(".security-not-code").click(function () {
            $('.security').hide();
            $('.reset').show();
        });

        //重置密码
        $("#reset-new-passwd").click(function () {
            $('#new-password-form').validate({
                rules: {
                    newpassword: {
                        required: true,
                        minlength: 6
                    },
                    confirmpassword: {
                        required: true,
                        minlength: 6,
                        equalTo: "#newpassword"
                    }
                },
                messages: {
                    confirmpassword: {
                        equalTo: 'Please enter the same password again.'
                    }
                }
            });
            var valid = $("#new-password-form").valid();
            if (!valid) {
                return false;
            }
            var email = $('#security_email').val();
            var token = $('#reset_token').val();
            var newpassword = $('#newpassword').val();
            var confirmpassword = $('#confirmpassword').val();
            $.ajax({
                url: '/customer/reset_phone',
                data: {'email':email,'token':token,'newpassword':newpassword,'confirmpassword':confirmpassword},
                type: 'post',
                dataType: 'json',
                success: function(retdat) {
                    if(retdat.status == 1 && retdat.code == 200) {
                        $('.new-password').hide();
                        $('.reset-success').show();
                    }
                }
            });
        });

        //跳过设置新密码
        $("#skip-new-passwd").click(function () {
            $('.new-password').hide();
            $('.reset-success').show();
        });
    });

