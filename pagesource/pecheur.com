    <script type="text/javascript">
        var reloadAjaxCall = true;
        var tabFlash = new Array();

        $('body').append('<a href="#top" class="top_link" title="Revenir en haut de page">Haut</a>');

        $('.top_link').css({
            'position': 'fixed',
            'right': '20px',
            'bottom': '50px',
            'display': 'none',
            'padding': '20px',
            'background': '#1e98d2',
            'color': '#FFFFFF',
            '-moz-border-radius': '40px',
            '-webkit-border-radius': '40px',
            'border-radius': '40px',
            'opacity': '0.9',
            'z-index': '2000'
        });

        $(window).scroll(function () {
            posScroll = $(document).scrollTop();
            if (posScroll >= 550)
                $('.top_link').fadeIn(600);
            else
                $('.top_link').fadeOut(600);
        });

        var isAutoComplete = false;
        var reloadAutoComplete = false;
        var isOver = false;
        var prefixe = '';

        $(document).ready(function () {
            prefixe = $("#prefixe").text();
            if (prefixe == '/fr/fr')
                prefixe = '';

            $("#txtSearch").attr('placeholder', $("#placeholderRech").html());

            $('#txtSearch').on('keyup', function (e) {
                //entree, gauche, haut, droite, bas
                if (e.which == 13 || e.which == 27 || e.which == 37 || e.which == 38 || e.which == 39 || e.which == 40)
                    return;

                loadAutoComplete();
            });

            $('#txtSearch').on('focus', function () {
                if ($('#txtSearch').val().length >= 3)
                    $('#suggestion').show();
            });

            $(document).on('mouseenter', '.suggestion', function () {
                isOver = true;
            });

            $(document).on('mouseleave', '.suggestion', function () {
                isOver = false;
            });

            $('#txtSearch').on('focusout', function () {
                if (!isOver)
                    $('#suggestion').hide();
            });

            if ($("#menuBtn").length > 0) {
                $("#menuBtn").click(function () {
                    $("#contenuMenu").toggle();
                });
            }
        });

        function capitalizeFirstLetter(string) {
            return string.charAt(0).toUpperCase() + string.slice(1);
        }

        function loadAutoComplete() {
            if (isAutoComplete) {
                reloadAutoComplete = true;
                return;
            }

            $('.suggestion').remove();
            $('#suggestion').hide();

            if ($('#txtSearch').val().length >= 3) {
                if (!$('#imgRechercher').hasClass('loader'))
                    $('#imgRechercher').addClass('loader');

                isAutoComplete = true;
                var datas = "action=GetAutoSuggestion&search=" + $('#txtSearch').val();

                if (reloadAutoComplete) {
                    reloadAutoComplete = false;
                    isAutoComplete = false;
                    loadAutoComplete();
                    return;
                }

                $.post(prefixe + "/ControlsCall.html", datas, function (reponse) {
                    if (!$('#imgRechercher').hasClass('loader'))
                        $('#imgRechercher').addClass('loader');

                    if (reponse != null && reponse.answers != null) {
                        $.each(reponse.answers, function (index) {
                            var htmlValue = reponse.answers[index].HtmlValue;
                            var section = reponse.answers[index].section;

                            if (htmlValue != '') {
                                if (section == "univers") {
                                    $('#sugCategories').append(htmlValue);
                                }
                                else if (section == "marque") {
                                    $('#sugMarques').append(htmlValue);
                                }
                                else if (section == "produit") {
                                    $('#sugArticles').append(htmlValue);
                                }
                            }
                        });

                        if (!reloadAutoComplete) {
                            if ($('.univers').size() == 0)
                                $('#sugCategories').hide();
                            else
                                $('#sugCategories').show();

                            if ($('.marque').size() == 0)
                                $('#sugMarques').hide();
                            else
                                $('#sugMarques').show();

                            if ($('.produit').size() == 0)
                                $('#sugArticles').hide();
                            else
                                $('#sugArticles').show();

                            if ($('.marque').size() == 0 && $('.univers').size() == 0) {
                                $('.suggestion.produit').addClass('onlyProduct');
                                $('#sugArticles').addClass('onlyProduct');
                                $('.withoutOne').removeClass('withoutOne');
                            } else if ($('.marque').size() == 0 || $('.univers').size() == 0 || $('.produit').size() == 0) {
                                $('.pannelSugestion').addClass('withoutOne');
                                $('.suggestion').addClass('withoutOne');
                                $('.onlyProduct').removeClass('onlyProduct');
                            } else {
                                $('.onlyProduct').removeClass('onlyProduct');
                                $('.withoutOne').removeClass('withoutOne');
                            }

                            if ($('.suggestion').size() > 0)
                                $('#suggestion').show();

                            $('#imgRechercher').removeClass('loader');
                        }
                    }
                }, 'json')
				.done(function () {
				    isAutoComplete = false;

				    if (reloadAutoComplete) {
				        reloadAutoComplete = false;
				        loadAutoComplete();
				    }
				});
            }
        }
    </script>

    
	
    
