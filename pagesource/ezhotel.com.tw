	ajax.request('GET','/weather_data.php?loc='+w_loc+'&date='+date,showWeatherInfo,true,false);
	w_date=date;
	document.getElementById('dateMenu').style.display='none';
}
function showWeatherInfo(str){
	var weatherInfo=JSON.parse(str);
	document.getElementById('w_locname').innerHTML=weatherInfo.LocName;
	document.getElementById('w_date').innerHTML=weatherInfo.HotelDate;
	document.getElementById('w_ltp').innerHTML=weatherInfo.ltemp;
	document.getElementById('w_htp').innerHTML=weatherInfo.htemp;
	document.getElementById('w_desc').innerHTML=weatherInfo.content;
	document.getElementById('w_icon').src=w_iconurl+weatherInfo.d_icon+'.gif';
	document.getElementById('w_icon').title=weatherInfo.content;
}
function weatherOpenMenu(which){
	if(which==1){
