	function SETLOC2(theForm) {
 		var args = SETLOC2.arguments;
 		var j = 1;
		while (0 < theForm.hotelloc_2.options.length) {
			theForm.hotelloc_2.options[(theForm.hotelloc_2.options.length - 1)] = null;
		}
		eval("theForm.hotelloc_2.options[0]=" + "new Option('請選擇','0')");
 		for (var i=2; i < args.length; i+=3) {
	 		if (args[i]==args[1]){
 				eval("theForm.hotelloc_2.options[j]=" + "new Option('" + args[i+2] + "','"+ args[i+1] +"')");
 				j++;
 			}
 		}
 	}
 	function GetPassword(){
 		$('#Login').toggle();
 		$('#GetPassword').toggle();
 	}
 	function checkForm(){
    var theForm = document.member;
        if(theForm.username.value == ''){
        	window.alert('請輸入您的帳號!');
        	return false;
        }
    	if(theForm.password.value == ''){
    		window.alert('請輸入您的密碼!');
        	return false;
        }
       	return true;
     }
 	function checkForm2(){
	var theform = document.pass_porg;
		if (theform.username.value==""){
		   alert("請輸入您的身分證字號!"); //請輸入您的身份証字號
		   return false;
		}
		if (theform.birth_year.value == '' || theform.birth_month.value == '' || theform.birth_day.value == ''){
		   alert("請輸入您的生日!"); //請輸入您的生日
		   return false;
		}
		return true;
	}

	function Show_Message_Hide(message,speed,color){
		if(typeof(speed) == 'undefined') //預設速度3秒
			speed = 3000;
		if(typeof(color) == 'undefined') //預設紅色
			color = 'red';
		$('#message').html('<font color='+color+'>'+message+'</font>').show();
		setTimeout("$('#message').hide();",speed);
	}

	function Password_Check() { //查密碼
		var username = document.pass_porg.username.value;
		var birth_year = document.pass_porg.birth_year.value;
		var birth_month = document.pass_porg.birth_month.value;
		var birth_day = document.pass_porg.birth_day.value;

		if(username == ''){ //無輸入身分證字號
			Show_Message_Hide('請輸入您的身份證字號!');
		}else if(birth_year == ''){ //無輸入生日
			Show_Message_Hide('請輸入您的生日!');
		}else{/*使用者名稱檢查*/
			$.ajax({
			   type: "POST",
			   url: "user_search_pass_ajax.php",
			   data: "username="+username+"&birth_year="+birth_year+"&birth_month="+birth_month+"&birth_day="+birth_day,
			   success: function(msg){
			   	if(msg == 1){ //通過
			   		Show_Message_Hide('您的密碼已寄到您的信箱中!',3000,'blue');
			   	}else if(msg == 0){ //沒通過
			   		Show_Message_Hide('請確認您輸入的資料是否正確!');
			   	}
			   }
			});

		}
	}
	// JavaScript Document

</script>

