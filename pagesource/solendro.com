		//disable sub catagory menu
		$j('li.level0').removeClass('menu-active');
		$j('ol.nav-primary li.level0').show();
	});

	//menu back button
	$j('.nav-first .menu-nav-back').click(function() {
		$j('ol.nav-primary li.level0').show();
