					'onReady': sideBarOnPlayerReady,
					'onStateChange': sideBarOnPlayerStateChange
				}
			});
		}
	}
}
function sideBarOnPlayerReady(event) {
		 event.target.setVolume(0);
 event.target.playVideo();
}

var sidebar_done = false;
function sideBarOnPlayerStateChange(event) {
	if (event.data == YT.PlayerState.PLAYING && !sidebar_done) {
			sidebar_done = true;
	}
 event.target.setVolume(0);
}



