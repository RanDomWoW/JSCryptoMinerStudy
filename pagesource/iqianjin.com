<script>
    seajs.use(['$', 'dialog', 'units'],function($, Dialog, units) {
        var Tools = units.Tools;
        /**
         * 是否显示弹窗
         */
        function isRegularGlobalShow() {
           var url = '/userCenter/regular/exit/prompt';
           $.ajax({
              url: url,
              dataType: 'json',
              type: 'GET'
           }).done(function (data) {
              if(data.code === 1 && data.isShow === 1) {
                  regularGlobalExit();
              }
           })
        }

        /**
         * 爱月投退出弹窗
         **/
        function regularGlobalExit() {
            var dGlobal = Dialog({
                title: '退出爱月投',
                onshow: function () {
                    var $container = $("#global-regularExit"),
                            node = this.node,
                            $d = $(this.node);

                    this.content($container.html());
                    Tools.ui.init(node);

                    $d.find("#treatyBtn").on("click", function () {
                        var $this = $(this);
                        if($this.prop('checked')) {
                            $d.find("button").removeClass("disabled")
                        } else {
                            $d.find("button").addClass("disabled")
                        }
                    })
                },
                fixed: 'true',
                width: 590,
                okValue: '立即转投整存宝+',
                ok: function () {
                    var _that = this;
                    ajaxGlobalExit(_that, 1);
                    return false;
                },
                onclose: function () {
                    dGlobal.remove();
                },
                cancelValue: '不转投，直接退出',
                cancel: function () {
                    var _that = this;
                    ajaxGlobalExit(_that, 2);
                    return false;
                },
                skin: 'ui-dialog-iqianjin regular-exit-global'
            });

            dGlobal.showModal();

            //由于此处ok与cancel位置对调，需取消close点击触发cancel callback,只触发close.
            dGlobal._$('close').off('click').click(function () {
                dGlobal.close();
            });

            /**
             * 解决ESC退出整存宝的问题
             */
            $(document).off('keydown', this._esc);
            $(document).on("keydown", function (event) {
                var keyCode = event.keyCode;
                if (keyCode == 27) {
                    d.close();
                }
            });
        }

        /**
         * 爱月投清退
         * @param el    当前爱月投DOM
         * @param _d    弹窗
         * @param type  1: 加入理财计划 2: 直接退出
         */
        function ajaxGlobalExit(_d, type) {
            if($(_d.node).find("button").hasClass("disabled")) {
                return;
            }

            _d.button([
                {
                    value: '提交中...',
                    disabled: true,
                    autofocus: true
                }
            ]);

            $.ajax({
                url: '/userCenterRegular/eixt/all',
                dataType: 'json',
                type: 'POST',
                data: {
                    "exitType": type
                }
            }).done(function (data) {
                if (data.code !== 1) {
                    alert(data.message || '接口请求失败');
                    _d.remove();
                } else {
                    $(".changeMoney").text(data.bean.totalAmt)
                    regularGlobalChangeSuccess(type);
                    _d.remove();
                }
            });
        }

        /**
         * 退出成功
         * @param el
         * @returns {*}
         */
        function regularGlobalChangeSuccess(type) {
            var title = ['转投申请成功', '申请退出成功'];
            var $class = ['.changePlan', '.changeExit'];
            return Dialog({
                title: title[type - 1],
                onshow: function () {
                    var ele = $("#global-regularChangeSuccess");
                    $($class[type - 1]).removeClass("fn-hide");
                    this.content(ele.html());
                },
                fixed: 'true',
                width: 590,
                okValue: '我知道了',
                ok: function () {

                },
                skin: 'ui-dialog-iqianjin regular-change-success'
            }).showModal()
        }

        isRegularGlobalShow()
    })
</script>


