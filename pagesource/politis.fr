<script type="text/javascript">
  $(function () {

    // home carousel
    // ---------------------------------
      var sync1 = $(".hero-carousel"),
          sync2 = $(".hero-navigation-carousel"),
          duration = 2000,
          syncedSecondary = false;
      //init home carousel

      sync1.owlCarousel({
        loop: true,
        margin: 15,
        nav: true,
        dots: true,
        items: 1,
        autoHeight: false,
        autoplay: true,
        lazyLoad: true,
        autoplayHoverPause: true,
        smartSpeed: duration/2,
        fluidSpeed: duration,
        autoplaySpeed: duration,
        autoplayTimeout: 7000,
        navContainer: '.hero-navigation-arrows',
        navText: ['<svg width="100%" height="100%" viewBox="0 0 11 20"><path style="fill:none;stroke-width: .5px;stroke: #000;" d="M9.554,1.001l-8.607,8.607l8.607,8.606"/></svg>','<svg width="100%" height="100%" viewBox="0 0 11 20" version="1.1"><path style="fill:none;stroke-width: .5px;stroke: #000;" d="M1.054,18.214l8.606,-8.606l-8.606,-8.607"/></svg>'],
        responsiveRefreshRate : 200,
        responsiveClass:true,
        responsive:{
          0:{
            margin: 10,
            dots: true,
            nav: false
          },
          768:{
            margin: 15,
            dots: false,
            nav: true
          },
          1024:{
            margin: 0,
            dots: false
          }
        }
      }).on('changed.owl.carousel', syncPosition);

      sync2
        .on('initialized.owl.carousel', function () {
          sync2.find(".owl-item").eq(0).addClass("current");
        })
        .owlCarousel({
          loop: false,
          margin: 10,
          nav: false,
          dots: false,
          items: 4,
          lazyLoad: true,
          smartSpeed: duration/2,
          fluidSpeed: duration,
          mouseDrag: true,
          slideBy: 1, //or 4
          responsiveRefreshRate : 100,
          responsiveClass: true,
          responsive:{
            0:{
              items:1
            },
            768:{
              items:3,
              margin: 30
            },
            1024:{
              items:4,
              margin: 10
            }
          }
      }).on('changed.owl.carousel', syncPosition2);

      function syncPosition(el) {
        //if you set loop to false, you have to restore this next line
        //var current = el.item.index;

        //if you disable loop you have to comment this block
        var count = el.item.count-1;
        var current = Math.round(el.item.index - (el.item.count/2) - .5);
        if(current < 0) {
          current = count;
        }
        if(current > count) {
          current = 0;
        }
        //end block

        sync2
          .find(".owl-item")
          .removeClass("current")
          .eq(current)
          .addClass("current");
        var onscreen = sync2.find('.owl-item.active').length - 1;
        var start = sync2.find('.owl-item.active').first().index();
        var end = sync2.find('.owl-item.active').last().index();

        //console.log(current, count, onscreen, start, end);

        if (current > end) {
          sync2.data('owl.carousel').to(current, 200, true);
        }
        if (current < start) {
          sync2.data('owl.carousel').to(current - onscreen, 200, true);
        }
      }

      function syncPosition2(el) {
        if(syncedSecondary) {
          var number = el.item.index;
          sync1.data('owl.carousel').to(number, 200, true);
        }
      }

      sync2.on("click", ".owl-item img", function(e){
        e.preventDefault();
        var number = $(this).parents('.owl-item').index();
        sync1.data('owl.carousel').to(number, duration/2, true);
      });
      // end home carousel
      // -----------------

    });
</script>

  
