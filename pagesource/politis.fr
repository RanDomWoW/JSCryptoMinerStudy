<script type="text/javascript">
  var staticHost = "https://static.politis.fr";
  var publicDomain = "https://www.politis.fr/";

  $(function () {
    // recent articles carousel
    // ---------------------------------
    var duration = 1000;
    var recents_owl = $('.recent-articles-carousel');
    recents_owl.owlCarousel({
      loop: true,
      margin: 15,
      nav: true,
      dots: false,
      items: 3,
      smartSpeed: duration/2,
      fluidSpeed: duration,
      stagePadding: 90,
      mouseDrag: true,
      navText: ['<svg width="100%" height="100%" viewBox="0 0 11 20"><path style="fill:none;stroke-width: .5px;stroke: #424242;" d="M9.554,1.001l-8.607,8.607l8.607,8.606"/></svg>','<svg width="100%" height="100%" viewBox="0 0 11 20" version="1.1"><path style="fill:none;stroke-width: .5px;stroke: #424242;" d="M1.054,18.214l8.606,-8.606l-8.606,-8.607"/></svg>'],
      responsiveClass: true,
      responsive:{
          0:{
            items:1,
            nav: false,
            dots: true,
            stagePadding: 15
          },
          375:{
            items:1,
            nav:false,
            dots: true,
            stagePadding: 45
          },
          568:{
            items:2,
            nav: false,
            dots: true,
            stagePadding: 45
          },
          1024:{
            items:2,
            nav: true,
            dots: false,
            stagePadding: 0
          },
          1200:{
            items:3,
            nav: true,
            dots: false,
            stagePadding: 0
          },
          1640:{
            items:4,
            nav: true,
            dots: false,
            stagePadding: 0
          }
        }
    })
    // end recent articles carousel
    // -----------------


    // select section to display
    // ---------------------------------
    function loadSectionCarousel(id) {

      $.ajax({
        url: '/recents_by_section-'+ id +'.json',
        dataType: 'json',
        success: function(data) {
          //console.log(data.data);
          var content = '';
          for (i in data.data) {

            var item =  data.data[i];

            //building item DOM markup

            content += '<article class="article article-medium">\
                          <div class="article-content">';
            if (item.thumbnail_wide_medium && item.thumbnail_wide_medium !='') {
              content +='<a class="article-media" href="/'+item.permalink +'">';
              if (item.meta.layout =='video') {
                content += '<span class="play-video"><i class="fa fa-play"></i></span>';
              }
              if (item.meta.layout =='carousel') {
                content += '<span class="play-carousel"><i class="fa fa-camera"></i></span>';
              }
              content += '<img class="img-responsive" src="' + staticHost + item.thumbnail_wide_medium + '" alt="' + item.title +'" /></a>';
            }

            content +='<h1><a href="/'+ item.permalink +'">'+ item.title +'</a></h1>\
                        <p>';

            for (j in item.sections) {
              content += '<a class="meta-taxonomy taxonomy-' + item.sections[j].slug + '" href="/' + item.sections[j].permalink +'" title="Afficher tous les articles de la catégorie '+ item.sections[j].title +'">' + item.sections[j].title + '</a>';
            }

            for (j in item.authors) {
              content += '<span class="meta-author">';

              if(j==0) content += ' &nbsp;par ';
              if(j>0) content += ' et ';
              content += ' <a href="/' + item.authors[j].permalink + '" title="afficher tous les article rédigés par ' + item.authors[j].screenname + '">' + item.authors[j].screenname + '</a>';
              content += '</span>';
            }

            if(item.public) {
              content += '<br><span class="meta-free-article">accès&nbsp;libre</span>';
            }

            if(!item.thumbnail_wide_medium || item.thumbnail_wide_medium == '') {
              content += '<p class="excerpt">'+ item.headline +'</p>';
            }

            content += '</p>\
                      <p><span class="meta-actions"><a href="/'+ item.permalink +'" class="action-link"><i class="fa fa-share"></i>&nbsp;lire</a>&nbsp;<span class="action-link a2a_kit a2a_default_style" data-a2a-url="'+ publicDomain + item.permalink +'" data-a2a-title="'+ item.title +' via @Politis_fr"><a class="a2a_dd" href="https://www.addtoany.com/share"><i class="fa fa-share"></i>&nbsp;partager</a></span></span></p>\
                    </div>\
                  </article>';

            //end item DOM markup
          }
          recents_owl.trigger('replace.owl.carousel', content).trigger('refresh.owl.carousel');
        }
      });
    }

    function selectSection(id) {
      var li = $('li#recent_section_'+id, '.recent-articles-switcher')
      //change btn active
      $('.recent-articles-switcher li.active').removeClass('active');
      li.addClass('active');

      //change carousel section
      loadSectionCarousel(id);
    }

    //section next/prev
    totalSection = $('.recent-articles-switcher li').length - 2;

    $('.section-prev','.recent-articles-switcher').on('click', function(event){
      event.preventDefault();
      var current = $('.active', '.recent-articles-switcher');
      if(current.index() > 1) {
        current.prev().children('a').trigger('click');
      } else {
        $('.recent-articles-switcher li').eq(totalSection).children('a').trigger('click');
      }
    });

    $('.section-next','.recent-articles-switcher').on('click', function(event){
      event.preventDefault();
      var current = $('.active', '.recent-articles-switcher');

      if(current.index() < totalSection ) {
        current.next().children('a').trigger('click');
      } else {
        $('.recent-articles-switcher li').eq(1).children('a').trigger('click');
      }
    });

    //select trigger
    $('#select-category-switcher').on('change', function(event){
      var id = $(this).val();
      selectSection(id);
    });

    $('.recent-section-select-link').on('click', function(event){
      event.preventDefault();
      var id = $(this).parent('li').data('section-id');
      selectSection(id);
    })
    .first()
    .trigger('click'); //init fist load
});
</script>


