			}

			function clearTxtStazione(){
				if (strTxtStazione==$('#txtStazione').val()) 
					$('#txtStazione').val('');
			}

			function onSubmitCercaStazione(){
				if(($('#txtStazione').val()=="")||($('#txtStazione').val()=="digita il nome della stazione che ti interessa")){
					alert("Inserire la stazione");
					clearTxtStazione();
					$('#txtStazione').focus();
					return false;
				}
				
				var indexIdStazione = searchTokenId_IgnoreCase($('#txtStazione').val(),listaLabelStazioni);
				var idStazione = '';
				if ((indexIdStazione!=null)&&(indexIdStazione>=0)&&(indexIdStazione<listaStazioni.length)){
					idStazione = listaStazioni[indexIdStazione][0];
				}
				$('#idHdnCercaStazione').val(idStazione);
				
