					$('#formCercaStazione').attr('action', $('#partenzeAddrVal').val());
				}
				
				//alert('*** id testo stazione inserita:' + $('idHdnCercaStazione').value + ', txt stazione inserito:'+ $('txtStazione').value +'***');
				return true;
			}

			function searchTokenId(token, tokens){
				for (var i=0;i<tokens.length;i++) 
					if(tokens[i]==token) return i;

				return null;
			}

			function searchTokenId_IgnoreCase(token, tokens){
				var tokenLowerCase = token.toLowerCase();
				for (var i=0;i<tokens.length;i++){
					if(tokens[i]==token) return i;
					if((tokens[i].toLowerCase())==tokenLowerCase) return i;		
				}
				return null;
			}
			
			function adjustHourRangeParams(){
				var curSelectedValue = $('#dalle option:selected').val();
				$('#alle').val(curSelectedValue.substring(0,3)+"59");
