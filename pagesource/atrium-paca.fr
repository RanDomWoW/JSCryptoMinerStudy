							toggleAuthLoader();
							displayAuthError('\u0055\u006e\u0065\u0020\u0065\u0072\u0072\u0065\u0075\u0072\u0020\u0074\u0065\u0063\u0068\u006e\u0069\u0071\u0075\u0065\u0020\u0065\u0073\u0074\u0020\u0073\u0075\u0072\u0076\u0065\u006e\u0075\u0065\u0020\u006c\u006f\u0072\u0073\u0020\u0064\u0065\u0020\u006c\u0061\u0020\u0063\u006f\u006e\u006e\u0065\u0078\u0069\u006f\u006e\u002e\u0020\u004d\u0065\u0072\u0063\u0069\u0020\u0064\u0065\u0020\u0072\u00e9\u0065\u0073\u0073\u0061\u0079\u0065\u0072\u002e');
						}
					}
				});
			}
			
			function initLoginTicket() {
				A.io.request('/connexion/login',
				{
					dataType: 'html',
					method: 'GET',
					timeout: 10000,
					on: {
						success: function(event, transactionId, xhr) {
							var casLoginPage = A.Node.create(this.get('responseData'));
							
							// Si on est deja connecte on redirige l'utilisateur vers la connexion avec le service en param
							if(casLoginPage.one('div.alert-success')){
								A.clearInterval(initLoginTicketInterval);
								A.config.win.location = '/connexion/login?service=https%3a%2f%2fwww.atrium-paca.fr%2fc%2fportal%2flogin';
							} else{
								toggleMainLoader();
								toggleForm();
								
								var loginTicket = casLoginPage.one('form#fm1 input[name=lt]').val();
								var execution = casLoginPage.one('form#fm1 input[name=execution]').val();
								var eventId = casLoginPage.one('form#fm1 input[name=_eventId]').val();
								
								A.one('#_atriumconnexion_WAR_atriumportlet_lt').val(loginTicket);
								A.one('#_atriumconnexion_WAR_atriumportlet_execution').val(execution);
								A.one('#_atriumconnexion_WAR_atriumportlet__eventId').val(eventId);
							}
						},
						
						failure: function(){
							toggleMainLoader();
							displayAuthError('\u004c\u0065\u0020\u0073\u0065\u0072\u0076\u0069\u0063\u0065\u0020\u0064\u0027\u0061\u0075\u0074\u0068\u0065\u006e\u0074\u0069\u0066\u0069\u0063\u0061\u0074\u0069\u006f\u006e\u0020\u0064\u0027\u0041\u0054\u0052\u0049\u0055\u004d\u0020\u0065\u0073\u0074\u0020\u0069\u006e\u0064\u0069\u0073\u0070\u006f\u006e\u0069\u0062\u006c\u0065\u002c\u0020\u0069\u006d\u0070\u006f\u0073\u0073\u0069\u0062\u006c\u0065\u0020\u0064\u0065\u0020\u0073\u0065\u0020\u0063\u006f\u006e\u006e\u0065\u0063\u0074\u0065\u0072\u002e');
						}
					}
				});
			}
			
			initLoginTicket();
			var initLoginTicketInterval = A.setInterval(function(){
				toggleForm();
				toggleMainLoader();
				initLoginTicket();
			}, 300000);
			
		})();});
// ]]>
</script>









