        function sendToRD(event, fields, identifier, url, pushevent, convtoken, message) {
            var form = jQuery(event.target).closest('form');
            var data_array = form.find(':input').serializeArray();
            var data_obj = {};

            jQuery.each(data_array, function(i, value) {
              if (fields[this.name]) {
                if (this.name.indexOf('[]') > - 1) {
                  if (!data_obj[fields[this.name]]) {
                    data_obj[fields[this.name]] = [];
                  }
                  data_obj[fields[this.name]].push(this.value);
                }
                else {
                  data_obj[fields[this.name]] = this.value;
                }
              }
            });

            data_obj['identificador'] = identifier;
            data_obj['token_rdstation'] = convtoken;
            if(form[0].checkValidity()) {
              if (message !== undefined) {
                alert(message);
              }

              jQuery.ajax({
                type: 'POST',
                data: JSON.stringify(data_obj),
                dataType: 'json',
                url: 'https://www.rdstation.com.br/api/1.3/conversions',
                contentType: 'application/json; charset=utf-8',
                success: function() {
                  dataLayer.push({'event': pushevent});
                  dataLayer.push({'event': 'conversao'});
                  window.location.href = url;
                }
              });
            }
            else {
              if ( form.find(':input:checkbox:checked').length == 0 ) {
                form.find('.checkbox').addClass('outed');
              }
              form.find(':input').addClass('outed');
            }
        }

        function getToken(url) {
            var token;
            if (url.indexOf("/es/") > -1 || url.indexOf("/?locale=es") > -1) {
                token = '399ed981053f5a142893fbb7431e9606';
            }
            else if (url.indexOf("/en/") > -1 || url.indexOf("/?locale=en") > -1) {
                token = 'dd6f5b36ac6a2178f883ebddbf914c78';
            }
            else {
                token = '4ac98d510af23fd1b39770575544b8e0';
            }
            return token;
        }

