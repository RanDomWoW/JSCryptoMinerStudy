<script language="javascript" type="text/javascript">
    $(function () {

        var currentPageMenu = $(".MainMenu .MenuItem.Selected .MainMenuLink");

        function showSubMenu(id) {
            var MenuItemClass = ".MainMenu .MenuItem #" + id + ".MainMenuLink";
            var SubMenuClass = '.SubMenuContainer #SubMenu_' + id;

            if ($(SubMenuClass).is(":visible")) {
                $(SubMenuClass + " .SubMenuInner").fadeOut("fast");
                $(SubMenuClass).slideUp("slow", function () {
                    $(MenuItemClass).parent().removeClass("MenuSelected");
                    currentPageMenu.parent().addClass("Selected");
                });
            }
            else {
                currentPageMenu.parent().removeClass("Selected");
                $(SubMenuClass + " .SubMenuInner").show();
                $(MenuItemClass).parent().addClass("MenuSelected");
                $(SubMenuClass).slideDown("slow");

            }
        }

        $(".MainMenu .MenuItem .MainMenuLink").click(function () {
            if ($(this).hasClass("True")) {

                var id = $(this).attr("id");
                showSubMenu(id);
                return false;
            }
        });

        $(".SubMenuContainer .SubMenu .CloseSubMenu").click(function () {
            var id = $(this).attr("id");
            showSubMenu(id);
            return false;
        });

        $('.MobileMenuItem .MobileMenuLink .Item').click(function () {
            if ($('.MobileSubMenu').is(':visible')) {
                $('.MobileSubMenu').css('display', 'none');
            }
            else {
                $('.MobileSubMenu').css('display', 'block');
                return false;
            }
        });

        $('.MobileSubMenu .Item').each(function () {
            if ($(this).closest("li").find("ul").length > 0) {
                $(this).addClass("HasChildren");

                if ($(this).closest("li").find(".Selected").length > 0) {
                    $(this).closest("li").find("ul").slideDown("fast");
                    $(this).addClass("Expanded");
                }
            }
        });

        $('.MobileSubMenu .Item').click(function () {
            if ($(this).closest("li").find("ul").length > 0) {
                $(this).closest("li").find("ul").slideToggle("fast");
                $(this).toggleClass("Expanded");

                return false;
            }
        });

        $('.MobileSubMenu .OtherItem').each(function () {
            if ($(this).closest("li").find("ul").length > 0) {
                $(this).addClass("HasChildren");

                if ($(this).closest("li").find(".Selected").length > 0) {
                    $(this).closest("li").find("ul").slideDown("fast");
                    $(this).addClass("Expanded");
                }
            }
        });

        $('.MobileSubMenu .OtherItem').click(function () {
            if ($(this).closest("li").find("ul").length > 0) {
                $(this).closest("li").find("ul").slideToggle("fast");
                $(this).toggleClass("Expanded");

                return false;
            }
        });
    });
</script>


