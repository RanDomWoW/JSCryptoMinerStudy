								<div class="block map-box">
									<div class="legend">
										<span>Woongebouwen met:</span>
										<div class="map-legend"><img src="/Resources/Images/Maps/green-marker.png" /><span>Beschikbaar</span></div>
										<div class="map-legend"><img src="/Resources/Images/Maps/red-marker.png" /><span>Niet beschikbaar</span></div>
										<div class="clear"></div>
									</div>
									<div id="complex_map" style="height:500px;"></div>
								</div>
								<script>
        // <![CDATA[
        loadMaps('AIzaSyAa2iN6oB3yh5qexfwgojHLj5VMpY8RQxM');
        var map;

        function initMap() {
            map = new google.maps.Map(document.getElementById('complex_map'), {
                zoom: 12,
                center: { lat: 51.924303, lng: 4.4756033 },
                styles: getMapStyles(),
                scrollwheel: false,
            });
            google.maps.event.addDomListener(window, "resize", function () {
                google.maps.event.trigger(map, "resize");
            });
            loadData();
        }

        function loadData() {
            $.post('/api/aanbod/geo/woongebouwen/nl')
                .done(function (data) {
                    map.data.forEach(function (feature) {
                        map.data.remove(feature);
                    });
                    map.data.addGeoJson(data.geojson);
                    map.data.setStyle(function (feature) {
                        var available = feature.getProperty('available');
                        var color = 'red';
