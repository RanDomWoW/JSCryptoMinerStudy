 var pattern = /^[\u4E00-\u9FA5]{1,6}$/;
 return pattern.test(name);
}
 
 //验证手机号
function isPhoneNo(phone) { 
 var pattern = /^(((13[0-9]{1})|(14[0-9]{1})|(17[0]{1})|(15[0-3]{1})|(15[5-9]{1})|(18[0-9]{1}))+\d{8})$/; 
 return pattern.test(phone); 
}



$('.xes-btn').click(function(){
	var name = $('#studentname').val();
	var phone = $('#mobilephone').val();
	var stu = $('#select-grade').html();
	//验证
	// 判断名
	 if($.trim(name).length == 0) {
	    alert('姓名不能为空');
		return false;
	 } else if(isChinaName($.trim(name)) == false) {
	    alert('姓名不合法');
		return false;
	 }
	 
	 // 判断手机号码
	 if ($.trim(phone).length == 0) { 
	   alert('手机号没有输入');
	   return false;
	 } else if(!isPhoneNo($.trim(phone))) {
	   alert('手机号码不正确');
	   return false;
	 }
   
	var postArr = {'name':name,'mobile':phone,'stu':stu};
	$.ajax({
		type:"POST",
		url:"http://www.eswine.com/baoming/addinx.php",
		data:postArr,
		//async: false,
		datatype: "json",
		//成功返回之后调用的函数 
		           
		success:function(data){		
		    console.log(data);
            alert('预约成功,客服会第一时间与你联系');
			//window.location.href="";//跳转
		},
		//调用出错执行的函数
		error: function(){
			//请求出错处理
			alert('服务器忙,请稍后再试!');
		}         
	 });
	 
})
</script>

