$(function() {
	
	$('#loginForm').validator();
	$('#header_order_all').on('click', function() {
		buttonType = 'all';
		var $availableHeaderItem = $('input[name=availableHeaderItem]');
		saveHeaderOrderItemTemp($availableHeaderItem, "구매 가능한 상품이 없습니다."); // 구매 가능한 상품이 없습니다.
	});
	
	$("#loginForm input[name=op_password]").on("keyup",function(event){
		if (event.keyCode == 13) {
			loginSubmit();
		}
	});
});

//삭제
function deleteCartItem(cartId) {
	var param = {
		cartId : cartId
	};
	
	$.post('/cart/delete', param, function(resp){
		Common.responseHandler(resp, function(){
			location.reload();
		});
    });	
}

function saveHeaderOrderItemTemp($availableHeaderItem, message, loginCheck) {
	if ($availableHeaderItem.size() == 0) {
		alert(message);
		return;
	}
	
	var isLogin = 'false';
	loginCheck = loginCheck == undefined ? true : loginCheck;
	
	var cartIds = {};
	var isSuccess = true;
	$.each($availableHeaderItem, function(i){
		
		var quantity = parseInt($('#quantity-' + $(this).val()).val());
		if (quantity <= 0) {
			isSuccess = false;
		}
		
		cartIds[i] = $(this).val();
	});
	
	if (isSuccess == false) {
		alert(Message.get("M01590"));	// 상품의 수량을 확인해주세요. 
		return;
	}
	
	var params = {'cartIds' : cartIds};
	$.post('/cart/save-order-item-temp', params, function(response){
		Common.responseHandler(response, function(response) {
			
			if (isLogin == 'false' && loginCheck == true) {
				Common.popup(OP_HTTPS + '/users/login?target=order&popup=1&redirect=/order/step1', 'popup_login', 500, 380, 1);
			} else {
				location.replace('/order/step1');
			}
			
		}, function(response){
			
			//$('div.stock_state').html('<p>' + response.errorMessage + '</p>');
			alert(response.errorMessage);
			//location.reload();
		});
	}, 'json');
}

function loginSubmit() {
	var $id = $("#loginForm input[name=op_username]");
	var $pw = $("#loginForm input[name=op_password]");

	if ($id.val() == null || $id.val() == "") {
		alert("아이디를 입력해주세요.");
		$id.focus();
		return false;
	}
	
	if ($pw.val() == null || $pw.val() == "") {
		alert("비밀번호를 입력해주세요.");
		$pw.focus();
		return false;
	}
	
	$('#loginForm').submit();
}


