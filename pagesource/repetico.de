                $.ajax({
                    type: "POST",
                    url: "error",
                    data: "message=" + messageToAdmin,
                    success: function (data) {
                                                $("html").data("jserror", currentError);
                    },
                    error: function (err) {
                                            }
                });
            // }
        }
    }

    window.onerror = function (message, url, lineNumber, col, error) {
        sendError(message, url, lineNumber, col, error);
        return true;
    };

