						{type: "button", name: "proceed", value: "ورود", offsetTop: 1, offsetRight: 1}
					]}
				]);
				myForm.attachEvent("onFocus", function(name){
					if (!myIds[name]) return;
					if (!myPop2) {
						var id2 = [];
						for (var a in myIds) id2.push(a);
						myPop2 = new dhtmlXPopup({mode: "left", form: myForm, id: id2});
					}
					myPop2.attachHTML("<div style='margin: 5px 10px;'>"+myIds[name]+"</div>");
					myPop2.show(name);
				});
				myForm.attachEvent("onBlur", function(name){
					if (myPop2) myPop2.hide();
				});
				myForm.attachEvent("onButtonClick", function(name){
					if (name == "proceed") {
						allFunc(30,getEVal2('lgUsPh'),getEVal2('lgUsNa'),getEVal2('lgUsPw'));
						
						//myPop.hide();
						return;
					}
				});
			}
			if (myPop.isVisible()) { myPop.hide();
			} else {
				var x = dhx4.absLeft(inp); var y = dhx4.absTop(inp);
				var w = inp.offsetWidth; var h = inp.offsetHeight;
				myPop.show(x,y,w,h);
			}
		}
		
