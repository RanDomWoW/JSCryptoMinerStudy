	//================
	function QueryString(url) {
	  var query = window.location.search.substring(1);
	  var vars = query.split("&"), niaz=0;
	  for (var i=0;i<vars.length;i++) {
		var pair = vars[i].split("=");
		if(decodeURIComponent(pair[0])==url)niaz=1;
	  } 
	  return niaz;
	};
	//================
	function QuerySearch(url){
		var query = window.location.search.substring(1);
		var vars = query.split("&"), data='';
		for (var i=0;i<vars.length;i++) {
			var pair = vars[i].split("=");
			if(decodeURIComponent(pair[0])==url)data=pair[1];
		} 
	return data;
	};
