	//--------------------------- setResize
	var lastPageWidth=0;
	function setResize(test){
		if(test==7){
			var w=_("mainItemList_"+lastTabItem).clientWidth;
			if(lastPageWidth==w)return;lastPageWidth=w;
		}
		setItemSize(1);
	}
	//--------------------------- setResize
	function setItemSize(mode){ var pageWidth=0;
		pageWidth=_("mainItemList_"+lastTabItem).clientWidth;
		var dh=_("mainItemList_"+lastTabItem).offsetWidth-3, cnt;
		var proItem = getEl('item'), preWidth=0;
		if(proItem.length>0){
			itemWidth=parseInt($(proItem[0]).width());
			cnt=parseInt((dh+10)/itemWidth);
		}else{
			var width = (window.innerWidth > 0) ? window.innerWidth : screen.width;
			if(width<550){cnt=2;} else if(width<750){cnt=3;} else if(width<1000){cnt=4;} else if(width<1250){cnt=5;}
			else if(width<1500){cnt=6;} else if(width<2000){cnt=7;} else {cnt=8;} itemWidth=parseInt(dh/cnt);
		}
		lineCount=cnt;
		if(proItem.length>0)preWidth=parseInt($(proItem[0]).width());	//getEl('item').width(itemWidth);		getEl('item').height(itemWidth * 1.5);
		if(proItem.length>0 && Math.abs(preWidth - itemWidth)>1)setTimeout(function(){fireResize();} ,2 );
		setTimeout(function(){ setAllItemGallery(lastTabItem,2); }, 100);
		showOne_changeSize(mode);
	}
	//======================================================== showOne changeSize
	function showOne_changeSize(mode){
		var lastItem=getlastItemNum(), lastItemIndex;
		if(isEl(lastItem)){ 
			lastItemIndex=getProItemIndex(lastItem);
			var newSheetNum, preSheet;
			newSheetNum=getLineLastItemId(lastItemIndex);
			preSheet = getlastSheetNum();
			if(newSheetNum != preSheet){ 
				_('sheetItem'+preSheet).style.display='none';
				_('sheetItem'+newSheetNum).style.display='block'; 
				
				setOneItemContent2(newSheetNum, preSheet); 
				setOneItemContent(preSheet, '' , '' , '' , '');
				preSheet=newSheetNum;
				setTimeout(function() {selectItem(lastItem);}, 300);
				addClassPro('sheet', newSheetNum, "selectedSheet");
			}
			
			selectItem(lastItem);
			setResponsiveSheet(preSheet);
			if(mode==1){
				var target = getEl('item', lastItem); 
				scrlToItem(target.offset().top-46);
			}

		}
	}
	
