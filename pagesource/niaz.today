	//=========== 1:Back 2:Up
	function backUpItem(t) {
		var one1, one1_top, one2_top, v1, nId, tmpId, lastSheet, lastItem=getlastItemNum();  one1=getlastSheetNum(); one1_top=getEl('sheet', one1 ).offset().top;
		nId = getEl('item', lastItem);
		if(t==1){ nId = nId.prev(".sheetItem").prev(".proItem"); nId2 = nId.data('num');}
		if(t==2){
			for(var i=1; i<=lineCount; i++){ tmpId = nId.prev(".sheetItem").prev(".proItem"); if (!isEl(tmpId.data('num'))) break; nId = tmpId;}
			if(i==1)return;	nId2 = nId.data('num');
		}
		changeItem(nId2); lastSheet=getlastSheetNum();  one2_top=getEl('sheet', lastSheet).offset().top ;
		var htmlTop=getBodyTop();
		v1=parseInt(one1_top-htmlTop);if(one1!=lastSheet ) setTimeout(function(){ scrlToItem(one2_top-v1+10); } ,420 );
		selectItem(getlastItemNum());
		//alert( nId  );
		if(isEl(nId.prev(".divShowBack").data('mode')))getEl('btnShowBack').click();
	}
	//=========== 1:Next 2:Down
	function nextDownItem(t) {
		var one1, one2, one1_top, one2_top, v1, lastItem=getlastItemNum();
		one1=getlastSheetNum(); one1_top=getEl('sheet', one1 ).offset().top;
		nId = getEl('item', lastItem);
		if(t==1){ nId = nId.next(".sheetItem").next(".proItem"); nId2 = nId.data('num');}
		if(t==2){ 
			for(var i=1; i<=lineCount; i++){ tmpId = nId.next(".sheetItem").next(".proItem"); if (!isEl(tmpId.data('num'))) break; nId = tmpId;}
			if(i==1)return; nId2 = nId.data('num');
		}
		changeItem(nId2); 
		var htmlTop=getBodyTop(); one2=getlastSheetNum();
		if(one1!=one2 ){ setTimeout(function(){  one2_top=$('#sheetItem' + one2).offset().top; v1= parseInt(one1_top-htmlTop -0.5 ); scrlToItem(one2_top-v1); } ,420 );}
		selectItem(getlastItemNum());
		if(isEl(nId.next(".sheetItem").next(".divShowRemain").data('mode')))getEl('btnShowRemain').click();
	}
	
	//========================== get Line Last Item Id
	function getLineLastItemId(id){
		var proItem = getEl('sheet'), index=0,a;
		index=(parseInt((id-1)/lineCount)+1)*lineCount-1;
		if(index>proItem.length-1)index=proItem.length-1; 
		a=$(proItem[index]).data('num'); 
		return a;
	}
	//========================== Change Item
	function changeItem(newItem){
		if (!isEl(newItem))return;
		var newSheetNum, preSheet, itemIndex;
		itemIndex=getProItemIndex(newItem);
		newSheetNum=getLineLastItemId(itemIndex);	
		
		preSheet = getlastSheetNum();
		if(preSheet!=undefined && preSheet!='' && preSheet!=newSheetNum)closeDetail2();
		addClassPro('item', newItem, "selectedItem");
		setDataToSheet(newSheetNum);
		setResponsiveSheet(newSheetNum);
		getEl('sheet',newSheetNum).slideDown(400);
		addClassPro('sheet', newSheetNum, "selectedSheet");
	}
	//--------------------------------------- Get Current Page Element
	function getEl(name, id){var e='', el='#'+tabName+lastTabItem; 
		if( id == 'undefined' )id='';
		switch(name){
			case 'sheet':e='sheetItem'; break;
			case 'detail':e='sheetDtl'; break;
			case 'pointer':e='itemPointer'; break;
			case 'item':e='proItem'; break;
			case '':e=''; break;
			case '':e=''; break;
		}
		if(e=='')e=name;
		if(isEl(id))el+=' #'+e+id; else {el+=' .'+e;} 
		return $(el);	alert(el);
	}
