                <ul class="no-list-style post-tag">
                    <li style="color: #e54e53;font-size: 14px;">
                        <i class="fa fa-tag"></i>
                    </li>
                    {{each value.tags as tag}}
                    <li>
                        <a href="{{value.tag_url}}?tagid={{tag.id}}" target="_blank">{{tag.name}}
                        </a>
                    </li>
                    {{/each}}
                </ul>
            </div>
            <div class="post-other">
               <span class="author-img fl">
                  <a href="{{value.user_url}}"><img src="{{value.user.profile && value.user.profile.avatar ? value.user.profile.avatar :'/assets/front/img/avatar.jpg' }}" />{{value.author_name ?  value.author_name : value.user.nick_name}}</a></span>
               <span class="fl"><i class="glyphicon glyphicon-time"></i>{{value.public_at}}</span>
               <span><i title="阅读" class="glyphicon glyphicon-zoom-in"></i>{{value.views}}</span>
               <span><i title="评论" class="glyphicon glyphicon-comment"></i>{{value.comments_count}}</span>
               <span><i title="喜欢" class="glyphicon glyphicon-heart"></i>{{value.likes}}</span>
               <span><i title="收藏" class="glyphicon glyphicon-star"></i>{{value.collection_count}}</span>
            </div>
        </div>
    </div>
{{/each}}
</script>
