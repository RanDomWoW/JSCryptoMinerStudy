                      <script language="javascript">
                        function searchforformkyscall(){
                          var APP_CATEGORY;
                          var a = document.getElementsByName("APP_CATEGORY");
                          for(i=0;i<a.length;i++)
                          {
                            if(a[i].checked)APP_CATEGORY=" APP_CATEGORY="+a[i].value;
                          }
                          var leixing =document.getElementById("leixing").options[window.document.getElementById("leixing").selectedIndex].value;
                          document.getElementById("channelid").value=leixing;
                          var NA_TITLE =makeDocumentSentence("na_title","text","NA_TITLE");

                          var NA_GOV_NAME =makeDocumentSentence("na_gov_name","text","NA_GOV_NAME");

                          var NA_NO =makeDocumentSentence("na_no","text", "NA_NO");

                          var DT_DATE =getDateUtil("DOCRELTIME1","DOCRELTIME2","DT_DATE");

                          var temp ="";
                          temp = makeSearchSentence(NA_TITLE,APP_CATEGORY);
                          temp = makeSearchSentence(temp,NA_GOV_NAME);
                          temp = makeSearchSentence(temp,NA_NO);
                          temp = makeSearchSentence(temp,DT_DATE);
                          //alert(temp);
                          document.getElementById("searchword").value=temp;

                        }
                        //拼接成查询语句searchword 使用方法：info1数据1 info2数据2 数据1为第一份 数据2为第二份 数据2为没此需要累加的数据
                        function makeSearchSentence(info1,info2){
                          if(info1==""){
                            return info2;
                          }else if(info2==""){
                            return info1;
                          }else{
                            return info1+" and " +info2;
                          }
                        }

                        //得到对象信息并且根据列名 拼成查询条件 testid在html里面的id type类型分为"text" "select"
                        function makeDocumentSentence(testid,type){
                          var temp = "";
                          if(type=="text"){
                            temp = document.getElementById(testid).value;
                          }else if(type=="select"){

                            temp = document.getElementById(testid).options[window.document.getElementById(testid).selectedIndex].value;
                          }

                          return temp;
                        }
                        //startDatename开始时间  endDatename结束时间  clumname要检索的列名
                        function getDateUtil(startDatename,endDatename,clumname){
                          var temp = "";
                          var startdate = document.getElementById(startDatename).value
                          var enddate = document.getElementById(endDatename).value
                          if(startdate!=""){

                            temp = clumname+">="+startdate;
                          }

                          if(enddate!=""){
                            if(temp!=""){
                              temp = temp+" and "+clumname+"<="+enddate;
                            }else{
                              temp = clumname+"<="+enddate;
                            }
                          }

                          if(temp!=""){
                            temp = temp.replaceAll("-",".");
                          }

                          return temp;
                        }

                        //得到对象信息并且根据列名 拼成查询条件 htmlid在html里面的id type类型分为"text" "select"
                        function makeDocumentSentence(htmlid,type,sqlclumname){
                          var temp = "";
                          if(type=="text"){
                            temp = document.getElementById(htmlid).value;
                          }else if(type=="select"){
                            temp = document.getElementById(htmlid).options[window.document.getElementById(htmlid).selectedIndex].value;
                          }
                          if(temp!=""){
                            temp = " "+sqlclumname+"='"+temp+"' ";
                          }
                          return temp;
                        }

                        String.prototype.replaceAll = function (AFindText,ARepText){
                          raRegExp = new RegExp(AFindText,"g");
                          return this.replace(raRegExp,ARepText);
                        }
