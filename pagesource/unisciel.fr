		$item.css({top: newPositionTop });
	}

	$.fn.arrowRotate = function(speed) {
		var $item = $(this);
		var data = $(this).data("animdata");
		var $parentBlock = $('.effectparentrow .fullwidth-block');
		var angel = data.rotatespeed*360*$(window).scrollTop()/1000;
		$item.css({
