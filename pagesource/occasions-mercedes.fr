                    //}
                    

                    //$element.find('min-slider-handle').prop('aria-valuenow', $min).change();
                    //$element.find('max-slider-handle').prop('aria-valuenow', $max).change();
                   
                    return;
                }
                $element = $element.parent();
                
            }

            updateDisplayRefreshAll();
        }


        function updateDisplayRefreshAll()
        {
        	var onVisible = false;
        	$('.fa-eraser').each(function() {
        		if ($(this).css('display') != 'none') {
        			onVisible = true;
        			return false;
        		}
        	});

        	if (onVisible) {
        		$('#refresh-all').show();
        	} else {
        		$('#refresh-all').hide();
        	}
        }
    </script>




