        function filterDepartment() {
            if (checkboxModels != null) {
                inputDepartment = $("#select-department").val();
                reloadResults();
            }
        }

        function placeTrierLaListe()
        {
            var $trier = $('#tab-trier');
            if (!isMobileView()) {
                if ($trier.css('position') == 'fixed') {
                    var right = ($(window).width() - 1000) / 2 + 15;
                    if (right < 0) {
                        right = 0;
                    }
                    $trier.css('right', right);
                    return;
                }
            }
            $trier.css('right', 15);
        }


        function controlAccordionHeight()
        {
            var maxHeight;

            if (isMobileView()) {
                maxHeight = $(window).height() - 270 - 60;	// Title + Boutton appliquer
                $('#tab-filtrer').css('top', $('.navbar').outerHeight());
                $('#tab-trier').css('top', $('.navbar').outerHeight());
            } else {
                maxHeight = $(window).height() - ($('footer').height() + $('.navbar-container').height() + ($('#tab-filtrer .panel-heading:eq(0)').height() * 3));
                maxHeight -= 350;	// Trier/Filtrer barre + Title
                $('#tab-filtrer').css('top', 'auto');
                $('#tab-trier').css('top', 'auto');
            }


            $('#tab-filtrer .panel-body').css({
                maxHeight: maxHeight+'px',
                overflowY: 'auto'
            });
        }

        var xhrLoadResults = null;

        function reloadResults() {
            //$(window).scrollTop(0);

            currentPage = 0;

            var url = '/pagevo'.split('?')[0];
            url += "?page=0";
            url += getCriteresQuerystring();

            if (xhrLoadResults) {
                xhrLoadResults.abort();
            }

            $('#results-box').loader('show');
            xhrLoadResults = $.ajax({
                url: url,
                success: function (data) {
                    $('#results-box').html(data);
                    $('#results-box').loader('hide');
                    updateAddToFavorisIcons();
                    xhrLoadResults = null;
                    //alert('done');
                }
            });
        }

        function updateTotalCounter(totalCount) {
            $('.vehicule-count-message').html(totalCount + " Véhicule" + (totalCount > 1 ? "s" : ""));
        }


        function getCriteresQuerystring()
        {
            var query = "";
            //for (var i = 0; i < checkboxMarques.length; i++) {
            //    if (checkboxMarques[i].checked) {
            //        query += '&marques=' + encodeURIComponent(checkboxMarques[i].value);
            //    }
            //}
            for (var i = 0; i < checkboxModels.length; i++) {
                if (checkboxModels[i].checked) {
                    query += '&modeles=' + encodeURIComponent(checkboxModels[i].value);
                }
            }

            for (var i = 0; i < checkboxDistributeurs.length; i++) {
                if (checkboxDistributeurs[i].checked) {
                    query += '&distributeurs=' + encodeURIComponent(checkboxDistributeurs[i].value);
                }
            }

            if (inputDepartment != null) {
                for (var i = 0; i < inputDepartment.length; i++) {
                    query += '&Department=' + encodeURIComponent(inputDepartment[i]);
                }

            }

            var myPriceSlider = $("#Slide1").slider();
            var slidervalue = myPriceSlider.slider('getValue');
            var $prixMin=slidervalue[0];
            var $prixMax=slidervalue[1];

            query += '&MinPrice=' + encodeURIComponent($prixMin);
            query += '&MaxPrice=' + encodeURIComponent($prixMax);


            var myKMSlider = $("#Slide2").slider();
            slidervalue = myKMSlider.slider('getValue');

            var $kMMin=slidervalue[0];
            var $kMMax=slidervalue[1];

            query += '&MinKM=' + encodeURIComponent($kMMin);
            query += '&MaxKM=' + encodeURIComponent($kMMax);

            var myYearSlider = $("#Slide3").slider();
            slidervalue = myYearSlider.slider('getValue');

            var $anneeMin=slidervalue[0];
            var $anneeMax=slidervalue[1];

            query += '&MinAnnee=' + encodeURIComponent($anneeMin);
            query += '&MaxAnnee=' + encodeURIComponent($anneeMax);




            // Orders
            var orderRadio = $('[name=orderby]:checked');
            if (orderRadio.length) {
                var datas = orderRadio.val().split('-');
                if (datas.length == 2) {
                    query += '&OrderColumn='+datas[0];
                    query += '&OrderBy='+datas[1];
                }
            }

            return query;
        }

        var currentPage = 0;

        function loadNextResults(page)
        {
            var url = '/pagevo'.split('?')[0];
            url += "?Page=" + (currentPage+1);
            url += getCriteresQuerystring();

            $('#results-box').loader('show');

            $.get(url, function(data) {
                $('#results-box').loader('hide');
                $('#results-box').find('.plus-resultats-container:eq(0)').remove();
                $('#results-box').append(data);
                currentPage++;
            });
        }

        $(document).ready(function () {
            $('#add-remove-vehicule-compare').on('click', addOrRemoveVehiculeCompare);
        });

        function addOrRemoveVehiculeCompare() {
            if ($(this).parent('a').hasClass('selected-vehicule')) {
                $(this).parent('a').removeClass('selected-vehicule');
            }
            else {
                $(this).parent('a').addClass('selected-vehicule');
            }
        };

        function thousandSeparator(x) {
            return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, " ");
        }

        function gotoComparateur() {
            if (getCountFavoris() > 0)  {
                document.location='/comparateur';
            }
        }



		function initFiltersCounter()
		{

			$('.filter-counter').each(function () {
				var $filterCounter = $(this);
				var $inputs = $(this).find('input');
				var $select = $(this).find('select');
                var $value = $filterCounter.find('.filter-counter-value:eq(0)');
				$select.change(function () {
					var $eraser = $filterCounter.find('.fa-eraser');

                    var count = $(this).val();
					if (!count || !count.length) {
						$value.html('');
						$eraser.hide();
					} else {
						$value.html('(' + count.length + ')');
						$eraser.show();
					}

					updateDisplayRefreshAll();
				});

				$inputs.change(function () {
					var $eraser = $filterCounter.find('.fa-eraser');

					var count = $inputs.filter(':checked').length;
					if (count == 0) {
						$value.html('');
						$eraser.hide();
					} else {
						$value.html('(' + count + ')');
						$eraser.show();
					}

					updateDisplayRefreshAll();
				});
				/// Force la mise à jour du compteur au chargement de la page
				if ($inputs.length) {
					$inputs.first().change();
                }
			});

			$('.filter-parameter').each(function() {

				$(this).find('.fa-eraser').hide();

				var $filterParameter = $(this);
				var $min = $(this).find('.min-slider-handle');
				var $max = $(this).find('.max-slider-handle');
				var $input = $(this).find('input');
				var $value = $(this).find('.filter-parameter-value');
				$input.change(function () {
					
					var $eraser = $filterParameter.find('.fa-eraser');

					var minval = parseInt($min.attr('aria-valuenow'));
					var maxval = parseInt($max.attr('aria-valuenow'));
					var min = parseInt($min.attr('aria-valuemin'));
					var max = parseInt($max.attr('aria-valuemax'));

					var unit;
					switch ($(this).attr('id')) {
						case 'Slide1':
							unit = '€';
							break;
						case 'Slide2':
							unit = 'km';
							break;
						default:
							unit = '';
					}
					if (minval > min || maxval < max) {
						$value.html('<span style="font-size:13px;">(de ' + thousandSeparator(minval) + unit + ' à ' + thousandSeparator(maxval) + unit + ')</span>');
						$eraser.show();
					} else {
						$value.html('');
						$eraser.hide();
					}

					updateDisplayRefreshAll();
				});
            
			});
		}

