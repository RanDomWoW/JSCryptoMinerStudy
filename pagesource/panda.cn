        function lanrenzhijia(_this) {
            $(_this).each(function () {
                var $this = $(this);
                var theMenu = $this.find(".SideMenu");
                var tarHeight = theMenu.height();
                theMenu.css({ height: 0 });

                $this.hover(
                    function () {
                        $(this).addClass("on");
                        theMenu.stop().show().animate({ height: tarHeight }, 400);
                        if ($this.find(".SideMenu dd").size() == 0) { theMenu.css("display", "none") }
                    },
                    function () {
                        $(this).removeClass("on");
                        theMenu.stop().animate({ height: 0 }, 400, function () {
                            $(this).css({ display: "none" });
                        });
                    }
                );
            });
        }
    </script>
