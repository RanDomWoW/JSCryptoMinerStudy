function get_fromdatetime_value()
{
  var fromdatetime_picker =  document.getElementById("datetimepicker_fromdate").value;
  var fromdatetime_days = document.getElementById('dayspicker').value;
  if (fromdatetime_days)
  {
    var fromdatetime_days = parseInt(fromdatetime_days);
    var currentDate = new Date();
    var nextdays = currentDate.removeDays(fromdatetime_days); 
    var outputdate = nextdays.getDate() + '.' + (nextdays.getMonth() + 1) + '.' + nextdays.getFullYear() + ' ' + nextdays.getHours() + ':' + nextdays.getMinutes();
    return outputdate;
  }
  return fromdatetime_picker;  
}
function strpos(haystack, needle, offset) {
  var i = (haystack + '').indexOf(needle, (offset || 0));
  return i === -1 ? false : i;
}

Date.prototype.removeDays = function(days) {
    this.setDate(this.getDate() - days);
    return this;
};

function get_todatetime_value()
{ 
  var todatetime_picker = document.getElementById('datetimepicker_todate').value;
  return todatetime_picker;
}

function reset_datetimepicker(what)
{
	if (what == 'fromdate')
	{
		document.getElementById("datetimepicker_fromdate").value = '__/__/____ __:__';
	}
	else if (what == 'todate')
	{
		document.getElementById("datetimepicker_todate").value = '__/__/____ __:__';
	}
}
function sc_removedatetime_value()
{
  var fromdate = document.getElementById("datetimepicker_fromdate");
  var todate = document.getElementById("datetimepicker_todate");
  var daysbased = document.getElementById("dayspicker");
  
  daysbased.onclick = function () {
        document.getElementById("datetimepicker_fromdate").value = '__/__/____ __:__';
        document.getElementById("datetimepicker_todate").value = '__/__/____ __:__';
   }
  fromdate.onclick = function () {
        document.getElementById("dayspicker").value = '';
   }
    todate.onclick = function () {
        document.getElementById("dayspicker").value = '';
   }     
}
