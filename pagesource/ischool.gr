<script>
function mobile_reset_specific_forum_select()
{
jQuery('#mobile_in_this_forumid').prop('selectedIndex',0);
}

function mobile_sc_do_livesearch()
{
			var search_query = jQuery("#mobile_query").val();
			if (search_query == '')
			{
				alert('Πληκτρολογήστε ένα κριτήριο αναζήτησης στο πεδίο.');
				return false;
			}
			
			if (search_query.length <= 3)
			{
				alert('Το κριτήριο αναζήτησης πρέπει να έχει μήκος τουλάχιστον 4 χαρακτήρες.');
				return false;			
			}

			if(jQuery('#mobile_subforums_include').is(':checked')) 
			{
				var subforums_include = 1;
			}
			else
			{
				var subforums_include = 0;
			}

			var who_username = document.getElementById("mobile_who_username").value;
			var inforumid = document.getElementById("mobile_where_forumid").value;
			jQuery('#mobile_livesearch_progress_img').fadeIn();
			jQuery.post("ajaxlivesearch.php",
						{
							"do": "dolivesearch",
							keyword: search_query,
							securitytoken: "guest", 
							search_phrase: jQuery("#mobile_search_phrase").val(), 
							search_order: jQuery("#mobile_search_order").val(), 
							tab: curTab,
							who_username: who_username, 
							forumid: inforumid,
							ismobile: 1,
						    humanverify: jQuery('#mobilelivesearch_captcha_form').serialize(), subforums_include: subforums_include
						},
						function(data) {
							if (data == 'hv_error')
							{
								jQuery("#mobile_search_results_box").html('Δεν απαντήσατε σωστά στην ερώτηση επιβεβαίωσης.');
								sc_mobile_gethumanverification();
							}
							else
							{
								jQuery("#mobile_search_results_box").html(data);
								
									sc_mobile_gethumanverification();
								
							}
							jQuery('#mobile_livesearch_progress_img').fadeOut();
						},
						"html"
					); 

}
	jQuery(document).ready(function() {
	jQuery("#mobile_header_quicksearch_form").submit(function(e) {
		// Stop the form submitting
		e.preventDefault(); 
		mobile_sc_do_livesearch();
	});

	jQuery("#mobile_query").click(function() {
	
		sc_mobile_gethumanverification();
	
	jQuery('#mobile_livesearch_progress_img').hide();
	jQuery("#mobile_query").focus();
		});
    jQuery('#mobile_search_button').click(function(event) {
			mobile_sc_do_livesearch(); 
        });			
});

function sc_toggle_visible_status()
{
    jQuery.ajax({url: "ajax.php?do=togglestatus", success: function(result){
       document.location.reload(true);
    }});
}
function sc_mobile_gethumanverification()
{
        jQuery.ajax({
            url : "ajaxlivesearch.php",
            type: "POST",
            data: {
                securitytoken: 'SECURITYTOKEN',
                do:'gethumanverify'
            },
            dataType: "html",
            success: function(data, textStatus, jqXHR){
              if (data) {
                jQuery('#sc_search_mobile_captcha').html(data);
                jQuery('#sc_search_mobile_captcha').show();
              }
              else {
                alert('Human Verification System Error');
              }
            }
        });
}   
</script>



