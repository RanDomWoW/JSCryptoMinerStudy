    function getBtn(btn){

       

        var home_btn='<a href="../" class="btn-evisa btn bg_parent color_white">Home</a>';
        var resend_btn='<a class="btn-evisa btn bg_parent color_white" onclick="send_email(\'submit\')">Resend email</a>';
        var back_btn='<a class="btn-evisa btn bg_parent color_white" onclick="prev();">Back</a>';
        var new_btn ='<a id="new_app_btn" class="btn-evisa btn bg_parent color_white" onclick="new_app();">Additional Applicant</a>';
        var new_btn_o='<a class="btn-evisa btn bg_parent color_white" href="application_new">New Form</a>';
        var next_btn='<a class="btn-evisa btn bg_parent color_white" onclick="next();">Next</a>';
        var apply_btn='<a class="btn-evisa btn bg_parent color_white" onclick="Security_submit();">Apply now</a>';
        var save_btn='<a id="submit" class="btn-evisa btn bg_parent color_white" onclick="submit();">Submit</a>';
        var confirm_btn='<a id="submit" class="btn-evisa btn bg_parent color_white" onclick="goToPayment(\'payment\');">Confirm Payment</a>';
        var cancel_btn='<a class="btn-evisa btn bg_parent color_white" onclick="delete_app();">Cancel</a>';

        
          
        if(btn=='home_btn')result=home_btn;
        else if(btn=='resend_btn')result=resend_btn;
        else if(btn=='back_btn')result=back_btn;
        else if(btn=='new_btn')result=new_btn;
        else if(btn=='new_btn_o')result=new_btn_o;
        else if(btn=='next_btn')result=next_btn;
        else if(btn=='apply_btn')result=apply_btn;
        else if(btn=='save_btn')result=save_btn;
        else if(btn=='confirm_btn')result=confirm_btn;
        else if(btn=='cancel_btn')result=cancel_btn;
        else result='';
          return  result;
      }
      function function_alert(message,country_id){
        type_alert='red';
        message_content=message;
        
        if (message=="is_cambodia") {
            type_alert='blue';
            message_content="You are already in the country so you can&#039;t apply eVisa. Because you are not entering again through the entry point. Please go to immigration depatment for tourist and business visa and at MFAI for NGO."; 
        }else if (message=="countryExemtion") {
            type_alert='blue';
            message_content=CountryExemptionMessage(country_id); 
        }else if (message=="emailSendsubmit") {
            type_alert='green';
            message_content="A referent number has been sent to your primary e-mail address.In some cases, e-mail messages might be categorized as “junk” or “spam” messages and sent to these folders automatically. If you did not receive the e-mail message please click button &quot;Resend email&quot;."; 
        }else if (message=="emailSendpayment") {
            type_alert='green';
            message_content="Receipt email has been sent!Please check your email."; 
        }else if(message=="maxApp"){
            type_alert='red';
            message_content="Sorry! you can apply only 10 applications per 1 request."; 
        }else if(message=="agree"){
            type_alert='red';
            message_content="please check agree on our terms and conditions"; 
        }else if(message=="app_existed"){
            type_alert='red';
            reference=$('#application_record').attr('reference_no');
            date=$('#application_record').attr('register_date');
            message_content="Your application already submited since "+date+" and here is your reference no  \" <span class='text-select-able'>"+reference+"</span> \". <a onclick='$(`#check_change`).submit();' class='contact'> Please go to check and change to continue your payment.</a>";
        }
        // alert(type_alert+'\n'+message_content);
        $.alert({
            title: "Message",
            columnClass: 'col-xs-12 col-sm-8 col-sm-offset-2 col-md-6 col-md-offset-3',
            type: type_alert,
            content: message_content,
            buttons: {
              Close:{
                text: "Close",
                btnClass: 'btn-danger btn btn-evisa pull-right ',
                action: function () {
                  if(message=='app_existed'){
                    $('#check_change').submit();
                  }
                }
              }
            }
        });
     }
     function condition(){
        type_alert='blue';
        
        $.alert({
            title: $('#tearm_name').html(),
            columnClass: 'col-xs-12 col-sm-8 col-sm-offset-2',
            type: type_alert,
            content: $('#tearm_description').html(),
            buttons: {
              Close:{
                text: "Close",
                btnClass: 'btn-danger btn btn-evisa pull-right ',
              }
            }
        });
     }
    
     

    </script>
