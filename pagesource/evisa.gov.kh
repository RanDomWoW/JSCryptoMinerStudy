    function autoSelectDate(){
      d=$('select[name=issueD]').val();
      m=$('select[name=issueM]').val();
      y=$('select[name=issueY]').val();

      if (d!='' && m!='' && y!='') {
      date=new Date(y+'/'+m+'/'+d);
      date.setDate(date.getDate() - 1);
      date.setFullYear(date.getFullYear() + 10);
      $('select[name=expiryD]').val(date.getDate());
      $('select[name=expiryM]').val(date.getMonth()+1);
      $('select[name=expiryY]').val(date.getFullYear());
      }

    }
    
