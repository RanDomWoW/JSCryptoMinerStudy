    <script type="text/javascript">
      function function_confirm (message , action ) {
        if(!action){
          action='';
        };
        if (message=="entryDate") {
            btn_name="Continue"; 
            message_content="Your entry date is too sort! We need​​ at least 3 working days in proccess eVisa approval. For more information contact us (+855)23 214 441/(+855)23 216 122."; 
        }
        else if (message=="removeBlacklist") {
            btn_name="Continue"; 
            message_content="Please, double check your application. All applications that been notice any problem will be remove. Are you still want to submit ?"; 
        }
        else if(message=="confirmDelete"){
            btn_name="Delete"; 
            message_content="<div style='text-align: center;'>Are you sure to delete this application?</div>"; 
        }
       $.confirm({
        title: "Message",
        columnClass: 'col-xs-12 col-sm-8 col-sm-offset-2 col-md-6 col-md-offset-3',
        content: '<div> '+message_content+' </div>',
        buttons: {
            cancel: {
                text: "Cancel",
                btnClass: 'btn-danger btn btn-evisa',
                action: function () {
                  val='cancel';
                  if(message=='entryDate'){
                    validateForm('step-1',val);
                  }else if(message=='removeBlacklist'){
                    //submit(val);
                    confirm_duplicate(val);
                  }else if(message=='confirmDelete'){
                  }
                }
            },
            confirm:  {
                text: btn_name,
                btnClass: 'btn-primary btn btn-evisa',
                action: function () {
                  val='go';
                  if(message=='entryDate'){
                    validateForm('step-1',val);
                  }else if(message=='removeBlacklist'){
                    //submit(val);
                    confirm_duplicate(val);
                  }else if(message=='confirmDelete'){
                    if (action!='') {
                      $('#'+action).submit();
                    }else{
                      delete_app();
                    }
                  }
                }
            },
        }
      });
      }
    </script>