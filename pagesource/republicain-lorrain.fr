function setUpDataDisplay(town, todayForecast, nextDaysForecasts) {
		$(".imagemeteo > img").attr('src', CreateImageUrl(todayForecast.WeatherSymbol));
		$(".imagemeteo > img").attr('alt', todayForecast.WeatherTitle);
		$("#meteoNewsTownName").empty();
		$("#meteoNewsTownName").append(town);
		$("#meteoNewsDayWeather").empty();
		$("#meteoNewsDayWeather").append(todayForecast.WeatherTitle);
		$(".temp_maxi").empty();
		$(".temp_maxi").append(todayForecast.AfternoonTemperature);
		$(".temp_mini").empty();
		$(".temp_mini").append(todayForecast.MorningTemperature);
		$(".blocprevision").empty();
		$(".blocprevision").append(setUpNextDaysForecastsDisplay(nextDaysForecasts));
	}

function setUpNextDaysForecastsDisplay(nextDaysForecasts) {
	var result ="";
    
	for (var i in nextDaysForecasts) {
		result = result.concat('<li>');
		result = result.concat('<p class="jour">nextDaysForecasts[i].WeatherDate</p>');
		result = result.concat('<a title="Voir les prévisions" href="/meteo/previsions-detaillees?im=57463" class="imageprevmeteo"><img  alt="' + nextDaysForecasts[i].WeatherTitle + '" src="' + CreateImageUrl(nextDaysForecasts[i].WeatherSymbol) + '" /></a>');
		result = result.concat('<div class="temperature"><span class="temp_mini">' + nextDaysForecasts[i].MorningTemperature + '</span> / <span class="temp_maxi">' + nextDaysForecasts[i].AfternoonTemperature +' </span></div>');
    result = result.concat('</li>');
  }

  return result;
}

function CreateImageUrl(symbol) {
  return 'https://cdn-files.prsmedia.fr/files/ERV/common/images/GMEX/' + symbol + '.png';
}

function LoadWeatherForecast(codeInsee, numberOfDaysDisplayed, isInitialPageLoad) {

    $ .ajax({
        type: "GET",
        url: "/services/gmex/weatherforecastloadingservice/getweatherforecastget",
        cache: true,
        data: "InseeCode=" + codeInsee + "&NumberOfDays=" + numberOfDaysDisplayed + "&IsInitialPageLoad=" + isInitialPageLoad,
        beforeSend: function () {
            $(".mod.GMEX_weather").css('display', 'none');
        },
        success: function (data) {
            if (data != null && data.Forecasts.length >= 1) {
				setUpDataDisplay(data.Town, data.Forecasts.shift(), data.Forecasts);
				$(".mod.GMEX_weather").css('display', 'block');
			}
			else {
				$(".mod.GMEX_weather").css('display', 'none');
			}
        },
        error: function () {
            $(".mod.GMEX_weather").css('display', 'none');
        }
    });
}

function LoadWeatherForecastCookie(codeInsee, numberOfDaysDisplayed, isInitialPageLoad, link) {

    $ .ajax({
        type: "GET",
        url: "/services/gmex/weatherforecastloadingservice/getweatherforecastget",
        cache: true,
        data: "InseeCode=" + codeInsee + "&NumberOfDays=" + numberOfDaysDisplayed + "&IsInitialPageLoad=" + isInitialPageLoad,
        beforeSend: function () {
            $(".mod.GMEX_weather").css('display', 'none');
        },
        success: function (data) {
            if (data != null && data.Forecasts.length >= 1) {
				setUpDataDisplay(data.Town, data.Forecasts.shift(), data.Forecasts);
				$(link).attr('href', '/meteo/previsions-detaillees?im=' + codeInsee);
				$(".mod.GMEX_weather").css('display', 'block');
			}
			else {
				$(".mod.GMEX_weather").css('display', 'none');
			}
        },
        error: function () {
            $(".mod.GMEX_weather").css('display', 'none');
        }
    });
}

function GetLocalisationFromCookie(def) {

	var loc = null;

    var c = getCookie("WeatherForecast");

    if (c != null) {
        arr = c.split('=');
        if (arr.length == 2) loc = arr[1];
    }

    if (loc == null) {
        return def;
    }
    else {
        return loc;
    }
}

function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
    }
    return "";
} 

