<script>
                    var CurrentDate = new Date();
                    var ZicaronDateStart = new Date(2017, 3, 30, 14);
                    var AtsmautDateStart = new Date(2017, 4, 1, 20);
                    var AtsmautDateEnd = new Date(2017, 4, 3, 8);
                    var ValentineDateStart = new Date(2018, 1, 13, 21);
                    var ValentineDateEnd = new Date(2018, 1, 15, 9);
                    var PurimDateStart = new Date(2018, 2, 23, 9);
                    var PurimDateEnd = new Date(2018, 2, 26, 21);
                    var WomenDayDateStart = new Date(2018, 2, 1, 9);
                    var WomenDayDateEnd = new Date(2018, 2, 31, 18);
                    var ny2015DateStart = new Date(2017, 8, 19, 9);
                    var ny2015DateEnd = new Date(2017, 9, 15, 9);
                    var ribbonDateStart = new Date(2017, 9, 15, 9);
                    var ribbonDateEnd = new Date(2017, 9, 31, 9);
                    var silvesterStart = new Date(2017, 11, 31, 17);
                    var silvesterEnd = new Date(2018, 0, 2, 21);
                    var gayprideStart = new Date(2017, 5, 1, 14);
                    var gayprideEnd = new Date(2017, 5, 10, 14);
                    var passoverStart = new Date(2017, 3, 9, 9);
                    var passoverEnd = new Date(2017, 3, 18, 9);
                    var hanukaStart = new Date(2017, 11, 12, 18);
                    var hanukaEnd = new Date(2017, 11, 20, 18);



                    if (CurrentDate > ZicaronDateStart && CurrentDate < AtsmautDateStart) {
                        $('#logo').append('<span class="candle"></span>');
                    }
                    else if (CurrentDate > AtsmautDateStart && CurrentDate < AtsmautDateEnd) {
                        $('#logo').addClass('atsmaut2014');
                    }
                    else if (CurrentDate > ValentineDateStart && CurrentDate < ValentineDateEnd) {
                        $('#logo').addClass('valentine2015');
                    }
                    else if (CurrentDate > PurimDateStart && CurrentDate < PurimDateEnd) {
                        $('#logo').addClass('purim2015');
                    }
                    else if (CurrentDate > WomenDayDateStart && CurrentDate < WomenDayDateEnd) {
                        $('#logo').addClass('wd-generic');
                    }
                    else if (CurrentDate > ny2015DateStart && CurrentDate < ny2015DateEnd) {
                        $('#logo').addClass('ny2015');
                    }
                    else if (CurrentDate > ribbonDateStart && CurrentDate < ribbonDateEnd) {
                        $('#logo').append('<span class="ribbon"></span>');
                    }
                    else if (CurrentDate > silvesterStart && CurrentDate < silvesterEnd) {
                        $('#logo').addClass('silvester');
                    }
                    else if (CurrentDate > gayprideStart && CurrentDate < gayprideEnd) {
                        $('#logo').addClass('gaypride');
                    }
                    else if (CurrentDate > passoverStart && CurrentDate < passoverEnd) {
                        $('#logo').addClass('passover');
                    }
                    else if (CurrentDate > hanukaStart && CurrentDate < hanukaEnd) {
                        $('#logo').addClass('hanuka');

                        var hanukaCandles = [
                            {start: new Date(2017, 11, 12, 18), end: new Date(2017, 11, 13, 18)}, // 1st candle
                            {start: new Date(2017, 11, 13, 18), end: new Date(2017, 11, 14, 18)}, // 2nd candle
                            {start: new Date(2017, 11, 14, 18), end: new Date(2017, 11, 15, 18)}, // 3rd candle
                            {start: new Date(2017, 11, 15, 18), end: new Date(2017, 11, 16, 18)}, // 4th candle
                            {start: new Date(2017, 11, 16, 18), end: new Date(2017, 11, 17, 18)}, // 5th candle
                            {start: new Date(2017, 11, 17, 18), end: new Date(2017, 11, 18, 18)}, // 6th candle
                            {start: new Date(2017, 11, 18, 18), end: new Date(2017, 11, 19, 18)}, // 7th candle
                            {start: new Date(2017, 11, 19, 18), end: new Date(2017, 11, 20, 18)}, // 8th candle
                        ];

                        for (var hanukaCandle = 0; hanukaCandle < hanukaCandles.length; hanukaCandle++) {
                            if (CurrentDate > hanukaCandles[hanukaCandle].start && CurrentDate < hanukaCandles[hanukaCandle].end) {
                                $('#logo').addClass('hanuka-'+(hanukaCandle+1));
                            }
                        }

                    }
                </script>
