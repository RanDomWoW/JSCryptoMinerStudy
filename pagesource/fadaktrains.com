        function setSubMenuPosition(element) {
            var topMenuElement = $("#" + element);
            var posTopMenuElement = topMenuElement.position();

            var subMenuElement = $(topMenuElement).data("submenu");
            var elementOffset = 0;

            switch (element) {
                case "FadakTrainDesignMenu": elementOffset = 260; break;
                case "TicketingMenu": elementOffset = 200; break;
                case "CustomerManagementMenu": elementOffset = 440; break;
                case "AboutUsMenu": elementOffset = 350;
            }

            $('#' + subMenuElement).css({
                position: "absolute", left: posTopMenuElement.left - elementOffset
            });
        }

