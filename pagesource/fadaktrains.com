<script>
    $(document).ready(function () {
        //-------- JSRender Setting --------
        $.views.settings.allowCode = true;
        //----------------------------------

        //-------------- NotifyJS Setting -------------
        $.notify.defaults({ position: "top center" });
        //---------------------------------------------

        setUpMaskInputs();
        setUpTooltips();

        $(document).on('change', 'input[required]', function () {
            if ($(this).val().trim() !== '')
                $(this).removeClass('input-validation-error');
            else
                $(this).addClass('input-validation-error');
        });

        $(document).on('change', 'input[data-val-persiandate]', function () {
            if (validatePersianDate($(this).val()) && $(this).val().trim() !== '') {
                $(this).removeClass('input-validation-error');
                $(this).val(moment($(this).val(), "jYYYY/jMM/jDD").format('jYYYY/jMM/jDD'));
            } else
                $(this).addClass('input-validation-error');
        });

        $(document).on('change', '.search-condition', function () {
            var errorMessage = "";
            var invalidControls = [];

            $('.input-group-addon').removeClass('field-validation-error');

            $("div#informationContainer").slideDown(200);

            if ($('#GetCarDateTime').val() === '') {
                invalidControls.push('#GetCarDateTime');
                errorMessage = 'لطفا تاریخ تحویل را وارد نمایید';

            } else if ($('#ReturnCarDateTime').val() === '') {
                invalidControls.push('#ReturnCarDateTime');
                errorMessage = 'لطفا تاریخ استرداد را صحیح وارد نمایید';

            }

            $(invalidControls).each(function (index, value) { $(value).parent().siblings('.input-group-addon').addClass('field-validation-error') });

            if (errorMessage === '') {
                $('#searchCarErrorDisplayContainer').slideUp('fast');
                $("#imgSearchCar").on('click', function () { document.forms['searchformCar'].submit(); });
            } else {
                $("#imgSearchCar").removeAttr('onclick');
                $('#searchCarErrorDisplay').text(errorMessage);
                $('#searchCarErrorDisplayContainer').slideDown('fast');
            }
        });

        $(".select2").select2({
            language: "fa",
            minimumResultsForSearch: 10
        });
    });

    function setHiddenValues() {
        var a = $('#SourceStationId').select2("data").text.trim();
        $('#SourceStationName').val(a);
        var b = $('#DestinationStationId').select2("data").text.trim();
        $('#DestinationStationName').val(b);
    }

    function setUpMaskInputs() {
        $('[data-mask-input]').each(function () {
            var mask = $(this).data('mask-input');
            var maskPlaceholder = $(this).data('mask-input-placeholder');

            $(this).mask(mask, { placeholder: maskPlaceholder, clearIfNotMatch: true });
        });
    }

    function setUpTooltips() {
        $('[data-toggle="tooltip"]').tooltip({ html: true, container: 'body' });
    }
</script>

