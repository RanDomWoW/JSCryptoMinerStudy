<script>
    $(document).ready(function () {
        //-------- JSRender Setting --------
        $.views.settings.allowCode = true;
        //----------------------------------

        //-------------- NotifyJS Setting -------------
        $.notify.defaults({ position: "top center" });
        //---------------------------------------------

        setUpMaskInputs();
        setUpTooltips();

        $(document).on('change', 'input[required]', function () {
            if ($(this).val().trim() !== '')
                $(this).removeClass('input-validation-error');
            else
                $(this).addClass('input-validation-error');
        });

        $(document).on('change', 'input[data-val-persiandate]', function () {
            if (validatePersianDate($(this).val()) && $(this).val().trim() !== '') {
                $(this).removeClass('input-validation-error');
                $(this).val(moment($(this).val(), "jYYYY/jMM/jDD").format('jYYYY/jMM/jDD'));
            } else
                $(this).addClass('input-validation-error');
        });

        $(document).on('change', '.search-condition', function () {
            var errorMessage = "";
            var invalidControls = [];
            var adultsCount = parseInt($('#AdultsCount').val());
            var janbazCount = parseInt($('#JanbazCount').val());
            var childrenCount = parseInt($('#ChildrenCount').val());
            var infantsCount = parseInt($('#InfantsCount').val());

            $('.input-group-addon').removeClass('field-validation-error');

            $("div#informationContainer").slideDown(200);

            if ($('#FromStation').val() === $('#ToStation').val()) {
                invalidControls.push('#FromStation', '#ToStation');
                errorMessage = 'ایستگاه مبدا و مقصد نمیتوانند یکسان باشند';

            } else if ($('#GoingDate').val() === '') {
                invalidControls.push('#GoingDate');
                errorMessage = 'لطفا تاریخ حرکت را وارد نمایید';

                //} else if (!moment($('#GoingDate').val(), "jYYYY/jMM/jDD").isValid()) {
                //    invalidControls.push('#GoingDate');
                //    errorMessage = 'لطفا تاریخ حرکت را صحیح وارد نمایید';

                //} else if ($('#ReturnDate').val() !== '' && !moment($('#ReturnDate').val(), "jYYYY/jMM/jDD").isValid()) {
                //    invalidControls.push('#ReturnDate');
                //    errorMessage = 'لطفا تاریخ برگشت را صحیح وارد نمایید';

            } else if (adultsCount + janbazCount + childrenCount + infantsCount > 4) {
                invalidControls.push('#AdultsCount', '#janbazCount', '#ChildrenCount', '#InfantsCount');
                errorMessage = 'حداکثر تعداد بلیت قابل خرید 4 می باشد';

            } else if (adultsCount + janbazCount + childrenCount + infantsCount === 0) {
                invalidControls.push('#AdultsCount', '#janbazCount', '#ChildrenCount', '#InfantsCount');
                errorMessage = 'لطفا حداقل یک مهمان انتخاب نمایید';

            } else if (adultsCount + janbazCount === 0) {
                invalidControls.push('#AdultsCount', '#janbazCount');
                errorMessage = 'امکان سفر خردسالان و یا اطفال به تنهایی وجود ندارد';

            } else if ($('#IsTwoWay').prop('checked') && $('#ReturnDate').val() === '') {
                invalidControls.push('#ReturnDate');
                errorMessage = 'لطفا تاریخ برگشت را وارد نمایید';
            } else if ($('#IsExclusiveCompartment').prop('checked') && infantsCount > 0) {
                invalidControls.push('#InfantsCount');
                errorMessage = 'همزمان امکان دربست کردن کوپه و گرفتن بلیت برای اطفال وجود ندارد';
            }


            $(invalidControls).each(function (index, value) { $(value).parent().siblings('.input-group-addon').addClass('field-validation-error') });

            if (errorMessage === '') {
                $('#searchTrainErrorDisplayContainer').slideUp('fast');
                $("#searchTrainImg").on('click', function () { document.forms['searchform'].submit(); });
            } else {
                $("#searchTrainImg").removeAttr('onclick');
                $('#searchTrainErrorDisplay').text(errorMessage);
                $('#searchTrainErrorDisplayContainer').slideDown('fast');
            }
        });

        $(".select2").select2({
            language: "fa",
            minimumResultsForSearch: 10
        });

        //$('#GoingDate').change(function () { //Validating Persian Date
        //    if ($('#GoingDate').val() !== '' && moment($('#GoingDate').val(), "jYYYY/jMM/jDD").isValid())
        //        $('#GoingDate').val(moment($('#GoingDate').val(), "jYYYY/jMM/jDD").format('jYYYY/jMM/jDD'));
