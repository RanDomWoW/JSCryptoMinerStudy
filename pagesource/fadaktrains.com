<script>
    $(document).ready(function() {
        //-------- JSRender Setting --------
        $.views.settings.allowCode = true;
        //----------------------------------

        //-------------- NotifyJS Setting -------------
        $.notify.defaults({ position: "top center" });
        //---------------------------------------------

        setUpMaskInputs();
        setUpTooltips();

        $(document).on('change', 'input[required]', function() {
            if ($(this).val().trim() !== '')
                $(this).removeClass('input-validation-error');
            else
                $(this).addClass('input-validation-error');
        });

        $(document).on('change', 'input[data-val-persiandate]', function() {
            if (validatePersianDate($(this).val()) && $(this).val().trim() !== '') {
                $(this).removeClass('input-validation-error');
                $(this).val(moment($(this).val(), "jYYYY/jMM/jDD").format('jYYYY/jMM/jDD'));
            } else
                $(this).addClass('input-validation-error');
        });

        $(document).on('change', '.search-condition', function() {
            var errorMessage = "";
            var invalidControls = [];
            var hotelAdultsCount = parseInt($('#AdultCount').val());
            var hotelChildrenCount = parseInt($('#ChildrenCount').val());

            $('.input-group-addon').removeClass('field-validation-error');

            $("div#informationContainer").slideDown(200);

            if ($('#FromDate').val() === '') {
                invalidControls.push('#FromDate');
                errorMessage = 'لطفا تاریخ ورود را وارد نمایید';

            } else if ($('#ToDate').val() === '') {
                invalidControls.push('#ToDate');
                errorMessage = 'لطفا تاریخ برگشت را وارد نمایید';

            } else if (hotelAdultsCount + hotelChildrenCount === 0) {
                invalidControls.push('#AdultCount', '#ChildrenCount');
                errorMessage = 'لطفا حداقل یک مهمان انتخاب نمایید';
            } 
            //else if ($('#CityId').val() === '' || $('#CityId').val() == null) {
            //    invalidControls.push('#CityId');
            //    errorMessage = 'لطفا شهر مورد نظر را انتخاب نمایید';
            //}

            $(invalidControls).each(function(index, value) { $(value).parent().siblings('.input-group-addon').addClass('field-validation-error') });

            if (errorMessage === '') {
                $('#searchHotelErrorDisplayContainer').slideUp('fast');
                $("#searchHotelImg").on('click', function(){document.forms['searchformhotel'].submit();});
            } else {
                $("#searchHotelImg").removeAttr('onclick');
                //$("#searchHotelImg").prop("onclick", false);
                //$("#searchHotelImg").on('click', function () {e.preventDefault(); return false; });
                $('#searchHotelErrorDisplay').text(errorMessage);
                $('#searchHotelErrorDisplayContainer').slideDown('fast');
            }
        });

        $(".select2").select2({
            language: "fa",
            minimumResultsForSearch: 10
        });
    });

    function setUpMaskInputs() {
        $('[data-mask-input]').each(function () {
            var mask = $(this).data('mask-input');
            var maskPlaceholder = $(this).data('mask-input-placeholder');

            $(this).mask(mask, { placeholder: maskPlaceholder, clearIfNotMatch: true });
        });
    }

    function setUpTooltips() {
        $('[data-toggle="tooltip"]').tooltip({ html: true, container: 'body' });
    }
</script>

