    <script type="text/javascript">
        function CadastraNews() {

            if ($("#email").val() != "") {
                var url = "/Home/InserirEmail";
                var email = $("#email").val();

                $("#loading").fadeIn(200).delay(500);

                $.ajax({
                    method: 'POST',
                    url: url,
                    data: { email: email },
                    success: function (data) {

                        $(".receba-nossas-ofertas .popup-retorno").fadeIn();

                        $("#loading").fadeOut(400);
                    },
                    error: function (error) {

                        $(".receba-nossas-ofertas .popup-retorno").fadeIn();

                        $("#loading").fadeOut(400);
                    }
                });
            }

        }
    </script>


    

