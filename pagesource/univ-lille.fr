<div id="c1083">
	

	
		
				
			

		

	
	
		<h2>Outil de guidance</h2>
	
	
	
	
	
	
	



		

		




		
				
			
	


	
	
<div class="tx-udlguidance col-md-12">
	<form class="form-inline">
		
    <div class="form-group">
        <label for="select-37" style="">Je suis</label>
        <select id="select-37" class="form-control _input-lg" onChange="guidance(this, 0)">
            <option></option>
            
            	
        			<option value="136" data-parent='37' data-link='' data-content='0'> étudiant international</option>
        	
            	
        			<option value="135" data-parent='37' data-link='' data-content='0'> lycéen, étudiant</option>
        	
            	
        			<option value="137" data-parent='37' data-link='' data-content='0'> une entreprise / partenaire</option>
        	
            	
        			<option value="138" data-parent='37' data-link='' data-content='0'> salarié, demandeur d'emploi</option>
        	
            	
        			<option value="139" data-parent='37' data-link='' data-content='0'> un alumni, ancien étudiant</option>
        	
        </select> 
    </div>
    <div id="quest-37" class="test" style="display: initial" >

    </div>
    <div id="guidecontent"></div>
    <div id="result"></div>

<script type="text/javascript">
    function guidance(select, home) {
		// NL
		if (home == 1) {
		$('#guide-modal').modal('show');
		
		var index = $("#_select-37 option:selected").index();
		$('#select-37 option').eq(index).prop('selected',true);
			home = 0;
		}
		// fin NL
		
		/*DEBUT Simuler le onchange sur la liste*/
		
/*
		$("#_select-37").change(function(event,index)
		{
			if (index!= "")
		    {
		       $("#_select-37 option[value='"+index+"']").attr("selected", "selected");
		    }
		    $("#_select-37").trigger("change", [ index ]);
		}
*/
		
		/*FIN Simuler le onchange sur la liste*/
		
        selectedOption = $('option:selected', select);
        var link = selectedOption.data("link");
        var parent = selectedOption.data("parent");
        var content = selectedOption.data("content");
        var value = $(select).val();
        if(link != "" && link != undefined && link != null)
        {
//             window.location.href = link;
			
            $("#result").html("<a href="+link+" class='btn btn-primary'>Voir le résultat</a>");
        }
        else if(content !=0 && content != undefined && content != null)
        {
	    	$("#guidecontent").html(content);
        }
        else
        {
            var ajaxURL = '/nc/?type=42&tx_udlguidance_outilguidance%5Baction%5D=guide&tx_udlguidance_outilguidance%5Bcontroller%5D=Question';
            $.ajax({
                url: ajaxURL,
                data:{
                    "tx_udlguidance_outilguidance[reponse]":value
                },
                success:function(question){
					
					
					if (question.idreponse == undefined) {
					$('#quest-'+parent).html(
                        "    <div class='well' style='margin-top:30px'>"+
                        "        <p style='' >"+question.libelle+"</p>"+
                        "    </div></div>"+
                        "    <div id='quest-"+question.uid+"' style='display:initial'></div>"
                    );
					} else {
						$('#quest-'+parent).html(
                        "    <div class='form-group'>"+
                        "        <label style='' for='select-"+question.uid+"'>"+question.libelle+"</label>"+
                        "        <select id='select-"+question.uid+"' class=' form-control  _input-lg' onChange='guidance(this, 0)'>"+
                        "        </select>"+
                        "    </div></div>"+
                        "    <div id='quest-"+question.uid+"' style='display:initial'></div>"
                    );
					}
                    
                    var html = "<option></option>";
					
                    for (var i = 0; i < question.idreponse.length; i++) {
					    var reponse = question.idreponse[i];
//                         html += "        <option value='"+reponse.uid+"' data-parent='"+question.uid+"' data-link='"+reponse.lien+"'> "+reponse.libelle+"</option>";
html += '<option value="'+reponse.uid+'" data-parent="'+question.uid+'" data-link="'+reponse.lien+'" data-content="'+reponse.gotocontent+'">'+reponse.libelle+'</option>';
console.log(html);
					}

                    $("#select-"+question.uid+"").html(html);
                },
                error:function(){
	                $("#result").html("");
	                $('#quest-'+parent).html("Un problème est survenu");
                }
            });
            return false;
        }
    };
</script>


	</form>
</div>

	


