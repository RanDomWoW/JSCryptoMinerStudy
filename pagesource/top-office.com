        jQuery('#data-mapheader .btn-search-shop').click(function(){
            navigator.geolocation.getCurrentPosition(function(position) {
                getShopsHeader(position.coords.latitude, position.coords.longitude);
            }, function() {
                var $listeMagChoix = jQuery('.popin-shop-header .listeMagChoix');
                jQuery('.listeMag-error', $listeMagChoix).remove();
                $listeMagChoix.prepend('<div class="listeMag-error">Impossible de récupérer votre position GPS</div>');
            });
        });

        if (!"geolocation" in navigator) {
            jQuery('#data-mapheader #mapLeft .choice-type-shearch-shop').removeClass('geoloc-active');
        }

