<script>
    jQuery.noConflict();

    function selectMagHeader(libelle,id, shopId){
        libelle = libelle.replace("\\","");
        jQuery('#stock_id-input').val(id);
        jQuery('.listeMagChoixLoading').show();
        jQuery('.listeMagChoix').hide();
        var $warehouseForm = jQuery('#warehouse-form');
        if (window.innerWidth < 1024) {
            $warehouseForm.attr('action', $warehouseForm.attr('action') + 'mobile/true' )
        }
        $warehouseForm.submit();
        return false;
    }

    function formSelectMagHeader(){

        jQuery('.listeMagChoix').hide();
        jQuery('.listeMagChoixLoading').show();

        jQuery.ajax({
            type: "POST",
            dataType: "json",
            url: jQuery('#choosenMag-url').val()+'byform/',
            data: "region="+jQuery('.postcode-magselect').val(),
            success: function(data){
                if(data){
                    jQuery('.listeMagChoix').html('');
                    var list = '';
                    if(data.length){
                        data.each(function(entry) {
                            var libelle = entry.libelle.replace("\\","");
                            list += '<li><a title="" onclick="selectMagHeader(\'\',' +entry.id+','+entry.shop_id+')" href="#"><span>'+entry.departement+' - '+libelle+'</span></a></li>';
                        });
                        jQuery('.listeMagChoix').html('<ul>'+list+'</ul>').show();

                    }else{
                        jQuery('.listeMagChoix').html('<div class="listeMag-error">Aucun magasin trouvé pour <strong style="font-size: 16px;">'+jQuery('.postcode-magselect').val()+'</strong></div>').show();
                    }
                    jQuery('.listeMagChoixLoading').hide();
                }
            }
        });
    }

    function getShopsHeader(customerlat, customerlong){

        var $listMag = jQuery('.popin-shop-header'),
            $listeMagChoix = jQuery('.listeMagChoix', $listMag),
            $listeMagChoixLoading = jQuery('.listeMagChoixLoading', $listMag);

        var ajaxActionUrl;

        if(document.location.protocol === 'https:') {
            ajaxActionUrl = 'https://www.top-office.com/shops/geoloc/ajax/';
        } else {
            ajaxActionUrl = 'http://www.top-office.com/shops/geoloc/ajax/';
        }

        $listeMagChoix.hide();
        $listeMagChoixLoading.show();

        jQuery.ajax({
            type: 'POST',
            dataType: "json",
            url: ajaxActionUrl,
            data: {
                lat: customerlat,
                long: customerlong
            },
            success: function (data){
                if(data) {
                    var $previousShops = $listeMagChoix.find('ul').detach();
                    $listeMagChoix.empty();

                    var $list = jQuery('<ul/>');
                    if (data.length) {
                        data.each(function (shop) {
                            var $li = jQuery('<li/>');
                            $li.append('<a class="near-shop" title="" onclick="selectMagHeader(\'\',' + shop.warehouse_id + ',' + shop.shops_id + ')" href="#"><div class="range-shop">' + shop.distance + 'Km</div><span>' + shop.departement + ' - ' + shop.label + '</span></a>');
                            $list.append($li);
                        });
                        $listeMagChoix.append($list).show();
                    } else {
                        $listeMagChoix.append('<div class="listeMag-error">Aucun magasin trouvé à moins de 15 km</div>');
                        $listeMagChoix.append($previousShops).show();
                    }
                    $listeMagChoixLoading.hide();
                }
            }
        })
    }


