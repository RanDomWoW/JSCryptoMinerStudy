<script type="text/javascript">

    jQuery(document).ready(function () {
        var selectCartouche = jQuery('.layers-links').find('.consommable_type');
        selectCartouche.attr('id', 'cartouche-menu');
    });

    var cartoucheAjax = false;
    var xhrcartoucheAjax;

    function loadModels(e, marque, type, referer) {

        if (cartoucheAjax) {
            xhrcartoucheAjax.abort();
        }

        cartoucheAjax = true;
        e.find('.cartoucheloader').show();
        xhrcartoucheAjax = jQuery.ajax({
            type: 'POST',
            dataType: "json",
            url: 'http://www.top-office.com/cartouches/list/getModels/',
            data: {
                marque: marque,
                type: type,
                referer: referer
            },
            success: function (data, textStatus, jqXHR) {

                if (data.result == 0) {
                    e.find('.modele_imprimante_compatible').attr('disabled', 'disabled');
                    e.find('.modele_imprimante_compatible').html('<option value="0">Sélectionnez votre modèle</option>');
                    e.find('.consommable_type').html(data.type);
                } else {
                    e.find('.modele_imprimante_compatible').removeAttr('disabled');
                    e.find('.modele_imprimante_compatible').html(data.model);
                }

                e.find('.cartoucheloader').hide();
                cartoucheAjax = false;
            }
        });
    }

    function loadByMarque(e) {

        var marque = e.val();
        var type = e.parent().parent().find('.consommable_type').val();
        var referer = e.attr('id');

        if (cartoucheAjax) {
            xhrcartoucheAjax.abort();
        }

        if (type == "0") {	//Load Type // disable modele

            cartoucheAjax = true;
            xhrcartoucheAjax = jQuery.ajax({
                type: 'POST',
                url: 'http://www.top-office.com/cartouches/list/getType/',
                data: {
                    marque: marque
                },
                success: function (data, textStatus, jqXHR) {
                    e.parent().parent().find('.modele_imprimante_compatible').attr('disabled', 'disabled');
                    e.parent().parent().find('.consommable_type').html(data);
                    cartoucheAjax = false;
                }
            });

        } else {	//Load Modele
            var parent = e.parent().parent();
            loadModels(parent, marque, type, referer);
        }

    }

    function loadByType(e) {

        var marque = e.parent().parent().find('.marque_imprimante_compatible').val();
        var type = e.val();
        var referer = e.attr('id');

        if (cartoucheAjax) {
            xhrcartoucheAjax.abort();
        }

        if (marque == "0") {	//Load Marque // disable modele

            cartoucheAjax = true;
            xhrcartoucheAjax = jQuery.ajax({
                type: 'POST',
                url: 'http://www.top-office.com/cartouches/list/getMarque/',
                data: {
                    type: type
                },
                success: function (data, textStatus, jqXHR) {
                    e.find('.modele_imprimante_compatible').attr('disabled', 'disabled');
                    e.parent().parent().find('.marque_imprimante_compatible').html(data);
                    cartoucheAjax = false;
                }
            });

        } else {	//Load Modele
            var parent = e.parent().parent();
            loadModels(parent, marque, type, referer);
        }

    }

    function redirectModel(e) {
        var url = e.val();
        jQuery(location).attr('href', url);
    }

</script>
