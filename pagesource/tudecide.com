	<script type="text/javascript">
		$("input[name=telefono]").mask("(00) 0000-0000");
		$("input[name=frmtelefono]").mask("(00) 0000-0000");
		
		$(".menu-vertical li").click(function(){
			if($(this).attr("rel") != undefined)
				$(".menu-vertical").hide();
				$("#"+$(this).attr("rel")).show("slow");
		});
		
		$(".menu-subvertical li").click(function(){
			if($(this).attr("rel") != undefined)
				$(".menu-subvertical").hide();
				$("#"+$(this).attr("rel")).show("slow");
		});
		
		$("#bclose").click(function(){
			$("#sidebar").css("width","0");
			$("#wrapper").fadeOut( "slow" );
			$("#bclose").hide();
		});
		
		$(document).scroll(function(){
			if($(document).scrollTop() > 1)
				$(".fix-header").show();
				else
					$(".fix-header").hide();
		});
		
		$(".info-table button").click(function(){
			$(".fix-form").slideToggle();
		});
		
		$(".bopen").click(function(){
			if($(window).width() <= 667)
				$("#sidebar").css("width","15rem");
				else
					$("#sidebar").css("width","21rem");
			$("#wrapper").fadeIn( "slow" );
			$("#bclose").show();
		});
		
		$("button[name=btnsolicitar]").click(function(){
			var filter = /[\w-\.]{3,}@([\w-]{2,}\.)*([\w-]{2,}\.)[\w-]{2,4}/;
			var nombre = $("input[name=frmnombre]").val();
			var telefono = $("input[name=frmtelefono]").val();
			var correo = $("input[name=frmcorreo]").val();
			var comentario = $("textarea[name=frmcomentario]").val();
			if(nombre == "")
				_alerta("Ingresa tu nombre completo");
			else if(telefono == "")
				_alerta("Ingresa tu número telefónico");
			else if(correo == "")
				_alerta("Ingresa tu correo electrónico");
			else if(!filter.test(correo))
				_alerta("Ingrese un correo electrónico valido");
			else if(comentario == "")
				_alerta("Ingrese un comentario");
			else{
				$.ajax({
					url: "/sticky-bar-coment",
					type: "post",
					data: {
						ctnom : nombre,
						cttel : telefono,
						ctcor : correo,
						ctcom : comentario
					}
				}).done(function( data ) {
					if(data == "true")
						_alerta("Gracias por contactanos, en breve un asesor se comunicara contigo");
					$(".form-table input").val("");
					$(".form-table textarea").val("");
				});
			}
		});
		
	</script>

