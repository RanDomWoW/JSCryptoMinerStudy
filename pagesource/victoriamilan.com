    <script>
        // Automatically resize text on CTA buttons:
        $(function(){
            var wrappers = $('.try-us-btn-wrap');
            wrappers.each(function(n, obj){
                var $obj = $(obj);
                var button = $obj.find('button');
                if (button.length === 0) {
                    button = $obj.find('.btn-try-us');
                }
                if (button.length === 0) {
                    button = $obj.find('.btn-scroll-to-footer');
                }
                if (button.length) {
                    var text = button.text();
                    text = text.replace(/\s+/g, ' ');
                    var size = Math.round(2.0 * 360 / text.length);
                    size = Math.min(size, 18);
                    button.css('font-size', size + 'px');
                }
            });
        });

