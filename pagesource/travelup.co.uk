<script type="text/javascript">
    function compareUnitsSearch(country, currency, locale, originPlace, destinationPlace, outboundDate, inboundDate, adults, children, infants) {
        var snowyowl = new compareunits.snowyowl();
        snowyowl.setSize(410, 510);
        snowyowl.setPosition(0, (window.screen.availHeight - 510) / 2);
        var validated = snowyowl.setFlightsQuery(country, currency, locale, originPlace, destinationPlace, outboundDate, inboundDate, adults, children, infants);
        if (validated)
            snowyowl.searchFlights(true);
    }

    function btnFlightOnlySearchSubmit(btn) {
        //debugger;
        var dateFormat = "yy-mm-dd";
        var depIATA = document.getElementById("ddlFlightDepartureAirport").value,
                desIATA = document.getElementById("txtFlightDestinationAirport").value,
                depDate = $jq.datepicker.parseDate('dd/mm/yy', document.getElementById("txtFlightDepartureDate").value),
                retDate = $jq.datepicker.parseDate('dd/mm/yy', document.getElementById("txtFlightReturnDate").value),
                adults = parseInt(document.getElementById("ddlAdult").value),
                children = parseInt(document.getElementById("ddlChild").value),
                infants = parseInt(document.getElementById("ddlInfant").value),
                flexibleSearch = parseInt(document.getElementById("ddlFlexibleDates").value);
        if (depIATA != "" && depIATA.length != 0)
            depIATA += "-iata";
        else return false;

        if (desIATA != "" && desIATA.length != 0) {

            if (desIATA.length < 3) return false;
            else if (desIATA.length === 3) desIATA += "-iata";
            else {
                var n = desIATA.indexOf("(");
                if (n >= 0) desIATA = desIATA.substr(n + 1, 3) + "-iata";
            }
        }
        else return false;

        if (flexibleSearch != 3) {
            var str = "UK|GBP|en-GB|" + depIATA + "|" + desIATA + "|" + $jq.datepicker.formatDate(dateFormat, depDate) + "|" + $jq.datepicker.formatDate(dateFormat, retDate) + "|" + adults + "|" + children + "|" + infants;
            compareUnitsSearch("UK", "GBP", "en-GB", depIATA, desIATA, $jq.datepicker.formatDate(dateFormat, depDate), $jq.datepicker.formatDate(dateFormat, retDate), adults, children, infants);
            $jq.post("BackOfficeDataService.asmx/LogRateFinderHit", { message: str }, function (data) { });
        }

    }
</script>
