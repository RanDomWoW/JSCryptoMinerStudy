});

$(this).on('click', '#new_applicant_confirm', function()
{
var nic = $('#cnic').val();
$('#new_applicant_confirm').attr('disabled', true); 
$.ajax(
{
type: 'post',url:'process_service_comission',
data :{action: 'check_existing', cnic: nic},
success : function(r)
{
$('#new_applicant_confirm').attr('disabled', false);

if( r.indexOf('Vacancies') != -1 )
{
$('.ajax_resp').html('You Are Already Registered.  For another Post click "Print existing form/apply for another post"');
return false;
}
r = r.split(':-');
if( r[0]== '1')
{
window.location.href = 'apply_online?dev';
}
else
{
$('.ajax_resp').html(r[1]);
}

} 
}); 
});
 
});
