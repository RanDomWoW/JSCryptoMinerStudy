					reg = new RegExp(/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,8})+$/);
					if(e.value==''){
					e.value='Confirme your E-mail';
					e.style.color = '#666';
					}else if((!reg.test(e.value))){
					alert('E-mail('+e.value+') is not valid!');
					return false;
					}
				}
