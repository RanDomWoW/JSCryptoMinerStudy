					reg = new RegExp(/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,8})+$/);
					if(document.getElementById('Subscribe_username').value=='' || document.getElementById('Subscribe_username').value=='Your name here'){
