<script>
    var swReg; // Service Worker Registration
    var swFile = '/user/themes/base/assets/js/serviceworker.js';
    var appServerPubKey = 'BKsTplTkwjdKpr4Xqhevawb8Ypd-9GiqDR3H7eJuDHLi5GVR8BjZ2h8ro67e7pS0Rl_l55fFd8l6Mi72YiRTjKg';
    var notificationBlock = $('#notification-block');
    var notificationToggle = notificationBlock.find('input');
    var isSubscribed;
    var subscriptionUrl = '/push-notification/persist.json';
    if ('serviceWorker' in navigator && 'PushManager' in window) {
        notificationToggle.on('click', function() {
            notificationToggle.disabled = true;
            if (isSubscribed) {
                unsubscribeUser();
            } else {
                subscribeUser();
            }
        });

        navigator.serviceWorker.register(swFile)
            .then(function(registration) {
                swReg = registration;
                initializeUI();
            });

    } else {
        notificationBlock.hide();
    }

    function initializeUI() {
        swReg.pushManager.getSubscription()
            .then(function(subscription) {
                isSubscribed = !(subscription === null);

                if (isSubscribed) {
                    notificationToggle.prop('checked', true);
                } else {
                    notificationToggle.prop('checked', false);
                }
            });
    }
    function subscribeUser() {
        return swReg.pushManager.subscribe({
            userVisibleOnly: true,
            applicationServerKey: urlBase64ToUint8Array(appServerPubKey)
        })
        .then(function(subscription) {
            persistSubscription(subscription);

            isSubscribed = true;

            notificationToggle.prop('checked', true);
            notificationToggle.disabled = false;

            return subscription;
        })
        .catch(function() {
            notificationToggle.prop('checked', false);
            notificationToggle.disabled = false;
            alert('We are unable to send you notifications due to your browser permissions. Please update your notification settings in your browser or disable private browsing to allow notifications from our website')
        });
    }
    function unsubscribeUser() {
        swReg.pushManager.getSubscription()
            .then(function(subscription) {
                if (subscription) {
                    return subscription.unsubscribe();
                }
            })
            .then(function() {
                isSubscribed = false;
                notificationToggle.disabled = false;
                notificationToggle.prop('checked', false);
            });
    }
    function persistSubscription(subscription) {
        var data;
        if (subscription) {
            data = subscription.toJSON();
        }
        $.ajax({
            type: 'post',
            url: subscriptionUrl,
            data: data,
            success: function (info) {
                console.log(info);
            }
        });
    }
    function urlBase64ToUint8Array(base64String) {
        const padding = '='.repeat((4 - base64String.length % 4) % 4);
        const base64 = (base64String + padding)
            .replace(/\-/g, '+')
            .replace(/_/g, '/');

        const rawData = window.atob(base64);
        const outputArray = new Uint8Array(rawData.length);

        for (let i = 0; i < rawData.length; ++i) {
            outputArray[i] = rawData.charCodeAt(i);
        }
        return outputArray;
    }
</script>
