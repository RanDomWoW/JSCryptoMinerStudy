<script>


    chargerConservationFonciere();
    initDropdown();

	function showSpinner() {
        $("#SpinnerLoading").show();
    }
    function hideSpinner() {
        $("#SpinnerLoading").hide();
    }


    hideSpinner();


    $(document)
        .ajaxStart(function () {
            showSpinner();
        })
        .ajaxStop(function () {
            hideSpinner();
        });


    function initDropdown() {
		$("#NatureProcedure").val('0');
		$("#NatureProcedure").prop('disabled', true);
		$("#typeAvis").val('0');
		$("#NatureReference").val('0');

    }

    function searchpub() {

        var conservationFonciere = $("#conservationFonciere").val();
        var NatureProcedure = $("#NatureProcedure").val();
        var NatureReference = $("#NatureReference").val();
        
        var typeAvis = $("#typeAvis").val();
        var conservationFonciereLibelle = $("#conservationFonciere option:selected").text();

        if (!conservationFonciere) addBorder("conservationFonciere");
        else deleteBorder("conservationFonciere");



        if (conservationFonciere) {

            $.ajax({
            type: "POST",
            dataType: 'json',
            cache: false,
            async: true,
            contentType: "application/json; charset=utf-8",
            url: window.location.protocol + '//' + window.location.host + '/AdminLogin/surface/PubliciteFonciereSurface/searchPub/',
            data: '{ "conservationFonciere" : "' + conservationFonciere + '" ,"conservationFonciereLibelle" : "' + conservationFonciereLibelle + '", "NatureReference" : "' + NatureReference + '" ,"NatureProcedure" : "' + NatureProcedure + '" , "typeAvis" : "' + typeAvis + '"}',
            complete: function (result) {

                    if (result.status == 200) {

            //lResult = JSON.parse(result.responseText);
                        window.location.href = '/adminlogin/Surface/PubliciteFonciereSurface/PubliciteFonciere';

        }
        },
            error: function (xhr, ajaxOptions, thrownError) {

                    console.log(xhr.status);
                    console.log(thrownError);
        }

        });
        }



    }


    function GetListProcedure(choixID) {

        var lResult = {};
        var resultatId = "";

        $("#typeAvis").empty();
        $("#typeAvis").append("<option value='0' >--Select--</option>");

            //Titre Foncier

            if (choixID == "T") {
    
                $("#NatureProcedure").prop('disabled', true);

                $.ajax({
                    type: "POST",
                    dataType: 'json',
                    cache: false,
                    async: false,
                    contentType: "application/json; charset=utf-8",
                    url: window.location.protocol + '//' + window.location.host + '/AdminLogin/surface/PubliciteFonciereSurface/GetListProcedureFr/',
                    data: '{ "choixID" : "' + choixID + '"}',

                    complete: function (result) {

                        if (result) {
                            if (result.status == 200) {

                                if (result.responseText) lResult = JSON.parse(result.responseText);
										   
                            }
                        }


                    },
                    error: function (xhr, ajaxOptions, thrownError) {
                        console.log(xhr.status);
                        console.log(thrownError);
                    }

                });

                $("#typeAvis").empty();
                $("#typeAvis").append("<option value='0' >--Select--</option>");

                if (lResult != "-1") {
                    for (var i = 0; i < lResult.length; i++) {

                        $("#typeAvis").append("<option value='" + lResult[i].id + "' >" + lResult[i].libelle + "</option>");
                    }
                }
            }

            else if (choixID == "R") {
                $("#NatureProcedure").prop('disabled', false);
            }


    }

    function GetListNormalSpeciale(choixID) {

        var lResult = {};
        var resultatId = "";

        $("#typeAvis").empty();
        $("#typeAvis").append("<option value='0' >--Select--</option>");

            //Requisition Normale

            if (choixID == 'O') resultatId = 2;
            if (choixID == 'S') resultatId = 3;

                $.ajax({
                    type: "POST",
                    dataType: 'json',
                    cache: false,
                    async: false,
                    contentType: "application/json; charset=utf-8",
                    url: window.location.protocol + '//' + window.location.host + '/AdminLogin/surface/PubliciteFonciereSurface/GetListProcedureFr/',
                    data: '{ "choixID" : "' + resultatId + '"}',

                    complete: function (result) {

                        if (result) {
                            if (result.status == 200) {

                                if (result.responseText) lResult = JSON.parse(result.responseText);

                            }
                        }


                    },
                    error: function (xhr, ajaxOptions, thrownError) {
                        console.log(xhr.status);
                        console.log(thrownError);
                    }

                });

                $("#typeAvis").empty();
                $("#typeAvis").append("<option value='0' >--Select--</option>");

                if (lResult != "-1") {
                    for (var i = 0; i < lResult.length; i++) {

                        $("#typeAvis").append("<option value='" + lResult[i].id + "' >" + lResult[i].libelle + "</option>");
                    }
                }

    }


    function chargerConservationFonciere() {

        var lResult = {};
        var choixID = "";

            $.ajax({
                type: "POST",
                dataType: 'json',
                cache: false,
                async: false,
                contentType: "application/json; charset=utf-8",
                url: window.location.protocol + '//' + window.location.host + '/AdminLogin/surface/PubliciteFonciereSurface/GetListConservationFonciere/',
                data: '{ "choixID" : "' + choixID + '"}',

                complete: function (result) {
                    if (result) {
                        if (result.status == 200) {

                            if (result.responseText) lResult = JSON.parse(result.responseText);

                        }
                    }


                },
                error: function (xhr, ajaxOptions, thrownError) {
                    console.log(xhr.status);
                    console.log(thrownError);
                }

            });

            $("#conservationFonciere").empty();
            $("#conservationFonciere").append("<option value='' >--Select--</option>");

            for (var i = 0; i < lResult.length; i++) {

                $("#conservationFonciere").append("<option value='" + lResult[i].id + "' >" + lResult[i].libelle + "</option>");
            }
        }


    function addBorder(elementID) {
        $('#' + elementID).css('border', 'solid 1px red');
    }
    function deleteBorder(elementID) {
        $('#' + elementID).css('border', 'solid 1px gray');
    }

</script>


    
