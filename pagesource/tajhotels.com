   function getPlaceid(callback, address) {
   
   address = address;
   
   geocoder = new google.maps.Geocoder();
   
   if (geocoder) {
   
   geocoder.geocode({
   
   'address': address
   
   }, function (results, status) {
   
   if (status == google.maps.GeocoderStatus.OK) {
   
   callback(results['place_id']);
   
   }
   
   });
   
   }
   
   }


   
   
   
