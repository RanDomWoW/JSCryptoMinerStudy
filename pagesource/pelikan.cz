        padding: 20px;
        border-radius: 3px;
        z-index: 2;
      }

      .blue-info-message-wrap .form-wrap:before {
        content: '';
        position: absolute;
        width: 0;
        height: 0;
        border-style: solid;
        border-width: 0px 8.5px 8px;
        border-color: transparent transparent #d5e8f8;
        transition: .45s;
        left: 40px;
        right: 0;
        top: 58px;
        width: 0px;
        z-index: 3;
      }

      .blue-info-message-wrap .form-wrap:after {
        content: '';
        position: absolute;
        width: 0;
        height: 0;
        border-style: solid;
        border-width: 0px 8.5px 8px;
        border-color: transparent transparent #9cb1cc;
        transition: .45s;
        left: 40px;
        right: 0;
        top: 57px;
        width: 0px;
        z-index: 1;
