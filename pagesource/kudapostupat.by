            //сузы - tabs-3
            $("#strsuz").autocomplete({
                source: "/zaved/search/type/1/",
                focus: function(event, ui) {
                    search_option = ui.item;
                },
                select: function(event, ui) {
                    search_option = ui.item;
                    $("#strsuz").autocomplete('widget')
                            .trigger('mousedown.choose_option');
                }
            })

                    .autocomplete('widget').bind('mousedown.choose_option',function() {
                        $("#strsuz").autocomplete('close');
                        document.location.replace("/zavedenie/id/"+search_option.id);
                    });

            //сузы - tabs-5
            $("#strptuz").autocomplete({
                source: "/zaved/search/type/2/",
                focus: function(event, ui) {
                    search_option = ui.item;
                },
                select: function(event, ui) {
                    search_option = ui.item;
                    $("#strptuz").autocomplete('widget')
                            .trigger('mousedown.choose_option');
                }
            })

                    .autocomplete('widget').bind('mousedown.choose_option',function() {
                        $("#strptuz").autocomplete('close');
                        document.location.replace("/zavedenie/id/"+search_option.id);
                    });

            //Квалификация - tabs-4
            var qualif_typev = 'all';
            var $qualifcatEl = $("#qualificat");
            var jUrl = "/zaved/jsonsearchspec/type/";
            var sUrl = "/zaved/searchspec/type/";

            $("#qualif_type").change(function () {
                qualif_typev = $('#qualif_type option:selected').val();
                $qualifcatEl.autocomplete( "option", "source", jUrl + qualif_typev + "/" );
            });

            $qualifcatEl.autocomplete({
                source: jUrl + qualif_typev + "/",
                minLength: 2,
                delay: 700,
                focus: function(event, ui) {
                    search_option = ui.item;
                },
                select: function(event, ui) {
                    search_option = ui.item;
                    $qualifcatEl.autocomplete('widget')
                            .trigger('mousedown.choose_option');
                }
            })

                    .autocomplete('widget').bind('mousedown.choose_option',function() {
                        $qualifcatEl.autocomplete('close');
                        document.location.replace("/qualifid/id/"+search_option.id);
                    });

            $qualifcatEl.keyup(function( event ) {
                if ( event.which == 13 ) {
                    document.location.replace(sUrl + qualif_typev + "/?term=" + $(this).val());
                }
            });

            $('#searchForm').submit(function() {
                if($('input[name="exs[]"]:checked',this).length == 0) {
                    alert('Пожалуйста, выберите вступительные испытания');
                    return false;
                }
            });

            $('#searchForm .search_cont input:checkbox').click(function() {
                if($('#searchForm .search_cont input:checkbox:checked').length > 0) {
                    $('#search_submit').addClass('active');
                } else {
                    $('#search_submit').removeClass('active');
                }
            });
        });
    </script>
