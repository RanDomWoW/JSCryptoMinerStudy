<script language="javascript" type="text/javascript">
$(document).ready(function () {
    //先將所有物件內容關閉
    //$('.organization2').slideUp("normal");
    // 當點到標題時，若答案是隱藏時則顯示它；反之則隱藏
    $( document ).on( "click", ".organization1", function(){
        //取得作用物件ID
        var thisid=$(this).prop('id');
        var thisid_aaray=thisid.split("_");
        var objid=thisid_aaray[1];
        //記錄作用物件內容display狀態
        var pc2_display=$('.pc2_'+objid).css('display');/*.css('')這個語法是在說抓css屬性的值*/
        //先將所有物件內容關閉
        $('.organization2').slideUp("normal");
        //判斷物件內容狀態選擇打開或關閉
        if(pc2_display=="none")
            $('.pc2_'+objid).slideDown("normal");
        else
            $('.pc2_'+objid).slideUp("normal");
    });

    $( document ).on( "click", ".showdisplay", function(){
        var thisid=$(this).prop('id');
		var _display=$('#_'+thisid).css('display');
        $('._showdisplay').slideUp("normal");
		$('.showdisplay div').removeClass("bt_in");
		if(_display=="none")
		{
            $('#_'+thisid).slideDown("normal");
			$('#'+thisid+" div").addClass("bt_in");
        }else{
            $('#_'+thisid).slideUp("normal");
			$('#'+thisid+" div").removeClass("bt_in");
		}
	});

});
</script>

