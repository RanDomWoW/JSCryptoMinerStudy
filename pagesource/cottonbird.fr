<script type="text/javascript">
		var tailleXS = "600"
		var L = $(window).width();
		/*console.log($("#viewport").length());
		var L = document.querySelector('meta[name="viewport"]').getAttribute‌​("width")*/
		var dossierslide=tailleXS;
		
		function refreshSlider(sliderID,L,tailleXS){
		
			if (L<768 || is_phone!==null) { dossierslide=tailleXS };
			if ((L>=768 && L<993 && is_phone==null) || is_tablet!==null ) { dossierslide="900" };
			if (L>=993 && is_phone==null && is_tablet==null)          { dossierslide="1200"};
			//console.log(L+"-"+dossierslide)		
			$(sliderID + " .item img").each(function() {
				var src = $(this).attr("src")
				var new_src = src.replace(new RegExp("Slide\/(.*)\/"),"Slide\/"+dossierslide+"\/"); 
				if (src!=new_src){	$(this).attr("src", new_src)}; 
			});
		}
		
		function chargeSlideSupp(sliderID){
			
			var ctr=0;
			$(sliderID + " .item img").each(function() {
				var src = $(this).attr("src")
				if (src==""){ $(this).attr("src",$(this).attr("data-src").replace(new RegExp("Slide\/(.*)\/"),"Slide\/"+dossierslide+"\/")); ctr++}
				if (ctr>0) {return false}
			});
			refreshSlider("#sliderPrincipal",L,tailleXS)
			setTimeout("chargeSlideSupp('#sliderPrincipal')",2000)
		}
		
		$( document ).ready(function() {
		  var md = new MobileDetect(window.navigator.userAgent);
		is_mobile = md.mobile() ;          // 'Sony'
		is_phone = md.phone() ;           // 'Sony'
		is_tablet = md.tablet() ;
		//console.log("is_mobile : " + is_mobile);
		//console.log("is_phone : " + is_phone);
		//console.log("is_tablet : " + is_tablet);
		if (is_phone!==null){
			var meta = document.createElement('meta');
			meta.name = "viewport";
			meta.id = "viewport";
			meta.content = "width=767";
			//document.getElementsByTagName('head')[0].appendChild(meta);	
		}
			//actualisation taille du slider principal
			refreshSlider("#sliderPrincipal",L,tailleXS)
			chargeSlideSupp("#sliderPrincipal")
			// redimensionne les slides au changement de taille de windows
			$( window ).resize(function() {
				L=$(window).width();
				refreshSlider('#sliderPrincipal',L,tailleXS);
			});			
  	});
	
	</script>
