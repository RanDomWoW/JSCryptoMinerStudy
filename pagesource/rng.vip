<script>
Vue.component('notice-popup',{
  template: '#notice-box',
  props: ['notice'],
  data:function(){
    return {
      state: vue_data.state,
      noticeIndex: 0,
    }
  },
  computed: {
    _notice:function() {
      var o = [];
      if(this.notice == 'notice'){
        this.state.notice.forEach(function(e,i){
          o[i]= {};
          o[i].title = e.bulletin_title;
          o[i].remark = e.bulletin_content.replace(/([^"])(https?:\/\/[^\s/$.?#].[^\s]*)/ig, '$1 <a href="$2" target="_blank">$2</a>');
          o[i].subtitle = poi.formatDate(new Date(Number(e.update_time)*1000));
        })
      }else {
        o.push(this.notice);
      }
      return o;
    },
  },
  methods: {
    to: function(v) {
      var max = this._notice.length - 1;
      if(v == 'next') {
        this.noticeIndex >= max ? this.noticeIndex = 0 : this.noticeIndex++;
      }else {
        this.noticeIndex <= 0 ? this.noticeIndex++ : this.noticeIndex = 0;
      }
    }
  }
})
