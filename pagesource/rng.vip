    },
    login: function() {
      ga('send', 'event', 'edit_request', 'login', {transport: 'beacon'});
      location.href = 'https://user.rennigou.jp/service/login?next_url=http%3A%2F%2Frennigou.jp%2Flogin%3Fredirect%3D' + this.state.pageUrl;
