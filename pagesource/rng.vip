<script>
  $(function(){
    Vue.component('rng-ban-show',{
      template:"#rng-ban",
      data: function(){
        return{
          state: vue_data.state,
          items: '',
          itemIndex: 0,
          isDropdown: false,
          isAjax: false,
        }
      },
      created: function(){
        scrollTo(0,0);
        $('body').css('overflow-x','hidden');
        poi.replaceUrl("/" + this.state.siteImg + "/" + poi.cutUrl(this.state.iframeUrl),poi.cutUrl(this.state.iframeUrl));
        //获取url
        var o = {},_this = this;
        o.site = poi.siteList[this.state.siteImg];
        if("/" + poi.cutUrl(this.state.iframeUrl) != o.site.default_path && poi.cutUrl(this.state.iframeUrl)){
          o.url = this.state.iframeUrl || poi.cutUrl(poi.siteList[o.site].url,'0',3) + "/" + poi.cutUrl(window.location.href,4);
          this.get_items(o.url);
        }
        bus.$on('gi',function(v){
          _this.get_items(v);
        })
      },
      mounted: function() {
        $('.ban-explain input').on('focus',function(){
          $(this).select();
        })
      },
      computed: {
        item: function() {
          var o = {},items = this.items[this.itemIndex];
          if(!this.items) return o;
          o.price = items.price;
          o.handling_fee = 0;
          $.each(items.fees,function(i,e){
            if(e.fee_type == 10){
              o.handling_fee = e.rmb;
            }
            if(e.fee_type == 0) {
              o.price = e.rmb;
            }
          })
          o.handling_fee_original = items.handling_fee_original;
          o.inventory = items.product.stock_zh;
          o.total = items.price;
          o.in_stock = items.product.in_stock;
          o.source_id = items.product.source_id;
          if(/img18/.test(items.product.pagedata.imgUrl) || (poi.siteList[this.state.siteImg].visible != 0)){
            o.img = '/img/r18.png';
          }else {
            o.img = items.product.pagedata.imgUrl;
          }
          if(poi.siteList[this.state.siteImg].original_freight){
            o.ship_charge = Math.ceil(Number(poi.siteList[this.state.siteImg].original_freight) * poi.exchange);
          }else {
            o.ship_charge = 0;
          }
          return o;
        },
        information: function() {
          var o = {};
          o.url = this.state.iframeUrl;
          o.link = this.state.iframeUrl;
          o.link_text = poi.cutUrl(poi.siteList[this.state.siteImg].url,2,3);
          o.site_name = poi.siteList[this.state.siteImg].name;
          o.proxy_name = poi.siteList[this.state.siteImg].proxy_name.replace(/[a-z]/g, function(ch) {return ch.toUpperCase()});
          //判断是否为不可展示页面
          if(poi.siteList[this.state.siteImg].visible != 0) {
            o.explain = '(/ω＼)';
            o.bg = 'background-image:url(/img/r18-bg.png)';
          }else {
            o.explain = '正在入侵，占领指日可待哼╭(╯^╰)╮';
            if(this.state.siteImg == 'toranoana'){
              o.bg = 'background-image:url(/img/' + this.state.siteImg +'-bg.png)';
            }else {
              o.bg = 'background-image:url(/img/r18-bg.png)';
            }
          }
          return o;
        }
      },
      methods: {
        get_items: function(v) {
          if(this.$refs.visible_url){
            if(!this.$refs.visible_url.value && !v) return poi.msg('请输入商品链接','danger');
          }
          var _this = this,_url = v || this.$refs.visible_url.value;
          var proxy = poi.proxyUrl(_url);
          if(proxy.site == 'amazon') {
            return poi.msg('亚马逊不行哦~','danger');
          }
          if(poi.siteList[proxy.site] && poi.siteList[proxy.site].visible != 0 && !v){
            location.href = proxy.path;
            return false;
          }
          vue_data.setIframeUrl(_url);
          _this.items = '';
          this.isAjax = true;
          $.ajax({
            url: poi.corsUrl + '/index/crawl',
            data: {
              url: _url,
            },
            success: function(data) {
              if(!data.items){
                location.href = "/o?url=" + encodeURIComponent(poi.encrypt33(_url));
              }
              //修改页面信息
              vue_data.setSiteImg(proxy.site);
              //去除ajax效果
              _this.isAjax = false;
              _this.items = data.items;
              vue_data.setFavoriteTitle(data.items[0].product.title);
              poi.replaceUrl("/" + proxy.site + "/" + poi.cutUrl(_url),_url);
              document.title = "任你购 - " + poi.siteList[proxy.site].name + " - " + data.items[0].product.title;
              //获取收藏
              $.ajax({
                url: poi.corsUrl + '/cart/getFavoriteInfo',
                data: {
                  url:_url
                },
                success: function(data) {
                  if(data.favorite_id) {
                    vue_data.setFavoriteInfo(data.favorite_id);
                  }else {
                    vue_data.setFavoriteInfo(false);
                  }
                },
                error: function() {
                  vue_data.setFavoriteInfo(false);
                }
              })
            },
            error: function() {
              _this.isAjax = false;
            }
          })
        },
        dropdownToggle: function() {
          this.isDropdown = !this.isDropdown;
        },
        dropdown_change: function(i){
          this.itemIndex = i;
          vue_data.setFavoriteTitle(this.items[i].product.title);
          document.title = "任你购 - " + poi.siteList[this.state.siteImg].name + " - " + this.items[i].product.title;
          this.isDropdown = false;
        },
        addCart: function() {
          var _this = this,cartInf={};
          this.isAjax = true;
          cartInf.url = this.$refs.visible_url.value || this.information.url;
          cartInf.amount = ['1'];
          cartInf.source_id = [this.item.source_id];
          $.ajax({
            url: poi.corsUrl + "/cart/add",
            data: cartInf,
            success: function(data) {
              vue_data.setCartNumber(data.badge + data.req_badge);
              poi.msg("添加商品成功ヾ(Ő∀Ő๑)ﾉ")
              _this.isAjax = false;
            },
            error: function() {
              _this.isAjax = false;
            }
          })
        },
        get_favoriteGroup: function () {
          $.ajax({
            url: poi.corsUrl + '/favorite/listFavoriteGroup',
            success: function(data) {
              vue_data.getFavoriteGroup(data);
            }
          })
        },
        favoriteGroup_change: function () {
          if(!(this.information.url || this.$refs.visible_url.value)){
            return poi.msg('请输入商品链接','danger');
          }
          if(!this.state.userInfo){
            ga('send', 'event', 'top', 'login', {transport: 'beacon'});
            location.href = 'https://user.rennigou.jp/service/login?next_url=http%3A%2F%2Frennigou.jp%2Flogin%3Fredirect%3D' + this.state.pageUrl;
