      <h4 class="notice-subtitle">{{_notice[noticeIndex].subtitle}}</h4>
      <div class="notice-content" v-html="_notice[noticeIndex].remark"></div>
      <div class="notice-bottom">
        <button class="btn-red btn-press" @click="$emit('nc')">我知道了</button>
      </div>
      <i class="left isBtn" @click="to('prev')" v-show="_notice.length > 1"></i>
      <i class="right isBtn" @click="to('next')" v-show="_notice.length > 1"></i>
    </div>
