<script>
Vue.component('requests-edit',{
  template: '#requests-edit-box',
  props: {
    'requests': Object,
    'isLink': {
      type: Boolean,
      default: true
    }
  },
  data:function(){
    return {
      state: vue_data.state,
      isAjax: false,
    }
  },
  computed: {
    favorite_img: function() {
      if(this.state.is_favoriteInfo){
        return '/img/fav-sel.png';
      }else {
        return '/img/fav-nor.png';
      }
    },
  },
  methods: {
    key_number: function(e){
      if(e.target.value > 99) e.target.value = 99;
      if(e.target.value < 1) e.target.value = 1;
      e.target.value = e.target.value.replace(/\D/g,'')
      this.amount = e.target.value;
    },
    get_cart: function () {
      $.ajax({
        url: poi.corsUrl + '/cart/jsonCart',
        success: function (data) {
          vue_data.getCart(data);
        }
      })
    },
    confirm: function(){
      var _this = this;
      _this.isAjax = true;
      $.ajax({
        url: poi.corsUrl + "/cart/editRequest",
        data: {
          variation: this.$refs.requests_variation.value,
          amount: this.$refs.amount.value,
          remark: this.$refs.requests_remark.value,
          title: this.$refs.requests_title.value,
          guid: this.requests.guid,
          unit_price: this.$refs.unit_price.value
        },
        success: function(data){
          _this.get_cart();
          _this.$emit('ccc');
          _this.isAjax = false;
        },
        error: function() {
          _this.isAjax = false;
        }
      })
    },
    add_requests: function() {
      var _this = this;
      _this.isAjax = true;
      $.ajax({
        url: poi.corsUrl + "/cart/addRequest",
        data: {
          variation: this.$refs.requests_variation.value,
          amount: this.$refs.amount.value,
          remark: this.$refs.requests_remark.value,
          title: this.$refs.requests_title.value,
          url: this.requests.url,
          unit_price: this.$refs.unit_price.value
        },
        success: function(data){
          vue_data.setCartNumber(data.badge + data.req_badge);
          poi.msg("添加申请成功ヾ(Ő∀Ő๑)ﾉ")
          _this.isAjax = false;
        },
        error: function() {
          _this.isAjax = false;
        }
      })
    },
    get_favoriteGroup: function () {
      $.ajax({
        url: poi.corsUrl + '/favorite/listFavoriteGroup',
        success: function(data) {
          vue_data.getFavoriteGroup(data);
        }
      })
    },
    favoriteGroup_change: function () {
      vue_data.favoriteGroupToggle();
      this.get_favoriteGroup();
    },
    login: function() {
      ga('send', 'event', 'edit_request', 'login', {transport: 'beacon'});
      location.href = 'https://user.rennigou.jp/service/login?next_url=http%3A%2F%2Frennigou.jp%2Flogin%3Fredirect%3D' + this.state.pageUrl;
    },
  }
})
</script><script type="text/x-template" id="requests-link-block">
  <a :href="jump.url" :target="jump.blank"><span>{{link.title}}</span></a>
</script>

<script>
Vue.component('requests-link',{
  template: '#requests-link-block',
  props: ['link'],
  computed: {
    jump: function(){
      var pageurl = this.link.url,o = {};
      o.url = pageurl;
      o.blank = '_blank';
      $.map(poi.siteList,function(e,i){
        if(e.url.indexOf(poi.cutUrl(pageurl,2,3)) != -1){
          o.url = "/" + i + "/" + poi.cutUrl(pageurl);
          o.blank = '_self';
        }
      })
      return o;
    },
  }
})
</script><script>
$(function(){
    Vue.component('cart-sidebar', {
    template: '#rng-cart-sidebar',
    data:function(){
      return {
        state: vue_data.state,
        cartActive: "goods",
        goodsCheck:[],
        requestsCheck:[],
        requests_no_price: false,
        siteCheck:[],
        cancel_all:[],
        isInvalid:false,
        explain:{},
        isWarning: false,
        isRequestsSubmit: false,
        RequestsEdit: false,
        cart: poi.corsUrl,
        ship_safe: true,
        is_ship_tip: false,
        shipTipTrue: '',
        shipTipFalse: '',
      }
    },
    updated: function () {
      $('[data-toggle="tooltip"]').tooltip();
      $('[data-toggle="popover"]').popover();
    },
    computed: {
      cart_results: {
        get: function(){
          var cart_data = {
            goods:[],
            requests:[],
            goods_invalid: [],
            requests_invalid: [],
            price_total: 0,
            goods_length: 0,
            requests_length: 0,
            site_length: 0,
          },
          price_total = 0,
          goods_length = 0,
          requests_length = 0,
          requests_amount = 0,
          proxy = '';
          status = true;
          if(this.state.cart.cart){
            this.state.cart.cart.items.forEach(function(e,i){
              cart_data.goods[i] = {};
              cart_data.goods[i].addition = {};
              cart_data.goods[i].price = e.price;
              cart_data.goods[i].jpy_price = parseInt(e.jpy_price);
              cart_data.goods[i].guid = e.guid;
              cart_data.goods[i].title = e.product.title;
              cart_data.goods[i].amount = e.amount;
              cart_data.goods[i].variation = e.product.variation;
              proxy = poi.proxyUrl(e.url);
              cart_data.goods[i].url = proxy.path;
              cart_data.goods[i].img = {
                backgroundImage:'url(/img/' + proxy.site + '.png)',
              };
              cart_data.goods[i].in_stock = e.product.in_stock;
              cart_data.goods[i].limit = e.limit;
              cart_data.goods[i].condition = e.product.condition;
              cart_data.goods[i].cart_item_id = e.cart_item_id;
              cart_data.goods[i].fees = e.fees;
              //发售日期
              cart_data.goods[i].release_date = e.product.release_date > 0 ? '发行日期：' + poi.formatDate(new Date(e.product.release_date*1000)) : '未知';
              //商品信息
              if(e.addition){
                cart_data.goods[i].addition.merchant_name = e.addition.merchant_name;
                if(e.addition.condition) cart_data.goods[i].addition.condition_detail = '品相：' + e.addition.condition.condition_detail;
              }
              //失效商品
              if(e.product.in_stock == 0 || e.limit == 0) {
                cart_data.goods_invalid.push(e.guid);
              }
            })
            //计算商品总数&总价
            cart_data.goods.forEach(function(e,i){
              if(e.in_stock != 0){
                price_total = price_total+Number(e.price);
              }
              goods_length = goods_length+Number(e.amount);
            })
            cart_data.price_total = price_total;
            cart_data.goods_length = goods_length;
          }
          if(this.state.cart.sites_requests){
            cart_data.requests = this.state.cart.sites_requests;
            cart_data.site_length = this.state.cart.sites_requests.length;
            this.state.cart.sites_requests.forEach(function(e,i) {
              requests_length = requests_length + e.requests.length;
              status = e.status;
              e.requests.forEach(function(e,i){
                requests_amount = requests_amount + Number(e.amount);
                //失效商品
                if(status == 2) {
                  cart_data.requests_invalid.push(e.guid);
                }
              })
            });
            cart_data.requests_length = requests_length;
            cart_data.requests_amount = requests_amount;
          }
          cart_data.part_settings = this.state.cart.part_settings;
          return cart_data;
        }
      },
      goods_total: function () {
        var o = {},_this = this;
        o.goods_length = this.goodsCheck.length;
        if(this.goodsCheck.length == 0) return o;
        o.price_total = this.cart_results.goods.reduce(function (sum, item) {
          if(_this.goodsCheck.indexOf(item.guid) != -1) {
            return sum + Number(_this.state.isRMB ? item.price : item.jpy_price);
          }else {
            return sum;
          }
        }, 0);
        return o;
      },
      req_site: function() {
        var _this = this,a = [];
        for(var i = 0; i < this.state.cart.sites_requests.length; ++i) {
          for(var j = 0; j < this.state.cart.sites_requests[i].requests.length; ++j) {
            var index = this.requestsCheck.indexOf(this.state.cart.sites_requests[i].requests[j].guid);
            if(index !== -1) {
              a.push(this.state.cart.sites_requests[i].site_id);
              break;
            }
          }
        }
        return a;
      },
      isDel: function() {
        var flag = false;
        if(this.cartActive == 'goods'){
          flag = this.goodsCheck.length > 0;
        }else {
          flag = this.requestsCheck.length > 0;
        }
        return flag;
      },
      emptyShow: function(){
        var flag = false;
        if(this.cartActive == 'goods'){
          flag = this.cart_results.goods_invalid.length > 0;
        }else {
          flag = this.cart_results.requests_invalid.length > 0;
        }
        return flag;
      },
    },
    watch: {
      requestsCheck: function () {
        //同站点全选
        var _this = this,selected,has_no_price = false;
        this.state.cart.sites_requests.forEach(function(e,i) {
          $.each(e.requests,function(i,e){
           selected = _this.requestsCheck.indexOf(e.guid) != -1;
           if(!selected) return false;
          })
          if(selected){
            if(_this.siteCheck.indexOf(e.site_id) == -1){
              _this.siteCheck.push(e.site_id);
            }
          }else {
            if($.inArray(e.site_id,_this.siteCheck) != -1){
              _this.siteCheck.splice($.inArray(e.site_id,_this.siteCheck),1);
            }       
          }
          $.each(e.requests,function(i,_e){
            if($.inArray(_e.guid,_this.requestsCheck) != -1 && _e.unit_price == 0) has_no_price = true;
            if(has_no_price) return false;
          })
          if(has_no_price){
            _this.requests_no_price = true;
          }else {
            _this.requests_no_price = false;
          }
        })
      }
    },
    methods: {
      modalClose: function () {
        vue_data.sidebarToggle();
        Vue.set(this.explain,'site_id',false)
      },
      set_cartActive: function (val) {
        this.cartActive = val;
      },
      cart_input: function(data,e) {
        var number = e.target.value,
          _this = this,
          val = {};
        if(number<1||number>99)return e.target.value=e.target._value;
        val.guid = data.guid;
        val.amount = number;
        if(this.cartActive == 'goods'){
