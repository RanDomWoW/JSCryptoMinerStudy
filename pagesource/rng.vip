    <button v-if="!requests.add && !isAjax" class="confirm" @click="confirm">确定</button>
    <button v-if="!requests.add && isAjax" class="confirm"><img src="/img/loading.svg"></button>
    <button v-show="requests.add && requests.title" type="button" @click="add_requests" class="btn-pool-gradient cart-btn pull-right"><img :src="isAjax ? '/img/loading.svg' : '/img/cart-add.png'" class="pool-icon" :disabled = "isAjax">加入购物车</button>
  </div>
  </transition>  
