            <i class="favorite-img isBtn" :class="{'favorite-sel-img': state.favoriteGroupPopup || state.is_favoriteInfo}" ></i>
            <span>喜欢</span>
          </div>
          <rng-jisthis></rng-jisthis>
        </div>
      </div>
    </div>
