  <script>
    $(function(){     
      var rng_app = new Vue({
        el:"#rng-app",
        data: {
          state: vue_data.state,
          isBuyMenu: false,
          isExplain: !localStorage["rng_isExplain"],
          isDropdown: false,
          isAjax: false,
          infSelected: false,
          isNotice: false,
          notice:{
            title:'网站购买须知',
            subtitle:'',
            remark:''
          },
        },
        computed: {
          requests: function(){
            return {
              url: this.state.iframeUrl,
              add: true,
              title: this.state.iframeTitle,
              buyMenu: true,
            }
          },
          favorite_img: function() {
            if(this.state.buyMenu){
              if(this.state.is_favoriteInfo) {
                return '/img/fav-sel.png';
              }
              return '/img/fav-nor.png';
            }else {
              return '/img/fav-w.png';
            }
          },
          buyMenu_inf: function() {
            if(!this.state.buyMenu){
              this.infSelected = false;
            }
            var o = JSON.parse(JSON.stringify(this.state.buyMenu));
            o.jp_price = o.price;
            o.price = Math.ceil(o.price * poi.exchange);
            o.fee = o.handling_fee ? o.handling_fee : o.handling_fee_original;
            o.total = o.handling_fee ? Number(o.price) + Math.ceil(Number(o.handling_fee) * Number(poi.exchange)) : Number(o.price);
            o.jpy_total = o.handling_fee ? Number(o.jp_price) + Number(o.fee) : Number(o.jp_price);
            if(poi.siteList[this.state.siteImg].original_freight){
              if(o.merchants == 'other'){
                o.jpy_ship = o.ship_charge;
                o.ship_charge = Math.ceil(Number(o.ship_charge) * poi.exchange);
                o.total += o.ship_charge;
                o.jpy_total += Number(o.jpy_ship);
              }else {
                o.jpy_ship = Number(poi.siteList[this.state.siteImg].original_freight);
                o.ship_charge = Math.ceil(Number(poi.siteList[this.state.siteImg].original_freight) * poi.exchange);
              }
            }
            return o;
          },
          site_name: function() {
            return poi.siteList[this.state.siteImg].name;
          }
        },
        methods: { 
          get_favoriteGroup: function () {
            $.ajax({
              url: poi.corsUrl + '/favorite/listFavoriteGroup',
              success: function(data) {
                vue_data.getFavoriteGroup(data);
              }
            })
          },
          favoriteGroup_change: function () {
            if(!this.state.userInfo){
              ga('send', 'event', 'fav_group', 'login', {transport: 'beacon'});
              location.href = 'https://user.rennigou.jp/service/login?next_url=http%3A%2F%2Frennigou.jp%2Flogin%3Fredirect%3D' + this.state.pageUrl;
              return false;
            }
            if(this.state.is_favoriteInfo) {
              vue_data.del_favorite(this.state.is_favoriteInfo);
              return false
            };
            vue_data.favoriteGroupToggle();
            this.get_favoriteGroup();
          },
          explain_close: function() {
            localStorage["rng_isExplain"] = true;
            this.isExplain = false;
          },
          buyMenuFalse: function() {
            this.isBuyMenu = false;
            vue_data.isbuyMenuFalse();
          },
          to_gp: function() {
            poi.iframeSend('toGp');
          },
          addCart: function() {
            var cartInf = JSON.parse(JSON.stringify(this.state.buyMenu)),_this = this;
            cartInf.url =  this.state.iframeUrl;
            this.isAjax = true;
            if(this.state.siteImg == 'amazon'){
              $.ajax({
                url: poi.corsUrl + "/cart/addAmazon",
                data: cartInf,
                success: function(data) {
                  vue_data.setCartNumber(data.badge + data.req_badge);
                  if(data.message) {
                    poi.msg(data.message,'danger');
                  }
                  if(data.overload){
                    poi.msg(data.overload_message,'danger');
                  }else {
                    poi.msg("添加商品成功ヾ(Ő∀Ő๑)ﾉ")
                  }
                  _this.isAjax = false;
                },
                error: function() {
                  _this.isAjax = false;
                }
              })
            }else {
              cartInf.source_id = [cartInf.source_id];
              cartInf.amount = [cartInf.amount];
              $.ajax({
                url: poi.corsUrl + "/cart/add",
                data: cartInf,
                success: function(data) {
                  vue_data.setCartNumber(data.badge + data.req_badge);
                  if(data.message) {
                    poi.msg(data.message,'danger');
                  }
                  if(data.overload){
                    poi.msg(data.overload_message,'danger');
                  }else {
                    poi.msg("添加商品成功ヾ(Ő∀Ő๑)ﾉ")
                  }
                  _this.isAjax = false;
                },
                error: function() {
                  _this.isAjax = false;
                }
              })
            }
          },
          dropdownToggle: function() {
            this.isDropdown = !this.isDropdown;
          },
          detailsChange: function (v,i) {
            if(this.state.infs[i].handling_fee){
              vue_data.buyMenuToggle(this.state.infs[i]);
              this.infSelected = this.state.infs[i].name;
            }else {
              vue_data.setBuyMenu({
                site_id: poi.siteList[this.state.siteImg].site_id,
                url: poi.cutUrl(poi.siteList[this.state.siteImg].url,'0',3) + "/" + poi.cutUrl(this.state.infs[i].url),
                price: this.state.infs[i].price,
                stock: this.state.infs[i].stock,
              },this.state.infs,i)
            }
            poi.iframeSend('anchor',v);
            this.isDropdown = false;
          },
          isNotice_close: function() {
            this.isNotice = false;
          },
          get_siteInf: function() {
            var _this = this;
            $.ajax({
              url: poi.corsUrl + '/index/crawl',
              data: {
                url: this.state.iframeUrl,
              },
              success: function(data) {
                _this.notice.subtitle = data.site.host;
                _this.notice.remark = data.site.remark;
                _this.isNotice = true;
              }
            })
          },
        }
      })
      var _resize =  function () {
        if($('.ban-background')[0]){
          $('.ban-background')[0].style.zoom = (window.innerHeight / 1024) > 1 ? (window.innerHeight / 1024) : null;
        }
      };
      _resize();
      window.addEventListener('resize', _resize);
    })
  </script>
