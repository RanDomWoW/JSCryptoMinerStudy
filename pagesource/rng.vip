<script>
Vue.component('sidebar',{
  template: '#rng-sidebar',
  props:['showId','close'],
  data:function(){
    return {
      state: vue_data.state,
      isClose: true,
    }
  },
  computed: {
  },
  methods: {
    sidebar_close:function() {
      this.isClose = false;
      vue_data._sidebar_remarkToggle(false);
    },
    popup_enter: function() {     
      var width = window.innerWidth - document.body.clientWidth;
      if(width != 0) {
        $('body').css({"overflow":"hidden","padding-right":width});
        $("#rng-nav").css({"padding-right":width+24});
      }
    },
    popup_leave: function() {
      if($('#iframe').length <= 0) {
        $('body').attr("style","")
        $('#rng-nav').attr("style","")
      }else {
        if($('#iframe').css('display') == 'none'){
          $('body').css({'overflow-y': 'auto', 'overflow-x': 'hidden',"padding-right":''})
          $('#rng-nav').attr("style","")
        };
      }
      //动画结束时关闭组件
      bus.$emit('sidebar',this.showId);
    },
  }
})
