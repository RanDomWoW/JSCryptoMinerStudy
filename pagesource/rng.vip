        var width = window.innerWidth-document.body.scrollWidth;
        if($('#iframe').length <= 0 || $('#iframe').css('display') == 'none') { 
          $('body').css({"overflow":"hidden","padding-right":width});
          $("#rng-nav").css({"padding-right":width+24});
        }
      },
      popup_leave: function() {
        if($('#iframe').length <= 0) {
          $('body').attr("style","")
          $('#rng-nav').attr("style","")
        }else {
          $('#iframe').css('display') == 'none' ? $('body').css({'overflow-y': 'auto', 'overflow-x': 'hidden'}) : false;
        }
      },
      checkedAll: function () {
        var _this = this;
        if(this.cartActive == 'goods'){ 
          //清空
          var goodsArr = [];
          this.cart_results.goods.forEach(function(e,i) {
            if(e.in_stock != 0 && e.limit != 0) {
              goodsArr.push(e.guid);
            }
          });
          if (this.goodsCheck.length == goodsArr.length) {
            this.goodsCheck = [];
          //全选
          }else{
            this.goodsCheck = [];
            this.cart_results.goods.forEach(function(e,i) {
              if(e.in_stock != 0 && e.limit != 0) {
                _this.goodsCheck.push(e.guid);
              }
            });
          }
        }else if(this.cartActive == 'requests') {
          var reqArr = [];
          this.cart_results.requests.forEach(function(e,i){
            if(e.status != 2){
              reqArr.push(e.site_id);
            }
          })
          if(this.siteCheck.length == reqArr.length) {
            this.requestsCheck = [];
          }else {
            this.cart_results.requests.forEach(function(e,i){
              if(e.status != 2){
                e.requests.forEach(function(_e,i){
                  if($.inArray(_e.guid,_this.requestsCheck) == -1)
                  _this.requestsCheck.push(_e.guid);
                })
              }
            })
          }
        }
      },
      site_CheckAll: function (data,onlyCheckAll) {
        var _this = this;
        if(_this.siteCheck.indexOf(data.site_id) != -1 && onlyCheckAll === undefined) {
          data.requests.forEach(function(e,i){
            _this.requestsCheck.splice($.inArray(e.guid,_this.requestsCheck),1)
          })
        }else {
          data.requests.forEach(function(e,i){
            if($.inArray(e.guid,_this.requestsCheck) == -1)
            _this.requestsCheck.push(e.guid);
          })
        }
      },
      RequestsEdit_close: function(){
        this.RequestsEdit = false;
      },
      set_explain: function(data) {
        Vue.set(this.explain,'site_id',data.site_id);
        Vue.set(this.explain,'host',data.host);
        Vue.set(this.explain,'remark',data.remark);
      },
      redirect_to_checkout: function() {
        if(this.goodsCheck.length < 1) {
          return poi.msg("请选择购物车商品","danger");
        }
        this.$refs.cart.submit();
      },
      setRequestsSubmit: function() {
        if(this.requestsCheck.length < 1) {
          return poi.msg("请选择代购申请","danger");
        }
        this.isRequestsSubmit = true;
      },
      req_submit: function() {
        this.$refs.req.submit();
      },
      login: function() {
        ga('send', 'event', 'cart', 'login', {transport: 'beacon'});
        location.href = 'https://user.rennigou.jp/service/login?next_url=http%3A%2F%2Frennigou.jp%2Flogin%3Fredirect%3D' + this.state.pageUrl;
      },
      toText: function(text) {
        return (text || '').replace(/<.+?>/g, '');
      },
      set_requests_edit: function(current,parent) {
        this.RequestsEdit = {
          current:current,
          parent:parent
        }
      },
      show_ship_tip: function(boolean,ev) {
        var _this = this;
        if(boolean) {
          clearTimeout(this.shipTipFlase);
          this.shipTipTrue = setTimeout(function() {
            _this.is_ship_tip = true;
            var ship_tip_icon = $('.heading.ship').offset();
            $('.ship-tip').css({'top':ship_tip_icon.top + 58,'left':56})
          }, 200);
        }else {
          clearTimeout(this.shipTipTrue);
          this.shipTipFlase = setTimeout(function() {
            _this.is_ship_tip = false;
          }, 200);
        }
      },
      item_price: function(data) {
        if(this.state.isRMB){
          return data.price + '元'
        }else {
          return data.jpy_price + '円'
        }
      },
      fees_content: function(fees) {
        var content = [];
        fees.forEach(function(e,i){
          var str = poi.consts['fee_type'][e.fee_type] + ': ';
          if(this.state.isRMB) {
            str += e.rmb + ' 元';
          } else {
            str += e.sum + ' 円';
          }
          content.push(str);
        },this);
        return content.join('<br/>');
      },
    }
  })
})
