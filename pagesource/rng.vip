<script>
Vue.directive('swiper', {
  bind: function(el,binding){
    $(el).data('swiper',new Swiper(el, binding.value));
  },
  update: function(el){
    setTimeout(function() {
      $(el).data('swiper').update(false);
    },0)
  }
})
Vue.component('mall-sidebar',{
  template: '#rng-mall-sidebar',
  data:function(){
    return {
      state: vue_data.state,
      goods: {},
      items:{},
      tradeNo: '',
      isOrder: false,
      isPay: false,
      isPayAgain: false,
      swiper: {
        slidesPerView: 'auto',
        paginationClickable: true,
        spaceBetween: 24,
        scrollbarHide: true,
      },
      goodsType:{
        list:[],
        pid: {},
        index: 0,
        active: [],
        active_pid: false
      },
      pid_type: false,
      type_list: [],
      pids:{},
      amount: 0,
      demand:'',
      allowSupplement: false,
    }
  },
  created: function() {
    this.goods = this.state.seriesData;
  },
  watch:{
    amount: function(){
      if(!this.goodsType.pid[this.pid_type]) return;
      Vue.set(this.pids[this.goodsType.pid[this.pid_type]],'amount',this.amount);
    }
  },
  computed: {
    product_type: function(){
      var typeArr = [],_this = this,numArr = [].push(this.goodsType.list);
      if(!this.goods.s_names) return typeArr;
      this.goods.s_names.forEach(function(e,i) {
        typeArr[i] = {};
        typeArr[i].key = e;
        typeArr[i].list = [];
        _this.goods.s_map[e].forEach(function(_e,_i){
          typeArr[i].list[_i] = {};
          typeArr[i].list[_i].name = _e;
        })
      });
      return typeArr;
    },
    cart: function(){
      var o ={};
      o.amount = 0;
      o.total = 0;
      o.deposit = 0;
      for(pid in this.pids){
        o.amount += Number(this.pids[pid].amount);
        o.total += Number(this.pids[pid].amount) * Number(parseInt(this.pids[pid].price));
        o.deposit += Number(this.pids[pid].amount) * (Number(parseInt(this.pids[pid].deposit)) || 0);
      }
      return o;
    },

  },
  methods: {
    remarkToggle:function() {
      vue_data._sidebar_remarkToggle();
    },
    type_switch:function(type,index,_index) {
      var _this = this,_pid,_list,activeList;
      this.goodsType.index = index;
      //判断当前层数
      if(this.goodsType.list.length >= index + 1){
        this.goodsType.list.splice(index ,this.goodsType.list.length);
        this.goodsType.list.push(type);
      }else {
        this.goodsType.list.push(type);
      }
      //添加当层active
      this.goodsType.active.splice(index,this.goodsType.active.length);
      this.goodsType.active.push(type);
      //判断是否存在下一层
      if(_list = _.get(this.goods.series_products,this.goodsType.list)){
        //判断是否存在pid
        if(_pid = _list.pid){
          Vue.set(_this.goodsType.pid, _this.goodsType.active[_this.goodsType.active.length - 2] + type,_pid);
          //创建pids内对象
          if(!_this.pids[_pid]){
            Vue.set(_this.pids,_pid,{amount:0,price:_list.price || 0,deposit:_list.pagedata.deposit || 0,in_stock:_list.in_stock});
          }
          //防止当前type重名，记录当前+上一级type名称
          _this.pid_type = _this.goodsType.active[_this.goodsType.active.length - 2]  + type;
          _this.goodsType.active_pid = true;
        }else {
          _this.goodsType.active_pid = false;
          this.type_list.splice(index ,this.type_list.length);
          Vue.set(_this.type_list,index,_list)
        }
      }
    },
    type_disable: function(type,index) {
      if(index - 1 < 0){
        return true
      }else {
        return _.has(this.type_list[index-1],type);
      }
    },
    type_number: function(type,index,_index) {
      var num = 0,_goods,_this = this;
      if(index - 1 < 0){
        type_loop(this.goods.series_products,type);
      }
      if(_goods = _.get(this.goods.series_products,this.goodsType.active)){
        if(_goods.pid){
          //判断当前选择是否为最后一层
          if(_goods.pid == _this.goodsType.pid[_this.pid_type] && _this.goodsType.active[_this.goodsType.active.length - 2] + type == _this.pid_type){
            num += _this.pids[_goods.pid] ? _this.pids[_goods.pid].amount : 0;
          }else {
            num += _this.pids[_this.goodsType.pid[_this.goodsType.active[_this.goodsType.active.length - 2] + type]] ? _this.pids[_this.goodsType.pid[_this.goodsType.active[_this.goodsType.active.length - 2] + type]].amount : 0;
          }
          return num;
        }else {
          type_loop(_goods,type);
        }
      }
      //递归增加数量
      function type_loop (goods,i){
        if(goods[i] && goods[i].pid){
          var arr = [];
          for(good in goods){
            arr.push(goods[good].pid);
          }
          //判断是否属于当前层下层
          if(arr.indexOf(_this.goodsType.pid[_this.goodsType.active[_this.goodsType.active.length - 1] + i]) != -1){
            num += _this.pids[_this.goodsType.pid[_this.goodsType.active[_this.goodsType.active.length - 1] + i]] ? _this.pids[_this.goodsType.pid[_this.goodsType.active[_this.goodsType.active.length - 1] + i]].amount : 0;
          }
          return;
        }
        for (var good in goods[i]) {
          if (goods[i][good].pid) {
            if(_this.pids[goods[i][good].pid]){
              num += (Number(_this.pids[goods[i][good].pid].amount) || 0);
            }
          } else {
            type_loop(goods[i][good],i);
          }
        }
      }
      return num;
    },
    order_submit: function() {
      var _this = this,val = {
        "pids":[],
        "amounts":{},
        "url": location.href,
        "demand": _this.demand,
        "allow_supplement": _this.allowSupplement ? 1: 0
      },form = window.open();
      $.map(_this.pids,function(e,i){
        if(e.amount > 0){
          val.pids.push(i);
          val.amounts[i] = e.amount;
        }
      })
      $.ajax({
        url: poi.corsUrl + "/checkout/checkoutSelfProduct",
        data:val,
        success: function(data){
          _this.tradeNo = data.trade_no;
          _this.items = data.items;
          form.location = poi.corsUrl + "/checkout/pay?trade_no=" + _this.tradeNo;
          _this.isPay = true;
        }
      })
    },
  },
  components: {
    'amount': {
      template: '<div class="amount-input">' +
                  '<button class="minus" type="button" @click="inner_amount > 0 ? inner_amount =  Number(inner_amount) - 1 : false"></button>' +
                  '<input type="text" class="number" v-model="inner_amount">' +
                  '<button class="plus" type="button" @click="inner_amount = Number(inner_amount) + 1"></button>' +
                '</div>',
      props: ['pid', 'number'],
      data: function () {
        return { inner_amount: 0};
      },
      created: function () {
        this.inner_amount = this.number;
      },
      methods: {
      },
      watch: {
        inner_amount: function () {
          if(this.inner_amount > 99) this.inner_amount = 99;
          this.$emit('input', isNaN(this.inner_amount) ? 0 : Number(this.inner_amount));
        },
        pid: function () {
          this.inner_amount = this.number;
        }
      }
    },
  }
})
</script><script type="text/x-template" id="notice-box">
  <transition name="notice-fade">
