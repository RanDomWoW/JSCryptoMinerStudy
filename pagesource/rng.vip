<script>
Vue.component('auction-request',{
  template: '#auction-request-box',
  props: {
    'auction': Object,
  },
  data:function(){
    return {
      state: vue_data.state,
      isDecide: false,
      winPrice: '',
      amount: 1,
      requests_remark: '',
      ship_safe: false,
      cart: poi.corsUrl,
      isAjax: false
    }
  },
  created:function () {
    //一口价模式
    if(this.auction.winPrice == this.auction.price){
      this.isDecide = true;
      this.winPrice = this.auction.winPrice;
    }
  },
  computed: {},
  methods: {
    key_number: function(e){
      if(e.target.value > 99) e.target.value = 99;
      if(e.target.value < 1) e.target.value = 1;
      e.target.value = e.target.value.replace(/\D/g,'')
      this.amount = e.target.value;
    },
    auction_bid:function() {
      if((this.auction.winPrice != 0) && this.auction.winPrice <= this.winPrice && !this.isDecide) return poi.msg('竞拍出价需小于一口价:' + this.auction.winPrice + "円",'danger');
      this.isAjax = true;
      var _this = this,$form;
      this.$nextTick(function(){
        $form = $(_this.$refs.cart).serializeArray();
        $.ajax({
          url: poi.corsUrl + "/checkout",
          data: $form,
          success: function(data) {
            $(_this.$refs.cart).submit();
            _this.isAjax = false;
          },
          error: function() {
            _this.isAjax = false;
          }
        })
      })

    },
    set_decide:function() {
      this.isDecide = !this.isDecide;
      this.winPrice = this.auction.winPrice;
    }
  }
})
