<script>
$(function(){
  Vue.component('favorite-group-popup',{
    template: '#rng-favorite-group-popup',
    data:function(){
      return {
        state: vue_data.state,
        dropdown: false,
        currentGroup: '未分组',
        isNewGroup: false,
        groupName: '',
        groupId: '',
        isAjax: false,
      }
    },
    methods: {
      modalClose: function () {
        vue_data.favoriteGroupToggle();
        this.dropdown = false;
      },
      dropdownToggle: function () {
        this.dropdown = !this.dropdown;
      },
      newGroupToggle: function () {
        this.isNewGroup = true;
        this.dropdown = false;
        this.currentGroup = '新建分组';
      },
      groupChange: function (i) {
        var current = this.state.favoriteGroup[i];
        this.currentGroup = current.name;
        this.groupId = current.group_id;
        this.isNewGroup = false;
        this.dropdown = false;
      },
      updateFavorite: function () {
        var _this = this;
        _this.isAjax = true;
        $.ajax({
          url: poi.corsUrl + '/favorite/updateFavorite',
          data: {
            url: this.state.iframeUrl,
            group_id: this.groupId?this.groupId:this.state.favoriteGroup[0].group_id,
            title: this.$refs.favoriteTitle.value
          },
          success: function(data) {
            poi.msg("收藏成功!","success");            
            _this.modalClose();
            vue_data.setFavoriteInfo(data.favorite_id);
            _this.isAjax = false;
          },
          error: function() {
            _this.isAjax = false;
          }
        })
      },
      updateFavoriteGroup: function () {
        var _this = this;
        $.ajax({
          url: poi.corsUrl + '/favorite/updateFavoriteGroup',
          data: {
            url: this.state.iframeUrl,
            name: this.groupName,
            title: this.$refs.favoriteTitle.value,
          },
          success: function(data) {
            poi.msg("收藏成功!","success");            
            _this.modalClose();
            vue_data.setFavoriteInfo(data.favorite_id);
            _this.isAjax = false;
          },
          error: function() {
            _this.isAjax = false;
          }
        })
      },
    },
  })
})
