    <script language="javascript">
        function onLoadScript() {
            document.frmhome.txtLoginId.setAttribute('autocomplete', 'off');
            document.frmhome.txtpwd.setAttribute('autocomplete', 'off');
            document.frmhome.txtLoginId.focus();
        }
        function onKeyDownFunc(e) {
            var key = window.event ? event.keyCode : e.which;
            var keychar = String.fromCharCode(key).toLowerCase();
            var ctrl = window.event ? event.ctrlKey : e.ctrlKey;
            switch (key) {
                case 13:
                    validate()
                    break;
            }

        }
        function VerifyLoginPwd() {
            if (strim(document.frmhome.txtLoginId.value) == "") {
                alert("Please enter Login id.")
                document.frmhome.txtLoginId.focus();
                return false;
            }
            if (strim(document.frmhome.txtpwd.value) == "") {
                alert("Please enter Password.")
                document.frmhome.txtpwd.focus();
                return false;
            }
            return true;
        }
        function validate() {
            if (VerifyLoginPwd()) {
                document.frmhome.hidLogin.value = "Login";
                document.frmhome.submit();
            }
            else {
                return false;
            }
        }
        function strim(strVal) {
            strVal = strVal.replace(/^\s+/, "");
            strVal = strVal.replace(/\s+$/, "");
            return strVal;
        }
    </script>

