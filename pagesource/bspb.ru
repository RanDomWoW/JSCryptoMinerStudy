<script>
  $(document).ready(function(){
    $.ajax(
      '/BSPBWeb/jsonCardRates',
      {
        success: function (data) {
          var dataObjects = JSON.parse(data);
          setCurrencyRates($('.cur-card-rates.eur'), 'EUR', dataObjects);
          setCurrencyRates($('.cur-card-rates.usd'), 'USD', dataObjects);

          $.ajax(
            '/BSPBWeb/jsonCashRates',
            {
              success: function (data) {
                finalCashAndMiniCalcSetup(data);
              },
              timeout: 30000
            })
            .fail(function () {
              $.ajax(
                '/BSPBWeb/jsonBestCashRates',
                {
                  success: function (data) {
                    finalCashAndMiniCalcSetup(data);
                  },
                  timeout: 30000
                })
            })
        },

        timeout: 30000
      })
  });

  function finalCashAndMiniCalcSetup(data) {
    var dataObjects = JSON.parse(data);
    setCurrencyRates($('.cur-cash-rates.eur'), 'EUR', dataObjects);
    setCurrencyRates($('.cur-cash-rates.usd'), 'USD', dataObjects);
    setupMinicalc();
  }

  function setCurrencyRates(element, currency, data) {
    var currencyData = data[currency];
    setCurrencyBuyOrSell(element.find('.buy'), currencyData.buyAt, currencyData.upward);
    setCurrencyBuyOrSell(element.find('.sell'), currencyData.sellAt, currencyData.upward);
  }

  function setCurrencyBuyOrSell(element, rate, upward){
    element.text(parseFloat(rate).toFixed(2));

    if(upward === null){
      element.addClass('exchange-rate-fixed');
    } else if (upward){
      element.addClass('exchange-rate-up');
    }else {
      element.addClass('exchange-rate-down');
    }
  }

  function setupMinicalc() {
    $.get('/BSPBWeb/currencyMiniCalc', function(data){
      $('#openCalcModal').html(data);
    });
  }

</script>

