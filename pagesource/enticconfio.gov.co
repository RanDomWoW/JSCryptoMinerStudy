							console.log($(".totalDownload"));
							files = $(".totalDownload");
								for(var i=0; i<files.length;i++){
									idFile = $(files[i]).attr("id").split("-")[0];
									idElement = $(files[i]).attr("id");
									   jQuery.ajax({
										  type: "POST",
										  async:false,
										  url: "https://www.enticconfio.gov.co/rest/Api.php?url=consultarDescargaxId",
										  data: {
											"idfile" : idFile
										  },
										  success : function(response){
											   //console.log(response);
											   $("#"+jQuery.trim(idElement)).text(response.count);
										  }
										  });
								}
							
							$(".fileLink").click(function(e){
								    
									var element = e.target;
									
									if($(element).hasClass("fileLink")){
										arrClass = $(element).attr('class').split(" ");
										elementId = arrClass[1].split("-")[0];
									}else{
									   arrClass = $(element).parent().attr('class').split(" ");
									   elementId = arrClass[1].split("-")[0];
									} 
									
									
										jQuery.ajax({
										  type: "POST",
										  async:false,
										  url: "https://www.enticconfio.gov.co/rest/Api.php?url=sumarDescarga",
										  data: {
											"idfile" : elementId
										  },
										  success : function(response){
											   //console.log(response);
											   $("#"+elementId+"-file").text(parseInt($("#"+elementId+"-file").text())+1);
										  }
										  });
									 
							})
							
						});
