<script type="text/javascript" charset="utf-8">
$(document).ready(function(){
//Show Banner
$(".main_image .desc").show(); //Show Banner
$(".main_image .block").animate({ opacity: 0.71 }, 1 ); //Set Opacity
//Click and Hover events for thumbnail list
$(".image_thumb ul li:first").addClass('active');
$(".image_thumb ul li").click(function(){
//Set Variables
var imgAlt = $(this).find('img').attr("alt"); //Get Alt Tag of Image
var imgTitle = $(this).find('a').attr("href"); //Get Main Image URL
var imgDescHeight = $(".main_image").find('.block').height(); //Calculate height of block 
var Title = $(this).find('a').attr("title"); //Get Title
var Link = $(this).find('a').attr("name"); //Get Title
var Date = $(this).find('.block small').html(); //Get Title
var Description = $(this).find('.hidedesc').html(); //Get Description
if ($(this).is(".active")) { //If it's already active, then...
return false; // Don't click through
$(".main_description").fadeOut();
} else {
//Animate the Teaser 
$(".main_image .block").animate({ opacity: 0, marginBottom: -imgDescHeight }, 250 , function() {
$(".main_image .block").html("<small>"+Date+"</small><h2>"+Title+"</h2>").animate({ opacity: 0.71, marginBottom: "0" }, 250 );
$(".main_image a").attr({ href: Link , alt: Title}); 
$(".main_image img").attr({ src: imgTitle , alt: Title});
$(".main_description").html(Description).fadeIn(500);
}); 
}
$(".image_thumb ul li").removeClass('active'); //Remove class of 'active' on all lists
$(this).addClass('active'); //add class of 'active' on this list only
return false;
}) .hover(function(){
$(this).addClass('hover');
}, function() {
$(this).removeClass('hover');
});
//Toggle Teaser
$("a.collapse").click(function(){
$(".main_image .block").slideToggle();
$("a.collapse").toggleClass("show");
});

/* Масштабирование изображений */
$(".thirdpictures li").each(function(index) {
var ImageHeight = $(this).find('img').height(); //Get Title
ImageHeight = ImageHeight+4; /* sum border height */
$(this).find('span').attr({ style: 'margin-top: -'+ImageHeight+'px'}); 
});

$("#header .searchForm .schBtn input.searchSbmFl").attr({value: ''});
$(".queryField").attr({ size: '', maxlength: ''});

});
</script>
