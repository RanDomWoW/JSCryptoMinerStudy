			<script type="text/javascript">
				//Controls logic for cookie message display
				if (Cookies.get('newsflash--1843') != undefined) {
					//Cookie exists, previously accepted, no action required.
				} else {
					//Show cookie message
					var attr = $('.newsflash--1843').hasClass('newsflash--NoJSOrHidden');

					// If News Flash has modifier class - remove the aria tag and the mod class
					if (attr) {
						$('.newsflash--1843').removeAttr("aria-hidden");
						$('.newsflash--1843').removeClass("newsflash--NoJSOrHidden");
					}

					// When you click close
					// Create the Cookie using the newsflash ID to store it.
					$('.newsflash--1843 > .newsflash__btn--close').on("click",
						function(event) {
							event.preventDefault();
							//Set cookie and hide message
							Cookies.set('newsflash--1843', 'true');
							$('.newsflash--1843').slideUp("slow",
								function() {
									$('.newsflash--1843').attr("aria-hidden", "true");
								});
						});
				}
			</script>
