        <script type="text/twig" id="HomeRankingsTemplate">

    

        {% set someRankings = false %}

        {% for ranking in rankings if ranking != null %}
            {% set someRankings = true %}
        {% endfor %}

        {% if someRankings == true %}
        {% for key, gameRanking in rankings if gameRanking.names | length != 0 %}

        <div class="RankingGroupContainer">
            <div class="TitleRankings">
                <p>{{ (("app.ranking.title" | trans) ~ " " ~ (("app.games.name." ~ key) | trans )) | upper }}</p>
            </div>

            <div class="RankingContainerRow">

                {% set names = gameRanking.names %}
                {% set scores = gameRanking.scores %}

                {% for keyRow, name in names %}

                {% if loop.index == 1 or loop.index == 6 or loop.index == 11 %}

                {% if loop.index == 1 %}
                <div class="ColRanking3 Col1">
                    {% elseif loop.index == 6 %}
                    <div class="ColRanking3 Col2">
                        {% elseif loop.index == 11 %}
                        <div class="ColRanking3 Col3">
                            {% endif %}

                            <table>
                            <thead>
                                <tr>
                                    <th>{{ "app.ranking.header.position" | trans }}</th>
                                    <th>{{ "app.ranking.header.user" | trans }}</th>
                                    <th class="Centered">{{ "app.ranking.header.level" | trans }}</th>
                                    <th class="Centered">{{ "app.ranking.header.score" | trans }}</th>
                                </tr>
                                </thead>
                                <tbody>
                                {% endif %}

                                <tr>
                                    <td class="RankPosition">
                                        {% if loop.index == 1 %}
                                            <span class="First">{{ loop.index }}</span>
                                        {% else %}
                                            <span>{{ loop.index }}</span>
                                        {% endif %}
                                    </td>
                                    <td>{{ name }}</td>
                                    <td class="Centered">{{ getUserLevel(scores[keyRow]) }}</td>
                                    <td class="Centered">{{ scores[keyRow] | number_format(0, ".", ".") }}</td>
                                </tr>

                                {% if loop.index == 5 or loop.index == 10 or loop.index == 15 or loop.last %}
                            </table>
                        </div>
                        {% endif %}

                        {% endfor %}

                    </div>
                </div>
                {% endfor %}
                {% else %}
                    <p style="text-align:center;">{{ "app.ranking.problem" | trans }}</p>
                {% endif %}

                













        </script>
    
</body>
