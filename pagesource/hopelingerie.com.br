<script type="text/javascript">
function getCookie(key) { 

	var value = undefined;
	var cookies = document.cookie.split(';');
	
	for (i = 0; i < cookies.length; i++) {
		if (cookies[i].trim().toLowerCase().indexOf(key.toLowerCase() + '=') > -1) {
			value = cookies[i].trim().replace(key + '=', '');
			break;
		}
	}
	
	return value;
	
}

