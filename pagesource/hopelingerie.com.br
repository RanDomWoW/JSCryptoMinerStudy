<script>
  //Verifica se está no dia e horario permitido
  var isAtendimentoAllowed = function(dateObject){
      //Verifica se está dentro do horario disponivel
      var isHourAllowed = function(dateObject){
          //Verifica se não é sexta
          if (dateObject.getDay() != 5){
              //Verifica o horario
              if ((dateObject.getHours() >= 8) && (dateObject.getHours() <= 18)){
                  //Retorna OK
                  return true;
              }
          }
          else {
              //Verifica o horario
              if ((dateObject.getHours() >= 8) && (dateObject.getHours() <= 16)){
                  //Retorna OK
                  return true;
              }
          }
  
          //Retorna falso
          return false;
      };
  
      //Verifica se está nos dias com atendimento
      if ((dateObject.getDay() > 0) && (dateObject.getDay() < 6)){
          //Verifica se está no horario permitido
          if (isHourAllowed(dateObject)){
              //Retorna OK
              return true;
          }
      }
  
      //Retorna falso
      return false;
  };
  
