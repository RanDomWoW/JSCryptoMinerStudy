<h1><a href="http://sendpic.org">SendPic.org</a> - надежный и быстрый хостинг изображений и фотографий</h1>	<script type="text/javascript">
		var progress_id = 'fec063e0264f9c887556aa3a1c141b03';
		var server_id = 4;
		var input_file_counter = 1;
		var update_interval = 5000;
		
		function add_file()
		{
			if (input_file_counter <= 10)
			{
				$('#upload_table').html($('#upload_table').html()+'<tr><td><input type="file" name="f'+ input_file_counter +'" /></td><td><span onclick="$(this).parent().parent().remove(); input_file_counter--;" style="cursor: pointer;"><img src="http://s.sendfile.su/images/cancel.png"></span></td></tr>');
				input_file_counter++;
			}
		}
		
		function get_size(input_bytes) 
		{
			if (input_bytes > 1024*1024)
			{
				return Math.round(input_bytes / (1024*1024), 2) + 'MB';
			}
			else
			{
				return Math.round((input_bytes / 1024), 2) + 'KB';
			}
		}

		function get_speed(transferred_bytes) 
		{
			return Math.round(transferred_bytes / (1024*(update_interval/1000))) + 'KB/s';
		}

		function time_left(left, speed)
		{
			var seconds_left = Math.round(left / speed);
			var sec = seconds_left % 60;
			var min = Math.round(seconds_left / 60);
			if (min < 10)
			{
				min = '0' + min;
			}
			if (sec < 10)
			{
				sec = '0' + sec;
			}
	
			return min + ':' + sec;
		}

		var prev_received = 0;

		function update_progress()
		{
			$.ajax({
			method: 'get',
			url : 'progress.php?server_id='+server_id+'&progress_id='+progress_id,
			dataType : 'text',
			success: function (res) { update_html(res); }
			});
		}

		function update_html(res)
		{
			var ev = eval(res);
			
			$('#time_left_id').html(time_left(ev.size - ev.received, (ev.received - prev_received)/(update_interval/1000)));
            $('#already_uploaded_id').html(get_size(ev.received));
            $('#total_size_id').html(get_size(ev.size)); 
            $('#update_speed_id').html(get_speed(ev.received - prev_received));
            $('#progress2').css({"width" : Math.round(ev.received/ev.size*100) + '%'}); 
            $('#progress2').html(Math.round(ev.received/ev.size*100) + '%');
            prev_received = ev.received;
		}

		function upload_init()
		{
			$('#upload_button').hide(1000);
			$('#progress_p').show(1000);
			var progress_updater = setInterval(update_progress, 5000); 
		}
	</script>
