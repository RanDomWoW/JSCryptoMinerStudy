    <script type="text/javascript">
        //< ![CDATA[
        var newsletterSubscriberFormDetail = new VarienForm('newsletter-validate-detail', false);


        $j('#newsletter-validate-detail .subscribe-btn').on('click', function(event){
            $j(this).attr('disabled', true).addClass('disabled');
            event.preventDefault();
            event.stopPropagation();
            subscribeNewsletter();
            enableNewsletterJoinButton();
        });

        function subscribeNewsletter() {
              if (newsletterSubscriberFormDetail.validator.validate()) {
                    $j.ajax({
                        type: 'post',
                        url: '/anewsletter/subscriber/newAjax',
                        data: $j('#newsletter-validate-detail').serialize(),
                        dataType: 'json',
                        success: function (data) {
                            if (!data.success && data.message) {
                                alert(data.message);
                            } else {
                                if (ara_pixel_enabled) _ap.aEmail($j('#newsletter').val());
                                $j('#newsletter-validate-detail').html('Thank you for subscribing');
                                if (ara_pixel_enabled) _ap.e('newsletter_success', {'email': $j('#newsletter').val()});

                                dataLayer.push({
                                   'event': 'endaiGAevent',
                                   'eventCategory': 'Successful Goal Submissions',
                                   'eventAction': 'Email Subscriber'
                                });
                            }
                        },
                        complete: function (jqXHR, textStatus) {
                            enableNewsletterJoinButton();
                        }
                    });
                } else {
                    $j('#newsletter-validate-detail .subscribe-btn').removeClass('disabled').removeAttr('disabled');
              }

        }

        function enableNewsletterJoinButton() {
            var btn = $j('#newsletter-validate-detail .subscribe-btn');
            if (btn.is(':disabled')) {
                btn.attr('disabled', false).removeClass('disabled');
            }
        }
        //]]>
    </script>
