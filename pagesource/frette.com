<script type="text/javascript">

    $j(function () {
        // check cookie
        var numVisited = Mage.Cookies.get('num_visited');
        if (numVisited == null) {
            numVisited = 1;
        }

        var visited = Mage.Cookies.get("visited");
        var isSubscribed = Mage.Cookies.get("isSubscribed");
        var gated = Mage.Cookies.get("gated_verified");
        var gated_visible = ($j('.gated-popup-holder.visible').length) ? true : false;

        var site_selected = true;

        
        site_selected = (Cookies.get('preferred_store') && Cookies.get('preferred_country'));
        

        if (numVisited == 1 && !isSubscribed) { //num = 1. popup on first page load
            var today = new Date();
            today.setTime(today.getTime());
            var expiress = 365 * 1000 * 60 * 60 * 24;
            var expires_date = new Date(today.getTime() + (expiress));
            Mage.Cookies.set('visited', 'yes', expires_date);

            // set current url
            var current_url = window.location.href;
            $j('.currentUrl').attr('value', current_url);

            $j(window).on('load', function() {
                $j('.subscribe__slide-down').removeClass('hidden');
                setTimeout(function () { //show after 2 seconds
                        $j('.newsletter_popup').addClass('visible').show();
                        $j('.subscribe__slide-down').addClass('show');
                        $j('.wrapper').addClass('popup-visible');
                }, 2000);

            });

            $j('.subscribe__slide-down .subscribe-btn, .popup-subscribe-btn').on('click', function (e) {
                $j(this).attr("disabled", "disabled").addClass('disabled');
                e.preventDefault();
                var subscribe_form = $j(this).closest('form');


                var btn = $j(this);
                    $j.ajax({
                        type: 'post',
                        url: '/newsletter/subscriber/newAjax',
                        data: $j(subscribe_form).serialize(),
                        dataType: 'json',
                        success: function (data) {
                            if (!data.success && data.message) {
                                alert(data.message);
                                btn.removeAttr("disabled").removeClass('disabled');
                            } else {
                                btn.text('Thank You');

                                dataLayer.push({
                                    'event': 'endaiGAevent',
                                    'eventCategory': 'Successful Goal Submissions',
                                    'eventAction': 'Email Subscriber',
                                    'eventLabel': 'Email Pop Up'
                                });

                                if (ara_pixel_enabled) {
                                    _ap.aEmail($j(subscribe_form).find('#mail:first').val());
                                    _ap.e('newsletter_success', {'email': $j(subscribe_form).find('#mail:first').val()});
                                }

                                Cookies.set('isSubscribed', true, expires_date);

                                setTimeout(function () {
                                    $j('.subscribe__slide-down').addClass('hidden');
                                    $j('.wrapper').removeClass('popup-visible');
                                    $j('.newsletter_popup').hide(250);
                                }, 1500);
                            }
                        }
                    });

            });

            $j('.subscribe__slide-down .btn-hide').on('click', function () {
                $j('.subscribe__slide-down').toggleClass('show');
                $j('.wrapper').removeClass('popup-visible');
            });

        }

        //show cookie popup only once
        numVisited++;
        Mage.Cookies.set('visited_path', location.pathname);
        Mage.Cookies.set('num_visited', numVisited);
    });
</script>
