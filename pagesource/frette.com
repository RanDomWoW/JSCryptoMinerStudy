        function setCookie(){
            var current_language = $j('#field-preferred-language').parent().find('.selecter-item.selected');
            var language_cookie =  current_language.data('lang').toLowerCase();
            var country_cookie =  $j('#field-shipping-destination').parent().find('.selected').data('value').toLowerCase();
            var redirect = false;

            Cookies.set('preferred_store', language_cookie , { expires: 999999,  domain : 'frette.com'});
            Cookies.set('preferred_country',country_cookie , { expires: 999999,  domain : 'frette.com'});

            // redirect as site language is different
            if(Cookies.get('preferred_store') && Cookies.get('preferred_store') != site_lang) {
                redirect = true;
            }

            // update language without page re-load
            $j('.footer_site_country').text(country_options[country_cookie.toUpperCase()]);
            $j('.footer_site_language').text(languages[language_cookie]);

            if(redirect){
                doRedirect(language_cookie);
            }
        }

