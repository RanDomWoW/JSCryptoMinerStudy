                } else if ( !Cookies.get('preferred_store') || prevent_autoredirect ) {

                    revealSiteSelectPopup();

                } else {
                    if (Cookies.get('preferred_store') && Cookies.get('preferred_country')) {
                        var pref_store = Cookies.get('preferred_store');
                        var pref_country = Cookies.get('preferred_country');
                        $j('.footer_site_country').text(country_options[pref_country.toUpperCase()]);
                        $j('.footer_site_language').text(languages[pref_store]);
                    }

                    // cookie is not for this store
                    if (Cookies.get('preferred_store') != site_code.toLowerCase()) {
                                                new_site_code = Cookies.get('preferred_store');
                        doRedirect(new_site_code);
                    }

