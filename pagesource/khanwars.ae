        <KW-LoginForm inline-template >
                <div v-bind:class="{ gateLoginForm: true, logged: authenticated }">
                                                <kw-button caption="أخبار" class-name="newsBtn defaultBtn " @click="goToNews"></kw-button>
                        <div class="separator"></div>
                        <div v-if="!authenticated" class="loginFormInner">
                                                                <kw-facebookbtn @success="authenticateByFacebook" class-name="fbLoginBtn"></kw-facebookbtn>
                                                                <div class="credentials" v-if="credentialsApi">
                                    <kw-button class-name="credentialsLoginBtn" v-on:click="getCredentials"></kw-button>
                                    <div class="credentialsRejected" v-if="!credentialsAvailable">
                                        <p>Could not obtain credentials.</p>
                                    </div>
                                </div>
                                <div class="inputs">
                                    <input name="username" type="text" v-model="username" autofocus  placeholder="اسم الملك" />
                                    <input name="password" type="password" v-model="password" placeholder="كلمه السر" v-on:keyup="keyPressed" />
                                </div>
                                <div v-if="!loading" class="buttonrow">
                                    <kw-button @click="authenticateByCredentials" caption="تسجيل الدخول" class-name="loginBtn"></kw-button>
                                </div>
                                 <div v-else><div class="loadingSpinner" ><div></div></div></div>
                                <kw-button @click="forgottenPasswordDialog" caption="نسيت كلمة السر" class-name="forgotPassBtn defaultBtn"></kw-button>
                        </div>
                        <div v-else class="loggedPlayerInfo">
                                <div class="welcomeMsg">
                                        مرحبا بعودتك, <span v-text="username"></span>
                                </div>
                                <kw-button v-if="credentialsApi" class-name="logoutBtn" v-on:click="logOut"></kw-button>
                                <div class="picker">
									<div class="rememberMe buttonrow">
										<input type="checkbox" v-model="rememberMe"  v-bind:true-value="1"  v-bind:false-value="0" />
										<p>تذكرني</p>
									</div> 
									<select v-model="worldId">
											<option v-for="world in worlds" v-bind:value="world.id" v-text="world.name" ></option>
									</select>
								</div>
                                
                                <kw-button  @click="login" caption="لعب" class-name="greenBtn"></kw-button>
                        </div>
                   
                  
                       
                </div>
        </KW-LoginForm>
        <KW-RegisterForm inline-template recommended="34">
                <div class="gateRegistrationForm">
                        <div id="gameLogo" class="logo logo_ar"></div>
                        <div class="registrationFormInner">
                                <h1>إنشاء حساب جديد</h1>

                                <div class="inputField">
                                        <input type="text" value="" placeholder="اسم الملك" v-model="username" /> 
                                        <span v-if="errorMessages.username" v-text="errorMessages.username" class="errorMsg"></span>
                                </div>

                                <div class="inputField">
                                        <input type="password" value="" placeholder="كلمه السر" v-model="password" />
                                        <span v-if="errorMessages.password" v-text="errorMessages.password" class="errorMsg"></span>
                                </div>

                                <div class="inputField">
                                        <input type="text" value="" placeholder="البريد الالكتروني" v-model="email" />
                                        <span v-if="errorMessages.email" v-text="errorMessages.email" class="errorMsg"></span>
                                </div>

                                <div class="inputField">
                                        <select v-model="worldId">
                                                                                                <option value="34"  >
                                                    World 34 (speed : 2x) 
                                                </option>
                                                                                                <option value="63"  >
                                                    World 63 (speed : 5x) 
                                                </option>
                                                                                        </select>
                                        <span v-if="errorMessages.worldId" v-text="errorMessages.worldId" class="errorMsg"></span>
                                </div>

                                <div class="registrationFormTerms">
                                        <input type="checkbox" checked="checked">
                                        <div class="termsText">أنا أوافق علي 
                                                <a href="https://madmoo.com/terms?l=ear&amp;new_l=ar_SA" target="_blank">الشروط والاحكام</a>
                                        
                                        </div>
                                </div>

                                <div  class="buttonrow">
                                        <div v-if="!loading" class="buttonrow">
                                        <kw-button @click="registerByCredentials" caption="لعب" class-name="greenBtn" ></kw-button>
                                                                                <kw-facebookbtn @success="registerByFacebook" class-name="fbRegBtn"></kw-facebookbtn>
                                                                                </div>
                                        <div v-else><div class="loadingSpinner"><div></div></div></div>
                                </div>

                                <span v-if="errorMessages.unknown" v-text="errorMessages.unknown" class="errorMsg unknown"></span>

                                <kw-borders></kw-borders>
                        </div>
                </div>
