	<script>
		jQuery(document).ready(function($) {
			var $window = $(window);
			var $header = $('#header');

			$('a.animateScrollingEffect').click(function() {
				scrollToElement( $( $(this).attr('href') ) );
			});

			$('.animateScrollingEffect a').click(function() {
				scrollToElement( $( $(this).attr('href') ) );
			});

			function scrollToElement($elm) {
				$offset = 0;
				if ($window.width() >= 1000) { $offset = $header.height(); }
				$('html, body').animate({
					scrollTop: $elm.offset().top - $offset
				}, 1500);
			}
		});
	</script>
