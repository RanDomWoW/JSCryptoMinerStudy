		$('#fecha_portadas_ant').datepicker('setStartDate', new Date(1950, 1 - 1, 02));
	}else if(seccion.seccionString() == "Tercera"){
		$('#fecha_portadas_ant').datepicker('setStartDate', new Date(1967, 1 - 1, 02));
	}else if(seccion.seccionString() == "Cuarta"){
		$('#fecha_portadas_ant').datepicker('setStartDate', new Date(2014, 3 - 1, 10));
	}


});
</script>

