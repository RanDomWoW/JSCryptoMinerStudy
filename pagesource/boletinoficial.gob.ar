<script type="text/javascript">

function aumentarTexto() {
    if(textsize == 1){
        $('.itemcontent .item p').addClass('size-2');
        textsize = textsize+1;
        $('a.sizedown').removeClass('sddisabled');
      } else if (textsize == 2){
        $('.itemcontent .item p').addClass('size-3');
        textsize = textsize+1;
        $(this).addClass('sudisabled');
        $(this).css('cursor', 'default');
      } else {
        return(false);
      }
      return(false);
}


function disminuirTexto() {
	if(textsize == 3){
	      $('.itemcontent .item p').removeClass('size-3');
	      $('.itemcontent .item p').addClass('size-2');
	      $('a.sizeup').removeClass('sudisabled');
	      textsize = textsize-1;
	    } else if (textsize == 2){
	      $('.itemcontent .item p').removeClass('size-2');
	      textsize = textsize-1;
	      $(this).addClass('sddisabled');
	      $(this).css('cursor', 'default');
	    } else {
	      return(false);
	    }
	    return(false);
}
</script>
 
 
 
		
