<script type="text/javascript">
function isValidEmailAddress(emailAddress) {
    var pattern = /^([a-z\d!#$%&'*+\-\/=?^_`{|}~\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]+(\.[a-z\d!#$%&'*+\-\/=?^_`{|}~\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]+)*|"((([ \t]*\r\n)?[ \t]+)?([\x01-\x08\x0b\x0c\x0e-\x1f\x7f\x21\x23-\x5b\x5d-\x7e\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]|\\[\x01-\x09\x0b\x0c\x0d-\x7f\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))*(([ \t]*\r\n)?[ \t]+)?")@(([a-z\d\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]|[a-z\d\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF][a-z\d\-._~\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]*[a-z\d\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])\.)+([a-z\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]|[a-z\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF][a-z\d\-._~\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]*[a-z\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])\.?$/i;
    return pattern.test(emailAddress);
};

function valid_newsletter(){
	var bestof = 0;
	var nouveaute = 0;
	var evenements = 0;
	if(isValidEmailAddress(jQuery("#newsletter_email").val())){
		if(jQuery("#captcha_value").val() != jQuery("#cap_respondent").val()){
			alert("Le code entré est incorrect.");
		}else{
			if(jQuery("#bestof").is(":checked")){
				bestof = 1;
			}
			if(jQuery("#nouveaute").is(":checked")){
				nouveaute = 1;
			}
			if(jQuery("#evenements").is(":checked")){
				evenements = 1;
			}
			if(jQuery("#bestof").is(":checked") || jQuery("#nouveaute").is(":checked") || jQuery("#evenements").is(":checked")){
				jQuery.post(
				    ajaxurl,
				    {
				        'action': 'valid_newsletter_function',
				        'newsletter_email': jQuery("#newsletter_email").val(),
				        'bestof': bestof,
				        'nouveaute': nouveaute,
				        'evenements': evenements
				    },
				    function(response){
				    	alert(response);
				    }
				);
			}else{
				alert("Vous devez avoir sélectionné au minimum une newsletter.");
			}
		}
	}else{
		alert("Email address est incorrect.");
	}
}
</script>
