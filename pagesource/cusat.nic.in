    <script language="javascript" type="text/javascript">

        function encryptPass() {
            debugger;

            if (document.getElementById("_ctl0_cpLogin_txt_username").value == "") {
                alert("Username required");
                return false;
            }
            if (checkPass()) {
                if (document.getElementById("_ctl0_cpLogin_txt_pass").value != "") {               
                  var passsha256 = SHA256(document.getElementById("_ctl0_cpLogin_txt_pass").value);
                    var passshasalt = (passsha256 + "464d2035-b73c-4ba7-bf57-b989f65108ef"); 
                    var passnsalt = SHA256(passshasalt);
                    document.getElementById("_ctl0_cpLogin_txt_passHidden").value = passnsalt;           
                    document.getElementById("_ctl0_cpLogin_txt_pass").value = "*********";

                }
                return true;
            }
            else {
                return false;
            }


        }

        function checkPass() {//source, args) {
            debugger;

            if (document.getElementById('_ctl0_cpLogin_txt_pass').value != "") {
                temp_value = document.getElementById('_ctl0_cpLogin_txt_pass').value;
                var h = (temp_value.length) - 1;
                var i = 0;
                var m = 0, n = 0, p = 0;
                for (i = 0; i <= h; i++) {
                    if (temp_value.charCodeAt(i) >= 48 && temp_value.charCodeAt(i) <= 57) {
                        n = n + 1;
                    }
                    if ((temp_value.charCodeAt(i) >= 33 && temp_value.charCodeAt(i) <= 47) || (temp_value.charCodeAt(i) >= 58 && temp_value.charCodeAt(i) <= 64) || (temp_value.charCodeAt(i) >= 91 && temp_value.charCodeAt(i) <= 96) || (temp_value.charCodeAt(i) >= 123 && temp_value.charCodeAt(i) <= 125)) {
                        p = p + 1;
                    }
                    if ((temp_value.charCodeAt(i) >= 65 && temp_value.charCodeAt(i) <= 90) || (temp_value.charCodeAt(i) >= 97 && temp_value.charCodeAt(i) <= 122)) {
                        m = m + 1;
                    }
                    else if (temp_value.charCodeAt(i) == 39 || temp_value.charCodeAt(i) == 60 || temp_value.charCodeAt(i) == 62) {
                        m = 0; n = 0; p = 0;
                        temp_value = "";
                    }
                }
                if (m == 0 || n == 0 || p == 0) {
                    document.getElementById('_ctl0_cpLogin_txt_pass').value = "";
                    alert('Incorrect username or password, try again.');
                    //document.getElementById("txt_Flag").value="12";
                    document.getElementById('_ctl0_cpLogin_txt_pass').value = "";

                    return false;
                }

                if (temp_value.length > 0 && temp_value.length < 8) {
                    alert('Incorrect username or password, try again.');
                    //document.getElementById("txt_Flag").value="12";
                    document.getElementById('_ctl0_cpLogin_txt_pass').value = "";

                    return false;
                }
                // args.IsValid = true;
                return true;

            }
            else {
                alert("Password required");
                return false;
            }
        }

        function avoidEnter() {
            if (window.event.keyCode == 13) {
                event.returnValue = false;
                event.cancel = true;
                alert('Please Click on the Button to Submit Your Data')
            }
        }
        function redirectPage(url) {
            ////           alert("You are being redirected to a secure area, you may continue with the site by adding exceptions as prompted");
            location.href = url;
        }
    </script>

