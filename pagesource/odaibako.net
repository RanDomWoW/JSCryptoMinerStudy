<script>
"use strict";

$(function(){
  var menu_list = new Vue({
    el: '#menu_list',
    data: {
      active_id: null,
      clicked_id: null,
    },
    methods: {
      menter: function(id, event){
        event.target.classList.add('hover');
        this.active_id = id;
        // iPhoneにはばぐがある
        if(navigator.userAgent.indexOf('iPhone') > 0){
          event.target.click();
        }
      },
      mleave: function(id, event){
        event.target.classList.remove('hover');
        this.active_id = null;
      },
      mclick: function(id, url, event){
        this.clicked_id = id;
        location.href = url;
      }
    }
  });
});
</script>

    
