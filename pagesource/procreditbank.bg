	var items = $('.orange .placeholder:not(.carousel-wrapper) .WidgetImageText, .blue .placeholder:not(.carousel-wrapper) .WidgetImageText');
	var total = items.length;

	for(i = 0; i < total; i+=3){
		var row = items.slice(i, Math.min(i + 3, total - 1));
		var maxHeight = 0;
		row.each(function() {
			maxHeight = $(this).height() > maxHeight ? $(this).height() : maxHeight;
		}).height(maxHeight);
	}

	var items_cats = $('.orange .subCategories li, .blue .subCategories li');
	var total_cats = items_cats.length;
	
	for(i = 0; i < total_cats; i+=3){
		var row = items_cats.slice(i, Math.min(i + 3, total_cats - 1));
		var maxHeight = 0;
		row.each(function() {
			maxHeight = $(this).height() > maxHeight ? $(this).height() : maxHeight;
		}).height(maxHeight);
	}



	$('#calendar-block').detach().appendTo('#treeMenu');
