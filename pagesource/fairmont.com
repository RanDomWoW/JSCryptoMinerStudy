    <script language="javascript" type="text/javascript">
        function setCookieWidget() {
            try {
                var ddlDestinationClientId = 'ctl00_Banner_bookingWidget_ddlDestination';
                var ddlPropertyClientId = 'ctl00_Banner_bookingWidget_ddlProperty';
                var ddlRateTypeClientId = 'ctl00_Banner_bookingWidget_ddlRateType';
                //                var txtPromotionCodeClientId = 'ctl00_Banner_bookingWidget_txtPromotionCode';
                //                var txtIATACodeClientId = 'ctl00_Banner_bookingWidget_txtIATACode';
                var txtArrivalDateClientId = 'ctl00_Banner_bookingWidget_txtArrivalDate';
                var txtDepartureDateClientId = 'ctl00_Banner_bookingWidget_txtDepartureDate';
                var ddlRoomsClientId = 'ctl00_Banner_bookingWidget_ddlRooms';
                var destinationIndexValue = document.getElementById(ddlDestinationClientId).selectedIndex;
                var propertyIndexValue = document.getElementById(ddlPropertyClientId).selectedIndex;
                var rateTypeIndexValue = document.getElementById(ddlRateTypeClientId).selectedIndex;
                //                var promoCodeValue = document.getElementById(txtPromotionCodeClientId).value;
                //                var iataCodeValue = document.getElementById(txtIATACodeClientId).value;
                var promoCodeValue = "";
                var iataCodeValue = "";
                var arrivalValue = document.getElementById(txtArrivalDateClientId).value;
                var departureValue = document.getElementById(txtDepartureDateClientId).value;
                var roomsValue = document.getElementById(ddlRoomsClientId).selectedIndex;

                var value = destinationIndexValue + '|' + propertyIndexValue + '|' + rateTypeIndexValue + '|' + promoCodeValue + '|' + iataCodeValue + '|' + arrivalValue
                    + '|' + departureValue + '|' + roomsValue;
                setCookie("FairmontWidget", value, 1);
            }
            catch (ex) {
            }
        }

        function setCookie(c_name, value, exdays) {
            var exdate = new Date();
            exdate.setDate(exdate.getDate() + exdays);
            var c_value = escape(value) + ((exdays == null) ? "" : "; expires=" + exdate.toUTCString());
            document.cookie = c_name + "=" + c_value;
        }

        function getCookie(c_name) {
            var i, x, y, ARRcookies = document.cookie.split(";");
            for (i = 0; i < ARRcookies.length; i++) {
                x = ARRcookies[i].substr(0, ARRcookies[i].indexOf("="));
                y = ARRcookies[i].substr(ARRcookies[i].indexOf("=") + 1);
                x = x.replace(/^\s+|\s+$/g, "");
                if (x == c_name) {
                    return unescape(y);
                }
            }
        }

        try {
            var currentUrl = window.location.pathname;
            if (currentUrl == "/" || currentUrl == "/home/") {
                var widgetCookie = getCookie("FairmontWidget");
                if (widgetCookie != null && widgetCookie != '') {
                    var values = widgetCookie.split("|");
                    var destinationIndexValue = values[0];
                    var propertyIndexValue = values[1];
                    var rateTypeIndexValue = values[2];
                    var promoCodeValue = values[3];
                    var iataCodeValue = values[4];
                    var arrivalValue = values[5];
                    var departureValue = values[6];
                    var roomsValue = values[7];

                    var ddlDestinationClientId = 'ctl00_Banner_bookingWidget_ddlDestination';
                    var ddlPropertyClientId = 'ctl00_Banner_bookingWidget_ddlProperty';
                    //var ddlRateTypeClientId = 'ctl00_Banner_bookingWidget_ddlRateType';
                    //                    var txtPromotionCodeClientId = 'ctl00_Banner_bookingWidget_txtPromotionCode';
                    //                    var txtIATACodeClientId = 'ctl00_Banner_bookingWidget_txtIATACode';
                    var txtArrivalDateClientId = 'ctl00_Banner_bookingWidget_txtArrivalDate';
                    var txtDepartureDateClientId = 'ctl00_Banner_bookingWidget_txtDepartureDate';
                    // var ddlRoomsClientId = 'ctl00_Banner_bookingWidget_ddlRooms';

                    if (destinationIndexValue != "" && destinationIndexValue != undefined)
                        document.getElementById(ddlDestinationClientId).selectedIndex = destinationIndexValue;
                    if (propertyIndexValue != "" && propertyIndexValue != undefined)
                        document.getElementById(ddlPropertyClientId).selectedIndex = propertyIndexValue;
                    //                    if (rateTypeIndexValue != "" && rateTypeIndexValue != undefined)
                    //                        document.getElementById(ddlRateTypeClientId).selectedIndex = rateTypeIndexValue;
                    //                    if (promoCodeValue != "" && promoCodeValue != undefined)
                    //                        document.getElementById(txtPromotionCodeClientId).value = promoCodeValue;
                    //                    if (iataCodeValue != "" && iataCodeValue != undefined)
                    //                        document.getElementById(txtIATACodeClientId).value = iataCodeValue;
                    if (arrivalValue != "" && arrivalValue != undefined)
                        document.getElementById(txtArrivalDateClientId).value = arrivalValue;
                    if (departureValue != "" && departureValue != undefined)
                        document.getElementById(txtDepartureDateClientId).value = departureValue;
                    //                    if (roomsValue != "" && roomsValue != undefined)
                    //                        document.getElementById(ddlRoomsClientId).selectedIndex = roomsValue;
                }
            }
        }
        catch (ex) {
        }

    </script>

