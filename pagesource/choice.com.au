                            "promoClick": {
                                "actionField": {
                                },
                                "promotions": [{
                                    "name": "Get basic access",
                                    "creative": "text-button-no-image",
                                    "position": "Article page - Member comments CTA"
                                }]
                            }
                        },
                        "eventCallback": function () {
                            document.location = "/member/get-basic-access"
                            //go to buy page
                        }
                    });
                    return false
                });
            });
        })(jQuery);
    </script>


