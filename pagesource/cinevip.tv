      <script>


        $(document).ready(function(){ 
            $('#search-q').autocomplete({
                source: "/home/buscador",
                minLength: 1,
                focus: function( event, ui ) {event.preventDefault();$('#search-q').val(ui.item.value.split(';')[1]);},
                select: function( event, ui ) {event.preventDefault();$('#search-q').val(ui.item.value.split(';')[1]);},
                change: function( event, ui ) {event.preventDefault();$('#search-q').val(ui.item.value.split(';')[1]);},
                context: this
            }).keydown(function(e){
            if (e.keyCode === 13){
                        e.preventDefault();
                            if($('#search-q').val().split(';')[1] != undefined)
                                $('#search-q').val($('#search-q').val().split(';')[1]);
            }
            });
               
            var newRender = function(ul, item) {
                        var i = item.value.split(';');
                        
                        return $("<li>")
                                .data("ui-autocomplete-item", i[1])
                                .append("<a class='elclick' style='clear:both;' href='"+i[3]+"'><img style='float:left; margin-right: 8px;' height='55' src='"+i[0]+"' /><div><strong>"+i[1]+"</strong><br>"+i[2]+"</div></a>")
                                .appendTo(ul);       
            }

            $('#search-q').data("ui-autocomplete")._renderItem = newRender;
            $(document).on('click','.elclick',function(e){
                e.preventDefault();
                $('#search-q').val($('#search-q').val());
                window.location.href = $(this).attr('href');
            });
            
            $("#search-q").keyup(function (e) {
                if (e.keyCode == 13) {
                    $('#search').submit();
                }
            }); 
});
