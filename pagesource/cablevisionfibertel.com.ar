<script>
	var $modalType;
	var $modalText;
	var $damageFound;
	var $buttonsOption = {};
	function showOutagePopup() {
		$('#menuFamilyStatus').val("");
		var $form = $('#statusForm');
		_xopa.sendRequest({
			url : $form.attr('action'),
			loader: true,
			data : _xopa.serialize($form),
			done : function(data) {
				if(data.damageFound){
					$damageFound = true;
					$modalType = 'success';
					$modalText = '<p>' + data.statusMessageError1 + '</p>';
					if(data.damageResolutionTime != null){
						var timeSplitted = data.damageResolutionTime.split(':');
						var horas = timeSplitted.length === 2 ? timeSplitted[0] : timeSplitted;
						var minutos = timeSplitted.length === 2 ? timeSplitted[1] : '';
						$modalText = $modalText.concat('<p><strong>Plazo estimado de soluci&oacute;n</strong></p>');
						$modalText = $modalText.concat('<div class="horest">');
						$modalText = $modalText.concat('<span>Horas: '
								+ horas
								+ ' - Minutos: '
								+ minutos
								+ '</span></div>'); 
					}
					$modalText = $modalText.concat('<p>' + data.statusMessageError2 + '</p>');
					$buttonsOption = {ok: {text: "cerrar"}};
				} else {
					$damageFound = false;
					$modalType = 'confirm';
					$modalText = '<p>' + data.statusMessageOk + '<p>';
					$buttonsOption = {ok: {text: "centro de ayuda"}, cancel: {text: "cancelar"}};
				}
				_xopa.swal({
					title: 'ESTADO DE SERVICIO',
					titleClass: 'lg cen',
					text: $modalText,
					type: $modalType,
					buttons: $buttonsOption
				}, function () {if(!$damageFound){onAsistenciaTecnicaClick('https://cablevisionfibertel.custhelp.com/app/answers/list/p/1','https://cablevisionfibertel.custhelp.com/')}}, undefined);
			}
		});
	}
</script>

