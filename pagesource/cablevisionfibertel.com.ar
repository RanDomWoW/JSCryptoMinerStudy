	phoneLoader.addListener(function(phone) {
		$("#telVentas a").text(phone).attr('href', 'tel:' + phone);;
		$("#telVentasMobile").html('<a href="tel:' + phone + '">Ventas</a>');
	});
	phoneLoader.load();
	
	phoneLoader.load({
		key: 'ATENCION_CLIENTE'
	}, function(phone) {
		$("#tel1").html('<a href="tel:' + phone + '">' + phone + '</a>');
		$("#telAtencionAlCliente a").text(phone).attr('href', 'tel:' + phone);
		$("#telAtencionAlClienteMobile").html('<a href="tel:' + phone + '">Atención al cliente</a>');
	});
	
	$('.footer_contacto #telAtencionAlCliente').on('click', 'a', function() {
		dataLayer.push({'event': 'trackEvent', 'eventCategory': 'Footer', 'eventAction': 'Contacto', 'eventLabel': 'Atencion al cliente'});
	});
	$('.footer_contacto #telVentas').on('click', 'a', function() {
		dataLayer.push({'event': 'trackEvent', 'eventCategory': 'Footer', 'eventAction': 'Contacto', 'eventLabel': 'Ventas'});
	});
</script>


		
			
			
