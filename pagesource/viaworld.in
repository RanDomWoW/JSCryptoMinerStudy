function initMainMenuNav() {
     
    
	
	$jQ(".primaryNavigation li").mouseover(function(){
		$jQ(this).addClass("selectedElement");
	});
	$jQ(".primaryNavigation li").mouseout(function(){
		$jQ(this).removeClass("selectedElement");
	});
	$jQ(".productNavigation li").mouseover(function(){
		$jQ(this).addClass("selectedElement");
	});
	$jQ(".productNavigation li").mouseout(function(){
		$jQ(this).removeClass("selectedElement");
	});
	//setInterval('blinkText()',500);
}
function getCancelTicket() {
	var refrenceNo = null;
	  if($jQ('.refrenceForm').val() == "") {
		  alert("Kindly enter your Via Booking Reference Number. Example: FMN123ABCD");		 
	  } else {
		  refrenceNo = $jQ('.refrenceForm').val().replace(/^\s+/, '').replace(/\s+$/, '');
		  if(refrenceNo.trim().substr(0,3).toUpperCase() != 'FMN') {
			  alert("Please enter a valid order reference id");				
		  }
		  
		  window.location.search ='?action1=ORDEKLNK239487&orderReference='+$jQ('.refrenceForm').val(); 		 
	}
	 
	/* return true; */
}
$jQ('.cancelReschedule').click(function(event){
	event.stopPropagation();
	$jQ('.cancelSchedule').show();
});
function cancelReschedule(event){
	event.stopPropagation();
	var $target = $jQ(event.currentTarget);
	$jQ('.cancelScheduleTab').children().removeClass('active');
	 $target.addClass('active');
	 $jQ('.cancelScheduleSteps').children().addClass('u_invisible')
	 $jQ('.'+$target.attr('id')+'Steps').removeClass('u_invisible')
}
$jQ('#cancelBooking').click(cancelReschedule);
$jQ('#rescheduleBooking').click(cancelReschedule);

$jQ('body').click(function(event) {
    var $target = $jQ(event.target);
    var $filter = $jQ('.allcancelList').is(':visible');
    if($filter && !($target.hasClass('allcancelList') || $target.parents().hasClass('allcancelList'))) {
    	$jQ('.cancelSchedule').hide();    
    }
  });
	
