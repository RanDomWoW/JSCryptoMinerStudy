  };
  var sessionPopulate = function () {
    jQuery('#session-signup').addClass ('session-signup-' + sessionStateBanner.state);
    jQuery('#session-login').addClass ('session-login-' + sessionStateBanner.state);
    jQuery('#session-subscribe').addClass ('session-subscribe-' + sessionStateBanner.state);
    jQuery('#session-upgrade').addClass ('session-upgrade-' + sessionStateBanner.state);
    jQuery('#session-holder').addClass ('session-holder-' + sessionStateBanner.state);
    jQuery('#session-usertypefont-one').addClass ('session-usertypefont-one-' + sessionStateBanner.state);
    jQuery('#session-username').addClass ('session-username-' + sessionStateBanner.state);
    jQuery('#session-username').text (sessionStateBanner.username);
    jQuery('#session-usertype').addClass ('session-usertype-' + sessionStateBanner.state);
    jQuery('#session-usertype').text (sessionStateBanner.usertype);
    jQuery('#session-imagepath').addClass ('session-imagepath-' + sessionStateBanner.state);
    jQuery('#session-launchmap').addClass ('session-launchmap-' + sessionStateBanner.state);
    jQuery('#session-account').addClass ('session-account-' + sessionStateBanner.state);
    jQuery('#session-savedwork').addClass ('session-savedwork-' + sessionStateBanner.state);
    jQuery('#session-savedwork-a').text (sessionStateBanner.savedWorkTerm);
    jQuery('#session-logout').addClass ('session-logout-' + sessionStateBanner.state);
    jQuery('#session-login-holder').removeClass ('hidden');
    // Cheating here. Only showing the sign up form for public users.
    if (sessionStateBanner.state == 'public') {
      jQuery("#session-form").removeClass ('hidden');
      jQuery('#open-map-cta').addClass ('open-map-cta-public');
      jQuery('#open-map-cta').removeClass ('btn-warning');
      jQuery('#open-map-cta').addClass ('btn-primary');
      jQuery('#left-text-subscription').addClass("hidden");
      jQuery('#left-text').removeClass("hidden");
    } else {
      jQuery('#open-map-cta').addClass ('open-map-cta-notpublic');
      jQuery('#open-map-cta').removeAttr ('data-original-title');
      jQuery('#open-map-cta').removeClass ('btn-primary');
      jQuery('#open-map-cta').addClass ('btn-warning');
      jQuery('#left-text').addClass("hidden");
      jQuery('#left-text-subscription').removeClass("hidden");
    }
    if (sessionStateBanner.firstRun === false) {
      return;
    }
    sessionStateBanner.firstRun = true;
    // Make "links" that show modals work. This is my account.
    jQuery("#session-myaccount-a").on ('click', function () {
      sessionStateBanner.modal.popup({
        "body": "<iframe src='/modal-myaccount' width='860' height='610' border='0'></iframe>",
        "header": "",
        "footer": false,
        "shadeBody": true,
        "shadeHeader": true,
        "shadeFooter": true,
        "size": "modal-lg"
      });
    });
    jQuery("#session-login").on ('click', function () {
      sessionStateBanner.modal.popup({
"body": "<iframe src='/modal-login?r=%2Fmaps' width='270' height='340' border='0'></iframe>",
"header": "Log in",
"footer": sessionStateBanner.footer,
"shadeFooter": true,
"size": "modal-sm"
      });
    });
    // Logout
    jQuery("#session-logout-a").on ('click', function () {
      jQuery.ajax ({
        url: '/d/?id=1&type=data&t=auth&logout=1',
        success: function (data) {
          sessionStateBanner.logout ();
        }
      });
    });
  };
  jQuery.ajax ({
    url: '/d/?id=1&type=data&t=session&act=g',
    success: function (data) {
      if (data.session._trial === true) {
        sessionStateBanner.state = 'trial';
        sessionStateBanner.usertype = 'Trial Edition';
      } else if (data.session._image_path != null) {
        sessionStateBanner.state = 'enterprise';
        sessionStateBanner.savedWorkTerm = 'Saved work';
        if (data.session._allow_login != null) {
          sessionStateBanner.state = 'enterpriselogin';
        }
        if (data.session._alu == false) {
          sessionStateBanner.state = 'enterpriseloggedin';
          sessionStateBanner.savedWorkTerm = 'My saved work';
        }
        if (data.session.__product_properties.is_clone != null && data.session.__product_properties.is_clone.value === true) {

          sessionStateBanner.state = 'clone';
        }
        sessionStateBanner.usertype = 'Site License';
        jQuery('#session-imagepath-a').attr ('href', data.session._site_license_url);
        jQuery('#session-imagepath-img').attr ('src', '/images/' + data.session._image_path);
      } else if (data.session._userType === 'Registered User') {
        sessionStateBanner.state = 'basic';
        sessionStateBanner.usertype = 'Basic Edition';
      } else if (data.session._userType === 'Standard Subscriber') {
        sessionStateBanner.state = 'standard';
        sessionStateBanner.usertype = 'Standard Edition';
      } else if (data.session._userType === 'Premium Subscriber') {
        sessionStateBanner.state = 'premium';
        sessionStateBanner.usertype = 'Premium Edition';
      }
      if (data.session._firstName != null) {
        sessionStateBanner.username = data.session._firstName;
      }
      sessionPopulate ();
    },
    error: function () {
      sessionStateBanner.logout ();
    }
  });
};
jQuery(document).ready (sessionUpdate);
</script>
