				if(city=='kolkata' ||  city==''){
					jQuery("#ncrtxt").hide();
					jQuery("#lucknowtxt").hide();
					jQuery("#chennaitxt").hide();
					jQuery("#hyderabadtxt").hide();
					jQuery("#vizagtxt").hide();
