				else if(city=='ncr'){
					jQuery("#kolkatatxt").prop('disabled',true).hide();
					jQuery("#lucknowtxt").prop('disabled',true).hide();
					jQuery("#chennaitxt").prop('disabled',true).hide();
					jQuery("#hyderabadtxt").prop('disabled',true).hide();
					jQuery("#vizagtxt").prop('disabled',true).hide();
					jQuery("#ncrtxt").removeAttr("disabled");
					jQuery("#ncrtxt").show();
				} 
				else if(city == 'lucknow') {
					jQuery("#kolkatatxt").prop('disabled',true).hide();
					jQuery("#ncrtxt").prop('disabled',true).hide();
					jQuery("#chennaitxt").prop('disabled',true).hide();
					jQuery("#hyderabadtxt").prop('disabled',true).hide();
					jQuery("#vizagtxt").prop('disabled',true).hide();
					jQuery("#lucknowtxt").removeAttr("disabled");
					jQuery("#lucknowtxt").show();
				}
				else if(city == 'hyderabad') {
					jQuery("#kolkatatxt").prop('disabled',true).hide();
					jQuery("#ncrtxt").prop('disabled',true).hide();
					jQuery("#lucknowtxt").prop('disabled',true).hide();
					jQuery("#chennaitxt").prop('disabled',true).hide();
					jQuery("#vizagtxt").prop('disabled',true).hide();
					jQuery("#hyderabadtxt").removeAttr("disabled");
					jQuery("#hyderabadtxt").show();
				}
				else if(city == 'chennai') {
					jQuery("#kolkatatxt").prop('disabled',true).hide();
					jQuery("#ncrtxt").prop('disabled',true).hide();
					jQuery("#lucknowtxt").prop('disabled',true).hide();
					jQuery("#hyderabadtxt").prop('disabled',true).hide();
					jQuery("#vizagtxt").prop('disabled',true).hide();
					jQuery("#chennaitxt").removeAttr("disabled");
					jQuery("#chennaitxt").show();
				}
				else if(city == 'vizag') {
					jQuery("#kolkatatxt").prop('disabled',true).hide();
					jQuery("#ncrtxt").prop('disabled',true).hide();
					jQuery("#lucknowtxt").prop('disabled',true).hide();
					jQuery("#hyderabadtxt").prop('disabled',true).hide();
					jQuery("#chennaitxt").prop('disabled',true).hide();
					jQuery("#vizagtxt").removeAttr("disabled");
					jQuery("#vizagtxt").show();
				}
