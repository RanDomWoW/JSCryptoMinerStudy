<div class="overlay-video-2"></div>
<div class="btn-close-departament">X</div>
<script>
$(document).ready(function(){
var overlay = '.overlay-video-2',
	container = '.container-video-banner-departament',
	video = '.video-banner-departament',
	close = '.btn-close-departament';

$('.play').on('click',function(e){
	e.preventDefault();

	var $container = $(this).closest(container);

	$(overlay).fadeIn();
	$(close).fadeIn();
	$container.find(video).clone().insertAfter('.overlay-video-2').fadeIn();
	$('main').addClass('zindex');
});
$('.btn-close-departament, .overlay-video-2').on('click',function(e){
	e.preventDefault();

	var $container = $(this).closest(container);

	$(overlay).fadeOut(function(){
		$(overlay).siblings('.video-banner-departament').remove();
		$(close).fadeOut();
	});
	
	$('main').removeClass('zindex');
});
$(".item-03 .play").on('click', function(){
        $('#video-01').attr('autoplay','')
})
});

</script>





