		return;
	}
	if(etc_address==''){
		alert("나머지 주소를 입력해주세요.");
		return;
	}
	if(update_agree_type=='N'){
		alert("광고성 수신 동의에 모두 체크해주세요.");
		return;
	}
	if(update_agree==undefined || update_agree=="N"){
		alert("회원정보 업데이트를 동의해 주세요.");
		return;
	}
	//조회후 조건에 맞으면 insert
	$.ajax({
		url : "/event/promotion/callender_event/insert",
		async :false,
		data : {
			user_name : user_name,
			user_email : user_email,
			phone : full_phone,
			postcode : postcode,
			select_address : select_address,
			etc_address : etc_address,
			full_address : full_address,
			update_agree : update_agree,
			type : '2018calender',
			tradecode : tradecode
		},
		dataType : "json",
		type : "post",
