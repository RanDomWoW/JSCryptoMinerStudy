<script type="text/javascript">
function requestSubs(subsEndPopup){
	var subscription_type = "";
	$.ajax({
		url: "/subscription/land/request",
		type: "post",
		dataType: "json",
		data: {admin_memo:"연장상담신청",subscription_type:subscription_type},
		success: function(data) {
			hideSubsEndPopup(subsEndPopup, 7);
			$("#subs_endpop3").show();
		}
	});
}
function hideSubsEndPopup(divID, day){
	if(divID == "font_pop"){
		if(day > 0){ $.cookie("c_fontrixpopup_close", "Y" , {expires:day, path : "/" }); }
	}else{
		if(day > 0){ $.cookie("c_subsendpopup_close", "Y" , {expires:day, path : "/" }); }
	}
	$("#" + divID).fadeOut();
}
function showSubsEndPopup(){
	var subsEndPopup = "";
	var fontrixPopup = "";
	if($.cookie('c_subsendpopup_close') != 'Y'){
		if(subsEndPopup != ""){
			$("#" + subsEndPopup).show();
		}
	}
	if($.cookie('c_fontrixpopup_close') != 'Y'){
		if(fontrixPopup == "Y"){
			$("#font_pop").show();
		}
	}
}
function save_memo(){
	var r_name = $('#request_name').val();
	var r_phone = $('#request_phone1').val()+"-"+$('#request_phone2').val()+"-"+$('#request_phone3').val();
	$.ajax({
		url: "/subscription/land/request_memo",
		type: "post",
		dataType: "json",
		data: {r_name:r_name,r_phone:r_phone},
		success: function(data) {
			hideSubsEndPopup('subs_endpop3',0);
		}
	});
	
}
window.onload = showSubsEndPopup;
