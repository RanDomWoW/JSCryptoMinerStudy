function order_callender(){
	//변수선언 및 값 할당
	var user_email = "";
	var user_name = $("#user_name").val();
	var phone1 = $('#phone1').val();
	var phone2 = $('#phone2').val();
	var phone3 = $('#phone3').val();
	var full_phone  = phone1 +"-"+ phone2 +"-"+ phone3;
	var postcode = $('#postcode').val();
	var select_address = $('#selectAddress').val();
	var etc_address = $('#etcAddress').val();
	var full_address = '';
	if($('#roadAddress').val()!='' && $('#etcAddress').val() !=''){
		full_address = $('#roadAddress').val() + " " + $('#etcAddress').val();
	}else{
		full_address = $('#selectAddress').val() + " " + $('#etcAddress').val();
	}
	if($('#update_agree_mail').is(':checked') && $('#update_agree_sms').is(':checked')){
		var update_agree_type = "Y";
	}else{
		var update_agree_type = "N";
	}
	if($('#agree1:checked').val()){
		var update_agree = "Y";
	}else if($('#agree2:checked').val()){
		var update_agree = "N";
	}
	var tradecode = $('#tradecode').val();

	//유효성 체크
	if(user_name==''){
		alert("받는사람을 입력해주세요.");
		return;
	}
	if(phone1=='' || phone2=='' || phone3=='' ){
		alert("핸드폰번호를 입력해주세요.");
