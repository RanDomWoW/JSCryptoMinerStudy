<script type="text/javascript">

	$(document).ready(function ()
	{
		//동영상 구매대행 및 빠른서칭도우미 눌렀을경우 온라인상담 레이어 뜨도록 추가 by 이근우 20180131
		$(".btnShowChat, .ban01, .ban02").click(function (e)
		{
			//$( "#liveContent_1" ).load( "/help/contact_us/popup", function() {
				
			//});
			 ShowDialog(false);
			e.preventDefault();
		});

		$("#btnClose, #btnContactUsClose").click(function (e)
		{
			HideDialog();
			$("#liveContent_1").show();
			$("#liveContent_3").hide();
			$("#liveContent_2").empty();
			$(".chatActive").removeClass("chatActive");
			$($('ul.liveTabs > li')[0].children).addClass('chatActive');
			//placeholder 수정 $("#contact_member_phone, #contact_subject, #contact_message, #contact_webbrowser, #txtFileName, #hidFileID").val('');
			$("#contact_member_phone, #contact_webbrowser, #txtFileName, #hidFileID").val('');
			$("#contact_subject").val(' *');
			$("#contact_message").val('내용을 입력해 주세요 *');
			$("input[name=contact_os]").attr('checked', false);
			$("input[name=contact_type]").attr('checked', false);
			$("#contact_type1").attr('checked', true);
			e.preventDefault();
		});

	});

	function ShowDialog(modal)
	{
		$("#overlay").show();
		$("#chatDialog").fadeIn(300);

		if (modal)
		{
			$("#overlay").unbind("click");
		}
		else
		{
			$("#overlay").click(function (e)
			{
				HideDialog();
			});
		}
	}

	function HideDialog()
	{
		$("#overlay").hide();
		$("#chatDialog").fadeOut(300);
	}         
</script>

