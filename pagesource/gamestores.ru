    <script>
    	(function() {
    		'use strict';

			var handleCommasInStats = function() {
			    $('.stat-tile .stat-num').each(function() {
			        var num = $(this).html();
			        $(this).html(num.replace(/\B(?=(\d{3})+(?!\d))/g, ","))
			    })
			}
			var handleLinksScroll = function() {
			    $('[data-scroll="true"]').on('click', function(evt) {
			        evt.preventDefault();
			        evt.stopPropagation();
			        var target = $(this).attr('href');
			        $('body, html').animate({
			            scrollTop: ($(target).offset().top)
			        }, 300)
			    })
			}
			var handleUserReviews = function() {
				var currentReview = 0;
				var reviews = $('.review-tile');
				var triggers = $('.review-slider-holder span')

				triggers.on('click', function(e) {
					currentReview = triggers.index(this)
					reviews.addClass('hide')
					reviews.eq(currentReview).removeClass('hide')
					triggers.removeClass('active')
					triggers.eq(currentReview).addClass('active')
				})
			}
			var handleFadeIn = function() {
			    setTimeout(function() {
			        $(".page-container").addClass('in')
			    }, 30)
			}
			var handleEmailInjection = function() {
			    var emailHtml = "<a href=\"mailto:help@gamestores.ru\">help@gamestores.ru</a>";
			    setTimeout(function() {
			        $("#email-placeholder").html(emailHtml);
			    }, 200)
			}



			var App = {
			    init: function() {
			        handleCommasInStats();
			        handleLinksScroll();
			        handleUserReviews();
			        handleFadeIn();
			        handleEmailInjection();
			    }
			}

			$(document).ready(function() {
				App.init()
			})
