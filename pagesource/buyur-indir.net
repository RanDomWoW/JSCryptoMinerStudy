																<script language="JavaScript" type="text/javascript">
    function size($byte) {
        $kbyte = 1024;
        $mbyte = 1048576;
        $gbyte = 1073741824;
        
        $size = Math.floor($byte/$gbyte);
        if ($size > 0) {
            $size = Math.floor($byte/$gbyte * 100) / 100;
            return $size + general.size.gbyte;
        }
        
        $size = Math.floor($byte/$mbyte);
        if ($size > 0) {
            $size = Math.floor($byte/$mbyte * 100) / 100;
            return $size + general.size.mbyte;
        }
        
        $size = Math.floor($byte/$kbyte);
        if ($size > 0) {
            $size = Math.floor($byte/$kbyte * 100) / 100;
            return $size + general.size.kbyte;
        }
        
        return $byte + general.size.byte;
    }
    
    function timeSocial(sec) {
        min = 60;
        hour = 3600;
        
        time = Math.floor(sec/hour);
        if (time > 0) {
            timeHour = Math.floor(sec/hour);
            timeMin = Math.floor((sec-time*hour) / min);
            timeSec = sec - timeHour*hour - timeMin*min;
            return timeHour + general.time.hour + timeMin + general.time.min  + timeSec + general.time.sec;
        }
        
        time = Math.floor(sec/min);
        if (time > 0) {
            time = Math.floor(sec/min);
            return time + general.time.min + (sec-time*min) + general.time.sec;
        }
        
        return sec + general.time.sec;
    }
    
    function progressHandler(progress) {
        $("#progress").show();
        $(".progress-box-bytes").html("Объем данных: <b>"+ size(progress.bytesLoaded) +"</b> / <b>"+ size(progress.bytesTotal) +"</b>");
        $(".progress-box-speed").html("Скорость: <b>"+ size(progress.speed) +"/сек</b>");
        $(".progress-box-percent").html(progress.percent+"%");
        $(".progress-box-remained").html("Осталось: <b>"+ timeSocial(progress.loseTime) +"</b>");
        $(".progress-bar-full").css("width", progress.percent + "%");
    }
    
    function ioErrorHandler(evt) {
		$.prompt("Сервер недоступен или перегружен, попробуйте снова через несколько минут");
        $.post("/upload/logUploadFlashError", { 
            fdtype: 'fdtype=flashupload',
            upload_type: 'upload_type=s',
            user_id: 'user_id=',
			flashver: 'flashver=' + flashVersion(),
            browser: 'browser=' + navigator.userAgent
        });
    }
    
    function httpStatusHandler(evt) {
        var text = "";
        if (evt.status == '404') {
            text =  "Сервер недоступен или перегружен, попробуйте снова через несколько минут";
        } else {
            text =  "Сервер вернул код ошибки - " + evt.status;
        }
        
        $.prompt(text);
        $("#progress").hide();
    }
        
    function completeHandler(evt) {
    }
    
    function uploadCompleteDataHandler(evt) {
        
    }
    
    function uploadCompleteDataHandler(evt) {
        if (evt.result) {
            var fileData = evt.id.split(' ');
            if(fileData[0]!='unknown') {
                window.location.href = "/newfile/edit/" + evt.id;
            } else {
                var re = new RegExp('newfiles=([^;]+)');
                var value = re.exec(document.cookie);
                if(value) {
                    var newFiles = eval('(' + value + ')');
                } else {
                    var newFiles = new Array();
                }
                var newData = {'key': fileData[1], 'name': encodeURIComponent(fileData[2]), 'id': null};
                newFiles.push(newData);
                expDate = new Date();
                expDate.setDate(expDate.getDate() + 3);
                document.cookie = 'newfiles='+$.toJSON(newFiles)+';expires='+expDate.toUTCString();
                window.location.href = "/newfile/wait/";
            }
        } else {
            $.prompt(evt.message);
        }
    }
    
    function errorHandler(evt) {
        var text = "";
        if (evt.type == 'bigSizeFiles') {
            text =  "Превышен максимальный размер файла " + size(evt.maxSize);
        }
        
        if (evt.type == 'zeroSizeFiles') {
            text =  "Select a file to upload:";
        }
        $.prompt(text);
    }
    
    function userRules(rules) {
        thisMovie("flashplayer").userRules(rules.checked);
    }
    
    function thisMovie(movieName) {
        if (navigator.appName.indexOf("Microsoft") != -1) {
            return window[movieName];
        } else {
            return document[movieName];
        }
    }

