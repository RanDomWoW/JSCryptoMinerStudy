  function open_nested(node_id){
    if( !node_id ) return;
    status = document.getElementById(node_id).getAttribute("status");
    if( status == "close" ){
          document.getElementById(node_id).setAttribute("status", "open");
          els = document.getElementsByTagName("li");
          for( i=0; i < els.length; i++ ){
                el = els.item(i);
                if( el.getAttribute("name") == node_id ){
                    el.style.display = "block";
                }
          }
    }else{
          document.getElementById(node_id).setAttribute("status", "close");
          els = document.getElementsByTagName("li");
          for( i=0; i < els.length; i++ ){
                el = els.item(i);
                if( el.getAttribute("name") == node_id ){
                    el.style.display = "none";
                }
          }
    }
  }

</script>





