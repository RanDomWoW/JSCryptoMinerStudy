function gf(etrgt){
  etarget=("id"+etrgt);
  if (k!=0){
    cleaning();
  }
  k=1;
  showed[k]=etrgt;
//  document.all[etarget].style.display="block";
//  document.all[etarget].display="block";
  document.getElementById(etarget).style.display="block";
  document.getElementById(etarget).display="block";

}


function srch(sw){
 if (k!=0){
   cleaning();
 }
 if (sw.length>2){
   k=0;
   for (var j=1; j<Entries; j++){
  if (namek[j]!=null){
     sw1=namek[j].toUpperCase();
     sw2=sw.toUpperCase();
     if (sw1.indexOf(sw2)!=-1){
       k++;
       showed[k]=j;
       eid=("id"+j);

       document.getElementById(eid).style.display="block";
       document.getElementById(eid).display="block";
//       document.all[eid].style.display="block";
//       document.all[eid].display="block";

     }

    }
   }
 }
}

function cleaning(){
  for (var j=0; j<k; j++){
     eid=("id"+showed[j+1]);

     document.getElementById(eid).style.display="none";
     document.getElementById(eid).display="none";
//     document.all[eid].style.display="none";
//     document.all[eid].display="none";

  }
  k=0;
}

function show_all(){
   k=0;
   for (var j=1; j<Entries; j++){
     k++;
     showed[k]=j;
     eid=("id"+j);

     document.getElementById(eid).style.display="block";
     document.getElementById(eid).display="block";
//     document.all[eid].style.display="block";
//     document.all[eid].display="block";

   }
}

function onenter(sw_){
   srch(sw_);
   location.href='#dan';
   return false;
}

function on_load(){
   if (!document.all){
      Node.prototype.__defineGetter__("all", function(){
         if (document.getElementsByTagName("*"), lenght){
            switch (this.nodeType){
               case 9:
                  return document.getElementsByTagName("*");
                  break;
               case 1:
                  return this.getElementsByTagName("*");
                  break;
            }
         }
         return "";
      });
      Node.prototype.__defineSetter__("all", function(){});
   }
}

