<script type="text/javascript">
function toggle(className, flg){
	if (flg) {
		$("."+className+"_iphone").hide();
		$("."+className+"_ipad").show();
		$("."+className+"_toggle_iphone img").attr("src","/images/iphone_off.png");
		$("."+className+"_toggle_ipad img").attr("src","/images/ipad_on.png");
		$("."+className+"_toggle_iphone a").css("cursor", "pointer");
		$("."+className+"_toggle_ipad a").css("cursor", "default");
	} else {
		$("."+className+"_ipad").hide();
		$("."+className+"_iphone").show();
		$("."+className+"_toggle_iphone img").attr("src","/images/iphone_on.png");
		$("."+className+"_toggle_ipad img").attr("src","/images/ipad_off.png");
		$("."+className+"_toggle_iphone a").css("cursor", "default");
		$("."+className+"_toggle_ipad a").css("cursor", "pointer");
	}
}
function spNavi(){
	var val = $('#select_menu').val();
	if(val != "") {
		location.href = "/ranking/?genre="+val;
	}
}
function getInfo(className, app_id){
	$.getJSON("http://itunes.apple.com/lookup?id="+app_id+"&cc=jp&callback=?",function(data){
		var json = data.results[0];
		var rating_img_src = (json.averageUserRating) ? "/images/rating_"+(json.averageUserRating*10)+".png" : "/images/rating_0.png";
		$("."+className+" .artwork a").attr("href", "/graph/?appid="+app_id);
		$("."+className+" .artwork img").attr("src", json.artworkUrl60);
		$("."+className+" .app_name a").attr("href", "/graph/?appid="+app_id);
		$("."+className+" .app_name a").text(json.trackName);
		$("."+className+" .price img").before(json.price+"JPY");
		$("."+className+" .price img").attr("src", rating_img_src);
		$("."+className+" .genre a").attr("href", "/ranking/?genre="+json.genreIds[0]);
		$("."+className+" .genre a").text("["+json.genres[0]+"]");
		$("."+className+" .developer").text(json.artistName);
	});
}

