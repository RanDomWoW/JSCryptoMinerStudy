        function ($, customerData) {
            function openPopin() {
                $("#popinshop").show();
                $("#close_popin").bind('click', function () {
                    $("#close_popin").unbind('click');
                    $('#popinshop').hide();
                });
            }

            function loadScript() {

                var script = document.createElement('script');
                script.setAttribute('type', 'text/javascript');
                script.setAttribute('id', 'wscript');
                script.setAttribute('data-target', '#widget-bridge');
                script.setAttribute('data-api-key', '58cbaafd8fb16710001f28b1_4cbf38d8-386c-4418-8603-c3d7c2b65018');
                script.setAttribute('data-api-secret', 'be8d5101-8bba-4e16-80f2-c87b983bfb8b');
                script.setAttribute('data-locale', 'fr');
                script.setAttribute('data-store-locator-url', 'https://magasin.mr-bricolage.fr/');
                script.setAttribute('data-search-placeholder', 'Renseignez votre ville, code postal pour vous connecter à votre magasin le plus proche.');
                script.setAttribute('data-custom-style-url', 'https://www.mr-bricolage.fr/static/version1520327882/frontend/Pictime/mrbricolage/fr_FR/Mrbricolage_Shop/css/popin.min.css')
                script.setAttribute('data-custom-redirection-url','https://www.mr-bricolage.fr/shop/');
                $("body").append(script);

                $("#wscript").attr('src', 'https://bridge-widget.v3.leadformance.com/release/v1.0.1/bridge-widget.js');
            }



            $(document).ready(function () {
                var shop_id = $.cookie('magasin') || 'national';
                if (shop_id === 'national') {
                    openPopin();
                    loadScript();
                }
                $(".link_change_shop").bind('click', function () {
                    openPopin();
                    if ($("#wscript").length == 0) {
                        loadScript();
                    }
                });
            });
        });
</script>
