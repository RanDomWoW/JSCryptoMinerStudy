<script>
function content_size(size_type) {
	
	$(".sizeble").each( function() {

		var fontSize = parseInt($(this).css("font-size"));

		if ( size_type == "i" ) {
			fontSize += 1 ;
		} 
		else {
			fontSize -= 1;
		}
		$(this).css({'font-size':fontSize + "px"});
	} );
	
	if ( $("#slider_text").length ) {
		var slider_len = parseInt($("#slider_text").css("width"));
		var slider_hei = parseInt($("#slider_text").css("height"));
		
		if ( size_type == "i" ) {
			slider_len += 10;
			slider_hei += 3;
		}
		else {
			slider_len -= 10;
			slider_hei -= 3;
		}		
		$("#slider_text").css({ 'width': slider_len, 'height': slider_hei });
	}
};
</script>

