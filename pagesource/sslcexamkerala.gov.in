        <script>
            $(document).ready(function(){
                typingfooter();
                setInterval("typingfooter()",15000);
            });
            function typingfooter()
            {
                Typed.new('.element', {
                  strings: ["Designed", "Developed", "Contents owned", "Maintained", "Updated"],
                  typeSpeed: 100
                });
            }            
            function refreshcaptcha()
            {
                imgSrc = "getimg.php";
                tmp = new Date();
                tmp = "?" + tmp.getTime();
                document.getElementById("captchaImage").src = imgSrc + tmp;
                document.getElementById("captcha").value = "";
            }            
            history.forward();
            function jsPreventAndStopEvent(ev) {
                ev.preventDefault();
                ev.stopPropagation();
            }
            
            function gologin(obj_form)
            {
                if (document.getElementById('chooseVal').value != 'others') {
                    if (_isEmpty(document.getElementById('school_code'), 'School Code', 'M') == false)
                        return false;
                }
                if (_isEmpty(document.getElementById('user_name'), 'User Name', 'M') == false)
                    return false;

                if (_isSpace(document.getElementById('user_name')) == false)
                    return false;

                if (_isSpclChar(document.getElementById('user_name'), "User Name", ['@', '.', '-', '_', '/'], 'M') == false)
                    return false;

                if (_isEmpty(document.getElementById('passwd'), 'Password', 'M') == false)
                    return false;

                callphp(obj_form, 'controller/loginController.php', handlegologin);
            }

            function handlegologin() {
                var aTok = document.getElementById('loginFrm').htok.value;
                //alert("handlegologin : "+aTok);
                if (http.readyState == 4) {
                    results = http.responseText;
                    //alert("handlegologin : "+results);
                    if (results != '' && results != null) {
                        //res = results.count("Mesg");
                        res = getResultCount(results, "Mes");
                        if (getResultCount(results, "Mes") >= 1)
                        {
                            results_arr = results.split("Mes");
                            res1 = results_arr[1];
                            if (res1 == '01') {
                                alert("Invalid Session. Please refresh the page and login again.");
                                window.location.href = "index.php";
                            } else if (res1 == '02') {
                                alert("Invalid Captua. Please type the text as shown in the image.");
                                window.location.href = "index.php";
                            } else if (res1 == '03') {
                                alert("Invalid User Credentials.");
                                window.location.href = "index.php";
                            } else if (res1 == '04') {
                                alert("You are trying to login through this application so many times. If you are a valid user please login using the correct login credentails or the user will be blocked");
                                window.location.href = "index.php";
                            } else if (res1 == '05') {
                                alert("This user is made inactive.");
                                window.location.href = "index.php";
                            } else if (res1 == '06') {
                                alert("This is no the appropriate time to login. Please login according to the time slot given for you");
                                window.location.href = "index.php";
                            } else if (res1 == '07') {
                                alert("Invalid Password Credentials.");
                                window.location.href = "index.php";
                            } else if (res1 == '08') {
                                alert("You are not authorised to login");
                                window.location.href = "index.php";
                            } else if (res1 == '11') {
                                window.parent.location.href = 'passwordchangefstlogother.php?tok=' + aTok;
                            } else if (res1 == '12') {
                                window.parent.location.href = 'loginpageIndexOther.php?tok=' + aTok;
                            } else if (res1 == '13') {
                                window.parent.location.href = 'passwordchangefstlogother.php?tok=' + aTok;
                            } else if (res1 == '14') {
                                window.parent.location.href = 'deopageIndex.php?tok=' + aTok;
                            } else if (res1 == '15') {
                                window.parent.location.href = 'passwordchangefstlog.php?tok=' + aTok;
                            } else if (res1 == '16') {
                                window.parent.location.href = 'loginpageIndex.php?tok=' + aTok;
                            } else if (res1 == '17') {
                                window.parent.location.href = 'passwordchangefstlog.php?tok=' + aTok;
                            } else if (res1 == '18') {
                                window.parent.location.href = 'loginpageIndexOther.php?tok=' + aTok;
                            } else if (res1 == '19') {
                                window.parent.location.href = 'passchangemappingfstlogHS.php?tok=' + aTok;
                            } else if (res1 == '20') {
                                window.parent.location.href = 'loginpageIndexHS.php?tok=' + aTok;
                            } else if (res1 == '21') {
                                window.parent.location.href = 'passchangemappingfstlogVHS.php?tok=' + aTok;
                            } else if (res1 == '22') {
                                window.parent.location.href = 'loginpageIndexVHS.php?tok=' + aTok;
                            }
                        } else {
                            alert("Please Contact Administrator");
                            window.location.href = "index.php";
                        }
                    }
                }
            }
            
            function showschoolcode(elem)
            {
                var value = $(elem).val();
                
                if(value == 'others')
                {
                    $('#school_code').attr('disabled','disabled');
                    $('#school_code').attr('placeholder','No need to fill this');
                }
                else
                {
                    $('#school_code').removeAttr('disabled');
                    $('#school_code').attr('placeholder','School Code');
                }
            }

        </script>
