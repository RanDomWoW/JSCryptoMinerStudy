    $('.raf-accordion__item-header').click(function() {
        console.log(1);
        var closestParent = $(this).parent();
        
        if (closestParent.hasClass('raf-accordion__item--active')) {
            closestParent.removeClass('raf-accordion__item--active');
        } else {
            closestParent.addClass('raf-accordion__item--active');
        }
    });
</script>
