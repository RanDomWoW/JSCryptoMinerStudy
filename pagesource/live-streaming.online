    $(document).ready(function () {
        hideAjaxProgress();

        var searchParameterVal = parameterVal["q"];
        if (searchParameterVal) {
            $("#searchBar").val(searchParameterVal);
            onSearchClicked($(".on-muzic"));
        }
    });

    var onPlayerApplied = function () {

        $('.add-scroll').scrollbar();

    }

    var checkPlayerCallBack = function () {
        if ($("#radioHomePage").is(":visible")) {
            if (!$("#sugested-songs").is(":visible")) {
                $("#sugested-songs").fadeIn();
            }
        } else if ($("#radioPlayer").is(":visible") && !$("#sugested-songs").is(":visible")) {
            $("#sugested-songs").fadeIn();
        } else if (!$("#radioPlayer").is(":visible") && $("#sugested-songs").is(":visible")) {
            $("#sugested-songs").fadeOut();
        }

        if (isFavStation(currentStation.Staion_Id)) {
            if ($("#radioPlayer .FavButton").hasClass("addfav")) {
                $("#radioPlayer .FavButton").removeClass("addfav").addClass("removefav");
            }
        } else if ($("#radioPlayer .FavButton").hasClass("removefav")) {
            $("#radioPlayer .FavButton").removeClass("removefav").addClass("addfav");
        }

    }

    var autoCompleteResponse;

    $("#searchBar").autocomplete({
        source: function (request, response) {
            autoCompleteResponse = response;
            autoComplete(request.term);
        }
    });
    jQuery.fn.extend({
        propAttr: $.fn.prop || $.fn.attr
    });

    $(".search .dropdown").on("click", function () {
        if ($(window.event.target).hasClass("dropdown")) {
            if ($(".search .dropdown-menu").is(":visible")) {
                $(".search .dropdown-menu").hide();
            } else {
                $(".search .dropdown-menu").show();
            }
        }
    });

    $(".search .dropdown-menu li").on("click", function () {
        $("#searchOn").val($(this).val());
        $(".search .dropdown span").html($(this).html());
        $(".search .dropdown-menu").hide();
    });

    $("#searchBarDiv .searchButton").on("click", function () {
        if ($("#search .search-btn .on-web").is(":visible")) {
            dosearch();
        } else {
            searchRadioStations();
        }
    });

    $("#searchBar").keydown(function (e) {
        if (e.keyCode == 13) {
            if ($("#search .search-btn .on-web").is(":visible")) {
                dosearch();
            } else {
                searchRadioStations();
            }
        }
    });

    $(".search-btn button").on("click", function () {
        var $this = $(this);
        onSearchClicked($this);
    });

    function onSearchClicked($this) {
        $(".search-btn button.pressed").removeClass("pressed");
        $($this).addClass("pressed");

        if ($("#searchBar").val().length > 0) {
            if ($($this).hasClass("on-muzic")) {
                searchRadioStations();
            } else {
                dosearch();
            }
        } else {
            $("#searchBar").focus();
        }
    }

    function dosearch() {
        if ($("#searchBar").val().length > 0) {
            var q = $("#searchBar").val();
            window.open('http://feed.live-streaming.online/?st=nt&q=' + q + '&barcodeid=521070000000000&publisher=live-streaming&publisherid=52107', '_blank');

        } else {
            $("#searchBar").focus();
        }
    }

</script>

