            $('[data-toggle="tooltip"]').tooltip();
            $('[data-spy="affix"]').each(function (index, element) {
                var top = $(element).offset().top;
                $(element).affix({ offset: { top: top } });
                //console.log(element);
                //console.log(top);

            });
        });
</script>
<script>
        $(window).on("shoppingcartloaded", function () {
            var result = document.cookie.match(new RegExp("cart=([^;]+)"));
            result && (result = JSON.parse(result[1]));
            var cart = result;
            var count = 0;
            $(cart).each(function (index, element) {
                count += element.count;
            });
            $(".shoppingcart .cart-count").html(count);
        });
</script>
