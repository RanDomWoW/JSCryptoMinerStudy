		function email(name, domain, withlink) {
		    var addr = name + '@' + domain;
		    if(withlink) {
		        document.write('<a href="mailto:' + addr + '">' + addr + '</a>');
		    } else {
		        document.write(addr);
		    }
		}

		function email_members(name, domain, text) {
			var addr = name + '@' + domain;
			html = []
			html.push("<a href='mailto:"+ addr + "'>");
			html.push(text +'<br />');
			html.push('<span>Email</span> <span class="icon-arrow-right"></span>');
			html.push('</a>');
			document.write(html.join(""));
		}

	</script>

