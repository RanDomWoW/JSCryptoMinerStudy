<script>
    $(function () {
        var mod = { IS_SHOW_ON_HOME: true, PageSize: 3, PageIndex: 1 };
        $.ajax({
            url: "/Umbraco/Surface/ajax/GetDealerNews",
            data: mod,
            method: 'post',
            async: false,
            dataType: 'json',
            success: function (data) {
                $.each(data, function (index, item) {
                    $.each(item, function () {
                        if (item.CREATE_TIME != undefined && item.CREATE_TIME.length > 0) {

                            if (item.TITLE.length > 30)
                                item.TITLE = item.TITLE.substr(0, 30) + "...";
                            if (item.SUMMARY.length > 120)
                                item.SUMMARY = item.SUMMARY.substr(0, 120) + "...";
                            if (item.IS_OUTNEWS == true)
                                item.Link = item.OUTNEWS_PC_URL;
                            item.LIST_PICTURE_URL = "/assets/resource/index-news/bg-news-001.jpg";
                        }

                    });
                    switch (index) {
                        case 0: $(".news-stage:first img").attr("src", "/assets/resource/index-news/bg-news-001.jpg");
                            $(".news-stage:first .news-title").text(data[index].TITLE);
                            $(".news-stage:first .news-brief p").text(data[index].SUMMARY);
                            $(".news-stage:first .news-content a").attr("href", data[index].Link)
                            break;
                        case 1: $(".news-stage:eq(1) img").attr("src", "/assets/resource/index-news/bg-news-001.jpg");
                            $(".news-stage:eq(1) .news-title").text(data[index].TITLE);
                            $(".news-stage:eq(1) .news-brief p").text(data[index].SUMMARY);
                            $(".news-stage:eq(1) .news-content a").attr("href", data[index].Link); break;
                        case 2: $(".news-stage:eq(2) img").attr("src", "/assets/resource/index-news/bg-news-001.jpg");
                            $(".news-stage:eq(2) .news-title").text(data[index].TITLE);
                            $(".news-stage:eq(2) .news-brief p").text(data[index].SUMMARY);
                            $(".news-stage:eq(2) .news-content a").attr("href", data[index].Link); break;
                    }

                })
                //var data = eval($.toJSON(data));
                //var html = template('t_test', { "list": data });
                //document.getElementById('news-container').innerHTML = html;
                $.getScript("../../assets/js/index.js");

            }

        });



    });
</script>
