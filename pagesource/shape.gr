function adShrinkToFit(containsAd, event, adMainContainerID, adInnerContainerID, isexpand) {
            var adcontainer = document.getElementById(adMainContainerID);
            var adcontainerParent = document.getElementById(adMainContainerID).parentElement;
            var billboards = document.getElementById(adInnerContainerID);

            if (containsAd == true) {
                var screenwidth = window.innerWidth;
                var shrinkpercent = 0;
                var adwidth = event.size[0];
                var adheight = event.size[1];
                var adminheight = event.size[1];

                //Ελεγχος εαν η διαφήμιση είναι μεγαλύτερη του window size
                //και υπολογισμός του zoom
                if (adwidth > screenwidth) {
                    shrinkpercent = Math.round((screenwidth * 100) / adwidth)
                    billboards.style.padding = "0";
                    billboards.style.position = "relative";
                    billboards.style.display = "block";
                    billboards.style.webkitTransform = "scale(0." + shrinkpercent + ")";
                    billboards.style.MozTransform = "scale(0." + shrinkpercent + ")";
                    billboards.style.msTransform = "scale(0." + shrinkpercent + ")";
                    billboards.style.OTransform = "scale(0." + shrinkpercent + ")";
                    billboards.style.transform = "scale(0." + shrinkpercent + ")";

                    //billboards.style.transformOrigin = '0 0' ;

                }

                //Container Height
                if (shrinkpercent != 0) {
                    adminheight = Math.round((adheight * shrinkpercent) / 100);
                }
                if (isexpand != 1) {
                    adcontainer.style.height = (adminheight + 10) + "px";
                } else {
                    adcontainer.style.height = "auto";
                }
            } else {
                adcontainer.style.display = "none";
                adcontainerParent.style.display = "none";
            }
        }


function align2device( id, event ) {
		var adwidth = event.size[0] > 1 ? event.size[0] : '970';
		var adheight = event.size[1] > 1 ? event.size[1] : '250';

	document.getElementById( id ).style.height = adheight + 'px';
			
	try{
		var screenwidth = window.innerWidth;
		var shrinkpercent = 0;

		//console.log( event.size );

		console.log( adwidth, screenwidth );
		 //Ελεγχος εαν η διαφήμιση είναι μεγαλύτερη του window size
		//και υπολογισμός του zoom
		if (adwidth > screenwidth) {
			shrinkpercent = Math.round((screenwidth * 100) / adwidth);
			if (shrinkpercent != 0) {
				adminheight = Math.round((adheight * shrinkpercent) / 100);
			}
			var divBBoard = document.getElementById( id );
			if ( divBBoard ){
				var leftOffset = ( adwidth - screenwidth ) / -2;
				divBBoard.style.padding  = "0px";
				divBBoard.style.position  = "relative";
				divBBoard.style.display  = "block";
				//divBBoard.style.width = adwidth + "px";
				//divBBoard.style.left = leftOffset + "px";
				divBBoard.style.webkitTransform  = "scale(0." + shrinkpercent + ") translateX(" + leftOffset + "px)";
				divBBoard.style.MozTransform  = "scale(0." + shrinkpercent + ") translateX(" + leftOffset + "px)";
				divBBoard.style.msTransform  = "scale(0." + shrinkpercent + ") translateX(" + leftOffset + "px)";
				divBBoard.style.OTransform  = "scale(0." + shrinkpercent + ") translateX(" + leftOffset + "px)";
				divBBoard.style.transform  = "scale(0." + shrinkpercent + ") translateX(" + leftOffset + "px)";
				divBBoard.style.height  = ( adminheight + 10 ) + " !important px";
			}
		}
	}
	catch(err){
		//console.log(err);
	}
}


