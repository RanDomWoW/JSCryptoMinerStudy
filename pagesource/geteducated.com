    function initPage(){
        $(document).ready(function(){
            
        var value = $( "#ddFieldOfStudy option:selected" ).data('categoryid')
         filterConcentration(value);
         $('#ddFieldOfStudy').bind('change',function(){
          var value = $( "#ddFieldOfStudy option:selected" ).data('categoryid')
           filterConcentration(value);
          $('#ddConcentration').val(-1); 
          $('#ddConcentration').selectpicker('refresh');
         });
        $('#ddConcentration').selectpicker('refresh');
        $('#btnQDF').bind('click',function(){
            var f   = $('#ddFieldOfStudy option:selected').data('categoryid');
            var c   = $('#ddConcentration option:selected').data('categoryid');
            var d   = $('#ddDegreeLevel option:selected').data('categoryid');
            var fn   = $('#ddFieldOfStudy option:selected').text();
            var cn   = $('#ddConcentration option:selected').text();
            var dn   = $('#ddDegreeLevel option:selected').text();
            
            if(f==-1){fn='';}
            if(c==-1){cn='';}
            if(d==-1){dn='';}
            
            var url ='//www.geteducated.com/online-degrees';
            if(d > -1){
                url += '/' + $('#ddDegreeLevel option:selected').data('displayname');
            }
            else {
                url += '/all';
            }
            if(f > -1){
                url += '/' + $('#ddFieldOfStudy option:selected').data('displayname');
               
            }
            if(c > -1){
                url += '/' + $('#ddConcentration option:selected').data('displayname');
            }
           
           var a = "DegreeLevel: " + dn + " Field Of Study: " + fn + " Concentration: " + cn;
           ga('send', 'event', 'Home Page QDF Widget Click', a, url, { 'page': location.pathname });
           window.location.href= url;
           return false;
           
    
        });
        
       $('.owl-carousel').each(function(){
