    function filterConcentration(id){
        $("#ddConcentration > option").each(function () {
                    if ($(this).attr('data-parentid') == id) {
                        $(this).show();
                    }
                    else {
                        if($(this).attr('data-categoryid') != -1){
                            $(this).hide();}
                    }
                });
         $('#ddConcentration [data-categoryid=-1]').show();
    } 
    
    initPage();
