<script type ="text/javascript">
function btn() {

    var name_value = document.getElementById('requiredField_name').value;
    var email_value = document.getElementById('requiredField_email').value;
    var message_value = document.getElementById('requiredField_message').value;

    var verif = /^[a-zA-Z][\w\.-]*[a-zA-Z0-9]@[a-zA-Z0-9][\w\.-]*[a-zA-Z0-9]\.[a-zA-Z][a-zA-Z\.]*[a-zA-Z]$/
    var error = false;

    if (name_value == "") {
        $("#span_name").html(" Ce champ est obligatoire.");
        error = true;
    } else {
        $("#span_name").html("");
    }

    if (verif.exec(email_value) == null) {
        $("#span_email").html(" Votre email est invalide.");
        error = true;
    } else {
        $("#span_email").html("");
    }

    if (message_value == "") {
        $("#span_message").html(" Ce champ est obligatoire.");
        error = true;
    } else {
        $("#span_message").html("");
    }

    if (error) {
        return false;    
    }

    $.ajax({
        url: "/users/contact",
        type: "POST",
        data: ({"name" : name_value, "email" : email_value, "message" : message_value}),
        dataType: "json",
        success: function(data){
            document.location.href="/";
            alert("Votre email a été envoyé avec succès !");
        }
    });

    return false;
}
</script>

