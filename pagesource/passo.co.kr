<script>
var set_nums = 0;
function change_top_banner(nums, set_time){
    if(set_time==1){
        if(set_nums == 2){
            set_nums = 0;
        }
        nums = set_nums++;
    }

    for(var i=0; i<"3"; i++){
        if(i == nums){
            document.getElementById("top_banner"+nums).style.display = "block";
            document.getElementById("top_banner_txt"+nums).style.fontWeight = "bold";
        }else{
            document.getElementById("top_banner"+i).style.display = "none";
            document.getElementById("top_banner_txt"+i).style.fontWeight = "normal";
        }
    }
}
</script>


