<script type="text/javascript">

function login_check(mode){

	var login_idx = "";
	var adult = "";
	if(login_idx == "" || mode == "adult" || mode == "admin"){
		switch(mode){
			case "comment" :
				alert('댓글 입력을 위해서는 로그인이 필요합니다.');
			break;
			case "message" :
				alert('쪽지 전송을 위해서는 로그인이 필요합니다.');
			break;
			case "adult" :
				//alert('성인인증이 필요한 서비스입니다.')
				adult = "&adult=on";
			break;
			case "admin" :
				alert('접근권한이 없습니다.\n자동로그아웃됩니다.');
			break;
			default :
		
				alert('서비스이용을 위해서는 로그인이 필요합니다.');
			break;
		}
		var return_url = document.URL;
		if(window.location.hash){
			return_url += window.location.hash;
		}
		return_url = encodeURIComponent(return_url);
		
		document.location.href = "http://passo.co.kr/members/index.php?mode=login" + adult + "&return_url=" + return_url;
	}
	else{
		return true;
	}
	
}
</script>

