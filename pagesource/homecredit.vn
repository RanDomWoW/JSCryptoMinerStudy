            $('#app-installtion-link').attr("href", "https://play.google.com/store/apps/details?id=vn.homecredit.hcvn&referrer=utm_source%3Dhomecredit.vn%26utm_medium%3Dbanner%26utm_content%3Dandroid%26utm_campaign%3Dbadge_icon_website");
        }

        var hideAppBadget = getCookie('hideAppBadget');
        if (IS_MOBILE && (hideAppBadget === '' || hideAppBadget.length === 0)) {
            $('.app-installtion').show();
			if (IS_IPAD) {
				$('body > div.carousel').css( "margin-top", "115px");
			} else {
				$('body > div.carousel').css( "margin-top", "85px");
			}
        }
    }

    // get cookie from name
    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }
    
    /*
     * Hien Huynh: enable dropdown hover
     */
    $('ul.nav li.dropdown').hover(function() {
      $(this).find('.dropdown-menu').stop(true, true).delay(200).fadeIn(500);
    }, function() {
      $(this).find('.dropdown-menu').stop(true, true).delay(200).fadeOut(500);
    });
</script>
