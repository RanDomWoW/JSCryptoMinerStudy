<!-- Your code to create an instance of Fine Uploader and bind to the DOM/template
====================================================================== -->
<script>
	function showError(theError) {
		$("#errorMessageAlert").html(theError).fadeIn();
	}

	function hideError() {
		$("#errorMessageAlert").fadeOut();
	}

	var fineUploader = $('#fine-uploader-gallery').fineUploader({
		template: 'qq-template-gallery',
		request: {
			endpoint: 'uploader/endpoint.php'
		},
		thumbnails: {
			placeholders: {
				waitingPath: 'assets/placeholders/waiting-generic.png',
				notAvailablePath: 'assets/placeholders/not_available-generic.png'
			}
		},
		validation: {
			allowedExtensions: ['jpeg', 'jpg', 'gif', 'png']
		}
	}).on('error', function (event, id, name, reason) {
		// do something
		console.info("Error " + reason);
		showError(reason);
	}).on('complete', function (event, id, name, responseJSON) {
		hideError();
		responseJSON['id'] = id;
		console.info("Complete " + JSON.stringify(responseJSON) + " id " + id + " name " + name);
		if (! isNaN(id)) {
			p = $(".qq-file-id-" + id).get(0);
			p.responseJSON = responseJSON;
			$(".qq-file-id-" + id + " > .view-button").fadeIn();

			if (responseJSON.hasOwnProperty('errorMessage')) {
				showError(responseJSON['errorMessage']);
			}

			if (responseJSON.hasOwnProperty('newCredit')) {
				console.info("credit : " + responseJSON['newCredit']);
				$("#user_credit").text(responseJSON['newCredit']);
			}
		}
	});

//	$("#payment-modal").modal('show');
	function viewclicked(clickedButton) {
		var p = $(clickedButton).parent().get(0);
		if (p.responseJSON) {
			var id = p.responseJSON['id'];
			// alert( JSON.stringify(p.responseJSON) );
			$("#result-modal .modal-title").html("متن یافت شده در " + p.responseJSON['uploadName']);
			$("#result-modal .ocr-text").text(p.responseJSON['ocrResult']);
			fineUploader.fineUploader('drawThumbnail', id, $('#result-modal .large-image'), 400, false);

			$("#result-modal").modal('show');
		} else {
			alert( "No response JSON was set for " + p );
		}

	};
</script>

