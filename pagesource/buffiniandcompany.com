  });

  $('#ad-nav').append("<a id='pbtn' class='pause'></a>");
  var bplplay = true;
  $('#pbtn').click(function() {
    if (bplplay) {
    $('#slideshow').cycle('pause');
      $('#pbtn').removeClass('pause').addClass('play')
      bplplay = false;
    }
    else {
      $('#slideshow').cycle('resume');
      $('#pbtn').removeClass('play').addClass('pause')
      bplplay = true;
    }
  });

});


