function geoip(ip, success_cb, failure_cb) {
    var url = "//freegeoip.net/json/"+(ip!=null?ip:'');
    $.ajax({
      url: url,
      cache: false,
      dataType: "jsonp",
      success: function(rs) {
        success_cb(rs, ip);        
      },
      error: function (e) { 
        console.log("error goip: "+ip);
      }
    });
}

