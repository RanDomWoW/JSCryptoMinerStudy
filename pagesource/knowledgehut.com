<script type="text/javascript">

    // On load
    $(function() {

        // Set
        $.cookie.true = true;
        $.cookie.json = true;

        
        
        
        // Set cookie
        objLoggedUser.country_id = (objLoggedUser.country_id.length) ? objLoggedUser.country_id : objLocation.country.id;
        objLoggedUser.isd = (objLoggedUser.isd.length) ? objLoggedUser.isd : objLocation.country.isd;
        objLoggedUser.city_id = (objLoggedUser.city_id.length) ? objLoggedUser.city_id : objLocation.city.id;
        if(objLoggedUser.logged)
            $.cookie('form-fields', objLoggedUser);

        // Form Fields Callback
        formFieldsCallback();

        
            // Check mobile
            if (window.innerWidth < 1024) {

                locationCallback();
            } else {

                // On mouse move or scroll
                $(document).on('mousemove mousescroll', function() {

                    locationCallback();
                });
            }
        
        // Subscription while registering with social buttons
        $('#is-subscription-social').change(function() {

            is_subscription = ($(this).is(':checked')) ? 1 : 0;

            $('.social-media-register a').each(function() {
                new_href = $(this).attr('data-href')+"?is_subscription="+is_subscription;
                $(this).attr('href', new_href);
            });
        });
    });

    // Form Fields callback
    function formFieldsCallback() {

        // Course dropdown
        if($('select[name="course_id"]').not('[data-course="true"]').length)
            $('select[name="course_id"]').not('[data-course="true"]').dropdownOptions({
                method: 'trending',
                values: courses,
                selected: objCourse.id,
                sorting: false,
                prepend: '<option value="">Select Course</option>'
            }).selectpicker({
                dropupAuto : 'true',
                size : '8'
            });

        // Event dropdown
        if($('select[name="event_id"]').not('[data-event="true"]').length)
            $('select[name="event_id"]').not('[data-event="true"]').dropdownOptions({
                method: 'event',
                values: events,
                selected: objEvent.id,
                sorting: false,
                columns: ['id', 'name', 'city', 'country', 'start_date', 'end_date'],
                prepend: '<option value="">Select Conference</option>'
            }).selectpicker({
                dropupAuto : 'true',
                size : '8'
            });

        // Country dropdown
        if($('select[name="country_id"]').not('[data-country="true"]').length)
            $('select[name="country_id"]').not('[data-country="true"]').dropdownOptions({
                method: 'trending',
                values: countries,
                sorting: false,
                prepend: '<option value="">Select Country</option>'
            }).selectpicker({
                dropupAuto : 'true',
                size : '8'
            });

        // Country dropdown
        if($('select[name="country_id"][data-country="true"]').length)
            $('select[name="country_id"][data-country="true"]').dropdownOptions({
                prepend: '<option value="">Select Country</option>'
            });

        // Isd dropdown
        if($('select[name="isd"]').not('[data-isd="true"]').length)
            $('select[name="isd"]').not('[data-isd="true"]').dropdownOptions({
                method: 'isd',
                values: countries,
                columns: ['value', 'name'],
                sorting: false,
                prepend: '<option value="">Select ISD</option>'
            }).selectpicker({
                dropupAuto : 'true',
                size : '8'
            });

        // Currency dropdown
        if($('select[name="currency_id"]').length)
            $('select[name="currency_id"]').dropdownOptions({
                values: currencies,
                columns: ['id', 'name'],
                prepend: '<option value="">Select Currency</option>'
            });

        // Form Fields
        objFormFields = $.extend(
            true,
            {
                name: '',
                first_name: '',
                last_name: '',
                email: '',
                phone: '',
                isd: '',
                company: '',
                job_title: '',
                specialization: '',
                billing_address: '',
                country_id: 0,
                city_id: 0
            },
            $.cookie('form-fields')
        );

        // Auto Fill
        $('form').autoFill({
            name: objFormFields.name,
            first_name: objFormFields.first_name,
            last_name: objFormFields.last_name,
            email: objFormFields.email,
            phone: objFormFields.phone,
            isd: objFormFields.isd,
            company: objFormFields.organization,
            job_title: objFormFields.designation,
            specialization: objFormFields.designation,
            country_id: objFormFields.country_id,
            city_id: objFormFields.city_id,
            billing_address: objFormFields.address1
        });
    }

    // Location callback
    function locationCallback() {

        // Not shown
        if(objLocation.shown == 0) {

            // Check location set
            setLocation(objLocation.city.id);

            // Set cookie
            $.cookie('form-fields', $.extend(
                true,
                typeof($.cookie('form-fields')) === 'undefined' ? {} : $.cookie('form-fields'),
                {
                    country_id: (objLoggedUser.logged && objLoggedUser.country_id.length && objLoggedUser.country_id == 0) ? objLoggedUser.country_id : objLocation.country.id,
                    city_id: (objLoggedUser.logged && objLoggedUser.city_id.length && objLoggedUser.city_id == 0) ? objLoggedUser.city_id : objLocation.city.id,
                    isd: (objLoggedUser.logged && objLoggedUser.isd.length && objLoggedUser.isd == '') ? objLoggedUser.isd : objLocation.country.isd
                }
            ));

            // Form Fields
            objFormFields = $.cookie('form-fields');

            // Country dropdown
            if($('select[name="country_id"]').not('[data-country="true"]').length)
                $('select[name="country_id"]').not('[data-country="true"]').val(objFormFields.country_id).selectpicker('refresh');

            // City dropdown
            if($('select[name="city_id"]').not('[data-city="true"]').length)
                $('select[name="city_id"]').not('[data-city="true"]').dropdownOptions({
                    method: 'city',
                    parent: objFormFields.country_id,
                    selected: objFormFields.city_id,
                    prepend: '<option value="">Select City</option>'
                });

            // Country dropdown
            if($('select[name="country_id"][data-country="true"]').not('[data-city-enable="true"]').length)
                $('select[name="country_id"][data-country="true"]').not('[data-city-enable="true"]').val(objLocation.country.id);

            // City dropdown
            if($('select[name="city_id"][data-city="true"]').not('[data-city-enable="true"]').length)
                $('select[name="city_id"][data-city="true"]').not('[data-city-enable="true"]').dropdownOptions({
                    method: 'city',
                    parent: objLocation.country.id,
                    selected: objLocation.city.id,
                    prepend: '<option value="">Select City</option>'
                });

            // Isd dropdown
            if($('select[name="isd"]').not('[data-isd="true"]').length)
                $('select[name="isd"]').not('[data-isd="true"]').setISD({country_id: objFormFields.country_id}).selectpicker('refresh');

            // Currency dropdown
            $('select[name="currency_id"]').setCurrency({country_id: objFormFields.country_id});

            // Set
            objLocation.shown = 1;
        }
    }

    // Set location
    function setLocation(city_id) {

        // Ajax Call
        ajaxResult = ajaxCall("https://www.knowledgehut.com/location/ajax_set_location", "GET", {city_id: city_id});

        // Check
        if(typeof(ajaxResult) === 'undefined' || typeof(ajaxResult.result) === 'undefined')
            return;

        // True
        if(ajaxResult.success == true) {

            // Set
            objLocation = $.extend(
                true,
                objLocation,
                {
                    country: {
                        id: ajaxResult.result.location.country.id,
                        name: ajaxResult.result.location.country.name,
                        isd: ajaxResult.result.location.country.isd
                    },
                    city: {
                        id: ajaxResult.result.location.city.id,
                        name: ajaxResult.result.location.city.name
                    },
                    currency: {
                        id: ajaxResult.result.location.currency.name
                    }
                }
            );
        }
    }

	// Social Logins
    $('[data-facebook-login="true"]').facebookLogin();
	$('[data-google-plus-login="true"]').googlePlusLogin();
	$('[data-linkedin-login="true"]').linkedInLogin();

    
    
    </script>
<script type="text/javascript">

    // Obj
    categoryList = $('.row .category-list');

    // Limit categories
    categoryLimit = '3';

    // Converting object to array
    arrCourses = $.map(courses, function(n, i){

        if(!n.trending)
        {
            n.name = n.name.replace('<sup>\u00ae<\/sup>', '\u00ae');
            return [n.name];
        }
    });

    // course obj in course modal.
    objListInput = $('.course-search-form input[name="q"]');

    // Course search on course modal.
    objListInput.typeahead({
       source : arrCourses,
       autoSelect: false,
       afterSelect: displayResult
    });

    // Call back function
    function displayResult(searchValue)
    {
        window.location =  'https://www.knowledgehut.com/search?q='+searchValue;
    }

    $('.body-top-section-close').on('click', function() {
        $('.body-top-section').css('height', '0');
       localStorage.setItem('know_more_banner', 1);
    });

    if(localStorage.getItem('know_more_banner') != 1)
        $('.body-top-section').removeClass('hidden');
</script>
<script type="text/javascript">

    $(document).off('change', $('select[name="category_id"]'));

    // On change of category
    $('select[name="category_id"]').change(function() {

        // Check value exist
        get_courses($(this).val());
    });

    // Get list
    function get_courses(category_id) {

        $('.trending-courses').addClass('loading-icon');

        // Call ajax
        ajaxResponse = ajaxCall("https://www.knowledgehut.com/courses/ajax_get_trending_courses_by_category_id/"+category_id, 'GET');

        $('.trending-courses').removeClass('loading-icon');

        if(ajaxResponse.result.response == true) {
            if(ajaxResponse.result.view != 'undefined') {
                $('.trending-courses-list').html(ajaxResponse.result.view),
                ($('select[name="category_id"]').val() != 0) ? $('.trending-course-url').attr('href',$('.trending-course-list-url:first').data('url')) : $('.trending-course-url').attr('href','https://www.knowledgehut.com/courses');
            }
        }
    }

    // to trigger form on search enter event
    $("input.typeahead").keydown(function(e){
        if (e.keyCode === 13){
            $(".course-search-form").submit();
        }
    });
</script>
