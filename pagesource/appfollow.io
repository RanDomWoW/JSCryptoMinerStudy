<script>
	$(function() {


		if ($('.a-page').length > 0) {
			var landing   = $('.a-page' ).attr('data-landing') || '';
			var auth      = $('.a-login').attr('data-auth'   );

			var page      = $('.a-page').attr('data-page'     ) || 'Unknown';
			var page_name = $('.a-page').attr('data-page-name') || 'Unknown';

			analytics.track('Opened Landing Page', {
				url : location.href,
				page : page,
				page_name: page_name,
				auth : auth
			});
		}

		// submenu

		$('.a-submenu').on('click', function() {
			var page_name = $(this).attr('data-page-name') || $(this).html() || 'Unknown';

			analytics.track('Clicked Submenu', {
				page_name: page_name,
				url: location.href
			});
		});

		// demo

		$('.a-demo').on('click', function() {
			analytics.track('Clicked Demo', {
				url: location.href
			});
		});

		// signup

		$('.a-signup, .a-pricing').on('click', function() {
			var button_name = $(this).html() || 'Unknown';

			analytics.track('Clicked Sign Up', {
				url: location.href,
				button: button_name
			});
		});

		$('.sign_up__button').on('click', function() {
			var pos       = $(this).attr('data-position') || 'top';
			var page      = $('.a-page').attr('data-page'     ) || 'Unknown';
			var page_name = $('.a-page').attr('data-page-name') || 'Unknown';

			analytics.track('Started Sign Up', {
				url: location.href,
				page: page,
				page_name: page_name,
				position: pos
			});
		});

		// login

		$('.a-login').on('click', function() {
			var auth = $(this).attr('data-auth');

			analytics.track('Clicked Sign In', {
				url: location.href,
				auth: auth
			});
		});

		if ($('#login_form').length > 0) {
			var auth = $('#login_form').attr('data-auth');

			analytics.track('Opened Login Page', {
				url: location.href,
				auth: auth
			});
		}

		$('.a-signin').on('click', function() {
			analytics.track('Started Sign In');
		});

		// forgot

		if ($('#forgot_form').length > 0) {
			analytics.track('Opened Forgot Page');
		}

		$('.a-forgot').on('click', function() {
			analytics.track('Started Forgot');
		});

		// reset

		if ($('#reset_form').length > 0) {
			analytics.track('Opened Reset Page');
		}

		$('.a-reset').on('click', function() {
			analytics.track('Started Reset');
		});

		// static

		if ($('#static-page').length > 0) {
			analytics.track('Opened Static Page', {
				url: location.href
			});
		}

		// external link
		$("a").on('click',function(e){
			var url = $(this).attr("href");

			// skip self links
			var check = url.match('://appfollow\.io');
			if (check) return true;

			if (e.currentTarget.host != window.location.host) {
				analytics.track('Clicked External Link', {
					host: e.currentTarget.host.replace(':80',''),
					url : url
				});

				if (e.metaKey || e.ctrlKey || this.target == "_blank") {
					var newtab = true;
				}

				if (!newtab) {
					e.preventDefault();
					setTimeout('document.location = "' + url + '"', 100);
				}
			}
		});
	});
</script>


