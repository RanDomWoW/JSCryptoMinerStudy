<script type="text/javascript">
$(document).ready(function(){
    setTimeout(function(){
        if (!!$('#sticky_300').offset()) {
            var fixtop1 = document.getElementById('sticky_300').getAttribute('fixtop');
            var fixbot1 = document.getElementById('sticky_300').getAttribute('fixbot');
            var stickyTop1 = $('#sticky_300').offset().top;
            var top_art_hot = $('.box_bussines').offset().top ;    
            var display = top_art_hot - stickyTop1;    
            if(display >605){
                $(window).scroll(function(){
                    var windowTop1 = $(window).scrollTop();
                    var currentTop1 = 0;   
                    var bottomTop1 = $('.box_bussines').offset().top - $('#sticky_300').height();           
                    if(windowTop1 < stickyTop1){
                        currentTop1 = stickyTop1 - fixtop1;
                        $('#sticky_300').css({position : 'absolute', top : currentTop1});
                    }else if((windowTop1 <= bottomTop1 ) && (windowTop1 >= stickyTop1)){
                        currentTop1 = 0;
                        $('#sticky_300').css({ position: 'fixed', top: currentTop1});
                    }else{
                        currentTop1 = bottomTop1 - fixbot1;
                        $('#sticky_300').css({position : 'absolute', top : currentTop1});
                    }
                });
            }else{        
                $("#sticky_300").css("display", "none");
            }
        }
    },3000);
});
</script>

