<script id='category_tmpl' type="text/x-jquery-tmpl">
<li class="category-item">
        <a class="title" href="javascript:void(0);">${ category_root }<span>〉</span></a>
        <div class="children cf {{if category_child.length > 10 }}children-col-3 {{/if}}{{if category_child.length > 5 }}children-col-2 {{/if}}">
                {{each(i,child) category_child}}
                    {{if (i+1) % 5 == 1 }}
                    <ul class="children-list children-list-col children-list-col-1">
                    {{/if}}
                        <li class="category-child-item">
                            {{if child.url == '' }}
                            <a class="link" href="http://www.pba.cn/search/${ tag }_1.html">
                            {{else}}
                            <a class="link" href="${ url }">
                            {{/if}}
                                <img src="${ img_url }">
                                <span class="text">${ tag }</span>
                            </a>
                            {{if child.url == '' }}
                                <a href="http://www.pba.cn/search/${ tag }_1.html" class="btn-select">选购</a>
                            {{else}}
                                <a href="${ url }" class="btn-select">选购</a>
                            {{/if}}
                        </li>
                    {{if (i+1) % 5 == 0 }}
                        </ul>
                            {{if (i + 1) != category_child.length }}
                                <ul  class="children-list children-list-col children-col-line"></ul>
                            {{/if}}
                    {{/if}}
                {{/each}}
        </div>
    </li>
</script>
