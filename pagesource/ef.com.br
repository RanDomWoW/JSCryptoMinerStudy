            DTMDataLayer.SubSection2 = segments[0] + ':' + segments[1] + ':' + segments[2] + ':' + segments[3];
            break;
        case 6:
        case 7:
            DTMDataLayer.SiteSection = segments[0] + ':' + segments[1];
            DTMDataLayer.SubSection1 = segments[0] + ':' + segments[1] + ':' + segments[2];
            DTMDataLayer.SubSection2 = segments[0] + ':' + segments[1] + ':' + segments[2] + ':' + segments[3];
            break;
        default:
