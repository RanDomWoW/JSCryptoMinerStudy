    console.error = function() {
        logOfConsole.push({ method: 'CONERR', arguments: arguments });
        if (typeof dl !== "undefined" && typeof dl.events !== "undefined"){
            dl.events.push({
                "event": "platform.messaging.auto.dataLayer.error",
                "error": {
