    window.addEventListener("error", function (e, url) {
        var eleArray = ["IMG", "SCRIPT", "LINK"];
        var resourceMap = {
            "IMG": "Picture file",
            "SCRIPT": "JavaScript file",
            "LINK": "CSS file"
        };
        var ele = e.target;
        if (eleArray.indexOf(ele.tagName) != -1) {
            var url = ele.tagName == "LINK" ? ele.href : ele.src;
            var msg = "src：" + url + " File " + resourceMap[ele.tagName] + " failed loading. ";
            logOfConsole.push({ method: 'LOADERR', arguments: msg });
            if (typeof dl !== "undefined" && typeof dl.events !== "undefined") {
                dl.events.push({
                    "event": "platform.messaging.auto.dataLayer.error",
                    "error": {
                        "code": "LOADERR",
                        "type": "client::" + msg
                    }
                });
            }
            return false; //show in console
        } else {
            var errMsg;
            if(e.error  != null)
                errMsg = e.error.stack;
            else
                errMsg = e.message;
            logOfConsole.push({ method: 'SCERR', arguments: errMsg });
            if (typeof efdl !== "undefined" && typeof efdl.events !== "undefined"){
