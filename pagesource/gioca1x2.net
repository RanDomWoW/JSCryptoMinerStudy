    function addJSToLoad(functionName, param) {
        if (!window.jQuery)
            jsOnLoad.push({ f: functionName, p: param });
        else
            window[functionName](param);
    }

    var bgs = new Array();

    function isBGExists(id) {
        for (var i = 0; i < bgs.length; i++) {
            if (bgs[i] == id) {
                return true;
            }
        }
        return false;
    }

    function getBodyHeight() {

        var w = window,
            d = document,
            e = d.documentElement,
            g = d.getElementsByTagName('body')[0],
            x = w.innerWidth || e.clientWidth || g.clientWidth,
            y = w.innerHeight || e.clientHeight || g.clientHeight;

        var headerHeight = document.getElementsByTagName("header")[0].offsetHeight;
        var footerHeight = document.getElementsByClassName("nav-footer-menu")[0].offsetHeight;
        //    var bodyContentHeight = document.getElementsByClassName('body-content')[0].offsetHeight;

        var bodyMinHeight = y - (headerHeight + footerHeight + 5);

        return bodyMinHeight;
    }

    function setUpFooterHeight() {
        var bodyMinHeight = getBodyHeight();
        var bodyHeights = document.getElementsByClassName('footer-height');
        for (var i = 0; i < bodyHeights.length; i++) {
            bodyHeights[i].style.minHeight = bodyMinHeight + "px";
        }
        elementsToStick = document.getElementsByClassName('body-height');

        for (var i = 0; i < elementsToStick.length; i++) {
            if (elementsToStick[i].firstChild.nextElementSibling != undefined)
                elementsToStick[i].firstChild.nextElementSibling.style.minHeight = (bodyMinHeight + 100) + "px";
        }
        var elementsToStick = document.getElementsByClassName('sticky-body-height');

    }

    function getPosition(element) {
        var xPosition = 0;
        var yPosition = 0;

        while (element) {
            xPosition += (element.offsetLeft - element.scrollLeft + element.clientLeft);
            yPosition += (element.offsetTop - element.scrollTop + element.clientTop);
            element = element.offsetParent;
        }
        return { x: xPosition, y: yPosition };
    }

    function getStyle(el, styleProp) {
        var value, defaultView = (el.ownerDocument || document).defaultView;
        // W3C standard way:
        if (defaultView && defaultView.getComputedStyle) {
            // sanitize property name to css notation
            // (hypen separated words eg. font-Size)
            styleProp = styleProp.replace(/([A-Z])/g, "-$1").toLowerCase();
            return defaultView.getComputedStyle(el, null).getPropertyValue(styleProp);
        } else if (el.currentStyle) { // IE
            // sanitize property name to camelCase
            styleProp = styleProp.replace(/\-(\w)/g, function (str, letter) {
                return letter.toUpperCase();
            });
            value = el.currentStyle[styleProp];
            // convert other units to pixels on IE
            if (/^\d+(em|pt|%|ex)?$/i.test(value)) {
                return (function (value) {
                    var oldLeft = el.style.left, oldRsLeft = el.runtimeStyle.left;
                    el.runtimeStyle.left = el.currentStyle.left;
                    el.style.left = value || 0;
                    value = el.style.pixelLeft + "px";
                    el.style.left = oldLeft;
                    el.runtimeStyle.left = oldRsLeft;
                    return value;
                })(value);
            }
            return value;
        }
    }

    function headerVisibleHeight() {
        var bounds = document.getElementsByTagName("header")[0].getBoundingClientRect();
        var visibleHeight = bounds.height + bounds.y;
        return visibleHeight > 0 ? visibleHeight : 0;
    }

    function footerVisibleHeight() {
        var bounds = document.getElementsByTagName("footer")[0].getBoundingClientRect();
        var visibleHeight = Math.max(document.documentElement.clientHeight, window.innerHeight) - bounds.y;

        return visibleHeight > 0 ? visibleHeight : 0;
    }

    function setUpStickyBodyHeight() {
        //if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) || navigator.userAgent.indexOf("Firefox") > -1) {
        if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
            return;
        }
        var w = window,
            d = document,
            e = d.documentElement,
            g = d.getElementsByTagName('body')[0],
            x = w.innerWidth || e.clientWidth || g.clientWidth,
            y = w.innerHeight || e.clientHeight || g.clientHeight;

        var longestHeight = 0;
        var bodyHeights = w.innerHeight; //document.getElementsByClassName('footer-height');
        var footerHeight = document.getElementsByTagName("footer")[0].clientHeight;
        var headerHeight = document.getElementsByTagName("header")[0].clientHeight;
        var st = window.scrollY;
        /*
            for (var i = 0; i < bodyHeights.length; i++) {
                var oh = bodyHeights[i].clientHeight;
                if (oh > longestHeight) longestHeight = oh;
            }*/
        longestHeight = w.innerHeight;
        var footerScreenPos = getPosition(document.getElementsByTagName("footer")[0]).y;
        if (w.innerHeight > footerScreenPos) {//if this is true the footer is wihtin the screen
            longestHeight = footerScreenPos;
        }

        var headerPos = getPosition(document.getElementsByTagName('header')[0]).y;
        var headerHeight = document.getElementsByTagName('header')[0].clientHeight;
        var headerRem = headerHeight + headerPos;
        if (headerRem > 0) {
            longestHeight = longestHeight - headerRem;
        }

        var elementsToStick = document.getElementsByClassName('sticky-body-height');

        try { 
            longestHeight = Math.max(document.documentElement.clientHeight, window.innerHeight) - (headerVisibleHeight() + footerVisibleHeight())
        }
        catch (e) {
        }

        for (var i = 0; i < elementsToStick.length; i++) {
            if (elementsToStick[i].firstChild.nextElementSibling != undefined) {
                var child = elementsToStick[i].firstChild.nextElementSibling;
                child.style.height = longestHeight + "px";
            }
        }

        var elementToLeft = document.getElementsByClassName('left-panel');
        if (elementToLeft.length > 0) {
            var elementToLeftChild = elementToLeft[0].firstChild.nextElementSibling;
            var positionType = window.getComputedStyle(elementToLeftChild).position;
            if (positionType == "fixed") {
                elementToLeftChild.style.left = getPosition(document.getElementsByClassName('row')[0]).x + "px";
            }
            else {
                elementToLeftChild.style.left = 0 + "px";
            }

            //with this the left menu of sport page won't loose the width when the user scroll down
            // elementToLeftChild.style.width = window.getComputedStyle(elementToLeft[0]).width;
        }
    }

    function setUpHeights() {
        setUpFooterHeight();
        setUpStickyBodyHeight();
    }
    </script>

