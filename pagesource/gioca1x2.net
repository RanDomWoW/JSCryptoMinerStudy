    <script>
        var pageClassName = $('body').attr('class');
        window.history.pushState({ url: "/", pageClassName: pageClassName }, "/", "/");
        function setUpStickeyBodyFixMenu() {

            //stop the execution
            if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
            //if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) || navigator.userAgent.indexOf("Firefox") > -1) {
                return;
            }

            var st = $(window).scrollTop();
            var headerHeight = document.getElementsByTagName("header")[0].offsetHeight;

            if (st < headerHeight) {
                $('.sticky-body-height>div:first-child').css('position', 'inherit').removeClass('col-xs-2').removeClass('col-md-2');
            }
            else {
                $('.sticky-body-height>div:first-child').addClass('col-xs-2').addClass('col-md-2')
                    .css('position', 'fixed')
                    .animate({
                        top: 0
                    }, 300);
            }

            setUpStickyBodyHeight();
        }
        $(document).ready(function () {

            var scrollSetBodyTimeout;
            $(window).scroll(function () {
                if (scrollSetBodyTimeout !== undefined) {
                    clearTimeout(scrollSetBodyTimeout)
                }
                scrollSetBodyTimeout = setTimeout(function () {
                    setUpStickeyBodyFixMenu();
                }, 100);
            });

            $(window).resize(function () {
                setUpHeights();
            });
            while (jsOnLoad.length) {
                var func = jsOnLoad.pop();
                if (window[func.f] != undefined) {
                    if (func.p != undefined) {
                        window[func.f](func.p);
                    }
                    else {
                        window[func.f]();
                    }
                }
            }
        });
    </script>

    

