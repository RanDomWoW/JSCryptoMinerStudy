<script>
function getCubaDate(){
var myDate = new Date();
var summerTime = new Date(2018, 3, 8, 23, 59, 59);
var utcDifference = myDate.getTimezoneOffset()*60000;
var cubaOffset = 5*3600000;
var thatDate = new Date(2018, 3, 19, 12, 59, 59);
if(myDate.getTime()-utcDifference+cubaOffset > summerTime.getTime()){
thatDate = new Date(2018, 3, 19, 11, 59, 59);
}else{
thatDate = new Date(2018, 3, 19, 12, 59, 59);
};
var counterDays = (thatDate.getTime()-myDate.getTime()-utcDifference+cubaOffset)/(1000*60*60*24);
var counterHours=0;
var counterMinutes=0;
if(counterDays.toString().indexOf(".")!=-1){
counterHours = Number("0."+counterDays.toString().split(".")[1])*24;
}
if(counterHours.toString().indexOf(".")!=-1){
counterMinutes = Number("0."+counterHours.toString().split(".")[1])*60;
}
return [Math.floor(counterDays), Math.floor(counterHours), Math.floor(counterMinutes)];
}
var dateCounter = document.getElementById("dateCounter");
var days = " días ";
var hours = " horas ";
var minutes = " minutos ";
if(getCubaDate()[0]==1){days=" día "};
if(getCubaDate()[1]==1){hours=" hora "};
if(getCubaDate()[2]==1){minutes=" minuto "};
if(getCubaDate()[0]==0){
dateCounter.innerHTML="<b>"+getCubaDate()[1]+"</b>"+hours+"<b>"+getCubaDate()[2]+"</b>"+minutes;
}else{
dateCounter.innerHTML="<b>"+getCubaDate()[0]+"</b>"+days+"<b>"+getCubaDate()[1]+"</b>"+hours+"<b>"+getCubaDate()[2]+"</b>"+minutes;
}
