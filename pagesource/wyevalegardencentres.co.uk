<!-- Begin Templates -->
<script type="text/ng-template" id="/assets/partials/address-form.html">
<div data-ng-show="af.addressBook.length && !af.hideAddressBook" id="choose_address_buttons_wrapper">
	<button type="button" class="button primary alt medium-6 columns" data-ng-click="af.switchAddressBookStatus()">
		<span data-ng-hide="af.showAddressBook" id="choose_address_button">Choose from address book</span>
		<span data-ng-show="af.showAddressBook">Enter address manually</span>
	</button>
	<button type="reset" class="button primary alt medium-5 columns" data-ng-click="af.addNewAddress($event)">Add a new address</button>
</div>
<div data-ng-if="af.showAddressBook && af.isSignedIn">
	<ul class="medium-block-grid-2">
		<li data-ng-repeat="address in af.addressBook">
			<address-summary address="address"></address-summary>
			<p class="address__actions">
				<button type="button" class="button primary tiny" data-ng-class="{'alt': af.model.address_id !== address.address_id}" data-ng-click="af.selectFromAddressBook(address)">Use this address</button>
			</p>
		</li>
	</ul>
</div>
<div data-ng-hide="af.showAddressBook">
	<section data-ng-show="!af.hideName">
		<div class="form-row form-row--inline-group">
			<div class="form-row form-row--title">
				<label for="title">Title</label>
				<select name="title" id="title" data-ng-model="af.model.title" data-ng-options="title for title in af.titles" data-ng-required="!af.hideName"></select>
			</div>
			<div class="form-row">
				<label for="first_name">First name</label>
				<input name="first_name" id="first_name" type="text" placeholder="" data-ng-model="af.model.first_name" data-ng-required="!af.hideName" />
				<i class="fa"></i>
			</div>
			<div class="form-row">
				<label for="last_name">Last name</label>
				<input name="last_name" id="last_name" type="text" placeholder="" data-ng-model="af.model.last_name" data-ng-required="!af.hideName" />
				<i class="fa"></i>
			</div>
		</div>
	</section>

	<section>
		<div class="form-row" data-ng-show="af.postcodeLookupError.length">
			<p class="inline-message inline-message--error"><span data-ng-bind="af.postcodeLookupError"></span></p>
		</div>
		<div class="form-row form-row--inline-group">
			<div class="form-row form-row--postcode">
				<label for="postcode_lookup">Postcode</label>
				<input name="postcode_lookup" id="postcode_lookup" type="text" placeholder="" data-ng-model="af.postcode_lookup" data-ng-keydown="af.handlePostcodeSubmit($event)" />
			</div>
			<div class="form-row form-row--no-label form-row--lookupButton">
				<button type="button" class="button primary" data-ng-class="af.lookupStatus" data-ng-click="af.lookupAddress()">Look up address</button>
			</div>
		</div>
		<div class="form-row" data-ng-show="af.addressOptions.length > 1">
			<label for="addressOptions" class="hidden-for-small-up">Choose address</label>
			<select id="addressOptions" name="addressOptions" data-ng-model="af.selectedAddress" data-ng-change="af.addressPicked(af.selectedAddress)"
			 data-ng-options="address as af.truncateAddress(af.oneLineAddress(address)) for address in af.addressOptions track by af.oneLineAddress(address)">
				<option value="">Choose address</option>
			</select>
		</div>
	</section>

	<section>
		<div class="form-row is-required">
			<label for="line_1">Address line 1</label>
			<input id="line_1" type="text" maxlength="60" placeholder="e.g. house number/name and street name or company name" data-ng-model="af.model.line_1" required />
			<i class="fa"></i>
		</div>
		<div class="form-row">
			<label for="line_2">Address line 2 (optional)</label>
			<input id="line_2" type="text" maxlength="60" placeholder="e.g. Flat, suite, unit, building, floor, etc." data-ng-model="af.model.line_2" data-ng-class="{'is-empty': !af.model.line_2.length}" />
			<i class="fa"></i>
		</div>
		<div class="form-row is-required">
			<label for="city">Town/city</label>
			<input id="city" type="text" maxlength="30" placeholder="" data-ng-model="af.model.city" required />
			<i class="fa"></i>
		</div>
		<div class="form-row">
			<label for="county">County (optional)</label>
			<input id="county" type="text" maxlength="30" placeholder="" data-ng-model="af.model.county">
		</div>
		<div class="form-row" id="postcode-validation-error" data-ng-show="af.model.postcodeValidationError">
			<p class="inline-message inline-message--error"><span data-ng-bind="af.model.postcodeValidationError"></span></p>
		</div>
		<div class="form-row form-row--postcode is-required">
			<label for="postcode">Postcode</label>
			<input id="postcode" maxlength="8" name="postcode" type="text" placeholder="" data-ng-model="af.model.postcode" required onkeypress="return event.charCode == 0 || event.charCode == 8 || (event.charCode >= 48 && event.charCode <= 57) || (event.charCode >= 65 && event.charCode <= 92) || (event.charCode >= 97 && event.charCode <= 124)  || event.charCode == 32;"/>
			<i class="fa"></i>
		</div>
	</section>

	<div class="form-row" ng-show="!af.model.id && !af.noSave && af.isSignedIn && !af.hideAddressBook">
		<input type="checkbox" id="saveAddress" name="saveAddress" data-ng-model="af.saveAddress" />
		<label for="saveAddress">Save to address book</label>
	</div>
</div>

</script>

<script type="text/ng-template" id="/assets/partials/address-summary.html">
<p class="address" data-ng-class="{'address--inline': inline}">
	<span class="address__line address__line--name">
			<span data-ng-bind="address.name"></span>
	</span>
  <span data-ng-if="!address.name" class="address__line" data-ng-bind="address.title + ' ' + address.first_name + ' ' + address.last_name"></span>
	<span class="address__line" data-ng-bind="address.line_1"></span>
	<span class="address__line" data-ng-bind="address.line_2"></span>
	<span class="address__line" data-ng-bind="address.city"></span>
	<span class="address__line" data-ng-bind="address.county"></span>
	<span class="address__line" data-ng-bind="address.postcode"></span>
	<span class="address__additional" data-ng-if="address.email || address.phone">
		<span class="address__line" data-ng-if="address.phone" data-ng-bind="address.phone"></span>
		<span class="address__line" data-ng-if="address.email" data-ng-bind="address.email"></span>
	</span>
</p>

</script>

<script type="text/ng-template" id="/assets/partials/article-list.html">
<div class="container main-body">
    <div class="row">
        <div class="columns small-12 large-12">
            <div data-cms-content="top_promo"></div>
        </div>
    </div>
    <div class="row">
        <div class="columns small-12 large-12">
            <div data-cms-content="main_body"></div>
        </div>
    </div>
