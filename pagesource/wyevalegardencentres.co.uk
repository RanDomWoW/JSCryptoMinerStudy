<script type="text/ng-template" id="/assets/partials/quick-look--ensemble.html">
<div data-ng-controller="QuickLookController as vm" class="row overlay--quick-look overlay__inner__body-wrap">

	<section class="column medium-7 zoomViewBox">
		<div data-amplience-carousel data-product="::vm.product"></div>
		<p class="description" preview text="::vm.product.description" expand="true" length="200"></p>
	</section>

	<section class="column medium-5 right">

		<div id="target-wrapper">
	        <div id="target"></div>
	    </div>

		<h1 class="product-detail__product-name" ng-bind-html="vm.product.name"></h1>

		<div class="price">
			<dl class="price-wrapper">
				<dt></dt>
				<dd> <span data-ng-bind="vm.product.totalPrice | currency:'£'"></span></dd>
			</dl>
			<!-- <price data-min-price="::vm.product.min_price" data-max-price="::vm.product.max_price"></price> -->
		</div>

		<a data-ng-href="{{vm.product.url}}#in-this-set" class="primary button">View Details</a>

		<div class="product-detail__ensemble">
			<h2>Included Items</h2>

			<ul class="product-list thumbnails small-block-grid-2">

				<li ng-if="vm.product.products" data-ng-repeat="item in vm.product.products | limitTo:vm.limit">

					<div class="product-info-wrapper-primary">
						<div ng-if="item.product" class="image-wrapper">
							<a data-ng-href="/shop/product-name-{{item.product.id}}/" class="product-img">
								<img data-ng-src="//i1.adis.ws/i/wgc/{{item.product.id}}.jpg?$product_list$" alt="">
							</a>
						</div>

						<div ng-if="!item.product" class="image-wrapper">
							<a data-ng-href="/shop/product-name-{{item.id}}/" class="product-img">
								<img data-ng-src="//i1.adis.ws/i/wgc/{{item.id}}.jpg?$product_list$" alt="">
							</a>
						</div>

					</div>
					<div class="product-info-wrapper-secondary">
						<h4 class="product-info__title"><a data-ng-href="/shop/product-name-{{item.product.id}}/"><span ng-bind-html="item.product.name"></span></a></h4>
					</div>
				</li>

			</ul>

			<a ng-if="vm.product.products.length > vm.limit" data-ng-href="{{vm.product.url}}" class="right">Plus {{vm.product.products.length - vm.limit}} more...</a>
		</div>
	</section>
</div>

</script>

<script type="text/ng-template" id="/assets/partials/quick-look.html">
<div data-ng-controller="QuickLookController as vm" class="row overlay--quick-look overlay__inner__body-wrap">
	<section class="column medium-6 large-7 zoomViewBox">
		<div id="qvZoomView" data-topWrapperElm="#overlay" data-amplience-carousel data-product="vm.product" class="amplience-quickview"></div>
		<p class="item-message" ng-repeat="promo in vm.product.promo_messages"><span data-bind-html-unsafe="promo"></span></p>
	</section>

	<section class="column medium-6 large-5 right">

		<div id="target-wrapper">
	        <div id="target"></div>
	    </div>

		<a href="{{vm.product.url}}" class="product-detail__product-name"><h2 ng-bind-html="::vm.product.name"></h2></a>
		<div class="product-detail__botanical-name" ng-bind-html="vm.product.product_name_3"></div>

		<div class="product-size-note">
			<span data-ng-bind="vm.product.sale_unit"></span>
			<!-- <a href="#">What's This?</a> -->
		</div>

		<div id="reviews-wrapper" class="small-7 medium-12">
			<rating id="ratings" data-product-sku="vm.product.id"></rating>
		</div>
		
		<div class="price-item-wrapper" data-ng-if="transactionalSite && vm.product.final_min_price && vm.product.final_max_price">
			<div class="price" ng-hide="vm.product.final_min_price == 0 && vm.product.final_max_price == 0">
				<price data-format="'PDP'" data-min-price="vm.product.final_min_price" data-max-price="vm.product.final_max_price" data-variants="vm.product.variants" data-price="vm.selectedVariant.final_price" data-min-was-price="vm.product.final_min_was_price" data-max-was-price="vm.product.final_max_was_price" data-was-price="vm.selectedVariant.final_was_price"></price>
				<garden-club-points data-min-price="vm.product.final_min_price" data-max-price="vm.product.final_max_price" data-min-points="vm.product.min_gc_points" data-max-points="vm.product.max_gc_points" data-min-points-value="vm.product.min_gc_points_value" data-max-points-value="vm.product.max_gc_points_value"></garden-club-points>
			</div>
			<span class="item-number product-size-note">Item <span data-ng-bind="vm.selectedVariant.sku_code || vm.product.id"></span></span>
    </div>

		<div class="price-item-wrapper" data-ng-if="transactionalSite && !vm.product.final_min_price && !vm.product.final_max_price">
			<div class="price" ng-hide="vm.product.min_price == 0 && vm.product.max_price == 0">
				<price data-format="'PDP'" data-min-price="vm.product.min_price" data-max-price="vm.product.max_price" data-variants="vm.product.variants" data-price="vm.selectedVariant.price"></price>
			</div>
			<span class="item-number product-size-note">Item <span data-ng-bind="vm.selectedVariant.sku_code || vm.product.id"></span></span>
    </div>


		<div class="price-item-wrapper ng-scope" data-ng-if="!transactionalSite">
			<div>
				<div>
					<dl class="price-wrapper">
						<dt>Price</dt>
						<dd class="price"><span>&pound;#.##</span></dd>
					</dl>
				</div>
			</div>
			<span class="item-number product-size-note">Item <span data-ng-bind="vm.selectedVariant.sku_code || vm.product.id" class="ng-binding">0850021323</span></span>
        </div>

		<ul id="product-badges" class="no-bullet">
			<li data-ng-repeat="badge in vm.product.wbadges track by $index">
				<img ng-src="{{badge}}" />
			</li>
		</ul>

		<div class="product-variants form-row" data-ng-show="::vm.product.variants.length > 1">
			<select data-ng-model="vm.selectedVariant" data-ng-options="variant as variant.color + ' ' + variant.size + ' - ' + '£' + (variant.final_price ? variant.final_price.toFixed(2) : variant.price.toFixed(2)) || variant.color for variant in vm.product.variants" data-ng-change="vm.determinBasketButtonState()">
				<option value="">Please choose an option...</option>
			</select>
			<p data-ng-show="vm.submittedWithoutVariant" class="info-bubble info-bubble--error">
				<span class="inline-message inline-message--error">Please select an option before adding to your basket</span>
			</p>
		</div>

		<form class="row add-to-basket" name="add-to-basket">
			<div class="medium-3 columns">
				<label for="product-quantity">Quantity</label>
				<input id="product-quantity" type="number" pattern="[0-9]*" inputmode="numeric" name="product-quantity" data-ng-model="vm.quantity" data-ng-disabled="vm.frontEndInfo.buyButtonType != 1" min="1" data-validate-quantity />
			</div>
			<div data-ng-if="transactionalSite" class="medium-9 columns">
				<div data-ng-if="vm.frontEndInfo.buyButtonType < 2">
					<button type="submit" class="button primary" data-ng-class="vm.status" data-ng-click="vm.addToBasket()" data-ng-disabled="!vm.frontEndInfo.buyButtonType" >
						<span data-ng-if="vm.frontEndInfo.buyButtonType === 1">Add to basket</span>
						<span data-ng-if="vm.frontEndInfo.buyButtonType === 0">Out of stock online</span>
					</button>
				</div>
				<div data-ng-if="vm.frontEndInfo.buyButtonType === 2" class="not-in-season">
					Out of season
				</div>
			</div>
		</form>
		<div class="row product-detail__secondary-cta">
			<div class="medium-6 columns left">
				<a href="{{vm.product.url}}#details">
					<span class="fa-stack">
					  <i class="fa fa-circle-thin fa-stack-2x"></i>
					  <i class="fa fa-info fa-stack-1x"></i>
					</span> Full item details</a><!-- Links to details tab further down the page /-->
			</div>
			<div class="medium-6 columns right" style="text-align:right">
				<a data-ng-if="transactionalSite" id="save-for-later" href="#save-to-wishlist" data-ng-click="vm.addToWishlist($event)">
					<span data-ng-show="vm.isSaved()">
						<span>Saved</span>
						<i class="fa fa-heart"></i>
					</span>
					<span data-ng-hide="vm.isSaved()">
						<span>Save for later</span>
						<i class="fa fa-heart-o"></i>
					</span>
				</a>
			</div>
		</div>
	</section>
	<section class="secondary-info row">
		<div class="large-12 columns">
			<p class="description" preview text="::vm.product.supplier_product_description" expand="true" length="200"></p>
		</div>
	</section>
</div>

</script>

<script type="text/ng-template" id="/assets/partials/rating.html">
<section>
  <div class="feefo-rating" ng-show="productRating.rating > 0.5">
    <div data-ng-class="{'rating-close-to-zero': productRating.rating < 0.5 , 'rating-stars-wrapper' : true}">
      <span data-ng-repeat="i in [].constructor(5) track by $index" data-ng-class="{
                                                  'rating-full': $index + 1 - productRating.rating <= 0,
                                                  'rating-half': $index + 1 - productRating.rating > 0 && $index + 1 - productRating.rating <= 0.5,
                                                  'rating-empty': $index + 1 - productRating.rating > 0.5,
                                                  'rating-icon': true
                                              }"></span>
      </div>
    <span class="reviews-count" data-ng-bind="'(' + productRating.reviewCount + ')'"></span>
  </div>
</section>

</script>

<script type="text/ng-template" id="/assets/partials/refine.html">
<div data-ng-show="refinementsFiltered.length">
	<ul class="no-bullet tag-list selected-refinements" data-ng-repeat="refinement in refinementsFiltered = (refinements | filter:isSelectedSection)">
		<li data-ng-repeat="value in refinement.values" data-ng-if="isSelected(refinement.key, value.value)">
			<a href="#" class="tag-item" ng-bind-html="value.display_name" data-ng-click="removeValue(refinement.key, value.value)"></a>
		</li>
	</ul>
	<ul class="no-bullet tag-list selected-refinements" data-ng-if="refinementsFiltered.length">
		<li><button class="button primary alt clear-all" data-ng-click="clearRefinements()">Clear all</button></li>
	</ul>
</div>
<div class="container">
	<div class="row collapse filter-options">
		<div class="columns small-12">
			<button class="narrow-results button alt" data-ng-click="refining = !refining" data-ng-init="isActive = false" data-ng-class="{'active': refining, 'refined': refinementsFiltered.length}" >
			<span>Narrow results</span>
			</button>
		</div>
	</div>
</div>
<div class="refinements container" data-ng-class="{'is-closed': !refining}">
	<div class="row">
		<ul class="unstyled">
			<li class="refinement-group" data-ng-repeat="refinement in refinements" data-ng-class="{'is-closed': !isSectionOpen(refinement.key)}" data-ng-if="(refinement.values.length > 1) || (meta.totalCount > refinement.values[0].count) || (refinement.keep_visible === true)">
				<h4 class="refinement-group__heading" data-ng-click="toggleOpenSection(refinement.key)" data-ng-keypress="handleKeypress($event, refinement.key)" tabindex="0" aria-controls="values-{{refinement.key}}"><span data-ng-bind="refinement.display_name"></span>
<span class="more fa-stack">
		  <i class="fa fa-circle-thin fa-stack-2x"></i>
		  <i class="fa fa-angle-down fa-stack-1x"></i>
		</span>
				</h4>
				<ul class="refinement-group__values unstyled" id="values-{{refinement.key}}">
					<li data-ng-repeat="value in refinement.values" data-ng-show="isTruncated(refinement, $index)">
						<input type="checkbox" name="{{value.value}}" id="{{refinement.key}}-{{value.value}}"
							data-ng-model="value.checked"
							data-ng-checked="isSelected(refinement.key, value.value)"
							data-ng-click="updateValue(refinement.key, value.value, value.checked)"
							data-ng-disabled="value.count === 0" />
						<label class="refinement" for="{{refinement.key}}-{{value.value}}">
							<div>
								<span class="refinement__name" data-ng-bind-html="::value.display_name"></span> <span class="refinement__count">(<span data-ng-bind="value.count">0</span>)</span>
							</div>
						</label>
					</li>
					<li data-ng-show="showTruncation(refinement)">
						<a href="#" data-ng-click="refinement.show_all = !refinement.show_all" class="refinement-group__toggle-link">
							<span data-ng-hide="refinement.show_all">&plus; View more</span>
							<span data-ng-show="refinement.show_all">&minus; View less</span>
						</a>
					</li>
				</ul>
			</li>
		</ul>

		<div class="refinements__footer">
			<p class="results-meta">Your selection returned <strong data-ng-bind="meta.totalCount"></strong> items</p>
			<p>
				<button class="close-results button primary" data-ng-click="refining = false">Close &amp; show results</button>
				<a href="#" data-ng-click="clearRefinements()"> Reset all selections</a>
			</p>
		</div>
	</div>
</div>

</script>

<script type="text/ng-template" id="/assets/partials/search.html">
<form name="search" method="get" action="/search/" data-ng-controller="SearchController" data-ng-submit="performSearch($event)">
	<h1>Product search</h1>
	<p><i class="fa fa-info-circle"></i> If you're looking for plants or gardening products, search here</p>
	<div class="row collapse">
		<div class="column medium-9 large-10">
			<input id="search-field" data-ng-model="query" type="search" name="query" data-ng-class="{'with-postfix': screenSize !== 'mobile'}" placeholder="E.g. Clematis, spade, lawn seed" autofocus />
		</div>
		<div class="column medium-3 large-2">
			<button type="submit" class="button primary postfix" data-ng-class="{'postfix': screenSize !== 'mobile'}">Search</button>
		</div>
	</div>
</form>

</script>

<script type="text/ng-template" id="/assets/partials/social-feed.html">
<ul class="live-feed no-bullet small-block-grid-1 medium-block-grid-2 large-block-grid-4" data-ng-show="feed.length">
	<li class="live-feed__item" data-ng-repeat="item in feed">
		<a data-ng-href="{{item.sender.profileURL}}" class="avatar"><img data-ng-src="{{item.sender.photoURL}}" alt="{{item.sender.name}}"></a>
		<div class="activity">
			<a data-ng-href="{{item.action.linkBack}}" data-ng-bind="item.action.title"></a>
			<p class="text-preview" data-ng-bind="item.action.userMessage"></p>
		</div>
	</li>
</ul>
</script>

<script type="text/ng-template" id="/assets/partials/tab-control.html">
<div class="tab-control">
	<ul class="unstyled" data-ng-keydown="tc.handleKeyboard($event)">
		<li data-ng-repeat="tab in tc.tabs" class="tab-control__tab" data-ng-hide="tab.hide">
			<a href="#{{tab.id}}" id="{{tab.id}}-button" class="tab" data-ng-class="{'selected': tab.selected }" role="tab" aria-selected="{{tab.selected}}" aria-expanded="{{tab.selected}}" aria-controls="{{tab.id}}" tabindex="{{tab.selected ? 0 : -1}}" data-ng-click="tc.selectTab($event, tab)">{{tab.name}}</a>
		</li>
	</ul>
</div>

<div data-ng-transclude class="tab-content-container"></div>
</script>

<script type="text/ng-template" id="/assets/partials/tab.html">
<div>
	<h2 class="show-for-small-only tab ac-head container" data-ng-class="{'selected': selected}" role="tab" aria-selected="{{selected}}" aria-expanded="{{selected}}" aria-controls="{{id}}" tabindex="{{selected ? 0 : -1}}" data-ng-click="selectTab($event, tab)" data-ng-keydown="handleKeyboard($event)">
		<span class="row collapse">
			<span class="">{{ name }}</span>
		</span>
		<span class="more fa-stack">
		  <i class="fa fa-circle-thin fa-stack-2x"></i>
		  <i class="fa fa-angle-down fa-stack-1x"></i>
		</span>
	</h2>
	<div data-ng-transclude class="tab-content container" data-ng-show="selected" id="{{id}}" role="tabpanel" aria-hidden="{{selected}}" tabindex="{{selected ? 0 : -1}}"></div>
</div>
</script>

<script type="text/ng-template" id="/assets/partials/terms-conditions.html">
<div data-ng-controller="TermsConditionsController as tc">
	<div data-ng-bind-html="tc.content"></div>
</div>
</script>

<script type="text/ng-template" id="/assets/partials/tooltip-html-unsafe-popup.html">
<span class="tooltip tip-{{placement}}"
  data-ng-class="{ in: isOpen(), fade: animation() }"
  style="width: auto">
  <span data-bind-html-unsafe="content"></span>
  <span class="nub"></span>
</span>

</script>

<script type="text/ng-template" id="/assets/partials/tooltip-popup.html">
<span class="tooltip tip-{{placement}}"
  data-ng-class="{ in: isOpen(), fade: animation() }"
  style="width: auto">
  <span data-ng-bind="content"></span>
  <span class="nub"></span>
</span>

</script>
<!-- End Templates -->

</body>
</html>