<script type="text/ng-template" id="/assets/partials/line-item-clippings.html">
<div class="container">
	<div class="row">
		<div class="product-info-wrapper-primary small-4 medium-5 columns">
			<div class="image-wrapper">
				<a href="{{product.url}}" class="product-img"><img data-ng-src="{{product.image}}" alt="{{product.name}}"><p class="item-message" ng-repeat="promo in product.promo_messages"><span class="promo-message" data-bind-html-unsafe="promo"></span></p></a>
			</div>
		</div>
		<div class="product-info-wrapper-secondary small-8 medium-7 columns">
			<!--
			<a class="save-to-wishlist-icon" href="#" data-ng-show="isSignedIn && !!wishlist" data-ng-click="addToWishlist()">
				<span data-ng-show="isSaved()">
					<span class="hidden-for-small-up">Save to wishlist</span>
					<i class="fa fa-heart"></i>
				</span>
				<span data-ng-hide="isSaved()">
					<span class="hidden-for-small-up">Saved</span>
					<i class="fa fa-heart-o"></i>
				</span>
			</a>
			-->
			<h4 class="product-info__title"><a href="{{product.url}}"><span ng-bind-html="product.product_name_1"></span> <span ng-bind-html="product.product_name_2"></span></a></h4>
			<price data-ng-if="product.final_min_price && product.final_max_price" ng-hide="product.final_min_price == 0 && product.final_max_price == 0" data-min-price="product.final_min_price" data-max-price="product.final_max_price" data-variants="product.variants" data-min-was-price="product.final_min_was_price" data-max-was-price="product.final_max_was_price"></price>
			<price data-ng-if="!product.final_min_price && !product.final_max_price" ng-hide="product.min_price == 0 && product.max_price == 0" data-min-price="product.min_price" data-max-price="product.max_price"></price>
			<div class="product-variants form-row" data-ng-show="product.variants.length > 1">
				<select data-ng-model="product.selectedVariant" data-ng-options="variant as variant.size || variant.color for variant in product.variants" data-ng-change="determinBasketButtonState()">
					<option value="">Please choose an option...</option>
				</select>
				<p data-ng-hide="product.selectedVariant" class="info-bubble info-bubble--error show-on-submit">
					<span class="inline-message inline-message--error">Please select an option before adding to your basket</span>
				</p>
				<p data-ng-show="meta.hasVarentError && !product.selectedVariant" class="info-bubble info-bubble--error">
					<span class="inline-message inline-message--error">Please select an option before adding to your basket</span>
				</p>
			</div>
		</div>
	</div>

	<div class="row">
		<div class="product-info-wrapper-secondary columns small-4 medium-5">
			<a href="javascript:void(0)" data-ng-click="removeFromWishlist()">Remove</a>
		</div>
		<div class="product-info-wrapper-secondary small-8 medium-7 columns">

		</div>
	</div>
	<div class="row">
		<div class="product-info-wrapper-secondary columns small-4 medium-5">
			<div class="add-to-basket__qty-input">
				<label for="quantity-{{product.id}}">Qty</label>
				<input type="number" pattern="[0-9]*" inputmode="numeric" id="quantity-{{product.id}}" data-ng-model="meta.quantity" min="1" data-validate-quantity  max="meta.variantMaxLimitQuantity"/>
			</div>
		</div>
		<div class="product-info-wrapper-secondary small-8 medium-7 columns">
			<div data-ng-if="meta.buyButtonType < 2">

				<button type="button" class="button primary add-to-basket" data-ng-class="status" data-ng-click="addToBasket()" data-ng-disabled="!meta.buyButtonType" >
					<span data-ng-if="meta.buyButtonType === 1">Add to basket</span>
					<span data-ng-if="meta.buyButtonType === 0">Out of stock online</span>
				</button>

			</div>
			<div data-ng-if="meta.buyButtonType === 2" class="not-in-season">
				Not in season
			</div>
		</div>


	</div>
</div>

</script>

