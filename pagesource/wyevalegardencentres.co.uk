<script type="text/ng-template" id="/assets/partials/pagination.html">
<ul class="pagination hidden-for-small-only" data-ng-if="vm.showPagination()">
	<li data-ng-repeat="page in vm.displayPages track by $index" data-ng-class="{'current': vm.currentPage() === page.number - 1, 'nav-arrow': page.arrow}">
		<a data-ng-if="page.arrow && $index == 0" data-ng-click="vm.prevPage()"><i class="fa fa-caret-left"></i></a>
		<a data-ng-if="page.arrow && $index != 0" data-ng-click="vm.nextPage()"><i class="fa fa-caret-right"></i></a>
		<a data-ng-if="!page.arrow && !page.dots" data-ng-bind="page.number" data-ng-click="vm.setPage(page.offset)"></a>
		<a data-ng-if="page.dots">...</a>
	</li>
</ul>

<div class="pagination hidden-for-medium-up" data-ng-if="vm.showPagination()">
	
	<span class="arrow prev" data-ng-class="{'unavailable': vm.currentPage()-1 < 0}" data-ng-show="vm.pages.length"><a data-ng-click="vm.prevPage()"><span><i class="fa fa-caret-left"></i></span> Back</a></span>

	<span class="arrow next" data-ng-class="{'unavailable': vm.currentPage()+1 >= vm.pages.length}" data-ng-show="vm.pages.length"><a data-ng-click="vm.nextPage()">Next <span><i class="fa fa-caret-right"></i></span></a></span>
	
	<div id="selectWrapper">
		Jump to page: 
		<select id="paginationSelect" ng-change="vm.updatePageSelect()" ng-model="vm.currentPageNumber" ng-options="page.number as page.text for page in vm.pages">
		</select>
	</div>
</div>
</script>

<script type="text/ng-template" id="/assets/partials/preview.html">
<div ng-bind-html="display_text"></div>
<a href="javascript:void(0)" data-ng-click="showToggle()" data-ng-if="show">
	<span class="read-more" data-ng-show="!open">Read more</span>
	<span class="read-less" data-ng-show="open">Read less</span>
</a>
</script>

<script type="text/ng-template" id="/assets/partials/price.html">
<section>
	<div ng-show="!minWasPrice || !maxWasPrice || (minWasPrice === minPrice && maxWasPrice === maxPrice)">
		<div data-ng-if="!variants && minPrice !== maxPrice">
			<dl class="price-wrapper">
				<dt></dt>
				<dd class="price"><span data-ng-bind="minPrice | currency:'£'"></span> -&nbsp;</dd>
				<dd> <span class="price" data-ng-bind="maxPrice | currency:'£'"></span></dd>
			</dl>
		</div>
		<div data-ng-if="variants.length == '1' || (!variants && minPrice === maxPrice )">
			<dl class="price-wrapper" data-ng-if="minPrice < maxPrice">
				<dt class="min-price">From</dt>
				<dd class="max-price"><del><span data-ng-bind="maxPrice | currency:'£'"></span></del></dd>
				<dd><span class="sale-price" data-ng-bind="minPrice | currency:'£'"></span></dd>
				<dt>to</dt>
			</dl>
			<dl class="price-wrapper" data-ng-if="minPrice === maxPrice">
				<dt></dt>
				<dd class="price"><span data-ng-bind="maxPrice | currency:'£'"></span></dd>
			</dl>
		</div>
		<div data-ng-if="variants.length > '1'">
			<dl class="price-wrapper">
				<dt></dt>
				<dd data-ng-if="minPrice === maxPrice">{{minPrice | currency:'£'}}</dd>
				<dd data-ng-if="minPrice !== maxPrice">From <span data-ng-bind="minPrice | currency:'£'"></span></dd>
			</dl>
		</div>
	</div>

	<div ng-show="minWasPrice && maxWasPrice && (minWasPrice !== minPrice || maxWasPrice !== maxPrice)">
		<div data-ng-if="!variants && minPrice !== maxPrice">
			<dl class="price-wrapper discounted-price">
				<dt></dt>
				<dd class="price"><span data-ng-bind="minPrice | currency:'£'"></span> -&nbsp;</dd>
				<dd> <span class="price" data-ng-bind="maxPrice | currency:'£'"></span></dd>
			</dl>
			<dl class="price-wrapper was-price">
				<dt></dt>
				<dd class="price"><span data-ng-bind="minWasPrice | currency:'£'"></span> -&nbsp;</dd>
				<dd> <span class="price was-price" data-ng-bind="maxWasPrice | currency:'£'"></span></dd>
			</dl>
		</div>
		<div data-ng-if="variants.length == '1' || (!variants && minPrice === maxPrice )">
			<dl class="price-wrapper discounted-price" data-ng-if="minPrice < maxPrice">
				<dt class="min-price">Now from</dt>
				<dd class="max-price"><del><span data-ng-bind="maxPrice | currency:'£'"></span></del></dd>
				<dd><span class="sale-price" data-ng-bind="minPrice | currency:'£'"></span></dd>
				<dt>to</dt>
			</dl>
			<dl class="price-wrapper was-price" data-ng-if="minPrice < maxPrice">
				<dt class="min-price"><br/>Was from</dt>
				<dd class="max-price"><del><span data-ng-bind="maxWasPrice | currency:'£'"></span></del></dd>
				<dd><span class="sale-price" data-ng-bind="minWasPrice | currency:'£'"></span></dd>
				<dt>to</dt>
			</dl>
			<dl class="price-wrapper" data-ng-if="minPrice === maxPrice">
				<dt></dt>
				<dd class="price"><span class="discounted-price" data-ng-bind="maxPrice | currency:'£'"></span>
					<span class="was-price" data-ng-bind="maxWasPrice | currency:'£'"></span></dd>
			</dl>
		</div>
		<div data-ng-if="variants.length > '1'">
			<dl data-ng-class="{'price-wrapper': (minPrice === maxPrice) || (minPrice !== maxPrice && format === 'PDP'), 'price-wrapper-range': (minPrice !== maxPrice && format !== 'PDP')}">
				<dt></dt>
				<dd data-ng-if="minPrice === maxPrice" class="price">
					<span class="discounted-price" data-ng-bind="minPrice | currency:'£'"></span>
					<span class="was-price" data-ng-bind="minWasPrice | currency:'£'"></span>
				</dd>
				<dd data-ng-if="minPrice !== maxPrice && format === 'PDP'">
					<span id="price-range-pdp">
						<p><span class="discounted-price-range-label">Now from</span></p>
						<p><span class="discounted-price" data-ng-bind="minPrice | currency:'£'"></span><span class="was-price-range" data-ng-bind="'Was from ' + (minWasPrice | currency:'£')"></span></p>
					</span>
				</dd>
				<dd data-ng-if="minPrice !== maxPrice && format !== 'PDP'">
					<span id="price-range">
						<p><span class="was-price-range" data-ng-bind="'Was from ' + (minWasPrice | currency:'£')"></span></p>
						<p><span class="discounted-price-range-label">Now from </span><span class="discounted-price" data-ng-bind="minPrice | currency:'£'"></span></p>
					</span>
				</dd>
			</dl>
		</div>
	</div>
</section>

</script>

<script type="text/ng-template" id="/assets/partials/product-list.html">
<div class="container" data-ng-show="vm.products.length && !vm.isInCentreOnly">
	<div class="row">
		<div class="small-12 medium-9 columns right lister">
			<div class="row">
				<div class="small-12 columns">
					<h1 class="hide-if-empty" data-ng-bind-html="::vm.productListTitle"></h1>
					<h4 class="hide-if-empty" data-cms-content="description"></h4>
				</div>
			</div>
			<div class="row">
				<div class="small-7 medium-3 large-3 columns right sort-by-area">
					<label class="hidden-for-small-up" for="sort-by">Sort by</label>
					<select id="sort-by" class="sort-by" data-ng-model="vm.order" data-ng-change="vm.changeOrder(vm.order)" data-ng-options="option.name for option in vm.orderOptions">
						<option value="">Sort by</option>
					</select>
				</div>
				<div class="small-5 medium-2 large-2 columns right toggle-switch-container">
					<div id="toggle-switch" class="toggle-switch grid" name="toggle-switch">
						<div class="switch-wrapper">
							<input class="radio-grid" id="radio-grid" value="grid" name="view" type="radio" checked="" data-ng-model="vm.view" ng-click="vm.updateView('grid')"><label for="radio-grid">Grid</label>
							<input class="radio-list" id="radio-list" value="list" name="view" type="radio" data-ng-model="vm.view"><label for="radio-list" data-ng-model="view" ng-click="vm.updateView('list')">List</label>
						</div>
					</div>
				</div>
				<div class="small-8 medium-4 columns ">
					<p class="results-meta">Showing <strong data-ng-bind="vm.search.offset + 1"></strong> - <strong data-ng-bind="vm.search.offset + vm.displayedPageSize()"></strong> of <strong data-ng-bind="vm.meta.totalCount"></strong> items</p>
				</div>
				<div class="small-7 medium-3 large-3 columns right results-per-page">
					<label class="hidden-for-small-up" for="products-per-page">Results per page</label>
					<select id="products-per-page" class="sort-by" data-ng-model="vm.pageSize" data-ng-change="vm.changePerPage(vm.pageSize)" data-ng-options="option.name for option in vm.perPageOptions">
						<option value="">Results per page</option>
					</select>
				</div>
			</div>
		</div>
		<div class="medium-3 columns left-column-refinement-wrapper">
			<div class="refinements-wrapper">
				<h2 class="hidden-for-small-only refinements__title">Refine by</h2>
				<div data-refine data-refinements="vm.refinements" data-view="vm.view" data-meta="vm.meta" data-search="vm.search"></div>
				<div data-ng-if="screenSize === 'tablet'" class="refinements__managed-content hide-if-empty" data-cms-content="below_refinements"></div>
			</div>
		</div>
		<div class="small-12 medium-9 columns right lister product-list-wrapper">
			<ul class="product-list" data-ng-class="{ 'product-list--grid': vm.view === 'grid' }">
				<li data-ng-repeat="product in vm.products" data-product-thumb data-product="product" data-layout="vm.view" data-wishlist="vm.wishlist"></li>
			</ul>
		</div>
		<div class="small-12 medium-9 columns right" data-ng-hide="$parent.screenSize === 'mobile'">
			<div class="small-7 medium-3 large-3 columns right">
				<label class="hidden-for-small-up" for="products-per-page">Sort by</label>
				<select id="products-per-page-bottom" class="sort-by" data-ng-model="vm.pageSize" data-ng-change="vm.changePerPage(vm.pageSize)" data-ng-options="option.name for option in vm.perPageOptions">
					<option value="">Results per page</option>
				</select>
			</div>
			<div class="small-9 medium-5 columns right">
				<p class="results-meta results-meta-right">Showing <strong data-ng-bind="vm.search.offset + 1"></strong> - <strong data-ng-bind="vm.search.offset + vm.displayedPageSize()"></strong> of <strong data-ng-bind="vm.meta.totalCount"></strong> items</p>
			</div>
		</div>
		<div class="small-12 medium-9 columns right" data-pagination data-meta="vm.meta" data-search="vm.search"></div>
	</div>
</div>

<div class="container" data-ng-show="vm.isInCentreOnly">
	<div class="row">
		<header class="small-12 medium-9 columns right">
			<h4 data-cms-content="description"></h4>
			<section>
				<div class="row">
					<div class="columns medium-12 hide-if-empty" data-cms-content="in_centre_only"></div>
				</div>
			</section>
		</header>
		<div class="small-12 medium-3 columns">
			<ul class="unstyled subcategory-menu">
				<li data-ng-repeat="subcategory in vm.parentCategoryData._metadata.descendents" ng-class="{'in-centre': vm.isInCentreOnlySubcategory(subcategory)}">
					<a data-ng-href="{{subcategory.link}}" ng-bind-html="subcategory.name"></a>
					<a data-ng-href="{{subcategory.link}}"><img src="https://i1.adis.ws/i/wgc/in-centre-icon-trans" alt="In centre only" data-ng-show="vm.isInCentreOnlySubcategory(subcategory)"></img></a>
					<i data-ng-if="subcategory.description" aria-haspopup="true" class="fa fa-question-circle has-tip" tooltip="{{subcategory.description}}"></i>
				</li>
			</ul>
		</div>
	</div>
</div>

</script>

<script type="text/ng-template" id="/assets/partials/product-thumb.html">
<div class="product-info product-info--primary">
	<div class="image-wrapper">
		<a href="{{product.url}}" class="product-img"><img data-ng-src="{{::product.image}}" data-ng-click="productClick()" alt="{{product.name}}"><p class="item-message" ng-repeat="promo in product.promo_messages"><span class="promo-message" data-bind-html-unsafe="promo"></span></p></a>
	</div>
</div>
