<script type="text/ng-template" id="/assets/partials/edit-store-address-form.html">


<div data-ng-hide="af.showAddressBook">
    <section data-ng-show="!af.hideName">
        <div class="form-row form-row--inline-group">
            <div class="form-row form-row--title">
                <label for="title">Title</label>
                <select name="title" id="title" data-ng-model="af.model.title" data-ng-options="title for title in af.titles" required="!af.hideName"></select>
            </div>
            <div class="form-row">
                <label for="first_name">First name</label>
                <input name="first_name" id="first_name" type="text" placeholder="" data-ng-model="af.model.first_name" data-ng-required="!af.hideName" />
                <i class="fa"></i>
            </div>
            <div class="form-row">
                <label for="last_name">Last name</label>
                <input name="last_name" id="last_name" type="text" placeholder="" data-ng-model="af.model.last_name" data-ng-required="!af.hideName" />
                <i class="fa"></i>
            </div>
        </div>
    </section>

    <section>
        <div class="form-row" data-ng-show="af.postcodeLookupError.length">
            <p class="inline-message inline-message--error"><span data-ng-bind="af.postcodeLookupError"></span></p>
        </div>
        <div class="form-row form-row--inline-group">
            <div class="form-row form-row--postcode">
                <label for="postcode_lookup">Postcode</label>
                <input name="postcode_lookup" id="postcode_lookup" type="text" placeholder="" data-ng-model="af.postcode_lookup" data-ng-keydown="af.handlePostcodeSubmit($event)" />
            </div>
            <div class="form-row form-row--no-label">
                <button type="button" class="button primary" data-ng-class="af.lookupStatus" data-ng-click="af.lookupAddress()">Look up address</button>
            </div>
        </div>
        <div class="form-row" data-ng-show="af.addressOptions.length > 1">
            <label for="addressOptions" class="hidden-for-small-up">Choose address</label>
            <select id="addressOptions" name="addressOptions" data-ng-model="af.selectedAddress" data-ng-change="af.addressPicked(af.selectedAddress)" data-ng-options="address as af.truncateAddress(af.oneLineAddress(address)) for address in af.addressOptions track by af.oneLineAddress(address)">
                <option value="">Choose address</option>
            </select>
        </div>
    </section>

    <section>
        <div class="form-row is-required">
            <label for="line_1">Address line 1 (optional)</label>
            <input id="line_1" maxlength="100" type="text" placeholder="e.g. house number/name and street name or company name" data-ng-model="af.model.line_1"/>
            <i class="fa"></i>
        </div>
        <div class="form-row">
            <label for="line_2">Address line 2 (optional)</label>
            <input id="line_2" maxlength="100" type="text" placeholder="e.g. Flat, suite, unit, building, floor, etc." data-ng-model="af.model.line_2" data-ng-class="{'is-empty': !af.model.line_2.length}" />
            <i class="fa"></i>
        </div>
        <div class="form-row">
            <label for="line_3">Town/city (optional)</label>
            <input id="line_3" maxlength="100" type="text" placeholder="" data-ng-model="af.model.line_3" data-ng-class="{'is-empty': !af.model.line_3.length}"/>
            <i class="fa"></i>
        </div>
        <div class="form-row is-required">
          <label for="county">County (optional)</label>
          <input id="county" maxlength="100" type="text" placeholder="" data-ng-model="af.model.line_4"/>
          <i class="fa"></i>
      </div>
        <div class="form-row form-row--postcode is-required">
            <label for="postcode">Postcode (optional)</label>
            <input id="postcode" maxlength="8" type="text" placeholder="" data-ng-model="af.model.postcode" onkeypress="return event.charCode == 0 || event.charCode == 8 || (event.charCode >= 48 && event.charCode <= 57) || (event.charCode >= 65 && event.charCode <= 92) || (event.charCode >= 97 && event.charCode <= 124)  || event.charCode == 32;"/>
            <i class="fa"></i>
        </div>
        <div class="form-row">
            <label for="phone">Phone (optional)</label>
            <input id="phone" maxlength="20" type="text" placeholder="" data-ng-model="af.model.phone">
        </div>
    </section>

    <div class="form-row" ng-show="!af.model.id && !af.noSave && af.isSignedIn && !af.hideAddressBook">
        <input type="checkbox" id="saveAddress" name="saveAddress" data-ng-model="af.saveAddress" />
        <label for="saveAddress">Save to address book</label>
    </div>
</div>

</script>

