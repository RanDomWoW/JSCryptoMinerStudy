<script type="text/ng-template" id="/assets/partials/checkout/order-summary.html">
<div data-ng-if="!order.status.length">
	<h2 class="order-summary__header">Order Summary</h2>
</div>
<div data-ng-if="order.status.length && !withItems">
	<h2 class="order-summary__header">Order Summary</h2>
</div>
<div data-ng-if="order.status.length && withItems">
	<h2 id="checkoutHeader" class="order-summary__header">Order Confirmation Details</h2>
</div>
<div class="order-summary__inner">
	<!-- <div data-ng-if="withAddresses"> -->
		<div class="order-summary__addresses">

			<h3 data-ng-if="order.delivery_address" class="order-summary__title"><span data-ng-if="order.delivery_method == 'POST'">Delivery to:</span><span data-ng-if="order.delivery_method == 'CFS'">Collect from:</span></h3>
			<address-summary address="order.delivery_address"></address-summary>
			<div class="delivery-method" data-ng-if="order.status!='Cancelled'">
				<h3 data-ng-if="order.delivery_method_description">Delivery Method: <span data-ng-if="order.delivery_method == 'POST'">Home Delivery</span><span data-ng-if="order.delivery_method == 'CFS'">Click & Collect</span></h3>
				<p data-ng-if="order.shipments.shipments_info.length == 1 && order.shipments.shipments_info[0].delivery_type == 'LARGE_ITEM' && order.shipments.large_items_delivery_charge == '0'">
					Large item delivery:  {{order.delivery_method_description}}  (FREE!)
				</p>
				<p data-ng-if="order.shipments.shipments_info[0].delivery_type == 'LARGE_ITEM' && order.shipments.large_items_delivery_charge == '0' && order.shipments.shipments_info.length > 1">
					<span>Large item delivery: {{order.delivery_method_description}} (FREE!)</span><br>
					<span data-ng-if="order.shipments.delivery_charge == '0'">Remaining Items:  {{order.delivery_method_description}} (Free!)</span>
					<span data-ng-if="order.shipments.delivery_charge > '0'">Remaining Items:  {{order.delivery_method_description}} <span data-ng-bind="::order.shipments.delivery_charge | currency : '£'"></span></span>
				</p>
				<p data-ng-if="order.shipments.shipments_info[0].delivery_type != 'LARGE_ITEM' && order.delivery_method == 'POST'">
					<span data-ng-if="order.shipments.delivery_charge == '0'">{{order.delivery_method_description}} (Free!)</span>
					<span data-ng-if="order.shipments.delivery_charge > '0'">{{order.delivery_method_description}} <span data-ng-bind="::order.shipments.delivery_charge | currency : '£'"></span></span>
				</p>
				<p data-ng-if="order.shipments.shipments_info[0].delivery_type != 'LARGE_ITEM' && order.delivery_method == 'CFS'">
					<span data-ng-if="order.shipments.delivery_charge == '0'">{{order.delivery_method_description}} (Free!)</span>
					<span data-ng-if="order.shipments.delivery_charge > '0'">{{order.delivery_method_description}}</span>
				</p>
				<p data-ng-if="order.shipments.large_items_delivery_charge > '0'">
					<span data-ng-if="order.shipments.large_items_delivery_charge > order.shipments.delivery_charge"> {{order.delivery_method_description}}  <span data-ng-bind="::order.shipments.large_items_delivery_charge | currency : '£'"></span></span>
					<span data-ng-if="order.shipments.delivery_charge > order.shipments.large_items_delivery_charge"> {{order.delivery_method_description}}  <span data-ng-bind="::order.shipments.delivery_charge | currency : '£'"></span></span>
				</p>
			</div>
			<div class="delivery-method" data-ng-if="order.status=='Cancelled'">
				<h3 data-ng-if="order.delivery_method_description">Delivery Method: <span data-ng-if="order.delivery_method == 'POST'">Home Delivery</span><span data-ng-if="order.delivery_method == 'CFS'">Click & Collect</span></h3>
				<p>{{order.delivery_method_description}}</p>
		</div>
	<!-- </div> -->
	<table class="order-summary__section" data-ng-repeat="shipment in order.shipments.shipments_info">
		<tbody data-ng-if="withItems">
			<tr class="order-summary__line-item">
				<th colspan="3">
					<h3 ng-class="{large_item_only: order.shipments.shipments_info.length == 1}" data-ng-if="(order.shipments.shipments_info.length == 1 && order.shipments.shipments_info[0].delivery_type == 'LARGE_ITEM') || order.shipments.shipments_info.length > 1">

						<span data-ng-if="order.shipments.shipments_info.length > 1">Delivery {{$index + 1}}</span>

						<div class="delivery-date selected" data-ng-if="shipment.delivery_type == 'LARGE_ITEM' &&  order.localData.cal.dateSet">
								Delivery Date: {{order.localData.cal.deliver.dd}}/{{order.localData.cal.deliver.mm}}/{{order.localData.cal.deliver.yyyy}} <span ng-click="summaryCalendar.showCal({calId:'summaryCalendarWrap', animateId:'summaryCalendarWrap', buttonId:'summaryCalendarButton'})" class="underline" data-ng-if="!withAddresses">change</span>
						</div>

						<div id="summaryCalendarButton" class="delivery-date" data-ng-if="shipment.delivery_type == 'LARGE_ITEM' && !order.localData.cal.dateSet" ng-click="summaryCalendar.showCal({calId:'summaryCalendarWrap', animateId:'summaryCalendarWrap', buttonId:'summaryCalendarButton'})"><span class="underline">Choose Delivery Date</span></div>

							<div id="summaryCalendarWrap" class="calendarWrap" data-ng-if="shipment.delivery_type == 'LARGE_ITEM'">
					            <div class="calendar">
					            	<table class="topDayMonthYear">
					            		<thead>
					            			<tr>
					            				<th class="arrow arrowLeft summaryCal">
					            					<span ng-if="summaryCalendar.calendar.canGoLastMonth" ng-click="summaryCalendar.advanceMonth({cal:'summaryCalendarWrap', iInt:-1, elmID:'summaryCalendarWrap', animate:true})" > &lsaquo; </span>
					            				</th>
					            				<th colspan=5>
					            					<div class="monthYear">
					            						<ul class="monthName">
					            							<li>{{summaryCalendar.calendar.lastMonth.monthName}} </li>
					            							<li>{{summaryCalendar.calendar.displayMonth.monthName}} </li>
					            							<li>{{summaryCalendar.calendar.nextMonth.monthName}} </li>
					            						</ul>
					            						<span>/</span>
					            						<ul class="yearNumber">
					            							<li>{{summaryCalendar.calendar.lastMonth.year}} </li>
					            							<li>{{summaryCalendar.calendar.displayMonth.year}} </li>
					            							<li>{{summaryCalendar.calendar.nextMonth.year}} </li>
					            						</ul>
					            					</div>
					            				</tH>
					            				<th class="arrow arrowRight" >
					            					<span ng-if="summaryCalendar.calendar.canGoNextMonth" ng-click="summaryCalendar.advanceMonth({cal:'summaryCalendarWrap', iInt:1, elmID:'summaryCalendarWrap', animate:true})">
					            						&rsaquo;
					            					</span>
					            				</th>
					            			</tr>
					            		</thead>
					            		<tbody>
					            			<tr>
					            				<td>Su</td>
					            				<td>M</td>
					            				<td>Tu</td>
					            				<td>W</td>
					            				<td>Th</td>
					            				<td>F</td>
					            				<td>Sa</td>
					            			</tr>
					            		</tbody>
					    			</table>

									<table class="month lastMonth">
										<tbody>
											<tr data-ng-repeat="row in summaryCalendar.calendar.lastMonth.display track by $index">
					    						<td ng-class="{'thismonth': cell.displayMonth}" data-ng-repeat="cell in row " class="{{cell.todayOffset.sStr}} {{cell.selectState}} {{cell.unselectable}}" >
					    							<div>{{cell.date}}</div>
					    						</td>
					    					</tr>
										</tbody>
									</table>

									<table class="month displayMonth">
										<tbody>
											<tr data-ng-repeat="row in summaryCalendar.calendar.displayMonth.display track by $index">
					    						<td ng-class="{'thismonth': cell.displayMonth}" data-ng-repeat="cell in row " class="{{cell.todayOffset.sStr}} {{cell.selectState}} {{cell.unselectable}} {{cell.canDeliver}}" ng-click="summaryCalendar.setDate({cal:'summaryCalendar', date:cell.date-1, flatIndex:cell.flatIndex })">
					    							<div>{{cell.date}}</div>
					    						</td>
					    					</tr>
										</tbody>
									</table>
									<table class="month nextMonth">
										<tbody>
											<tr data-ng-repeat="row in summaryCalendar.calendar.nextMonth.display track by $index">
					    						<td ng-class="{'thismonth': cell.displayMonth}" data-ng-repeat="cell in row " class="{{cell.todayOffset.sStr}} {{cell.selectState}} {{cell.unselectable}}">
					    							<div>{{cell.date}}</div>
					    						</td>
					    					</tr>
										</tbody>
									</table>
					    		</div>
				    		</div>
					</h3>
				</th>
			</tr>
			<tr data-ng-if="shipment.delivery_type == 'STANDARD'  && shipment.supplier_code == ''">
				<td colspan="3" class="shippingDetails">
					<div data-cms-content="standard_delivery"></div>
				</td>
			</tr>
			<tr data-ng-if="shipment.delivery_type == 'STANDARD' && shipment.supplier_code">
				<td colspan="3" class="shippingDetails">
					<div data-cms-content="dsv_delivery"></div>
				</td>
			</tr>
			<tr data-ng-if="shipment.delivery_type == 'LARGE_ITEM' && order.shipments.large_items_delivery_charge == '0'">
				<td colspan="3" class="shippingDetails">
					<div>Large Item - FREE Delivery</div>
				</td>
			</tr>
			<tr data-ng-if="shipment.delivery_type == 'LARGE_ITEM'">
				<td colspan="3" class="shippingDetails">
					<div data-cms-content="large-item_delivery"></div>
				</td>
			</tr>
			<tr data-ng-if="shipment.delivery_type == 'PRE_ORDER'">
				<td colspan="3" class="shippingDetails">
					<div>Pre-order – Your chosen delivery week: w/c {{shipment.delivery_date  | date:'d/M/yyyy'}}</div>
				</td>
			</tr>
			<tr data-ng-if="shipment.supplier_code && shipment.supplier_name">
				<td colspan="3" class="shippingDetails">
					<div>Shipped by: {{shipment.supplier_name}}</div>
				</td>
			</tr>
			<tr data-ng-repeat="item in order.shipments.shipments_info[$index].line_items">
				<td class="image-cell">
					<a class="image-link" data-ng-href="/shop/{{urlPartFor(item.product.name, item.product.code)}}/">
						<img data-ng-src="{{::imagePathFor(item.product.code)}}" height="100" width="100" alt />
					</a>
				</td>
				<td>
					<h4 class="order-summary__line-item__product-name">
						<a data-ng-href="/shop/{{urlPartFor(item.product.name, item.product.code)}}/"><span data-ng-bind-html="::item.product.product_name_1"></span> <span ng-bind-html="::item.product.product_name_2"></span></a>
					</h4>
					<div class="order-summary__details">
						<div data-ng-if="::item.product.size">
							<span class="order-summary__title">Size:</span> <span class="order-summary__value" data-ng-bind="::item.product.size"></span>
						</div>
						<div data-ng-if="::item.product.color">
							<span scope="row" class="order-summary__title">Colour:</span> <span class="order-summary__value" data-ng-bind="::item.product.color"></span>
						</div>
						<div>
							<span scope="row" class="order-summary__title">Qty:</span> <span class="order-summary__value" data-ng-bind="::item.quantity"></span>
						</div>
						<div>
							<span scope="row" class="order-summary__title">Item price: </span>
							<span>
								<span class="order-summary__value" data-ng-if="::item.product.unit_price === 0">FREE</span>
								<span class="order-summary__value" data-ng-if="::item.product.unit_price !== 0" data-ng-bind="::item.product.unit_price | currency : '£'"></span>
							</span>
						</div>
					</div>
					<p class="order-summary__stock-message" data-ng-bind="::item.product.status" data-ng-if="shipment.delivery_type !== 'PRE_ORDER'"></p>
				</td>
				<td class="price-cell" data-ng-bind="item.subtotal | currency : '£'"></td>
			</tr>
		</tbody>
	</table>
	<div data-ng-if="withAddresses" >
		<div class="order-summary__addresses">
			<h3 class="order-summary__title">Payment</h3>
			<div data-ng-if='order.name_on_card !== "Free Order"'>

				<p>
					<span class="address__line">Name on Card: <span data-ng-bind="order.name_on_card"></span></span>
					<span class="address__line"><span data-ng-bind="order.card_type"></span>: <span data-ng-bind="order.card_number"></span></span>
					<span class="address__line">Exp <span data-ng-bind="order.exp_month"></span>/<span data-ng-bind="order.exp_year"></span></span>
				</p>

			</div>

			<address-summary address="order.billing_address"></address-summary>
			<p data-ng-if="order.promotions[0].name">Promo code: <span data-ng-bind="order.promotions[0].name"></span></p>
		</div>
	</div>
	<table class="order-summary__section order-totals">
		<tbody class="order-summary__details order-summary__details--totals" data-ng-if="order.status!='Cancelled'">
			<tr>
				<th scope="row" colspan="2" class="order-summary__title">Items subtotal:</th>
				<td class="price-cell" data-ng-bind="order.grand_items_total | currency : '£'"></td>
			</tr>
			<tr data-ng-if="order.grand_orders_discount_total && order.grand_orders_discount_total > 0">
				<th scope="row" colspan="2" class="order-summary__title price-cell--discount">Order discount:</th>
				<td class="price-cell price-cell--discount">–<span data-ng-bind="order.grand_orders_discount_total | currency : '£'"></span></td>
			</tr>
			<tr data-ng-show="showTotals" class="ng-hide">
				<th scope="row" colspan="2" class="order-summary__title">Delivery charge:</th>
				<td class="price-cell">
					<span data-ng-if="order.delivery_charge === 0">FREE</span>
					<span data-ng-if="order.delivery_charge > 0" data-ng-bind="order.delivery_charge | currency : '£'"></span>
				</td>
			</tr>
		</tbody>
		<tfoot class="order-summary__details order-summary__details--totals order-summary__details--grand-total ng-hide" data-ng-show="showTotals">
			<tr>
				<th scope="row" colspan="2" class="order-summary__title order-summary__title--large">Total:</th>
				<td class="price-cell" data-ng-if="true" data-ng-bind="order.total | currency : '£'"></td>
			</tr>
		</tfoot>
	</table>

	<section class="order-summary__need-help" data-cms-content="need_help"></section>
</div>
</div>

</script>

<script type="text/ng-template" id="/assets/partials/cms-ensemble.html">
<div>
    <div class="product-info-wrapper-secondary">
        <h4 class="product-list__title">
            <span data-ng-bind-html="ensemble.name"></span>
            <span data-ng-bind-html="ensemble.botanical_name"></span>
        </h4>
        <form class="row">
            <ul class="product-list thumbnails small-block-grid-1 medium-block-grid-2">
                <li data-ng-hide="ensemble.products.length">Sorry, this ensemble doesn't appear have any products in.</li>
                <li data-ng-repeat="line_item in ensemble.products" class="product-list__item">
                    <div class="product-info-wrapper-secondary">
                        <h4 class="product-list__title">
                            <a class="quickview-link" href="#" data-ng-click="quickLook($event, line_item.product)" aria-haspopup="true">
                                <span data-ng-bind-html="::line_item.product.product_name_1"></span>
                                <span data-ng-bind-html="::line_item.product.product_name_2"></span>
                            </a>
                        </h4>
                        <form class="row">
                            <div class="small-1 columns">
                                <input id="product-quantity" type="number" name="product-quantity" data-ng-model="line_item.meta.quantity"  min="1" data-validate-quantity placeholder="Qty"/>
                            </div>
                        </form>
                    </div>
                    <div class="clr"></div>
                </li>
            </ul>
