	<body data-template="{{screenSize}}" data-ng-class="{&apos;preview-mode&apos;: previewMode, &apos;highlight-content-blocks&apos;: highlightContentBlocks}">
		<div class="off-canvas-wrap" data-ng-class="{&apos;move-left&apos;: showNav &amp;&amp; screenSize === &apos;mobile&apos;}">

			<div data-ng-if="!noHeader" data-ng-include="&apos;/assets/partials&apos;+ ((isCheckout || isThreeDSecure) ? &apos;/checkout/&apos; : &apos;/&apos;) +&apos;header_&apos; + (isThreeDSecure ? &apos;threedsecure_&apos; : &apos;&apos;) + screenSize + &apos;.html&apos;" data-ng-controller="HeaderController as vm"></div>

			<div class="off-canvas-wrap__content">
				<div class="container" data-ng-class="{&apos;checkout&apos;: isCheckout, &apos;threedsecure&apos; : isThreeDSecure}" data-ui-view="" data-autoscroll="true" role="main"></div>

				 <aside class="container recently-viewed" data-ng-controller="RecentController as rv" data-ng-cloak="" data-ng-show="rv.products.length" data-ng-if="!isCheckout &amp;&amp; !isThreeDSecure &amp;&amp; !noRecentlyViewed">
					<div class="row">
						<div class="large-12 columns">
							<h2 class="hero-heading">Recently viewed</h2>
							<div class="product-list product-list--grid product-list--full-width product-slider slider" id="rv-products" product-slider="" data-sliderwindow="true" data-autoplay="true" data-slidertype="recently-viewed">
								<div data-ng-repeat="product in rv.products | limitTo:rv.rvNumLimit track by product.id" data-product-thumb="" data-product="product" data-wishlist="rv.wishlist" data-layout="&apos;grid&apos;" product-slider="" class="sliderItem"></div>
							</div>
						</div>
					</div>
				</aside>

				<div data-ng-if="!noFooter" data-ng-include="&apos;/assets/partials&apos;+ (isCheckout ? &apos;/checkout/&apos; : &apos;/&apos;) +&apos;footer_&apos; + screenSize + &apos;.html&apos;" data-ng-controller="FooterController as vm"></div>
			</div>

			<aside class="off-canvas-wrap__menu" id="left-off-canvas-menu" aria-expanded="{{showNav}}" data-ng-if="!isCheckout &amp;&amp; !isThreeDSecure &amp;&amp; screenSize === &apos;mobile&apos;" data-ng-controller="NavigationController as vm" data-ng-click="vm.stopProp($event)">

				<div data-ng-if="transactionalSite" data-ng-controller="UserController as vm">
					<div id="m_sign-in">
						<div data-ng-if="vm.isSignedIn">
							<div class="left">
								<a href="/my-account/" class="m_myAccount">
									<i class="fa fa-user"></i> My Account
								</a>
							</div>
							<div class="right">
								<a href="#" data-ng-click="vm.signOut($event)" class="logout">Logout</a>
							</div>
						</div>
						<div data-ng-if="!vm.isSignedIn">
							<a href="/my-account/sign-in/" class="signIn-account" data-ng-click="vm.signIn($event)">
								<i class="fa fa-user"></i> Sign in
							</a>
						</div>
					</div>
				</div>

				<div id="navigation" role="navigation">
					<ul data-ng-show="vm.menus.length">
						<li data-ng-repeat="menu in vm.menus" data-ng-class="{&apos;has-sub&apos;: menu.items.length}">
							<a data-ng-href="{{menu.link}}" data-ng-click="vm.toggleMobileItems($event, menu)"><span title="{{menu.description}}" data-ng-click="vm.dontHandleClick()" data-ng-bind="menu.name"></span></a>
							<span class="more fa-stack" data-ng-click="vm.toggleMobileItems($event, menu)">
							  <i class="fa fa-circle-thin fa-stack-2x"></i>
							  <i class="fa fa-angle-down fa-stack-1x"></i>
							</span>
							<ul data-ng-if="menu.items.length" data-ng-class="{&apos;is-open&apos;: menu.open}">
								<li data-ng-repeat="category in ::menu.items" data-ng-class="{&apos;has-sub&apos;: category.items.length}">
									<a data-ng-href="{{category.link}}" data-ng-click="vm.toggleMobileItems($event, category)"><span title="{{category.description}}" data-ng-click="vm.dontHandleClick()" data-ng-bind="category.name"></span></a>
									<span class="more fa-stack" data-ng-click="vm.toggleMobileItems($event, category)">
									  <i class="fa fa-circle-thin fa-stack-2x"></i>
									  <i class="fa fa-angle-down fa-stack-1x"></i>
									</span>
									<ul data-ng-if="category.items.length" data-ng-class="{&apos;is-open&apos;: category.open}">
										<li data-ng-repeat="link in ::category.items">
											<a data-ng-href="{{link.link}}" title="{{link.description}}"><span data-ng-bind="link.name"></span></a>
										</li>
									</ul>
								</li>
							</ul>
						</li>
					</ul>
				</div>
			</aside>

			<div class="off-canvas-wrap__overlay">
				<!-- TODO find better solution to vm.stopProp($event) -->
				<p class="right closeMobileNav" data-ng-hide="$parent.screenSize === &apos;mobile&apos;">
					<button data-ng-click="vm.closeNav()" class="close-btn">
						<span>Close Menu</span>
					</button>
				</p>
			</div>

		</div>

		<article data-ng-cloak="" id="overlay" class="overlay" data-ng-controller="OverlayController" data-ng-class="{&apos;is-hidden&apos;: data.isHidden}" data-ng-click="hide()">
			<div class="overlay__inner" data-ng-click="stopProp($event)">
				<div class="modal-content-wrap" data-ng-include="data.template"></div>
					<button data-ng-click="hide()" class="overlay__close close-btn">Close</button>
			</div>
		</article>

		<article id="search-box-overlay" class="overlay search-overlay is-hidden" data-ng-controller="SearchBoxController as vm" data-ng-click="vm.hide()">
			<div class="overlay__inner" data-ng-click="vm.stopProp($event)">
				<div class="modal-content-wrap">
					<form name="search" method="get" action="/search/" data-ng-submit="vm.performSearch($event)">
						<h2>Product search</h2>
						<p>
							<span class="fa-stack">
							  <i class="fa fa-circle-thin fa-stack-2x"></i>
							  <i class="fa fa-info fa-stack-1x"></i>
							</span>
							<span id="info">If you're looking for plants or gardening products, search here</span>
						</p>
						<div class="row collapse">
							<div class="column medium-9 large-9">
								<!--<angucomplete-alt
									id="search-field"
									input-name= "query"
									name="query"
									type="search"
						            placeholder="E.g. Roses, or How do I plant a Rose?"
						            pause="400"
						            selected-object="vm.populateSuggestion"
						            remote-url-data-field="hits"
									remote-api-handler="searchAPI"
						            title-field="result.suggestion"
						            description-field="email"
						            match-class="highlight"
						            image-field="profilePic"
						            data-ng-class="{'with-postfix': screenSize !== 'mobile'}"
						            />-->
												<input id="search-field" name="query" type="search" placeholder="E.g. Clematis, spade, lawn seed" data-ng-class="{&apos;with-postfix&apos;: screenSize !== &apos;mobile&apos;}">
							</div>
							<div class="column medium-3 large-3">
								<button type="submit" class="button primary postfix centered {{vm.status}}">Search</button>
							</div>
						</div>
                        <div id="search-lightbox-mc_wrapper" class="container" data-ng-show="header.search_lightbox_help">
                        	<div class="row">
                        		<div class="small-12 columns" data-ng-bind-html="header.search_lightbox_help"></div>
                        	</div>
                        </div>
					</form>
					<button data-ng-click="vm.hide()" class="overlay__close close-btn"></button>
				</div>
			</div>
		</article>


		<article id="preoder-dates-box-overlay" class="overlay is-hidden" data-ng-controller="PreorderDatesController as vm" data-ng-click="vm.hide()">
			<div class="overlay__inner" data-ng-click="vm.stopProp($event)">
				<div id="preorder-delivery-week-panel">
					<h2>Your delivery week</h2>
					<p>We will deliver your order during the week beginning {{vm.formattedFirstDeliveryDate}}. Please confirm this by selecting the date below:</p>
					<p class="inline-message inline-message--error" ng-show="itemPreorderDateAvailableQuantityError">The actual row quantity is higher than the available quantity of {{availableStockForPreoderDate}} for the dates range {{selectedPreorderDateRange}}. Please choose a different pre-order date range or reduce the line quantity.</p>
					<div id="preorderDateRadioButtons" ng-model="itemPreorderDate"></div>
					<p><br>Please note, delivery will be between Wednesday and Friday on your chosen week.</p>
					<div class="confirmation-row row collapse">
						<div class="columns small-5 medium-3 medium-centered small-centered">
							<div class="row confirm-selected-preorder-date"><button ng-disabled="disableConfirmationButton" class="button primary" data-ng-class="vm.status" data-ng-click="updatePreoderDateValue()">Confirm</button></div>
						</div>
					</div>
				</div>
        <button data-ng-click="vm.hide()" class="overlay__close close-btn">Close</button>
			</div>
		</article>

		<!-- angucomplete-alt template (search suggestions) -->
		<script type="text/ng-template" id="/angucomplete-alt/index.html">
		  	<div class="angucomplete-holder" ng-class="{'angucomplete-dropdown-visible': showDropdown}">
			  <input
			  		id="{{id}}"
			  		name="{{inputName}}"
			  		ng-class="{'angucomplete-input-not-empty':notEmpty}"
			  		ng-model="searchStr"
			  		ng-disabled="disableInput"
			  		type="{{inputType}}"
			  		placeholder="{{placeholder}}"
			  		maxlength="{{maxlength}}"
			  		ng-focus="onFocusHandler()"
			  		class="{{inputClass}}"
			  		ng-focus="resetHideResults()"
			  		ng-blur="hideResults($event)"
			  		autocapitalize="off"
			  		autocorrect="off"
			  		autocomplete="off"
			  		ng-change="inputChangeHandler(searchStr)"
			  		required="true" />
			  <div id="{{id}}_dropdown" class="angucomplete-dropdown" ng-show="showDropdown">
			    <div class="angucomplete-searching" ng-show="searching" ng-bind="textSearching"></div>

			    <div class="angucomplete-row" ng-repeat="result in results" ng-click="selectResult(result)" ng-mouseenter="hoverRow($index)" ng-class="{'angucomplete-selected-row': $index == currentIndex}">

			      <div class="angucomplete-title" ng-if="matchClass" match-class="highlight" ng-bind-html="result.title"></div>
			      <div class="angucomplete-title" ng-if="!matchClass"><span ng-bind-html="result.title"></span></div>
			    </div>
			  </div>
			</div>
		</script>
		<!-- angulcomplete-alt template end -->

		<div class="bottom-bar-wrapper" data-ng-cloak="">
			<a href="#" data-ng-if="previewMode" class="hightlight-content-block-toggle" data-ng-controller="HighlightContentBlocksController as vm" data-ng-click="vm.toggleHighlight($event)">
				<i class="fa" data-ng-class="{&apos;fa-eye&apos;: !highlightContentBlocks, &apos;fa-eye-slash&apos;: highlightContentBlocks}" title="Highlight content blocks"></i>
			</a>

			<div data-ng-controller="MessageController" data-ng-if="!noMessages">
				<div data-ng-repeat="message in messages | limitTo:4" class="message alert-box" data-ng-class="message.type">
					<span data-ng-bind-html="message.content"></span>
					<a class="close" data-ng-click="closeMessage($index)">Close</a>
				</div>
			</div>
		</div>

		
