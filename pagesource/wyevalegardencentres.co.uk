	<price data-ng-if="$root.transactionalSite && !product.final_min_price && !product.final_max_price" ng-hide="product.min_price == 0 && product.max_price == 0 || product.type == 'ensemble'" data-min-price="product.min_price" data-max-price="product.max_price" data-variants="product.variants"></price>
