<script type="text/javascript">
jQuery(document).ready(function($) {
  'use strict';

	function toggleActiveTab(currentTab) {
		var featureTabs = $('a[data-toggle="feature-tab"]');
		featureTabs.each(function(index, tab) {
			$(tab.parentElement).removeClass('active');
		});

		$(currentTab.parentElement).addClass('active');
	}

	function hideFeature(feature) {
		$('.FeatureCard[data-toggle="' + feature + '"]').removeClass('is-fading-in');
		$('.FeatureCard[data-toggle="' + feature + '"]').removeClass('z-index-3');
		$('.FeatureCard[data-toggle="' + feature + '"]').addClass('is-fading-out');
	}

	function showFeature(feature) {
		$('.FeatureCard[data-toggle="' + feature + '"]').addClass('is-fading-in');
		$('.FeatureCard[data-toggle="' + feature + '"]').addClass('z-index-3');
		$('.FeatureCard[data-toggle="' + feature + '"]').removeClass('is-fading-out');
	}

  $('a[data-toggle="feature-tab"]').click(function(e) {
    e.preventDefault();

		var feature = this.title;
		var currentFeature = $('.js-feature-tab.active').children()[0].title;

		if (feature === currentFeature) return null;

		toggleActiveTab(this);
		hideFeature(currentFeature);
		showFeature(feature);
  });
});
</script>
