  <script>
  (function() {
    // Track clicks to calls to action whose links include the url_path using this map of
    //    event_name : url_path
    var CALLS_TO_ACTION = {
      "Sign Up Clicked":        "a[href*='/users/new']",
      "Demo Request Clicked":   "a[href*='/request']",
      "Gustavo Video Clicked":  "a[data-lity-target*='www.youtube.com/embed/PYbrpVzKrSw']"
    };

    // Query string parameters to look for campaign attributes
    var TRACKER_KEYS = ['http_referer', 'utm_campaign', 'utm_source', 'utm_medium', 'utm_term', 'utm_content', 'gclid'];

    var trackClicksToCallsToActions = function( ctaMap ) {
      if(!analytics || !analytics.trackLink || !jQuery)
        return false;

      Object.keys(ctaMap).forEach(function(event_name){
        var selector = ctaMap[event_name];
        analytics.trackLink(
          jQuery(selector),
          event_name,
          {
            'title': document.title,
            'url'  : window.location.href,
            'path' : window.location.origin + window.location.pathname
          }
        );
      });
    }

    var parseQueryString = function() {
      return (function(a) {
        if (a == "") return {};
        var b = {};
        for (var i = 0; i < a.length; ++i) {
          var p=a[i].split('=', 2);
          if (p.length == 1)
            b[p[0]] = "";
          else
            b[p[0]] = decodeURIComponent(p[1].replace(/\+/g, " "));
        }
        return b;
      })(window.location.search.substr(1).split('&'));
    }

    var getCampaignTrackers = function(tracker_keys) {
      var trackers = {};
      var queryString = parseQueryString();
      // Get the tracking codes out of the query string, like UTM codes
      tracker_keys.forEach(function(key) {
        if( queryString[key] ) {
          trackers[key] = queryString[key];
        }
      });

      // Get the HTTP referrer if and only if the
      // HTTP_REFERER is from an external domain.
      // Be mindful to ignore subdomains like www. to blog. or secure.
      var root_domain = document.domain.split('.').splice(-2).join('\\.');
      // Be mindful of protocol changes from http to https or the reverse
      var domain_regexp = "^https?:\\/\\/[^\\/]*" + root_domain;

      if(document.referrer && document.referrer.search(domain_regexp) != 0) {
        trackers['http_referer'] = document.referrer;
      }
      return trackers;
    }

    var prefixDictionary = function(prefix, dict) {
      var result = {};
      for(var k in dict) {
        if(dict.hasOwnProperty(k)) {
          result[prefix + k] = dict[k];
        }
      }
      return result;
    }

    var trackCampaignHistory = function(trackers) {
      if (!analytics || !analytics.ready || Object.keys(trackers).length <= 0)
        return false;

      analytics.ready(function() {
        // Track their first campaign if we've never tracked one before
        if(!analytics.user().traits().first_campaign) {
          analytics.identify({'first_campaign': Date()});
          analytics.identify(prefixDictionary('first_campaign_', trackers));
          analytics.track('First Campaign Tracked', trackers);
        }

        // Track each campaign
        analytics.track('Campaign Tracked', trackers);

        // Remember the last campaign specifically
        analytics.identify({'last_campaign': Date()});
        analytics.identify(prefixDictionary('last_campaign_', trackers));
      });
    }

    trackClicksToCallsToActions(CALLS_TO_ACTION);
    trackCampaignHistory(getCampaignTrackers(TRACKER_KEYS));
  })();
</script>
