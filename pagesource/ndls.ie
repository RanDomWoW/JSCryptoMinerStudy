<script>
/* Opens a node in a slider with the class .nn_sliders
which: last number in the id of the slider
place: number of the node to open, starting from 1 from the top*/
function loadnode(which, place){
var amount = jQuery('#set-nn_sliders-' + which).children().length - 1;
var $node;
for (i = 1; i <= amount; i++) {
$node = jQuery('#set-nn_sliders-' + which + ' > div:nth-of-type(' + i + ')');
$node.removeClass('active');
$node.children('.accordion-toggle').addClass('collapsed');
$node.children('.accordion-body').removeClass('in').css('height', '0px');
}
$node = jQuery('#set-nn_sliders-' + which + ' > div:nth-of-type(' + place + ')');
$node.addClass('active');
$node.children('.accordion-toggle').removeClass('collapsed');
$node.children('.accordion-body').addClass('in').css('height', 'auto');
};
/* smooth scroll to id with offset for the title of the node and sticky header */
function scroll(id){
var $head = jQuery('.mm_sticky_header');
if ($head.css('position') == 'fixed'){
var header = $head.outerHeight();
}else{
var header = 0;
}
var offset = jQuery('.accordion-toggle[href$="' + id + '"]').offset().top - header;
jQuery('html,body').animate({scrollTop: offset}, 1000);
};
/*scrolls to the slider on another page be checking the id in the url*/
jQuery(window).load(function() {
var hash = document.URL.substr(document.URL.indexOf('#')+1);
if(hash != null && hash.match(/^[0-9a-zA-Z-]{1,200}$/) && hash.slice(-1) == '-'){
hash = hash.slice(0, -1);
scrollandopen(hash);
} else if(hash != null && hash.match(/^[0-9a-zA-Z-]{1,200}$/) && hash.slice(-1) != '-'){
/*in case there are still old links without the hyphen*/
setTimeout(function () { scroll(hash); }, 500);
}
});
/*open and scroll to slide on the same page*/
function scrollandopen(hash){
var slider = jQuery('#' + hash).parents('.nn_sliders.accordion').attr('id').slice(-1);
var node = 1;
jQuery('#set-nn_sliders-' + slider + ' > div').each(function(i){
if(jQuery(this).children('#' + hash).length){
node = i + 1;
return false;
}
});
loadnode(slider, node);
scroll(hash);
};
</script>

