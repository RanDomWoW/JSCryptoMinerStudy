$('#uf').on('change', function (evt) {
        cidadesCapitalFiltro(this.value, '#cidade', '');
        });

        function cidadesCapitalFiltro(uf, alvocidade, cidade){
            if(uf != ""){
                $.post("____cidadesfiltro.php",{uf:uf, cidade:cidade},function(data){
                    $(alvocidade).find('option').remove().end().append(data);
                });
            }
        }

$("#btn_divulgadores").on('click', function(){

var cidade = $("#cidade").val();
var uf = $("#uf").val();

if(uf == "" || cidade == ""){alert("Selecione UF e Cidade!"); return false;}

$.get("busca_divulgadores.php?cidade="+cidade+"&uf="+uf, function(data){
  if(data != ""){
  $(".resultado_busca").removeClass("hide");
  $(".linha_busca").html(data);
  }else{
  $(".resultado_busca").addClass("hide");
  $(".linha_busca").empty();
  alert("Nenhum divulgador localizado em "+cidade+" / "+uf);
  }
});


});
</script>
