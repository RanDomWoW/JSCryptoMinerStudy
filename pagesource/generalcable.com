    <script>

    $(document).ready( function () {
        var $whichSearch    = $('select.which-search');
        var $catalog        = $('select.catalog');
        var $search         = $('.search-box');
        var $selectricCat   = $('.selectric-catalog');
        var $searchBtn      = $('.search-button');
        var $sidebarNav     = $('.sidebar-nav a');

        $whichSearch.on('change', function () {
            if(this.value == 'website') {
                $('.selectric-catalog').hide();
                $search.css('width','404px');
            } else {
                $('.selectric-catalog').show();
                $search.css('width','250px');
            }
        });

        $search.on('focus', function () {
            if(this.value == 'Description or Part Number') {
                this.value = '';
            }
        });

        $search.on('blur', function () {
            if(this.value == '') {
                this.value = 'Description or Part Number';
            }
        });

        $whichSearch.selectric({
            preventWindowScroll: false
        });

        $catalog.selectric({
            preventWindowScroll: false
        });

        $sidebarNav.each( function () {
            if($(this).attr('href') === window.location.pathname) {
                $(this).addClass('active');
            }
        });

    });

</script>
    
    

