<noscript><div><img src="https://mc.yandex.ru/watch/22896043" style="position:absolute; left:-9999px;" alt="" /></div></noscript>
<!-- /Yandex.Metrika counter -->

<div class="modal-window relative" id="prodAppend" style="display: none;">
    <div class="workspace" style="width: 640px;">
        <ul class="modal__tabs alone">
            <li data-id="widgetLoginFrm" data-target="modal-tab" class="modal__tabs_tab_active alone"><span>Товар добавлен в корзину</span></li>
        </ul>
        <span class="modal__close"></span>
        <div class="clearfix"></div>
        <div class="modal__content modal__content_active"  style="padding-top: 0px;">
            <h3 style="text-align: center;" id="prod-name"></h3>
            <div class="row">
              <div class="col-xs-7">

              </div>
              <div class="col-xs-5">
                <p>Артикул <nobr><span id="prod-sku"></span></nobr></p>
                <p>Количество <nobr><span id="prod-value"></span> шт.</nobr></p>
                <p>Цена <nobr><span id="prod-price"></span>&nbsp;&#8381;</nobr></p>
                <p>Сумма: <nobr><span id="prod-total"></span>&nbsp;&#8381;</nobr><br /><i>Без учета доставки</i></p>
              </div>
            </div>
            <div class="row">
              <div class="col-xs-6">
                <a href="?route=checkout/cart" class="item__buy-together_to-cart">Перейтив корзину</a>
              </div>
              <div class="col-xs-6">
                <a href="#close" data-target="appendProductTestClose" class="item__buy-together_to-cart">Продолжить покупки</a>
              </div>
            </div>
        </div>
    </div>
</div>
<script>
$(document).ready(function(){
$('*[data-target="prod-append"]').click(function(){
    var prod_val = parseInt($(this).attr('data-value-ex') == "" ? ($(this).attr('data-value')):($($(this).attr('data-value-ex')).val()));
    addToCart($(this).attr('data-id'),prod_val);
    $('#prod-value').html(prod_val);
    $('#prod-sku').html($(this).attr('data-sku'));
    $('#prod-price').html($(this).attr('data-price'));
    $('#prod-total').html(parseFloat($(this).attr('data-price'))*prod_val);
    $('#prod-name').html($(this).attr('data-name'));
    $('#prod-img').attr('src',$(this).attr('data-img'));
    $('#prod-img').attr('alt',$(this).attr('data-name'));
    $('#prodAppend').toggle();
  });
  $('*[data-target="appendProductTestClose"]').click(function(){$('#prodAppend').toggle();});
    
});
</script>
</body>
