<script>
function Sleep(milliseconds) {
  var start = new Date().getTime();
  for (;;) {
    if ((new Date().getTime() - start) > milliseconds){
      break;
    }
  }
}
var slider = new function(){
    this.init = function(id){
        this.id = id;
        this.total = $(this.id+' a').length;
        this.current = 1;
		this.currentTimeout = 0;
    };
    this.setTimeout = function(time){
        this.timeout = time;
		this.currentTimeout = time;
    };
    this.goto = function(item) {
        if(item > this.total || item < 1){return;}
        $(this.id+' a').css({'display':'none'});
        $(this.id+' a:nth-child('+item+')').css({'display':'block'});
        $('#lideshownav li').removeClass('active');
        $('#lideshownav li[data-id="'+item+'"]').addClass('active');
		var ttl = parseInt($(this.id+' a:nth-child('+item+')').attr('data-timeout'));
		this.currentTimeout = ttl > 0 ? (ttl*1000):this.timeout;
        this.current = item;
    };
    this.next = function()
    {
        var item = (this.current+1 > this.total) ? 1:this.current+1;
        this.goto(item);
    };
    this.autoplay = function() {
		autoplaySlider();
		/*setInterval(function(){
			var ts = new Date().getTime();
            slider.next();
			Sleep(slider.currentTimeout);
        },500);*/
    };
};
function autoplaySlider()
{
	slider.next();
	setTimeout(autoplaySlider,slider.currentTimeout);
}
$(document).ready(function(){
        slider.init("#slideshow0");
        slider.setTimeout(500);
        slider.autoplay();
        $('#lideshownav li input').click(function(){
            slider.goto($(this).attr('data-id'));
        });
    });
</script>


