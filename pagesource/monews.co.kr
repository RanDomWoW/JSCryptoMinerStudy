	page_point_new_140 = page_num_new_140 - 1; // 현재페이지
	page_buffer_new_140 = page_num_new_140 - 1; // 현재페이지 임시저장

	$tg_new_140.mouseenter(function() {
		page_num_new_140 = $tg_new_140.index(this);
		page_cmd_new_140('current');
	});

	$body_new_140.eq(page_point_new_140).show();
	// 페이지 계산
	function page_cmd_new_140(cmd) {
		if(cmd == 'prev') { // 이전
			page_point_new_140 -= 1;
			if(page_point_new_140 < 0) page_point_new_140 = page_layer_count_new_140 - 1;
		}
		else if(cmd == 'next') { // 다음
			page_point_new_140 += 1;
			if(page_point_new_140 >= page_layer_count_new_140) page_point_new_140 = 0;
		}
		else if(cmd == 'current') { // 지정한 페이지
			page_point_new_140 = page_num_new_140		}

		$body_new_140.eq(page_buffer_new_140).stop().hide().end().eq(page_point_new_140).show();
		$tg_new_140.eq(page_buffer_new_140).removeClass('active').end().eq(page_point_new_140).addClass('active');
		page_buffer_new_140 = page_point_new_140;
		page_num_new_140 = page_point_new_140 + 1;
	}

	// 영역에 접근하면 자동전환 중지
	$(page_top_area_new_140).bind({
		  mouseenter: function() { // 마우스가 들어가면
			page_in_status_new_140 = false;
		  },
		  mouseleave: function() { // 마우스가 빠져나오면
			page_in_status_new_140 = true;
		  }
	});

	// 자동 바꿈
	if(page_auto_play_new_140) {
		page_auto_change_new_140 = window.setInterval(function () {
			if(page_in_status_new_140) {
				page_cmd_new_140('next');
			}
		},page_auto_time_new_140);
	}

})(jQuery);
</script>
