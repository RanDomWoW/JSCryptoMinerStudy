                var imgPreloadCount = 0;                
                
                function SpotlightController() {
                    this._spotlightImageDefinition = "div.spotlight div.spotlight_image img";
                    this._spotlightNavDefinition = "div.spotlight div.spotlight_nav img.spotlight_nav_image";

//                    $(this._spotlightImageDefinition).each(function(i, domelement) {
//                        if (typeof ($(domelement).attr("src")) != "undefined") {
//                            var img = new Image();
//                            img.src = $(domelement).attr("src");
//                            imgPreload[imgPreloadCount] = img;
//                            imgPreloadCount++;
//                        }
//                    });
                    $(this._spotlightNavDefinition).each(function(i, domelement) {
                        var img = new Image();
                        img.src = $(domelement).attr("spotlightNavOff");
                        imgPreload[imgPreloadCount] = img;
                        imgPreloadCount++;
                        img = new Image();
                        img.src = $(domelement).attr("spotlightNavOn");
                        imgPreload[imgPreloadCount] = img;
                        imgPreloadCount++;
                        img = new Image();
                        img.src = $(domelement).attr("spotlightImage");
                        imgPreload[imgPreloadCount] = img;
                        imgPreloadCount++;
                    });
                    
	                this._spotlightImageObject = $(this._spotlightImageDefinition).get(0);
	                
	                this._spotlightIndexOfficially = 0;
	                this._spotlightImageOfficially = this._spotlightImageObject.src;	                

                    
                    var rotationInterval = $(".rotationIntervalTime").val();                    
                    if(rotationInterval!='')
                        rotationInterval = rotationInterval*1000;
                    else
                        rotationInterval=8000;                        
                        
	                this._intervalValue = rotationInterval;
	                this._setI = null;
                }

                SpotlightController.prototype = {
                    clearAll: function() {
                        $(this._spotlightNavDefinition).map(
                            function() {
                                this.src = $(this).attr("spotlightNavOff");
                            }
                        );
                    },

                    hoverOn: function(navImage) {
                        if ($(navImage).attr("spotlightIndex") != this._spotlightIndexOfficially) {
                            navImage.src = $(navImage).attr("spotlightNavOn");                            
                        }
                    },

                    hoverOff: function(navImage) {
                        if ($(navImage).attr("spotlightIndex") != this._spotlightIndexOfficially) {
                            navImage.src = $(navImage).attr("spotlightNavOff"); 
                        }
                    },

                    temporarilyOn: function(navImage) {
                        if ($(navImage).attr("spotlightIndex") != this._spotlightIndexOfficially) {
                            navImage.src = $(navImage).attr("spotlightNavOn"); 
                            //this._spotlightImageObject.src = $(navImage).attr("spotlightImage");

                            $(this._spotlightImageObject).siblings().remove();
                            $(this._spotlightImageObject).closest("div.spotlight_image").css("height", $(this._spotlightImageObject).height() + "px");
                            $(this._spotlightImageObject).css("position", "absolute").css("z-index", "0").after("<img src='" + $(this._spotlightImageObject).attr('src') + "' style='z-index:1;position:absolute;' />");
                            $(this._spotlightImageObject).css("z-index", "2").css({opacity: 0.0}).attr('src', $(navImage).attr("spotlightImage")).stop().animate({opacity:'1'}, 1000, function(){$(this).siblings().remove();});
                        }
                    },

                    offFromTemporarilyOn: function(navImage) {
                        if ($(navImage).attr("spotlightIndex") != this._spotlightIndexOfficially) {
                            navImage.src = $(navImage).attr("spotlightNavOff"); 
                            //this._spotlightImageObject.src = this._spotlightImageOfficially;

                            $(this._spotlightImageObject).siblings().remove();
                            $(this._spotlightImageObject).closest("div.spotlight_image").css("height", $(this._spotlightImageObject).height() + "px");
                            $(this._spotlightImageObject).css("position", "absolute").css("z-index", "0").after("<img src='" + $(this._spotlightImageObject).attr('src') + "' style='z-index:1;position:absolute;' />");
                            $(this._spotlightImageObject).css("z-index", "2").css({opacity: 0.0}).attr('src', this._spotlightImageOfficially).stop().animate({opacity:'1'}, 1000, function(){$(this).siblings().remove();});
                        }
                    },

