<script type="text/javascript">
function showThenLoadedForOpera(o) {
try {
if (o.find(".flash").get(0).PercentLoaded() == 100)
o.find(".anPicImg").css("z-index", 10).end().find(".anPicFlash").css("z-index", 100);
else setTimeout(showThenLoadedForOpera, 100, o);
} catch (e) {
o.find(".anPicImg").css("z-index", 10).end().find(".anPicFlash").css("z-index", 100);
}
}

function showThenLoadedForSafariChrome(o) {
o.find(".flash").css("width", "240px").end().find(".anPicImg").css("z-index", 10).end().find(".anPicFlash").css("z-index", 100);
}

function showThenLoadedForFirefox(o) {
o.find(".anPicImg").css("z-index", 10).end().find(".anPicFlash").css("z-index", 100);
}

$(function () {
$('.pic-an').mouseenter(
function () {
var fl = $(this).find(".anPicFlash");
var gif = $(this).find(".anPicGif");
if (fl.length != 0) {
fl.html(getFlashCode(8, fl.attr("data-flash-url"), fl.attr("data-alt-maket-url"), fl.attr("data-alt-maket-link"), fl.attr("data-banner-width"), fl.attr("data-banner-height")));
if ($.browser.opera) {
setTimeout(showThenLoadedForOpera, 100, $(this));
} else if ($.browser.safari) {
fl.find(".flash").css("width", 0);
setTimeout(showThenLoadedForSafariChrome, 2500, $(this));
} else if ($.browser.mozilla) {
setTimeout(showThenLoadedForFirefox, 500, $(this));
} else {
$(this).find(".anPicImg").hide();
}
}
if (gif.length != 0) {
var link = gif.attr("data-link");
if (link != "") gif.html('<a href="' + link + '" target="_blank"><img src="' + gif.attr("data-url") + '"/></a>');
else gif.html('<img src="' + gif.attr("data-url") + '"/>');
$(this).find(".anPicImg").hide();
}
}).mouseleave(function () {
$(this).find(".anPicImg").css("z-index", 100).end().find(".anPicFlash").css("z-index", 10);
$(this).find(".anPicImg").show();
$(this).find(".anPicFlash").html("");
})
});
</script>
