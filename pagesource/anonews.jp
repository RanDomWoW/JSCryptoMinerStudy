        ar.css({'overflow':'hidden'});
        cp = $('#container').css('position');
        p='static';
  
        w=$(window);
        l=b.offset().left;
        lc=c.offset().left;
        t=ar.offset().top;
  
        s=function(){
            bh=b.outerHeight();
            ch=c.outerHeight();
            ah=ar.outerHeight();
            cb=c.offset().top+ch;
  
            wt=w.scrollTop();
            wl=w.scrollLeft();
            wh=w.height();
            nf=(wt>t)&&(ch>bh)?true:false;
            p=!nf?'static':(wt+ah)>cb?'absolute':'fixed';
  
