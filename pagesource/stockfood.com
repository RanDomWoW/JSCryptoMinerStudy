            var container = document.querySelector('.MobileMainMenu');
            var body = document.body;

            if (!open) {
                body.classList.add("MobileMainMenuOpen");
            } else {
                body.classList.remove("MobileMainMenuOpen");
            }

            open = !open;
            return false;
        }
    })();

