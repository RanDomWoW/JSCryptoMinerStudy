function enviarMensajeFacebook() {
var idRecomendacion = $.trim($('#idRecomendador').html());
window
.open('http://www.facebook.com/sharer.php?s=100&p[title]='
+ encodeURIComponent('Recomienda y Gana un descuento')
+ '&p[summary]='
+ encodeURIComponent('Hola, quiero recomendarte juguetilandia.com. Te envían de mi parte un descuento para tu próxima compra! Tienen envíos gratis y en 24-72 horas!')
+ '&p[url]='
+ encodeURIComponent('https://www.juguetilandia.com/recomendaciones/descuentoRecomendado.htm?idRecomendado='
+ encodeURIComponent(idRecomendacion))
+ '&p[images][0]='
+ encodeURIComponent('https://www.juguetilandia.com/images/juguetilandia.png'));
mostrarModalTwitter();
}
function enviarMensajeTwitter() {
var recomendacion = $.trim($('#nombre-recomendacion').val());
var idRecomendacion = $.trim($('#idRecomendador').html());
window
.open('http://twitter.com/intent/tweet?via=juguetilandia&amp;text='
+ encodeURIComponent(recomendacion)
+ '&amp;url='
+ encodeURIComponent('https://www.juguetilandia.com/recomendaciones/descuentoRecomendado.htm?idRecomendado='
+ encodeURIComponent(idRecomendacion)));
mostrarModalGooglePlus();
}
function enviarMensajeGooglePlus() {
var idRecomendacion = $.trim($('#idRecomendador').html());
window
.open('https://plus.google.com/share?url=https://www.juguetilandia.com'
+ encodeURIComponent('https://www.juguetilandia.com/recomendaciones/descuentoRecomendado.htm?idRecomendado='
+ encodeURIComponent(idRecomendacion)));
mostrarModalCorreo();
}
function enviarMensajeCorreo() {
enviarMailRecomendacion();
}
function enviarMailRecomendacion() {
var emailDestino = $.trim($('#email-recomendacion').val());
var mensaje = $('#mensaje-recomendacion').val();
var nombre = $.trim($('#nombreRecomendado').val());
var producto = $.trim($('#articuloIdRecomendacion').val());
var data;
mensaje = encodeURIComponent(mensaje);
$.ajaxSetup({
scriptCharset : "utf-8",
contentType : "application/json; charset=utf-8"
});
$
.ajax({
url : "recomendaciones/enviarMailRecomendacion.htm?email="
+ emailDestino + "&mensaje=" + mensaje
+ "&producto=" + producto,
data : "nocache=" + Math.random(),
type : "POST",
dataType : "json",
/* scriptCharset: "utf-8",
contentType: "application/json; charset=UTF-8", */
success : function(data) {
//var obj = JSON.parse(data);
$("#error-dialog-recomendacion-email").empty();
for (var i = 0; i < data.length; i++) {
if (data[i]['respuesta'] == "okLogin") {
$("#error-dialog-recomendacion-email")
.removeClass("errorCode").addClass(
"okCode");
$("#error-dialog-recomendacion-email")
.append(
"<div class='alert alert-success'><div class='glyphicon glyphicon-ok-sign'></div><strong> Recomendación enviada correctamente</strong></div>");
mostrarModalFacebook();
} else if (data[i]['respuesta'] == "errorClienteExiste") {
$("#error-dialog-recomendacion-email")
.removeClass("okCode").addClass(
"errorCode");
if (data[i]['social'] == "no") {
$("#error-dialog-recomendacion-email")
.append(
"<div class='alert alert-danger'><div class='glyphicon glyphicon-info-sign'></div><b> "+data[i]['email']+" </b>ya ha sido recomendado por otro usuario. ¡Inténtalo con otro amigo!</div>");
} else {
$("#error-dialog-recomendacion-email")
.append(
"<div class='alert alert-danger'><div class='glyphicon glyphicon-info-sign'></div><strong> Oh vaya</strong>, ya has sido recomendado. Esta promoción solo es válida una vez por usuario.</div>");
}
} else if (data[i]['respuesta'] == "errorMismoCliente") {
$("#error-dialog-recomendacion-email")
.removeClass("okCode").addClass(
"errorCode");
$("#error-dialog-recomendacion-email")
.append(
"<div class='alert alert-danger'><div class='glyphicon glyphicon-info-sign'></div><strong> Error</strong>, los correos no pueden coincidir</div>");
} else {
$("#error-dialog-recomendacion-email")
.removeClass("okCode").addClass(
"errorCode");
$("#error-dialog-recomendacion-email")
.append(
"<div class='alert alert-danger'><div class='glyphicon glyphicon-info-sign'></div><strong> Error enviando la recomendación</strong>. Inténtelo de nuevo más tarde y si el problema persiste, póngase en contacto con el administrador.</div>");
}
}
},
error : function(dato) {
$("#error-dialog-recomendacion-email").removeClass(
"okCode").addClass("errorCode");
$("#error-dialog-recomendacion-email")
.append(
"<div class='alert alert-danger'><div class='glyphicon glyphicon-info-sign'></div><strong> Error enviando la recomendación</strong>. Inténtelo de nuevo más tarde y si el problema persiste, póngase en contacto con el administrador.</div>");
}
});
event.preventDefault();
javaScriptEscape = "true";
}
function enviarLoginMostrarRecomendaciones() {
var emailLogin = $.trim($('#emailLogin').val());
var password = $.trim($('#passwordLogin').val());
var data;
$
.ajax({
url : "cliente/login.htm",
data : "email=" + emailLogin + "&pass=" + password,
type : "POST",
dataType : "json",
/* contentType: false,
processData: false, */
success : function(source) {
data = source;
if (data['respuesta'] == "okLogin") {
$("#formularioLogin").empty().append(
"Login Ok, redirigiendo...");
$('#modal-login-general').modal('hide');
$('#dialog-recomendacion').modal('show');
$(".enlaceSuperiorLogin")
.empty()
.append(
"<a href='/login.htm'>Mi Cuenta</a> <a href='/logout.htm' style='color:#333333;'>[Desconectar]</a>");
$("#modalReco").attr("data-target",
"#dialog-recomendacion");
$("#idRecomendador").empty().append(data['id']);
} else if (data['respuesta'] == "koLogin") {
$("#errorLoginFormularioLogin")
.empty()
.append(
"<div class='alert alert-danger'><div class='glyphicon glyphicon-info-sign'></div><strong> Error!</strong> email y/o contraseña incorrectos.</div>");
}
else {
$("#errorLoginFormularioLogin")
.empty()
.append(
"<div class='alert alert-danger'><div class='glyphicon glyphicon-info-sign'></div><strong> Ha ocurrido un error</strong>. Inténtalo de nuevo pasados unos minutos.</div>");
}
},
error : function(dato) {
$(".errorLogin")
.empty()
.append(
"<div class='alert alert-danger'><div class='glyphicon glyphicon-info-sign'></div><strong> Ha ocurrido un error</strong>. Inténtalo de nuevo pasados unos minutos.</div>");
}
});
event.preventDefault();
javaScriptEscape = "true";
}
