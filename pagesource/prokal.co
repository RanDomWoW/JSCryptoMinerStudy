	    if(scrolltop >= 215) {
	      $('#fixedbar').fadeIn(250);
	    }
	    
	    else if(scrolltop <= 210) {
	      $('#fixedbar').fadeOut(250);
	    }
	});
    
    $('.pgwSlider').pgwSlider({
        adaptiveHeight:true,
        intervalDuration : 5000,
        maxHeight: 340
    });

    $(".feature").owlCarousel({
        items:4,
        loop:true,
        autoplay:true
    });

    $('.newsticker').newsTicker({
        row_height: 40,
        max_rows: 1,
        duration: 5000
    });
    
    //$("#newsupdate").load("http://www.prokal.co/home/newsmore.html");
   	$("#newsupdate").niceScroll({autohidemode: true});
   	$("#loadnewsupdate").hide();
   	$("#scrollnewsupdate").show();
    //$.fancybox("#banner");
	$("#banner").fancybox({
		fitToView: false,
		closeClick: true
	});
	
	function myclock(){
		var today = new Date();
        var h = today.getHours();
        var m = today.getMinutes();
        var s = today.getSeconds();
        
        if(s<10){s = "0"+s;}
        if(m<10){m = "0"+m;}
        if(h<10){h = "0"+h;}

        $(".clock").text(h+"."+m+"."+s);
        setTimeout(function(){myclock()}, 500);
	}
	myclock();
	
	/*
	$("#banner").trigger('click');
	$(".fancybox-wrap").css('margin-top', '30px');
	*/
});
</script>

