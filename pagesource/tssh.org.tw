<script>
function CheckLogin(){
 	
	var pass = true;
	while (true){

		if(document.form1.MAccName.value == ""){
			pass = false;
			alert("請輸入帳號。");
			document.form1.MAccName.focus();
			break;
		}
		if(document.form1.MPwd.value == ""){
			pass = false;
			alert("請輸入密碼。");
			document.form1.MPwd.focus();
			break;
		} 
		break;
	}
	if(pass){
		form1.submit();
	}	
}
