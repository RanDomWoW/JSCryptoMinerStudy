<script type="text/javascript" src="/_ui/shared/js/generatedVariables.js" defer></script><script type="text/javascript" src="/_ui/responsive/common/js/build/combined.min.js?_=1521104718200" defer></script>
	<script type="text/javascript">
defer(function() {
    if (typeof R3_COMMON !== "undefined" && typeof rr_flush_onload !== "undefined") {
        rr_flush_onload();
        r3();
    }
});
</script><script id="rr-default-template" type="text/template">
	{{#.}}
	<div class="product-tease has-promotion product-tease--small product-tease--vertical">
		<div class="product-tease__wrapper equal-height__block">

			
				{{#xif "this.product.hasDiscountForLoggedInUser"}}
					{{#variable "product"}}
						{"promotionText": "LOGG INN FOR KUNDEKLUBBPRIS"}
					{{/variable}}
					{{#variable "product"}}
						{"promotionClass": "sticker--orange"}
					{{/variable}}
				{{/xif}}
			
			{{#xif "this.product.promotionText"}}
				<div class="product-tease__stickers">
					<span class="sticker sticker--arrow-bottom {{product.promotionClass}}">{{product.promotionText}}</span>
				</div>
			{{/xif}}
			<figure class="product-tease__figure">
				{{#xif "this.product.discountPct > 0"}}
					<div class="promotion promotion--percent">
						<span class="promotion__text"><div class="sales-custom">-{{x "Math.round(this.product.discountPct)"}}%</div></span>
					</div>
				{{/xif}}
				<a href="{{product.url}}" data-product-id="{{product.code}}" class="js-reference-item" title="{{product.cardTitle}}" >
					{{#product.images}}
						{{#xif "this.imageType === 'PRIMARY' && this.format === 'thumbnail' " }}
						<img src="{{url}}" alt="{{../../name}}" title="{{../../name}}">
						{{else}}
						{{/xif}}
					{{/product.images}}
				</a>
			</figure>
			<div class="product-tease__body">
				<div class="rating" style="height:25px;">
					<span class="lipscore-rating-small"
						  ls-product-id="{{product.variantId}}"
					>
					</span>
				</div>
				<div class="product-tease__header">
					<a href="{{product.url}}" title="{{product.cardTitle}}">
						<h5 class="product-tease__title tag tag--medium">{{product.secondaryTitle}}</h5>
						<h3 class="product-tease__sales-title h6 text-uppercase">{{product.cardTitle}}</h3>
					</a>
				</div>

				<div class="product-tease__price">
					<span class="price price--medium">
						{{#xif "this.product.campaignPrice && this.product.campaignPrice.formattedValue"}}
							<div class="product-price">
								<span class="price__amount">{{{product.campaignPrice.formattedValueHtml}}}</span>
								<span class="price__old">Før: {{{product.price.formattedValueHtml}}}</span>
							</div>
						{{else}} <div class="price__amount">{{{product.price.formattedValueHtml}}}</div>
						{{/xif}}
					</span>
				</div>
			</div>
			<div class="product-tease__status">
				<ul class="status-list status-list--small">
					{{#xif "this.product.stock.stockLevelStatus.code != 'outOfStock'"}}
					<li class="inStock">
						<span class="status-list__icon simpleicon-checked"></span>
						<span class="status-list__text">På nettlager  [{{x "this.product.stock.stockLevel >= 50 ? '50+' : this.product.stock.stockLevel"}}]</span>
					</li>
					{{else}}
					<li class="outOfStock">
						<span class="status-list__icon simpleicon-cross"></span>
						<span class="status-list__text">Ikke på nettlager</span>
					</li>
					{{/xif}}
					{{#xif "this.product.stock.availableInStores > 0"}}
					<li class="inStock">
						<span class="status-list__icon simpleicon-checked"></span>
						<span class="status-list__text">Klikk & hent</span>
					</li>
					{{else}}
					<li class="outOfStock">
						<span class="status-list__icon simpleicon-cross"></span>
						<span class="status-list__text">Ikke tilgjengelig i butikk</span>
					</li>
					{{/xif}}
				</ul>
			</div>
		</div>
	</div>
	{{/.}}
</script></body>

</html>