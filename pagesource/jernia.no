<script type="text/javascript">
    dataLayer = [];
    var GA = (function() {
        self = this,
        wishlistCreated = function() {
            dataLayer.push({
                'event': 'wishlistCreated'
            });
        },
        wishlistShared = function() {
            dataLayer.push({
                'event': 'wishlistShared'
            });
        },
        pickupInStore = function(orderData, store){
            var layerData = {
                "event" :"orderCompleted",
                'ecommerce': {
                    'purchase': {
                        'actionField': {
                            'id': orderData.orderCode,
                            'affiliation': '',
                            'revenue': orderData.totalPrice,
                            'tax':orderData.totalTax,
                            'shipping': '',
                            'coupon': ''
                        },
                        'products': [
                            {
                                'name': orderData.productName,
                                'id': orderData.productCode,
                                'price': orderData.productPrice,
                                'brand': '',
                                'category':'',
                                'variant': '',
                                'quantity': orderData.quantity,
                                'coupon': '',
                                'dimension1': orderData.deliveryMode,
                                'dimension4': store.storeName
                            }
                        ]
                    }
                }
            };
            dataLayer.push(layerData);
        },
        addToCart = function(productCode, quantity){
            dataLayer.push({
                'event': 'addToCart',
                'ecommerce': {
                    'currencyCode': '',
                    'add': {
                        'products': [{
                            'name': '',
                            'id': productCode,
                            'price': '',
                            'brand': '',
                            'category': '',
                            'variant': '',
                            'quantity': quantity
                        }]
                    }
                }
            });
        },
        removeFromCart = function(productCode, quantity){
            dataLayer.push({
                'event': 'removeFromCart',
                'ecommerce': {
                    'remove': {
                        'products': [{
                            'name': '',
                            'id': productCode,
                            'price': '',
                            'brand': '',
                            'category': '',
                            'variant': '',
                            'quantity': quantity
                        }]
                    }
                }
            });
        },
        checkAndRegisterTransaction = function(tranId, fn) {
            if(!self.checkCookieExists(tranId)){
                self.writeCookie(tranId);
                fn();
            }
        },
        writeCookie = function(tranId){
            var d = new Date();
            var currentTime = Math.round(d.getTime() / 1000);
            d.setTime(d.getTime()+(365*24*60*60*1000));
            document.cookie = self.cookieName(tranId) +"=" + currentTime + "; expires=" + d.toGMTString()+"; path=/; secure;";
        },
        checkCookieExists = function(tranId){
            var r = new RegExp("^(.*;)?\\s*"+self.cookieName(tranId)+"\\s*=\\s*[^;]+(.*)?$");
            return (document.cookie.match(r) || []).length > 0
        },
        cookieName = function (tranId) {
            return "TID_" + tranId;
        }

        return {
            wishlistCreated : wishlistCreated,
            addToCart : addToCart,
            removeFromCart : removeFromCart,
            wishlistShared : wishlistShared,
            check : checkAndRegisterTransaction,
            pickupInStore: pickupInStore
        }
    })();
</script>
