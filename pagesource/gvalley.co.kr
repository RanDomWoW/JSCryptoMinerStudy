		$SecName_new_6 = $('.name_new_6') // 섹션명
		page_top_area_new_6 = ".wrap_new_6"; // 전체 영역
		page_num_new_6 = 1; // 현재 페이지
		page_auto_play_new_6 = true; // 자동전환
		page_auto_time_new_6 = 5000; // 자동전환 시간
		page_layer_count_new_6 = $body_new_6.length; // 총 컨텐츠
		page_in_status_new_6 = true; // 마우스 접근 여부
		page_point_new_6 = page_num_new_6 - 1; // 현재페이지
		page_buffer_new_6 = page_num_new_6 - 1; // 현재페이지 임시저장

		$body_new_6.eq(page_point_new_6).show();
		$SecName_new_6.eq(page_point_new_6).show();

		// 클릭시 선택한 영역으로 변경
		$ClickBtn_new_6.click(function() {
			var index = $ClickBtn_new_6.index(this);
			page_num_new_6 = index + 1;
			page_cmd_new_6('current');
		});

		// 좌우 화살표 클릭시
		$tg_new_6.click(function() {
			page_num_new_6 = $tg_new_6.index(this) + 1;
			$cmd_new_6 = $(this).attr('cmd');
			page_cmd_new_6($cmd_new_6);
		});

		// 페이지 계산
		function page_cmd_new_6(cmd) {
			if(cmd == 'prev') { // 이전
				page_point_new_6 -= 1;
				if(page_point_new_6 < 0) page_point_new_6 = page_layer_count_new_6 - 1;
			}
			else if(cmd == 'next') { // 다음
				page_point_new_6 += 1;
				if(page_point_new_6 >= page_layer_count_new_6) page_point_new_6 = 0;
			}
			else if(cmd == 'current') { // 지정한 페이지
				page_point_new_6 = page_num_new_6 - 1;
			}
			// 선택한 내용으로 변경
			$body_new_6.eq(page_buffer_new_6).stop().hide().end().eq(page_point_new_6).show();
			$SecName_new_6.eq(page_buffer_new_6).stop().hide().end().eq(page_point_new_6).show();

			// 하단메뉴 제어
			$ClickBtn_new_6.eq(page_buffer_new_6).removeClass('active').end().eq(page_point_new_6).addClass('active');

			page_buffer_new_6 = page_point_new_6;
			page_num_new_6 = page_point_new_6 + 1;

		}

		// 영역에 접근하면 자동전환 중지
		$(page_top_area_new_6).bind({
			  mouseenter: function() { // 마우스가 들어가면
				page_in_status_new_6 = false;
			  },
			  mouseleave: function() { // 마우스가 빠져나오면
				page_in_status_new_6 = true;
			  }
		});

		// 자동 바꿈
		if(page_auto_play_new_6) {
			page_auto_change_new_6 = window.setInterval(function () {
				if(page_in_status_new_6) {
					page_cmd_new_6('next');
				}
			},page_auto_time_new_6);
		}

		page_cmd_new_6('current');

	});
})(jQuery);

</script>

