        var sliderWidth = container.width();
        var screenWidth = $(window).width();
        var itemsInSlide = 5;
        var slidesHtml = '';
        switch (true) {
            case sliderWidth < 370:
                itemsInSlide = 4;
                break;
            case sliderWidth >= 370 && screenWidth <= 768:
                itemsInSlide = 3;
                break;
        }
        
        var ii = 0;
        $(posts).each(function(){
            if (!(ii % itemsInSlide) ) slidesHtml += '<div class="item">';
            slidesHtml += template(this);
            if (!(++ii % itemsInSlide) || ii === posts.length) slidesHtml += '</div>';
        });
        
        container.html(slidesHtml);
        container.owlCarousel({
            items: 1, //10 items above 1000px browser width
            itemsDesktop: [1200, 2], //5 items between 1024px and 901px
            itemsDesktopSmall: [900, 2], // 3 items betweem 900px and 601px
            itemsTablet: [768, 1], //2 items between 600 and 0;
            itemsMobile: [320, 1],
            navigation: true,
            navigationText: ["<a class=\"flex-prev\"></a>", "<a class=\"flex-next\"></a>"],
            slideSpeed: 500,
            pagination: false
        });

        resizeManager.init();
        $(window).resize(function(){
            resizeManager.init();
        });
