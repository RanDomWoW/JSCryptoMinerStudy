<script type="text/javascript">

    var $ = jQuery.noConflict();
    
    $(function(){
    	var mostrar = "Mostrar texto del video";
    	var ocultar = "Ocultar texto del video";
    	$(".skl-transcript-video").find("span:first").html(mostrar);
    	$(".skl-transcript-video>div").hide();
    	$(".skl-transcript-video>span").click(function(){
    		
    		if($(this).parent().find("div").is(":visible") ){
    			$(this).parent().find("div").slideUp(function(){
    				$(this).siblings("span").html(mostrar);
    				$(this).siblings("span:last").hide();
    			});
    		} else {
    			$(this).siblings("span:last").show();
    			$(this).parent().find("div").slideDown(function(){
    				$(this).siblings("span").html(ocultar);
    			});
    		}

    	});
    });

</script>

