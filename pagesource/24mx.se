    <!-- Mobile filtering -->
<div data-ng-controller="mobileFilteringCtrl" data-ng-if="$productlist.vars.showMobileFiltering && showMobileMenu">
  <div class="mobile-filtering-container">
    <div class="mobile-filtering-graphic" ng-if="$backend.context.action !== 'result'">
      <div class="filtering-match-text">Antal produkter som matchar ditt filter</div>
      <div class="filter-graphic-center">
        <div class="filter-graphic-container">
          <div class="filter-graphic" data-progress="{{ $productlist.getFilterGraphPercentage() }}">
            <div class="filter-graphic-circle">
              <sb-circle-graph graph-size="70"></sb-circle-graph>
            </div>
            <div class="filter-graphic-center">
              <div class="filter-graphic-filtered">
                <span data-ng-bind="$productlist.vars.info.filteredCount"></span>
              </div>
              <div class="filter-graphic-total">
                /<span data-ng-bind="$productlist.vars.info.count"></span>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="mobile-filtering-content">
      <div ng-if="!showWintersaleCampaign">
        <h2>Filter</h2>
        <div class="filter-column filter-column-standard">
          <div class="filterMenu" data-ng-if="$productlist.vars.productList.brands.length > 0">
              <div data-ng-click="toggleBrands = !toggleBrands">
                <i class="fa fa-check-circle" aria-hidden="true" data-ng-class="{'fa-check-active': $productlist.vars.parameters.brands.length > 0}"></i>
                <p data-ng-class="{'filter-cat-active': $productlist.vars.parameters.brands.length > 0}">Varum&#228;rken</p>
                <i class="fa" data-ng-class="{'fa-plus-circle': !toggleBrands, 'fa-minus-circle': toggleBrands}"></i>
              </div>
              <ul class="filter-list" sb-slide-toggle="toggleBrands">
                <li class="filter-list-item brand-filter" data-ng-class="{'highlight': brand.state.checked, 'inactive': brand.state.inactive}" data-ng-click="$productlist.brandFilter(brand)" data-ng-repeat="brand in $productlist.vars.productList.brands track by brand.id">
                  <span data-ng-bind="brand.name"></span>
                </li>
              </ul>
          </div>
          <div class="filterMenu" data-ng-if="$productlist.vars.productList.categories && $productlist.vars.productList.categories.length > 0 && !$productlist.vars.parameters.prefilterCategory">
              <div data-ng-click="toggleCategories = !toggleCategories">
                <i class="fa fa-check-circle" aria-hidden="true" data-ng-class="{'fa-check-active': $productlist.vars.parameters.categories.length > 0}"></i>
                <p data-ng-class="{'filter-cat-active': $productlist.vars.parameters.categories.length > 0}">Kategori</p>
                <i class="fa" data-ng-class="{'fa-plus-circle': !toggleCategories, 'fa-minus-circle': toggleCategories}"></i>
              </div>
              <ul class="filter-list" sb-slide-toggle="toggleCategories">
                <li class="filter-list-item category-filter" data-ng-class="{'highlight': category.state.checked, 'inactive': category.state.inactive}" data-ng-click="$productlist.categoryFilter(category)" data-ng-repeat="category in $productlist.vars.productList.categories">
                  <span data-ng-bind="category.title"></span>
                </li>
              </ul>
          </div>
          <div class="filterMenu" data-ng-repeat="filterSet in $productlist.vars.productList.filters" data-ng-show="$productlist.vars.productList.filters">
              <div data-ng-click="toggleCustom[$index] = !toggleCustom[$index]">
                  <i class="fa fa-check-circle" aria-hidden="true" data-ng-class="{'fa-check-active': filterSetCount(filterSet) > 0}"></i>
                  <p data-ng-class="{'filter-cat-active': filterSetCount(filterSet) > 0}">
                      <span data-ng-bind="filterSet.title"></span>
                  </p>
                  <i class="fa" data-ng-class="{'fa-plus-circle': !toggleCustom[$index], 'fa-minus-circle': toggleCustom[$index]}"></i>
              </div>
              <ul class="filter-list" sb-slide-toggle="toggleCustom[$index]" data-filter-setid="filterSet.group" data-ng-show="filterSet.values">
                  <li class="filter-list-item custom-filter" data-ng-class="{'highlight': filter.state.checked, 'inactive': filter.state.inactive}" data-ng-click="$productlist.customFilter(filter, filterSet)" data-ng-repeat="filter in filterSet.values">
                      <span data-ng-bind="filter.value"></span>
                  </li>
              </ul>
          </div>
          <div class="filterMenu" data-ng-hide="$productlist.vars.type === 'campaign' || ($productlist.vars.type == 'outlet' && !poweruser) || $productlist.vars.type === 'search' || !$productlist.vars.info.hasSale">
              <div data-ng-click="toggleShowOnly = !toggleShowOnly">
                  <i class="fa fa-check-circle" aria-hidden="true" data-ng-class="{'fa-check-active': $productlist.vars.parameters.priceFlag === 'CAMPAIGN' || $productlist.vars.parameters.store}"></i>
                  <p data-ng-class="{'filter-cat-active': $productlist.vars.parameters.priceFlag === 'CAMPAIGN' || $productlist.vars.parameters.store}">Visa endast</p>
                  <i class="fa" data-ng-class="{'fa-plus-circle': !toggleShowOnly, 'fa-minus-circle': toggleShowOnly}"></i>
              </div>
              <ul class="filter-list" sb-slide-toggle="toggleShowOnly" data-ng-show="$productlist.vars.info.hasSale">
                  <li class="filter-list-item show-only-filter" data-ng-class="{'highlight': $productlist.vars.parameters.priceFlag === 'CAMPAIGN', 'inactive': !$productlist.vars.info.filteredHasSale}" data-ng-click="$productlist.saleOnlyFilter()">
                    <span>Nedsatta produkter</span>
                  </li>
              </ul>
          </div>


          <!-- Loop search facets -->
          <div class="filterMenu filterMenu-loopsearchfacets" data-ng-repeat="facet in $searchLoop.data.facets" data-ng-show="$backend.context.controller === 'search' && $backend.context.action === 'result'" ng-if="facet.name !== 'Category_2'">
              <div data-ng-click="toggleDrawer(facet.name)">
                  <i class="fa fa-check-circle" aria-hidden="true" data-ng-class="{'fa-check-active': facet.active === true}"></i>
                  <p data-ng-class="{'filter-cat-active': facet.active === true}">
                      <span data-ng-bind="facet.label"></span>
                  </p>
