                    speed: 300,
                    maxwidth: 540
                });

                // Slideshow 3
                $("#slider3").responsiveSlides({
                    manualControls: '#slider3-pager',
                    maxwidth: 540
                });

                // Slideshow 4
                $("#slider4").responsiveSlides({
                    auto: false,
                    pager: false,
                    nav: true,
                    speed: 500,
                    namespace: "callbacks",
                    before: function () {
                        $('.events').append("<li>before event fired.</li>");
                    },
                    after: function () {
                        $('.events').append("<li>after event fired.</li>");
                    }
                });                
            });

            $(document).ready(function(){
                var vars = getUrlVars();
                if (vars != null && vars.length > 0)
                {
                    if (vars["downloadpopup"] == 'true')
                    {
                        launchGame();
                    }
                }
            });

            function getUrlVars() {
                var vars = [], hash;
                var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
                for (var i = 0; i < hashes.length; i++) {
                    hash = hashes[i].split('=');
                    vars.push(hash[0]);
                    vars[hash[0]] = hash[1];
                }
                return vars;
            }

            function launchGame() {                
                openDialog($('#gameLaunchOptions'));
                hidePlayInBrowserButton();
                addGoogleTags();
            }

            function addGoogleTags()
            {
                var GTMScriptLocation = "https://media.jumpstart.com/SODWebsite/Script/Js/GTM_SOD.js";
                $.getScript(GTMScriptLocation, function () { var dummyVal = 1;});
            }

            function hidePlayInBrowserButton() {
                $('#p_playSODInBrowser').hide();
                /*
                var is_chrome = navigator.userAgent.toLowerCase().indexOf('chrome') > -1;
                var is_edge = navigator.userAgent.toLowerCase().indexOf('edge/12.0') > -1;
                var is_firefox = navigator.userAgent.toLowerCase().indexOf('firefox') > -1;
                if (is_chrome || is_edge || is_firefox) { $('#p_playSODInBrowser').hide(); }
                */
            }

            function DownloadGameByOS() {
                DownloadStandaloneGame('SOD');
                $('.close').click();
            }

            function PlayGameInBrowser() {
                location.href = '/Game';
            }

            var currentOverlay;
            function openDialog(p, onload, onclose) {
                var params = {
                    mask: {
                        color: '#fff',
                        loadSpeed: 10,
                        opacity: 0.8
                    },
                    closeOnClick: false,
                    closeOnEsc: false,
                    fixed: false,
                    api: true
                }
                if (onload)
                    params.onLoad = onload;
                if (onclose)
                    params.onClose = onclose;
                currentOverlay = p.overlay(params);
                currentOverlay.load();                
                p.css('display', 'block');
                $(".overlay").overlay({ top: "center" });
            }
    </script> 
