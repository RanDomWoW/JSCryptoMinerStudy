<script src="/common/js/jquery.sliderPro.min.js"></script>
<script>
  $( document ).ready(function( $ ) {
    var slider = $( '#slider' );
    slider.sliderPro({
      width: 1360,
      height: 522,
      visibleSize: '100%',
      slideDistance: 60,
      forceSize: 'fullWidth',
      arrows: false,
      fadeArrows: false,
      autoplay: true,
      autoplayDelay: 3000,
      keyboard: false,
      smallSize: 768,
      breakpoints: {
        767: {
          aspectRatio: 1.6,
          slideDistance: 8,
        },
      }
    });
    var nextBtn = $('#slider-control .next-btn');
    var prevBtn = $('#slider-control .prev-btn');
    $( nextBtn ).on( 'click', function() {
      slider.sliderPro( 'nextSlide' );
    } );
    $( prevBtn ).on( 'click', function() {
      slider.sliderPro( 'previousSlide' );
    } );
    var sliderIndex = $('.sp-slide');
    $ ( sliderIndex ).each(function(){
      $(this).focusin(function(){
        slider.sliderPro( 'gotoSlide', $(this).data('index') );
        slider.sliderPro( 'autoplay', false );
      }).focusout(function(){
        slider.sliderPro( 'autoplay', true );
      });
    });
    $('#slider-pbutton' ).on( 'click', function(){
      if( $(this).hasClass('pause') ){
        slider.sliderPro( 'autoplay', true );
        $(this).removeClass('pause');
      } else {
        slider.sliderPro( 'autoplay', false );
        $(this).addClass('pause');
      }
    });
  });
</script>
