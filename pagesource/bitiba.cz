<script type="text/html" id="productreco-template-variants-checkout">
    <ul id="${$data.scheme}-list">
        {{each items}}
        <li class="product-box">

            <header>
                <a href="${$data.languagePrefix}${$value.detailURL}" name="${$data.scheme}-product-${$value.asid}" title="${$value.name}">
                    <img src="${constructFullPictureURL($value.picturePath, 130)}" alt="${$value.name}">
                </a>
            </header>

            <div class="body">
                <meta content="" itemprop="itemreviewed">
                <meta content="5" itemprop="rating">
                <a class="product-box__price-feedbackurl" href="${$data.languagePrefix}${$value.feedbackUrl}">
                    <img alt="${$value.name}" src="${mediaServerDomainUrl}/image/rating/rating-${$value.ratingValue}.png">
                </a>
                <h5>
                    <a href="${$data.languagePrefix}${$value.detailURL}" name="${$data.scheme}-product-${$value.asid}" title="${$value.name}">
                        ${$value.shortName}
                    </a>
                </h5>

                <p>${$value.variantName}</p>
            </div>

            <footer class="checkout">
                <span itemprop="price" class="smalltextPrices">${$value.standardPriceText}</span>
                <br/>

                <span class="${$value.styleSalePrice}">
                    ${$value.reducedPriceLabel} <span class="val" itemprop="price">${$value.reducedPrice} </span>
                    <br/>
                    <span class="smalltext smalltextPrices">${$value.pricePerUnitPriceText}</span>
                </span>

                <form action="/checkout/lastMinuteOffer.htm" name="add${$value.asid}" method="POST" novalidate="">
                    <input type="hidden" value="${$value.asid}" name="articleId">
                    <button type="submit" class="checkout__reco" name="${$data.scheme}-buy_button-${$value.asid}">
                        <span>${$data.addToCart}</span>
                    </button>
                </form>
            </footer>

        </li>
        {{/each}}
    </ul>
</script>


