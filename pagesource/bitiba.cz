<script>
    var rawData = '';
    try {
        var refererURL = document.referrer;
        var originalURL = document.URL;
        window.dynamicDataDeferred = $.Deferred();
        $.ajax({
            type: "GET",
            url: "/web/cart/dynamicData.htm",
            data: {
                pgId: $('meta[name="application-name"]').data('pg-id')
            },
            dataType: "json",
            async: false,
            headers: {
                "seo-referer": refererURL,
                "original-url": originalURL
            },
            success: function (cart) {
                rawData = cart;
                window.dynamicDataDeferred.resolve(cart);
            }
        });

    }
    catch (e) {
        // push exception to Application
        log(e);
        // bind ajax request of cart on "dom_loaded" event
