<script type="text/javascript">
	function popup_slider(sra_src){
		var i=0;
		var img = {};
		var all_images = [];
		$('img[src="'+sra_src+'"]').parent().parent().find(".post_in_image").each(function(){
			if(sra_src==$(this).attr("src")){
				img = {src:$(this).attr("src"),position:i};
			}
			all_images[i] = {src:$(this).attr("src"),position:i};
			i++;
		})
		//console.log(all_images);
		var html = '';
		if(img.position!=0){
			var prev_index = img.position-1;
			html += '<a class="prev_img" onclick="popup_slider(\''+all_images[prev_index].src+'\')"></a>';
		}
		html += '<img src="'+img.src+'" class="post_img">';
		if(img.position!=i-1){
			var next_index = img.position+1;
			html += '<a class="next_img" onclick="popup_slider(\''+all_images[next_index].src+'\')"></a>';
		}
		$(".popup_content").html(html);
		$(".overlay").show();
		$(".popup").show();
	}
