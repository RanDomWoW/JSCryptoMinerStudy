margin:0 0 0 0;
padding:0 0 0 0;
}
.sidebar li {
margin:0 0 0 0;
padding:3px 0;
border-bottom:1px dotted #ddd
}
/* label */
.label-size-1,.label-size-2,.label-size-3,.label-size-4,.label-size-5 {
