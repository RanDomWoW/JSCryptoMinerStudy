width:160px;
margin:12px 0;
}
.sidebar-container {
margin:0 0;
}
.sidebar h2 {
font:bold normal 11px Arial, sans-serif;
background:#117AC9;
color:#ffffff;
margin:0 0 10px 0;
padding:4px 8px;
text-transform:uppercase;
border-top: 1px solid #3690D4;
border-bottom:1px solid #0F6DB3;
}
.sidebar .widget {
padding:0 0 10px;
}
.sidebar ul, .sidebar ol {
