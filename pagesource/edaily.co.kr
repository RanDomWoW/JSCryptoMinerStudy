	/* 순차로딩 */
	
	var fn_component_main_success_interval;
	var count = 0;
	var fn_component_main_success_end = function(){
		$('.main_layout_component').each(function(i){
			var self = $(this);
			var val;
			var id = self.attr('id');
			try { val = eval(id); } catch (e) {}
			if(typeof val == 'string'){
                try {
                    var html_str = '<div>' + val + '</div>';
                    $("#" + id).html($(html_str).html()).load(fn_component_main_success_callback).load();
                } catch (e) {
                }
			}
		});		
		// 광고 스크립트 추가
		var script1 = document.createElement("script");
		script1.type = "text/javascript";
		script1.src = "/resources/js/adDfp.js?ver=1.4";	
		document.getElementsByTagName("head")[0].appendChild(script1);
	}
	// 오른쪽 컴퍼넌트 콜백 기본 함수
	fn_component_main_success_callback = function(self){
		var self = $(this);
		self.find('.main_component,.dev_component').each(function(i){
			fn_create_component($(this));
		});
	}
	// 오른쪽 컴퍼넌트 처리 함수 
	var fn_component_main_success_InnerHtml = function(){
		if(typeof APP != 'undefine'){			
			if(typeof main_layout_component_01 != 'undefined' && typeof main_layout_component_09 != 'undefined'){
				clearInterval(fn_component_main_success_interval);
				
				
				var self = $('#main_layout_component_01');
				var val;
				var id = self.attr('id');
				try { val = eval(id); } catch (e) {}
				if(typeof val == 'string'){						
					var html_str =  '<div>'+val+'</div>';
					$("#"+id).html($(html_str).html()).load(fn_component_main_success_callback).load();
					
					setTimeout(function() {
						fn_component_main_success_end();
					},400);
				}
				
			}
		}
	}

	$(function(){
		fn_component_main_success_interval = setInterval(fn_component_main_success_InnerHtml, 800);
	});
	</script>
