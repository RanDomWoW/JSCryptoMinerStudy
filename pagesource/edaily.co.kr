    <script type="text/javascript">

        $(function () {
            if ($.cookie('popup') != 'no')
            {         
                $.ajax({
                    type: "GET",
                    url: 'https://webapi2.edaily.co.kr/BoardMgnt/popup',
                    data: {},
                    success: function (resultTxt) {
                        if (resultTxt != null) {
                            try {
                                var randomVal = Math.floor(Math.random() * 10) + 1;
                                window.open("/popup/viewpopup.html?v=" + randomVal, "popup", "width=" + parseInt(resultTxt.popup_width + 5) + ",height=" + parseInt(resultTxt.popup_height + 10) + ",top=100,left=100,scrollbars=no");
                            } catch (e) { }
                            
                        }
                    }
                });   
            }
