<script type="text/javascript">
$(document).ready(function(){       
    $('#search_word').focus();
    $("#type_select").bind("change", function() { 
        var op = $("#type_select").val();
        switch(op)
        {
        case "op_dict":
          $("#search_button").val('Lookup');
          $("#index_form").attr("action", "http://www.romajidesu.com/dictionary/");
          $("#search_word").attr("name", "w");
          $('#tip_tran').hide();$('#tip_kana').hide();$('#tip_kanj').hide();$('#tip_dict').show();
          break;
        case "op_kanj":
          $("#search_button").val('Lookup');
          $("#index_form").attr("action", "http://www.romajidesu.com/kanji/");
          $("#search_word").attr("name", "w");
          $('#tip_tran').hide();$('#tip_kana').hide();$('#tip_dict').hide();$('#tip_kanj').show();
          break;
        case "op_tran":
          $("#search_button").val('Convert');
          $("#index_form").attr("action", "http://www.romajidesu.com/translator/");
          $("#search_word").attr("name", "romaji");
          $('#tip_dict').hide();$('#tip_kana').hide();$('#tip_kanj').hide();$('#tip_tran').show();
          break;
        case "op_kana":
          $("#search_button").val('Convert');
          $("#index_form").attr("action", "http://www.romajidesu.com/romaji_kana/");
          $("#search_word").attr("name", "romaji");
          $('#tip_dict').hide();$('#tip_kanj').hide();$('#tip_tran').hide();$('#tip_kana').show();
          break;
        }
    });
    
    $("#index_form").bind("submit", function() {
        var action=$("#index_form").attr("action");
        w = $("#search_word").val();
        if ('http://www.romajidesu.com/dictionary/'==action  && w != '') {
            myurl = 'http://www.romajidesu.com/dictionary/meaning-of-'+w+'.html';
            window.location.href = myurl;
        }
        else{
            myurl = action+w;
            window.location.href = myurl;
        }
        return false;
    });
});
</script>  
