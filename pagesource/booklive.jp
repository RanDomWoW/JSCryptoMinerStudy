            $(this).addClass("current");
            $("#main_carousel").trigger("slideTo",[parseInt($(this).attr("data-target"))-1,{duration:10},true]);
        }
        $("#main_carousel").carouFredSel({
            onCreate: function(data){
                    $(this).trigger("currentPosition", function( pos ) {
                        pos = pos+1; if(pos > 4) pos = 0;
                        $("#main_carousel li").css("opacity",0.5);
                        $("#main_carousel .slide0").css("opacity",1);
                        $(".main_visual_list").removeClass("current");
                        $(".main_visual_list.slideinfo"+(pos)).addClass("current");
                    });
                },
            width   :   "100%",
            circular    : true,
            infinite    : true,
            auto    : {
                play    : true,
                timeoutDuration : 3000
                    },
            scroll  : {
                easing          : "quadratic",
                duration        : 300,
                pauseOnHover    : true,
                items           : 1,
                onBefore         : function(data){
                    $(this).trigger("currentPosition", function( pos ) {
                        pos = pos+1; if(pos > 4) pos = 0;
                        $(".main_visual_list").removeClass("current");
                        $(".main_visual_list.slideinfo"+(pos)).addClass("current");
                        $("#main_carousel li").css("opacity",0.5);
                        $("#main_carousel .slide"+pos).css("opacity",1);
                    });
                },
                onAfter         : function(data){
                    $(this).trigger("currentPosition", function( pos ) {
                        pos = pos+1; if(pos > 4) pos = 0;
                    });
                }
            },
            items   : {
                visible : 'odd+2',
                start   : -1
            },
            prev    : {
                button  : "#main_carousel_prev"
            },
            next    : {
                button  : "#main_carousel_next"
            }
        });
        $(".main_visual_list").mouseover(switch_visual);
        </script>
