<script>
    $(function() {
        var h = 0;
        $(window).scroll(function() { //when window is scrolled
            h = $(window).scrollTop();
            winh = Math.max(document.documentElement.clientHeight || 0);
            if (h > winh/2) {
                $("#pgtop").addClass("in");
            } else {
                $("#pgtop").removeClass("in");
            }

            if($("#footer").offset().top-h+($("#pgtop").height())+4 < winh){
                $("#pgtop").addClass("footerpos");
            }else{
                $("#pgtop").removeClass("footerpos");
            }
        });
        $('#pgtop').click(function(){
            $("html, body").animate({ scrollTop: 0 }, 600);
            return false;
        });
    });
</script>

