                    $_images.css({'opacity' : '0'});
                    $_images.eq(0).css({'opacity' : '1'});
                } else if (scrollTop > endX) {
                    $_images.css({'opacity' : '0'});
                    $_images.eq(numFrames-1).css({'opacity' : '1'});
                }
            }

            updateFrame();
            //$_images.eq(0).removeClass('hide');
            $(window).on('scroll', updateFrame);
            $_images.css({ 'width' : $('img.boximage').width() });

        });
    }
