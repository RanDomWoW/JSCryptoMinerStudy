<script>

function checkFBLogin(area) {
    FB.getLoginStatus(function(response) {
        if (response.status === 'connected') {
            getFBdetails(area);
        }
    });
}

function getFBdetails(area) {
    FB.api('/me?fields=id,name,email', function(response) {
        if (response.email) {
            $.ajax({
                type: 'POST', url: '/landing/speakeasyemail', dataType: 'json',
                data: { email: response.email, position: area, tz: tz, fid: response.id, name: response.name },
                success: function(res) { if (res) {
                    if (res.error) {
                        $('.form-mail-speakeasy').fadeOut('fast').fadeIn('fast');
                        $('div.out').html(res.error);
                    } else {
                        setTimeout( function() { location.href = '/waitlist'; }, 250);
                    }
                } }, statusCode: { 302: function() { } }
            });
        } else {
            $('.form-mail-speakeasy').fadeOut('fast').fadeIn('fast');
        }
    });
}

</script>



