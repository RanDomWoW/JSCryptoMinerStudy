                var scrollTop = window.pageYOffset + window.innerHeight;
                var delta = endX - startX;
                var step = numFrames / delta;

                if (scrollTop > startX && scrollTop < endX) {
                    var position = Math.floor(step * (scrollTop - startX));
                    //$_images.addClass('hide');
                    //$_images.eq(position).removeClass('hide');
                    $_images.css({'opacity' : '0'});
