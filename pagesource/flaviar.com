<script>
addPageLoadEvent(function() {

    function afterLoginGlobalPopupLogin(){
        if (typeof afterLoginTypeGlobalPopupLogin === 'undefined'){
            afterLoginTypeGlobalPopupLogin = { action:'refresh' };
        }
        if (afterLoginTypeGlobalPopupLogin.action == 'refresh')  location.reload();
        else if (afterLoginTypeGlobalPopupLogin.action == 'url' && (typeof afterLoginTypeGlobalPopupLogin.option !== 'undefined'))  location.href = afterLoginTypeGlobalPopupLogin.option;
        else if (afterLoginTypeGlobalPopupLogin.action == 'submit' && (typeof afterLoginTypeGlobalPopupLogin.option !== 'undefined'))  $(afterLoginTypeGlobalPopupLogin.option).submit();
        else if (afterLoginTypeGlobalPopupLogin.action == 'function' && (typeof afterLoginTypeGlobalPopupLogin.option !== 'undefined'))  afterLoginTypeGlobalPopupLogin.option();
    }

    function showLoginFormGlobalPopupLogin(){
        $(".login_info_GlobalPopupLogin .signupdiv").hide();
        $('.login_info_GlobalPopupLogin .login_email1').val($('.login_info_GlobalPopupLogin .login_email').val()).removeClass('blur');
        $('.login_info_GlobalPopupLogin .login_email').val('');
        $(".login_info_GlobalPopupLogin .memberdiv").show();
    }


$(document).ready(function() {
    $('.login_terms_GlobalPopupLogin .row').css('height', $('.login_info_GlobalPopupLogin').outerHeight()+'px');

    $('.login_info_GlobalPopupLogin .notmember').click(function(event){
        event.preventDefault(); $(".login_info_GlobalPopupLogin .memberdiv").hide(); $(".login_info_GlobalPopupLogin .signupdiv").show(); $('.login_info_GlobalPopupLogin .formerror').html('&nbsp;'); //$('.login_info_GlobalPopupLogin .login_email').focus();
    });

    $('.login_info_GlobalPopupLogin #already').click(function(event){
        event.preventDefault(); $(".login_info_GlobalPopupLogin .signupdiv").hide(); $(".login_info_GlobalPopupLogin .memberdiv").show(); $('.login_info_GlobalPopupLogin .formerror').html('&nbsp;');
    });

    $('.login_info_GlobalPopupLogin .signupform').submit(function(event) {
        event.preventDefault();

        ga('send', 'event', 'popup_signup_submit', 'submit', 'popup');
        
        if ($('.login_info_GlobalPopupLogin .login_email').val() != '' && $('.login_info_GlobalPopupLogin .login_email').val() != $('.login_info_GlobalPopupLogin .login_email').attr('title')) {
            $.ajax({
                type: 'POST', url: '/login/landing', dataType: 'json',
                data: { process: 'signup', email: $('.login_info_GlobalPopupLogin .login_email').val(), forcesource: 'Login', forcelogin: 'email' },
                success: function(res) { if (res) {
                    if (res.error) {
                        $('.login_info_GlobalPopupLogin .formerror').html(res.error);
                    } else if (res.exist) {
                        $('.login_info_GlobalPopupLogin .formerror').html('User already exists. <a href="#" id="requestnew">Lost password?</a>');
                        showLoginFormGlobalPopupLogin();
                    } else if (res.ok) {
                        $('.login_info_GlobalPopupLogin .formerror').html('Loading...');
                        setTimeout( function() { afterLoginGlobalPopupLogin(); }, 550);
                    }

                } },
                statusCode: { 302: function() { } }
            });
        } else {
            $('.login_info_GlobalPopupLogin .formerror').html('Oopsy daisy, is email ok?');
        }
    });

    $('.login_info_GlobalPopupLogin .loginform').submit(function(event) {
        event.preventDefault();

        ga('send', 'event', 'popup_login_submit', 'submit', 'popup');


        if ($('.login_info_GlobalPopupLogin .login_email1').val() != '' && $('.login_info_GlobalPopupLogin .login_email1').val() != $('.login_info_GlobalPopupLogin .login_email1').attr('title')) {
            $.ajax({
                type: 'POST', url: '/login/landing', dataType: 'json',
                data: { process: 'login', email: $('.login_info_GlobalPopupLogin .login_email1').val(), password: $('.login_info_GlobalPopupLogin .login_regPassField').val() },
                success: function(res) { if (res) {
                    if (res.error) {
                        $('.login_info_GlobalPopupLogin .formerror').html(res.error);
                    } else if (res.ok) {
                        $('.login_info_GlobalPopupLogin .formerror').html('Loading...');
                        setTimeout( function() { afterLoginGlobalPopupLogin(); }, 550);
                    }

                } },
                statusCode: { 302: function() { } }
            });
        } else {
            $('.login_info_GlobalPopupLogin .formerror').html('No go. Try with different input?');
        }
    });

            $('.login_info_GlobalPopupLogin').on('click', '#requestnew', function(event) {
    
        event.preventDefault();
        if ($('.login_info_GlobalPopupLogin .login_email1').val() != '' && $('.login_info_GlobalPopupLogin .login_email1').val() != $('.login_info_GlobalPopupLogin .login_email1').attr('title')) {
            $.ajax({
                type: 'POST', url: '/login/landing', dataType: 'json',
                data: { process: 'password', email: $('.login_info_GlobalPopupLogin .login_email1').val() },
                success: function(res) { if (res) {
                    if (res.error) {
                        $('.login_info_GlobalPopupLogin .formerror').html(res.error);
                    } else if (res.ok) {
                        $('.login_info_GlobalPopupLogin .formerror').html('Password sent, check your inbox');
                    }

                } },
                statusCode: { 302: function() { } }
            });
        } else {
            $('.login_info_GlobalPopupLogin .formerror').html('Email field is empty');
        }
    });

        $('.login_info_GlobalPopupLogin .fbLogin').click(function(event){
        event.preventDefault();
        
        $.oauthpopup({
            path: '/login/loginfb',
            callback: function(){
                $.getJSON('/login/get-fb-data', function(data) {
                    if (data.email) {
                        if (data.email=="already"){
                            // facebook login
                            $.ajax({
                                type: 'POST', url: '/login/landing', dataType: 'json',
                                data: { process: 'facebook' },
                                success: function(res) { if (res) {
                                    if (res.error) {
                                        $('.login_info_GlobalPopupLogin .formerror').html('Something went wrong. Try again!');
                                    } else if (res.ok) {
                                        $('.login_info_GlobalPopupLogin .formerror').html('Loading...');
                                        setTimeout( function() { afterLoginGlobalPopupLogin(); }, 550);
                                    }
                                } },
                                statusCode: { 302: function() { } }
                            });
                        } else if (data.email != "") {
                            $.ajax({
                                type: 'POST', url: '/landing/speakeasyemail', dataType: 'json',
                                data: { email: data.email, fid: data.id, name: data.name, dob: data.birthday, gender: data.gender, source: 'SpeakeasyLogin' },
                                success: function(res) { if (res) {
                                    if (res.error) {
                                        $('.login_info_GlobalPopupLogin .formerror').html('Something went wrong. Try again!');
                                    } else {
                                          setTimeout( function() { location.href = '/waitlist'; }, 250);
                                    }
                                } },
                                statusCode: { 302: function() { } }
                            });
                        }
                    } else {
                        $('.login_info_GlobalPopupLogin .formerror').html('Something went wrong. Try again!');
                    }
                });
            }
        });
    });
    

});


            showLoginFormGlobalPopupLogin();
        //$('.login_info_GlobalPopupLogin .login_email1').focus();
    

});
</script>


