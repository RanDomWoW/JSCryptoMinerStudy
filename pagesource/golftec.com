				maptype:maptype
			})

			.done(function(data) {
                                
				setCenters(data,maptype);
				if(window.centercount==1){
					setTimeout(function (){
						window.thelocmap.setZoom(15);
					}, 1000);
				}
			});

		}

		else if(maptype == 'address' || maptype == 'ecom'){
			maptype == 'address'?console.log('Search by address: ' + mapvar1):console.log('Search by ecom: ' + mapvar1);
            window.searchzip = mapvar1;
			var geocoder = new google.maps.Geocoder();

			if(geocoder) {
				geocoder.geocode({ 'address': mapvar1 }, function (results, status) {
					if (status == google.maps.GeocoderStatus.OK) {
						geocode_lat = results[0].geometry.location.lat();
                        window.geocode_lat = geocode_lat;
						geocode_long = results[0].geometry.location.lng();
                        window.geocode_long = geocode_long;
						console.log('Geocoded Lat: ' + geocode_lat);
						console.log('Geocoded Long: ' + geocode_long);
						console.log('Results type: ' + results[0].types[0]);
					   console.log('Set zoom initial map: window.thelocmap.setCenter(new google.maps.LatLng('+geocode_lat+', '+geocode_long+'))');
						thisresultstype = results[0].types[0];

						var mapOptions = {
							center: new google.maps.LatLng(geocode_lat, geocode_long),
							zoom: 10
						};

						var locations_map = new google.maps.Map(document.getElementById('map-canvas'),mapOptions);

						$.getJSON("https://"+window.wcmstarget+"/loadmarkers_6.php", {
							thelong:geocode_long,
							thelat:geocode_lat,
							scope: results[0].types[0],
							pagever:"prod",
							maptype:maptype
						})

						.done(function(data) {

                            setCenters(data,maptype);
							
						})

					}
					else {
						locError();
						
					}
				});
			}

		}

		else if(maptype == 'region'){
		
			if(mapvar1){
				wcms_target_region = mapvar1;
			}
			else {
				wcms_target_region = 'home';
			}

			$.getJSON("https://"+window.wcmstarget+"/loadregionmarkers_6.php", {
				theregion:wcms_target_region,
				maptype:maptype
			})

			.done(function(data) {
				setCenters(data,maptype);
			});

		}
        else if(maptype == 'center'){
            console.log('Load Center: ' + mapvar1);
		
			$.getJSON("https://"+window.wcmstarget+"/loadcentermarker_6.php", {
				thecenter:mapvar1,
				maptype:maptype
			})

			.done(function(data) {
				setCenters(data,maptype);
			});

		}
		

        if(window.centercount==1 || maptype == 'center'){
			setTimeout(function (){
                console.log('Delayed Load');
				window.thelocmap.setZoom(15);
			}, 750);
		}

	}
 	//if(maptype=='center')console.log('center map: '+locations_map.getZoom());

</script>
