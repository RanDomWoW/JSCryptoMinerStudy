        var inAjaxing = false;
        function login() {
            if (inAjaxing) {
                return;
            }
// 		var datas = FormToStr("loginForm");
// 		if (datas == null) {
// 			inAjaxing = false;
// 			return;
// 		}
            var nike = $('#nick').val();
            if (!nike) {
                JuaBox.showTip("请输入用户名");
                return;
            }
            var pwd = $('#pwd').val();
            if (!pwd) {
                JuaBox.showTip("请输入密码");
                return;
            }

            var encrypt = new JSEncrypt({default_public_exponent : '65537'});
            var pubTag = $("input[name='pubTag']").val();
            encrypt.setPublicKey(pubTag);
            vpwd = encrypt.encrypt(pwd);

            var datas = '&nike='+encodeURIComponent(nike)+'&pwd='+vpwd+'&pubTag='+pubTag;
            var isSafe = $('#isSafe').val();
            if (isSafe && isSafe == 1)
                datas = datas + "&safe=1";
            else
                datas = datas + "&safe=0";
            inAjaxing = true;

            $.getJSON(chbtc.vipDomain + "/user/doLogin?callback=?", datas, function(json) {
            	
                if (json.isSuc) {
                    if(json.datas && json.datas.needJump){
                        window.top.location.href = json.datas.jumpUrl;
                    }else{
                        if ($.cookie(ZNAME+"fromurl")) {
                            window.top.location.href = $.cookie(ZNAME+"fromurl");
                        } else {
                            window.top.location.href = DOMAIN_TRANS + '/btc';
                        }
                    }
                }else {
                		inAjaxing = false;
                    JuaBox.sure(json.des,{
                        closeFun: function() {
                            window.top.location.href = DOMAIN_VIP + "/user/login";
                        }
                    });
                }
            });
        }
    </script>
