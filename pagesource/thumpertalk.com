<script>
    var setDfpKeyWords = function()
    {
        var _pattern = /[^\sA-Za-z0-9_]/g;
        var _keyWords = document.title.split(' - ');
        _keyWords.pop();
        _keyWords = _keyWords.join(' ').replace(_pattern, '').split(' ');
        if($('.ipsPageHeader .ipsTag_prefix').length) {
            _keyWords.push($('.ipsPageHeader .ipsTag_prefix').text().replace(_pattern, ''));
        }
        if($('.ipsPageHeader .ipsTags').length) {
            $('.ipsPageHeader .ipsTags').find('a').not('[data-ipsmenu]').each(function() {
                _keyWords.push($(this).text().replace(_pattern, ''));
            });
        }
        ips.setSetting( "dfpKeyWords", _keyWords.join(',') );
    };
</script>

