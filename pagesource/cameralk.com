.mmobile-menu {
  display: block;
  width: 100%;
  background: #545454;
  line-height: 1.6em;
  font-weight: 400;
  width: 100%;
  text-align: center;
  position: relative;
  margin: 0 auto;
}
/*Strip the ul of padding and list styling*/
.mmobile-menu ul {
  list-style-type: none;
  margin: 0 auto;
  padding-left: 0;
  text-align: center;
  width: 100%;
  position: absolute;
  background: #e70300;
}

/*Create a horizontal list with spacing*/
.mmobile-menu li {
  display: inline-block;
  /*float: left;
	margin-right: 1px;*/
}

/*Style for menu links*/
.mmobile-menu li a {
     display: block;
    min-width: 140px;
    text-align: center;
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    color: #222;
    text-transform: uppercase;
    background: #ffffff;
    text-decoration: none;
    margin-left: 0px;
    padding: 10px 0;
    -webkit-transition: all 0.4s ease 0s;
    -moz-transition: all 0.4s ease 0s;
    -ms-transition: all 0.4s ease 0s;
    -o-transition: all 0.4s ease 0s;
    transition: all 0.4s ease 0s;
}

/*Hover state for top level links*/
.mmobile-menu li:hover a {
      color: #ffffff;
    background-color: #e70300;
}

/*Style for dropdown links*/
.mmobile-menu li:hover ul a {
  background: #f3f3f3;
  color: #2f3036;
  height: 40px;
  line-height: 40px;
}

/*Hover state for dropdown links*/
.mmobile-menu li:hover .mobile-menu ul a:hover {
  color: #4db3ff;
}

/*Hide dropdown links until they are needed*/
.mmobile-menu li ul {
  display: none;
}

/*Make dropdown links vertical*/
.mmobile-menu li ul li {
  display: block;
  float: none;
}

/*Prevent text wrapping*/
.mmobile-menu li ul li a {
  width: auto;
  min-width: 100px;
  padding: 0 20px;
}

/*Style 'show menu' label button and hide it by default*/
.mmobile-menu .show-menu {
  text-decoration: none;
  color: #fff;
     background: #545454;
  text-align: center;
  padding: 10px 15px!important;
  display: none;
  cursor: pointer;
  text-transform: uppercase;
}

.mmobile-menu .show-menu span {
  padding-left: 35px;
}

/*Hide checkbox*/
.mmobile-menu input[type="checkbox"] {
  display: none;
}

/*Show menu when invisible checkbox is checked*/
.mmobile-menu input[type="checkbox"]:checked ~ #menu {
  display: block;
}

/*Responsive Styles*/

@media screen and (max-width: 991px) {
  .mmobile-menu .lines {
        border-bottom: 19px double #ffffff;
    border-top: 0px solid #f8f8f8;
    content: "";
    height: 6px;
    width: 25px;
    padding-right: 10px;
    float: right;
  }
  /*Make dropdown links appear inline*/
  .mmobile-menu ul {
    position: static;
    display: none;
  }
  /*Create vertical spacing*/
  .mmobile-menu li {
    margin-bottom: 1px;
  }
  /*Make all menu links full width*/
  .mmobile-menu ul li,
  .mmobile-menu li a {
    width: 100%;
  }
  /*Display 'show menu' link*/
  .mmobile-menu .show-menu {
    display: block;
  }
}

