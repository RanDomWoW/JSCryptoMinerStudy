            var height = 430;
            var totalWidth = 985;
            
            var top = ($(window).height() - height) / 2 + $(window).scrollTop();
            var left = (totalWidth - width) / 2;

            $('.popup-betbox').css('left', left + 'px').css('top', top + 'px').show();

            return false;
        }
    
