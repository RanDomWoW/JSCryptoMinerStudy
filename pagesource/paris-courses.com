            }
            

            if (horse_num) {
                urlLeturfBetbox += "&designation=" + horse_num;
            }

            if(pari_id) {
                urlLeturfBetbox += "&id_pari=" + pari_id;
            }
            
            $(".popup-betbox .content").html('<iframe id="iframe-betbox-leturf" width="315" height="400" marginWidth="0" marginHeight="0" frameBorder="0" scrolling="auto" />');
