jQuery(".pariez-link").live("click", function() {
	jQuery(".bloc_pub_pariez").hide();
	jQuery(this).next(".bloc_pub_pariez").children().attr("src","/pariez/?url="+escape(jQuery(this).attr("rel"))
		+"&course_id="	+ jQuery(this).attr("data-course-id")
		+"&type=" 		+ jQuery(this).attr("data-type-lien")
		+"&date=" 		+ jQuery(this).attr("data-course-date")
		+"&numR=" 		+ jQuery(this).attr("data-course-numR")
		+"&numC=" 		+ jQuery(this).attr("data-course-numC"));
	jQuery(this).next(".bloc_pub_pariez").show();
	jQuery(this).next(".bloc_pub_pariez").delay(5000).fadeOut(400);
	jQuery(this).next(".bloc_pub_pariez").bind('mouseenter', function(){
		$(this).clearQueue();
		jQuery(this).bind('mouseleave', function(){jQuery(this).delay(4000).fadeOut(400);});
	});
	return false;
});
