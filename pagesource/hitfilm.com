      <script>
      $(function() {
        function subscribeToNewsletter(email, method, callback) {
          $.ajax({
            type: 'POST',
            url: '/api/user/subscribeToNewsletter',
            data: {email:email, method:method},
            dataType: 'json',
            success: function(data){
              if (data.result != 'success') {
                callback(data.msg);
                return;
              }

              callback();
            },
            error: function(data, error){
              callback(error);
            }
          });
        }

        $('#newsletterSubscribeButton').click(function() {
          var button = $(this);
          button.children().hide();
          button.children('.loader').show();

          subscribeToNewsletter($('#newsletterSubscribeEmail').val(), 'hitfilm-footer', function(err) {
            if (err) {
              alert(err);
              button.children().hide();
              button.children('.action').show();
              return;
            }

            $('#newsletterSubscribe').children().hide();
            $('#newsletterSubscribeSuccess').show();
          });
        });
      });
      </script>
