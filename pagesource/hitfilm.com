  $(document).ready(function() {
    if (isMobile()) {
      return;
    }

    $('.is-mobile').removeClass('is-mobile');

    var sectionCurrent = 1; // the currently active section
    var rotateTime = 3000; // number of ms between scrolls
    var sectionCount = 4; // number of sections (doesn't include the footer)
    var autoScrolling = true; // automatically scrolling

    $('#fullpage').fullpage({
      // anchors: ['jump-to-hitfilm-pro', 'jump-to-halo-jump', 'jump-to-hitfilm-studio', 'jump-to-hitfilm-express', 'jump-to-ignite-pro'],
      anchors: ['jump-to-hitfilm-studio', 'jump-to-halo-jump', 'jump-to-hitfilm-pro', 'jump-to-hitfilm-express', ''],
      navigation: true,
      navigationPosition: 'right',
      // navigationTooltips: ['HitFilm Pro', 'Halo Jump', 'HitFilm Studio', 'HitFilm Express', 'Ignite Pro', 'Site Navigation'],
      navigationTooltips: ['HitFilm Studio', 'Halo Jump', 'HitFilm Pro', 'HitFilm Express', 'Site Navigation'],
      menu: '#jump-to-menu',

      onLeave: function(index, nextIndex, direction) {
        if (autoScrolling) { // if we're autoScrolling then go to the next
          if (nextIndex <= sectionCount) {
            resetInterval();
            sectionCurrent = nextIndex;
          } else { // we're on the footer so don't scroll
            clearInterval(timer);
          }
        }
        if (nextIndex > sectionCount) {
          $("#jump-to-menu").fadeOut();
        }
        else {
          $("#jump-to-menu").fadeIn();
        }

        // start the community count when a section is loaded into view
        if ($(".section:nth-child("+nextIndex+") #community-count").length > 0) {
          $(".section:nth-child("+nextIndex+") #community-count").trigger('startCounting');
        }
      },
    });

    resetInterval(); // starts our scrolling timer

    function resetInterval() {
      clearInterval(timer);
      timer = setInterval(myTimer, rotateTime);
    }

    $("#jump-to-menu > li").hover(function() { // if the user hovers on an li it jumps to that section
      autoScrolling = false;
      clearInterval(timer);
      sectionCurrent = $(this).index() + 1;
      $.fn.fullpage.moveTo(sectionCurrent);
    });

    function myTimer() {
      sectionCurrent++;
      if (sectionCurrent > sectionCount) {
        sectionCurrent = 1;
      }
      $.fn.fullpage.moveTo(sectionCurrent);
    }
  });
</script>

		
