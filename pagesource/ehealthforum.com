<script type="text/javascript">
/*Slider*/
var slider_time = 7000;
var slider_start_at = 1;
var slider_timer = setInterval("slider_rotate()",slider_time);

if(document.addEventListener) {
	document.getElementById('hp_slide').addEventListener('mouseover',function(){clearInterval(slider_timer);},false);
	document.getElementById('hp_slide').addEventListener('mouseout',function(){slider_timer = setInterval('slider_rotate()',slider_time)},false);
}
else {
	document.getElementById('hp_slide').attachEvent('onmouseover',function(){clearInterval(slider_timer);},false);
	document.getElementById('hp_slide').attachEvent('onmouseout',function(){slider_timer = setInterval('slider_rotate()',slider_time)},false);
}

function slider_click(item) {
	clearInterval(slider_timer);
	for (i = 1; i <= 4; i++) {
		slider_change_class_name('main_image_'+i, 'fp_cont fp_na');
		slider_change_class_name('thumb_image_'+i, 'fp_thumb fp_na');
	}

	slider_change_class_name('main_image_'+item, 'fp_cont fp_a');
	slider_change_class_name('thumb_image_'+item, 'fp_thumb fp_a');
	slider_start_at = item;
	slider_timer = setInterval("slider_rotate()",slider_time);
}

function slider_rotate() {
	slider_start_at++;
	if (slider_start_at >= 5) slider_start_at = 1;
	slider_click(slider_start_at);
}

function slider_change_class_name(changeID, classname) {
	document.getElementById(changeID).className = classname;
}
/*END Slider */
</script>
