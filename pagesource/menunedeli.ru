$(document).ready(function() {
	//
	// сохранение эл. почты в куки при отправке формы
	(function($, undefined) {
		var $frms = $('form').has('input[type="email"]');
		if ( $frms.length > 0 ) {
			$frms.on('submit', function() {
				var $frm = $(this);
				Cookies.set('subsc_activ_email', $frm.find('input[type="email"]').val(), {expires: 5});
			});
		}
	})(jQuery);
	//
	// для страницы https://menunedeli.ru/2016/06/kak-podtverdit-podpisku-na-rassylku/
	// чтение эл. почты из куков, показ на странице, ссылка на почтового провайдера
	(function($, undefined) {
		if ( $('a.js.op-btn-email').length >= 1 && $('.js.op-email').length >= 1 ) {
			var providerLst = {
			 'ya.ru': 'https://mail.yandex.ru/'
			,'yandex.ru': 'https://mail.yandex.ru/'
			,'yandex.by': 'https://mail.yandex.ru/'
			,'yandex.com': 'https://mail.yandex.ru/'
			,'yandex.kz': 'https://mail.yandex.ru/'
			,'yandex.ua': 'https://mail.yandex.ru/'
			,'mail.ru': 'https://e.mail.ru/'
			,'inbox.ru': 'https://e.mail.ru/'
			,'list.ru': 'https://e.mail.ru/'
			,'bk.ru': 'https://e.mail.ru/'
			,'gmail.com': 'https://gmail.com/'
			,'rambler.ru': 'https://mail.rambler.ru/'
			,'lenta.ru': 'https://mail.rambler.ru/'
			,'autorambler.ru': 'https://mail.rambler.ru/'
			,'myrambler.ru': 'https://mail.rambler.ru/'
			,'ro.ru': 'https://mail.rambler.ru/'
			,'rambler.ua': 'https://mail.rambler.ru/'
			,'yahoo.com': 'https://ru.mail.yahoo.com/'
			,'ukr.net': 'https://mail.ukr.net/'
			};
			var providerLink = '';
			var email = Cookies.get('subsc_activ_email');
			if ( email.indexOf('@') === -1 ) {
				return;
			}
			$('.js.op-email').text(email);
			providerLink = email.substr(email.indexOf('@')+1);
			providerLink = providerLst[providerLink];
			if ( providerLink === undefined ) {
				$('a.js.op-btn-email').attr('href', 'http://' + email.substr(email.indexOf('@')+1));
			} else {
				$('a.js.op-btn-email').attr('href', providerLink);
			}
		}
	})(jQuery);
});
