// Function to determine if the search was for rental listings
function isRental() {
    if ($("input[type='hidden'][name='Groups/Group_SaleRentSold'][value$='4107']").length)
        return true;
    return false;
}

var mapSearchPresent;
var autoCompleteSelected = false;
$(function() {
    
    // When we're on the map search page we'll handle the location address search
    // when the map search criteria change, otherwise we'll handle it when the user
    // selects an item from the auto-suggest.
    mapSearchPresent = typeof MapSearch != "undefined";
    if(mapSearchPresent) {
    	// When the user changes a search criterion...
    	$(document).on("criteriaitemchange criteriachange maploaded", function(myEvent) {
    		handleLocationAddressSearch(null);
    	});
    } else {
    	// When the user selects an item from the autosuggest...
    	$("input.location").on("result", function(data, value) {
            autoCompleteSelected = true;
            console.log(isRental());
            console.log(JSON.stringify(value));
            if (isRental()) {
                redirectRentalSearch(value);
            }
            else
                handleLocationAddressSearch(value);
    	});
    }

    // if the selection is for rentals stop processing the submit so 
    // we can redirect to rental beast
    $("*[id^=homepage],#header_qs").on('submit', function (event) {
        if (isRental()) {
            var locationBox = "";
            $("#" + this.id + " input[name='Criteria/LocationBox']").each(function (index, value) {
                locationBox = value.value;
                return false;
            });
            if (locationBox.indexOf('(') >= 0 && locationBox.indexOf(')') >= 0) {
                autoCompleteSelected = true;
            }
            if (autoCompleteSelected)
                return false;
            parseRentalLocation(locationBox);
            return false;
        }
    });

    $('#rental-beast-form').on("submit", function () {
        console.log("submitted");
    });
});

function parseRentalLocation(location) {
    var validateAddressApiUrl = "/Include/AJAX/api.aspx?op=ValidateAddress&subjAddress={0}".format(location);
    $.ajax({
        url: validateAddressApiUrl,
        dataType: 'json'
    }).done(function (result) {
        console.log(JSON.stringify(result));
        if (typeof result != "undefined" && result && result.MatchScore >= 50) {
            if (result.City != "" && result.State != "") {
                console.log(result.City + ", " + result.State);
                $('#rb-city').val(result.City);
                $('#rb-state').val(result.State);
            }
            else if (result.PostalCode != "") {
                $('#rb-zip').val(result.PostalCode);
            }
        }
        $('#rental-beast-form').submit();
    });
}

function redirectRentalSearch(selectedAutosuggestItem) {
    if (typeof selectedAutosuggestItem != "undefined" && selectedAutosuggestItem != null) {
        console.log(selectedAutosuggestItem.Type);
        switch (selectedAutosuggestItem.Type) {
            case "City":
                $('#rb-city').val(selectedAutosuggestItem.Name);
                $('#rb-state').val(selectedAutosuggestItem.State);
                break;
            case "Zip Code":
                $('#rb-zip').val(selectedAutosuggestItem.Name);
                break;
            case "Address":
                $('#rb-address').val(selectedAutosuggestItem.Name);
                break;
            case "Neighborhood":
                $('#rb-neighborhood').val(selectedAutosuggestItem.Name);
                $('#rb-city').val(selectedAutosuggestItem.City);
                $('#rb-state').val(selectedAutosuggestItem.State);
                break;
            case "MLS #":
                $('#rb-mlsid').val(selectedAutosuggestItem.Name);
                break;
            case "Custom":
                var criteria = selectedAutosuggestItem.DisplayName.split(',');
                if (criteria.length > 1) {
                    $('#rb-city').val(criteria[0]);
                    console.log(criteria[0]);
                    $('#rb-state').val(criteria[1]);
                    console.log(criteria[1]);
                }
                break;
            default:
                $('#rb-city').val(selectedAutosuggestItem.City);
                $('#rb-state').val(selectedAutosuggestItem.State);
        }
    }
    $('#rental-beast-form').submit();
}

// Forces the property search to search For Sale, For Rent and Sold properties when
// the current locations(s) being search are all addresses. This is done so that there is a
// better chance that an address search will find a property.
var prevLocationList = null;
function handleLocationAddressSearch(selectedAutosuggestItem) {

	// Check whether the incoming value an address.
	var allLocationsAreAddresses = true;
	var currentLocationList = "";
	if(selectedAutosuggestItem) {
		allLocationsAreAddresses = allLocationsAreAddresses && selectedAutosuggestItem.Type == "Address";
		currentLocationList += selectedAutosuggestItem.Name;
	}

	if(!allLocationsAreAddresses) {
	    return;
	}

	if(mapSearchPresent) { 
		var locations = searchLocation.getFlattenedLocations(); // searchLocation is a JavaScript object from searchLocation.js

		// Don't do anything when there are no locations present.
		if(!locations || locations.length == 0) {
			return;
		}

		// Loop through each location that's already been stored and verify they're all addresses
		$.each(locations, function(index, value) {
			allLocationsAreAddresses = allLocationsAreAddresses && value.type == "Address";
			currentLocationList += "|" + value.value;
		});
	}

	// Prevent the function from forcing the search criteria if the user is seearching exactly the same locations
	// as last time. The first time this function runs prevLocationList will be null; let it through.
	if(prevLocationList != null && prevLocationList == currentLocationList) {
	    prevLocationList = currentLocationList;
	    return;
	}

	if(!allLocationsAreAddresses) {
	    prevLocationList = currentLocationList;
	    return;
	}
	
	// If all locations are addresses then force the search to search For Sale, For Rent and Sold properties
	
	// If checkboxes are present, check them all.
	var $checkboxes = $("input[type='checkbox'][name='Groups/Group_SaleRentSold']");
	if($checkboxes.length > 0) {
		$.each($checkboxes, function(index, value) {
			if(!$(this).prop("checked"))
				$(this).click(); // Click instead of setting the prop, in case there are handlers bound to the checkboxes.
		});
	}

	// If hidden inputs are present, put the values of each checkbox in them.
	// Ignore the 'mapsearch-criteria-hidden-status' input which tell the search to "or" these critieria.
	var $hiddenInputs = $("input[type='hidden'][name='Groups/Group_SaleRentSold']"); // :not(#mapsearch-criteria-forrentsalesold-andor)]
	if($hiddenInputs.length > 0) {
		$.each($hiddenInputs, function(index, value) {
			$(this).val("1,4107,4108,4109,4113,4114"); // The value "1" causes the search to "or" the criteria together.
		});
	}

	var $hiddenListingTypeIds = $("input[type='hidden'][name='Criteria/ListingTypeID']");
	if($hiddenListingTypeIds.length > 0) {
		$.each($hiddenListingTypeIds, function(index, value) {
			$(this).val("99"); // Use a non-null value that is not legit. We want the map search's default Listing Type(s) NOT to get selected on the UI.
		});
	}

	prevLocationList = currentLocationList;
	
	return;
}

