<script type="text/javascript">
    $(function() {

        // searchIsEmpty and getCityStateFromIpLocation defined in QuickSearch.js
        // lastSearch defined in end of html layout
        var moreCriteria = "";
        if (!searchIsEmpty(lastSearch)) {
            moreCriteria = "search=" + lastSearch.cookieGuid + "&";
        }

        moreCriteria = moreCriteria.replace(/&\s*$/, "");

         var carouselOptions = {
            minResults: 3,
            maxResults: 24,
            slidesToShow: 3,
            slidesToScroll: 3,
            autoplay: false,
            autoplaySpeed: 7000,
            progressiveLocationSearch: true,
            agentExtension: '',
            moreCriteria: moreCriteria
        };

        // determine what day of the week it is
        var today = new Date(),
            day = today.getDay(),
            hour = today.getHours(),
            jumpSearch;

        if (day > 0 && day < 4) { /* new finds (mon - wed) */
            jumpSearch = 9143027;
        } else if (day > 4 && day <= 6) { /* open houses (fri - sat) */
            jumpSearch = 9141516;
        } else if (day == 0) { /* check what time of day on Sunday it is */
            if (hour < 16) { /* it's earlier than 4pm, so open houses should show */
                jumpSearch = 9141516;
            } else { /* it's 4pm or later, so new finds should show */
                jumpSearch = 9143027;
            }
        } else { /* check what time of day on Thursday it is */
            if (hour < 16) { /* it's earlier than 4pm, so new finds should show */
                jumpSearch = 9143027;
            } else { /* it's 4pm or later, so open houses should show */
                jumpSearch = 9141516;
            }
        }

        /* console.log('jumpsearch id is: ' + jumpSearch); */
        carouselOptions.jumpSearchId = jumpSearch;

        // Gets the user's location according to their IP address. This is an
        // async call which calls a callback upon completion.
        rbw.geolocation.getIpLocationAsync(null, initJumpSearchSlider);

        function initJumpSearchSlider(ipLocation) {
            // debugger;
            if (ipLocation !== null && ipLocation.City && ipLocation.State) {

                if (!rbw.utils.isState(ipLocation.State)) {
                     ipLocation.State = rbw.utils.getStateCodeFromName(ipLocation.State);
                }
                
                // If the location is not in LF's market area then don't use the location at all
                if ($.inArray(ipLocation.State, ["MD", "VA", "DC", "PA", "NJ", "DE"]) !== -1) {

                    // *Add* the criteria city and state to the moreCriteria
                    if(moreCriteria && moreCriteria.length > 0)
                        moreCriteria += "&";
                    
                    moreCriteria +=  "Criteria-City=" + ipLocation.City + "&Criteria-State=" + ipLocation.State;
                }
                
                carouselOptions.moreCriteria = moreCriteria;
            }

           $("#carousel-target").lazy({
               carouselLoader: function(element) {
                   element.jumpSearchSlider(carouselOptions).load();
               }
           });
        }

        // change header class from lf_sub to lf_home
        $('.lf_sub').removeClass('lf_sub').addClass('lf_home');
    });
</script>
