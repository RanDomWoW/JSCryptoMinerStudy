    <script>
        $(function () {
            $('#product_message').hide();
            $("#search__box").keyup(function () {
                $('#product_message').hide();
                page = 0;
                var GetSearch = "/Search/SearchProduct";
                $.get(GetSearch + "?title=" + this.value, function (data) {
                    if (data != null && data != '') {
                        $('#search_table').html(data);
                    } else {
                        $('#search_table').html(data);
                        $('#product_message').show();
                    }
                });
            });
        });

        $(function () {
            $('input[data-modal-id]').click(function (e) {
                e.preventDefault();
                var modalBox = $(this).attr('data-modal-id');
                $('#' + modalBox).fadeIn($(this).data());
                $('#search__box').focus();
            });
        });
    </script>

    <script>
        var page = 0;
        $(function () {
            $('div#loadGif').hide();

            var _inCallback = false;

            function loadItems() {
                var title = document.getElementById('search__box').value;
                if (page > -1 && !_inCallback) {
                    _inCallback = true;
                    page++;
                    $('div#loadGif').show();

                    $.ajax({
                        type: 'GET',
                        url: '/Search/SearchProduct/?title=' + title + "&page=" + page,
                        success: function (data, textstatus) {
                            if (data != null) {
                                $("#search_table").append(data);
                            } else {
                                page = -1;
                            }
                            _inCallback = false;
                            $("div#loadGif").hide();
                        }
                    });
                }
            }

            // обработка события скроллинга popup
            $('#popup').scroll(function () {
                if ($(this).scrollTop() + $(this).innerHeight() >= $(this)[0].scrollHeight) {
                    loadItems();
                }
            });
        });
    </script>

    <script>
        function OnErrorEmail() {
            alert("Введите email адрес");
        }
        function OnShowEmail(data) {
            $("#updEmail").load('http://tolkuchka.bar/Home/EmailSubscriber');
            var message = [data], $h3 = $('#text');
            function show() {
                var text = message.shift();
                message.push(text);
                $h3.text(text).fadeIn(500, hide);
                document.getElementById('text').style.display = 'block';
            }

            function hide() {
                $h3.delay(3000).fadeOut(300, null);
            }
            document.getElementById('text').style.display = 'none';
            show();

        }

