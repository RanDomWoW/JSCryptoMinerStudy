<script type="text/javascript">

    //Override the default ASP.Net function so we can set display block instead of inline
    function ValidatorUpdateDisplay(val) {
        if (typeof (val.display) == "string") {
            if (val.display == "None") {
                return;
            }
            if (val.display == "Dynamic") {
                val.style.display = val.isvalid ? "none" : "block";
                return;
            }
        }
        if ((navigator.userAgent.indexOf("Mac") > -1) && (navigator.userAgent.indexOf("MSIE") > -1)) {
            val.style.display = "inline";
        }
        val.style.visibility = val.isvalid ? "hidden" : "visible";
    }

</script>




    

    
    

    <!-- Quick Shop Popup-->
    <div id="quickShopFB" class="quick-shop-modal container">
        <div class="product quick-shop-product">
            <div class="clearfix" style="position:relative;">
                <div id="ember_QS_ProductSaleViewRoot"></div>
                <div id="product-carousel" class="quick-shop-carousel"></div>
                <div class="info quick-shop-info">
                    <h2 id="ember_QS_ProductTitleViewRoot"></h2>
                    <p class="cost">
                        <span id="ember_QS_ProductPriceViewRoot"></span>
                    </p>
                    <div class="short-description">&nbsp;</div>
                    <span id="ember_QS_AttributeListViewRoot"></span>
                    <p id="ember_QS_ProductCodeViewRoot" class="style-code">Style Code: </p>
                </div>
            </div>

            <div style="display:none;">
                <span id="ember_QS_SelectedVarationViewRoot-Cart"></span>
                <span id="ember_QS_SelectedVarationViewRoot-Wishlist"></span>
            </div>

            <script type="text/x-handlebars" data-template-name="tmplQuickShopTitleView">
                {{#if QuickShop.Controllers.QuickShopController.product}}
                    {{QuickShop.Controllers.QuickShopController.product.title}}
                {{/if}}
            </script>
            
            <script type="text/x-handlebars" data-template-name="tmplQuickShopSaleView">
                {{#if view.hasDiscount}}
                    <span class="flag-detail-sale">Sale</span>
                {{/if}}
            </script>

            <script type="text/x-handlebars" data-template-name="tmplQuickShopCodeView">
                {{#if QuickShop.Controllers.QuickShopController.product}}
                    {{QuickShop.Controllers.QuickShopController.product.code}}
                {{/if}}
            </script>

            <script type="text/x-handlebars" data-template-name="tmplQuickShopPriceView">
                {{#if view.selectedVariation}}
                    {{#if view.hasDiscount}}
                        <span class="default"><strike>{{view.defaultPrice}}</strike></span>
                        <span class="sale">{{view.currentPrice}}</span>
                        <span class="currency-code">{{view.currency}}</span>
                    {{else}}
                        <span class="cost">{{view.currentPrice}}</span>
                        <span class="currency-code">{{view.currency}}</span>
                    {{/if}}
                {{/if}}
            </script>

            <script type="text/x-handlebars" data-template-name="tmplQuickShopAttributeListView">
                {{#if QuickShop.Controllers.QuickShopController.product}}
                    <ul class="controls">
                        {{#each attributeType in QuickShop.Controllers.QuickShopController.productAttributes}}
                            {{#view QuickShop.Views.AttributeControlView attributeTypeBinding="attributeType"}}
                                <label>Choose a {{attributeType.title}}: <span>{{bind "view.selectedValue"}}</span></label>                              
                                <div>
                                    {{#if view.useShortValues}}
                                        <ul class="ui-checkboxes clearfix">            
                                            {{#each attribute in attributeType.values}}
                                                {{#view QuickShop.Views.AttributeView attributeBinding="attribute"}}
                                                    <a>{{unbound attribute.value}}</a>
                                                {{/view}}
                                            {{/each}}
                                        </ul>
                                    {{else}}
                                        {{view QuickShop.Views.AttributeSelectView contentBinding="attributeType.values" titleBinding="attributeType.title"}}
                                    {{/if}}
                                </div>
                            {{/view}}           

                        {{/each}}
                        {{#if QuickShop.Controllers.QuickShopController.product.isAvailable}}
                            {{#unless QuickShop.Controllers.QuickShopController.product.onlyAvailableInStore}}
                                <li class="att att-qty">
                                    <label>Quantity</label>
                                    {{view Ember.TextField value="1" viewName="txtQty" size="5"}}                                        
                                </li>
                            {{/unless}}
                        {{/if}}

                    </ul>
                    <div id="quickshop-cart-add" class="cart-actions">
                        {{#if QuickShop.Controllers.QuickShopController.product.isAvailable}}
                            {{#if QuickShop.Controllers.QuickShopController.product.onlyAvailableInStore}}
                                <span><i class="fa fa-info-circle"></i>&nbsp;This product only available in store</span>
                                <div class="add-to-cart">
                                    <a href="javascript:void(0);" onclick="RipCurl.Lib.gtm_FindStore('/stores.html')" class="ui-button ui-button-secondary find-store" title="Find a store">Find a Store</a>
                                </div>
                            {{else}}            
                                {{#view QuickShop.Views.OutOfStockView}}
                                    <span><i class="fa fa-info-circle"></i>&nbsp;This size & colour is currently not available</span>
                                    <div class="add-to-cart">
                                        <a href="javascript:void(0);" onclick="RipCurl.Lib.gtm_FindStore('/stores.html')" class="ui-button ui-button-secondary find-store" title="Find a store">Find a Store</a>
                                    </div>
                                {{/view}}
                                {{#view QuickShop.Views.VariationNotAvailableView}}
                                    <span class="no-product-exists" style="display:none;"><i class="fa fa-info-circle"></i>&nbsp;This size & colour is not available</span>
                                {{/view}}
                                {{#view QuickShop.Views.ProductComingSoonView}}
                                    <span><i class="fa fa-info-circle"></i>&nbsp;Coming Soon! This product will be available on the following date. {{view.availablilityDate}}</span>
                                {{/view}}
                                <div class="add-to-cart">
                                    {{#view QuickShop.Views.AddtoCartButtonView}}<i class="fa fa-shopping-cart"></i>&nbsp;Add to Cart{{/view}}
                                </div>
                            {{/if}}
                        {{else}}
                            <span><i class="fa fa-info-circle"></i>&nbsp;This product is not available at this time</span>
                            <div class="add-to-cart">
                                <a onclick="RipCurl.Lib.gtm_FindStore('/stores.html')" class="ui-button ui-button-secondary find-store" title="Find a store">Find a Store</a>
                            </div>
                        {{/if}}                             
                        <div class="add-to-wishlist">
                            {{#view QuickShop.Views.AddtoWishlistButtonView}}<i class="fa fa-heart"></i>{{/view}}
                        </div>
                    </div>
                    <div id="quickshop-cart-added" style="display:none">
                        <span><i class="fa fa-info-circle"></i>&nbsp;Item successfully added to your cart</span>
                        <div class="clearfix">
                            <a href="/cart.html" class="ui-button quickshop-cart-added-checkout">Checkout</a>
                            <a href="javascript:void(0);" class="ui-button ui-button-secondary quickshop-cart-added-shop">Keep Shopping</a>
                        </div>
                    </div>
                    <div id="quickshop-wishlist-added" style="display:none">
                        <span><i class="fa fa-info-circle"></i>&nbsp;Item successfully added to your wishlist</span>
                        <div class="clearfix">
                            <a href="/wishlist.html" class="ui-button quickshop-cart-added-wishlist">View Wishlist</a>
                            <a href="javascript:void(0);" class="ui-button ui-button-secondary quickshop-cart-added-shop">Keep Shopping</a>
                        </div>
                    </div>
                {{/if}}
                        
                {{#if QuickShop.Controllers.QuickShopController.activityIndicator.working}}
                    <div class="acitivity-indicator"></div>
                {{/if}}
            </script>

            <script type="text/x-handlebars" data-template-name="tmplQuickShopSelectedVarationView">
                {{#if QuickShop.Controllers.QuickShopController.selectedVariation}}
                    <div class="three columns alpha">
                        <img {{bindAttr src="view.imageUrl"}} />
                    </div>        
                    <div class="seven columns alpha">
                        <span class="product-display-description">{{QuickShop.Controllers.QuickShopController.selectedQty}} &times; {{QuickShop.Controllers.QuickShopController.product.title}}</span>
                        <ul class="product-display-attributes">
                            {{#each QuickShop.Controllers.QuickShopController.selectedVariation.attributes}}
                                <li><span class="attr">{{title}}:</span> {{value}}</li> 
                            {{/each}}
                        </ul>
                    </div>
                {{/if}}
            </script>
        </div>
    </div>

    
