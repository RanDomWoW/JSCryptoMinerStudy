                    {{else}}
                        <a class="first" data-link="slider:#productGallery; index:{{counter}}" data-on="opacity: 1.0;" data-off="opacity: 0.6;">
                            <img src="{{thumb}}" alt="Product Thumbnail"/>
                        </a>
