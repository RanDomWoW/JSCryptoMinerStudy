<script>
	$(function(){
		$(".header_rt .box").hover(function(){
			$(this).find('.dropdown').show();
		},function(){       
			$(this).find('.dropdown').hide();
		});    

		$(document).on('click','.psd-eye',function(){
			var $this=$(this),
			$pr=$this.parent();
			$this.toggleClass('icon-eye-open icon-eye-close');
			var $showInput=$pr.find('input.hide'),
			$hideInput=$showInput.siblings('input');
			var showInputName = $showInput.attr("name");
			var hideInputName = $hideInput.attr("name");
			$showInput.attr("name",hideInputName);
			$hideInput.attr("name",showInputName);
			$showInput.removeClass('hide').val($hideInput.val());
			$hideInput.addClass('hide');
		})
		//modal
		$(document).on('click','.modal_close',function(){
			$('.modal,.modal_mask').hide();
		})
		
		//back-to-top
		var sT = $(window).scrollTop(),
			$bT = $(".sidebar_top");
		sT == "0" ? $bT.hide() : $bT.fadeIn("slow");
		/*背景图修改部分*/
		
		var oBodyH=$('.wrapper').height();
		
		var oFooterH=$('.wrapper #bottom_box').height();
		var oWinH=$(window).height();
		var bgTop=oBodyH-1000-oFooterH;
		setBg();
		$(window).scroll(function() {
			sT = $(this).scrollTop();
			sT == "0" ? $bT.fadeOut("slow") : $bT.fadeIn("slow");
			
			/*背景图修改部分*/
			oBodyH=$('.wrapper').height();
			oWinH=$(window).height();
			bgTop=oBodyH-1000-oFooterH;
			setBg()
		});
		/*背景方法，圣诞背景，可删*/
		
		function setBg(){
			if(oBodyH-sT-oWinH<=oFooterH){
				$('.wrapper').css({
					'backgroundPositionY':bgTop,
					'backgroundPositionX':"center",
					'backgroundAttachment':'scroll',
					
				});
			}else{
				var oHead=$('#header_box').height();
				if(sT<oHead){
					$('.wrapper').css({
						'backgroundPositionY':oHead,
						'backgroundPositionX':"center",
						'backgroundAttachment':'scroll',
					});
				}else{
					$('.wrapper').css({
					'backgroundPositionY':"top",
					'backgroundAttachment':'fixed'
					});
				}
			}
		}
		$bT.click(function() {
			$("html, body").animate({scrollTop: 0}, "slow");
		});
		$.post("/h-prize-prizeCount.html",function(chance){
			if(chance > 0){
				$(".prize_chance").html(chance);
				$(".notice-tip").show();
			}else{
				$(".prize_chance").html(0);
				$(".notice-tip").hide();
			}
		});
	})
</script>
