    Bty_Unlock();
  }
  //$('#info_contents').html('');
}
function toShow(){
  lockBody();
  $('#page_mask').show();
  $('#mask_info').show();
}
// 取得瀏覽器視窗高度
function getBrowserHeight() {
  if ('undefined' == typeof(document.body.style.maxHeight)) {
    return document.compatMode == "CSS1Compat" ? document.documentElement.clientHeight : document.body.clientHeight;
  } else {return self.innerHeight;}
}
// 取得瀏覽器視窗寬度
function getBrowserWidth() {
  if ('undefined' == typeof(document.body.style.maxHeight)) {
    return document.compatMode == "CSS1Compat" ? document.documentElement.clientWidth : document.body.clientWidth;
  } else {return self.innerWidth;}
}
// 取得置中的偏移值
function get_offset_val(type, val){
  if(!type){return '0';} // 沒給值回傳 0
  var _results = '0';
  switch(type){
    case 'h':
      var _h = getBrowserHeight();
      var _tmp = ((_h - val) / 2 - 40);
      _results = _tmp > '0' ? _tmp : '0';
      break;
    case 'w':
      var _w = getBrowserWidth();
      var _tmp = ((_w - val) / 2 - 10);
      _results = _tmp > '0' ? _tmp : '0';
      break;
  }
  return _results;
}
function lockBody(){$('Body').css('overflow', 'hidden');}
function unLockBody(){$('Body').css('overflow', 'inherit');}
