function displayMask(_contents, _width, _height, _type, _haveClose){
  _contents = _contents || "";
  _type   = _type   || "html";
  _width   = _width   || getBrowserWidth();
  _height  = _height  || getBrowserHeight();
  if(typeof(_haveClose) != 'boolean'){_haveClose = true;}
  switch(_type) {
    case "html":
      $('#info_contents').html(_contents);
      break;
    case "url":
      $('#info_contents').html("<iframe id='mask_iframe' src='"+_contents+"' frameborder='0' width='100%' height='100%'></iframe>");
      break;
  }
  if(_haveClose ){$('#mask_close').show();}
  if(_width > 0){
    $('#mask_info').css({
      'width': _width + 'px',
      'left' : get_offset_val("w", _width) + 'px'
    });
  }else{
    $('#mask_info').css({
      'width': _width,
      'left' : 0,
    });
  }
  if(_height > 0){
    $('#mask_info').css({
      'height': _height + 'px',
      'top' : get_offset_val("h", _height) + 'px'
    });
  }else{
    $('#mask_info').css({
      'height': _height,
      'top' : 0,
    });
  }
  toShow();
}
function maskToHide(callback){
  unLockBody();
  $('#mask_info').hide();
  $('#page_mask').hide();
