<script>
if(typeof _chkLogin == 'function') {
var service_info = {'cm':{'tel':'02-77312666'}, 'tm':{'tel':'02-77312055'}} ;
var item_deliver_from = '0';
var swcalendar = '';
var actionFrom = '';
$(document).ready(function(){
  is_wktime = false;
  is_show_tel = false;
  is_show_contact = false;
  var $clientDate = new Date();
  var $serviceDay = [1, 2, 3, 4, 5];
  try{
    if(typeof(swcalendar) == 'object' && Object.keys(swcalendar).length > 0 ){
      dt = $clientDate.getFullYear()+'-'+('0'+($clientDate.getMonth()+1)).substr(-2)+'-'+('0'+$clientDate.getDate()).substr(-2);
      if(dt != swcalendar.calendar_date){
        // 日期不相符,用預設方式處理
        throw "";
      }
      if(swcalendar.is_cs == 't'){
        // 檢查上下班時間
        st = new Date(swcalendar.cs_service_start_uxtimestamp*1000);
        et = new Date(swcalendar.cs_service_end_uxtimestamp*1000);
        nh = $clientDate.getHours();
        if(st.getHours()<=nh && nh < et.getHours()){
          //上班時間
          is_wktime = true;
        }
      }

      if(is_wktime){
        // 檢查上班時間提供服務
        if(typeof(swcalendar.cssets.W) != 'object' || Object.keys(swcalendar.cssets.W).length <= 0 ){
          throw "";
        }

        if(item_deliver_from == 9 ){
          is_show_tel = swcalendar.cssets.W.is_tr_horder_tel == 't' ? true : false;
          is_show_contact = swcalendar.cssets.W.is_tr_horder_contact == 't' ? true : false ;
        }else{
          is_show_tel = swcalendar.cssets.W.is_cm_horder_tel == 't' ? true : false;
          is_show_contact = swcalendar.cssets.W.is_cm_horder_contact == 't' ? true : false ;
        }
      }else{
        // 檢查休假時間提供服務
        if(typeof(swcalendar.cssets.H) != 'object' || Object.keys(swcalendar.cssets.H).length <= 0 ){
          throw "";
        }

        if(item_deliver_from == 9 ){
          is_show_tel = swcalendar.cssets.H.is_tr_horder_tel == 't' ? true : false;
          is_show_contact = swcalendar.cssets.H.is_tr_horder_contact == 't' ? true : false ;
        }else{
          is_show_tel = swcalendar.cssets.H.is_cm_horder_tel == 't' ? true : false;
          is_show_contact = swcalendar.cssets.H.is_cm_horder_contact == 't' ? true : false ;
        }
      }
    }else{
      throw "";
    }
  }catch(er){
    // 預設處理方式
    //alert('catch');
    /*if($.inArray(parseInt($clientDate.getDay()), $serviceDay)!==-1){
      if($clientDate.getHours()>=9 && $clientDate.getHours()<=17) {
        //is_show_tel = true;
      }
    }*/
    is_show_tel = true;
  }

  if(is_show_tel){
    _Consultation.showTelBTN(true);
  }

  if(is_show_contact){
    _Consultation.showContactBTN(true);
  }

  /* $('.Consultation_P1block div#cmCall').click(function(){
    if(actionFrom != 'app'){
      dr = parseInt(item_deliver_from);
      tel = dr != 9 ? service_info.cm.tel : service_info.tm.tel;
      location.href = 'tel:'+tel;
    }
  });*/

  $('.leave_a_message').click(function(){
    _Consultation.clickContactBTN();
  });

  $('.Consultation_back').click(function(){
   $('.Consultation_Inside_Wrap').animate({ left: '0'}, "400");
   $('.Consultation_Inside_Page1').css("opacity", "1");
  });

  $('.Consultation_Close').click(function(){
   unLockBody();
   $('.Consultation_Inside_Wrap').animate({ left: '0'}, "0");
   $('.Consultation_Mask_Wrap').fadeOut(150);
   $('.Consultation_Inside_Page1').css("opacity", "1");
  })

  $('.Consultation p').click(function(){
    // 檢查是否是已售完
   is_soldout = $(this).find('span#_item_soldout_').length > 0 ? true : false ;
   if(is_soldout){
      _Consultation.showTelBTN(false);
      _Consultation.showContactBTN(true);
   }
   $('.Consultation_Mask_Wrap').fadeIn(150);
   lockBody();
   if(is_soldout){
    _Consultation.clickContactBTN();
   }
  });

  $('#ConsultationSubmit').click(function(){
    // mail 格試驗證
    var mail_reg = /^[_a-z0-9-]+(\.[_a-z0-9-]+)*@[a-z0-9-]+(\.[a-z0-9-]+)*(\.[a-z]{2,3})$/;
    // 市話
    var phone_WithArea_1 = /^[0]{1}[1-9]{1,2}[0-9]{5,10}$/;
    var phone_WithArea_2 = /^[0]{1}[1-9]{1,2}-[0-9]{5,10}$/;
    // 手機
    var mobile_reg = /^[09]{2}[0-9]{8}$/;

    var err = new Array();
    var data = new FormData();

    $('.Consultation_P2block div > [data-type="contact_us"]').each(function(){
      var tmp_row = $(this).attr('name');
      var tmp_val = $(this).val();
      var tmp_ch = $(this).attr('data-ch');
      if($(this).hasClass('required')){
        if(!tmp_val){
          err.push(tmp_ch+'不可為空');
        }
      }
      if(tmp_val){
        switch(tmp_row){
          case 'email':
            if(!tmp_val.toLowerCase().trim().match(mail_reg)) {
              err.push(tmp_ch+'格式錯誤，範例：demo@test.gov.tw(字串不可包含空白文字)');
            }
            break;
          case 'tel':
            if(tmp_val.indexOf('-') >= 0){
              if(!tmp_val.match(phone_WithArea_2)){
                err.push(tmp_ch+'格式錯誤，範例:02-12345678');
              }
            }else if(tmp_val.match(phone_WithArea_1)){
            }else{
              if(tmp_val.length < 10 || !tmp_val.match(mobile_reg)) {
                err.push(tmp_ch+'格式錯誤，範例:0910123456');
              }
            }
            break;
        }
      }
      data.append(tmp_row, tmp_val);
    });

    if(!err.length){
      data.append('item_deliver_from',item_deliver_from);
      data.append('cfrom','consultation');
      data.append('contact_type', 31);
      data.append('title', $('#cmItemID').text());
      data.append('json_return', true);
      $.ajax({

        url: 'https://pc.crazymike.tw/contact_chk',
        type: 'POST',
        crossDomain: true,
        data: data,
        datatype: 'json',
        contentType: false,
        cache: false,
        processData: false,
        xhrFields: { withCredentials: true},
        beforeSend: function(){
          $('#ConsultationSubmit').hide();
          $('.Consultation_P2block .Loading_Start').show();
        },
        complete: function(){
          $('.Consultation_P2block .Loading_Start').hide();
          $('#ConsultationSubmit').show();
        },
        success: function(reply, textStatus, jqXHR){
          var response = jQuery.parseJSON(reply);
          alert(response.msg);
          if(!response.isErr){
            $('.Consultation_Close').click();
          }
        },
        error: function(jqXHR, textStatus, errorThrown){
          alert("系統異常，請稍候再試..");
        }
      });
    }else {
      alert(err.join('\n'));
    }
  });

  if(_chkLogin()){
    var user = GetCookie('user'), c_type = GetCookie('C_type');
    corsRequest('/openapi', {'func': 'member', 'tfunc': 'get', 'C_type': c_type, 'user': user}, 'setMemberData(a)');
  }
});


var _Consultation = {
  showTelBTN:function(is_show){
    if(is_show){
      $('.cmCallTel').show();
    }else{
      $('.cmCallTel').hide();
    }
  },
  showContactBTN:function(is_show){
    if(is_show){
      $('.Consultation_P1block > div > div').eq(6).show();
      $('.leave_a_message').show();
    }else{
      $('.Consultation_P1block > div > div').eq(6).hide();
      $('.leave_a_message').hide();
    }
  },
  clickContactBTN:function(){
    $('.Consultation_Inside_Wrap').animate({ left: '-100%'}, "400");
    $('.Consultation_Inside_Page1').css("opacity", "0");
  },
  P1block_Reset:function(){
    var dr = parseInt(item_deliver_from);
    $('.Consultation_P1block span[name="tel"]').each(function(){
      tel = dr != 9 ? service_info.cm.tel : service_info.tm.tel;
      $(this).html(tel);
    });
    $('.Consultation_P1block span[name="telext"]').each(function(){
      if(dr != 9){
        $(this).show();
      }else{
        $(this).hide();
      }
    });
  } ,
};

function setMemberData(data) {
  var response = data[0];
  if(response.result){
    $('.Consultation_P2block input[name="name"]').val(response.rtn.name);
    $('.Consultation_P2block input[name="tel"]').val(response.rtn.tel);
    $('.Consultation_P2block input[name="email"]').val(response.rtn.email);
  }
}
// for 跨網域呼叫 接收處理 by izumi.wang, 20170309.
function html5_message(e) {
  if(e.origin != 'https://pc.crazymike.tw' && e.origin != 'https://crazymike.tw'){
    return false;
  }
  var $args = typeof(e.data)=='string' ? e.data.split('<@#@>') : [];
  switch($args[0]) {
    case 'Consultation':
      $('#cmItemID').html('產品編號:'+$args[1]);
      $('#cmRemark').html($args[2]+'\n產品編號:'+$args[1]);
      dr = (typeof($args[3]) == 'string') ? $args[3] : '0' ;
      item_deliver_from = dr;
      _Consultation.P1block_Reset();
      $('.Consultation_Mask_Wrap').fadeIn(150);
      break;
    case 'CartDone':
      window.cartDone($args[1]);
      break;
  }
}
// for 監聽跨網域呼叫 接收 by izumi.wang, 20170309.
window.addEventListener("message", html5_message, false);
}
</script>
