<script>
function _comAddTrace() {
  $(document).on('click', '.trace', function(event) {
    if(traceLock) return ;

    var tcode = $(this).attr('tcode');
    var fcode = $(this).attr('fcode');
    if(!tcode) {
      tcode='0';
    }
    if(!fcode) {
      fcode = '';
    }
    var target = $(this).attr('target') ? $(this).attr('target') : '';
    if(!$(this).attr('nolink') && target.toLowerCase() != '_blank') {
      event.preventDefault();
          }

    _setTrace(event, tcode, fcode, $(this));
  });
}
function _setTrace(event, tcode, fcode, obj) {
  var url ='';
  url='/ajax-trace';

  url+= '?tcode='+tcode;
  if(fcode && fcode!='') {
    url+= '&fcode='+fcode;
  } else {
    var v = '';
    if(_ITEM_ID && _ITEM_ID != '' && _ITEM_ID != '0') {
      fcode='';
      v = _ITEM_ID;
    } else if(_NOW_TAG != '' && _NOW_TAG != '0') {
      fcode='T';
      v = _NOW_TAG;
    } else if(_NOW_CHANNEL != '' && _NOW_CHANNEL != '0') {
      fcode='C';
      v = _NOW_CHANNEL;
    }

    fcode+= tcode.substr(0, 1)+v;
    url+= '&fcode='+fcode;
  }

  corsRequest(url, {}, 'traceDone(d)', obj);
}
function traceDone(obj) {
  var href = obj.attr('href');
  var target = obj.attr('target');
  var no_redirect = obj.attr('no_redirect');
  var backUrl = obj.attr('backUrl');
  var _name = obj.attr('name');

  href = href ? href : '';
  var mkt3 = parseUrl('mkt3');
  if(mkt3 && href!='' && href.indexOf('/item-')>=0) {
    href+= (href.indexOf('?')>0?'&':'?')+'mkt3='+mkt3;
  }

  
  target = target ? target : '';
  no_redirect = no_redirect ? no_redirect : false;

  if(!target) {
    target = '_self';
  }

  if(!no_redirect && target.toLowerCase() != '_blank') {
    if(href != '') {
      window.location.href = href;
    }
  }
}
