    <script type="text/javascript">

        jQuery(document).ready(function() {

            jQuery('body').on('click','.open-login',function(){
                jQuery('#login-modal').modal('show');
            });

            var modalLoginForm = new VarienForm('modal-login-form', true);
            jQuery('#modal-login-form').submit(function(e){
                e.preventDefault();
                if (modalLoginForm.validator.validate()) {
                    callAjaxControllerLogin(jQuery('#modal-login-form input[name="login[username]"]').val(),jQuery('#modal-login-form input[name="login[password]"]').val());
                }

            });

            var modalRegisterForm = new VarienForm('modal-register-form', true);
            jQuery('#modal-register-form').submit(function(e){
                e.preventDefault();
                if (modalRegisterForm.validator.validate()) {
                    callAjaxControllerRegistration(jQuery('#modal-register-form input[name="firstname"]').val(),jQuery('#modal-register-form input[name="lastname"]').val(),jQuery('#modal-register-form input[name="email"]').val(),jQuery('#modal-register-form input[name="password"]').val());
                }

            });


            var onAjax = false;
            function callAjaxControllerLogin(email,password){
                if (onAjax != true){
                    onAjax = true;

                    var ajaxRegistration = jQuery.ajax({
                        url: 'https://www.lesgrappes.com/ajaxlogin/ajax/index',
                        type: 'POST',
                        data: {
                            ajax : 'login',
                            email : email,
                            password : password
                        },
                        dataType: "html"
                    });
                    ajaxRegistration.done(function(msg) {
                        if (msg != 'success'){
                            var windowText = 'Connexion';
                            window.pnotifyMe(windowText, msg, 'error');
                        }
                        else {
                            onAjax = false;
                            window.pnotifyMe('Bienvenue !', '', 'success');
                            window.location.reload();
                        }
                        onAjax = false;
                    });
                    ajaxRegistration.fail(function(jqXHR, textStatus, errorThrown) {
                        onAjax = false;
                    });
                }
            }
            function callAjaxControllerRegistration(firstname,lastname,email,password){
                if (onAjax != true){
                    onAjax = true;
                    jQuery('#modal-register-form .error-msg').html('');

                    var ajaxRegistration = jQuery.ajax({
                        url: 'https://www.lesgrappes.com/ajaxlogin/ajax/index',
                        type: 'POST',
                        data: {
                            ajax : 'register',
                            firstname : firstname,
                            lastname : lastname,
                            email : email,
                            password : password
                        },
                        dataType: "html"
                    });

                    ajaxRegistration.done(function(msg) {
                        if (msg != 'success'){
                            //setError(msg, 'register');
                            msg = msg.replace(',','<br>');
                            jQuery('#modal-register-form .error-msg').html(msg);
                            jQuery('#modal-register-form .error-msg').show();
                            //window.pnotifyMe('Inscription', msg, 'error');
                        } else {
                            onAjax = false;
                            window.location.reload();
                        }
                        onAjax = false;
                    });
                    ajaxRegistration.fail(function(jqXHR, textStatus, errorThrown) {
                        onAjax = false;
                    });
                }
            }


        });




    </script>


