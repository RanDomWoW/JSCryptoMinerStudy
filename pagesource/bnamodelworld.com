<script type="text/javascript">



/*  Change the items below to create your countdown target date and announcement once the target date and time are reached.  */



var weekend='<div style="margin-top:-25px;font-size:16px;color:#F60;">Next Dispatch is on<br /><br /><span style="color:#F60;font-size:23px;">Monday</span></div>'



var current='<div style="margin-top:-20px;">Same Bussiness<br /><br />Day Service</div>'     // enter what you want the script to display when the target date and time are reached, limit to 20 characters



var year=2018       //?a>Enter the count down target date YEAR

var month= 3        //?a>Enter the count down target date MONTH

var day= 31          //?a>Enter the count down target date DAY

var hour=15       //?a>Enter the count down target date HOUR (24 hour clock)

var minute=0        //?a>Enter the count down target date MINUTE

var tz=11            //?a>Offset for your timezone in hours from UTC (see http://wwp.greenwichmeantime.com/index.htm to find the timezone offset for your location)





//    DO NOT CHANGE THE CODE BELOW!    



var montharray=new Array("Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec")



function countdown(yr,m,d,hr,min){

    theyear=yr;themonth=m;theday=d;thehour=hr;theminute=min

    var today=new Date()

    var todayy=today.getYear()

    if (todayy < 1000) {

    todayy+=1900 }

    var todaym=today.getMonth()

    var todayd=today.getDate()

    var todayh=today.getHours()

    var todaymin=today.getMinutes()

    var todaysec=today.getSeconds()

    var todaystring1=montharray[todaym]+" "+todayd+", "+todayy+" "+todayh+":"+todaymin+":"+todaysec

    var todaystring=Date.parse(todaystring1)+(tz*1000*60*60)

    var futurestring1=(montharray[m-1]+" "+d+", "+yr+" "+hr+":"+min);

    var futurestring=Date.parse(futurestring1)-(today.getTimezoneOffset()*(1000*60));

    var dd=futurestring-todaystring



    var dday=Math.floor(dd/(60*60*1000*24)*1) 



	var dhour=Math.floor((dd%(60*60*1000*24))/(60*60*1000)*1)

    var dmin=Math.floor(((dd%(60*60*1000*24))%(60*60*1000))/(60*1000)*1)

    var dsec=Math.floor((((dd%(60*60*1000*24))%(60*60*1000))%(60*1000))/1000*1)



	if ((dday<=0&&dhour<=0&&dmin<=0&&dsec<=0&&today.getDay()!=5&&today.getDay()!=6&&today.getDay()!=0)||(dday<=0&&dhour<=0&&dmin<=0&&dsec<=0&&today.getDay()==5&&today.getHours()<15)||(dday<=0&&dhour<=0&&dmin<=0&&dsec<=0&&today.getDay()==0&&today.getHours()>=20)){

        document.getElementById('count2').style.display="none";

        document.getElementById('dhour').innerHTML=dhour+25;

        document.getElementById('dmin').innerHTML=dmin+60;

        document.getElementById('dsec').innerHTML=dsec+60;

        setTimeout("countdown(theyear,themonth,theday,thehour,theminute)",500);

    }  /*Activated to avoid any problem when the target date is passed*/



	else if ((today.getDay() == 5 && today.getHours() >= 15)||(today.getDay() == 6)|| (today.getDay() == 0 && today.getHours() < 20)){

        document.getElementById('count2').innerHTML=weekend;

		setTimeout("countdown(theyear,themonth,theday,thehour,theminute)",500);

        return;

    }

	

	else if (dhour>=10 && today.getTimezoneOffset() >-300){

        document.getElementById('count2').innerHTML=current;

		setTimeout("countdown(theyear,themonth,theday,thehour,theminute)",500);

        return;

    } /*Added by Ashely on 03/04/2014 for European and American customers*/



	/*else if ((today.getDay() == 5 && today.getHours() >= 15)||(today.getDay() == 6 && today.getHours() < 15)) {

        document.getElementById('count2').style.display="none";

        document.getElementById('dhour').innerHTML=dhour+48;

        document.getElementById('dmin').innerHTML=dmin;

        document.getElementById('dsec').innerHTML=dsec;

        setTimeout("countdown(theyear,themonth,theday,thehour,theminute)",500);

	  }

    else if ((today.getDay() == 6 && today.getHours() >= 15)||(today.getDay() == 0 && today.getHours() < 15)) {

        document.getElementById('count2').style.display="none";

        document.getElementById('dhour').innerHTML=dhour+24;

        document.getElementById('dmin').innerHTML=dmin;

        document.getElementById('dsec').innerHTML=dsec;

        setTimeout("countdown(theyear,themonth,theday,thehour,theminute)",500);

	  }*/



    else {

        document.getElementById('count2').style.display="none";

        document.getElementById('dhour').innerHTML=dhour;

        document.getElementById('dmin').innerHTML=dmin;

        document.getElementById('dsec').innerHTML=dsec;

        setTimeout("countdown(theyear,themonth,theday,thehour,theminute)",500);

	  }

}

window.onload = function() {


    countdown(year,month,day,hour,minute);

    doTimer();


}



</script>





