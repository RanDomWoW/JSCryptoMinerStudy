		var zind = $('.items').data('z-index');
		zind ++;
		var tooltip_wrapper = $('#justSold div.items');
		$(this).parent()
			.find('.tooltip')
			.clone()
			.appendTo(tooltip_wrapper)
			.fadeIn('fast')
			.css('left', ($(this).parent().position().left+55)+'px')
