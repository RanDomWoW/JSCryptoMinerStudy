		if (item.data('timer')) {
			clearInterval(item.data('timer'));
		}
	};

	var func_tooltip_hide = function(){
		if ($(this).hasClass('tooltip'))
		{
			var itemcode = $(this).data('itemcode');
			var that = $('.items .item[data-itemcode='+itemcode+']');
		} else {
			var itemcode = $(this).find('.tooltip').data('itemcode');
			var that = $(this);
		}

		var tooltip_wrapper = $('#justSold div.items');
		that.data('timer', setTimeout(function(){
			tooltip_wrapper.find('.tooltip[data-itemcode='+itemcode+']').hide();
		}, 300));
	};

	$('#justSold .items .item').mouseleave(func_tooltip_hide);

	$('#justSold .kamo-wrapper ul').h_auto_scroll(2000);
});

$.fn.h_auto_scroll = function(interval){
	var that = this;
	var scrolling = function(){
		that.find('li').first().animate({marginLeft:'-188px'}, {complete:function(){
			that.find('li').first().css({marginLeft:''}).appendTo(that);
			setTimeout(scrolling, interval);
		}});
	}
	if (that.find('li').length > 5)
	{
		setTimeout(scrolling, interval);
	}
};

