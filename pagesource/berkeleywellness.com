    function setupNavigation(slider) {
        var current = slider.currentSlide;
        var slides = slider.slides;
        var totalSlides = slides.length;

        var nextSlide = current + 1;
        if (nextSlide == totalSlides)
            nextSlide = 0;

        var prevSlide = current - 1;
        if (prevSlide == -1)
            prevSlide = totalSlides - 1;

        var child1 = jQuery(slides[nextSlide]).children();
        var child2 = jQuery(child1[0]).children();
        var nextSrc = jQuery(child2[0]).attr("src");

        var child3 = jQuery(slides[prevSlide]).children();
        var child4 = jQuery(child3[0]).children();
        var prevSrc = jQuery(child4[0]).attr("src");

        jQuery(".previous_slider").css({background: "url(" + prevSrc + ") no-repeat 100% 0"}).animate({}, 600, 'slide');
        jQuery(".next_slider").css({background: "url(" + nextSrc + ") no-repeat 0 0"});
    }

    jQuery(document).ready(function() {
        initialFlexContent = jQuery('.homepageslider').html();
        jQuery('.flexslider').flexslider({
            animation: "slide",
            pauseOnAction: false,
            pauseOnHover: true,
            start: function(slider) {
                jQuery('body').removeClass('loading');
                setupNavigation(slider);
            },
            after: function(slider) {
                setupNavigation(slider);
            }
        });
    });
</script>
