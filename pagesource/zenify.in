<script>
    $('.js-apartment-typeahead').on('typeahead:change', function() {
        if ( $(this).val() != window.apartName ) {
            $($(this).attr('data-update-id')).val('');
        }
    });
    $('.js-apartment-typeahead').on('typeahead:select', function(ev, suggestion) {
        window.apartName = $(this).val();
        $($(this).attr('data-update-id')).val(suggestion.id);
    });
    $('.js-apartment-typeahead').on('typeahead:autocomplete', function(ev, suggestion) {
        window.apartName = $(this).val();
        $($(this).attr('data-update-id')).val(suggestion.id);
    });
</script>