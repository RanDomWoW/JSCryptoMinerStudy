function validatePhone(element) {
	var pattern = new RegExp(/^\d{8,15}$/);
	var phone = $(element).val();
	phone = phone.replace(/\(/g, '').replace(/\)/g, '').replace(/-+/g, '').replace(/ +/g, '').replace(/\++/g, '');
	return pattern.test(phone);
}
$(document).ready(function(){
