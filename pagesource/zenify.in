<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&signed_in=true&libraries=places"></script>
<script>
	function initialize_autocomplete() {
		var input = document.getElementById('LocationSelect');
        var validPlace = false;
        var suggestionsTracker;
        var options = {
            componentRestrictions: {country: 'in'}
        };
        autocomplete = new google.maps.places.Autocomplete(input, options);

        function trackSuggestions(){
            if(typeof suggestionsTracker != 'undefined')
                clearInterval(suggestionsTracker);

            var maxTime = 2000; // 2 seconds
            var time = 0;
            suggestionsTracker = setInterval(function () {
                if($('.pac-container:visible').length) {
                    toggleSearch(false);
                    clearInterval(suggestionsTracker);
                }
                else {
                    toggleSearch(true);

                    if (time > maxTime) {
                        clearInterval(suggestionsTracker);   // tracking for too long, stop now
                        return;
                    }

                    time += 100; // not yet visible
                }
            }, 200);
        }
        function toggleSearch(doWarn, event){
            if(validPlace){
                $('#SearchBtn').attr('type', 'submit');
                doWarn = false;
            }
            else{
                $('#SearchBtn').attr('type', 'button');

                try{
                    event.preventDefault();
                    event.stopPropagation();
                }
                catch(err){}
            }
            var text = $('#LocationSelect').val();

            if(doWarn && (typeof text == 'string' && text.trim().length > 0)) $('.location-error').removeClass('hidden');
            else $('.location-error').addClass('hidden');
        }

		google.maps.event.addDomListener(input, 'keydown', function(e) {
            if(e.keyCode == 13) {
                if($('.pac-container:visible').length) {

                    if (e.type == "keydown") {
                        e.preventDefault();
                    }
                    if (!$('.pac-item-selected').length) {
                        google.maps.event.trigger(this, 'keydown', {keyCode: 40});
                        google.maps.event.trigger(this, 'keydown', {keyCode: 13, triggered: true});
                    }
                }
                else toggleSearch(true, e);
			}
            else{
                validPlace = false;
				if(e.keyCode == 8 || e.keyCode == 46)
                	trackSuggestions();
                else
                    clearInterval(suggestionsTracker);
            }
		});

        $('#LocationSelect').focus(function(){
            var text = $('#LocationSelect').val();
            if(typeof text == 'string' && text.trim().length > 0)
                trackSuggestions();
        });

		$('#SearchBtn').click(function(e) {
			var search_obj = autocomplete.getPlace();
			if(search_obj == undefined|| $('#LocationSelect').val().trim() == "" || Object.size(search_obj) < 2 ) {
                toggleSearch(true, e);
                return false;
			}
			var locationStr = $('#LocationSelect').val().replace(' ','-').split(',');
			var aptSearchStr = ''+$('.js_apt_search .selected-value').val()+'BHK-flats-for-rent-in-'+locationStr[0];
			$('.js_apt_search').attr('action','/'+aptSearchStr+'/search');
		});
		google.maps.event.addListener(autocomplete, 'place_changed', function() {
			var place = autocomplete.getPlace();
			if (!place.geometry) {
				return;
			}
            validPlace = true;
            toggleSearch(false);

            document.getElementById('latitude').value = place.geometry.location.lat();
			document.getElementById('longitude').value = place.geometry.location.lng();
			document.getElementById('place').value = place.name;
			document.getElementById('placeTypes').value = place.types.toString();

            document.getElementById('placeId').value = place.place_id;
            if(place.geometry.hasOwnProperty('viewport')) {
                document.getElementById('swLat').value = place.geometry.viewport.getSouthWest().lat();
                document.getElementById('swLng').value = place.geometry.viewport.getSouthWest().lng();
                document.getElementById('neLat').value = place.geometry.viewport.getNorthEast().lat();
                document.getElementById('neLng').value = place.geometry.viewport.getNorthEast().lng();
            }
            else {
                document.getElementById('swLat').value = "";
                document.getElementById('swLng').value = "";
                document.getElementById('neLat').value = "";
                document.getElementById('neLng').value = "";
            }
		});
	}
	document.addEventListener("DOMContentLoaded", function(event) {
		initialize_autocomplete();
	});
</script>
