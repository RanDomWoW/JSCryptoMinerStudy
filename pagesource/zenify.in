	function validateRentApart() {
		var errorFlag = 0;
		var validFlag = 0;
		$('.js-validate-empty').each(function(){
			$_this = $(this);
			if (!validateEmpty(this)) {
				$_this.closest('.js-valid-input').addClass('has-error');
				errorFlag=1;
			}
			else {
				$_this.closest('.js-valid-input').removeClass('has-error');
			}
		});
		var typeAheadInput = $("#rentApartment").find('.js-apartment-typeahead').eq(1);
		if ( typeAheadInput.val() == "" ) {
			typeAheadInput.closest('.js-valid-input').addClass('has-error');
			errorFlag=1;
		}
		else {
			typeAheadInput.closest('.js-valid-input').removeClass('has-error');
		}
		if (errorFlag == 0) {
			$('.js-confirm-error').hide();
		}
		else {
			$('.js-confirm-error').show();
			return false;
		}
		$('.js-validate-email').each(function(){
			$_this = $(this);
			if (!validateEmail(this)) {
				$_this.closest('.js-valid-input').addClass('has-error');
				validFlag=1;
			}
			else {
				$_this.closest('.js-valid-input').removeClass('has-error');
			}
		});
		$('.js-validate-phone').each(function(){
			$_this = $(this);
			if (!validatePhone(this)) {
				$_this.closest('.js-valid-input').addClass('has-error');
				validFlag=1;
			}
			else {
				$_this.closest('.js-valid-input').removeClass('has-error');
			}
		});
		if (validFlag == 0) {
			$('.js-validation-error').hide();
			return true;
		}
		else {
			$('.js-validation-error').show();
			return false;
		}
	}
	var complexList = new Bloodhound({
		datumTokenizer: Bloodhound.tokenizers.obj.whitespace('value'),
		queryTokenizer: Bloodhound.tokenizers.whitespace,
		prefetch: '/xhr/owner/search/a',
		remote: {
			url: '/xhr/owner/search/%QUERY',
			wildcard: '%QUERY',
			transform: function(response) {
				var tempArr = [];
				$.each(response, function(i, val) {
					var tempJson = {};
					tempJson.id=i;
					tempJson.value=val;
					tempArr.push(tempJson);
				});
	//                tempArr.sort(function(a, b){
	//                   return (a.value.localeCompare(b.value));
	//                });
				return tempArr;
			}
		}
	});

	$('.js-apartment-typeahead').typeahead(null, {
		name: 'id',
		display: 'value',
		limit: 40,
		source: complexList,
