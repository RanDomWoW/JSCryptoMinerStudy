    <script>
        //update this with your js_form selector 
        const form_id_js = "javascript_form";
        const data_js = {
            "access_token": "2hz7pzw6biw3hy9e5ye1efqb"
        };

        const js_onSuccess = () => { // remove this to avoid redirect 
            window.location = window.location.pathname + "?message=Email+Successfully+Sent%21&isError=0";
            alert('消息成功发送');
        }

        const js_onError = error => {
            // remove this to avoid redirect 
            window.location = window.location.pathname + "?message=Email+could+not+be+sent.&isError=1";
        }
        const sendButton = document.getElementById("js_send");
        const js_send = () => {
            sendButton.value = 'Sending…';
            sendButton.disabled = true;
            const request = new XMLHttpRequest();
            request.onreadystatechange = () => {
                if (request.readyState == 4 && request.status == 200) {
                    js_onSuccess();
                } else if (request.readyState ==
                    4) {
                    js_onError(request.response);
                }
            };
            const subject = document.querySelector("#" + form_id_js + " [name='subject']").value;
            const message = document.querySelector("#" + form_id_js + " [name='text']").value;
            data_js['subject'] = subject;
            data_js['text'] = message;
            const params = toParams(data_js);
            request.open("POST", "https://postmail.invotes.com/send", true);
            request.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
            request.send(params);
            return false;
        }
        sendButton.onclick = js_send;

        const toParams = data_js => {
            var form_data = [];
            for (const key in data_js) {
                form_data.push(encodeURIComponent(key) + "=" + encodeURIComponent(data_js[key]));
            }
            return form_data.join("&");
        }
        const js_form = document.getElementById(form_id_js);
        js_form.addEventListener("submit", e => e.preventDefault());
    </script>
