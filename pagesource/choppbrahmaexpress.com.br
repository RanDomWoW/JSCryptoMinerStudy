<script>
jQuery("#form-search-postcode").submit(function(e){
    consultaCepFranquiado();
    return false;
});

function consultaCepFranquiado(){
    var postcode = jQuery("#search-input-postcode").val();
    if (!postcode) { postcode = jQuery("#search-input-no-postcode").val() }
    postcode = postcode.replace(/[^0-9]/g, '');
    if(jQuery.type(postcode) === jQuery.type(undefined) || postcode === "" || postcode.length != 8){
        jQuery("#form-search-submit").addClass("disabled");
        jQuery("#form-search-no-postcode-submit").addClass("disabled");
        jQuery("#input-postcode-wrapper").addClass("invalid");
        setTimeout(function(){
            jQuery("#form-search-submit").removeClass("disabled");
            jQuery("#form-search-no-postcode-submit").removeClass("disabled");
        }, 2000);
    } else {
        var getUrl = window.location;
        var baseUrl = getUrl .protocol + "//" + getUrl.host + "/";
        var formData = jQuery('#form-search-postcode').serialize();
        if (jQuery('#form-search-postcode').parent().css('display') == 'none') { formData = jQuery('#form-search-no-postcode').serialize(); }
        jQuery.ajax({
            dataType: 'json',
            url: baseUrl + 'webjump_affiliate/consult',
            data: formData,
            type: 'POST',
            beforeSend: function() {
                jQuery("#search-input").hide();
                jQuery("#searching").show();
                jQuery("#search-postcode-no-postcode").hide();
            },
            success: function(data){
                //ERROR_CODE_POSTCODE_NOT_FOUND     = 100; //There is no postcode in correios' table
                //ERROR_CODE_AFFILIATE_NOT_FOUND    = 200; //There is no affiliate related to the postcode
                if(data.success){
                    jQuery("#searching").hide();
                    jQuery("#search-postcode-no-postcode").hide();
                    var html = data.neighborhood_name+ ', ' + data.city_name + ' / ' + data.uf;
                    jQuery('#address-found').html(html);
                    jQuery('#search-postcode-found').show().delay(1000);
                    window.location.href = 'http://choppbrahmaexpress.com.br/produtos/?franquia=' + data.url;
                } else if(data.error_code == 200){
                    jQuery("#searching").hide();
                    jQuery("#search-postcode-no-affiliate").show();
                    jQuery("#postcode-to-save").val(postcode);
                } else if (data.error_code == 100) {
                    jQuery("#searching").hide();
                    jQuery("#search-postcode-no-postcode").show();
                    jQuery("input[name='zip']").val(postcode);
                    jQuery("#input-no-postcode-wrapper").addClass("is-focused");
                    jQuery("#search-input-postcode").val('');
                    setTimeout(function(){
                        jQuery("button[type='button']").removeClass("disabled");
                    }, 2000);
                }
            }
        });
    }
}
