  </script>

<script type="text/javascript">
var msg_alert = new Object();
function dismiss_msg() {
  if (!$.isEmptyObject(msg_alert)) {
    (msg_alert.obj).hide();
    $('#surv_'+msg_alert.id+' footer h2').remove();
    $('#surv_'+msg_alert.id+' footer a').show();
    msg_alert = new Object();
  }
}

function msg_callback() {}
function sea_lis_top(sea_lc_ids, sea_lma_ids, sea_price_minimum, sea_price_maximum, sea_length_minimum, sea_length_maximum, sea_distance_mi, sea_pc_value, sea_sp_abbrev) {
  if ($('#skel-layers-search').html() == '') {
    $('body').append('<input type="hidden" id="sea_lc_ids" value="">');
    $('body').append('<input type="hidden" id="sea_lma_ids" value="">');
    $('body').append('<input type="hidden" id="sea_price_minimum" value="">');
    $('body').append('<input type="hidden" id="sea_price_maximum" value="">');
    $('body').append('<input type="hidden" id="sea_length_minimum" value="">');
    $('body').append('<input type="hidden" id="sea_length_maximum" value="">');
    $('body').append('<input type="hidden" id="sea_distance_mi" value="">');
    $('body').append('<input type="hidden" id="sea_pc_value" value="">');
    $('body').append('<input type="hidden" id="sea_sp_abbrev" value="">');
  }
  sea_form_submit_id=this.id;
  $('#sea_lc_ids').val(sea_lc_ids);
  $('#sea_lma_ids').val(sea_lma_ids);
  $('#sea_price_minimum').val(sea_price_minimum);
  $('#sea_price_maximum').val(sea_price_maximum);
  $('#sea_length_minimum').val(sea_length_minimum);
  $('#sea_length_maximum').val(sea_length_maximum);
  $('#sea_distance_mi').val(sea_distance_mi);
  $('#sea_pc_value').val(sea_pc_value);
  $('#sea_sp_abbrev').val(sea_sp_abbrev);
  sea_lis('', 'URL-Only');
}

function sea_lis_quick(sea_stock_number) {
  dismiss_msg();
  if ($('#skel-layers-search').html() == '') {
    $('body').append('<input type="hidden" id="sea_stock_number" value="">');
  }
  if (sea_stock_number != '') {
    $('#sea_stock_number').val($('#sea_stock_number_quick').val());
    sea_lis('', 'URL-Only');
    $('#quick_search_submit').hide();
    $('#quick_search footer li:first-child').append('<div class="fa_loading"><span class="fa fa-spinner fa-spin"></span></div>');
  }
  else {
    msg_alert.obj = alertify.alert('Please enter a 5-digit or 6-digit stock # to continue.', msg_callback, '');
    msg_alert.id = 1;
  }
}

</script>


