                case 'txtEmail':
                    if ($(this).val() == 'Email address') {
                        $(this).val("");
                    }
                    break;
                default:
                    break;
            }
        });

        $('.modInput').blur(function () {
            if ($(this).val() == "") {
                switch ($(this).attr('name')) {
                    case 'txtFirstName':
                        $(this).val('First name');
                        break;
                    case 'txtLastName':
                        $(this).val('Last name');
                        break;
                    case 'txtEmail':
                        $(this).val('Email address');
                        break;
                    default:
                        break;
                }
            }
        });

