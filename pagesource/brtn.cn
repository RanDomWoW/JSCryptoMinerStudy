                {{if item.is_ad}}
                    <li class="list-group-item list-group-item-ad" data-index="{{i+1}}" data-gid="{{item.ad_id}}" ad_track="1">
                        <a class="list-group-item-title" href="{{item.ad_material.url}}" title="{{item.ad_material.text}}">
                            <i class="icon-dot"></i><span class="list-group-item-desc">{{item.ad_material.text}}</span><span class="icon-ad"></span>
                        </a>
                    </li>
                {{else}}
                    <li class="list-group-item" data-index="{{i+1}}" data-gid="{{item.gid}}">
                        <a class="list-group-item-title single-truncate" href="{{item.open_url}}" title="{{item.data.title}}"><i class="icon-dot"></i>{{item.data.title}}</a>
                    </li>
                {{/if}}
            {{/each}}
        </ul></script>
