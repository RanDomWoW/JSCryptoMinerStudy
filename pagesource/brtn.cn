<script type="text/html" id="j-tpl-province"><ul class="nav nav-province">
        {{each list as value key}}
            {{if active == value.upper }}<li class="active">{{else}}<li>{{/if}}
                {{if value.unique }}
                    <a href="{{value.id | catUrl}}" data-type="province" data-unique="1" data-id="{{value.id}}">{{value.title | cutCityProvince}}</a>
                {{else}}
                    <a href="{{value.id | catUrl}}" data-type="province" data-id="{{value.id}}">{{value.title | cutCityProvince}}</a>
                {{/if}}
            </li>
        {{/each}}
    </ul></script>

<script type="text/html" id="j-tpl-city"><ul class="nav nav-city" data-province="{{province_code}}">
        <li>
            <a href="javascript:;" data-type="back" data-wrap="local">返回上一级</a>
        </li>
        {{if active == province_code }}<li class="active">{{else}}<li>{{/if}}
            <a href="{{province_code | catUrl}}" data-type="city" data-id="{{province_code}}">{{province_name | cutCityProvince}}</a>
        </li>
        {{each list as value key}}
        {{if active == value.id }}<li class="active">{{else}}<li>{{/if}}
            <a href="{{value.id | catUrl}}" data-type="city" data-id="{{value.id}}">{{value.title | cutCityProvince}}</a>
        </li>
        {{/each}}
    </ul></script>

<script type="text/html" id="j-tpl-btv-first"><ul class="nav nav-btv">
        {{each list as value key}}
        {{if active == value.upper }}<li class="active">{{else}}<li>{{/if}}
            {{if value.unique }}
                <a href="{{value.url}}" data-type="btv" data-unique="1" data-key="{{key}}" data-url="{{value.url}}">{{value.cname}}</a>
            {{else}}
                <a href="{{value.url}}" data-type="btv" data-key="{{key}}" data-url="{{value.url}}">{{value.cname}}</a>
            {{/if}}
        </li>
        {{/each}}
    </ul></script>

<script type="text/html" id="j-tpl-btv-next"><ul class="nav nav-btv-next" data-btv="{{btv_code}}">
        <li>
            <a href="javascript:;" data-type="back" data-wrap="btv">返回上一级</a>
        </li>
        {{each list as value key}}
        {{if active == value.cindex }}<li class="active">{{else}}<li>{{/if}}
            <a href="{{value.url}}" data-type="btv-next" data-url="{{value.url}}">{{value.cname}}</a>
        </li>
