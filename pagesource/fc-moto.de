  function GetPixiDeliveryPeriod(ProductID, WebPath, ProductDeliveryPeriod, ManufacturerDeliveryPeriod, StockLevelAlert, MinOrder, ProductStockAvailableText, ProductStockCriticalText, ProductLongerDeliveryTimeText, ProductNoAddToBasketText, ProductPreorderDataText, ProductNoStockDataText, ICProductIsAvailable, IsAvailable, IsVisible, IsNew, IsMaster, ICFormError){
    var BaseURL                       = "https://www.fc-moto.de";
    var AjaxURL                       = BaseURL + WebPath + "/stockstate_" + ProductID + ".json?PreventCache=" + Date.now();
    var JSONUrl                       = BaseURL + WebPath + "/stockstate_" + ProductID + ".json";
    var LowOnStockAction              = 3;
    var BContainer                    = jq('#BasketLink_' + ProductID);
    var BButton                       = jq('button[name=AddToBasket]', BContainer);
    var RButton                       = jq('button.ICBasketButton');
    var ICMinOrder                    = parseInt(MinOrder);

    if(ManufacturerDeliveryPeriod == ''){ return; } 
    if(ProductDeliveryPeriod == ''){ ProductDeliveryPeriod = '2 - 3'; }

    if(JsonDataUrl == JSONUrl){ 
      if(typeof BButton.attr('data-productid') === 'undefined') {
        ChangeDeliveryPeriod(ProductID, ProductDeliveryPeriod, ManufacturerDeliveryPeriod, ProductStockAvailableText, ProductStockCriticalText, ProductLongerDeliveryTimeText, ProductNoAddToBasketText, ProductPreorderDataText, ProductNoStockDataText, LowOnStockAction, ICProductIsAvailable, IsAvailable, IsNew, StockLevelAlert, ICMinOrder, JsonData);        
        ChangeTrafficLight(ProductID, JsonData.AvailableStock, StockLevelAlert, ICProductIsAvailable, ICMinOrder);  
        if(typeof RButton !== 'undefined' && RButton.length > 0) {
            ChangeRefreshButton(RButton, ProductID, ICProductIsAvailable, IsNew, JsonData.AvailableStock, LowOnStockAction, ICMinOrder);
        }
      } else {
        ChangeDeliveryPeriod(ProductID, ProductDeliveryPeriod, ManufacturerDeliveryPeriod, ProductStockAvailableText, ProductStockCriticalText, ProductLongerDeliveryTimeText, ProductNoAddToBasketText, ProductPreorderDataText, ProductNoStockDataText, LowOnStockAction, ICProductIsAvailable, IsAvailable, IsNew, StockLevelAlert, ICMinOrder, JsonData);        
        ChangeTrafficLight(ProductID, JsonData.AvailableStock, StockLevelAlert, ICProductIsAvailable, ICMinOrder);        
        ChangeMicroDataAvailability(ProductID, JsonData.AvailableStock, StockLevelAlert, ICMinOrder, LowOnStockAction, ICProductIsAvailable, IsNew);
        ChangeBasketButton(ProductID, LowOnStockAction, ICProductIsAvailable, IsNew, ICMinOrder, IsAvailable, IsVisible, IsMaster, 0, JsonData);
      }    
    
      return;
    }

    jQuery.ajax({      
      type: 'GET',
      async: false,
      url: AjaxURL,
      dataType: 'json',
      success: function(data) {
        JsonData = data;
        JsonDataUrl = JSONUrl;         
        
        if(typeof BButton.attr('data-productid') === 'undefined') {
          ChangeDeliveryPeriod(ProductID, ProductDeliveryPeriod, ManufacturerDeliveryPeriod, ProductStockAvailableText, ProductStockCriticalText, ProductLongerDeliveryTimeText, ProductNoAddToBasketText, ProductPreorderDataText, ProductNoStockDataText, LowOnStockAction, ICProductIsAvailable, IsAvailable, IsNew, StockLevelAlert, ICMinOrder, data);        
          ChangeTrafficLight(ProductID, data.AvailableStock, StockLevelAlert, ICProductIsAvailable, ICMinOrder);       
          if(typeof jq('span.Refresh') !== 'undefined' && RButton.length > 0) {
              ChangeRefreshButton(RButton, ProductID, ICProductIsAvailable, IsNew, data.AvailableStock, LowOnStockAction, ICMinOrder);
