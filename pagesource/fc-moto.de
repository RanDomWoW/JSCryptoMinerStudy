  function ChangeBasketButton(ProductID, LowOnStockAction, ICProductIsAvailable, IsNew, MinOrder, IsAvailable, IsVisible, IsMaster, ToHide, Pixi) {
    var BContainer                    = jq('#BasketLink_' + ProductID);
    var BButton                       = jq('button[name=AddToBasket]', BContainer);       
    
    if(ToHide) {    
      if(IsMaster == '1') {
        BContainer.addClass('HideElement');      
      } else {
        if(IsAvailable == '1' && IsVisible == '1') {
          if(Pixi.AvailableStock < MinOrder) {
            if(LowOnStockAction == '0') {
                BContainer.removeClass('HideElement');                
            } else if(LowOnStockAction == '2') {
                if(ICProductIsAvailable == 'Y') {
                    BContainer.removeClass('HideElement'); 
                } else {
                    BContainer.addClass('HideElement');    
                }
            } else { 
              if(ICProductIsAvailable == 'Y') {
                BContainer.removeClass('HideElement');  
              } else {
                BContainer.addClass('HideElement');
              }              
            }        
          } else {
            BContainer.removeClass('HideElement');    
          }    
        } else {
          BContainer.addClass('HideElement'); 
        }
      }
    } else {
      if(IsMaster == '1') {
        BContainer.addClass('Disabled');
        BButton.addClass('Disabled');
        BButton.attr('disabled', 'disabled'); 
      } else {
        if(IsAvailable == '1' && IsVisible == '1') {
          if(Pixi.AvailableStock < MinOrder) {
            if(LowOnStockAction == '0') {
                BContainer.removeClass('Disabled');
                BButton.removeClass('Disabled');
                BButton.removeAttr('disabled');                     
            } else if(LowOnStockAction == '2') {
                if(ICProductIsAvailable == 'Y') {
                    BContainer.removeClass('Disabled');
                    BButton.removeClass('Disabled');
                    BButton.removeAttr('disabled');  
                } else {
                    BContainer.addClass('Disabled');
                    BButton.addClass('Disabled');
                    BButton.attr('disabled', 'disabled');    
                }                  
            } else { 
              if(ICProductIsAvailable == 'Y') {
                BContainer.removeClass('Disabled');
                BButton.removeClass('Disabled');
                BButton.removeAttr('disabled');     
              } else {
                BContainer.addClass('Disabled');
                BButton.addClass('Disabled'); 
                BButton.attr('disabled', 'disabled');   
              }              
            }        
          } else {
            BContainer.removeClass('Disabled');
            BButton.removeClass('Disabled');
            BButton.removeAttr('disabled');        
          }    
        } else {
          BContainer.addClass('Disabled'); 
          BButton.addClass('Disabled'); 
          BButton.attr('disabled', 'disabled');   
        }      
      }       
    }    
   
    return;  
  }
  
  function ChangeTrafficLight(ProductID, ProductStock, StockLevelAlert, ICProductIsAvailable, MinOrder){
    var TraficLight = "";
    var IContainer  = jq('#DeliverPeriodDisplay_' + ProductID + ' p i'); 
    
    if(ProductStock < MinOrder){ 
      if(ICProductIsAvailable == 'Y') {
          TraficLight = "ProductWarnStockIcon"; 
      } else {
        TraficLight = "ProductOutStockIcon"; 
      }
    }else if(StockLevelAlert != -1 && ProductStock < StockLevelAlert){
      TraficLight = "ProductWarnStockIcon";    
    }else{
      TraficLight = "ProductOnStockIcon";          
    } 
    
    IContainer.removeClass("ProductOutStockIcon");
    IContainer.removeClass("ProductWarnStockIcon"); 
    IContainer.removeClass("ProductOnStockIcon");
    IContainer.addClass(TraficLight);
    
    return;
  }  
        
  function ChangeDeliveryPeriod(ProductID, ProductDeliveryPeriod, ManufacturerDeliveryPeriod, ProductStockAvailableText, ProductStockCriticalText, ProductLongerDeliveryTimeText, ProductNoAddToBasketText, ProductPreorderDataText, ProductNoStockDataText, LowOnStockAction, ICProductIsAvailable, IsAvailable, IsNew, StockLevelAlert, MinOrder, PixiData ){
    var PContainer    = jq('#DeliverPeriodDisplay_' + ProductID + ' p');
    var AvaibilyText  = '<i class="Icon"></i> ';

    if(typeof PixiData.AvailableStock === "undefined"){ return; }
    //alert(LowOnStockAction + " " + PixiData.AvailableStock + " " + MinOrder + " " + ProductID);
    
    if(ProductStockAvailableText.indexOf(ManufacturerDeliveryPeriod)) {
      //alert(ProductStockAvailableText + ManufacturerDeliveryPeriod+ProductDeliveryPeriod);
      ProductStockAvailableText = ProductStockAvailableText.replace(ManufacturerDeliveryPeriod, ProductDeliveryPeriod);      
    }    

    if(PixiData.AvailableStock < MinOrder){
      if(LowOnStockAction == '0') {
          if(IsAvailable == '0') {
              AvaibilyText += ProductNoAddToBasketText;
          } else {
              AvaibilyText += ProductLongerDeliveryTimeText;
          }          
      } else if(LowOnStockAction == '2') {
          if(ICProductIsAvailable == 'Y') {
              AvaibilyText += ProductLongerDeliveryTimeText; 
          } else if(IsNew == '1') {
              AvaibilyText += ProductPreorderDataText;
          } else {
              AvaibilyText += ProductNoAddToBasketText;   
          }                     
      } else { 
        if(ICProductIsAvailable == 'Y') {
          AvaibilyText += ProductStockAvailableText.replace(ProductDeliveryPeriod, ManufacturerDeliveryPeriod);   
        } else if(IsNew == '1') {
          AvaibilyText += ProductPreorderDataText;
        } else {
          AvaibilyText += ProductNoAddToBasketText;
        }              
      }   
    } else {
      if(PixiData.InternalStock >= MinOrder) {
        if(PixiData.AvailableStock <= StockLevelAlert) {
          AvaibilyText += ProductStockCriticalText;        
        } else {
          AvaibilyText += ProductStockAvailableText;
        }              
      } else {
        AvaibilyText += ProductStockAvailableText.replace(ProductDeliveryPeriod, ManufacturerDeliveryPeriod);      
      }
    }
    
    //alert(PixiData.InternalStock + ' _ ' + ProductStockAvailableText);
    
    PContainer.html(AvaibilyText);
    
    return;  
  }

