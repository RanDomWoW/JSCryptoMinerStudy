<script type="text/javascript">

var teaserRunning = 0;

function getGParam(pname)  {
  pname = pname.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]") ;
  var rx = new RegExp("[\\?&]"+pname+"=([^&#]*)") ;
  var results = rx.exec(window.location.href) ;
  if(results == null) { return ""; } else return results[1] ;
}  

function showTeaser(ts, stopRunning) {
  document.getElementById('ts-paket').style.visibility = 'hidden' ;
  document.getElementById('ts-paket').style.opacity = '0' ;
  document.getElementById('ts-rechnung').style.visibility = 'hidden' ;
  document.getElementById('ts-rechnung').style.opacity = '0' ;
  document.getElementById('ts-buchhaltung').style.visibility = 'hidden' ;
  document.getElementById('ts-buchhaltung').style.opacity = '0' ;
  document.getElementById('ts-verein').style.visibility = 'hidden' ;
  document.getElementById('ts-verein').style.opacity = '0' ;
  document.getElementById('li-paket').className = '' ;
  document.getElementById('li-rechnung').className = '' ;
  document.getElementById('li-buchhaltung').className = '' ;
  document.getElementById('li-verein').className = '' ;
  document.getElementById('ts-'+ts).style.visibility = 'visible' ;
  document.getElementById('ts-'+ts).style.opacity = '1' ;
  document.getElementById('li-'+ts).className = 'current' ;
  if (stopRunning === 1) clearInterval(teaserRunning) ;
  return false ;
}

function switchNext() {
  var nxt = '' ; 
  if (document.getElementById('ts-paket').style.visibility == 'visible') { nxt = 'buchhaltung' } else
  if (document.getElementById('ts-buchhaltung').style.visibility == 'visible') { nxt = 'rechnung' } else
  if (document.getElementById('ts-rechnung').style.visibility == 'visible') { nxt = 'verein' } else
  { nxt = 'paket' } 
  if (nxt != '') showTeaser(nxt, 0) ;  
  return true ;
}

</script>
