	{
		alert("The E-mail field is required.");
		enewsletterForm.eemail.focus();
		return (false);
	}
	if (!enewsletterForm.eemail.value.match( /^[_a-zA-Z0-9.-]+@([_a-zA-Z0-9-]+\.)+[a-zA-Z0-9]{2,3}$/ ) )
	{
		alert("The email format is invalid.");
		enewsletterForm.eemail.focus();
		return (false);
	} 
	var interest = enewsletterForm.einterest;
	var error_interest = true;
	for( ic=0; ic<interest.length; ic++ ){
		if( interest[ic].checked == true ) {
			error_interest = false;
			break;
		}
	}
	if( error_interest == true ){
			alert("Please select which ATP Product lines may interest you.");
			interest[0].focus();
			return (false);
	}
	var othersolution = enewsletterForm.esolution;
	var othersolutioncheck = false;
	if( othersolution[6].checked == true ) {
		othersolutioncheck = true;
	}
	if(othersolutioncheck == true){
		if (enewsletterForm.esolother.value.length == 0) {
			alert("Please input the other industry solution.");
			enewsletterForm.esolother.focus();
			return false;
		}
	}
	if (enewsletterForm.ePostAuthCode.value.length == 0) {
		alert("The Captcha field is required.");
		enewsletterForm.ePostAuthCode.focus();
		return false;
	}
}
