}
.main-small-navigationnbt ul ul a {
padding-left: 60px;
}
.main-small-navigationnbt ul ul ul a {
padding-left: 80px;
}
}
h2.date-header {
margin: 10px 0px;
display: none;
}
.main .widget {
margin: 0 0 5px;
padding: 0 0 2px;
}
.main .Blog {
border-bottom-width: 0;
}
#header .description {
}
