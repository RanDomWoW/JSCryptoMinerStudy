<script>
$(function () {
	$("#ftw").bind("load", function() {
		var footerHeight = 0;
		var footerTop = 0;
		positionFooter();
		function positionFooter() {
			footerHeight = $("#ftw").height();
			footerTop = ($("#ftw").scrollTop()+$("#ftw").height()-footerHeight)+"px";
			//如果页面内容高度小于屏幕高度，div#footer将绝对定位到屏幕底部，否则div#footer保留它的正常静态定位
			if(($(document.body).height()+footerHeight) < $("#ftw").height()) {
				$("#ftw").css({position: "absolute",left:"0"}).stop().css({top:footerTop});
			}
		}
		$("#ftw").scroll(positionFooter).resize(positionFooter);
	});


	$(".cuet_im").mousemove(function () {
		$(".cuet").removeClass("hide").siblings().addClass("hide");
	});
	$(".cuet").mouseout(function () {
		$(".cuet_im").removeClass("hide").siblings().addClass("hide");
	});

	$(".back_top").mouseover(function () {
		$(".ret_top").removeClass("hide").siblings().addClass("hide");
	});
	$(".ret_top").mouseout(function () {
		$(".ret_top").addClass("hide").siblings().removeClass("hide");
	});
})
</script>
