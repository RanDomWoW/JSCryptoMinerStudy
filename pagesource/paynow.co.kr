			$('.btn-cs, .btn-sns').bind( "click", function(e) {
				var $target = $(e.target),
					$html = $('html'),
					$btn = $(this);

				if($html.is('[class*="dim"]')) {
					if($html.attr('class') == $btn.data('target')) {
						$html.removeClass('dim-cs')
							.removeClass('dim-sns');
						return false;
					}
				}

				$('header .sect-util i.dim').on('click', function() {
					$html.removeClass('dim-cs')
						.removeClass('dim-sns');
				});

				if(($target.parent()[0] == $('.btn-cs')[0] || $target[0] == $('.btn-cs')[0]) || ($target.parent()[0] == $('.btn-sns')[0] || $target[0] == $('.btn-sns')[0])) {
					$html.removeClass('dim-cs')
						.removeClass('dim-sns');
					$html.addClass($btn.data('target'));

					return false;
				}

				if($html.is('[class*="dim"]')) {
					$html.removeClass('dim-cs')
						.removeClass('dim-sns');
				} else {
					$html.addClass($btn.data('target'));
				}
			});

			$('header nav').find('.active')
				.attr('data-active', 'active');

			$('header nav a')
				.on('focus',  function() {
					$(this).parents('li').addClass('active focus');
				})
				.focusout( function() {
					$(this).parents('li').removeClass('focus')
						.filter(':not([data-active="active"])').removeClass('active');
				});
			$('header nav ul ul').hover( function() {
				$(this).parents('li').toggleClass('active');
			});
			$('.wrap-title button').on( "click", function() {
				var $menu = $('.menu-2dep'),
					$doc = $(document);

				$menu.toggleClass('active');

				$doc.on("click.depth-menu touchstart.depth-menu", function(e){
					var _target = e.target;
					if(_target != $('.wrap-title button')[0] && $(_target).parents('.menu-2dep').length <= 0) {
						$menu.removeClass('active');
						$doc.off("click.depth-menu touchstart.depth-menu");
					}
				});
			});

