//Add click events for BreadCrumbs
function loadUrl(thisURL) {
	//window.location.hash=thisURL;
	$('div#content').load(thisURL, function() {//use CallBack load breadCrumbs
		htmlobj = $.ajax({
			url : "htmlcnt/showbreadcrumbs",
			async : false,
			cache : false
		});//BreadCrumbs
		$("div#bradecurmbs").html(htmlobj.responseText);
	});
}

//Add click events for BreadCrumbs
function clickUrl(thisURL) {
	//window.location.hash=thisURL;
	$('div#contentBody').load(thisURL, function() {//use CallBack load breadCrumbs
		htmlobj = $.ajax({
			url : "htmlcnt/showbreadcrumbs",
			async : false,
			cache : false
		});//BreadCrumbs
		$("div#bradecurmbs").html(htmlobj.responseText);
	});
}

//Add security URL
function openWithLoginURL(srcURL) {
	var securityURL = "";
	$.ajax({
		type : "post",
		url : "system/security/url",
		dataType : "text",
		cache : false,
		async : false,

		success : function(data) {
			if (srcURL.indexOf("http://") == -1) {
				srcURL = "http://" + srcURL
			}
			if ("" != data) {
				window.open(srcURL + data);
			} else {
				window.open(srcURL);
			}
		}
	});
}

function submitFullTextSearch() {
	var searchUrl = $("#full_text_search_host").val();
	$.ajax({
		type : "post",
		url : "system/security/url",
		dataType : "text",
		async : false,

		success : function(data) {
			if ((searchUrl.indexOf("http://") == -1)) {
				searchUrl = "http://" + searchUrl;
			}
			if ("" != data) {
				searchUrl = searchUrl + data;
			}
		}
	});
	$("#fullTextSearchFrm").attr('action', searchUrl);
	$("#fullTextSearchFrm").submit();
}

function checkRdoFunc(type){
	if(type=="gcDiv"){
		$("#index").val("book");
		$("#gcDiv").addClass('checked');
		$("#wzDiv").removeClass('checked');
	}else{
		$("#index").val("web");
		$("#gcDiv").removeClass('checked');
		$("#wzDiv").addClass('checked');
	}
}

var ie = navigator.appName == "Microsoft Internet Explorer" ? true : false;

function keyDown(e) {
	if (!ie) {
		var nkey = e.which;
		if (nkey == 13) {
			login();
			return false;
		}
	}
	if (ie) {
		var iekey = event.keyCode;
		if (iekey == 13) {
			login();
			return false;
		}
	}
}

function removeKeyDown(e) {
	if (!ie) {
		var nkey = e.which;
		if (nkey == 13) {
			return null;
		}
	}
	if (ie) {
		var iekey = event.keyCode;
		if (iekey == 13) {
			return null;
		}
	}
}
</script>

