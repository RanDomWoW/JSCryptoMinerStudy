            $('#az-contents').load(portal_url + '/az_catalog');
            $('#home-sections').load('/home_sections', function() {

                $('.home-panel').on('click', function(e) {
                    if ($(e.target).is('button')) {
                        e.preventDefault();
                        return;
                    }
                    window.location.href = $(this).attr('rel');
                });
            });
        });


        if ($("body").innerWidth() > 767) {
            $("article>.row").each(function() {
                var height = 0;
                $(".home-panel", this).each(function() {
                    if ($(this).innerHeight() > height) {
                        height = $(this).innerHeight();
                    }
                });

                $(".home-panel", this).height(height - 20);
            });
        }
    </script></body>

