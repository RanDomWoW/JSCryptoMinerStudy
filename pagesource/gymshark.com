<script>
  $(document).ready(function() {
    /* GEOIP LOOKUP (if required) */
    if ($.cookie("GSBlogCountry")) {
      var _countryName = $.cookie("GSBlogCountry");
      
      $("#mce-MMERGE5").val(_countryName);
      
    } else {

      // Use new JS GeoIP service to get Country
      var geoip = new GeoIPService();
      geoip.countryLookup();
      setTimeout(function() {
        
        $("#mce-MMERGE5").val(geoip.getCountryName());
        

        $.cookie('GSBlogCountry', geoip.getCountryName(), { expires: 1 })
      }, 1500);

    }

    /* Close newsletter popup */
    $(document).on("click", ".footer__newsletter-popup-close", function(e) {
      e.preventDefault();
      $(".footer__newsletter-popup, .newsletter-popup-overlay").addClass("visually-hidden") ;
    });

    /* Merge Day/Month/Year fields into 1 and assign to hidden DOB field */
    $(document).on("click", ".footer__newsletter-popup-submit", function(e) {

      var day = $("#birthday-block-day").val();
      var month = $("#birthday-block-month").val();
      var year = $("#birthday-block-year").val();

      if (day != null && month != null && year != null) {
        var dob = month + '/' + day + '/' + year;
        $("#mce_DOB").val(dob);
      }

    });

    /* Check email has been provided and show additional data popup */
    $(document).on("click", "#mc-embedded-subscribe", function(e) {
      e.preventDefault();
      var email = $("#mce-EMAIL").val();
      if (email != '' && ValidateEmail(email)) {

        $(".mce_email_field_error").html('');

        $("#mce_FNAME, #mce_LNAME, #mce_DOB").val('').removeClass("mce_inline_error");
        $("#birthday-block-day, #birthday-block-month, #birthday-block-year, #mce_GENDER").val('').removeClass("mce_inline_error").trigger("change");
        $("#mce-error-response, #mce-success-response").html('').hide();

        $("#mc_hidden_email").val(email);
        $(".footer__newsletter-popup, .newsletter-popup-overlay").removeClass('visually-hidden');
      } else {
        $(".mce_email_field_error").html('Please enter a valid email address');
      }
    });

    function ValidateEmail(email)
    {
      if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(email))
      {
        return (true)
      }
      return (false)
    }

  });
</script>
