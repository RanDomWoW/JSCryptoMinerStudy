  $(document).ready(function() {

    // If product detail page, update add to cart button label on click
    // Once all cart requests are complete, update the item counter
    $(document).on('cart.requestComplete', function(event, cart) {
      $(".cart-header__counter").html(cart.item_count);});

    // General AJAX success handler (add, remove, change qty)
    $(document).ajaxSuccess(function(event, jqxhr, settings) {

      if(settings.url == "/cart/change.js") {
        rebuildMiniCart();
      }if(settings.url == "/cart/add.js") {

        rebuildMiniCart();

        var variantID = settings.data.split("&")[0].split("=")[1];
        var parent = miniCartSelectors.quickBuyHome;

        $("[data-cart-add='" + variantID + "']").closest(parent).find('.full-tick-anim').remove();
        $("[data-cart-add='" + variantID + "']").closest(parent).append('<div class="full-tick-anim drawn"><div class="trigger"></div><svg version="1.1" id="tick" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 37 37" style="enable-background:new 0 0 37 37;" xml:space="preserve"><path class="circ path" style="fill:none;stroke:#000000;stroke-width:3;stroke-linejoin:round;stroke-miterlimit:10;" d="M30.5,6.5L30.5,6.5c6.6,6.6,6.6,17.4,0,24l0,0c-6.6,6.6-17.4,6.6-24,0l0,0c-6.6-6.6-6.6-17.4,0-24l0,0C13.1-0.2,23.9-0.2,30.5,6.5z"	/><polyline class="tick path" style="fill:none;stroke:#000000;stroke-width:3;stroke-linejoin:round;stroke-miterlimit:10;" points="11.6,20 15.9,24.2 26.4,13.8 "/></svg><span style="display:none;">Added to Cart</span></div>');

        resetLimitedQuickBuy(variantID);

		$(".quickBuySizeInput").trigger("blur");
        
        window.setTimeout(function () {
          $(".trigger").toggleClass("drawn");
          $(".full-tick-anim span").hide().fadeIn(1000);
        }, 250);

        window.setTimeout(function () {
          $(".full-tick-anim").fadeOut();
        }, 2500);
      }});

    // Handle errors when adding to cart
    $(document).ajaxError(function(event, jqxhr, settings, thrownError) {
      if(settings.url == "/cart/add.js") {
        var response = JSON.parse(jqxhr.responseText);

        var message = response.description;

        // Trying to add more into the cart than available
        if (message.indexOf('You can only add') != -1) {
          var count = message.split('You can only add')[1].split('to the cart')[0].trim();
          var message = 'Sorry, you can only add a further {{ count }} into your cart'.replace('\{\{ count \}\}', count);
        }
        if (message.substring(0, 3) === "All") {
          var product = message.substring(3).split('are in your cart')[0].trim().split(" ");
          product.shift();
          product = product.join(" ");
          var message = 'Sorry, you already have all available {{ product }} in your cart'.replace('\{\{ product \}\}', product);
        }

        // Show modal with error message
        $(".country-select-ui-ajax-error").remove();
        $("body").append('<div class="country-select-ui-ajax-error">\
          <div class="country-select-box">\
          <h3>' + message + '</h3><button class="btn btn--primary">OK</button>\
          </div>\
          </div>\
          </div>');
        $(".country-select-ui-ajax-error").show();

        $(miniCartSelectors.addToCart).prop('disabled', false);
        $(miniCartSelectors.addToCartText).html(theme.strings.addToCart);
      }
    });

  });

  // Reload mini cart by calling the cart with a view of "minicart"
  // Replace the current form with the form from the response
  // Pass item count to localCart function
  function rebuildMiniCart(open) {
      $.get("/cart?view=minicart", function(data) {
        $("#minicart form").replaceWith($(data).find("form"));
        localCart(CartJS.cart.item_count);
        if (open) {
          $("#minicart").data( "mmenu" ).open();
        }
      });
  }

  // Store cart in local storage if NOT the US store
  function localCart(items) {}

  // Reset limited collection / HP quick buy
  function resetLimitedQuickBuy(variantID) {

    var target = $(".quickBuySizeInput[data-cart-add='" + variantID + "']");
    var isLimited = $(target).data("limited");

    if (isLimited == true) {
        $(target).parents(".quickBuyButtons").html('<div class="twelve columns quickBuySizeContainerNone"><button class="button btn btn--full quickBuySizeInput disabled" onclick="window.location=\'/products/rfdtest-nikkib-earlyaccess\';">' + theme.strings.already_in_cart + '</div>');
    }
  }


  // Reset the product UI if limited product has been removed
  function resetLimitedProduct(cart) {

    var isLimited = 'false';
    var product = '';

    // If the product is limited to 1 per order, and the "data-taken" attribute is true
    if (isLimited == 'true') {
      if ($("[data-taken='true']").length) {

        // Check if the current product is still in the cart. If not, reset the UI so it's available again
        var inCart = false;
        $.each(cart.items, function(index, obj){
          if (obj.product_id == product) {
            inCart = true;
          }
        });

        // If the product is no longer in the cart, reset the page UI
        if (!inCart) {
          console.log("resetting ui");
          $(miniCartSelectors.addToCart).attr("data-taken", "false").prop("disabled", false);
          $('[data-single-option-selector]').val('').trigger("change");
          $(".selector-buttons li").removeClass("selected");
          $("#featureimage .quick-buy-options ul.variants li.limited-already-taken").hide();
          $("#featureimage .quick-buy-options ul.variants li:not(.limited-already-taken)").show();
        }
      }
    }
  }

  $(document).on("click", ".country-select-ui-ajax-error button", function(e) {
    e.preventDefault();
    $(".country-select-ui-ajax-error").remove();
  });

  // Update cart when product added via Foursixty
  window.onFoursixtyCartUpdated = rebuildMiniCart;
</script>

  
