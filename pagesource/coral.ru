<script type="text/html" id="nightsSelector-template">
    <div class="search-panel_dropdown" data-bind="dropdown: { key: 'searchDetailsNights', role: 'container'}">
        <a class="search-panel_action-link" href="javascript:void(0);" data-bind="click: customNightsSelectedTab, dropdown: { key: 'searchDetailsNights', role: 'openBtn'}">
            на
            <span data-bind="text: nightsFrom"></span>
            <!-- ko if: nightsFrom() != nightsTo() -->
            <span> - </span>
            <span data-bind="text: nightsTo"></span>
            <!-- /ko -->
            <!-- ko text: localize('nights', nightsTo) --><!-- /ko -->
        </a>
        <div class="search-panel_dropdown-menu">
            <div class="dropdown-menu period-dropdown">
                <div class="dropdown-menu_arrow period-dropdown_arrow"></div>
                <!--ko foreach: nightCombosList-->
                <div class="period-dropdown_option">
                    <label class="radio __period" data-bind="css: {'__selected': $root.nightsFromSelected() == unwrap(nightsFrom) && $root.nightsToSelected() == unwrap(nightsTo) && !$root.customNightsSelected()}">
                        <input type="radio" class="radio_control" value="1" name="period_opt" data-bind="click: $root.setNights.bind($root, $data)">
                        <span class="radio_label">
                            <!--ko text: nightsFrom--><!--/ko--><!-- ko if: nightsFrom() != nightsTo() --> - <!--ko text: nightsTo--><!--/ko--><!--/ko--> <!-- ko text: localize('nights', nightsTo) --><!-- /ko -->
                        </span>
                    </label>
                </div>
                <!--/ko-->
                <div class="period-dropdown_option">
                    <label class="radio __period" data-bind="click: function() { customNightsSelected(true) }, css: {'__selected': customNightsSelected }">
                        <input type="radio" class="radio_control" name="period_opt"><span class="radio_label">Произвольный период</span>
                    </label>
                </div>
                <!--ko if: customNightsSelected-->
                <span class="period-dropdown_tip">Количество ночей</span>
                <div class="persons-dropdown_row">
                    <div class="persons-dropdown_option">
                        <label class="persons-dropdown_label">От</label>
                        <div class="select __mini">
                            <select data-bind="selectric: {}, value: $root.nightsFromSelected, options: Array.apply(0, Array(30)).map(function (x, y) { return y + 1; })"></select>
                        </div>
                    </div>
                    <div class="persons-dropdown_option">
                        <label class="persons-dropdown_label">До</label>
                        <div class="select __mini">
                            <select data-bind="selectric: {}, value: $root.nightsToSelected, options: Array.apply(0, Array(8)).map(function (x, y) { return $root.nightsFromSelected() + y; })"></select>
                        </div>
                    </div>
                </div>
                <!--/ko-->
                <button type="button" class="btn period-dropdown_apply-btn __blue" data-bind="click: applyNights, dropdown: {key: 'searchDetailsNights', role: 'closeBtn'}">Применить</button>
            </div>
        </div>
    </div>
</script><script type="text/html" id="priceSelector-template">
    <div class="search-panel_field-select search-panel_dropdown" data-bind="dropdown: {key: 'priceSelector', role: 'container'}">
        <a href="javascript:void(0);" class="search-panel_action-link" data-bind="dropdown: { key: 'priceSelector', role: 'openBtn' }">
            <!--ko priceText: minPrice--><!--/ko--> —
            <!--ko if:!infiniteMaxPrice()--><!--ko priceText: maxPrice--><!--/ko--><!--/ko-->
            <span class="search-panel_inf" data-bind="visible: infiniteMaxPrice"></span>
            <i data-bind="css: currencyCss"></i>
        </a>
        <div class="search-panel_dropdown-menu">
            <div class="dropdown-menu price-dropdown __bordered">
                <div class="dropdown-menu_arrow price-dropdown_arrow"></div>
                <div class="price-dropdown_row">
                    <div class="price-dropdown_range" data-bind="slider: { name: 'tourSearchPriceFilter', mode: 'range', elementType: 'container', min: priceSelector.minValue, max: priceSelector.maxValue, margin: priceSelector.priceMargin, step: priceSelector.priceStep, withInfiniteMaxValue: true, infiniteValueReached: priceSelector.infiniteMaxPrice }">
                        <div class="price-dropdown_range-inputs">
                            <input type="text" class="input-1 __medium" data-bind="slider: { name: 'tourSearchPriceFilter', value: priceSelector.minPrice, elementType: 'minvalueinput' }"> &mdash;
                            <span class="input-1 __medium __inf" data-bind="visible: priceSelector.infiniteMaxPrice"></span><input type="text" class="input-1 __medium" data-bind="visible: !priceSelector.infiniteMaxPrice(), slider: { name: 'tourSearchPriceFilter', value: priceSelector.maxPrice, elementType: 'maxvalueinput'}">&nbsp&nbsp<i class="price-dropdown_currency" data-bind="css: $root.currencyCss"></i>
                        </div>
                        <div class="price-dropdown_range-inputs">
                            <div class="price-dropdown_range-slider range-slider" data-bind="slider: { name: 'tourSearchPriceFilter', elementType: 'slider', showPips: true, pipsDensity: 4 }"></div>
                        </div>
                    </div>
                </div>
                <div class="price-dropdown_row">
                    <label class="checkbox">
                        <input type="checkbox" class="checkbox_control" data-bind="checked: priceSelector.showDiscount" autocomplete="off"><span class="checkbox_box"></span><span class="checkbox_label">Со скидкой</span>
