body {font: normal normal 12px Georgia, Utopia, 'Palatino Linotype', Palatino, serif;color: #000000;background: #eeeeee none repeat scroll top left;}
a:link {text-decoration:none;color: #0000f2;}
a:visited {text-decoration:none;color: #bb3131;}
a:hover {color: #4cac4c;}
.color {color: #9900ff;}
.border {border-color: #9900ff;}
.bg {background-color: #9900ff;}
.header-bg {background-color:#ff9900;}
.wrapper {background-color:#ffffff;max-width: 100%;margin: auto;}
.post-body {font: normal normal 21px Calibri;}
.main-menu {border-top: 1px solid #9900ff;}
.main-menu ul.sub-menu li:hover > a {border-left: 2px solid #9900ff;}
.main-menu .menu-item-mega > .menu-item-inner > .sub-menu > li li:hover a {border-left: 1px solid #9900ff;}
.main-menu ul.sub-menu li:hover > a,
.main-menu .menu-item-mega > .menu-item-inner > .sub-menu,
.main-menu .menu-item-mega > .menu-item-inner > .sub-menu > li li:hover a {border-color: #9900ff!important;}
.header-social-icons ul li a:hover {color: #9900ff;}
.feed-widget-header .widget-title {font: italic 700 20px Paytone One;}
.slider-item .item-readmore:hover {background: #9900ff}
.owl-dot.active {background: #9900ff;}
#sidebar .widget > h2, #sidebar .feed-widget-header, #sidebar .feed-widget-header h2 {font: italic 700 14px Paytone One;}
#sidebar .widget.FollowByEmail .follow-by-email-submit {background: #9900ff;}
#footer .widget.social_icons li a:hover {color: #9900ff;}
#footer .FollowByEmail .follow-by-email-submit {background: #9900ff;}
a.comments-title.active {border-bottom: 2px solid #ffffff!important;}
h1.post-title {font: normal normal 31px Paytone One;}
.feed.widget .feed-widget-header {border-color: #9900ff;}
.feed.widget.box-title h2.widget-title {background: #9900ff;}
.social_counter {color: #9900ff}
.social_counter .button {background: #9900ff}
.main-menu {
background: #bb5454;
}
.main-menu ul.menu > li > a {
color: #ffffff;
font: normal bold 14px Josefin Sans;
}
.main-menu ul.menu > li:hover > a {
color: #ff3d00;
background: #ffffff;
}
.main-menu ul.sub-menu,
.main-menu .menu-item-mega-label .menu-item-inner {
background: #666666;
}
.main-menu ul.sub-menu a {
color: #ffffff;
}
.main-menu ul.sub-menu li:hover > a {
color: #d48f28;
}
.main-menu ul.menu > li.menu-item-current > a {background: #ffffff; color: #ffffff!important;}
.feed.widget .thumbnail {
background: #000000!important;
}
/*MAIN LAYOUT*/
.wrapper {
padding: 20px 20px 30px 20px;
max-width: 100%;
width: /*-var-wrapper-width-start*/1055px/*-var-wrapper-width-end*/;
box-sizing: border-box;
}
#content {
