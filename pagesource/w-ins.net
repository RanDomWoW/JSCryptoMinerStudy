    <script>
        GetNewsRecently = function () {
            
            var dp_rslt = "#news_rslt_data";
            $(dp_rslt).html("<tr><td colspan='2' class='text-center'>데이타를 조회중입니다.</td></tr>");

            $.ajax({
                url: "/api/BBS/getRecently",
                data: "",
                type: 'POST',
                ContentType: "application/x-www-form-urlencoded; charser=UTF-8",
                success: function (data) {
                    
                    eval("var r = " + JSON.stringify(data));
                    
                    if (r.returnCode == "1") {

                        var rslt_html = "";                      

                        for (i = 0; i < r.Response.news.length; i++) {                     
                            
                            rslt_html += "<tr>";
                            rslt_html += "<td style='height:35px'><a href='/news/" + r.Response.news[i].bbs_seq + "'>" + r.Response.news[i].title + "</a></td><td class='text-right' style='font-size:11px;color:#6e6e6e'>" + r.Response.news[i].write_date + "</td>";                            
                            rslt_html += "</tr>";
                        }

                        $(dp_rslt).html(rslt_html);
                        $(dp_rslt + " tr").each(function () {
                            $(this).children('td').css('text-align', 'left');
                        });

                    }
                    else {
                        $(dp_rslt).html("<tr><td colspan='2' class='text-center'>" + r.returnDesc + "</td></tr>");
                    }

                },
                //error: function (request, status, error) {
                //    toastr.error(request.responseText); return;

                //}
            });
        }

        GetNoticeRecently = function () {

            var dp_rslt = "#noti_rslt_data";
            $(dp_rslt).html("<tr><td class='text-center'>데이타를 조회중입니다.</td></tr>");

            $.ajax({
                url: "/api/BBS/getRecently",
                data: "",
                type: 'POST',
                ContentType: "application/x-www-form-urlencoded; charser=UTF-8",
                success: function (data) {

                    eval("var r = " + JSON.stringify(data));

                    if (r.returnCode == "1") {

                        var rslt_html = "";

                        for (i = 0; i < r.Response.notice.length; i++) {
                            rslt_html += "<tr><td style='height:35px'><a href='/notice/" + r.Response.notice[i].bbs_seq + "'>" + r.Response.notice[i].title + "</td></tr>";
                        }    

                        $(dp_rslt).html(rslt_html);
                        $(dp_rslt + " tr").each(function () {
                            $(this).children('td').css('text-align', 'left');
                        });

                    }
                    else {
                        $(dp_rslt).html("<tr><td class='text-center'>" + r.returnDesc + "</td></tr>");
                    }

                },
                //error: function (request, status, error) {
                //    toastr.error(request.responseText); return;

                //}
            });
        }

        $(function () {
            GetNewsRecently();
            GetNoticeRecently();
        });
        
    </script>



