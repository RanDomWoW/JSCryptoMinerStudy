    if (setTitle == undefined) {
        setTitle = true;
    }
    if (forceModal == undefined) {
        forceModal = true;
    }
    if (otherParams == undefined) {
        otherParams = 'toolbar=no,directories=no,menubar=no,modal=yes,dependent=yes,resizable=yes';
