<div ng-app="mortgageForm">
    <form class="mortgage-form" ng-controller="MortgageFormController" ng-submit="submit(0)" name="mortgageForm" novalidate>
        <h2>I need a mortgage</h2>

        <div class="mortgage-type-buttons row">
            <div class="col-xs-6">
                <button type="button" id="purchase-button" ng-class="data.type == 1 ? 'active' : ''" ng-click="setType(1)">Purchase</button>
            </div>
            <div class="col-xs-6">
                <button type="button" id="refinance-button" ng-class="data.type == 2 ? 'active' : ''" ng-click="setType(2)">Refinance</button>
            </div>
        </div>

        <div class="form-group row">
            <div class="col-xs-6">
                <input type="text" class="form-control" ng-class="errorClass(mortgageForm.firstName)" name="firstName" ng-model="data.firstName" placeholder="First Name" required/>
                <p ng-cloak ng-show="showError(mortgageForm.firstName)" class="validation-error">Please enter your first name</p>
            </div>
            <div class="col-xs-6">
                <input type="text" class="form-control" ng-class="errorClass(mortgageForm.lastName)" name="lastName" ng-model="data.lastName" placeholder="Last Name" required/>
                <p ng-cloak ng-show="showError(mortgageForm.lastName)" class="validation-error">Please enter your last name</p>
            </div>
        </div>

        <div class="form-group">
            <input type="email" class="form-control" ng-class="errorClass(mortgageForm.email)" name="email" ng-model="data.email" placeholder="Email" required/>
            <p ng-cloak ng-show="showError(mortgageForm.email)" class="validation-error">Please enter your email address</p>
            <p ng-cloak ng-show="showError(mortgageForm.email, 'email')" class="validation-error">Please enter a valid email address</p>
        </div>
        
		<div class="form-group row">
			<div class="col-xs-6">
				<input type="tel" class="form-control" ng-class="errorClass(mortgageForm.phone)" name="phone" ng-model="data.phone" placeholder="Phone" required />
				<p ng-cloak ng-show="showError(mortgageForm.phone)" class="validation-error">Please enter your phone number</p>
			</div>
			<div class="col-xs-6">
				<input type="text" class="form-control" ng-class="errorClass(mortgageForm.propertyZip)" name="propertyZip" ng-model="data.propertyZip" placeholder="Property Zip" required />
				<p ng-cloak ng-show="showError(mortgageForm.propertyZip)" class="validation-error">Please enter property zip</p>
			</div>
		</div>
			
        <div class="form-group row">
            <div class="col-xs-6">
                <select class="form-control" ng-class="errorClass(mortgageForm.currentState)" name="currentState" ng-model="data.currentState" ng-options="state.abbreviation as state.abbreviation for state in states" required/>
                    <option value="">Current State</option>
                </select>
                <p ng-cloak ng-show="showError(mortgageForm.currentState)" class="validation-error">Please select a state</p>
            </div>
            <div class="col-xs-6">
                <select class="form-control" ng-class="errorClass(mortgageForm.propertyState)" name="propertyState" ng-model="data.propertyState" ng-options="state.abbreviation as state.abbreviation for state in states" required/>
                    <option value="">Property State</option>
                </select>
                <p ng-cloak ng-show="showError(mortgageForm.propertyState)" class="validation-error">Please enter a value</p>
            </div>
        </div>

        <div class="form-group row" ng-if="data.type == 1">
            <div class="col-xs-6">
                <input type="text" ng-class="errorClass(mortgageForm.salesPrice)" class="form-control" name="salesPrice" ng-model="data.salesPrice" placeholder="Sales Price of Home" required="data.type == 1"
                    numeric min="0" decimals="2" />
                <p ng-cloak ng-show="showError(mortgageForm.salesPrice)" class="validation-error">Please enter a value</p>
            </div>
            <div class="col-xs-6">
                <input type="text" ng-class="errorClass(mortgageForm.downPayment)"  class="form-control" name="downPayment"  ng-model="data.downPayment" placeholder="Down Payment" required="data.type == 1"
                    numeric min="0" decimals="2"/>
                <p ng-cloak ng-show="showError(mortgageForm.downPayment)" class="validation-error">Please enter a value</p>
            </div>
        </div>

        <div class="form-group row">
            <div class="col-xs-6">
                <select class="form-control" ng-class="errorClass(mortgageForm.homeType)" name="homeType" ng-model="data.homeType" ng-options="option.value as option.name for option in typeOfHomeOptions" required/>
                    <option value="">Type of Home</option>
                </select>
                <p ng-cloak ng-show="showError(mortgageForm.homeType)" class="validation-error">Please select a value</p>
            </div>
            <div class="col-xs-6">
                <select class="form-control" ng-class="errorClass(mortgageForm.homeUse)" name="homeUse" ng-model="data.homeUse" ng-options="option.value as option.name for option in useOfHomeOptions" required/>
                    <option value="">Use of Home</option>
                </select>
                <p ng-cloak ng-show="showError(mortgageForm.homeUse)" class="validation-error">Please select a value</p>
            </div>
        </div>

        <div class="form-group">
            <select class="form-control" ng-class="errorClass(mortgageForm.creditScore)"  name="creditScore" ng-model="data.creditScore" ng-options="option.value as option.name for option in creditScoreOptions" required/>
                <option value="">Estimate Your Credit Score</option>
            </select>
            <p ng-cloak ng-show="showError(mortgageForm.creditScore)" class="validation-error">Please select a value</p>
        </div>

    
        <!-- Shared -->
        <div class="checkbox">
            <label class="disclaimer">
                <input type="checkbox" name="agreedToTerms"  ng-model="agreedToTerms" required/> I consent to being contacted concerning this request for mortgage assistance at any e-mail address or cellular or mobile telephone number I have provided to Rushmore. This includes text messages and telephone calls (including those made by an automated dialer) to my cellular or mobile telephone.
            </label>
        </div>

        <div class="form-group row">
            <div class="col-xs-6">
            </div>
            <div class="col-xs-6">
                <button type="submit" ng-disabled="!agreedToTerms || submitting">{{submitText}}</button>
            </div>
        </div>

        <div class="modal fade modal-vantage-result" tabindex="-1" role="dialog">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-body">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>

                        <h3>{{result.title}}</h3>

                        <p ng-bind-html="result.message"></p>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>
			</div>
		</div>
		
