<script type="text/javascript">
    $(document).ready(function () {
        var angle = 0;
        var rotatestop = false;
        $('#img').rotate(angle);
        // var rotatestop = false;
        setInterval(function () {
            if (rotatestop)
                return;
            angle += 0.5;
            $("#img").rotate(angle);
        }, 100);
        $("#img").hover(function () {
            if (!rotatestop) {
                $('#img').rotate({ animateTo: angle += 30, duration: 100, callback: function () {
                    $('#img').rotate({ animateTo: angle -= 30, duration: 500, callback: function () { $('#img').stopRotate() } })
                }
                });
            }
            rotatestop = true;
            //rotatestop = true;
        }, function () {
            rotatestop = false;
            //rotatestop = false;
        })
    });
</script>


    





       

