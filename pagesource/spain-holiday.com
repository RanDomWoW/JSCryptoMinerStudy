    function showNewslettersubscription(blnForce) {
        var intNewsletterPopShownForVisitorCount = 0;
        var blnNewsletterPopShownThisVisit = false;
        if ($.cookie('newsletterPopShownForVisitorCount') != null) {
            intNewsletterPopShownForVisitorCount = parseInt($.cookie('newsletterPopShownForVisitorCount'));
        }
        if ($.cookie('newsletterPopShownThisVisit') != null) {
            if ($.cookie('newsletterPopShownThisVisit') == "1") {
                blnNewsletterPopShownThisVisit = true;
            }
        }
        // Hvis brugeren har set tilmelding under 3 gange, og hvis den ikke er blevet vist på dette besøg endnu, eller brugeren selv har klikket
        // så vis den efter 30 sek. og tæl op og gem status om visning
        if (blnForce == true || (intNewsletterPopShownForVisitorCount < 3 && blnNewsletterPopShownThisVisit == false)) {
            $("#newslettersubscription").show();

            $("#newslettersubscriptioncontent").animate({"top" : "0px"}, 1000, function () {/*_gaq.push(['_trackPageview', '/nyhedsbrev-tilmelding-boks-vist']);*/});

            if (blnForce != true) {
                intNewsletterPopShownForVisitorCount++;
                $.cookie('newsletterPopShownForVisitorCount', intNewsletterPopShownForVisitorCount, {expires : 3650,path : '/'});
            }
            blnNewsletterPopShownThisVisit = true;
            $.cookie('newsletterPopShownThisVisit', "1", {expires : null,path : '/'});
        }
    }

    function hideNewslettersubscription() {
        $("#newslettersubscriptioncontent").animate({top : "312px"}, 500, function () {$("#newslettersubscription").hide();});
