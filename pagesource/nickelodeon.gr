	<script>
	(function($){
		$(document).ready(function() {
			if($('.show-hide-close').length){
				$('.show-hide-close').each( function(){

					var element = $(this);
					var cookie 	= $(element).data('cookie');
					var days 	= $(element).data('days');
					var toggler = $(element).find('.close-me');
					var timeExpires = 1000 * 60 * 60 * 24 * parseInt(days);
					var expires = new Date((new Date()).valueOf() + timeExpires);

					if (document.cookie.indexOf(cookie + '=true') == -1) {
						element.show();
					} else {
						element.hide();
					}

					$(toggler).click(function(e) {
						e.preventDefault();
						document.cookie = cookie + "=true;expires=" + expires.toUTCString()+'; path=/';
						element.hide();
					});
				});
			}
		});
	})(window.jQuery);
	</script>
