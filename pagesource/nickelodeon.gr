		e.stopPropagation();
		$('#splash_overlay').hide();
		return false;
	});

	// $('div').last().hide(); //hide ad which appears as the last div
	// document.querySelectorAll('[title="mystery item"]')[0].parentNode.nextElementSibling.style = "display: none";
	document.querySelectorAll('a.mystery_item')[0].parentNode.onclick = function(e) {
		e.stopPropagation();
		displayMysteryItem();
		return false;
	}
});

var displayMysteryItem = function() {
	$('#splash_overlay').show();
	$('.splash_image').click(function(){
		window.location.assign(document.querySelectorAll('a.mystery_item')[0].attributes["data-competition"].value)
	});
}
</script>
