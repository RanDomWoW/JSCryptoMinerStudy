<script id="model-range-my-cars-template" type="text/x-handlebars-template">
   <div class="container">
      <header class="section-title">
         <h1 class="text-center">My Cars</h1>
      </header>
    <section class="modelrange clearfix">
        <div class="row">
            {{#each cars}}
            {{#if this.dummy}}
            <a href="/car-configurator" class="col-xs-6 col-sm-3 col-md-2 btn-edit">
                <div class="modelitem finance tcc-models-item">
                    <div class="tcc-models-thumb dummy">
                        <img alt="new config" src="/images/icon_model.svg">
                    </div>
                    <span>Start new configuration</span>
                </div>
            </a>
            {{else}}
            <a href="/car-configurator/{{this.code}}" class="col-xs-6 col-sm-3 col-md-2 btn-edit">
                <div class="modelitem finance tcc-models-item" data-id="{{this.id}}" data-saved="{{this.saved}}">
                    <div class="tcc-models-thumb">
                        <img alt="{{this.ids.model}}" src="/api/ccis/config/{{this.code}}?width=154&height=95&view=exterior&angle=27">
                        <div class="tcc-models-labels">
                            {{#if this.hybrid}}
                            <div class="model-decoration hybrid">Hybrid</div>
                            {{/if}}
                            <div class="model-decoration btn-remove"><i class="icon icon-remove"></i></div>
                        </div>
                    </div>
                    <span class="name">{{this.car}}</span>
                    <h5>{{this.priceFormatted}}</h5>
                    <span>Toyota code (eg. ABC123) {{this.code}}</span>
                    {{#if this.saved}}
                    <span><b>Saved!</b> {{this.storeDate}}</span>
                    {{/if}}
                </div>
                <div class="overlay-delete" id="delete-{{this.id}}">
                    <p><strong>Delete</strong></p>
                    <button class="btn btn-functional btn-flexwidth btn-yes x-s-margin-b">Yes</button>
                    <button class="btn btn-functional btn-flexwidth btn-no x-s-margin-b">No</button>
                    <h5></h5>
                </div>
            </a>
            {{/if}}
            {{/each}}
        </div>
    </section>
    </div>
</script>


