                    $(".homepage-slides ul.pager li").css('width', (100 / $(".homepage-slides ul.pager li").length) + "%");

                    setTimeout(function () {
                        homepageAddSlides();
                    }, 1500);
                    function homepageAddSlides() {
                        $('.homepage-slides .slide').each(function (index) {
                            $(this).css('background-image', 'url(' + $(this).data('img') + ')');
                        });
                    }
                }

                $('.motion-filter-slider ul.slides').cycle({
                    fx: 'scrollHorz',
                    slides: "> li",
                    timeout: 4000,
                    width: "100%",
                    autoHeight: false,
                    easing: 'easeOutQuad',
                    pager: '.motion-filter-slider ul.pager',
                    pagerTemplate: '<li><a href="#"></a></li>',
                    pagerActiveClass: 'current',
                    swipe: true
                });
                /*if ( !$is_mobile ){
                 console.log($mfs);
                 $mfs.on('cycle-after',function(e, optionHash, outgoingSlideEl, incomingSlideEl, forwardFlag){
                 console.log('cycle after');
                 if ( $(incomingSlideEl).hasClass('video') ){
                 console.log('after video found');
                 $(incomingSlideEl).find('video').get(0).play();
                 }
                 });
                         
                 }*/

                if (!$is_mobile)
                    $('.promo-panel-centre ul.slides').cycle({
                        fx: 'fade',
                        slides: "> li",
                        timeout: 0,
                        width: "100%",
                        autoHeight: false,
                        easing: 'easeOutQuad',
                        pager: '.promo-panel-centre ul.pager',
                        pagerTemplate: '<li><a href="#"></a></li>',
                        pagerActiveClass: 'current',
                        swipe: true
                    });

                // Homepage Anim
                if (!$is_mobile) {
                    $(".home-anim").hide();
                    $('html, body').css({'overflow': 'hidden', 'height': '100%'})

                    var scrollPosition = [
                        self.pageXOffset || document.documentElement.scrollLeft || document.body.scrollLeft,
                        self.pageYOffset || document.documentElement.scrollTop || document.body.scrollTop
                    ];
                    var html = jQuery('html');
                    html.data('scroll-position', scrollPosition);
                    html.data('previous-overflow', html.css('overflow'));
                    html.css('overflow', 'hidden');
                    $footer.hide();

                    window.scrollTo(scrollPosition[0], scrollPosition[1]);

                    $('.homepage-slides a.scroll-down').on('click', function (event) {
                        event.preventDefault();
                        $(window).trigger('scroll');
                    });
                    $(document).keydown(function (e) {
                        if (e.keyCode == 40)
                            $(window).trigger('scroll');
                    });

                    $(window).on('scroll touchmove mousewheel', function (event) {
                        if ($first_scroll) {
                            event.preventDefault();

                            setTimeout(stopslideshow, 10);
                            $delay = 150;

                            $(".home-anim").show().css('top', $wh * 1.05);

                            $(".home-anim").each(function (index, element) {
                                $(this).delay(2000 + ($delay * index)).transition({'top': 0}, 1000, 'cubic-bezier(0.920, 0.355, 0.790, 1.060)');
                            });

                            $otc.removeClass('overview-text-cont-transparent');
                            $('.homepage-slides a.scroll-down, .homepage-slides ul.pager').fadeOut();

                            var html = jQuery('html');
                            var scrollPosition = html.data('scroll-position');
                            html.css('overflow', html.data('previous-overflow'));
                            window.scrollTo(scrollPosition[0], scrollPosition[1])

                            $first_scroll = false;

                            setTimeout(function () {
                                $('.motion-filter-slider ul.slides li').each(function () {
                                    if ($(this).find('video').length !== 0) {
                                        $(this).find('video').get(0).play();
                                    }
                                })
                            }, 4000);

                            $footer.delay(3000).fadeIn('slow', 'easeOutQuint', function () {
                                $('html, body').removeAttr('style');
                            });
                        }
                    });

                    function stopslideshow() {
                                    $('.homepage-slides').addClass('blurred');
                                }
                }
            });
        </script>   
