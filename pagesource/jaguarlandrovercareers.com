<script type="text/javascript">

jQuery(document).ready(function($){
	
	$(".footer .menu ul li a").not("a.error-reporting-open").click(function(event){
		event.preventDefault();
		var $parts = $(this).attr('href').split('/');
		var $parts_slice = $parts.splice(-3, 2);
		window.location.href = '/' + $parts_slice[0] + "#" + $parts_slice[1];
	});

	$(window).resize(function() {
		$('.motion-filter-slider ul:not(.pager), .resize-slider').each(function(){
			newWidth = $(this).parent('div').width()
			$(this).width(newWidth)
			$(this).children('div').width(newWidth)
		});

		var $h = 0;
		var $wh = $(window).height();
		var $ww = $(window).width();
		var $fh = $('.footer').height()+65;

		$( ".section" ).each(function( index ) {
			$h += $(this).height();
		});		 

		if ( $wh-$fh > $h ){
			$('.footer').addClass('fixed');
		} else {
			$('.footer').removeClass('fixed');
		}

		if ( $ww > 640 ){
			$(".header .span_9_of_12 ul.menu").css("display","block");
		} else {
			$(".header .span_9_of_12 ul.menu").css("display","none");
		}		
			
	});
	$(window).trigger("resize");


	// GENERAL PAGE FUNCTION
	$(".auto-open ul ul").css("display","none");    			
	$(".auto-open ul h4, .business-area-apprentice-auto-open .auto-open ul h2").click(function () {
		$(this).toggleClass('collapsed');
		$(this).parent().toggleClass('open');
		$(this).parent().find("ul li").css('opacity','0');
		$(this).parent().find("ul").slideToggle("fast",'easeOutQuad',function() {
			$(this).parent().find("ul li").delay(200).animate({"opacity": '1'},'normal');
		});		
	});
	$("a.autoscroll").click(function(event) {
		event.preventDefault();
		$val = $(this).data("autoscroll");
		$val = $val.replace(/#/g,'');
		
		var $elem = $('#section-'+$val+'-cont');
		if ( $($elem).length ){
			$('html, body').animate({scrollTop: $elem.offset().top - 200}, 500, 'easeInOutQuad');
			update_hash( $val );
		}
	});
	
	// SOCIAL PANEL
	$("a.social-panel-btn").click(function(event) {
		event.preventDefault();
		$sp = $("#social-sidebar");
		
		if ( $sp.hasClass('open') )
			$sp.transition({'right':'-100%'}, 1000, 'cubic-bezier(0.920, 0.355, 0.790, 1.060)'); 
		else
			$sp.transition({'right':0}, 1000, 'cubic-bezier(.06,.98,.95,1.01)');
			 
		$sp.toggleClass('open');

		$(".page-cover").stop().toggleClass('page-cover-total').fadeToggle();
		
	});

	// Error Reporting
	$issue_type = $('#error-issue'), $error_cont = $('#error-reporting');	
	$issue_type.prepend("<option value='' selected='selected' disabled='disabled'>Choose your issue type</option>");
	$('a.error-reporting-open').on('click', function(event){
		event.preventDefault();
		$error_cont.fadeIn();
	});
	$('a.error-reporting-close').on('click', function(event){
		event.preventDefault();
		$error_cont.fadeOut();
	});

	// Auto Scroll
	var hash = window.location.hash;

	if( hash ) {
		hash = hash.replace(/#/g, '');
		hash = hash.replace(/\//g, '');
		var $elem = $('#section-'+hash+'-cont');
		if ( $elem.length )
			$('html, body').animate({scrollTop: $elem.offset().top - 200}, 500, 'easeInOutQuad');
			}

	
});	

</script>
