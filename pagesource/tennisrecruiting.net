<script type="text/javascript">
            function countdown(year, month, day, hour, minute, format, color)
                     {
                     Today = new Date();
                     Todays_Year = Today.getFullYear() - 2000;
                     Todays_Month = Today.getMonth();                  
                     
                     Todays_Date = (new Date(Todays_Year, Todays_Month, Today.getDate(), 
                                             Today.getHours(), Today.getMinutes(), Today.getSeconds())).getTime();                                 
                     Target_Date = (new Date(year, month - 1, day, hour, minute, 00)).getTime();                  
                     
                     Time_Left = Math.round((Target_Date - Todays_Date) / 1000);
                     
                     if(Time_Left < 0)
                        Time_Left = 0;
                     
                     var html = '';
                     var countdown = null;
                     if (document.getElementById) {
                     countdown = document.getElementById("countdown");
                     } else if (document.all) {
                     countdown = document.all["countdown"];
                     } else if (document.layers) {
                     countdown = document.layers["countdown"];
                     }
                     if (countdown != null) {
                     
                       switch(format)
                             {
                             case 0:
                                  html = '<font color="' + color + '">' + Time_Left + '</font> Seconds';
                                  break;
                             case 1:
                                  days = Math.floor(Time_Left / (60 * 60 * 24));
                                  Time_Left %= (60 * 60 * 24);
                                  hours = Math.floor(Time_Left / (60 * 60));
                                  Time_Left %= (60 * 60);
                                  minutes = Math.floor(Time_Left / 60);
                                  Time_Left %= 60;
                                  seconds = Time_Left;
                                  
                                  dps = 's'; hps = 's'; mps = 's'; sps = 's';
                                  if(days == 1) dps ='';
                                  if(hours == 1) hps ='';
                                  if(minutes == 1) mps ='';
                                  if(seconds == 1) sps ='';
                                  
                                  html = '<font color="' + color + '">' + days + '</font> Day' + dps + ', ';
                                  html += '<font color="' + color + '">' + hours + '</font> Hour' + hps + ', ';
                                  html += '<font color="' + color + '">' + minutes + '</font> Minute' + mps + ', and ';
                                  html += '<font color="' + color + '">' + seconds + '</font> Second' + sps;
                                  break;
                             default: 
                                  html = '<font color="' + color + '">' + Time_Left + '</font> Seconds';
                             }
                           }  
                       countdown.innerHTML = html;    
                       setTimeout('countdown(' + year + ',' + month + ',' + day + ',' + hour + ',' + minute + ',' + format + ',"' + color + '");', 1000);
                     }
                   
          countdown(18, 04, 11, 00, 00, 1, '#e40c1c');
          </script>
