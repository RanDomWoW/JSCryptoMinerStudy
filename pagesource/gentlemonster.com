		$('.search_wraper_bg').removeClass('display_n');
	}
	function search_wraper_close(){

		$('.search_wraper').addClass('display_n');
		$('.search_wraper_bg').addClass('display_n');
	}

	function location_submit(){
		var ro=$("select[name='od_region']").val();
			if(ro==""){
				alert("Please select your Shipping Country");
				return false;
			}

			$.ajax({
			  type: "POST",
			  url: "./_proc.php",
			  data: { mode: "location_ajax", shipping:ro,url:document.location.href },
			  dataType:'json'
			}).done(function( msg ) {

				if(msg.msg=="OK"){

					location.href=msg.subdomain;

				}else{
					alert("Error.");
				}
			});
	}

	function location_submit_m(){
		var ro=$("select[name='od_region_m']").val();
			if(ro==""){
				alert("Please select your Shipping Country");
				return false;
			}

			$.ajax({
			  type: "POST",
			  url: "./_proc.php",
			  data: { mode: "location_ajax", shipping:ro,url:document.location.href },
			  dataType:'json'
			}).done(function( msg ) {

				if(msg.msg=="OK"){

					location.href=msg.subdomain;

				}else{
					alert("Error.");
				}
			});
	}



