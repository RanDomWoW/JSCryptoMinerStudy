<script>

function form_check_bag(act) {
    var f = document.frmcartlist_bag;

	if($("input[name='bag_item_length']").val()=="true"){
		alert("Please select at least one product that you order.");
		return false;
	}
   if($(".it_id_baglist").length==0){
		alert("Please select at least one product that you order.");
		return false;
   }

	f.act.value = act;
    f.submit();
}

function del_item_bag(id){
	$.ajax({
	  type: "POST",
	  url: "./shop/_proc.php",
	  data: { mode: "select_del_item", it_id: id }
	}).done(function( msg ) {
		bag_contents_ajax_del();
	});
	
}

function del_item_bag_sub(id,sub){
	$.ajax({
	  type: "POST",
	  url: "./shop/_proc.php",
	  data: { mode: "select_del_item_sub", it_id: id, ct_id: sub }
	}).done(function( msg ) {
		bag_contents_ajax();
	});
	
}

function change_item_bag(t){
	var area_session="us";
	if(parseInt($(t).val())>10){
		if(area_session=="kr"){
			
			alert("최대주문수량은 10개입니다.");
			
		}

		if(area_session=="other"){
			alert("You may purchase up to 10 pcs in a single order.");
			
		}

		if(area_session=="cn"){
			alert("最多可以选择10个");
			
		}
		$(t).val($(t).attr("qty"));

		return false;
	}
	$.ajax({
	  type: "POST",
	  url: "./shop/_proc.php",
	  data: { mode: "select_change_itemcount", it_id: $(t).attr('it_id'),count:$(t).val() }
	}).done(function( msg ) {
		if(msg!=""){
			alert(msg);
			bag_contents_ajax();
		}else{
			bag_contents_ajax();
		}
	});
	
}

function change_item_bag_sub(t){
	var area_session="us";
	if(parseInt($(t).val())>10){
		if(area_session=="kr"){
			alert("최대주문수량은 10개입니다.");
			
		}
		if(area_session=="other"){
			alert("You may purchase up to 10 pcs in a single order.");
			
		}
		if(area_session=="cn"){
			alert("最多可以选择10个");
			
		}
		$(t).val($(t).attr("qty"));

		return false;
	}


	$.ajax({
	  type: "POST",
	  url: "./shop/_proc.php",
	  data: { mode: "select_change_itemcount_sub", it_id: $(t).attr('it_id'),ct_id: $(t).attr('ct_id'),count:$(t).val() }
	}).done(function( msg ) {
		if(msg){
			alert(msg);
			bag_contents_ajax();
		}else{
			bag_contents_ajax();
		}
	});
	
}

</script>



