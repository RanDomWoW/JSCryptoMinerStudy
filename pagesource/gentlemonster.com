	function sidebag_open(){

		bag_contents_ajax();//ajax_contents_ajax
		$('.sidebag').removeClass('display_n');
		$('.dellay_layer').removeClass('display_n');
		$('.sidebag').stop().animate({ right: "0px" }, 500, function() {
			$('.checkout_bag_btn').removeClass('display_n').stop().animate({ opacity: "1" }, 100);
			$('.sidebag_bg').removeClass('display_n').stop().animate({ opacity: "0.3" }, 100);
			$('.bag-close-button').removeClass('display_n');
			$('.bag_bottom_menu_wrap').removeClass('display_n').stop().animate({ opacity: "1" }, 100);
			bag_on="on";

			setTimeout(function(){
			  $('.dellay_layer').addClass('display_n');
			}, 1000);

		});
	}
	function sidebag_close(){
		$('.dellay_layer').addClass('display_n');
		$('.bag-close-button').addClass('display_n');
		$('.checkout_bag_btn').addClass('display_n');
		$('.bag_bottom_menu_wrap').addClass('display_n').stop().animate({ opacity: "0" }, 1);
		$('.sidebag').stop().animate({ right: "-330px" }, 200, function() {
			$('.sidebag').addClass('display_n');
			bag_on="off";
		});
		$('.sidebag_bg').animate({ opacity: "0" }, 100).addClass('display_n');
	}



	function shipping_ajax(){


		var language_v = $("select[name='od_region']").val();
		$("#language_country_ajax").html("<center><img src='./img/ajax-loaders.gif' height='20'/></center>");
		$.ajax({
		  type: "POST",
		  url: "./layout/shipping_country_ajax.php",
		  data: { language:language_v }
		}).done(function( msg ) {
			$("#language_country_ajax").fadeOut("fast",function(){
				$(this).html(msg).fadeIn("fast");
			});
		});

	}

	function country_wraper_open(){

		$('.country_wraper').removeClass('display_n');
