		  $(".top_subcon_right_wrap").css("height",(parseInt(box_height)+120));
		});
	}
	//신규추가 끝


	function codepink_layer(flag){
		if(flag=="on"){
			$('body').css('overflow','hidden');
			$('.codepink_layer').fadeIn();
			$('.codepink_layer_bg').fadeIn();
			sidemenu_close();
		}else{
			$('body').css('overflow','auto');
			$('.codepink_layer').fadeOut();
			$('.codepink_layer_bg').fadeOut();
		}
	}


	function tracking_layer(flag){
		if(flag=="on"){
			$('body').css('overflow','hidden');
			$('.tracking_layer').fadeIn();
			$('.tracking_layer_bg').fadeIn();
			sidemenu_close();
		}else{
			$('body').css('overflow','auto');
			$('.tracking_layer').fadeOut();
			$('.tracking_layer_bg').fadeOut();
		}
	}


	function mobilemenusubopen(subname){
		if($('.mobilemenu_wrap_'+subname).hasClass('onopen')){
			$('.mobilemenu_arrow').empty();
			$('.mobilemenu_arrow').html('<i class="fa fa-angle-down" aria-hidden="true"></i>');
			$('.mobilemenu_wrap_'+subname).removeClass('onopen');
		}else{
			$('.mobilemenu_arrow').empty();
			$('.mobilemenu_arrow').html('<i class="fa fa-angle-up" aria-hidden="true"></i>');
			$('.mobilemenu_wrap_'+subname).addClass('onopen');
		}
		$('.mobilemenu_wrap_'+subname).slideToggle();
	}

