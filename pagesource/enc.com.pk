function sendMeBack(parentUrl){
        $.ajax({
            type: "POST",
            url: parentUrl,
            success: function(response){
                    $("#nec_response").html(response);
            }
        });
}


$(function() { 

    $('a.ajx').click(function (event) 
    { 
        //alert('ddd');
        event.preventDefault(); 
        $.ajax({
        url: $(this).attr('href'),
        success: function(response) {
            $("#nec_response").html(response);
        }
    });
return false; 
});



    $(document).on('submit','form.redirectme',function(e){
        var url = $(this).attr( 'action' );
        var parentUrl = $(this).attr( 'name' );
       // alert($(this).find("select,textarea, input").serialize());
        //alert(url);
        
        $.ajax({
            type: "POST",
            url: url,
            data: $(this).find("select,textarea, input, button").serialize(), // serializes the form's elements.
            success: function(response){
                    $("#nec_response").html(response);
                    setTimeout(function(){sendMeBack(parentUrl)},3000);
            }
        });
        e.preventDefault();
    });  

    $(document).on('submit','form.stopme',function(e){
        var url = $(this).attr( 'action' );
       // alert($(this).find("select,textarea, input").serialize());
        //alert(url);
        $.ajax({
            type: "POST",
            url: url,
            data: $(this).find("select,textarea, input, button").serialize(), // serializes the form's elements.
            success: function(response){
                    $("#nec_response").html(response);
                    // setTimeout(function(){stopme(url)},3000);
            }
        });
        e.preventDefault();
    });

    $(document).on('submit','form.uploadpic',function(e){
        e.preventDefault();

        var url = $(this).attr( 'action' );
        // var parentUrl = $(this).attr( 'name' );

        e.preventDefault();
        var formData = new FormData($(this)[0]);

        document.querySelector('div.form_cunsumer').id = 'form_cunsumer';
        document.getElementById("loading").style.display = 'block';

        setTimeout(function(){ 
            $.ajax({
                type: "POST",
                url: url,
                data: formData,
                async: false,
                cache: false,
                contentType: false,
                enctype: 'multipart/form-data',
                processData: false,
                success: function(response){
                        $("#nec_response").html(response);
                        // setTimeout(function(){sendMeBack(parentUrl)},3000);
                }
            });
        },3000);
        e.preventDefault();
    });    
}); 
 




</script>

