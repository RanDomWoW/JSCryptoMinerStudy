<script>
    $(window).load(function(){
        $('#panoMobileSlider').flexslider({
            animation: "slide",
            slideshow: false,
            controlNav: false,
            before:function(slider){
                flexSliderLazyLoader(slider);
            }
        });
    });
    $(document).ready(function(){

        $('.panoramaSlider').pslider({
            //debug: true,
            interval: '800000',
            angleLeft: 'icon icon-left',
            angleRight: 'icon icon-right'
        });

        $('.pano_select_btn').on('click', function (e) {
            e.preventDefault();
            $(this).parent('.panoSelect').toggleClass('active');
            $('.pano_select_dropdown a').on('click', function (e) {
                e.preventDefault();
                var slideId = $(this).attr('href');
                var slideName = $(this).text();
                $('.panoramaSlider.active').fadeOut(600, function() {
                    $(this).removeClass('active');
                    $(slideId).addClass('active').fadeIn(600);
                    $(slideId).pslider({
                        interval: '800000'
                    });
                    //remove active class and
                    $('.panoSelect').removeClass('active');
                    //add text to btn
                    $('.pano_select_btn').text(slideName);
                });
            });
        });
    });
    $(window).resize(function() {
        if ($(window).width() <= 800) {
            $('.panoramaSlider.active').fadeOut(200);
        } else {
            $('.panoramaSlider.active').fadeIn(200);
        }
    });
</script>
