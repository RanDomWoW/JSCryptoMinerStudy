function check_keys( ev ) {
    var c = get_code( ev );
    var p = document.form1.text.value;

    p = p.replace( /^\s+/, "" );
    p = p.replace( /\s+$/, "" );

    if (c == 13 && p == "") { document.form1.text.blur(); }
    return true;
}

function get_code( _event ) {
    if( !_event ) _event = window.event;
    if (_event.which) { code = _event.which; }
    else { code = _event.keyCode; }
    return code;
}

function submit_chk(form){
    var ent = form.org.value;
    ent = ent.replace( /\s+/, " " );
    ent = ent.replace( /\s+$/, "" );
    ent = ent.replace( /^\s+/, "" );
    if(ent == ""){
        return false;
    }
    if(ent.match(/\W/)) {
        form.orgs.value = ent;
        form.org.value = "";
    } else {
        form.org.value = ent;
