    ------------------------------------------ */
    var wW = $(window).innerWidth();
    const drawer = $("#g_navi>nav");
    const drawerLink = $("#g_navi>nav a");
    isDrawer = false;

    function menuClose() {
      drawer.slideUp();
      $('.menu_trigger').removeClass('active');
      isDrawer = false;
    }

    function menuOpen() {
      drawer.slideDown();
      $('.menu_trigger').addClass('active');
      isDrawer = true;
    }

    $('.js-drawer').on('click', function() {
      var wW = $(window).innerWidth();
      if (wW < 767) {
        if (isDrawer) {
          menuClose();
        } else {
          menuOpen();
        }
      }
      return false;
    });

    drawerLink.on('click', function() {
      var wW = $(window).innerWidth();
      if (wW < 767) {
        menuClose();
      }
    });

    $(window).on('load resize', function() {
      var wW_resized = $(window).innerWidth();
      if (wW != wW_resized) {
        if (wW_resized < 767) {
          drawer.hide();
          menuClose();
        } else {
          drawer.slideDown();
        }
      }
    });

  });
</script>
