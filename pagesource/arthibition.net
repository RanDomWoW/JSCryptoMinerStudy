    <script type="text/javascript">

        var size_before;
        $(window).resize(function () {
            loadData();
        });
        $(document).ready(function () {
            loadData();
            $('.carousel.slide').carousel({
                interval: 5000,
                cycle: true
            });
        });

        function loadData()
        {
            var size = senseWindowSize();


            if (size == 'desktop' && size_before != 'desktop')
            {
                size_before = 'desktop'
                $("#wrap").html('');
                $("#newindex-desktop").load('http://arthibition.net/en/index/newindex_ajax?type=desktop', function () {
                    $("#video-top-new").css("margin-top", "50px");
                });
            }
            else if( ( size == 'tablet' || size == 'mobile' ) && ( size_before != 'tablet' && size_before != 'mobile' ) )
            {
                size_before = size;
                $("#newindex-desktop").html('');
                $("#wrap").load('http://arthibition.net/en/index/newindex_ajax?type=mobile', function () {
                    $("#video-top-new").css("margin-top", "0px");
                });
            }
            
            if (size_before == '')
            {
                size_before = size;
            }

        }


        function senseWindowSize()
        {
            var w = $(window).width();
            if (w < 767)
            {
                return 'mobile';
            }
            else if (w >= 767 && w < 979)
            {
                return 'tablet';
            }
            else if (w >= 979)
            {
                return 'desktop';
            }
        }

    </script>
