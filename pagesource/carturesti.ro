						list = $('<ul />');
					} else{
						list = $('#' + resultList);
						list.html('');
						var attList = [], catList = [];
					}
					$.each(data, function(i, el){
						if($.isNumeric(i)){
							var aLi = createListElement(el, i), aA = aLi.find('a').eq(0);
							list.append(aLi);
							//shortenA(list, aA);
							aA.addClass('block');
						} else {
							if (i == 'attributeLinks') {
								if (el.length)
									list.prepend($('<hr/>').addClass("separator"));
								$.each(el, function(ai,ael) {
									var aLi = createListElement(ael), aA = aLi.find('a').eq(0);
