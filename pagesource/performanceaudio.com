		function toggleNav() {
			closeAcct();
			if (navOpen == false) {
				openNav();
			} else {
				closeNav();
			}
		}
		/* Set the width of the side navigation to 250px and show black background */
		function openAcct() {
			document.getElementById("side-nav-acct").style.width = "250px";
			document.getElementById("side-nav-acct-bg").style.width = "100%";
			acctOpen = true;
		}
		/* Set the width of the side navigation to 0 and hide black background */
		function closeAcct() {
			document.getElementById("side-nav-acct").style.width = "0";
			document.getElementById("side-nav-acct-bg").style.width = "0";
			acctOpen = false;
		}
		/* Automatically open or close the side navigation */
		function toggleAcct() {
			closeNav();
			if (acctOpen == false) {
				openAcct();
			} else {
				closeAcct();
			}
		}
		/* show hide pagination */
	    $(".showHideButton").click(function() {
		    if (paginationOpen == true) {
			    $(this).html(' <span class="icon-plus" style="font-size:14px; color:#0088ff;"></span> ');
			    paginationOpen = false;
			} else {
			    $(this).html(' <span class="icon-minus" style="font-size:14px; color:#0088ff;"></span> ');
			    paginationOpen = true;
			}
			$(".pagination-wrapper .bottom-half .left").toggle(200);
			$(".pagination-wrapper .bottom-half .right").toggle(200);
	    });
		/* submit a list change by price range */
	    $('#priceRangeForm').submit(function( event ) {
		    event.preventDefault();
		    var baseurl = $("#priceRangeForm input[name=baseurl]").val();
		    var sort = $("#priceRangeForm input[name=sort]").val();
		    var display = $("#priceRangeForm input[name=display]").val();
		    var rangeFrom = $("#priceRangeForm input[name=rangeFrom]").val();
		    var rangeTo = $("#priceRangeForm input[name=rangeTo]").val();
		    var newurl = baseurl + rangeFrom + '-' + rangeTo + '/' + sort + '/' + display + '/';
		    window.location.href = newurl;
	    });
		/* submit a list change by price range - bundles version */
	    $('#priceRangeFormBundles').submit(function( event ) {
		    event.preventDefault();
		    var baseurl = $("#priceRangeFormBundles input[name=baseurl]").val();
		    var brand = $("#priceRangeFormBundles input[name=brand]").val();
		    var page = $("#priceRangeFormBundles input[name=page]").val();
		    var sort = $("#priceRangeFormBundles input[name=sort]").val();
		    var display = $("#priceRangeFormBundles input[name=display]").val();
		    var rangeFrom = $("#priceRangeFormBundles input[name=rangeFrom]").val();
		    var rangeTo = $("#priceRangeFormBundles input[name=rangeTo]").val();
		    var newurl = baseurl + brand + '/' + page + '/' + rangeFrom + '-' + rangeTo + '/' + sort + '/' + display + '/';
		    window.location.href = newurl;
	    });
		$(window).on('load', function(){
			$(".pagination-wrapper .bottom-half .left").hide();
			$(".pagination-wrapper .bottom-half .right").hide();
	    });
	</script>
	</div>
</body>
</html>