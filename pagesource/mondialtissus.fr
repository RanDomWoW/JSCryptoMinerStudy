<!-- Instantsearch wrapper template -->
<script type="text/template" id="instant_wrapper_template">
    {{#findAutocomplete}}
    <div id="algolia-autocomplete-container"></div>
    {{/findAutocomplete}}
    <div id="algolia_instant_selector" class=" with-facets">
                                    
        <div class="col-left sidebar col-l-4 col-s-18" id="algolia-left-container">
            <div id="category-layered-navigation">
                <div class="block block-layered-nav">
                    <div id="instant-search-category-container"></div>
                </div>
            </div>
        </div>


        <div class="col-main col-l-13 col-s-18">
                                                
            <div id="facets-layered-navigation">
                <div class="block block-layered-nav">
                    <div class="block-content toggle-content">
                        <div class="mobile-btn-show-filters" id="mobile_btn_show_filters"><span>Filtrer</span></div>
                        <div class="top-filters-category-container clearer">
                            <div id="instant-search-facets-container" class="instant-search-facets-container top-filters-category">
                                <div class="mobile-filters-header">
                                    Filtres                                    <span id="mobile_filters_header_close">&nbsp;</span>
                                </div>
                            </div>
                            <div id="more-filters-button" class="more-filters-button"></div>
                            <div class="sorter" id="algolia-sorts"></div>
                            <div id="more-filters-container" class="more-filters-container" style="display:none;"></div>
                        </div>

                        <div class="toolbar toolbar-top clearer">
                            <div class="pull-left" id="algolia-stats"></div>
                            <div class="pager">
                                <div id="algolia-limiter" class="limiter"></div>
                                <div id="instant-search-pagination-container" class="pages"></div>
                            </div>
                        </div>
                        <div id="current-refinements" class="currently"></div>
                        <div id="clear-all-current-refinements" class="actions"></div>
                    </div>
                </div>
            </div>

            <div id="catalog-listing-algolia">
                <div id="instant-search-results-container" class="products-grid last"></div>
                <div class="toolbar-bottom">
                    <div class="toolbar clearer">
                        <div class="pager">
                            <div id="algolia-limiter-bottom" class="limiter"></div>
                            <div id="instant-search-pagination-bottom-container" class="pages"></div>
                        </div>
                        <div class="sorter" id="algolia-sorts-bottom"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>

<script type="text/javascript">

    $j(".catalogsearch-result-index").on('click', '.facet .ais-header', function () {
        if ( $j(this).parents(".facet").hasClass('active') ) {
            $j(this).parents(".facet").removeClass('active')
        } else {
            $j(".facet").removeClass('active');
            $j(this).parents(".facet").addClass('active')
        }
    });

    $j(".catalogsearch-result-index").on('click', '#mobile_btn_show_filters', function () {
        $j(".top-filters-category-container").addClass('active');
    });
    $j(".catalogsearch-result-index").on('click', '#mobile_filters_header_close', function () {
        $j(".top-filters-category-container").removeClass('active');
    });

