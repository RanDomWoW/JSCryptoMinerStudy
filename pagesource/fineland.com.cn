

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
  	<script>
  		//判断是否为电脑端
  	    var userAgentInfo = navigator.userAgent;
  	    var Agents = ["Android", "iPhone",
  	                "SymbianOS", "Windows Phone",
  	                "iPad", "iPod"];
  	    var flag = true;
  	    for (var v = 0; v < Agents.length; v++) {
  	        if (userAgentInfo.indexOf(Agents[v]) > 0) {
  	            flag = false;
  	            break;
  	        }
  	    }
  	    
  	    //跳转到主页
	  	var url = "http://www.fineland.com.cn:80/finelandos/index/toIndex.spring";
  	    if (!flag) {
	  	    //如果不是电脑端跳则转到移动端主页
	  		url = "http://www.fineland.com.cn:80/mobileFinelandos/index/toIndex.spring";
  	    }
	  	window.location.href = url;
  	</script>
</html>