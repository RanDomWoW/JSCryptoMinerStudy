jQuery(document).ready(function() {
jQuery('#background-cycle').cycle({
fx: 'fade',
speed: 1500,
manualSpeed: 500,
timeout: 10000,
slides: '> li',
pager: '#sfnt-hero-icon-nav'
});
jQuery('#foreground-cycle').cycle({
fx: 'fade',
speed: 500,
manualSpeed: 500,
timeout: 0,
slides: '> li',
pager: '#sfnt-hero-icon-nav',
pagerTemplate: ''
});
var slideshows = jQuery('.slideshow');
jQuery('#foreground-cycle li:eq(0)').addClass('fadeInUpBig');
jQuery('a.hero-icon-nav').on( 'click', function(e){
e.preventDefault();
var navIndex = jQuery('a.hero-icon-nav').index(this);
jQuery('#foreground-cycle li:eq(' +navIndex + ')').addClass('fadeInDownBig');
});
jQuery('#background-cycle').on( 'cycle-before', function(e, opts, outgoingSlideEl, incomingSlideEl, forwardFlag) {
var currentSlide = opts.currSlide
var nextSlide = opts.nextSlide
var currentForegroundSlide = jQuery('#foreground-cycle li').eq(currentSlide + 1);
var nextForegroundSlide = jQuery('#foreground-cycle li').eq(nextSlide + 1);
jQuery('#foreground-cycle li').removeClass('fadeInDownBig');
currentForegroundSlide.removeClass('fadeInUpBig').addClass('fadeInDownBig');
});
jQuery( '#background-cycle' ).on( 'cycle-after', function(e, opts, outgoingSlideEl, incomingSlideEl, forwardFlag) {
var currentSlide = opts.currSlide
var nextSlide = opts.nextSlide
var currentForegroundSlide = jQuery('#foreground-cycle li').eq(currentSlide + 1);
var nextForegroundSlide = jQuery('#foreground-cycle li').eq(nextSlide + 1);
jQuery('#foreground-cycle').cycle('goto', nextSlide);
jQuery('#foreground-cycle li').removeClass('fadeInUpBig');
nextForegroundSlide.removeClass('fadeInDownBig').addClass('fadeInUpBig');
});
});
</script>



