<script type="text/javascript">
    jQuery(document).ready(function() {

      $(".choose_city_popup .regions a").click(function(){
          $(".choose_city_popup .regions li").removeClass("active").removeClass("sprite-side");
          $(this).parent().addClass("active").addClass("sprite-side");
          return false;
        });

      $(".default_popup .popup-close").click(function(){
        var $popup = $(this).parent();
        // делаем не активный элемент доработок
        if ($popup.attr('data-revised-sid') != null){
            var sid = $popup.attr('data-revised-sid');
            $('div.revised-item[data-sid='+sid+']').css('z-index', '0').css('position','static');
        }
        $popup.fadeOut(300);
        jQuery('.bx-wrapper').removeClass('blur');
        $('.station.active_st').removeClass('disabled');
        return false;
      });

      var get_showRegionPopup = $.cookie('show_region_popup');
      if(get_showRegionPopup == 1) {
          $("#regionsPopup").show();
      }

      jQuery(".choose_city_popup .regions a").click(function(){
        var iDistrictID = jQuery(this).attr("rel");
        jQuery(".subregions").hide();
        jQuery("#district_"+iDistrictID).show();
        return false;
      });


      // Выбор городов.
      jQuery(".choose_city_popup .region_link").click(function() {
        var iCityID = jQuery(this).attr("rel");
        var iCityNAME = jQuery(this).attr("data-city");
        var backURL = window.location.href;
    		BX.setCookie('pander', null, {
          domain: '.shop.kz',
          path: '/'
        });
        if((window.location.href).match(/about\/shops\/\d*\//) || (window.location.href).match(/about\/shops\/cities\/\d*\//)) {
            backURL = '/about/shops/' + iCityID + '/';
        }
        var szUrl = '/bitrix/components/whitewind/geoip.choose.city/get.php';
        jQuery.ajax({
          url: szUrl,
          global: false,
          type: 'POST',
          data: ({
            action: 'changeSubRegion', 
            regsectid: iCityID, 
            regsectname: iCityNAME, 
            curdir: '/', 
            curpage: '/', 
            backurl: backURL}),
          dataType: "json",
          success: function(response){
            if(response.result) {
              window.location.href = response.data.BACK_URL;
            }
          }
        });

        //window.location.href = szUrl;
        return false;
      });

      jQuery(".header_city_selector > a.selected").click(function() {
        jQuery('body').append(jQuery('.choose_city_popup'));
        jQuery(".choose_city_popup").toggle(0, function(){
          if(jQuery('.bx-wrapper').hasClass('blur')){
            jQuery('.bx-wrapper').removeClass('blur');
          } else 
            jQuery('.bx-wrapper').addClass('blur');
        });
        if (window.PIE) {
          $('.choose_city_popup').each(function(){
            PIE.detach(this);
            PIE.attach(this);
          });
        }
        return false;
      });

      jQuery("#right_region").click(function() {
        if(get_showRegionPopup == 1) {
          $.cookie('show_region_popup', '0', {
            expires : 0,
            path    : '/',
            domain  : window.location.host,
            secure  : true
          });
        }
        jQuery(this).parent().remove();
        return false;
      });

      jQuery("#wrong_region").click(function() {
        if(get_showRegionPopup == 1) {
          $.cookie('show_region_popup', '0', {
            expires : 0,
            path    : '/',
            domain  : window.location.host,
            secure  : true
          });
        }

        jQuery(this).parent().remove();
        jQuery(".header_city_selector > a.selected").click();
        return false;
      });

      jQuery(".shade2").click(function() {
          if (jQuery(".choose_city_popup:visible").length > 0) {
            jQuery(".choose_city_popup:visible > a.popup-close").click();
          }
      });

      if(BX.browser.IsMobile()){
          var ccDialog = document.querySelector('.choose_city_popup'),
              regions, regionnsLink, subregions, backbtn;

          if(!!ccDialog){
              regions = ccDialog.querySelector('.regions');

              if(!!regions){
                  regionnsLink = regions.querySelectorAll('li a');
                  for(var i in regionnsLink){
                      BX.bind(regionnsLink[i], 'click', BX.proxy(regionSelect, BX(regions)));
                  }
              }
          }

          subregions = ccDialog.querySelectorAll('.subregions');
          if(!!subregions){
              for(var i in subregions){
                  BX.style(subregions[i], "display", "none");
              }
          }

          backbtn = ccDialog.querySelectorAll('.subregions .back_to_region');
          if(!!backbtn){
              for(var i in backbtn){
                  BX.bind(backbtn[i], 'click', BX.proxy(function(el){
                      BX.removeClass(this[0], "hidden");
                      BX.style(this[1], "display", "none");
                  }, [BX(regions), backbtn[i].parentElement]));
              }
          }
      }
    });

    function regionSelect() {
        BX.addClass(this, "hidden");
    }
</script>
