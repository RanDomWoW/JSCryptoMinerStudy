<script>
	$(function(){
    'use strict';
    var clone = $(".move_total li").first().clone();
    
    $(".move_total").append(clone);
    
    var i = 0;
    var len = $(".move_total li").size();
    var spanArr = [];
    
    for(var j = 1; j < len; j++){
        var spans = '<span></span>';
        spanArr.push(spans);
    }
    
    $(".move_index").html(spanArr.join(""));
    
    $(".move_index span").first().addClass("active");
    
    function moveR(){
        i++;
        if(i == len){
            $(".move_total").css({left:0});
            i = 1;
        }
        $(".move_total").stop().animate({left:-i*737},500);
        
        if( i == len - 1){
            $(".move_index span").eq(0).addClass("active").siblings().removeClass("active");
        }else{
            $(".move_index span").eq(i).addClass("active").siblings().removeClass("active");
        }       
    }
    
    $(".move_index span").hover(function(){
        var index = $(this).index();
        i = index;
        $(".move_total").stop().animate({left:-i*737},500);
        $(this).addClass("active").siblings().removeClass("active");
    });
    
    var t = setInterval(function(){
        moveR();
    },2500);
    
    $(".move").hover(function(){
        clearInterval(t)},function(){
        t = setInterval(function(){
            moveR();
        },2500);
    });
    
    //tv
    (function(){
        var hide = $(".botton").siblings(".hide");
        var ul = hide.find(".broadcast_l");
        var lis = ul.find("li");
        var left = parseInt(ul.css("left"),10);
        
        $(".btn_1").on("click",function(){        
        if(left >= 0){
            return;
        }
        left += 248;
        ul.animate({left:left + 'px'},200);        
    });

    $(".btn_2").on("click",function(){    
        if(lis.size() <= 4){
            return;
        }
        var minleft = -(lis.size() - 4) * 248;
        if(left <= minleft){
            return;
        }
        left -= 248;
        ul.animate({left:left+'px'},200);       
    });
    })();
});


