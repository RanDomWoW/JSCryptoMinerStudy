      $li.prepend("<a href=\"#\"></a>").children("a").append($child).addClass("dropdown-toggle").attr("data-toggle", "dropdown").append("<b class=\"caret\"></b>");
      $li.append($ul);
    }
  });

});
</script>
<script>
$("#body, [role=main]").fitVids({ignore:""});
</script>  </body>
