<script type="text/javascript">
    require(['underscore', 'jquery', 'jquery.jqueryui'], function(_) {


        /******************************************************************
         * AUTOCOMPLETE SHENANIGANS
         ******************************************************************/
        window.spanishAutocompleteCache = {};
        window.spanishAutocompleteQuery = null;

        /*
         * Custom autocomplete implementation for categories
         */
        $.widget( "custom.catcomplete", $.ui.autocomplete, {
            _renderMenu: function(ul, items) {
                var that = this;
                var currentCategory = '';
                $.each(items, function(index, item) {
                    if (item.category != currentCategory) {
                        var langCode = (item.category==='English') ? 'en' : 'es';
                        ul.append( "<li class='ui-autocomplete-category georgia_font' data-langcode='" + langCode + "'>" + item.category + "</li>" );
                        currentCategory = item.category;
                    }
                    that._renderItemData(ul, item);
                });
            }
        });

        /*
         * Instantiating autocomplete
         */
        var catComplete = $("#spanish_search").catcomplete({
            appendTo: "#autocomplete_holder",
            messages: {
                noResults: '',
                results: function() {}
            },
            position: {
                my: "left top",
                at: "left bottom"
            },
            minLength: 2,
            delay: 500,
            source: function(request, response) {
                var term = request.term;
                if (term in spanishAutocompleteCache) {
                    response(spanishAutocompleteCache[term]);
                    return;
                }
                $.getJSON( "/ac", request, function(data, status, xhr) {
                    /*
                     * Spanish comes first
                     */
                    var spanish = [];
                    var english = [];
                    _.forEach(data, function(entry) {
                        if ('category' in entry) {
                            if (entry.category === 'Spanish') {
                                spanish.push(entry);
                            }
                            if (entry.category === 'English') {
                                english.push(entry);
                            }
                        }
                    });
                    data = spanish.concat(english);
                    spanishAutocompleteCache[term] = data;

                    response(data);
                });
            },

            /*
             * On click or enter - add lang code and go there
             */
            select: function(event, ui) {
                limitSearchLength();
                submitSearch(ui.item.value, (ui.item.category === 'Spanish') ? 'es' : 'en');
            },

            /*
             * When focusing somewhere - set text for future language help
             */
            focus: function(event, ui) {
                $("#spanish_search").attr('data-lang_code', (ui.item.category === 'Spanish') ? 'es' : 'en');
                $("#spanish_search").attr('data-text_string', ui.item.value);
            },

            /*
             * Nulling after close
             */
            close: function(event, ui) {
                //$("#spanish_search").attr('data-lang_code', 'null');
                //$("#spanish_search").attr('data-text_string', 'null');
            }
        });

        /******************************************************************
         * INJECTING CHARCTERS AT CARET
         ******************************************************************/

        /*
         * This is required for search/translate
         */
        jQuery.fn.extend({
            insertAtCaret: function(myValue){
                return this.each(function(i) {
                    if (document.selection) {
                        //For browsers like Internet Explorer
                        this.focus();
                        var sel = document.selection.createRange();
                        sel.text = myValue;
                        this.focus();
                    }
                    else if (this.selectionStart || this.selectionStart == '0') {
                        //For browsers like Firefox and Webkit based
                        var startPos = this.selectionStart;
                        var endPos = this.selectionEnd;
                        var scrollTop = this.scrollTop;
                        this.value = this.value.substring(0, startPos)+myValue+this.value.substring(endPos,this.value.length);
                        this.focus();
                        this.selectionStart = startPos + myValue.length;
                        this.selectionEnd = startPos + myValue.length;
                        this.scrollTop = scrollTop;
                    } else {
                        this.value += myValue;
                        this.focus();
                    }
                });
            }
        });

        /*
         * Inserting at caret
         */
        $('[data-character]').on('click', function(e) {e.preventDefault();e.stopPropagation();e.stopImmediatePropagation();
            if ( $('[name=spanish_search]').val() ==  $('[name=spanish_search]').attr('data-placeholder')) {
                 $('[name=spanish_search]').val('').removeClass('spanish_search_form_placeholder');
            }
            $('#spanish_search').insertAtCaret($(this).attr('data-character'));
            $('#spanish_search').trigger('focus');

            /*
             * Also - autocomplete closed - so let's reopen it
             */
            catComplete.catcomplete("search", $('#spanish_search').val()).focus();
        });


        /******************************************************************
         * SUBMITTING AND CHAR LIMITING
         ******************************************************************/
        /*
         * This action redirects user to a search page
         */
        var submitSearch = function(textToSubmit, langCode) {
            var textArea = $('[name=spanish_search]');

            /*
             * Don't submit if search contains default text
             */
            if (textToSubmit === textArea.attr('data-placeholder')) {
                return;
            } else {
                /*
                 * If value is the same that was last clicked - we then should submit
                 * a language code along with search
                 */
                var postfix = '';
                if (langCode !== null) {
                    postfix = '?l=' + langCode;
                }

                /*
                 * This is temporary fix that strips linefeeds
                 */
                textToSubmit = textToSubmit.replace(/(\r\n|\n|\r)/gm,' ');

                /*
                 * Url-encoding that word
                 */
                textToSubmitEncoded = encodeURIComponent(textToSubmit);

                /*
                 * Mantis ticket 1849: Any words that contain + character should get replaced with %20
                 * P.S. This is silly illogical to me. (Andrei)
                 */
                textToSubmitEncoded = textToSubmitEncoded.replace('%2B', '%20');

                /*
                 * Submit a search
                 */
                window.location = '/translate/' + textToSubmitEncoded + postfix;
            }
        };

        /*
         * This action checks textarea length and limits it
         */
        var limitSearchLength = function() {
            var textArea = $('[name=spanish_search]');
            textArea.val(textArea.val().substring(0, 400));
        };

        /*
         * This is what happens on ENTER in textarea
         */
        $('#spanish_search_form').on('keydown', '[name=spanish_search]', function(e) {
            var textField  = $(this);
            if (e.keyCode == 13) {
                /*
                 * Submitting the search on enter
                 */
                e.preventDefault();
                e.stopPropagation();

                /*
                 * Submit a search
                 */
                var textFieldTextString = textField.attr('data-text_string');
                var textFieldLangCode = ((textField.attr('data-lang_code') !== 'null') && (textFieldTextString===textField.val())) ? textField.attr('data-lang_code') : null;

                limitSearchLength();
                submitSearch(textField.val(), textFieldLangCode, textFieldTextString);
            } else {
                /*
                 * Nulling autocomplete lang code on anything else
                 */
                //textField.attr('data-text_string', 'null');
                //textField.attr('data-lang_code', 'null');
            }
        });


        /*
         * this is what happens on textarea click and keyup.
         * (purely to limit search entry length)
         */
        $('[name=spanish_search]').on('keyup change', function(e) {
            limitSearchLength();
            if ($(this).val().length === 0 && $(this).val() !== $(this).data('placeholder')) {
                $('.search-close').removeClass('search-close-override');
            } else {
                $('.search-close').addClass('search-close-override');
            }
        });

        /*
         * this is what happens on translate click
         */
        $('.translate-trigger').on('click', function(e) {e.preventDefault();
            limitSearchLength();
            submitSearch($("#spanish_search").val(), null);
        });

        $body         = $('body');
        $mainWrapper  = $('#content_wrapper_central_right_bottom_left');
        $closeButton  = $('.search-close');
        $searchInput  = $('[name=spanish_search]');

        var contain   = $body.hasClass('contain-search');
        var isDesktop = function() {
            return (window.innerWidth > 767);
        };

        function searchDidFocus() {
            if (! contain) {
                $body.addClass('contain-search');
            }

            $('html, body').animate({
                scrollTop: 0
            }, 10);

            $body.addClass('search-focus');

            $searchInput.catcomplete('search');

            if (typeof killDfpAds !== 'undefined') {
                killDfpAds(['ad_slot_bottom']);
            }
        }

        function searchDidBlur() {
            if (! contain) {
                $body.removeClass('contain-search');
            }
            $body.removeClass('search-focus');
            googletag.pubads().refresh();
        }

        $closeButton.on('click touchend', function(e) {
            e.preventDefault();
            $searchInput.val('');
            $closeButton.removeClass('search-close-override');
            $searchInput.focus();
        });

        /*
         * Implementing cross-browser placeholder
         */
        $searchInput.focus(function() {
            var input = $(this);
            if (input.val() == input.attr('data-placeholder')) {
                input.val('').removeClass('spanish_search_form_placeholder');
            }
            searchDidFocus();
        }).blur(function(e) {
            var input = $(this);

            if (e.relatedTarget && ($(e.relatedTarget).is('.search-close')) || $(e.relatedTarget).is('.search-button')) {
                return;
            }

            if ((input.val() == '') || (input.val() == input.attr('data-placeholder'))) {
                input.addClass('spanish_search_form_placeholder');
                input.val(input.attr('data-placeholder'));
            }

            if (! isDesktop()) {
                searchDidBlur();
            }
        }).blur();

    });
</script>
