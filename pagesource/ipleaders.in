	<script>
		$('.contact-form .form-control').focusin(function(e){
			$('.contact-form label.active').removeClass('active');
			$(this).prev('label').addClass('active');
		});
		
		$(window).scroll(function() {	
		var windscroll = $(window).scrollTop();
		if (windscroll >= 0) {
				$('body section').each(function(i) {
						if ($(this).position().top <= windscroll) {
								$('a.page-scroll.active').removeClass('active');
								$("a.page-scroll[href='#"+$(this).attr('id')+"']").addClass('active');
						}
				});
		
		} else {
		
			 // $('nav').removeClass('fixed');
				//$('a.page-scroll.active').removeClass('active');
				//$('a.page-scroll:first').addClass('active');
		}
		
		}).scroll();
		$('a.page-scroll').on('click', function() {
		
		// var scrollAnchor = $(this);
		var $anchor = $(this);
				$('html, body').stop().animate({
					
						scrollTop: $($anchor.attr('href')).offset().top + 5
				}, 500);
				event.preventDefault();
				$anchor.addClass('active');
		//return false;
		
		});
	</script>
