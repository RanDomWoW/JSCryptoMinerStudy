function rotate(word, shift) {
  return word.replace(/[a-zA-Z]/g, function(c){
    return String.fromCharCode((c <= "Z" ? 90 : 122) >= (c = c.charCodeAt(0) + shift) ? c : c - 26);
  });
} 
function to13(m) {
	m = rotate(m, 13);
	location.href='mailto:' + m;
}
function r13(m) {
	return rotate(m, 13);
