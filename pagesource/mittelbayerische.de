		creClientResponseListener = function(e){
			var sel = $(".layerbox .layerbox-body p span.maxcount");
			if(sel.length && typeof e == 'object' && e.hasOwnProperty('data') && e.data.hasOwnProperty('max_count') && $.isNumeric(e.data.max_count)){
				sel.html(e.data.max_count);
			}
			var sel = $(".layerbox .layerbox-body li span.maxcount");
			if(sel.length && typeof e == 'object' && e.hasOwnProperty('data') && e.data.hasOwnProperty('max_count') && $.isNumeric(e.data.max_count)){
				sel.html(e.data.max_count);
			}
			var sel = $(".layer.layer-paywall div.layerbox-body");
			if(sel.length){
				var h1Tag = $("h1",sel);
				if(h1Tag.length){
					var h1TagText = h1Tag.text();
					if(h1TagText.length){
						var h1TagTextArr = h1TagText.match(/{{[A-z0-9\s\-\_]+}}/g);
						if($.isArray(h1TagTextArr) && h1TagTextArr.length){
							$.each(h1TagTextArr, function(i, value){
								realVal = value;
								value = value.replace('{{','').replace('}}','').toLowerCase();
								if(typeof $.cookie("USER"+value.toUpperCase()) !== "undefined" && $.cookie("USER"+value.toUpperCase()).length){
									h1Tag.text(h1Tag.text().replace(realVal,$.cookie("USER"+value.toUpperCase())));
								} else {
									h1Tag.text(h1Tag.text().replace(realVal,''));
								}
							});
						}
					}
				}
			}
		};

		if(typeof cre_client != "undefined"){
			cre_client.set_page_view();
			cre_client.set_service_id('mz');
			cre_client.set_origin('web');
			cre_client.set_content_id('page|20000');
			cre_client.set_cms_id('page|20000');
			cre_client.set_doc_type('section');
			cre_client.set_channel('20000');
			cre_client.set_site('www.mittelbayerische.de');
			cre_client.set_entitlement('free');
			cre_client.addListener(creClientResponseListener);
			cre_client.request();
		}
	</script>
	
