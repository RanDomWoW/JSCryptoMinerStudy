var MenuRightVisible = false;

function toggleSideMenu()
{
	if(MenuRightVisible) {
		hideMenuRight();
	} else {
		showMenuRight();
	}
}

function hideSideMenu()
{
	hideMenuRight();
}

function showMenuRight()
{
	BarRightWidth = $('#menu-right-wrapper').width();
	startPosRight = (BarRightWidth+30);
	if(BarRightWidth>400){
		BarRightWidth = 400;
	}
	BarRightWidth = 320;
	endPos = $('.wrapper').width(); //$(window).width();
	endPos -= BarRightWidth;
	startPos = $('.wrapper').width();

	$("body").append('<div class="navbarOverlay"></div>');

	$('#menu-right-wrapper').css({'position': 'absolute'})
	$('#menu-right-wrapper').width(BarRightWidth+'px');
	$('#menu-right-wrapper').height($(document).height());
	$('#menu-right-wrapper').css('left',endPos + 'px');
	$('#menu-right-wrapper').show();

	$('#menu-right').css('margin-left',(BarRightWidth + 30) + 'px');
	$('#menu-right').show();
	$('#menu-right').animate(
		{ marginLeft: '0px'},600,
		function() {
		}
    );

	MenuRightVisible = true;
}

function hideMenuRight()
{
	BarRightWidth = 320;
	$('#menu-right').animate(
		{ marginLeft: (BarRightWidth + 30) + 'px'},600,
		function() {
			$('#menu-right-wrapper').hide();
			$('.navbarOverlay').remove();
			MenuRightVisible = false;
		}
    );
}

$(document).ready(function()
{
	$(document).on('click', '.navbarOverlay' , function() {
		hideSideMenu();
	});

	$( "#side-menu" ).focusout(function() {
		alert('out');
		if($("#side-menu").is(":visible")){
			$("#side-menu").slideUp();
		}
	});

	$("[data-toggle=popover]").popover({html:true});
});
