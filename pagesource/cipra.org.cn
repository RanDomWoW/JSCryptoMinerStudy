 <script type="text/javascript" language="javascript">
     function CheckForms() {
         var name = $("#tbUserName").val();
         var password = $("#tbPassWord").val();
         if (name == null || name == "") {
             alert("用户名不能为空");
             $('#tbUserName').focus();
             return false;
         }
         if (password == null || password == "") {
             alert("密码不能为空");
             $('#tbPassWord').focus();
             return false;
         }
//         var ValidateCode = ReadCookie("WebPlugValidateCode1");
//         if ($("#tbVerificationCode").val() != null && $("#tbVerificationCode").val() != "") {
//             if ($("#tbVerificationCode").val() != ValidateCode) {
//                 alert('验证码输入不正确');
//                 $('#tbVerificationCode').focus();
//                 return false;
//             }
//         }
//         else {
//             alert('验证码不能为空');
//             $('#tbVerificationCode').focus();
//             return false;
//         }
         if (name != "" && password != "") {
             $.ajaxSetup({ async: false }); //设置同步
             var url = "MemberInfo/ajax/ajaxLogin.aspx?name=" + escape(name) + "&password=" + escape(password) + "&time=" + new Date().getTime();
             $.get(url, function (date) {
                 if (date != null && date != "" && date == "Success") {
                     location.href = "MemberInfo/index.aspx";
                 }
                 else if (date == "None") {
                     alert('用户名不存在');
                     $('#trUserName').focus();
                     return false;
                 }
                 else if (date == "error") {
                     alert('密码错误');
                     $('#trPassWord').focus();
                     return false;
                 }
                 else if (date == "NoCheck") {
                     alert('对不起，你的账号目前处于审核状态');
                     $('#trUserName').focus();
                     return false;
                 }
                 else {
                     alert('对不起，系统忙请稍后再登陆');
                     return false;
                 }
             });
         }
     }
     function LoginEnter(e) {
         var keycode = 0;
         if (window.event) {
             keycode = event.keyCode;
         }
         else {
             keycode = e.which;
         }
         if (keycode == 13) {
             CheckForms();
             return false;
         }
         else {
             return true;
         }
     }
    </script>
<script language="javascript" type="text/javascript">
    window.onload = function () {
        var timer, timer1;
        var elem = document.getElementById('elem');
        var elem1 = document.getElementById('elem1');
        var elem2 = document.getElementById('elem2');
        elem2.innerHTML = elem1.innerHTML;
        timer = setInterval(Scroll, 50);
        function Scroll() {

            if (elem.scrollTop >= elem1.offsetHeight) {
                elem.scrollTop -= elem1.offsetHeight;
            } else {
                elem.scrollTop += 1;
                if (elem.scrollTop % 94 == 0 && elem.scrollTop != 0) {
                    clearInterval(timer);
                    timer1 = setTimeout(function () {
                        timer = setInterval(Scroll, 50)
                    }, 8000); //滚动时间 每3秒滚动下一条
                }
            }
        }

        elem.onmouseover = function () {
            clearInterval(timer);
            clearTimeout(timer1);
        }

        elem.onmouseout = function () {
            timer = setInterval(Scroll, 50);
        }
    }
</script>
</form>
