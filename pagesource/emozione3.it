<script>
    (function(){
        // Get url pathname e.g '/fr/nos-smartbox/sejour'
        // split into an array of directory names
        var pathString = document.location.pathname,
            pathArray = pathString.split("/"),
            elemsRemoved = 0;

        // remove 'whitespace-only' entries in array
        pathArray = pathArray.filter(function(str) {
            return /\S/.test(str);
        });

        // iterate through keyword array
        for (var i = 0; i < pathArray.length; i++) {
            // find & hide any associated universe elements
            var idElem = document.getElementById("universe-" + pathArray[i]);
            if (idElem) {
                idElem.parentNode.removeChild(idElem);
                elemsRemoved++;
            }
        }

        // if elements are removed, reset grid cols to match (default: 5)
        if (elemsRemoved > 0) {
            document.getElementById("universe-blocks-list").className = document.getElementById("universe-blocks-list").className.replace(/\large-block-grid-5\b/, 'large-block-grid-' + (5 - elemsRemoved));
        }
    })();
</script>

