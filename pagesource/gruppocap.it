<script type="text/javascript">

    $("#menusmalllivello1").hide();
    $("#menusmallattivita").hide();
    $("#menusmallgruppo").hide();

    $(document).ready(function () {
        
        $("#accordiongruppo3small").accordion({ active: false, collapsible: true });
        $("#accordiongruppo4small").accordion({ active: false, collapsible: true });
        $("#accordiongruppo5small").accordion({ active: false, collapsible: true });
        $("#accordiongruppo6small").accordion({ active: false, collapsible: true });

        $("#accordion-gruppo-small").accordion({ active: false, collapsible: true });
        $("#accordion-attivita-small").accordion({ active: false, collapsible: true });

        $('.second-level-accordion-small').each(function (i, e) {
            $(this).accordion({ active: false, collapsible: true });
        });

        $('.second-level-accordion-small h3 > a').click(function (e) {
            window.location = $(this).attr('href');

            return false;
        });
        $('#accordiongruppo5small h3 > a').click(function (e) {
            window.location = $(this).attr('href');

            return false;
        });

        $("#togglemenusmall").click(function () {
            var stato = $(this).attr('src');

            if (stato == '/Skins/GruppoCAP/img/apri.jpg') {
                $(this).attr('src', '/Skins/GruppoCAP/img/chiudi.jpg');
                $("#menusmalllivello1").show(500);
            }

