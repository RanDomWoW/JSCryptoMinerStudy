<script type="text/javascript">

	function MobileAuth(){
		$("#AuthNum").empty();

    	if ($("#HandPhone2").val()=="")
    	{
    		alert('두번째 핸드폰번호를 입력해주세요.');
    		$("#HandPhone2").focus();
    		return;
    	}
    	if ($("#HandPhone3").val()=="")
    	{
    		alert('세번째 핸드폰번호를 입력해주세요.');
    		$("#HandPhone3").focus();
    		return;
    	}

    	if ($("#Number_chk").val()=="1")
    	{
    		alert("인증번호를 입력해주세요.\nSMS가 발송되지 않은경우 관리자에게 문의하세요.");
    		return;
    	}

        var Number_chk = $('#Number_chk').val();
        var HandPhoneNumber = $('#HandPhone1').val()+$('#HandPhone2').val()+$('#HandPhone3').val();

		var url="/member/prcSmsHpCK2.asp?SMSNewNumber_chk="+Number_chk+"&SMSNewNumber="+HandPhoneNumber;

		$.ajax({
			type:"get"	
			,url:url
			,dataType:"json"
			,cache:false 
			,success:function(data){
				var Result = data.Result;
				if (Result == "Y")
				{
					if (data.SMSAuthNum!=undefined){
           				$("#AuthNum").val(data.SMSAuthNum);
						alert(data.sResultMsg);
					}
				}else{
    				alert(data.sResultMsg);
				}
			}
			,error:function(e) {
				alert(e.responseText);
			}
		});	
    }

    function do_smsCheck(){

    	var obj = document.frm_hpchk2;

    	if ($("#HandPhone2").val()=="")
    	{
    		alert('핸드폰번호를 입력해주세요.');
    		$("#HandPhone2").focus();
    		return;
    	}
    	if ($("#HandPhone2").val()=="")
    	{
    		alert('핸드폰번호를 입력해주세요.');
    		$("#HandPhone2").focus();
    		return;
    	}
    	if ($("#AuthNum").val()=="N")
    	{
    		alert("휴대폰 인증이 필요합니다.");
    		return;
    	}
    	if ($("#HandPhoneChk").val()=="")
    	{
    		alert("인증번호를 입력해주세요.");
    		$("#HandPhoneChk").focus();
    		return;
    	}
    	if ($("#HandPhoneChk").val() != $("#AuthNum").val())
    	{
    		alert("입력하신 인증번호가 다릅니다. 인증번호를 정확히 입력해주세요.");
    		$("#HandPhoneChk").focus();
    		return;
    	} else {
    		alert("입력하신 인증번호가 일치합니다.\nSMS수신 및 개인정보수집활용에 대한 동의하신 후 무료추천주신청을 클릭해주세요.");
		}
    }

    function do_SMSsubmit(){

		//alert("hi");
    	var obj = document.frm_hpchk2;

    	if ($("#UserName").val()=="")
    	{
    		alert('이름을 입력해주세요.');
    		$("#UserName").focus();
    		return;
    	}
    	if ($("#HandPhone2").val()=="")
    	{
    		alert('핸드폰번호를 입력해주세요.');
    		$("#HandPhone2").focus();
    		return;
    	}
    	if ($("#HandPhone3").val()=="")
    	{
    		alert('핸드폰번호를 입력해주세요.');
    		$("#HandPhone3").focus();
    		return;
    	}
    	if ($("#HandPhoneChk").val()=="")
    	{
    		alert("인증번호를 입력해주세요.");
    		$("#HandPhoneChk").focus();
    		return;
    	}
    	if ($("#HandPhoneChk").val() != $("#AuthNum").val())
    	{
    		alert("입력하신 인증번호가 다릅니다. 인증번호를 정확히 입력해주세요.");
    		$("#HandPhoneChk").focus();
    		return;
    	} 
    	if ($("#agreechk1").prop("checked") != true )
    	{
    		alert('SMS 수신에 동의하셔야 무료 추천주를 받으실수 있습니다.');
    		$("#agreechk1").focus();
    		return;
    	}

		/*
    	if ($("#agreechk2").prop("checked") != true )
    	{
    		alert('개인정보 수집과 활용에 동의 하셔야 무료 추천주를 받으실수 있습니다.');
    		$("#agreechk2").focus();
    		return;
    	}
		*/

  		obj.submit();
    }

</script>


