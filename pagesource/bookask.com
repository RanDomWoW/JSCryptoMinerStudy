</script><script src="/pc/$css$/common.js?v=1.1.2"></script><script src="/pc/$css$/ScrollText.js"></script><script type="text/javascript">	var pn =1;	var tid="";	var loadpage=true;	function scrollActive() {		if ($(document).scrollTop()>=230) {			$(".top-box ").addClass("scroll");			$(".main").css("padding-top","304px");			//$(".article-r").addClass("scroll");		} else {			$(".top-box ").removeClass("scroll");			$(".main").css("padding-top","0");			//$(".article-r").removeClass("scroll");		}		if ($(document).scrollTop()>=910&&$(document).width()>1000) {			$(".tag-box").addClass("scroll");		} else {			$(".tag-box").removeClass("scroll");		}		if ($(document).scrollTop()>=310&&$(document).width()>1000) {			$(".feilei-tit").addClass("scroll");			$(".bottom-box").css("padding-top","40px");		} else {			$(".feilei-tit").removeClass("scroll");			$(".bottom-box").css("padding-top","0");		}		if($(".main .scroll").length>0){			$("#ulItems").hide();		}	}	$(window).scroll(function(){		scrollActive();		if (getScrollTop() + getClientHeight()>=getScrollHeight()-5 && getScrollTop() + getClientHeight() <= getScrollHeight()+5) {			if(loadpage){				pn=pn-(-1);				getarticle(pn,tid);			}		}	});	window.onresize=function(){		scrollActive();	}	$(".tit-box li").on("click",function(){		tid=this.getAttribute("data-tid");		pn=1;		$(".tit-box a").removeClass("active");		$(this).find("a").addClass("active");		loadpage=true;		getarticle(pn,tid);	})	//获取文章	function getarticle(pn,tid){		var url = '/find/moreArticle.html';		tid=tid?tid:0;		if(!loadpage){			return false;		}else{			loadpage=false;			$.get(url,{'j':1,'pn':pn,'tid':tid},function(data){				data=JSON.parse(data);				if(data.length<=0){					$(".load-more").html("暂无更多数据");				}else{					var cont=loaddata(data);					if(pn==1){						$("#news-main").html("");					}					$("#news-main").append(cont);				}				loadpage=true;			},'html');		}	}	//获取滚动条当前的位置	function getScrollTop(){		var scrollTop = 0;		if (document.documentElement && document.documentElement.scrollTop) {			scrollTop = document.documentElement.scrollTop;		}		else if (document.body) {			scrollTop = document.body.scrollTop;		}		return scrollTop;	}	//获取当前可视范围的高度	function getClientHeight(){		var clientHeight = 0;		if (document.body.clientHeight && document.documentElement.clientHeight) {			clientHeight = Math.min(document.body.clientHeight, document.documentElement.clientHeight);		}		else {			clientHeight = Math.max(document.body.clientHeight, document.documentElement.clientHeight);		}		return clientHeight;	}	//获取文档完整的高度	function getScrollHeight(){		return Math.max(document.body.scrollHeight, document.documentElement.scrollHeight);	}	$(document).ready(function(){		scrollActive();		$(".fenlei-more").click(function(){			$(".tit-box-more").slideToggle();			$(".tit-unfold").toggle();			$(".tit-top").toggle();		});	});	//加载数据	function loaddata(data){		var cont="";		if(data!=null)		{			for(var i=0;i<data.length;i++){				if(data[i]['sid']==0 && i==9){					cont+='<div class="article-item article1">';					cont+='<h3><a href="/find/'+data[i]['id']+'.html" title="'+data[i]['title']+'" class="ellipsis-2" target="_blank">'+data[i]['title']+'</a></h3>';					cont+='<div class="fl home-author">';					cont+='<span class="text-truncate">阅读'+data[i]['num']+'</span><span>'+getdate(data[i]['modifieddate'])+'</span>';					cont+='</div>';					cont+='<a href="javascript:void(0);" class="fr home-fenlei text-truncate" target="_blank">';					cont+=data[i]['cata_name'];					cont+='</a>';					cont+='<div class="clear"></div>';					cont+='<div class="home-line-x"></div>';					cont+='</div>';				}else if(data[i]['pics'] &&　data[i]['pics'][2]){					cont+='<div class="article-item article2">';					cont+='<h3><a href="/find/'+data[i]['id']+'.html" title="'+data[i]['title']+'" class="ellipsis-1" target="_blank">'+data[i]['title']+'</a></h3>';					cont+='<div class="three-box">';					cont+='<a href="/find/'+data[i]['id']+'.html" class="three fl" target="_blank">';					cont+='<img src="'+data[i]['pics'][0]+'">';					cont+='</a>';					cont+='<a href="/find/'+data[i]['id']+'.html" class="three fl" target="_blank">';					cont+='<img src="'+data[i]['pics'][1]+'">';					cont+='</a>';					cont+='<a href="/find/'+data[i]['id']+'.html" class="three fl" style="margin-right:0" target="_blank">';					cont+='<img src="'+data[i]['pics'][2]+'">';					cont+='</a>';					cont+='<div class="clear"></div>';					cont+='</div>';					cont+='<div class="fl home-author">';					cont+='<span class="text-truncate">阅读'+data[i]['num']+'</span><span>'+getdate(data[i]['modifieddate'])+'</span>';					cont+='</div>';					cont+='<a href="javascript:void(0);" class="fr home-fenlei text-truncate" target="_blank">';					cont+=data[i]['cata_name'];					cont+='</a>';					cont+='<div class="clear"></div>';					cont+='<div class="home-line-x"></div>';					cont+='</div>';				}else{					cont+='<div class="article-item article3">';					cont+='<div class="article-cover fl">';					cont+='<a href="/find/'+data[i]['id']+'.html" target="_blank">';					cont+='<img src="'+data[i]['path']+'">';					cont+='</a>';					cont+='</div>';					cont+='<div class="article-text fr">';					cont+='<h3><a href="/find/'+data[i]['id']+'.html" title="'+data[i]['title']+'" class="ellipsis-2" target="_blank">'+data[i]['title']+'</a></h3>';					cont+='<div class="article-text-info">';					cont+='<div class="fl home-author">';					cont+='<span class="text-truncate">阅读'+data[i]['num']+'</span><span>'+getdate(data[i]['modifieddate'])+'</span>';					cont+='</div>';					cont+='<a href="javascript:void(0);" class="fr home-fenlei text-truncate" target="_blank">';					cont+=data[i]['cata_name'];					cont+='</a>';					cont+='</div>';					cont+='</div>';					cont+='<div class="clear"></div>';					cont+='<div class="home-line-x"></div>';					cont+='</div>';				}			}		}		return cont;	}	//获取时间	function getdate(timestamp){		var curtimestamp=Date.parse(new Date())/1000;//当前时间戳		var cha=curtimestamp-timestamp;//当前时间戳与传入时间戳的差值		var resdate="";		if(cha<60)//1分钟以内		{			resdate="刚刚";		}else if(cha<60*60){//1小时以内			resdate=parseInt(cha/60)+" 分钟";		}else if(cha>=60*60 && cha<60*60*2){//1小时到2小时			resdate="1小时前";		}else{			var newDate = new Date();			newDate.setTime(timestamp * 1000);			/* resdate=newDate.format('yyyy-MM-dd h:m:s'); */			resdate=newDate.format('yyyy-MM-dd');		}		return resdate;	}	Date.prototype.format = function(format) {		var date = {			"M+": this.getMonth() + 1,			"d+": this.getDate(),			"h+": this.getHours(),			"m+": this.getMinutes(),			"s+": this.getSeconds(),			"q+": Math.floor((this.getMonth() + 3) / 3),			"S+": this.getMilliseconds()		};		if (/(y+)/i.test(format)) {			format = format.replace(RegExp.$1, (this.getFullYear() + '').substr(4 - RegExp.$1.length));		}		for (var k in date) {			if (new RegExp("(" + k + ")").test(format)) {				format = format.replace(RegExp.$1, RegExp.$1.length == 1						? date[k] : ("00" + date[k]).substr(("" + date[k]).length));			}		}		return format;	}	/***********************	 * 函数：绑定鼠标滚轮上下滚动事件 相对的操作	 * 参数：event	 * 返回：滚轮方向 1：向上 -1：向下	 * 		FireFox 向上滑动detail值为—3,向下为3;	 * 		Ie      向上滑动wheelDelta 值为120，向下为-120	 *************************/	var scrollFunc=function(e){		//var direct=0;		e=e || window.event;		if(e.wheelDelta){//IE/Opera/Chrome			if(e.wheelDelta==120)			{				$(".home-foot").addClass("scroll");				//向上滚动事件			} else {				$(".home-foot").removeClass("scroll");	//向下滚动事件			}		} else if (e.detail) {//Firefox			if(e.detail==-3) {				$(".home-foot").addClass("scroll"); //向上滚动事件<br>			} else {				$(".home-foot").removeClass("scroll");//向下滚动事件<br>			}		} //		ScrollText(direct); } /*注册事件*/	if(document.addEventListener){ document.addEventListener('DOMMouseScroll',scrollFunc,false); }//W3C	window.onmousewheel=document.onmousewheel=scrollFunc;	//IE/Opera/Chrome	$(document).ready(function(){		$(".hot>li").mouseenter(function(){			$(this).addClass("active").siblings().removeClass("active");		});	});	window.onload=function(){		$.get('/api.php?u/u.html&aparams={"action":"getmsg"}',function(data){			if(data){				var cont="";				if(data['qlst'] && data['qlst'].length>0){//群发					var readmsg=getCookie("READMSG");					for(var i=0;i<data['qlst'].length;i++){						if(readmsg==null || readmsg.indexOf(data['qlst'][i]['id']+",")<0){							cont+='<li data-type="1" data-msgid="'+data['qlst'][i]['id']+'">';							cont+='<a href="/u/msgdetail/'+data['qlst'][i]['id']+'.html">';							cont+='<h3 class="home-mes-con ellipsis-2"><span>'+data['qlst'][i]['title']+'</span></h3>';							cont+='<div class="home-mes-time">'+data['qlst'][i]['t1']+'</div>';							cont+='</a>';							cont+='</li>';						}					}				}				if(data['dlst'] && data['dlst'].length>0){//单发					for(var j=0;j<data['dlst'].length;j++){						if(data['dlst'][j]['readtimes']<=0){							cont+='<li data-type="2" data-msgid="'+data['dlst'][j]['id']+'">';							cont+='<a href="/u/msgdetail/'+data['dlst'][j]['id']+'.html">';							cont+='<h3 class="home-mes-con ellipsis-2"><span>'+data['dlst'][j]['title']+'</span></h3>';							cont+='<div class="home-mes-time">'+data['dlst'][j]['t1']+'</div>';							cont+='</a>';							cont+='</li>';						}					}				}				if(cont.length>0){					$(".home-mes-list").html(cont);					$(".home-mes-dot").show();				}else{					$(".noread-lst").hide();					$(".read-all-btn").hide();					$(".home-mes-none").show();				}			}		});	}	function wrmsg(obj){		var type=$(obj).attr("data-type");		var msgid=$(obj).attr("data-msgid");		if(type==1){			var readmsg=getCookie("READMSG");			var str="";			if(readmsg){				if(readmsg.indexOf(msgid+",")<0){					str=readmsg+msgid+',';				}else{					str=readmsg;				}			}else{				str=msgid+',';			}			setCookie("READMSG",str,7500);		}		if(type==2){			$.post('/api.php?u/u.html&aparams={"action":"upmsgread","params":{"id":"'+msgid+'"}}',function(data){			});		}	}	function readall(){		var msglst=$(".home-mes-list li");		$.each(msglst,function(i,v){			wrmsg(v);		});		$(".read-all-btn").attr("href","javascript:void(0);");		$(".read-all-btn").text("消息已标为已读");	}	//写cookies	function setCookie(name,value,days)	{		var Days = days;		var exp = new Date();		exp.setTime(exp.getTime() + Days*24*60*60*1000);		document.cookie = name + "="+ escape (value) + ";expires=" + exp.toGMTString();	}	function getCookie(name)	{		var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");		if(arr=document.cookie.match(reg))			return unescape(arr[2]);		else			return null;	}	if(window.attachEvent)	{		window.attachEvent("onload",function() {initpage();});	}	else	{		window.addEventListener("load",function() {initpage();},true);	}	function initpage(){		var name=getCookie("username");		var logo=getCookie("ulogo");		if(name){			$(".login-logo img").attr("src",logo);			$(".login-logo span").text(decodeURIComponent(escape(name)));			$(".login-logo").show();		}else{			$(".nologin-logo").show();		}		var img=new Image();		img.src="/api.php?web/cachefile.html";	}</script></body></html>