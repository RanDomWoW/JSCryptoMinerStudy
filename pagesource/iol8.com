	<script>
		$(function(){
			var htmlobjh = $.ajax({url:"common/view/header.html",async:false});
			$('.myHeader').html(htmlobjh.responseText);
			var htmlobjf = $.ajax({url:"common/view/footer.html",async:false});
			$('.myFooter_b').html(htmlobjf.responseText);
			
			mouse("i_platform_b");
			mouse("i_facilities_b");
			mouse("i_service_b");
			
			function mouse(dom){
				$('.'+dom+' ul li').on('mouseenter',function(){
					$(this).addClass('active').stop().siblings().removeClass('active');
				})
			}
			var timer = null;
			window.onscroll = function(){
				var cleX = $(window).scrollTop();
		   		var h = $('header').height();
		   		var accessY = $('.access').offset().top;
		   		if(cleX>accessY-h){
		   			$('header').css('background','rgba(0,0,0,1)')
		   		}else{
		   			$('header').css('background','rgba(0,0,0,0)')
		   		}
				//getDom("access");
//				getAmi("access");
				getAmi('i_facilities_b');
				getAmi('i_service_b');
				getAmi('i_platform_b');
				
			};
			function getNum(addNum,s,data,time,fixed){
				var i = 0;
				timer = setInterval(function(){
					i+=addNum;
					if(i>=data){
						$('.nu_'+s+'').html(data);
						clearInterval(timer);
					}else{
						$('.nu_'+s+'').html(i.toFixed(fixed));
					}
					
				},time)
			}
			//请求后台数据
			getBackstage();
			function getBackstage(){
				$.ajax({
					type:"post",
					url:"https://www.iol8.com/iol8_api/number/getNumber",
					async:true,
					dataType:"jsonp",
					jsonp:'jsonpcallback', 
					success:function(res){
						//console.log(res);
						if(res.result==1){
							$('.nu_1').html(res.data.translateLetterCounts);
							$('.nu_2').html(res.data.clientCounts);
							$('.nu_3').html(res.data.serverCounts);
						}
					}
				});
			}
			var myTimer = null;
			myTimer = setInterval(function(){
				getBackstage();
			},30000);
//			function getDom(dom){
//	   			var t = $(window).scrollTop();
//		   		var m = $(window).height();
//		   		var n = $('.'+dom+'').offset().top-m-50;
//		   		if(t>n&&t<n+m){
//		   			if($('.'+dom+'').attr('data-t')==0){
//		   				getNum(3,1,230,50,0);
//						getNum(3,2,100,50,0);
//						getNum(3,3,30,50,0);
//		   				$('.'+dom+'').attr('data-t',1);
//		   			}else{
//		   				return false;
//		   			}
//		   		}
//	   		}
			
			function getAmi(dom){
	   			var t = $(window).scrollTop();
		   		var m = $(window).height();
		   		var n = $('.'+dom+'').offset().top-m+50;
		   		if(t>n&&t<n+m){
		   			if($('.'+dom+'').attr('data-s')==0){
		   				$('.'+dom+'').attr('data-s',1);
		   				$('.'+dom+' .addAni').each(function(i){
		   					var className = $(this).attr('data-class');
		   					//alert(className);
		   					$(this).addClass(className);
		   				})
		   			}else{
		   				return false;
		   			}
		   		}
	   		}
			
			//点击出现弹框
			$('.iol_c').on('click',function(){
				$('.shadow').show();
				$('.ask').show();
			})
			
			
		})
