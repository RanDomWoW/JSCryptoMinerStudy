<script type="text/javascript">

// initTagsBg
(function initTagsBg(){
	var fixPositionX = - 7;
	var fixPositionY = - 40; 
	jQuery('.wp-tag-cloud').each(function(){
		var list = jQuery(this);
		var items = list.find('>li');
		var listOffset = list.offset();
		items.each(function(){
			var item = jQuery(this);
			var link = item.find('>a');
			var linkOffset = link.offset();
			var positionX = - (linkOffset.left - listOffset.left - fixPositionX);
			var positionY = - (linkOffset.top - listOffset.top - fixPositionY);
			link.css({backgroundPosition: positionX +'px ' + positionY + 'px'});
		});
	});
})();

</script>




