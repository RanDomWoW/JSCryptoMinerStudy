<style>
[data-title] {
position: relative;
}
[data-title]:hover:before {
visibility: visible;
opacity: 0.95; /* 透明度 */
bottom: calc(100% + 12px);
box-shadow: 0 0 5px #666; /* 外框陰影 */
}
[data-title]:hover:after {
visibility: visible;
opacity: 0.90; /* 透明度 */
bottom: calc(100% + 2px);
}
[data-title]:before {
visibility: hidden;
position: absolute;
content: attr(data-title);
background: #6fa8dc; /* 背景顏色 */
color: #fff; /* 訊息顏色 */
width: 130px;
padding: 5px;
left: calc((100% - 140px) / 2);
bottom: calc(100% + 40px);
font-size: 16px; /* 文字大小 */
line-height: 140%;
border-radius: 4px;
text-align: center;
white-space: pre-line;
word-wrap: break-word;
opacity: 0;
transition: All 0.5s ease; /* 動畫效果&#12289;時間 */
-webkit-transition: All 0.5s ease;
-moz-transition: All 0.5s ease;
-o-transition: All 0.5s ease;
}
[data-title]:after {
visibility: hidden;
position: absolute;
color: #6fa8dc; /* 背景顏色 */
left: calc(50% - 11px);
content: '';
bottom: calc(100% + 50px);
width: 0px;
height: 0px;
border-width: 11px 11px 0px;
border-style: solid dashed dashed dashed;
border-left-color: transparent;
border-right-color: transparent;
opacity: 0;
transition: All 0.5s ease; /* 動畫效果&#12289;時間 */
-webkit-transition: All 0.5s ease;
-moz-transition: All 0.5s ease;
-o-transition: All 0.5s ease;
}


