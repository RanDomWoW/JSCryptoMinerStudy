  <script>
    // 工具
(function() {
window._ajax = function(settings) {
    settings || (settings = {});
    var xmlhttp;
    if (window.XMLHttpRequest) {
        xmlhttp = new XMLHttpRequest();
    } else { // ie6
        xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
    }
    xmlhttp.onreadystatechange = function() {
        if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
            settings.success && settings.success(xmlhttp.responseText);
        }
    };
    xmlhttp.open(settings.method || 'GET', settings.url, true);
    xmlhttp.send();
}
})();

// 首页搜索统计
(function() {
  var eApiCount1 = document.getElementById('api_count_1');
  if (!eApiCount1) {;
      return;
  }
  var eApiCount7 = document.getElementById('api_count_7');
  window._ajax({
    url: '/node/api_count?show=1&t=' + parseInt(Date.now()/100000),
    success: function(data) {
        data = JSON.parse(data);
        var result = '';
        data.day1.forEach(function(i) {
            result += '<tr><td><a href="/api/' + i.module + '.html?count=0#' + i.module + '_' + i.id + '">' + i.syntax + '</a></td><td>' + i.count + '</td></tr>';
        });
        eApiCount1.innerHTML = result; 
        document.getElementById('api_count_1_time').innerHTML = data.day1Time;
        result = '';
        data.day7.forEach(function(i) {
            result += '<tr><td><a href="/api/' + i.module + '.html?count=0#' + i.module + '_' + i.id + '">' + i.syntax + '</a></td><td>' + i.count + '</td></tr>';
        });
        eApiCount7.innerHTML = result;    
        document.getElementById('api_count_7_time').innerHTML = data.day7Time;
    }
  });
})();
    </script>
