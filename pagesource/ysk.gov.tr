    <script>
        $(document).ready(
            function () {
                var bildirimWidth;
                var path = '/app-resources/bildirim.html';
                $.get(path, function (result) {
                    var html = jQuery('<div>').html(result);
                    var show = html.find("div#bildirim-ozellikleri").attr(
                        "data-show");
                    if (show == 'true') {
                        var bildirimWidth = html.find(
                            "div#bildirim-ozellikleri").attr(
                            "data-bildirim-width");
                        $("#extruderRight").buildMbExtruder(
                            {
                                position: "right",
                                width: bildirimWidth,
                                extruderOpacity: 1,
                                positionFixed: false,
                                hidePanelsOnClose: true,
                                accordionPanels: true,
                                onExtOpen: function () {
                                    $('#bildirim-panel-hover').addClass(
                                        'bildirim-durum');
                                },
                                onExtContentLoad: function () {

                                },
                                onExtClose: function () {
                                    $('#bildirim-panel-hover').removeClass(
                                        'bildirim-durum');
                                }
                            });
                    }

                });


                var baglanti = $("#baglantilar");

                baglanti.owlCarousel({
                    items: 6, //10 items above 1000px browser width
                    itemsDesktop: [1000, 2], //5 items between 1000px and 901px
                    itemsDesktopSmall: [900, 2], // betweem 900px and 601px
                    itemsTablet: [600, 2], //2 items between 600 and 0
                    itemsMobile: [600, 2]
                    // itemsMobile disabled - inherit from itemsTablet option
                });

                // Custom Navigation Events
                $(".baglanti-next").click(function () {
                    baglanti.trigger('owl.next');
                })
                $(".baglanti-prev").click(function () {
                    baglanti.trigger('owl.prev');
                })

                var hizli = $("#hizli-erisim");

                hizli.owlCarousel({
                    items: 1, //10 items above 1000px browser width
                    itemsDesktop: [1000, 1], //5 items between 1000px and 901px
                    itemsDesktopSmall: [900, 1], // betweem 900px and 601px
                    itemsTablet: [600, 1], //2 items between 600 and 0
                    itemsMobile: [600, 1]
                    // itemsMobile disabled - inherit from itemsTablet option
                });

                // Custom Navigation Events
                $(".hizli-next").click(function () {
                    hizli.trigger('owl.next');
                })
                $(".hizli-prev").click(function () {
                    hizli.trigger('owl.prev');
                })

                $("#animated-container").fadeOut('slow');

            });

        window.onscroll = function () {
            scrollFunction()
        };

        function scrollFunction() {
            if (document.body.scrollTop > 20
                || document.documentElement.scrollTop > 20) {
                document.getElementById("topBtn").style.display = "block";
            } else {
                document.getElementById("topBtn").style.display = "none";
            }
        }

        // When the user clicks on the button, scroll to the top of the document
        function topFunction() {
            $("html, body").animate({ scrollTop: "0px" });
            //document.body.scrollTop = 0; // For Chrome, Safari and Opera
            //document.documentElement.scrollTop = 0; // For IE and Firefox
        }
    </script>
