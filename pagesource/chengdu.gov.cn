$(function () {		
	//搜索地址以及热搜索词获取范围
	//jsonp 跨域
	getHotWord("http://new.chengdu.gov.cn/search/","jsonp","5101000028","all");
});

function getHotWord(url,type,siteCode,database){
	$.ajax({  
	     type : "post",  
	     async:false,  
	     url : url+"hotword?type="+type+"&siteCode="+siteCode+"&database="+database,  
	     dataType : "jsonp",  
	     jsonp: "callbackparam",//服务端用于接收callback调用的function名的参数  
	     jsonpCallback:"success_jsonpCallback",//callback的function名称  
	     success : function(data){
	    	 var html = "";
	    	 if(data.response=="true"){
	    		 var hotword = data.hotword.split(",");
				  html = "热词：";
	    		 for(var i=0;i<hotword.length;i++){
	    			 html += "<a target='_blank' href='http://www.chengdu.gov.cn/search/s?q=1&database="+database+"&siteCode="+siteCode+"&qt="+encodeURIComponent(hotword[i])+"'>" + hotword[i] + "</a><span>|</span>"; 
	    		 }
	    	 }else{
	    		 html += data.msg;
	    	 }
	    	 $("#hotword").html(html);
			 $('#hotword span').last().remove();
	    	 
	     }
	 });
	 	
}

</script>
