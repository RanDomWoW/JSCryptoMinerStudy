    <script type="text/javascript">
        $(document).ready(function(){

            $("#formLogin").validate({});
            $("#formPassword").validate({});
                                                            $(".fancyboxLogin").fancybox({type: 'inline'});
            $('.switchForm').on('click',function(e) {
                e.preventDefault();
