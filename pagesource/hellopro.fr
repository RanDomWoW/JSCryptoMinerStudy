				$('.plus-de-demande').css('margin','33px 0 0 33px');
				$('.plus-de-demande').css('float','none');
			}
			else
			if(window.matchMedia("(min-width:769px) and (max-width:1024px)").matches) {
				$("#div_scroll").css('max-height','904px');
				$("#div_scroll").css('overflow','visible');
				$('.plus-de-demande').css('position','absolute');
				$('.plus-de-demande').css('margin','10px 0 0 33px');
				$('.plus-de-demande').css('float','none');	
