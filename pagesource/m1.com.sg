    $(document).ready(function () {
        getSearchSuggestions();
        //this make sure that it triggers only when header search textbox have some value.
        $('#' + 'header_0_imgSearch').bind("click", function () {
            var testTextBox = $('#' + 'header_0_txtSearch');
            if ($.trim($(testTextBox).val()) == "") {
                return false;
            }
        });
    });

    function getSearchSuggestions() {
        var cache = {};
        var testTextBox = $('#' + 'header_0_txtSearch');
        var hdnTextBox = $('#' + 'header_0_hdnSearchText1');
        var txtSearch;
        testTextBox.keypress(function (e) {
            code = (e.keyCode ? e.keyCode : e.which);
            if (code == 13) {
                txtSearch = $.trim($(testTextBox).val());
                if (txtSearch != '') {
                    $(hdnTextBox).val(txtSearch);
                    $('#' + 'header_0_imgSearch').click();
                }
            }
        });
        var myterms;
        if ($(testTextBox).autocomplete != null)
            $(testTextBox).autocomplete({
                minLength: 3,
                source: function (request, response) {
                    var term = request.term;
                    if (term in cache) {
                        response(cache[term]);
                        return;
                    }
                    $.getJSON('/M1Services/Ajax.aspx', request, function (data, status, xhr) {
                        cache[term] = data;
                        response(data);
                    });
                },
                appendTo: "#ulsuggestion",
                delay: 400,
                autoFocus: true
            });
    }

    function SearchClick1() {
        var testTextBox = $('#' + 'header_0_txtSearch');
        var hdnTextBox = $('#' + 'header_0_hdnSearchText1');
        var txtSearch = $.trim($(testTextBox).val());
        if (txtSearch != '') {
            $(hdnTextBox).val($(testTextBox).val());
            $('#' + 'header_0_imgSearch').click();
        }
    }

</script>

