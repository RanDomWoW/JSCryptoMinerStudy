<script>
    function GetQueryStringParams(sParam) {
        var returnVal = '';
        var sPageURL = window.location.search.substring(1);
        var sURLVariables = sPageURL.split('&');
        for (var i = 0; i < sURLVariables.length; i++) {
            var sParameterName = sURLVariables[i].split('=');
            if (sParameterName[0] == sParam) {
                return sParameterName[1];
            }
        }
        return returnVal;
    }

    var idleTimer = 0;
    var idleState = false;
	
    function validate_session() {
        idleState = true;
        var myDate = new Date();
        var t = myDate.getHours() + "" + myDate.getMinutes() + "" + myDate.getSeconds() + "" + myDate.getMilliseconds();
        $.ajax({
            url: "/m1services/validatesession.asmx/check?t=" + idleTimer + t,
            type: "POST",
            contentType: "application/json; charset=utf-8",
            data: "{}",
            dataType: "json",
            success: function(checkResult) {
                
                if (!checkResult) {
                    // handle error
                }
                var m = checkResult.d.Message;
                if (!m) {
                    // handle error
                } else {
                    //console.log(m);
                    if (m.toLowerCase() != 'log in / register') {
                        $("span[id $= 'lblWelcome']").text("Welcome");
                        $("a[id $= 'hrefUserName']").text(m);
                        $("a[id $= 'hrefUserName']").attr('href', '/accountsummary');
                        $("span[id $= 'lblWelcome']").show();
                        $("a[id $= 'Login']").text('/ Log Out');
                    } else if (($("a[id $= 'hrefUserName']").text() != '')
					    && ($("a[id $= 'hrefUserName']").text() != 'log in / register') 
					    && (m.toLowerCase() == 'log in / register')) {
                        $("span[id $= 'lblWelcome']").text("");
                        $("a[id $= 'hrefUserName']").text("");
                        $("a[id $= 'Login']").text(m);
                        showM1OverLay('SessionTimeout');
                    } else {
                        $("span[id $= 'lblWelcome']").text("");
                        $("a[id $= 'hrefUserName']").text("");
                        $("a[id $= 'Login']").text(m);
                    }
            }
                
            },
            error: function(xhr, status, error) {
                //on error clear timeout and run again
                clearTimeout(idleTimer);
                var interval = 1000 * 5; 
                idleTimer = setTimeout(validate_session, interval);
            },
            complete: function(xhr, status) { // I run after success and error callbacks have fired.
                clearTimeout(idleTimer);
                var interval = 1000 * 5; 
                idleTimer = setTimeout(validate_session, interval);
            }
        });
    }

    $(function () {        
        
        
        
        
        var logoutStatus = GetQueryStringParams("logout");
        if (logoutStatus != "" && logoutStatus == "true") {
            showM1OverLay('LogoutSuccess');
        }

        $("#LogoutSuccess").bind('reveal:close', function () {
            // 3292 Fix (25 Sept): fix for window.location.origin (not available for IE)
            window.location.href = "/"; //window.location.href.split('?')[0];
            //window.location.reload(true);
        });


        var sessionStatus = GetQueryStringParams("SessionTimeout");
        if (sessionStatus != "" && sessionStatus == "true") {
            showM1OverLay('SessionTimeout');
        }

        $("#SessionTimeout").bind('reveal:close', function () {
            //var homeURL = window.location.origin;
            //window.location.href = homeURL;
            window.location.href = "/"; //window.location.href.split('?')[0];
            //window.location.reload(true);
        });
    });
</script>


