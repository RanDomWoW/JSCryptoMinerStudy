    var motion_speed = 300;
    // total = 10
    function getTotal() {
        return $("#meul li").length
    }
    function slide(dir) {
        if (dir == "next") {
            if (getTotal() - max_view > curr_min_show * view_set) {
                tmp_index = (curr_min_show * view_set + view_set)
                $("#meul li:lt(" + tmp_index + ")").hide(motion_speed);
                curr_min_show += 1;
