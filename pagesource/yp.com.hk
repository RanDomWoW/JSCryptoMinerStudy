                curr_min_show -= 1;
                tmp_index = (curr_min_show * view_set + view_set)
                for (i = curr_min_show * view_set; i <= tmp_index; i++) {
                    $("#meul li:eq(" + i + ")").show(motion_speed);
                }
            }
        }
    }
