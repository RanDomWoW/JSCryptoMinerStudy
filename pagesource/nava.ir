        function playlisttogle() {
        var height = $( ".mejs__container.mejs__audio" ).outerHeight();
        if (height==40){
            if ($( ".mejs__container.mejs__audio" ).hasClass("testtetete")){
                $(".mejs__container.mejs__audio").removeClass("testtetete");
            }else{
                $(".mejs__container.mejs__audio").addClass("testtetete");
