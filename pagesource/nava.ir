        }else{
            if (height < 50){
                $(".mejs__container.mejs__audio").addClass("testtetete");
            }else{
                $(".mejs__container.mejs__audio").removeClass("testtetete");
            }
        }
    }
    function customplaylistplay(src,elementid,sizeof) {
        var playerId = $('#player2').closest('.mejs__container').attr('id');
        var player = mejs.players[playerId];
        player.setSrc(src);
        player.load();
        var i=1;
        for (i=1; i<=sizeof; i++){
            var id='playlpuse';
            id += i;
            if (elementid === id){
                var classname = document.getElementById(elementid).className;
                if (classname == 'fa fa-2x fa-pause'){
                    document.getElementById(id).setAttribute("class","fa fa-2x fa-play");
                    player.pause();
                }else{
                    document.getElementById(id).setAttribute("class","fa fa-2x fa-pause");
                    player.play();
                }
            }else{
                document.getElementById(id).removeAttribute("class");
                document.getElementById(id).setAttribute("class","fa fa-2x fa-play");
            }
        }
    }

