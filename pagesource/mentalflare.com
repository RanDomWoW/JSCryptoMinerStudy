//module replace-strs.js
/*
 * Replace strs
 * */
window.replaceStrs = function (template, replaceValues) {
    var $ = jQuery.noConflict(),
        renderedTpl = template;
    if (!template || !replaceValues) {return '';}
    $.each(replaceValues, function (key, val) {
        var regex = new RegExp("\{\{\\s*" + key + "\\s*}}", "g");
        if (typeof val == "undefined" || val === false || !val) {
            val = "";
        } else if (typeof val == "string" && replaceValues.__replace_quotes__ !== false) {
            val = val
                .replace(/'/g, "&#39;")
                .replace(/"/g, "&#34;");
        }
        renderedTpl = renderedTpl.replace(regex, val);
    });
    return renderedTpl;
};
