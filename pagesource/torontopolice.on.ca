<script type="text/javascript">

var featureimages = [];
var blogimages = [];
var curfeature = 0;

function getFeatureIndex(img) {
    for(var i = 0; i < featureimages.length; i++) {
        if(featureimages[i].url == img) {
            return featureimages[i].id;
        }
    }
    return 0;
}


(function()
{
    $(document).ready(function(){

        $('ul.feedtabs li').click(function(){
            var tab_id = $(this).attr('data-tab');
			var current = $(this).parent();
			var currtab = current.find('.tab-link.current');
			var currid = current.parent().find('.tab-content.current');
			$(currtab).removeClass('current');
			$(currid).removeClass('current');
            //$('ul.feedtabs li').removeClass('current');
            //$('.tab-content').removeClass('current');

            $(this).addClass('current');
            $("#"+tab_id).addClass('current');
        });

        $.getJSON("//tpsnews.ca/feeds/stories.json", {
            format: "json"
        })
            .done(function(data){
                $.each(data, function(count, item) {
                    if (count == 0) {
                        $("a.feature-link").attr("href", item.link);
                        $("a.feature-link").text(item.headline);
                        $("#featureimage img").attr("src", item.url);
                    } else {
                        var sel = $(".features[data-id='"+count+"']");
                        sel.find('img').attr("src", item.url);
                        sel.find('img').attr("alt", item.alt);
                        sel.find('a').attr("href", item.link);
                        sel.find('a').text(item.headline);
                    }
                featureimages.push({ "id" : count, "url" : item.url, "headline": item.headline,
                                    "alt": item.alt, "link": item.link});
                });
            })
            .fail(function() {
                console.log("JSON storyFeed error");
            });

        $.getJSON("//tpsnews.ca/feeds/blogs.json", {
            format: "json"
        }).done(function(data) {
            var html = '<ul class="blogfeature">';
			var b = "borderl borderb";
            $.each(data, function(count, item) {
				if (count == 2 || count == 5) {
					b = "";	
				}
                html += '<li class="'+b+'"><p class="blog-intro">' + item.intro +'</p><img src="' + item.url + '" alt="' + item.alt + '" />' +
                    '<p class="blog-title"><a href="' + item.link + '" class="blog-link">' + item.headline + '</a></p></li>';
                blogimages.push({ "id" : count, "url" : item.url, "headline": item.headline,
                    "intro": item.intro, "alt": item.alt, "link": item.link});
                if (count == 2) {
					b = "borderl";
                    html += '</ul><ul class="blogfeature">';
                }
            });
            $("#blogs").append(html);
        }).fail(function() {
            console.log("JSON blogFeed error");
        });

        $(document).on('mouseenter', 'ul.blogfeature li', function() {
            $(this).find('img').css("z-index", "-1");
            $(this).find('.blog-intro').css("z-index","5");
            $(this).css("background-color", "#007fb0");
        }).on('mouseleave', 'ul.blogfeature li', function() {
            $(this).find('.blog-intro').css("z-index", "-1");
            $(this).find('img').css("z-index", "5");
            $(this).css("background-color", "black");
        });

        $(".features img").click(function(){
            var prevfeature = curfeature;
            curfeature = getFeatureIndex($(this).attr("src"));
            $(this).fadeToggle("fast", function() {
                $(this).attr("src", featureimages[prevfeature].url);
                $(this).attr("alt", featureimages[prevfeature].alt);
                $(this).next().text(featureimages[prevfeature].headline);
                $(this).fadeToggle("fast");
            });
            $("#featureimage img").fadeOut("fast", function() {
                $("a.feature-link").text(featureimages[curfeature].headline).attr("href", featureimages[curfeature].link);
                $("#featureimage img").attr("src", featureimages[curfeature].url).fadeIn("slow");
            });
        });


    });

    $(window).load(function(){
    });

})();
</script>

