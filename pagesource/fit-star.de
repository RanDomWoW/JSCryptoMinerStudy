        $('.megadropdown').fadeOut();
      });
      // Close menu on click somewhere
      $(document).mouseup(function(e) 
      {
          var container = $(".megadropdown");
          if ($('#header_left').hasClass("hover")) { 
            container.fadeIn();
          }
          else {
            if (!container.is(e.target) && container.has(e.target).length === 0) 
            {
              container.fadeOut();
            }
          }
      });
