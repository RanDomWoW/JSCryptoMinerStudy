<script type="text/javascript">
// global vars
var targetPanels = document.querySelectorAll(".df-panel__content");
var observers = [];
var configObservers = { childList: true, characterData: true, attributes: true, subtree: true };


// start code (execute on document ready)
jQuery(document).ready(function(){
    initObservers();
});


// functions....
function initObservers(){
    for (var i = 0; i < targetPanels.length; i++) {
        observers[i] = new MutationObserver(mutationHandler);
        observers[i].observe(targetPanels[i], configObservers);
    }
}

function connectObservers()
{
    for (var i = 0; i < targetPanels.length; i++) {
        observers[i].observe(targetPanels[i], configObservers);
    }
}

function disconnectObservers()
{
    for (var i = 0; i < targetPanels.length; i++) {
        observers[i].disconnect();
    }
}

function mutationHandler (mutationRecords) {
    mutationRecords.forEach ( function (mutation) {
        if (mutation.type == "childList"){
            dfSortsItems(mutation);
        }
    } );
}

function dfSortsItems(mutation)
{
    var filterActive = false;
    var container = jQuery(mutation.target);
    if (jQuery(container).is(":button")){
        container = jQuery(container).parent();
    }
    if (container.hasClass("df-panel__content")){
        var button = container.find("button").first();
        if (button.is(":visible") && container.find(".df-term").length > 0){
            if (container.find(".df-term:visible").length == 1 && container.find(".df-term:visible").first().attr("data-selected") != undefined){

            } else {
                var currentText = button.text().trim();
                if (currentText !=  button.attr('data-text-normal')){
                    // sorted list view
                    dfShowItemsOrdered(container);
                } else {
                    // default view
                    dfShowItemsDefault(container);
                }
            }
        }
    }
}

function dfShowItemsOrdered(container)
{
    if (container.find('.secondView').length > 0) {
        return false;
    }
    disconnectObservers();
    var items = container.find('a.df-term');
    var itemsArray = Array();
    items.addClass("firstView").addClass("hidden");
    items.each(function(i, o){
        var count = parseInt(jQuery(o).find("span.df-term__count").first().text().trim());
        var newItem = { "data_value":jQuery(o).attr("data-value"), "count":count };
        itemsArray.push(newItem);
    });

    itemsArray.sort(function(a, b){
        if(a.data_value > b.data_value) return 1;
        if(a.data_value < b.data_value) return -1;
        return 0;
    })
    var newItemsHtml = '';
    for(var i=0; i < itemsArray.length; i++){
        newItemsHtml += '<a class="df-term secondView" href="#" data-facet="brand" data-value="'+itemsArray[i].data_value+
            '" >'+itemsArray[i].data_value+' <span class="df-term__count">'+itemsArray[i].count+'</span></a>';

    }
    container.prepend(newItemsHtml);
    connectObservers();
}

function dfShowItemsDefault(container)
{
    disconnectObservers();
    container.find(".secondView").remove();
    container.find(".firstView").removeClass("hidden");
    connectObservers();
}
</script>
