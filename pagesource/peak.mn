    function saveCustomGridOrders() {
        var order = [];
        $('#user-custom-grid').children().each(function() {
            order.push($(this).attr("id"));
        });
        localStorage.setItem('usercustomgrid', order);
    }
    
    function showCustomGridOrders() {
        var customGridOrder = localStorage.getItem('usercustomgrid');
        customGridOrder = customGridOrder !== null ? customGridOrder.split(',') : []; 

        var userGrid = $("#user-custom-grid");
        var dataGrid = $("#user-custom-grid-data");

        if(customGridOrder.length) {
            var gridItems = {};
            dataGrid.children().each(function() {
                var itemId = $(this).attr("id");
                gridItems[itemId] = $(this);
            });

            for (ind in customGridOrder) {
                var itemId = customGridOrder[ind];
                userGrid.append(gridItems[itemId]);
            }
        } else {
            userGrid.html(dataGrid.html());
        }
        dataGrid.remove();
    }
    
    showCustomGridOrders();
    
