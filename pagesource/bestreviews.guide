window.timePageInfo = {
  TIME_LAPSE_FOR_SEND: 5000,

  data: {
    time_on_site:   { type: 'time_active', value: 0 },
    time_no_active: { type: 'time_inactive', value: 0 },
    scroll:         { type: 'scroll', value: 0 },
