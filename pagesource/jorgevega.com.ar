<script>
  Js(['vendor/bootstrap', function () {
      var cache = {}, lastModal;
      
      $('#adap-izq-shadow').css( 'min-width' , $('.adaptable-izquierda').css('width') );
      
      $('.hasTooltip').tooltip({html: true});
      
      $(document).on( 'click.vj.note' , '[data-toggle="note"]' , function( event ){
        var $this = $(this);
        
        if( $this.is('a') )
          event.preventDefault();

        if( !$this.data('bs.note') ){
          $this.note('show');
        }

      }); 
      
      $(document).on( 'click' , '[data-play]' ,  function(e){
        var $this = $(this); 
        
        if( $this.is('a') )
          e.preventDefault();
        
        var url = $this.data( 'play' ) || $this.attr( 'href' ), 
            iframe = '<iframe width="100%" height="315" src="http://www.youtube.com/embed/' + url + '" frameborder="0" allowfullscreen ></iframe>',
            target = $this.data( 'play-target' ) || $this.data( 'target' );

        $( target ).html( iframe );
        
        $this.removeAttr('data-play');
        
        return false;
      });
      
      $(document).on('show.bs.modal', function(e){
        if( lastModal )
          $(lastModal).modal('hide');
        
        lastModal = e.target;
      } );

      $(document).on('hidden.bs.modal', function(e){
        if( lastModal === e.target )
          lastModal = null;
      } );

      $(document).on( 'click' , '[data-toggle="modal-load"]' , function (e) {
        e.preventDefault();

        var link = $(this),
            url = link.attr('href');

        if (!url)
          return;

        if (url.indexOf('#') == 0) {
          $(url).modal('show');
        } else {

          if( lastModal ){
            $( lastModal ).one( 'hidden.bs.modal' , loadModal.bind(null,url) ).modal( 'hide' );
          } else {
            loadModal(url);
          }
        }
      });
      
//      $(document).on( 'click' , '[data-toggle="modal-login"]' , function (e) {
//        e.preventDefault();
//
//        var $this = $(this); 
//
//        
//      }
      
//      $(document).on( 'click' , '[data-message]' , function(){
//        var $this = $(this),
//            message = $this.data('message');
//        
//        if( $this.data() )
//        
//      } );
      
      function loadModal( url ){
        var elem = cache[ url ];

        if( elem ){
          elem.modal( 'show' );
          $('input:text:visible:first' , elem ).focus();
        } else {
          var isModal = url.indexOf('my-layout=modal') !== -1 ,
              nUrl = (isModal ? url.replace( 'my-' , '' ) : url ) + ( url.indexOf('?') !== -1 ? '&' : '?' ) + 'tmpl=modal';        

          $.get(nUrl, function (data) {
            elem = $(isModal 
              ? data 
              : '<div class="modal fade in"><div class="modal-dialog"><div class="modal-content modal-body">' + data + '</div></div></div>');

            cache[ url ] = elem;

            $('body').append(elem);

            elem.modal('show');

          }).success(function () {
            $('input:text:visible:first' , elem ).focus();
          });
        }
      }
      
      
      $(document).on( 'keyup.vj.flex' , 'textarea[data-toggle="text-flex"]' , 
        (function( event ){
          var reg = /[\n\r]+/g , 
              lines = 0;
          
          if( !this._orows )
            this._orows = this.rows;
          
          while( reg.exec( this.value ) )
            lines++;
          
          this.rows = Math.max( lines + 1 , this._orows );
          
        }).postpone(200) ); 
      
    }]);
</script>

