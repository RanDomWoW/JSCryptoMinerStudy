                        <script >
                            function search_msisdn(){
                                $('.search-block #notification').html('Zəhmət olmasa, gözləyin...&nbsp;<img src="http://narsim.az/images/ico_loading.gif"/>');
                                $('.result-block').append('<div class="disable_layer"></div>');
                                var request = $.ajax({
                                  url: 'http://narsim.az/mainpage',
                                  type: "POST",
                                  data: $('#search_frm').serialize()
                                });
                                request.done(function(msg) {
                                    //console.log(msg);
                                    var r=msg.split(';');
                                    $('#token').val(r[0]);
                                    var page=r[1];
                                    msg=r[2];
                                    if (msg=='antif5'){
                                        $('.search-block #notification').html('Zəhmət olmasa, axtarış etmək üçün xanaları doldurub Axtar düyməsini sıxın');
                                    } else if (msg=='not found'){
                                        $('.search-block #notification').html('Axtarış nəticə vermədi, zəhmət olmasa, digər kombinasiyanı yoxlayın');
                                        if (page>1){
                                            $('.search-block #notification').html('');
                                            $('.result-block .disable_layer').remove();
                                            $('a#more_btn').hide();
                                        }
                                    } else if (msg=='wrong prefix'){
                                        $('.search-block #notification').html('Zəhmət olmasa, prefiksi düzgün daxil edin');
                                    } else if (msg=='wrong price category'){
                                        $('.search-block #notification').html('Qiymət kateqoriyası səhv daxil edilib');
                                    } else if (msg=='minimum not required'){
                                        $('.search-block #notification').html('Axtarış uğursuz nəticələndi, ən azı 3 rəqəm daxil edilməlidir');
                                    } else if (msg.length >= 100) {
                                        
                                        $('.search-block #notification').html('Nəticələr'+r[3]);
                                        //$('.search-block #notification').html('Nəticələr ');
                                        $('.result-block .disable_layer').remove();
                                        if (page == 1) $('.result-block').html(msg);
                                        else $('.result-block table').append(msg);
                                        if (r[4]=='hide_more') {
                                            $('#more_btn').hide();
                                        }
                                        $('.banner-block').hide();
                                        $('.oneitem').hide();
                                        $('.result-block').show();
                                        $('.result-block input').styler();  
                                    } else {
                                        $('.loading').hide();
                                        $('.search-block #notification').html('Sorğunun göndərilməsində nasazlıq baş vermişdir, zəhmət olmasa, bir daha yoxlayın');
                                    }
                                });
                                request.fail(function(jqXHR, textStatus) {
                                        $('.loading').hide();
                                        $('.search-block #notification').html('Sorğunun göndərilməsində nasazlıq baş vermişdir, zəhmət olmasa, bir daha yoxlayın');
                                });
                            }
                            
                            $(document).ready(function() {
                                $('#search_btn').click(function(){
                                    $('#page').val('1');
                                    $('#search_frm #order_type').val(0);
                                    search_msisdn();
                                    return false;
                                });
                                $('#reset_btn').click(function(){
                                    for(var i=1; i<=7; i++) $('#number'+i).val('');
                                    return false;
                                });
                                $( document ).on( "click", "a#more_btn", function() {
                                    $('#page').val($('#page').val()*1+1);
                                    search_msisdn();
                                    return false;
                                });
                                                                    
                                    
                                                                $( document ).on( "click", "a.order_popup_btn", function() {
                                        $('#order_frm #msisdn').val($(this).attr('data-msisdn'));
                                        orderPopupShow();
                                        return false;
                                });
                                
                                $( document ).on( "click", "a.sort-ico", function() {
                                    $('.sort-bar').toggle();
                                    return false;
                                });
                                
                                $( document ).on( "change", '.sort-bar input[type="checkbox"]', function() {
                                    var clicked_item=this;
                                    if($(this).is(':checked')){
                                        $('#search_frm #order_type').val($(this).val());
                                        $( '.sort-bar input[type="checkbox"]' ).each(function() {
                                            if (clicked_item != this) {
                                                $(this).prop('checked',false).trigger('refresh');
                                            }
                                        });
                                    } else {
                                         $( '.sort-bar input[type="checkbox"]' ).each(function() {
                                            if (clicked_item != this) {
                                                $(this).prop('checked',true).trigger('refresh');
                                                $('#search_frm #order_type').val($(this).val());
                                            }
                                        });
                                    }
                                    search_msisdn();
                                });
                                
                                
                                if ($('#mf_dealer_id').val() != ''){
                                    search_msisdn();
                                }
                            });
                            </script>
			
