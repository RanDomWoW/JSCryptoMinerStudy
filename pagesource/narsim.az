        <script type="text/javascript">
            $('#order_holder #name_surname').alpha({allow:" "});
            $('#order_holder #contact').numeric();
            
            function orderPopupShow(){
                $('#__code__').trigger('click');
                $('#order_holder #notification').html('DİQQƏT! Aşağıda qeyd olunan sifariş formasını doldurarkən məlumatların doğruluğuna əmin olun.');
                $('#order_holder input[type="text"]').val('');
                $('#order_holder').show();
            }
            function orderPopupHide(){
                $('#order_holder').hide();
            }
            $('#order_btn_cancel').click(function(){
                orderPopupHide();
            })
            $(document).mouseup(function (e)
            {
               
                var container = $(".order_frm,.order-block, .sort_bar, .sort-ico");

                if (!container.is(e.target) // if the target of the click isn't the container...
                    && container.has(e.target).length === 0) // ... nor a descendant of the container
                {
                    $('#order_holder').hide();
                    $('.sort_bar').hide();
                }
            });
            var dot_interval;
            $('#order_btn_ok').bind('click',function(){            
              
                $('#order_holder #notification').html('Sifariş göndərilir, zəhmət olmasa, gözləyin<span id="dots"></span>');
                
                var dots = 0;
                clearTimeout(dot_interval);
                dot_interval=setInterval (function(){
                    if(dots < 3) {
                        $('#dots').append('.');
                        dots++;
                    } else {
                        $('#dots').html('');
                        dots = 0;
                    }
                }, 500);
                
                var checkorderterms = $("#orderterms_chk").prop("checked");
                if(!checkorderterms) {
                     $('#order_holder #notification ').html("Zəhmət olmasa, Şərtlərlə tanış olduğunuzu təsdiqləyin");
                     return;
                }
                
                
                var post_data={action:'order'};
                
                var a=$.trim($('#order_holder #name_surname').val());
                if (a!='') post_data['name_surname']=a;
                else {
                    $('#order_holder #notification ').html("Zəhmət olmasa, Ad və Soyadınızı daxil edin.");
                    $('#order_holder #name_surname').focus();
                    return; 
                }
                
                var a=$.trim($('#order_holder #contact').val());
                if (a!='') post_data['contact']=a;
                else {
                    $('#order_holder #notification ').html("Zəhmət olmasa, Əlaqə Nömrənizi daxil edin.");
                    $('#order_holder #contact').focus();
                    return;
                }
                
                var a=$.trim($('#order_holder #msisdn').val());
                if (a!='') post_data['msisdn']=a;
                else {
                    $('#order_holder #notification ').html("Zəhmət olmasa, nömrə seçin.");
                    return;
                }
                
                post_data['code']=$.trim($('#code').val());
                
                $.ajax({
                    url: 'http://narsim.az/index.php',
                    type: "post",
                    data: post_data,
                    success: function( data ) {
                        console.log(data);
                        if (data=='success') {
                            $('#order_holder #notification ').html("Sifariş uğurla göndərildi.");
                            $('#order_holder #msisdn').val('');
                            $('#order_holder input[type="text"]').val('');
                            setTimeout(function(){$('#order_holder').fadeOut();},2000);
                        } else if (data=='wrong_code'){
                            $('#order_holder #notification ').html("Təhlükəsizlik kodu səhv daxil edilib");
                        }
                        else $('#order_holder #notification ').html("Sifarişin göndərilməsində nasazlıq baş verdi.");
                    }
                });
            });
        </script>
        
        
        






        
        
        
        

