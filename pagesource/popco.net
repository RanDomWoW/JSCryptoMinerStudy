                fbname = user.name; 
				fbemail = user.email;  
			
                //response.authResponse.userID  
                $.post("./zboard/fbloginprocess.php", { "userid": user.id, "email": fbemail,"username": fbname,"userlink": user.link, "fbaccesstoken":accessToken},  
