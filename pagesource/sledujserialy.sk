<script type="text/javascript">
jQuery(function() {
    jQuery(window).load(function() { jQuery('.content-upcoming').hide(); });
    jQuery('.upcoming-button').click(function(e) {
        jQuery('.content-upcoming').slideToggle();
        jQuery('#calendar').fullCalendar( 'refetchEvents' );
        e.preventDefault();
    });

    jQuery(window).on('load resize', function() {
        if (jQuery(window).width() < 640) {
            jQuery('#calendar').fullCalendar( 'changeView', 'basicDay' );
        } else {
            jQuery('#calendar').fullCalendar( 'changeView', 'basicWeek' );
        }
    });

    jQuery('#calendar').fullCalendar({
        weekends: true,
        firstDay: 1,
        height: 400,
        eventRender: function(event, element, view) {
            return jQuery('<div class="fc-day-grid-event fc-h-event fc-event fc-start fc-end"><div class="fc-content"><a href="/serial/'+ event.show_slug +'" title="'+ event.show +'"><span class="fc-title" style="font-size: 14px; display: block; font-weight: 700; color: #fff;">'+ event.show +'</span></a><span class="fc-title" style="opacity: 0.7; display: block;">Epizóda '+ event.season +'x'+ event.episode +'</span><span class="fc-title">'+ event.title +'</span></div></div>');
        },
        events: '/theme/json/home.calendar.php?032018',
        defaultView: 'basicWeek',
        columnFormat: 'ddd DD.MM.',
        titleFormat: {
            week: 'D MMMM YYYY'
        },
        header: {
            left: 'title',
            center: '',
            right: 'prev,today,next'
        },
	    	        dayNames: ["Nedeľa", "Pondelok", "Utorok", "Streda", "Štvrtok", "Piatok", "Sobota"],
	        dayNamesShort: ["Ne", "Po", "Ut", "Str", "Štv", "Pia", "So"],
	        monthNames: ["Január", "Február", "Marec", "Apríl", "Máj", "Jún", "Júl", "August", "September", "Október", "November", "December"],
	        buttonText: {today: "Dnes", month: "Mesiac", week: "Týždeň", day: "Deň" },
	    
        viewRender: function(currentView){
        	var minDate = moment(),
        	maxDate = moment().add(8,'weeks');
        	// Past
        	if (minDate >= currentView.start && minDate <= currentView.end) {
        		$(".fc-prev-button").prop('disabled', true);
        		$(".fc-prev-button").addClass('fc-state-disabled');
        	}
        	else {
        		$(".fc-prev-button").removeClass('fc-state-disabled');
        		$(".fc-prev-button").prop('disabled', false);
        	}
        	// Future
        	if (maxDate >= currentView.start && maxDate <= currentView.end) {
        		$(".fc-next-button").prop('disabled', true);
        		$(".fc-next-button").addClass('fc-state-disabled');
        	} else {
        		$(".fc-next-button").removeClass('fc-state-disabled');
        		$(".fc-next-button").prop('disabled', false);
        	}
        }
    });
});
</script>




