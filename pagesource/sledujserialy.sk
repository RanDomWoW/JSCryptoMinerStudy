            $(document).ready(function() {
                var ajaxInput = $('#ajaxsearch input');
                var ajaxResult = $('#ajaxsearch .ajaxsearch_results');
                ajaxInput.keyup(function() {
                    if ( ajaxInput.val().length >= 3) {
                        $.ajax({
                            url: '/theme/json/searchform1.php',
                            type: 'POST',
                            data: 'term='+ajaxInput.val()+'&selected_language_id=1&v=628',
                            dataType: 'json',
                            success: function (result) {
                                var outputHtml = '<div>';
                                $.each(result, function (key, val) {
                                	console.log(val);
                                    if ( val.tvshow_id != "0" ) {
                                        outputHtml += '<p><a href="/serial/' + val.slug + '"><img height="80px" src="/upload/tvshow/' + val.tvshow_id + '-album.jpg?v='+val.updated_at+'" /><span>' + val.title + '<em>Najnovšia séria/epizóda: ' + val.season + 'x' + val.episode + '</em></span></a></p>';
                                    } else {
                                        outputHtml += '<p><a><span>' + val.title + '</span></a></p>';
                                    }
                                });
                                outputHtml += '</div>';
                                ajaxResult.html(outputHtml);
                            }
                        });
                    } else {
                        ajaxResult.html('');
                    }
                });

            });
        </script>
