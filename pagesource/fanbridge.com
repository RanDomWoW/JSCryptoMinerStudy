<script type="text/javascript">
        jQuery(document).ready(function() {
                jQuery('.vertical_link_container').click(function() {
                        if(jQuery(this).attr('data-vertical') == 'fan') {
                                jQuery('.user-type-options').hide();
                                jQuery('.zendesk-widget').addClass('zendesk-widget-opened').show();
                        }
                        else {
                                window.location.href = 'https://www.fanbridge.com/register/?plan_id=136&free_trial=1&ref=homepage_cta&vertical=' + jQuery(this).attr('data-vertical');
                        }
                });
        });
        jQuery(document).on('click', '.modal-user-type-toggler', function() {
                jQuery('.user-type-options').hide();
                jQuery('.zendesk-widget').addClass('zendesk-widget-opened').show();
        });
        jQuery('#zFormer').validate({
                ignore: ".ignore",
                rules: {
                        z_name: {
                            required: true
                        },
                        z_requester: {
                          required: true,
                          email: true
                        },
                        z_description: {
                            required: true
                        },
                        "data[_recaptcha]": {
                                required: function () {
                                        if (grecaptcha.getResponse() == '') {
                                                jQuery('.g-recaptcha').addClass('error-captcha');
                                                return true;
                                         } else {
                                                jQuery('.g-recaptcha').removeClass('error-captcha');
                                                return false;
                                        }
                                }
                        }

                },
                messages: {
                        z_name: "Please, enter your name",
                        z_requester: "Please, enter your email",
                        z_description: "Please, enter your message",
                        "data[_recaptcha]": "Please complete the captcha"
                },
                submitHandler: function(form) {
                        var url = '/ajax/zendesk_sender.php';
                        $.ajax({
                                type: 'POST',
                                url: url,
                                data: jQuery('#zFormer').serialize(),
                                success: function(data) {
                                        if(data.status == 'captcha-error') {
                                                jQuery('.g-recaptcha').addClass('error-captcha');
                                                return;
                                        }
                                        jQuery('.g-recaptcha').removeClass('error-captcha');
                                        jQuery('.z-form-success').show();
                                        jQuery('#zFormer').slideUp();
                                },
                                error: function(data){
                                        jQuery('.z-form-error').show();
                                        jQuery('#zFormer').slideUp();
                                }
                        });
                }
        });
        jQuery(document).on('click', '.close', function() {
                jQuery('.user-type-options').show();
                jQuery('.zendesk-widget').removeClass('zendesk-widget-opened').hide();
        });
