<script type="text/javascript" language=javascript>
     
   
var nonce = ""; //php variable stored in a JS variable nonce
function hash() //function to calculate the salted hash
{    
    if(validate1())
    {
        nonce=document.mem_login.saltvalue.value;
        //alert("nonce="+nonce);
        var temp=calcMD5(document.mem_login.pass.value);
        var temptwo=temp+nonce;
        document.mem_login.pass.value=calcMD5(temptwo);
    }
}
function validate1()
{
    if(document.mem_login.user_name.value=="")
    {
        alert("Kindly enter Username");
        document.mem_login.user_name.focus();
        return false;
    }
    if(document.mem_login.pass.value=="")
    {
        alert("Kindly enter Password");
        document.mem_login.pass.focus();
        return false;
    }
    
        return true;
    
}
</script>
