<script src="js/public.js?1.0"></script>

    <script>
        //头部基础脚本
        A6.headFn();
        //底部计算器
        A6.counter();
        //滚动图
        A6.slide({
            id: '#slider',
            speet: 500,
            auto: 4000,
            btn: true
        });
        seajs.use('./js/plugins/jquery.slides.min', function () {
            $('.slides').slidesjs({
                width: 468,
                height: 482
            });
        });
        //选项卡
        $('.guide-news .tabTit .tit').click(function () {
            var index = $(this).index();
            $(this).addClass('active').siblings().removeClass('active');
            $(this).parents('.guide-news').find('.tabLayer>div').eq(index).siblings().hide().end().show().find('img').lazyload();
            var obj = $(this).parents('.tabTit').children('.pull-right');
            if (index == 1) {
                obj.eq(0).hide();
                obj.eq(1).show();
            } else {
                obj.eq(0).show();
                obj.eq(1).hide();
            }
        });
        //服务css3动画
        $('.service').mouseover(function () {
            $(this).find('.animate').addClass('active');
            $('.service .p-8 .ico').one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function () {
                $('.service .animate').removeClass('active');
            })
        });
        $('.topad [data-controller="close"]').click(function () {
            $('.topad').fadeOut('slow');
        });
        $(document).ready(function () {
            jj_location_c_id = readck("jj_location_c_id");
            console.log('二次获取地址：' + jj_location_c_id);
            //切换底部友情链接
            $(".link-tab a").click(function () {
                var id = $(this).attr("id");
                $(this).attr("class", "link-tab-active");
                $(".link-tab a").each(function () {
                    if (id != ($(this).attr("id"))) {
                        $(this).removeAttr("class");
                        $("#link_list_"+$(this).attr("id")).hide();
                    } else {
                        $("#link_list_"+$(this).attr("id")).show();
                    }
                });
            });
        });
    </script>

</body>
