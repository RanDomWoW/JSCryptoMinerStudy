<script language="JavaScript" type="text/javascript">
// JavaScript Document
function objeto(){
	
        var xmlhttp=false;
        
		if(window.XMLHttpRequest)
		{
			xmlhttp=new XMLHttpRequest;
		}
		else if (window.ActivexObject) 
		{
			XMLHttpRequest= new ActivexObjet("Microsoft.XMLHTTP");
		}

        if (!xmlhttp && typeof XMLHttpRequest!='undefined') {
        	xmlhttp = new XMLHttpRequest();
        }
		
        return xmlhttp;
		
}

var tiempo;

function select_phone(){
	
	cityid = document.form_geo.ciudad.value;
	
	result=document.getElementById('divphone');
	ajax=objeto();
	ajax.open("POST","combophone.php",true);
	
	ajax.onreadystatechange=function() {
		if (ajax.readyState==4) {
			if(ajax.status==200)
			{
				clearTimeout(tiempo);
				result.innerHTML = ajax.responseText
			}
			
		}
					
	}
	
	ajax.setRequestHeader("Content-Type","application/x-www-form-urlencoded");
		
	ajax.send("cityid="+cityid);
	
	tiempo=setTimeout("finDeEspera()",3000);
		
}

function finDeEspera()
{
  ajax.abort();
}
</script>



