<div id="history_div"></div>

<script language="JavaScript" type="text/javascript">
// JavaScript Document
function objeto(){
        var xmlhttp=false;
        
		if(window.XMLHttpRequest)
		{
			xmlhttp=new XMLHttpRequest;
		}
		else if (window.ActivexObject)
		{
			XMLHttpRequest= new ActivexObjet("Microsoft.XMLHTTP");
		}

        if (!xmlhttp && typeof XMLHttpRequest!='undefined') {
                xmlhttp = new XMLHttpRequest();
        }
        return xmlhttp;
}

var tiempos;
	
	function history_stats(city,region,country,countryCode,organization,visitCount,pageCountForThisVisit,pageCountAcrossAllVisits,secondsSpentForThisVisit,secondsSpentAcrossAllVisits,currentPageTitle,currentPageURL,conversationCount,isConversing,referrer,referredByPaidAdvertisingThisVisit,referredByPaidAdvertisingEver,referredByCampaignThisVisit,referredByCampaignEver,searchTextForThisVisit,searchTextForPreviousVisits,action){
		
		resultado=document.getElementById('history_div');
		ajax=objeto();
		ajax.open("POST", "xerp/custom/history.php",true);
		
		ajax.onreadystatechange=function() {
			if (ajax.readyState==4) {
				if(ajax.status==200) {
					clearTimeout(tiempos);
					resultado.innerHTML = ajax.responseText
				}					
			}								
		}
		
		ajax.setRequestHeader("Content-Type","application/x-www-form-urlencoded");
		
		ajax.send("city="+city+"&region="+region+"&country="+country+"&countryCode="+countryCode+"&organization="+organization+"&visitCount="+visitCount+"&pageCountForThisVisit="+pageCountForThisVisit+"&pageCountAcrossAllVisits="+pageCountAcrossAllVisits+"&secondsSpentForThisVisit="+secondsSpentForThisVisit+"&secondsSpentAcrossAllVisits="+secondsSpentAcrossAllVisits+"&currentPageTitle="+currentPageTitle+"&currentPageURL="+escape(currentPageURL)+"&conversationCount="+conversationCount+"&isConversing="+isConversing+"&referrer="+referrer+"&referredByPaidAdvertisingThisVisit="+referredByPaidAdvertisingThisVisit+"&referredByPaidAdvertisingEver="+referredByPaidAdvertisingEver+"&referredByCampaignThisVisit="+referredByCampaignThisVisit+"&referredByCampaignEver="+referredByCampaignEver+"&searchTextForThisVisit="+searchTextForThisVisit+"&searchTextForPreviousVisits="+searchTextForPreviousVisits+"&action="+action);
		tiempos=setTimeout("finDeEspera()",3000);
	}
	
function finDeEspera()
{
  ajax.abort();
}
</script>

<script language="javascript">
	olark('api.visitor.getDetails', function(details){
		
		history_stats(details.city,details.region,details.country,details.countryCode,details.organization,details.visitCount,details.pageCountForThisVisit,details.pageCountAcrossAllVisits,details.secondsSpentForThisVisit,details.secondsSpentAcrossAllVisits,document.title,window.location.pathname+window.location.search,details.conversationCount,details.isConversing,details.referrer,details.referredByPaidAdvertisingThisVisit,details.referredByPaidAdvertisingEver,details.referredByCampaignThisVisit,details.referredByCampaignEver,details.searchTextForThisVisit,details.searchTextForPreviousVisits,"action");
	});
</script>

