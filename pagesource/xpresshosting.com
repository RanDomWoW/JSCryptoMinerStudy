<script type="text/javascript">

	$(document).ready(function () {

		var downArrowHtml = '<span class="downarrow"></span>';

		var selectedTextHtml = '<span class="selectedTlds">Varios</span>';

		$("#MenuDropdown #searchAllExtensions").click(function (e) {

			//e.stopImmediatePropagation();

			if ($(this).attr('checked')) {

				$("#MenuDropdown div.title").html('Todos ' + downArrowHtml);

			}

			else {

				SetTitleText();

			}

		});

		$("#MenuDropdown div.tlist :checkbox").click(function (e) {

			var selectAll = $("#MenuDropdown #searchAllExtensions");

			if (selectAll != null && selectAll.attr('checked')) return; //Don't set anything if select all is already checked



			SetTitleText();

		});

		function countTldsChecked() {

			var n = $("#MenuDropdown div.tlist input:checked").length;

			return n;

		}

		function SetTitleText() {

			if (countTldsChecked() > 1) {

				$("#MenuDropdown div.title").html(selectedTextHtml + downArrowHtml);

			}

			else {

				var selectedTld = $("#MenuDropdown div.tlist input:checked");

				if (selectedTld.length > 0) {

					$("#MenuDropdown div.title").html( selectedTld.val() + downArrowHtml);

				}

				else {

					$("#MenuDropdown div.title").html('.com' + downArrowHtml);

				}

			}

		}



	});

</script>



