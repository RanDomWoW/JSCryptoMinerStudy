      jQuery(document).ready(function($) {
        /**
         * for options, see http://www.starplugins.com/cloudzoom/quickstart
         */
        var cloudzoom_options = {
          zoomPosition: 'inside',
          tintOpacity: 0,
          zoomOffsetX: '0',
          zoomFlyOut: false,
          touchStartDelay: true,
          disableOnScreenWidth: 640 // no zoom on tiny viewports
        }
        /**
         * shows link to mediathek in upload widget, if user is logged in,
         * otherwise removes the link
         * @return {void}
         */
        var checkMediathekLink = function() {
          // check if user is logged in
          if ($.fn.Mediathek().isLoggedIn()) {
            // add open mediathek link
            $mediatheklink = $('<a>', {
              id: 'logopreview-open-mediathek',
              text: 'Meine Dateien öffnen',
              title: 'Mediathek',
              //href: '', // no href with pound sign here to prevent manual reloading error
              click: function(e) {
                e.stopPropagation();
                showMediathekLogopreviewIntegration();
              }
            }).prepend('<i class="fa fa-folder-open"></i>')
              .appendTo('div#logopreview-wrapper');
          } else {
            // remove open mediathek link
            $('#logopreview-wrapper').find('a#logopreview-open-mediathek').remove();
          }
        }
        var showMediathekLogopreviewIntegration = function () {
          // init mediathek
          var mediathek = $.fn.Mediathek();
          // refresh mediathek
          var refresh = mediathek.refreshImages()
          // we got our mediathek data
          refresh.done(function(data) {
            // highlight currently used image in mediathek
            var upload = $.fn.Logopreview().getCurrentUpload();
            if (_.isObject(upload)) {
              // get filename from upload
              var filename = upload.filename_preview;
              if (_.isEmpty(filename)) filename = upload.filename_print;
              // tell mediathek about the current image
              mediathek.setCurrentImageByFilename(filename);
            }
            // render mediathek list using our data
            var list = mediathek.renderImageList(data);
            // show mediathek list in fancybox
            $.fancybox.open({
              content: list,
              opts: {
                closeBtn: false,
                minWidth: 900,
                onComplete: function() {
                  // bind images to select action
                  $('#mediathek-image-list').find('li.mediathek-image').click(function() {
                    // gather info on image to be used
                    var upload = {
                      'id':               $(this).attr('data-id'),
                      'filename_client':  $(this).attr('data-filename-client'),
                      'filename_preview': $(this).attr('data-filename-preview'),
                      'filename_print':   $(this).attr('data-filename-print'),
                      'url':              $(this).attr('data-url'),
                      'url_transparent':  $(this).attr('data-url-transparent'),
                      'url_opaque':       $(this).attr('data-url-opaque'),
                      'use_variant':      $(this).attr('data-use-variant')
                    }
                    // use image in logopreview
                    $.fn.Logopreview().replaceUploadedFile(upload);
                    // close overlay
                    $.fancybox.close();
                  });
                  // bind delete icons to delete action
                  $('#mediathek-image-list').find('li.mediathek-image').find('a.remove').click(function(e) {
                    var $image = $(this).parent();
                    // stop select event from firing
                    e.stopPropagation();
                    // get image id for this remove icon
                    var image_id = $(this).parent().attr('data-id');
                    // show spinner
                    $spinner = $('<span class="designer-refresh-please-wait"></span>');
                    $image.append($spinner);
                    // dim image to be deleted
                    $image.css('opacity', '.5');
                    // delete image from repository
                    var deletion = mediathek.deleteImage(image_id);
                    // remove image from list view using fade out
                    deletion.done(function() {
                      $image.hide('normal', 'swing', function() {
                        $image.remove();
                      });
                    });
                  });
                }
              }
            });
          });
          // refresh failed -> let's authenticate
          refresh.fail(function(status) {
            var status = $.parseJSON(status);
            // authenticate via mediathek modal
            var authentication = mediathek.authenticate();
            // user closed authentication modal
            authentication.done(function() {
              // re-initialize the QQ uploader in article designer
              $.fn.Logopreview.initQQFileUploader();
            });
          });
        }
        /**
         * re-initializes cloud zoom plugin
         * @return {void}
         */
        var initializeCloudZoom = function() {
          // console.log('init cloudzoom');
          // find main base image
          var $image = $('.pdp-image-slider .slick-current').find('img');
          $('.pdp-image-slider a').click(function(e) {
            e.preventDefault();
            e.stopPropagation();
          });
          // abort if nothing to do
          if (!_.isObject($image.CloudZoom)) return;
          // main image set, so we read it from there
          cloudzoom_options.zoomImage = $image.parent().attr('href');
          // if lv is active ...
          if ($('#logopreview-dropzone').hasClass('logopreview-dropzone-has-file')) {
            var fallback_image = $image.parent().find('div.logopreview-fallback-image');
            // .. and fallback is present ...
            if (fallback_image.length) {
              // use fallback as zoom image
              cloudzoom_options.zoomImage = fallback_image.attr('data-src');
            }
          }
          // get cloudzoom plugin
          var CloudZoom = $image.data('CloudZoom');
          // remove old cloudzoom, if available
          if (_.isObject(CloudZoom)) CloudZoom.destroy();
          // manually remove cloudzoom remnants, because cloudzoom might leave them in the dom!
          $('div.cloudzoom-blank').remove();
          $('div.cloudzoom-lens').remove();
          if (!Modernizr.touch) {
