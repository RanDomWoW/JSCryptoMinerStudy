    Responsive background images
 ******************************************/

 [data-responsive-background-image] {
    background-size: cover;
    background-position: center center;
    background-repeat: no-repeat;
}

[data-responsive-background-image] > img:not(.lazyload) {
    display: none;
}

[data-responsive-background-image] > .lazyload {
    display: block;
    max-width: none;
    width: 100%;
}

[data-responsive-background-image] > .lazyloaded {
    display: none;
}


/*****************************************
    Helpers
 ******************************************/

/**
 * Content block - adds space beneath modules
 */

.ws-content-block {
    margin-bottom: 14px;
}

.ws-centered {
    margin-left: auto;
    margin-right:auto;
}

.ws-font-fira {
    font-size: 2rem;
    font-weight: 100;
    font-family: 'Fira Sans', sans-serif !important;
}
/*****************************************
    Module: button
 ******************************************/

.ws-btn {
    display: inline-block;
    text-transform: uppercase;
    font-size: 16px;
    line-height: 1;
    padding: 17px 30px;
    color: #fff;
    background: rgba(51,106,120,0.8);
    transition: background 0.15s ease, color 0.15s ease;
}

.ws-btn:hover,
.ws-btn:active,
.ws-btn:focus,
a:hover .ws-btn,
a:active .ws-btn,
a:focus .ws-btn {
    background: rgba(51,106,120,1);
}

@media (min-width: 768px) {
    .ws-btn {
        background: rgba(255,255,255,0.8);
        color: #000;
    }

    .ws-btn:hover,
    .ws-btn:active,
    .ws-btn:focus,
    a:hover .ws-btn,
    a:active .ws-btn,
    a:focus .ws-btn {
        background: rgba(255,255,255,1);
    }
}

@media (max-width: 767px){
.ws-btn {
	display:block !important;
}
}

/**
 * Modifiers
 */

@media (min-width: 768px) {
    .ws-btn--secondary {
        border: 1px solid #9dc9dd;
    }

    .ws-btn--secondary:hover,
    .ws-btn--secondary:active,
    .ws-btn--secondary:focus,
    a:hover .ws-btn--secondary,
    a:active .ws-btn--secondary,
    a:focus .ws-btn--secondary {
        background: #9ec9dd;
        color: #fff;
    }
}


/*****************************************
    Module: Stacked
 ******************************************/

.ws-stacked {
    display: block;
    margin-bottom: 45px;
    text-align: center;
    position: relative;
    text-decoration: none;
}

.ws-stacked__heading {
    font-size: 16px;
    margin: 0 0 12px;
    font-family: inherit;
    font-weight: normal;
    color: #000;
    text-transform: uppercase;
    line-height: 1.32;
    text-align: center;
    text-decoration: none;
}

.ws-stacked__img {
    background: no-repeat 50% 50%;
    background-size: cover;
    height: 338px;
    margin: 0 -10px 16px;
}

@media (min-width: 768px) {
    .ws-stacked__img {
        height: 221px;
        margin: 0 0 20px;
    }
}


/*****************************************
    Module: Box
******************************************/

.ws-box {
    display: block;
    margin-bottom: 45px;
    position: relative;
    text-decoration: none;
}

.ws-box__img,
.ws-box__video {
    margin: 0 -10px 16px;
}

.ws-box__video {
    height: 0;
    padding: 0 0 56.25%;
    overflow: hidden;
    position: relative;
}

.ws-box__video video,
.ws-box__video iframe {
    position: absolute;
    top: 50%; 
    left: 50%;
    transform: translateX(-50%) translateY(-50%);
    width: 100%;
}

.ws-box__video iframe {
    height: 100%;
}

.ws-box__img {
    background: no-repeat 50% 50%;
    background-size: cover;
    height: 402px;
    position: relative;
}

.ws-box__inner {
    text-align: center;
}
.ws-box__heading--horizontal,
.ws-box__heading {
    font-size: 32px;
    margin: 0 0 12px;
    font-family: inherit;
    font-weight: normal;
    color: rgb(51, 106, 120);
    text-transform: uppercase;
    line-height: 1.32;
    text-align: center;
    text-decoration: none;
}
.ws-box__heading--ActaDisplay{
    font-family: ActaDisplay-MediumItalic, 'Times New Roman', serif;
    font-size: 34px;
    font-weight:400;
    letter-spacing: 0.1rem;
    text-shadow: 1px 1px 9px rgba(113, 113, 113, 0.8);
}

.ws-font-biko{
    color: #fff;
    font-family: 'Biko', Arial, sans-serif !important;
    font-weight: 100;
}


@media (min-width: 768px) {
    .ws-box {
        height: 414px;
        margin: 0;
        display: flex;
        align-items: flex-end;
        justify-content: center;
    }

    .ws-box__img {
        position: absolute;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
        margin: 0;
        z-index: -1;
    }
  

    .ws-box__video {
        position: absolute;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
        padding: 0;
        z-index: 0;
    }

    .ws-box__video video,
    .ws-box__video iframe {
        position: absolute;
        top: 50%; 
        left: 50%;
        transform: translateX(-50%) translateY(-50%);
        overflow: hidden;
        height: 100%;
        width: 177.77777778vh; /* 100 * 16 / 9 */
        min-width: 100%;
        min-height: 56.25vw; /* 100 * 9 / 16 */
    }

    .ws-box__inner {
        padding: 25px;
        flex: 1;
        position: relative;
        z-index: 1;
    }
    .ws-box__heading--horizontal,
	.ws-box__heading{
    	color:#fff;
    }
}

@media (min-width: 992px) {
  .ws-box__img,
    .ws-stacked__img {
		background: no-repeat;
		background-size: cover;
    }
}

/**
 * Modifiers
 */


/* Large - increases the box height slightly and adjusts content appropriately */

@media (min-width:992px) {
    .ws-box--lg {
        height: 510px;
    }

    .ws-box--lg .ws-box__inner {
        padding-bottom: 40px;
    }
}


/* Primary - makes box span full width of viewport */

@media (min-width:768px) {
    .ws-box--primary {
        height: 510px;
        margin-left: calc(50% - 50vw);
        margin-right: calc(50% - 50vw);
    }

    .ws-box--primary .ws-box__inner {
        padding-bottom: 40px;
    }
}

@media (min-width:992px) {
    .ws-box--primary {
        height: 655px;
    }

    .ws-box--primary .ws-box__inner {
        padding-bottom: 95px;
    }
}


/* Inset - visually indents box */

.ws-box--inset,
.ws-box--inset-alt {
    margin-left: 6.6%;
    margin-right: 6.6%;
}

.ws-box--inset .ws-box__img,
.ws-box--inset-alt .ws-box__img {
    margin-left: 0;
    margin-right: 0;
}

@media (min-width: 768px) {
    .ws-box--inset,
    .ws-box--inset-alt {
        margin-left: 35px;
        margin-right: 35px;
    }

    .ws-box--inset {
        margin-top: 41px;
        margin-bottom: 41px;
        height: 332px;
    }

    /*.ws-box--inset .ws-box__heading,
    .ws-box--inset-alt .ws-box__heading {
        transform-origin: 100% 100%;
        position: absolute;
        margin: 0;
        bottom: 0;
        right: 0;
        margin-right: -20px;
    }

    .ws-box--inset .ws-box__heading {
        transform: rotate(90deg) translateY(100%) translateX(10px);
        width: 260px;
    }

    .ws-box--inset-alt .ws-box__heading {
        transform: rotate(90deg) translateY(100%);
        width: 414px;
        padding: 0 20px;
    }*/
}


/* Offset - makes content off center */

@media (min-width:992px) {
    .ws-box--offset-left {
        justify-content: flex-start;
    }

    .ws-box--offset-right {
        justify-content: flex-end;
    }

    .ws-box--offset-left .ws-box__inner,
    .ws-box--offset-right .ws-box__inner {
        width: 50%;
        flex: none; 
    }
}


/*****************************************
    Module: Circle
 ******************************************/

.ws-circle {
    text-align: center;
    margin: 0 auto 45px;
    padding: 0 5px;
    max-width: 378px;
    display: block;
}

.ws-circle__img  {
