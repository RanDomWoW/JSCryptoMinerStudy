    function select_allNewsletters(element)
	{
  		var elements = element.form.elements;

  		if(element.checked == false)
  		{
			element.checked = true
		}
		else
		{
			element.checked = false;
		}
  
  		for(
    		var elementIndex = 0, elementsLength = elements.length;
    		elementIndex < elementsLength;
   			 elementIndex++
  		)
  		{
    		var current = elements[ elementIndex ];
    		if(current.getAttribute('name') === 'gid')
    		{
      			current.checked = element.checked;
     
    	}
  	}
}
