<script type="text/javascript">
	function truncate(str, len) {
		var txt = str;
		var suffix = '…';
		var b = 0;
		for (var i = 0; i < txt.length; i++) {
			b += txt.charCodeAt(i) <= 255 ? 0.5 : 1;
			if (b > len) {
				txt = txt.substr(0, i) + suffix;
				break;
			}
		}
		return txt;
	}
	$(document).ready(function() {
		// タブ設定
		$('#news-cnt').cookieTab();
		// リスト読み込み
		$.ajax({
			url: 'http://cont.t-com.ne.jp/assets/parts/articles.json?callback=?',
			dataType: 'jsonp',
			jsonpCallback: "newsCb",
			success: function(data) {
				showContents(data);
			},
			error: function(data) {},
			complete: function(data) {}
		});

		function showContents(data) {
			var maxStrLen = 20;
			var maxStrLen4pic = 15;
			var maxItems4Topics = 7;
			var maxItems = 10;
			var mainTopics = data.topTopics;
			// トップトピックス(画像)
			if (data.topTopicsEntame) {
				if (data.topTopicsEntame.length >= 1) {
					data.topTopicsEntame[0].title = truncate(data.topTopicsEntame[0].title, maxStrLen4pic);
					$("#container_topics_entame1 .data_link").attr({
						title: data.topTopicsEntame[0].title,
						href: data.topTopicsEntame[0].url
					});
					$("#container_topics_entame1 .img").css({
						"background": "url('" + data.topTopicsEntame[0].thumbnail + "')",
						"background-size": "contain",
						"background-repeat": "no-repeat",
						"background-position": "center top"
					});
					$("#container_topics_entame1 .data_title").text(data.topTopicsEntame[0].title);
				}
				if (data.topTopicsEntame.length >= 2) {
					data.topTopicsEntame[1].title = truncate(data.topTopicsEntame[1].title, maxStrLen4pic);
					$("#container_topics_entame2 .data_link").attr({
						title: data.topTopicsEntame[1].title,
						href: data.topTopicsEntame[1].url
					});
					$("#container_topics_entame2 .img").css({
						"background": "url('" + data.topTopicsEntame[1].thumbnail + "')",
						"background-size": "contain",
						"background-repeat": "no-repeat",
						"background-position": "center top",
					});
					$("#container_topics_entame2 .data_title").text(data.topTopicsEntame[1].title);
				}
			}
			// トップトピックス
			for (var i in mainTopics) {
				if (i >= maxItems4Topics) break;
				$("#tabbody1 .newsTxtList").append('<li><a href="' + mainTopics[i].url + '">' + truncate(mainTopics[i].title, maxStrLen) + '</a></li>');
			}
			// スポーツ
			for (var i in data.sports) {
				if (i >= maxItems) break;
				$("#tabbody2 .newsTxtList").append('<li><a href="' + data.sports[i].url + '">' + truncate(data.sports[i].title, maxStrLen) + '</a></li>');
			}
			// エンタメ
			for (var i in data.entame) {
				if (i >= maxItems) break;
				$("#tabbody3 .newsTxtList").append('<li><a href="' + data.entame[i].url + '">' + truncate(data.entame[i].title, maxStrLen) + '</a></li>');
			}
			// コネタ
			for (var i in data.dpz) {
				if (i >= maxItems) break;
				$("#tabbody4 .newsTxtList").append('<li><a href="' + data.dpz[i].url + '">' + truncate(data.dpz[i].title, maxStrLen) + '</a></li>');
			}
		}
	});
</script>
