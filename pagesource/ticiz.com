        $.ui.autocomplete.prototype._renderMenu_orig = $.ui.autocomplete.prototype._renderMenu;
        $.ui.autocomplete.prototype._renderMenu_main_page_search = function(ul, items) {
            var self = this;
            if (items.length > 0){
                last_search_value = ''
                if(typeof(items[0]['href']) != 'undefined') {
                    ul.append('<span class="header">Önerilen Kategoriler:</span>');
                }
            }
            $.each( items, function( index, item ) {
                self._renderItem( ul, item );
            });
        };

        $.ui.autocomplete.prototype._renderItem_orig = $.ui.autocomplete.prototype._renderItem;
        $.ui.autocomplete.prototype._renderItem_main_page_search = function(ul, item) {
            return $( "<li></li>" )
                .data( "item.autocomplete", item )
                .append('<a class="'+ item["class"] +'">' + item.label + '</a>')
                .appendTo( ul );
        };
        $('#search_text').focus(function() {
            $.ui.autocomplete.prototype._renderMenu = $.ui.autocomplete.prototype._renderMenu_main_page_search;
            $.ui.autocomplete.prototype._renderItem = $.ui.autocomplete.prototype._renderItem_main_page_search;
        });
        $('#search_text').focusout(function() {
            $.ui.autocomplete.prototype._renderMenu = $.ui.autocomplete.prototype._renderMenu_orig;
            $.ui.autocomplete.prototype._renderItem = $.ui.autocomplete.prototype._renderItem_orig;
        });
        
        (function($) {
            var div = $("<div>");
            last_search_value = ''
            $('#search_text').autocomplete({
                source: '/main_page/suggest_search?catalog_type=product',
                select: function(event, ui) {
                    if (ui.item["class"] == 'query'){
                        $('#search_text').val(ui.item.value);
                        $('#search_form').submit();
                    } else {
                        location.href=ui.item.href
                    }
                },
                focus: function(event, ui) {
                    ui.item.value = div.html(ui.item.value).text();
                },
                search: function(event, ui){
                    if (last_search_value && this.value.indexOf(last_search_value) == 0) {return false;}
                    last_search_value = this.value;
                },
                delay: 400,
                minLength: 3
            });
        })(jQuery);
    

