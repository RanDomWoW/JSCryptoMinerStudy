        @media screen and (min-width: 1000px) {
            header {
                padding: 27px 0 18px; }
            header svg {
                height: 90px; } }

        main,
        footer {
            margin: 27px auto; }
        @media (max-width: 767px) {
            main,
            footer {
                padding: 0 18px; } }
        @media (min-width: 768px) {
            main,
            footer {
                padding: 0 57px; } }
        @media (min-width: 1000px) {
            main,
            footer {
                max-width: 1008px; } }
        @media (min-width: 1000px) {
            main,
            footer {
                padding: 0 9px; } }
        @media screen and (min-width: 1000px) {
            main,
            footer {
                max-width: 792px; } }

        main .dak-maintenance__content {
            display: flex;
            flex-flow: row nowrap; }
        @media (max-width: 767px) {
            main .dak-maintenance__content {
                flex-direction: column; } }

        main .dak-maintenance__text {
            flex: 1 0 66.67%;
            padding-right: 18px; }

        main .dak-maintenance__image {
            flex: 1 0 33.33%;
            text-align: center; }
        main .dak-maintenance__image svg {
            margin-bottom: 27px;
            width: 100%;
            max-width: 117px;
            height: auto; }

        footer {
            text-align: center; }
        footer p {
            border-top: 1px solid #ec7d27;
            padding-top: 27px;
            color: #ec7d27; }

