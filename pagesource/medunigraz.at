	<script>
  //console.log("Test...");
  
  //Zieldatum
  var jahr=2017, monat=10, tag=13, stunde=11, minute=00, sekunde=00;
  //var jahr=2017, monat=1, tag=17, stunde=0, minute=0, sekunde=0;
  var zielDatum = new Date(jahr,monat-1,tag,stunde,minute,sekunde);
  
  function getTimeValue(total, divider)
  {
    var val = Math.floor(total/divider);
    var rest = total - val * divider;
    return { value: val, rest: rest};
  }

  function pad(num, size) 
  {
    var s = num+"";
    while (s.length < size) s = "0" + s;
    return s;
  }
  
  function countdown() 
  {
    startDatum=new Date(); // Aktuelles Datum

    var oneDay = 24*60*60*1000;
    var timediff = (zielDatum.getTime() - startDatum.getTime());
    if(timediff < 0)
    {
      timediff = 0;
    }

    var result = getTimeValue(timediff, 24*60*60*1000);
    var days = result.value;
    result = getTimeValue(result.rest, 60*60*1000);
    var hours = pad(result.value, 2);
    result = getTimeValue(result.rest, 60*1000);
    var minutes = pad(result.value, 2);
    result = getTimeValue(result.rest, 1000);
    var seconds = pad(result.value, 2);

    var countdownTextElem = document.getElementById('medcampus_countdownvalue');
    if(countdownTextElem)
    {
      countdownTextElem.style.fontSize = "x-large";
      (days!=1)?days=days+" Tage ":days=days+ " Tag ";
      countdownTextElem.innerHTML = days + " " + hours + ":" + minutes + ":" + seconds;
    }
    
    var countdownTextElem = document.getElementById('medcampus_countdownvalue_en');
    if(countdownTextElem)
    {
      countdownTextElem.style.fontSize = "x-large";
      (days!=1)?days=days+" days ":days=days+ " day ";
      countdownTextElem.innerHTML = days + " " + hours + ":" + minutes + ":" + seconds;
    }
    
    setTimeout('countdown()',200);
  }
  
  countdown();
  
</script>


	


