    <script src="/Content/libs/jquery.tmpl-1.0.0/jquery.tmpl.min.js"></script>
    <script src="/Content/libs/fullPager2.8.9/jquery.fullpage.min.js" type="text/javascript"></script>
    <script src="/Content/libs/imagesloaded/imagesloaded.pkgd.min.js" type="text/javascript"></script>
    <script src="/Content/libs/jquery.twbsPagination-1.4.1/jquery.twbsPagination.min.js"></script>
    <script src="/Content/libs/broswer0.1.0/jquery.browser.min.js" type="text/javascript"></script>

    <script type="text/javascript">


        $(function () {
            $UI.render();
        })


        var $UI = (function ($, underfine) {

            var that = {};

            that.httpPrefix = '/';
            that.pageSize = 7;
            that.menuElement = '#menu';
            that.registerForm = '#registerForm';
            that.registerFloatForm = '#registerFloatForm';


            that.init = function () {
                $("#tradeName").on("change", function () {
                    var selected = $(this).find("option:selected");
                    var companyName = selected.data('companyName');
                    var userId = selected.data('id');
                    var userName = selected.data('name');
                    $("#companyName").val(companyName);
                    that.getKeywords(userId);
                    that.getCompanyDatas(userName, true);
                });

                $("#smsCodeBtn", this.registerForm).on("click", function () {
                    var _this = $(this);
                    var mobile = $("#mobile", that.registerForm).val();

                    that.getSmsCode(mobile);
                });

                $("#mobile", this.registerForm).on("change", function () {
                    var _this = $(this);
                    var mobile = $("#mobile", that.registerForm).val();
                    that.checkMobile(mobile);
                })

                $("#registerBtn", this.registerForm).on("click", function () {
                    var _this = $(this);
                    var mobile = $("#mobile", that.registerForm).val();
                    var password = $("#password", that.registerForm).val();
                    var smsCode = $("#smsCode", that.registerForm).val();

                    that.register(mobile, password, smsCode);

                });

                $("#smsCodeBtn", this.registerFloatForm).on("click", function () {
                    var _this = $(this);
                    var mobile = $("#mobile", that.registerFloatForm).val();

                    that.getSmsCode(mobile, true);
                });

                $("#mobile", this.registerFloatForm).on("change", function () {
                    var _this = $(this);
                    var mobile = $("#mobile", that.registerFloatForm).val();
                    that.checkMobile(mobile, true);
                });

                $("#mobile, #smsCode, #password", that.registerFloatForm).on("focus", function () {
                    $(this).closest(".form-group").removeClass("has-error has-feedback").removeAttr("attr");
                })


                $("#registerBtn", this.registerFloatForm).on("click", function () {
                    var _this = $(this);
                    var mobile = $("#mobile", that.registerFloatForm).val();
                    var password = $("#password", that.registerFloatForm).val();
                    var smsCode = $("#smsCode", that.registerFloatForm).val();

                    that.register(mobile, password, smsCode, true);

                });

                this.onePagerInit();
                that.getTradeNames();


            };

            that.calcSize = function () {
                var windowWidth = $(window).width();
                $('body').css({ fontSize: windowWidth < 968 ? "100%" : windowWidth / 1120 * 100 + '%' });
                var sHeight = $(window).height();
                var sWidth = $(window).width();
                $('.section').each(function (index, item) {
                    var _this = $(this);
                    var title = $('.docs-section-title', _this);
                    var content = $('.docs-content', _this);
                    content.css({
                        height: sHeight - title.height() - sHeight * 0.15 - 90
                    })

                });


                //section1
                var section1 = $('.section1');
                var title1 = $('.docs-section-title', section1);
                var content1 = $('.docs-content', section1);
                var content1Img = $('.picture', content1);
                content1Img.css({
                    marginLeft: -(943 * content1Img.height() / 500) / 2
                });

                //section2
                var section2 = $('.section2');
                var title2 = $('.docs-section-title', section2);
                var content2 = $('.docs-content', section2);

                //section3
                var section3 = $('.section3');
                var title3 = $('.docs-section-title', section3);
                var content3 = $('.docs-content', section3);
                var content3Img = $('.picture', content3);
                content3Img.css({
                    marginLeft: -(1017 * content3Img.height() / 789) / 2
                });

                //section4
                var section4 = $('.section4');
                var title4 = $('.docs-section-title', section4);
                var content4 = $('.docs-content', section4);
                var content4Text = $('.docs-text', content4);
                content4Text.css({
                    paddingTop: (content4.height() - content4Text.height()) / 2
                });

                //section5
                var section5 = $('.section5');
                var title5 = $('.docs-section-title', section5);
                var content5 = $('.docs-content', section5);
                var content5Img = $('.picture', content5);
                content5Img.css({
                    marginLeft: -(880 * content5Img.height() / 297) / 2
                });

                //section6
                var section6 = $('.section6');
                var title6 = $('.docs-section-title', section6);
                var content6 = $('.docs-content', section6);

                //section7
                var section7 = $('.section7');
                var title7 = $('.docs-section-title', section7);
                var content7 = $('.docs-content', section7);
                var content7Img = $('.picture', content7);
                content7Img.css({
                    marginLeft: -(1753 * content7Img.height() / 381) / 2
                });

                //section8
                var section8 = $('.section8');
                var title8 = $('.docs-section-title', section8);
                var content8 = $('.docs-content', section8);
                var content8Img = $('.picture', content8);
                content8Img.css({
                    marginLeft: -(1800 * content8Img.height() / 596) / 2
                });

                //section9
                var section9 = $('.section9');
                var title9 = $('.docs-section-title', section9);
                var content9 = $('.docs-content', section9);
                var content9Text = $('.docs-text', content9);
                content9Text.css({
                    paddingTop: (content9.height() - content9Text.height()) / 2
                });

                //section10
                var section10 = $('.section10');
                var title10 = $('.docs-section-title', section10);
                var content10 = $('.docs-content', section10);

                //section11
                var section11 = $('.section11');
                var title11 = $('.docs-section-title', section11);
                var content11 = $('.docs-content', section11);
                var content11Img = $('.picture', content11);
                content11Img.css({
                    marginLeft: -(1115 * content11Img.height() / 263) / 2
                });

                //section12
                var section12 = $('.section12');
                var title12 = $('.docs-section-title', section12);
                var content12 = $('.docs-content', section12);
                content12.css({
                    height: sHeight - title12.height() - 161
                });

                //section13
                var section13 = $('.section13');
                var title13 = $('.docs-section-title', section13);
                var content13 = $('.docs-content', section13);
                content13.css({
                    height: sHeight - title13.height() - $('.docs-bottom', section13).height() - 111,
                    bottom: $('.docs-bottom', section13).height()
                });

            }

            that.onePagerInit = function () {

                if ($.browser.desktop) {
                    $("html").addClass("docs-fullpager");
                    this.calcSize(),
                    $(window).resize(function () {
                        that.calcSize()
                    })
                    $('#dowebok').fullpage({
                        menu: that.menuElement,
                        anchors: ['page1', 'page2', 'page3', 'page4', 'page5', 'page6', 'page7', 'page8', 'page9', 'page10', 'page11', 'page12', 'page13'],
                        css3: true,
                        scrollingSpeed: 700,
                        onLeave: function (index, nextIndex, direction) {
                        },
                        afterLoad: function (anchorLink, index) {
                        },
                        afterRender: function () {
                            $(that.menuElement).css({ marginTop: 0 - $(that.menuElement).height() / 2 });
                        }
                    });
                }
            }

            that.getTradeNames = function () {
                $.post('/v2/Home/getTradeNames', function (data) {
                    $.tmpl('<option value="${TradeName}" data-company-name="${CompanyName}" data-id="${UserId}" data-name="${UserName}">${TradeName}</option>', data).appendTo('#tradeName');
                    $("#companyName").val(data[0].CompanyName);
                    that.getKeywords(data[0].UserId);
                    that.getCompanyDatas(data[0].UserName, true);

                })
            }

            that.getCompanyDatas = function (userName, initPager, pageIndex) {
                pageIndex = pageIndex || 1;
                var tbody = $('#companyDatas tbody');
                $("tr:not(.table-loading)", tbody).hide();
                $("tr.table-loading", tbody).show();
                $.post('/v2/Home/GetCompanyDatas', { userName: userName, pageIndex: pageIndex, pageSize: that.pageSize }, function (data) {
                    $("tr:not(.table-loading)", tbody).remove();
                    $("tr.table-loading", tbody).hide();
                    var tableData = [];
                    $.each(data, function (k, v) {
                        var pageNum;
                        if (v.pageNum == 1) {
                            pageNum = "第一页"
                        }
                        if (v.pageNum == 2) {
                            pageNum = "第二页"
                        }
                        if (v.pageNum == 3) {
                            pageNum = "第三页"
                        }
                        tableData.push({
                            keyword: v.Keyword,
                            engineName: v.SearchEngineName,
                            localFile: v.LocalFile,
                            pageNum: pageNum
                        })
                    })
                    $.tmpl('<tr><td>${keyword}</td><td>${engineName}</td><td><a href="${localFile}" target="_blank">查看快照</a></td><td>${pageNum}</td></tr>', tableData).appendTo(tbody);
                    var totalCounts = 0;
                    if (tableData.length) totalCounts = data[0].DataCount;
                    initPager && $("#tablePager").twbsPagination('destroy') && $("#tablePager").twbsPagination({
                        totalPages: totalCounts % that.pageSize == 0 ? totalCounts / that.pageSize : totalCounts / that.pageSize + 1,
                        visiblePages: 5,
                        startPage: 1,
                        first: '首页',
                        prev: '上一页',
                        next: '下一页',
                        last: '尾页',
                        initiateStartPageClick: false,
                        onPageClick: function (event, page) {
                            that.getCompanyDatas(userName, false, page);
                        }
                    });

                })
            }

            that.getKeywords = function (userId) {
                $.post('/v2/Home/GetKeywords', { userID: userId }, function (data) {
                    $("#KeyWordHasRankCount").text(data.KeyWordHasRankCount);
                    $("#TotalOnlineKeyWords").text(data.TotalOnlineKeywords);
                    $("#YesterdayProductCoverCount").text(data.YesterdayProductCoverCount);
                    $("#ProductCoverCount").text(data.ProductCoverCount);
                }
              )

            }

            that.getSmsCode = function (mobile, isFloat) {
                var vaild = this.vaildRegister([{ field: 'mobile', value: mobile }], isFloat);
                if (!vaild) return;

                $.post('/v2/Home/CreateMobileVerificationCode', { mobile: mobile }, function (data) {
                    if (!data.IsSuccess) {
                        alert(data.Msg);
                    }
                });

                var time = 60;
                var smsCodeBtn = $("#smsCodeBtn", isFloat ? that.registerFloatForm : that.registerForm);
                smsCodeBtn.data('text', smsCodeBtn.html()).html(time + '秒').prop('disabled', true);
                var smsCodeTimeOut = setInterval(function () {
                    smsCodeBtn.html(--time + '秒');
                    if (time == 0) {
                        clearInterval(smsCodeTimeOut);
                        smsCodeBtn.html(smsCodeBtn.data('text')).prop('disabled', false);
                    }
                }, 1000);
            }

            that.checkMobile = function (mobile, isFloat) {
                var vaild = this.vaildRegister([{ field: 'mobile', value: mobile }], isFloat);
                if (!vaild) return;
                var data = {
                    mobile: mobile
                };
                $.post('/v2/Home/CheckMobileExists', data, function (data) {
                    if (!data.IsSuccess) {
                        if (isFloat)
                            $("#mobile", that.registerFloatForm).closest(".form-group").addClass("has-error has-feedback").attr("title", data.Msg);
                        else
                            $("#mobileTooltip").html(data.Msg).fadeIn(400).delay(3000).fadeOut(400);
                    }
                    else
                        $("#mobile", that.registerFloatForm).closest(".form-group").removeClass("has-error has-feedback").removeAttr("title");
                }).done(function () {

                }).fail(function () {
                    alert("系统错误！");
                });
            }

            that.register = function (mobile, password, smsCode, isFloat) {

                var vaild = this.vaildRegister([{ field: 'mobile', value: mobile }, { field: 'password', value: password }, { field: 'smsCode', value: smsCode }], isFloat);
                if (!vaild) return;

                var data = {
                    mobile: mobile,
                    password: password,
                    confirmPassword: password,
                    verificationCode: smsCode,
                    source: 'Official',
                }
                var registerBtn = $("#registerBtn", isFloat ? that.registerFloatForm : that.registerForm);
                $.post('/v2/Home/RegisterByMobilePhone', data, function (data) {
                    if (data.IsSuccess) {
                        // window.location.href = "/v2/User/Login";
                        window.location.href = data.Msg;
                    }
                    else {
                        if (data.Msg == "已存在相同手机号") {
                            if (isFloat)
                                $("#mobile", that.registerFloatForm).closest(".form-group").addClass("has-error has-feedback").attr("title", "已存在相同手机号，请登录！");
                            else
                                $("#mobileTooltip", that.registerForm).html('已存在相同手机号，请登录！').fadeIn(400).delay(3000).fadeOut(400);
                        }
                        else {
                            if (isFloat)
                                $("#smsCode", that.registerFloatForm).closest(".form-group").addClass("has-error has-feedback").attr("title", data.Msg);
                            else
                                $('#smsCodeTooltip', that.registerForm).html(data.Msg).fadeIn(400).delay(3000).fadeOut(400);
                        }
                    }

                }).done(function () {
                    registerBtn.prop("disabled", false);
                }).fail(function () {
                    alert("系统错误！");
                });
                registerBtn.prop("disabled", true);
            }

            that.vaildRegister = function (fields, isFloat) {

                fields = fields || [];
                var flag = true;
                $.each(fields, function (index, item) {
                    if (item.field == 'mobile') {
                        var pattern = /^1(3[0-9]|4[57]|5[0-35-9]|7[0135678]|8[0-9])\d{8}$/;
                        if (!pattern.test(item.value)) {
                            if (isFloat)
                                $("#mobile", that.registerFloatForm).closest(".form-group").addClass("has-error has-feedback").attr("title", "请输入正确的手机号码！").trigger("mouseover");
                            else
                                $("#mobileTooltip", that.registerForm).html("请输入正确的手机号码！").fadeIn(400).delay(3000).fadeOut(400);
                            flag = false;
                        }
                        else
                            $("#mobile", that.registerFloatForm).closest(".form-group").removeClass("has-error has-feedback").removeAttr("title");
                    }
                    if (item.field == 'password') {
                        var pattern = /[A-Za-z0-9]{6,20}/;
                        if (!pattern.test(item.value)) {
                            if (isFloat)
                                $("#password", that.registerFloatForm).closest(".form-group").addClass("has-error has-feedback").attr("title", "请输入6至20位字母或数字！");
                            else
                                $("#passwordTooltip", that.registerForm).html("请输入6至20位字母或数字！").fadeIn(400).delay(3000).fadeOut(400);
                            flag = false;
                        }
                        else
                            $("#password", that.registerFloatForm).closest(".form-group").removeClass("has-error has-feedback").removeAttr("title");
                    }
                    if (item.field == 'smsCode') {
                        if (!item.value.length) {
                            if (isFloat)
                                $("#smsCode", that.registerFloatForm).closest(".form-group").addClass("has-error has-feedback").attr("title", "请输入手机验证码！");
                            else
                                $("#smsCodeTooltip", that.registerForm).html("请输入手机验证码！").fadeIn(400).delay(3000).fadeOut(400);
                            flag = false;
                        }
                        else
                            $("#smsCode", that.registerFloatForm).closest(".form-group").removeClass("has-error has-feedback").removeAttr("title");
                    }
                });
                return flag;
            }

            that.render = function () {
                that.init();
            };

            return that;
        })(window.jQuery, undefined);
    </script>


