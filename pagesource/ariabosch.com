    jQuery(document).ready(function($){

    $("#ktss-preview").on('show.bs.modal',function(e){

        //Clear previous Content
        $('#ktss-preview .row').empty();

        //send the ajax request
        var product_id = $(e.relatedTarget).attr('data-product-id');

        $.ajax({
            url: "https://ariabosch.com/wp-content/themes/kanishop-theme/ajax/modal-product-quick-view.php",
            type: 'GET',
            data: 'product_id=' + product_id ,
            beforeSend: function() {
                $(".quick-view-loading").show();
            },
            success: function(response) {
                // alert(response)
                $('#ktss-preview .row').html(response);
            },
            complete: function() {
                $(".quick-view-loading").hide();
            }
        });
    });
});
