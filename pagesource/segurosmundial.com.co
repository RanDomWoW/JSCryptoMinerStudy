$('.soat-cot').click(function(){
  var price = $(this).attr('data-price');
    $('.textPadding.priceHold .total').html(price+'COP');
});  
  $('.soat-widget #enviar.btn').click(function(e){
    e.preventDefault();  
    var submit = true;
    $('.soat-widget input').css('border','none');
    $('.soat-widget input[name="Correo_electronico"]').each(function(index){
      
      if(validateMail($(this).val())==false){
      	$(this).css('border','1px solid red');
        submit=false;
      } 
     });

    if(submit==true){
      $('.soat-widget input.field').each(function(){

        if(validateText($(this).val())==false){
          submit=false;
          $(this).css('border','1px solid red');
        }     
      });

    }

    

    if(submit==true){
      $('.bigForm input.numbers').each(function(index) {

