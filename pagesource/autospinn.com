<script type="text/javascript">
	$(document).ready(function() {

		function doTruncate() {
			$('.js-truncate').each(function() {
				var $this = $(this);

				var settings = {};

				if ($this.data('truncate-height')) {
					settings.height = $this.data('truncate-height');
				}

				var lala = $this.dotdotdot(settings);
			});
		}
		doTruncate();

		$('.js-video-gallery--trigger').on('click', function(e) {
			e.preventDefault();
			var title = $(this).data('post-title');
			var video_src = $(this).data('post-video-src') + '?autoplay=1';
			var url = $(this).data('post-url');
			var excerpt = $(this).data('post-excerpt');
			var id = $(this).data('post-id');

			$('.js-video-gallery--iframe').attr('src', video_src);
			$('.js-video-gallery--title').attr('href', url);
			$('.js-video-gallery--title').attr('title', title);
			$('.js-video-gallery--title').html(title);
			$('.js-video-gallery--excerpt').html(excerpt);
		});

		var mmClass_mobileFalse = 'dropdown--active-on-hover--mobile-false';

		function bsDropdownOnHover() {
			$('.dropdown--active-on-hover').on('mouseenter', function() {
				if ($(this).hasClass(mmClass_mobileFalse)) {
					return;
				}

				if (!$(this).hasClass('open')) {
					$('.dropdown').removeClass('open');
					$(this).addClass('open');
					doTruncate();
				}
			});

			$('.dropdown--active-on-hover').on('mouseleave', function() {
				if ($(this).hasClass(mmClass_mobileFalse)) {
					return;
				}

				$('.dropdown').removeClass('open');
			});

			$('.dropdown--active-on-hover a[data-toggle="dropdown"]').on('click', function() {
				window.location.href = $(this).attr('href');
			});
		};

		ssm.addState({
			id: 'mobile',
			query: '(max-width: 992px)',
			onEnter: function() {
				// console.log('enter mobile');
				$('.dropdown--active-on-hover').addClass(mmClass_mobileFalse);
				$('body').addClass('body--mobile');
			}
		});
		ssm.addState({
			id: 'desktop',
			query: '(min-width: 993px)',
			onEnter: function() {
				// console.log('enter desktop');
				$('.dropdown--active-on-hover').removeClass(mmClass_mobileFalse);
				bsDropdownOnHover();
				$('body').removeClass('body--mobile');
			}
		});
	});
</script>

