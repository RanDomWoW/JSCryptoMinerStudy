		$('#supertipus-cerca > option[value="' + $("#session_supertipus").val() + '"]').attr('selected', 'selected');
		$('#localitat-cerca').load(WEBSITE_URL + '/ajax/get_poblacions_cerca.php', {'provincia': $("#session_provincia").val() , 'operacion': $("#session_operacio").val() , 'supertipus': $("#session_supertipus").val() }, function(response, status, xhr) {
			locked_changing = false;
			$("#cerca_button").css({"background":"#a5048e url(../images/search.png) no-repeat 5px 5px"});
			$("#loading-select-pc").hide();
			$('#localitat-cerca > option[value="' + $("#session_localitat").val() + '"]').attr('selected', 'selected');
			$("#explicacio_mapa #nomlocalitat").text($('select[id=localitat-cerca]').val());
		});
		
	}
	/* FIN: para recargar valores en el buscador de la home (Versión PC)*/
	
    if (primera_vez_provincia) $("#operacio-cerca-m")[0].selectedIndex = 0;
    if (primera_vez_provincia) $("#provincia-cerca-m")[0].selectedIndex = 0;
    if (primera_vez_provincia) $("#supertipus-cerca-m")[0].selectedIndex = 0;
    $("select#provincia-cerca-m").change();
    $("select#localitat-cerca-m").change();
	
	/* INICIO: para recargar valores en el buscador de la home (Versión Movil) */
	if ($("#session_operacio").val() != ''){
		$('#operacio-cerca-m > option[value="' + $("#session_operacio").val() + '"]').attr('selected', 'selected');
		$('#provincia-cerca-m > option[value="' + $("#session_provincia").val() + '"]').attr('selected', 'selected');
		$('#supertipus-cerca-m > option[value="' + $("#session_supertipus").val() + '"]').attr('selected', 'selected');
		$('#localitat-cerca-m').load(WEBSITE_URL + '/ajax/get_poblacions_cerca.php', {'provincia': $("#session_provincia").val() , 'operacion': $("#session_operacio").val() , 'supertipus': $("#session_supertipus").val() }, function(response, status, xhr) {
			locked_changing = false;
			$("#cerca_button").css({"background":"#a5048e url(../images/search.png) no-repeat 5px 5px"});
			$("#loading-select-mobile").hide();
			$('#localitat-cerca-m > option[value="' + $("#session_localitat").val() + '"]').attr('selected', 'selected');
		});
		$("#session_operacio").val('');//Una vez recargados los datos inicializamos a '' para hacer uso normal del buscador.
		
	}
	/* FIN: para recargar valores en el buscador de la home (Versión Movil)*/	
});
</script><!-- FI CERCADOR -->
