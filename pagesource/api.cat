	$('#supertipus_m_hidden').val($('#supertipus-cerca').val());
    
    if (primera_vez_provincia) $("#operacio-cerca")[0].selectedIndex = 0;
    if (primera_vez_provincia) $("#provincia-cerca")[0].selectedIndex = 0;
    if (primera_vez_provincia) $("#supertipus-cerca")[0].selectedIndex = 0;
    $("select#provincia-cerca").change();
    $("select#localitat-cerca").change();
	
	/* INICIO: para recargar valores en el buscador de la home (Versión PC) */
	if ($("#session_operacio").val() != ''){
		$('#operacio-cerca > option[value="' + $("#session_operacio").val() + '"]').attr('selected', 'selected');
