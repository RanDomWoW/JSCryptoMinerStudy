        <script type="text/javascript">
	idioma = "ca";
	var localize_provincia = '';
	var geocoder;

	function find_provincia() {
		if ( (localize_provincia.toUpperCase() == 'BARCELONA') || (localize_provincia.toUpperCase() == 'TARRAGONA') || (localize_provincia.toUpperCase() == 'LLEIDA') || (localize_provincia.toUpperCase() == 'GIRONA') ) {
			$("#provincia-cerca option").each(function(){
				if ( $(this).text().toUpperCase() == localize_provincia.toUpperCase() ) { $(this).prop('selected', true); $("#provincia-cerca").change(); }
			});
		}
	}

	if (navigator.geolocation) {
    	navigator.geolocation.getCurrentPosition(successFunction, errorFunction);
	}

	//Get the latitude and the longitude;
	function successFunction(position) {
		var lat = position.coords.latitude;
		var lng = position.coords.longitude;
		codeLatLng(lat, lng);
	}
	function errorFunction(){
		//alert("Geocoder failed");
	}
	function initialize_city_location() {
		geocoder = new google.maps.Geocoder();
	}
	function codeLatLng(lat, lng) {
		localize_provincia = '';
		var latlng = new google.maps.LatLng(lat, lng);
		geocoder.geocode({'latLng': latlng}, function(results, status) {
			if (status == google.maps.GeocoderStatus.OK) {
				if (results[0].address_components[4].long_name) {
					localize_provincia = results[0].address_components[4].long_name;
				}
				//city data
				if ( localize_provincia != '') { find_provincia(); }
		  } else {
			//alert("Geocoder failed due to: " + status);
		  }
		});
	}
	initialize_city_location();

	/* Cercador antiguo*/
	$(document).ready(function() {
		var localitat = $("#localitat-cerca").val();
		$('#localidad_hidden').val(localitat);

		$('.tabs .tab-links a').on('click', function(e)  {
			var currentAttrValue = $(this).attr('href');
			if(currentAttrValue == "#tabs1")
			{
				$("#tab2").removeClass("boton_yellow");
				$("#tab1").addClass("boton_yellow");
				$("#tab2").addClass("boton_grey_noactive");
				$("#tab1").removeClass("boton_grey_noactive");
				$("#tabs1").fadeIn(400).siblings().show();
				$("#tabs1").addClass("active");
				$("#tabs2").fadeIn(400).siblings().hide();
				$("#tabs2").removeClass("active");
			}
			if(currentAttrValue == "#tabs2")
			{
				$("#tab2").removeClass("boton_grey_noactive");
				$("#tab2").addClass("boton_yellow");
				$("#tab1").removeClass("boton_yellow");
				$("#tab1").addClass("boton_grey_noactive");
				$("#tabs1").fadeIn(400).siblings().hide();
				$("#tabs1").removeClass("active");
				$("#tabs2").fadeIn(400).siblings().show();
				$("#tabs2").addClass("active");
			}

			e.preventDefault();
		});
	});

	/* Map Drawing */
	function validate_search_map() {
        if( $('#donde').val() == '') {
			alert('Introdueixi el nom de ciutat o població per cercar per mapa.');
		} else {
			veure_mapa( $('#donde').val() );
		}
    }

	function veure_mapa(on) {
		$("#comptador").hide();
			$("div#mapa").slideDown("slow", function() {
			initializeMap();
			$("#explicacio_mapa").slideDown("slow");
			$("div#instruccions").slideDown("slow");
		});
	}

	function refrescar_mapa() {
		$("div#mapa").html("");
		$("#comptador").hide();
		initializeMap();
	}

</script>
