$("#cerca_paraules").click(function(e){
    e.preventDefault();
    $(this).addClass("inactive");
    $("#cerca_classica").removeClass("inactive");
    $("#classica").hide();
    $("div.field").show();
});
$("#cerca_classica").click(function(e){
    e.preventDefault();
    $(this).addClass("inactive");
    $("#cerca_paraules").removeClass("inactive");
    $("div.field").hide();
    $("#classica").show();
});
$('#provincia-cerca').change(function() {
	/* INICIO: Mostrar cargando en Select Localidad al cambiar los otros selects anidados */
	$("#localitat-cerca").html('<option class="">&nbsp;</option>');
	$("#loading-select-pc").show();
	/* FIN: Mostrar cargando en Select Localidad al cambiar los otros selects anidados */
	if ($("#session_operacio").val() == ''){ /* Solo entra en la 1 busqueda. */
		locked_changing = true;
		$("#cerca_button").css({"background":"#F1F1F1 url(../images/search.png) no-repeat 5px 5px"});
		$('#localitat-cerca').load(WEBSITE_URL + '/ajax/get_poblacions_cerca.php', {'provincia': $(this).val(), 'operacion': $('#operacio-cerca').val(), 'supertipus': $('#supertipus-cerca').val()}, function(response, status, xhr) {
