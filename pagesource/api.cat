    <script>

	$(document).ready(function(){
		$('#newsletter_footer_success').hide();
		$('#newsletter_footer_success_2').hide();
		$('#Rebre_novetats_footer').removeClass("border-error");
	});

	function add_list_mailchimp_footer(){
		//Add list mailchimp
		var email = $('#Rebre_novetats_footer').val();

		if (email == ''){
			$('#Rebre_novetats_footer').addClass("border-error");
			$('#newsletter_footer_success_2').show();
			$('#newsletter_footer_success').hide();
			$('#error-x-mail-footer').css("display","block");
            $('#ok-v-mail-footer').css("display","none");
		} else {
			$('#Rebre_novetats_footer').removeClass("border-error");
			$('#newsletter_footer_success_2').hide();
            $('#error-x-mail-footer').css("display","none");
            $('#ok-v-mail-footer').css("display","block");
			if ($('#botemiel').val() == '') {
				$.ajax({
					url: 'ajax/add_mailchimp.php',
					data: {email: email},
					type: 'POST',
					success: function(data){
						$('#newsletter_footer_success').html('<span class="glyphicon glyphicon-thumbs-up"></span> '+ data );
						$('#newsletter_footer_success').show();
					},
					error: function(){
						$('#newsletter_footer_success').html('<span class="glyphicon glyphicon-thumbs-up"></span> '+ data );
						$('#newsletter_footer_success').show();
					}
				});
			}
		}
	}
</script>
