	<script type="text/javascript" src="https://ddtech.mx/assets/js/messenger-theme-flat.js?1514312336"></script>
<script>
	/*============================================================
		SE DECLARAN LAS OPCIONES PARA LOS MENSJAES DE ALERTA
	=============================================================*/
	Messenger.options = {
		extraClasses: 'messenger-fixed messenger-on-top messenger-on-right',
		theme: 'flat'
	}

	/*=========================================
		AGREGAR UN PRODUCTO AL CARRITO
	==========================================*/
	$('.add-cart').click(function(){

		// SE INICIALIZAN LAS VARIABLES
		var url        = $('#url-location').data('url');
		var product_id = $(this).data('product-id');
		var qty;

		// SI TIENE LA CLASE ADD ONE
		if($(this).hasClass('add-one'))
		{
			// SE PONE EL VALOR DE UN PRODUCTO
			qty = 1;
		}
		else
		{
			// SE OBTIENE EL VALOR DEL INPUT
			qty = $('#qty').val();
		}

		// SE EJECUTA EL AJAX
		$.ajax({
			type:     'get',
			dataType: 'json',
			url:      url + 'ajax/add_cart.json',
			data:     'product_id=' + product_id + '&quantity=' + qty,
			success:  function(response)
			{
				// SI LA RESPUESTA ES OK
				if(response.msg == 'ok')
				{
					// SE LIMPIA EL FORMULARIO 
					$('#qty').val('1');

					// SE MUESTRA UN MENSAJE
					Messenger().post({
					  message: 'El producto se ha agregado con éxito.',
					  type: 'success',
					  showCloseButton: true
					});

					// SE AGREGA LA CANTIDAD AL CARRITO
					$('#cart-quantity').html(response.quantity);
				}
				else
				{
					if(response.msg == 'overflow')
					{
						// SE MUESTRA UN MENSAJE
						Messenger().post({
						  message: 'Se ha excedido la cantidad de productos.',
						  type: 'error',
						  showCloseButton: true
						});
					}
				}
			}
		});

		return false;
	});//click
</script>
