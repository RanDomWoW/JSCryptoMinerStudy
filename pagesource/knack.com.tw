        //登入專用的js
        function submitExternalLoginForm(provider) {
            var rUrl = $('#returnUrl').val();
            $('#frmExternalLogin').append('<input type="hidden" name="returnUrl" value="' + rUrl + '" />');
            if (provider == 'Facebook')
                $('#frmExternalLogin').append('<input type="hidden" name="provider" value="Facebook" />');
            if (provider == 'Google')
                $('#frmExternalLogin').append('<input type="hidden" name="provider" value="Google" />');

            $('#frmExternalLogin').submit();
        }
        function initEnterpriseLoginFormSubmit() {
            $('#enterprise-form').submit(function (event) {
                var rUrl = $('#returnUrl').val();
                // cancels the form submission
                event.preventDefault();
                // do whatever you want here
                var data = {};
                data.Email = $('#login-email').val();
                data.Password = $('#login-password').val();
                data.RememberMe = $('#login-rememberme').is(":checked");
                data.ReturnUrl = rUrl;
                $.post("/Account/LS", data, function (result) {
                    if (result.id == 1) {
                        location.href = result.url;
                    }
                    else {
                        $('#login-password').val('');
                        alert(result.msg);
                    }
                });
            });
        }
        function initForgotPasswordFormSubmit() {
            $('#forgot-form').submit(function (event) {
                // cancels the form submission
                event.preventDefault();
                // do whatever you want here
                var data = {};
                data.Email = $('#forget-email').val();
                $.post("/Account/FP", data, function (result) {
                    if (result.id == 1) {
                        $('#forget-password-form').modal('hide');
                        toastr["success"]("<div>" + result.msg + "</div>")
                    }
                    else {
                        alert(result.msg);
                    }
                });
            });
        }
    </script>

