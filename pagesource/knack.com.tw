        function initReturnUrl(url) {
            if (url == null) {
                var rUrlPath = getParameterByName("ReturnUrl", location.href);
                if (rUrlPath == undefined) {
                    url = location.href;
                }
                else {
                    url = rUrlPath;
                }
            }
            $('#returnUrl').val(url);
        }

        function showChanegePwdModal() {
            //若第一次登入才show
            var cookieIsEnterpriseUserFirstLogin = getCookie("IsEnterpriseUserFirstLogin");
            if (cookieIsEnterpriseUserFirstLogin == "IsEnterpriseUserFirstLogin")
            {
                //自動展開
                $('#change-password-form').modal('show');
                //關閉後清cookie
                $('#change-password-form').on('hidden.bs.modal', function () {
                    //不再顯示
                    dontShowChanegePwdModal();
                })
            }
        }
        function dontShowChanegePwdModal() {
            //清掉此cookie
            setCookie("IsEnterpriseUserFirstLogin", "", -100);
        }

        function loadCartCount() {
            var cookie = getCookie("CourseCart");
            if (cookie == "" || Array.isArray(JSON.parse(cookie)) == false) {
                $('#supCartCount').text(0);
            }
            else {
                $('#supCartCount').text(JSON.parse(cookie).length);
            }
        }
