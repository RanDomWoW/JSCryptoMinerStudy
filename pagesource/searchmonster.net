        <script type="text/javascript">
            function preloadImages() {
                var cache = [];
                var args_len = arguments.length;
                for (var i = args_len; i--; ) {
                    var cacheImage = document.createElement('img');
                    cacheImage.src = arguments[i];
                    cache.push(cacheImage);
                }
            }
            $(window).load(function(e) {
                preloadImages("images/technology-mark-hover.png", "images/news-mark-hover.png", "images/entertaintment-mark-hover.png", "images/color-burn.jpg");
                $("#main-menu ul li a").each(function() {
                    $(this).click(function(e) {
                        $("#main-menu ul li").removeClass('active');
                        $(this).parent().addClass('active');
                    });
                })
                $("#inner-header ul li a").each(function() {
                    $(this).click(function(e) {
                        $("#inner-header ul li").removeClass('active');
                        $(this).parent().addClass('active');
                    });
                })
                var currBg = null;

                $(".box").each(function(index) {
                    $(this).mouseenter(function(e) {
                        $(this).find('.bb-top').stop().animate({
                            top: 0
                        }, 500);
                        try {
                            var currBox = this;
                            var blendImg = new Image();
                            blendImg.onload = function() {
                                $(currBox).find('.thumb .inner-g > img:eq(0)').pixastic("blend", {
                                    amount: 1,
                                    mode: "screen",
                                    image: blendImg});
                            }
                            blendImg.src = $(currBox).find('.thumb .inner-g > img:eq(0)').attr('src');
                        } catch (e) {

                        }


                        $(this).find('.bb-left').stop().animate({
                            left: 0
                        }, 500)
                        $(this).find('.bb-right').stop().animate({
                            right: 0
                        }, 500)
                        $(this).find('.red-more').stop().animate({backgroundColor: '#ef4c07'}, 200);
                        $(this).find('.green-more').stop().animate({backgroundColor: '#5ed190'}, 200);
                        $(this).find('.orange-more').stop().animate({backgroundColor: '#ffe440'}, 200);



                        $(this).find('.ent-section').stop().css({background: 'url(images/entertaintment-mark-hover.png) no-repeat'});
                        $(this).find('.news-section').stop().css({background: 'url(images/news-mark-hover.png) no-repeat'});
                        $(this).find('.tech-section').stop().css({background: 'url(images/technology-mark-hover.png) no-repeat'});
                    })
                    $(this).mouseleave(function(e) {
                        $(this).find('.bb-top').stop().animate({
                            top: '-27px'
                        }, 80)
                        $(this).find('.bb-left').stop().animate({
                            left: '-3px'
                        }, 80)
                        $(this).find('.bb-right').stop().animate({
                            right: '-3px'
                        }, 80);
                        try{
                            Pixastic.revert(document.getElementById($(this).find('canvas').prop('id')));
                        }catch(e){
                            
                        }
                        

                        $(this).find('.red-more').stop().animate({backgroundColor: '#c53e04'}, 80);
                        $(this).find('.green-more').stop().animate({backgroundColor: '#54a477'}, 80);
                        $(this).find('.orange-more').stop().animate({backgroundColor: '#ffc500'}, 80);

                        $(this).find('.ent-section').stop().css({background: 'url(images/entertaintment-mark.png) no-repeat'});
                        $(this).find('.news-section').stop().css({background: 'url(images/news-mark.png) no-repeat'});
                        $(this).find('.tech-section').stop().css({background: 'url(images/technology-mark.png) no-repeat'});
                    })
                })

                $(".boxs").each(function(index) {
                    $(this).mouseenter(function(e) {
                        var currBox = this;
                        var blendImg = new Image();
                        blendImg.onload = function() {
                            $(currBox).find('.thumb img:eq(0)').pixastic("blend", {
                                amount: 1,
                                mode: "screen",
                                image: blendImg});
                        }
                        blendImg.src = $(currBox).find('.thumb img:eq(0)').attr('src');
                    });
                    $(this).mouseleave(function(e) {
                        Pixastic.revert(document.getElementById($(this).find('canvas').prop('id')));
                    });
                })
            })
        </script>
