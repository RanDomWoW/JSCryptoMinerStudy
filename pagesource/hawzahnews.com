<script type="text/javascript">

    function Scorll(holder, ScrollSiz, Step) {
        var ScrolSize = ScrollSiz;
        var clickIndex = 1;
        var q = $(".abs", $(holder));
        var stepOfScroll = (Math.ceil($(".itemmm", q).length / Step));
        q.width(Math.ceil($(".itemmm", q).length) * ScrolSize);
        $(".next", $(holder)).unbind('click');
        $(".back", $(holder)).unbind('click');

        $(".next", $(holder)).click(function () {
            //$(".pager", $(holder)).removeClass("Sel");
            //$($(".pager", $(holder)).get(clickIndex)).addClass("Sel")
            if (clickIndex < stepOfScroll) {
                clickIndex++
                $(".abs", $(holder)).animate({ right: "-=" + ScrolSize * Step }, "slow");
            }
            else {
                $(".abs", $(holder)).animate({ right: "0" }, "slow");
                clickIndex = 1;
            }
        })
        $(".prev", $(holder)).click(function () {
            //$(".pager", $(holder)).removeClass("Sel");
            //$($(".pager", $(holder)).get(clickIndex)).addClass("Sel")
            if (clickIndex == 1) {
                clickIndex = 1;
                $(".abs", $(holder)).animate({ right: "0" }, "slow");

            }
            else {
                clickIndex--
                $(".abs", $(holder)).animate({ right: "+=" + ScrolSize * Step }, "slow");

            }
        });
        $(".pager", $(holder)).unbind('click');
        $(".pager", $(holder)).click(function () {
            //if (clickIndex < stepOfScroll) {
            //clickIndex++
            clickIndex = $(this).index() + 1;
            PagerIndex = $(this).index();

            $(".pager", $(holder)).removeClass("Sel");
            $(this).addClass("Sel");
            $(".abs", $(holder)).animate({ right: -(ScrolSize * Step) * PagerIndex }, "slow");
            //}
        })

    }

    //**********


    Scorll('#mainPlace_WI_11', 95, 4);
    
</script>

