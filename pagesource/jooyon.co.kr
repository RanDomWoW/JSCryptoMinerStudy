// 이벤트 레이어팝업 자동 토스트 처리
function layEventPopUp(){
	return;
	if(!get_cookie('toast')){
		setTimeout(function(){
			$(".right_nav_titie02").click();
		}, 100);
		setTimeout(function(){
			$(".right_nav_titie03").click();
		}, 3000);
		setTimeout(function(){
			$(".right_nav_titie03").click();
		}, 6000);
		
		setTimeout(function(){
			$(".right_nav_titie04").click();
		}, 9000);
		
		eventExecu = true;
		set_cookieWithoutSave('toast',true, g5_cookie_domain);
	}
}


// 쿠키 생성 (브라우저 종료시 쿠키 만료 처리)
function set_cookieWithoutSave(name, value, domain)
{
    document.cookie = name + "=" + escape( value ) + "; path=/;";
    if (domain) {
        document.cookie += "domain=" + domain + ";";
    }

	
}



