<script type="text/javascript">
// code for retrieving the Krux user id and segments from either local storage or a cookie
window.Krux || ((Krux = function() {Krux.q.push(arguments);}).q = []);

(function() {
    function retrieveKruxValue(key) {
        var ret = "";
        // check for value in local storage
        if (window.localStorage) {
            ret = window.localStorage.getItem(key);
        }
        if (ret != undefined && ret != "") {
            return ret;
        }
        // check for value in cookie
        if (navigator.cookieEnabled) {
            ret = document.cookie.match(key + '=([^;]*)');
            ret = (ret && unescape(ret[1])) || "";
        }
        return ret;
    }
    var kruxUserId = "";
    // there are two possible variables that hold the krux user id - try them both
    kruxUserId = retrieveKruxValue('kxuser');
    if (kruxUserId == undefined || kruxUserId == "") {
        kruxUserId = retrieveKruxValue('kxkuid');
    }
    Krux.user = kruxUserId;
    Krux.segments = retrieveKruxValue('kxsegs') && retrieveKruxValue('kxsegs').split(',') || [];

})();

// used in _ad_tech.html
window.krux_adtech_param = ';kvkrx=' + window.Krux.segments.join(":");
</script>
