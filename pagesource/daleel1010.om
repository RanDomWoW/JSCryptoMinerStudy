function slideit(){
	//if browser does not support the image object, exit.
	if (!document.images)
		return;
	if (document.getElementById('slide') != null) {
		document.getElementById('slide').src = slideimages[step][0].src;
		whichimage = step;
		if (step < slideimages.length) {
			step++;
		}
		if (step == slideimages.length) {
			step=0;
		}
		setTimeout("slideit()",5000);
	}
}

