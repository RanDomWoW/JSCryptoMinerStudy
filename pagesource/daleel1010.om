<script type="text/javascript">
	$(document).ready(function() {
		var $= jQuery.noConflict();
		$('#forgot_submit').click(function() {
			
			$('#forgot_password').validate();
			var test = isValidEmailAddress(  $('#forgot_email').val() );
			if( $('#forgot_email').val() != '' && test ) {			
				var email_val = $('#forgot_email').val();
				var forgot_csrf = encodeURIComponent( $( '#forgot_csrf' ).val() );
				$( '#forgot_submit' ).hide();
				$.ajax({
					url: 'https://www.daleel1010.om/en' + '/user/aforgot',
					dataType: 'json',
					type: 'post',
					data: 'email=' + email_val+'&forgot_csrf='+forgot_csrf,
					beforeSend : function(){
						$('#forgot_pop').hide();
					    document.getElementById('loader_pop').style.display='block';
					},
					success: function(Data) {
						
						if (Data['status'] == true) {
							$('#forgot_email').val( '' );
							window.location.reload();
						} else {
							
							$('#forgot_email').val( '' );
							window.location.reload();
						}
					}
				});
				return false;
			} else {
				$('#forgot_password').validate();
				$('#forgot_email_serror').html( $('#forgot_email').attr( 'placeholder' ) );
				$('#forgot_email').addClass( 'error_msg' );				
				return false;
			}			
		});
		$( '#forgot_email' ).keyup( function(){
			$('#forgot_email_serror').html( '' );
			$('#forgot_email').removeClass( 'error_msg' );
			$('.error_msg').html( '' );
		});
	});
	function isValidEmailAddress(emailAddress) {
		var pattern = new RegExp(/^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);
		return pattern.test(emailAddress);
	};
</script>
