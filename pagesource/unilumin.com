                var code = $("[name='submitCode']").val();
                if (code == "") {
                    layer.msg("Please enter a code", 1, 9);
                    return false;
                }
                var model = {};
                model.Content = content;
                model.ClassId = 154;
                model.SortId = 100;
                model.IsLock = 1;
                model.Ver = "en";
                layer.load();
                Feedback.AddNewSiteFeedback(model, code, function (res) {
                    layer.closeLoad();
                    if (res.value > 0) {
                        layer.msg("Submit Success", 1, 1);
                        var url = window.location.href;
                        setTimeout("window.location.href='" + url + "'", 1000);
                    } else {
                        if (res.vaule == "-2") {
