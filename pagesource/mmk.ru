<script type="text/javascript">



$(document).ready(function() {	
    	
	var $width = parseInt($('.main-width').css('width'));
	var $height = parseInt($('.slider').css('height'));
	if ($width != 1000) {
		$height = ($height * $width)/ 1000 // в пропорции увеличить необходимо и высоту
	}
	$(".slider").css("height", $height + 'px');
	$("#gallery").css("height", $height + 'px');
	//Execute the slideShow
	slideShow();

});

function slideShow() {
   	//Set the opacity of all images to 0
	$('#gallery a').css({opacity: 0.0});
	
	//Get the first image and display it (set it to full opacity)
	$('#gallery a:first').css({opacity: 1.0});
	
	//Set the caption background to semi-transparent
	$('#gallery .caption').css({opacity: 0.7});
	
	document.getElementById('text_baner').innerHTML=$('.content_2 h3:first').html();
	

	//Resize the width of the caption according to the image width
	//$('#gallery .caption').css({width: parseInt($('#gallery a').find('img').css('width'))/3});
	$('#gallery .caption').css({width: parseInt($('.main-width').css('width'))/3});
	$('#gallery .caption_dubl').css({width: parseInt($('.main-width').css('width'))/3});
	$('#gallery .caption_dubl .content_2').css({width: parseInt($('#gallery .caption_dubl').css('width'))-70});
	//Get the caption of the first image from REL attribute and display it
	$('#gallery .content').html($('#gallery a:first').find('img').attr('rel'))
	.animate({opacity: 1}, 400);
	
	//Call the gallery function to run the slideshow, 8000 = change to next image after 8 seconds
	setInterval('gallery()',16000);
	
}

function gallery() {
	
	//if no IMGs have the show class, grab the first image
	var current = ($('#gallery a.show')?  $('#gallery a.show') : $('#gallery a:first'));
	
	var current_h3 = ($('.content_2 h3.show_text')?  $('.content_2 h3.show_text') : $('.content_2 h3:first'));

	//Get next image, if it reached the end of the slideshow, rotate it back to the first image
	var next = ((current.next().length) ? ((current.next().hasClass('caption'))? $('#gallery a:first') :current.next()) : $('#gallery a:first'));	
	
	if (current_h3.next().length==0) {var next_h3=$('.content_2 h3:first');}
    else {var next_h3=current_h3.next();}
	
	document.getElementById('text_baner').innerHTML=next_h3.html();
	
	//Get next image caption
	var caption = next.find('img').attr('rel');	
	
	//Set the fade in effect for the next image, show class has higher z-index
	next.css({opacity: 0.0})
	.addClass('show')
	.animate({opacity: 1.0}, 1000);
	
	next_h3.addClass('show_text');

	//Hide the current image
	current.animate({opacity: 0.0}, 1000)
	.removeClass('show');
	
	current_h3.removeClass('show_text');
	
	//Set the opacity to 0 and height to 1px
	$('#gallery .caption').animate({opacity: 0.0}, { queue:false, duration:0 }).animate({height: '1px'}, { queue:true, duration:300 });	
	
	//222!!!!Set the opacity to 0 and height to 1px
	$('#gallery .caption_dubl').animate({opacity: 0.0}, { queue:false, duration:0 }).animate({height: '1px'}, { queue:true, duration:300 });
	
	//Animate the caption, opacity to 0.7 and heigth to 100px, a slide up effect
	$('#gallery .caption').animate({opacity: 0.7},100 ).animate({height: '340px'},500 );
	
	//222!!!!Animate the caption, opacity to 0.7 and heigth to 100px, a slide up effect
	$('#gallery .caption_dubl').animate({opacity: 1},100 ).animate({height: '340px'},500 );
	
	//Display the content
	$('#gallery .content').html(caption);
	
	
}

</script>
