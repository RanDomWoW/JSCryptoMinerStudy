			$('#_tab_audio,#_tab_image,#_tab_text').click(function () {
				if ($(this).closest('li').hasClass('now')) {
					return false;
				}
				$.cookie('top_view', $(this).attr('id').substring(5));
				return true;
			});
			$('#_switch_classic,#_switch_modern').click(function () {
				if ($(this).closest('div').hasClass('now')) {
					return false;
				}
				$.cookie('top_audio_mode', $(this).attr('id').substring(8));
				return true;
			});
			$('.filtermenu').click(function () {
				var $btn = $(this).find('.btn');
				var $filterlist = $(this).parent().find('.filterlist');
				if ($filterlist.is(':visible')) {
					$btn.css('background-position', '100% 0%');
				} else {
					$btn.css('background-position', 'right -56px');
				}
				$filterlist.slideToggle(100);
			});
			$(document).mouseup(function (e) {
				if (!$(e.target).closest('#_filterbox_character').size()) {
					$('#_filterbox_character .btn').css('background-position', '100% 0%');
					$('#_filterbox_character .filterlist').fadeOut('fast');
				}
				if (!$(e.target).closest('#_filterbox_category').size()) {
					$('#_filterbox_category .btn').css('background-position', '100% 0%');
					$('#_filterbox_category .filterlist').fadeOut('fast');
				}
			});
			$(window).load(function() {
				$('.flexslider').flexslider({
					animation: "slide"
				});
			});
		});
