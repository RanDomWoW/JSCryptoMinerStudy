				<script>				
					$(function(){
						var keyword;
						var resultText = $('#resultTextList');
						
						
						
						//keycode가 백스페이스일때 혹은 keyword가 공백일때 50%쪼개지는것 원복시키기
						$('#srchKeyword').keyup(function(e){
							keyword = $('#srchKeyword').val();
							
							if(keyword == ""){
								//autocomplete 전체 창 hide
								$('#entireWrap').hide();
								$('.autosrch_list_area .txt_list').css({'float':'left', 'width':'100%'});
				                $('.autosrch_list_area .img_list').css({'display':'none', 'width':'50%'});
				                $('.autosrch_list_area .txt').css({'width':'100%', 'font-size':'16px'});
								
							}else{
								//autocomplte 전체 창 show
								resultData(resultText,keyword);
								//data length 0 이라면 처음부터 hide 그리고 초기화 작업 
								//$('#entireWrap').show();
								
							}	
							
							var event = e || window.event; 
							
							if(event.stopPropagation){
								event.stopPropagation();	
							}else{
								event.cancelBubble = true; 
							}
						});
						
						
						
					});
					
					
					
					
					
					function resultData(resultTextList,keyword){
						var html = "";
						
						var url = "http://search.cgntv.net:8090/api/programs/phraseprefix";
						var param =  "query="+encodeURI(keyword)+"&limit=10&sort=recent";
						
						var ie9url = url+"?"+param;
						
						var browserCheck = $('html').attr("id");
						
						if(browserCheck.indexOf("ie") != -1){
							var ieNum = browserCheck.split("ie")[1];
							if(Number(ieNum) <= 9){
								$.getJSON(encodeURI(ie9url)).done(function(data){
									if (!data) {
										return false;
									}
									
									$('.autosrch_list_area .txt_list').css({'float':'left', 'width':'100%'});
					                $('.autosrch_list_area .img_list').css({'display':'none', 'width':'50%'});
					                $('.autosrch_list_area .txt').css({'width':'100%', 'font-size':'16px'});
					                resultProgramDataUi(data,keyword,resultTextList);
								});	
								
								/* $.ajax({
									type : "GET",
									dataType : "JSON",
									data : "query="+encodeURI(keyword)+"&limit=10&sort=recent",
									url : "http://search.cgntv.net:8090/api/programs/phraseprefix",
									done : function(data){
										
										$('#entireWrap').hide();
										$('.autosrch_list_area .txt_list').css({'float':'left', 'width':'100%'});
						                $('.autosrch_list_area .img_list').css({'display':'none', 'width':'50%'});
						                $('.autosrch_list_area .txt').css({'width':'100%', 'font-size':'16px'});
						
						                resultProgramDataUi(data,keyword,resultTextList);
										
										
									}					
								});	 */
								
							}else{
								$.ajax({
									type : "GET",
									dataType : "JSON",
									data : "query="+encodeURI(keyword)+"&limit=10&sort=recent",
									url : "http://search.cgntv.net:8090/api/programs/phraseprefix",
									success : function(data){
										
										$('#entireWrap').hide();
										$('.autosrch_list_area .txt_list').css({'float':'left', 'width':'100%'});
						                $('.autosrch_list_area .img_list').css({'display':'none', 'width':'50%'});
						                $('.autosrch_list_area .txt').css({'width':'100%', 'font-size':'16px'});
						
						                resultProgramDataUi(data,keyword,resultTextList);
										
										
									}					
								});	
								
							}
							
						}else{
							$.ajax({
								type : "GET",
								dataType : "JSON",
								data : "query="+encodeURI(keyword)+"&limit=10&sort=recent",
								url : "http://search.cgntv.net:8090/api/programs/phraseprefix",
								success : function(data){
									
									$('#entireWrap').hide();
									$('.autosrch_list_area .txt_list').css({'float':'left', 'width':'100%'});
					                $('.autosrch_list_area .img_list').css({'display':'none', 'width':'50%'});
					                $('.autosrch_list_area .txt').css({'width':'100%', 'font-size':'16px'});
					
					                resultProgramDataUi(data,keyword,resultTextList);
									
									
								}					
							});	
						}
						
					}
					
					function resultContentsData(id){
						var html = "";
						
						var url =  "http://search.cgntv.net:8090/api/contents/"+id;
						var param =  "offset=0&limit=4&sort=recent";
						
						var ie9url = url+"?"+param;
						var browserCheck = $('html').attr("id");
						
						if(browserCheck.indexOf("ie") != -1){
							var ieNum = browserCheck.split("ie")[1];
							if(Number(ieNum) <= 9){
								$.getJSON(encodeURI(ie9url)).done(function(data){
									if (!data) {
										return false;
									}
									
									resultContentDataUi(data);
								});	 
								/* $.ajax({
									type : "GET",
									dataType : "JSON",
									data : "offset=0&limit=4&sort=recent",
									url : "http://search.cgntv.net:8090/api/contents/"+id,
									crossDomain: true,
									done : function(data){
										resultContentDataUi(data);
									}					
								});	 */
								
							}else{
								$.ajax({
									type : "GET",
									dataType : "JSON",
									data : "offset=0&limit=4&sort=recent",
									url : "http://search.cgntv.net:8090/api/contents/"+id,
									
									success : function(data){
										resultContentDataUi(data);
									}					
								});	
								
							}
							
						}else{
							$.ajax({
								type : "GET",
								dataType : "JSON",
								data : "offset=0&limit=4&sort=recent",
								url : "http://search.cgntv.net:8090/api/contents/"+id,
								
								success : function(data){
									resultContentDataUi(data);
								}					
							});	
						}
						
					
					}
					
					
					function resultProgramDataUi(data,keyword,resultTextList){
						
						
						if(data.length != 0){
							var html ="";
							
							html += "<ul id = 'resultTextUl'>";
							
							for(var index = 0; index < data.length; index++){
								html += "<li id = '"+data[index].pgm_id+"' >";
								
								html += "	<a  href='/tv/"+data[index].pcg_id+"/"+data[index].pgm_id+".cgn'>";
								html += "		<div class='ico'><div class='circle'><img id='p_info_thumbnail' src='http://"+data[index].pgd_bnn+"' alt='"+data[index].pgm_snm+"'></div></div>";
								html += "		<div class='txt'>"+data[index].pgm_snm.toLowerCase().replace("<" , "&#60;").replace(keyword.toLowerCase(),"<strong>"+keyword.toLowerCase()+"</strong>")+"</div>";	
								html += "	</a>";
								
								html += "</li>";
								
								
								
							}
							
							html += "</ul>";
							html += "<a id = 'moreSearchClick' href='/search/result.cgn?q="+encodeURI(keyword)+"&m=p' class='btn_more'>프로그램 검색결과 더보기</a>"
							resultTextList.html(html);
							
							$('#resultTextUl > li').on("mouseover",function(e){
								var id = $(this).attr("id");
								
								$('.autosrch_list_area .txt_list').css({'float':'left', 'width':'50%'});
								$('.autosrch_list_area .txt_list ul').css('border-right','1px solid #d7d7d7');
								$('.autosrch_list_area .txt_list .btn_more').css('width','50%');
			                    $('.autosrch_list_area .img_list').css({'display':'block', 'width':'50%'});
			                    $('.autosrch_list_area .img_list .btn_more').css('width','250px');
			                    $('.autosrch_list_area .txt').css({'width':'82%', 'font-size':'16px'});
								
								resultContentsData(id);
								
								var event = e || window.event; 
								
								if(event.stopPropagation){
									event.stopPropagation();	
								}else{
									event.cancelBubble = true; 
								}
								
							});
							
							
							$('#entireWrap').show();
						}else{
							$('#entireWrap').hide();
							$('.autosrch_list_area .txt_list').css({'float':'left', 'width':'100%'});
			                $('.autosrch_list_area .img_list').css({'display':'none', 'width':'50%'});
			                $('.autosrch_list_area .txt').css({'width':'100%', 'font-size':'16px'});
						}
						
					}
					
					function resultContentDataUi(data){
						var html = "";
						var tr1Html = "";
						var tr2Html = "";
						for(var index = 0; index < 4; index++){
							//링크 정보 입력 
							
							
							if(data[index] != null){
								$('#pgmListUrl').attr("href", "/tv/"+data[index].pcg_id+"/"+data[index].pgm_id+".cgn");
								html += "	<td>";
								html += "		<a href='/player/home.cgn?vid="+data[index].pgc_id+"'>";
								html += "			<div class='img'>";
								html += "				<img src='http://"+data[index].pct_hth+"'  alt='"+data[index].pgc_tit+"'>";
								html += "			</div>";
								html += "			<div class ='tit'>'"+data[index].pgc_tit+"'</div>";
								html += "		</a>";
								html += "	</td>";
							}else{
								html +=  "<td>&nbsp</td>";
							}
							
							if(index >= 0 && index <= 1){
								tr1Html += html;
							}else if(index >= 2 && index <= 3){
								tr2Html += html;	
							}
							
							html = "";
							
						} 
						
						$("#tr01").html(tr1Html);
						$("#tr02").html(tr2Html);
						
					}
					
					/* 자동완성리스트 키보드 제어 */
					var index = -1;
					
					$('#srchKeyword').live('keydown', function(e){
						e.stopPropagation();
						var code = (e.keyCode || e.which);
						var resultTxtListLength = $('#resultTextUl li').length;
						var srchKeywordVal = $('#srchKeyword').val();
						
						if(srchKeywordVal == ""){
							if(code == 9) {
								$('.srch_box input[type="image"]').focus();
							}
						}
						
						if(code == 40 || code == 9) {
							e.preventDefault();
							
							index=0;
							inputKeydown(0);
						}
					});
					
					$('#resultTextUl li').live('keydown', function(e){
						e.stopPropagation();
						var code = (e.keyCode || e.which);
						var resultTxtListLength = $('#resultTextUl li').length;
						

						if(code == 40 || code == 9) {
							
							if(e.shiftKey){
								--index;
								inputKeydown(index+1);
								
								if(index < 0){
									index=resultTxtListLength-1;
									inputKeydown(resultTxtListLength-1);
								}
								
								return;
							}
							
							e.preventDefault();
							++index;
							inputKeydown(index);
							
							if(index >= resultTxtListLength){
								if(code == 40){
									setTimeout(function(event){
										$("#srchKeyword").focus();
										$('#entireWrap.autosrch_list_area').hide();
									},200);
								}else{
									if(index >= resultTxtListLength){
										$('#moreSearchClick').focus();
									}
								}
								
							}
						}else if(code == 38 ) {
							--index;
							inputKeydown(index);
							
							if(index < 0){
								index=resultTxtListLength-1;
								inputKeydown(resultTxtListLength-1);
							}
						}
						
						var event = e || window.event; 
						
						if(event.stopPropagation){
							event.stopPropagation();	
						}else{
							event.cancelBubble = true; 
						}
					});

					function inputKeydown(index){
						
						$('#resultTextUl li').eq(index).find('a').focus();
						
						var id = $('#resultTextUl li').eq(index).attr("id");
						
						$('.autosrch_list_area .txt_list').css({'float':'left', 'width':'50%'});
						$('.autosrch_list_area .txt_list ul').css('border-right','1px solid #d7d7d7');
						$('.autosrch_list_area .txt_list .btn_more').css('width','50%');
						$('.autosrch_list_area .img_list').css({'display':'block', 'width':'50%'});
						$('.autosrch_list_area .img_list .btn_more').css('width','250px');
						$('.autosrch_list_area .txt').css({'width':'82%', 'font-size':'16px'});
						
						resultContentsData(id);
				    }
					
					$('#pgmListUrl').blur(function(){
						$('#entireWrap').hide();
					});
					
					//자동완성리스트 닫기 (리스트 내에 닫기 기능 X, body 영역 클릭시 닫히도록) : 2018-01-08
					$('body').click(function(){
						$('#entireWrap').hide();		
					});
