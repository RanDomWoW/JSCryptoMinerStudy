<script type="text/javascript">
$(document).ready(function() {
    cookiedata = document.cookie;    
         
    if ( cookiedata.indexOf("top_banner=done") < 0 ){  
    	
    	$( "#topbanner" ).slideDown( "slow");
             
          } else {
          	
          	$("#topbanner").css("display","none");
          }
	
	$("#today_no_open").click(function(){
		var checkAgree = $("input:checkbox[id='chk_box']").is(":checked") == true;
		
		if(checkAgree==true){
			 notice_setCookie( "top_banner", "done" , 1);
		}
		$( "#topbanner" ).slideUp( "slow");
	});
	
	$("#topbanner_close").click(function(){
		$( "#topbanner" ).slideUp( "slow");
		return false;
	});

});

function notice_setCookie( name, value, expiredays ){ //Cookie 값 확인 function
  var todayDate = new Date();
    todayDate.setDate( todayDate.getDate() + expiredays );
    document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";"
}
 
		
</script> 
 
