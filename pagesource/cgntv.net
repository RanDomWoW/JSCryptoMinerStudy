	function getCookie_popup( name ){
		var nameOfCookie = name + "=";
  		var x = 0;
  		while ( x <= document.cookie.length ) {
  			var y = (x+nameOfCookie.length);
  				if ( document.cookie.substring( x, y ) == nameOfCookie ) {
  				if ( (endOfCookie=document.cookie.indexOf( ";", y )) == -1 )
  					endOfCookie = document.cookie.length;
  				return unescape( document.cookie.substring( y, endOfCookie ) );
  			}
  				
  			x = document.cookie.indexOf( " ", x ) + 1;
  			
  			if ( x == 0 )
  			break;
	  	}
	}
	
	function popupCall(){
		
		$.ajax({
			type :'GET',
			asyn : true,
			url: "/popup/searchPopup.cgn",
			dataType:"json",
			data:{categoryId:"1"},//category : CGN-TV
			success:function(action){
				if (action != null){
					for (var i=0;i<action.length;i++){
						var id = action[i].id;
						var width = action[i].width;
						var height = action[i].height;
						
						/* default Position */
						var leftPosition = 0;
						var topPosition = 20;
						
						if(i == 0){
		                /* Center Position */
		               		leftPosition=(screen.width)?(screen.width-width)/2:100;
		                	topPosition=(screen.height)?(screen.height-height)/2:100;
						}else{
						/* Random Position*/
							leftPosition=(screen.width)?Math.floor(Math.random()*((screen.width-width)-75)):100;
		                	topPosition=(screen.height)?Math.floor(Math.random()*((screen.height-height)-75)):100;
						}
						
						if(getCookie_popup("cookie_popup_"+id) != "done"){
							
							var wnd = window.open("/popup/view.cgn?id="+id,"cookie_popup_"+id,
									"width="+width+",height="+height+",left="+leftPosition+",top="+topPosition+",location=no,toolbar=no,status=no,directories=no,scrollbars=no,resizable=no,menubar=no");
							if(wnd != null) {
								wnd.focus();
							}
							
						}
					}
				}
			},
			error:function(){
				//alert("팝업 호출에 실패 하였습니다\n관리자에게 문의하세요.");
			}
		});
		
	}
