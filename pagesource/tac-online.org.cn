            <script>
                function marquee(s){
                    var time=s.time?s.time:80;
                    var context=s.context?$(s.context):null;
                    var minValue=s.minValue?s.minValue:0;
                    var scrollHeight=s.scrollHeight?s.scrollHeight:0;
                    var height=s.height?s.height:0;
                    context.each(function(index){
                        var con=$(this);
                        con.attr("data_tag","1");
                        //init
                        var scrollH = scrollHeight==0?con.get(0).scrollHeight:scrollHeight;
                        var heightH=height==0?con.height():height;
                        if(scrollH<minValue){//没有达到最小高度
                            return;
                        }
                        con.css('overflow','hidden');
                        con.css('height',height);
                        con.html(con.html()+con.clone().html());
                        var scrollH = scrollHeight==0?con.get(0).scrollHeight:scrollHeight*2;
                        var maxHeight=scrollH-height;
                        if(null!=con){
                            scrollSelf(con,maxHeight,time);
                        }
                    });
                    function scrollSelf(con,maxHeight){
                        if(con.scrollTop()<maxHeight){
                            con.scrollTop(con.scrollTop()+1);
                        }else{
                            con.scrollTop(0);
                        }
                        if($(con).attr("data_tag")=="1"){
                            setTimeout(function(){scrollSelf(con,maxHeight);},time);
                        }
                    }
                    context.on("mouseenter",function(){$(this).attr("data_tag","0");});
                    context.on("mouseleave",function(){$(this).attr("data_tag","1");
                        var scrollH = scrollHeight==0?$(this).get(0).scrollHeight:scrollHeight*2;
                        scrollSelf($(this),scrollH-$(this).height(),time);
                    });
                }

                function initMarquee(){
                    marquee({
                        'context':$(".marquee"),
                        'minValue':250,
                        'scrollHeight':420,
                        'height':280
                    });
                }
                $(function(){
                    var $this = $(".scrollNews");
                    var scrollTimer;
                    $this.hover(function(){
                        clearInterval(scrollTimer);
                    },function(){
                        scrollTimer = setInterval(function(){
                            scrollNews( $this );
                        }, 3000 );
                    }).trigger("mouseleave");
                    initMarquee();

                });
                function scrollNews(obj){
                    var $self = obj.find("ul:first");
                    var lineHeight = $self.find("li:first").height(); //获取行高
                    $self.animate({ "marginTop" : -lineHeight-8+"px" }, 800 , function(){
                        $self.css({marginTop:0}).find("li:first").appendTo($self); //appendTo能直接移动元素
                    })
                }
            </script>
