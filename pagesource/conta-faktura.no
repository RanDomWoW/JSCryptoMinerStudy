	                                        else{
	                                            $(list).append('<ul class="children" ></ul>');
	                                            $(list).find('.children').append(data);
	                                        }
	                                    }
	                                }
	                            } else {
	                                $("#commentsbox").prepend('<ol class="commentlist"> </ol>');

	                                $('ol.commentlist').html(data);
	                                $('.comment-wrap h3.hidden-div').removeClass('hidden-div');
	                            }
	                            commentform[0].reset();
	                        }
		                } else {
		                    statusdiv.html('<p class="alert alert-info" >Du må vente litt før du sender inn på nytt.</p>');
		                    commentform.find('textarea[name=comment]').val('');
		                }
		            }
		        });
		        return false;
		    });
		});
	</script>

	<script type="text/javascript">
	    $(document).ready(function () { $('.single .single-cnt p.intro:first').remove(); });
	</script>

	</body>
