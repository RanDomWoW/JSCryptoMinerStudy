	<script type="text/javascript">
		$(document).ready(function () {
			// 스크롤에 따른 상단 Ui 처리
			$(window).scroll(function(){
				var scrTop = $(window).scrollTop();
				// 해더 변경
					if(scrTop>100){
						$('.scroll-ui').addClass('scroll');
					}else{
						$('.scroll-ui').removeClass('scroll');
					}
				}
			);
		});

		function syncMain(obj) {

			var mainFrm = document.searchFrm;
			var query = mainFrm.query;
			var sel_searchfield = mainFrm.sel_searchfield;
			var research = mainFrm.research;

			var query_s = document.getElementById("query_s");
			var sel_searchfield_s = document.getElementById("sel_searchfield_s");
			var research_s = document.getElementById("research_s")
			
			query.value = query_s.value; // 검색어 sync
			sel_searchfield.options[sel_searchfield_s.selectedIndex].selected = true; // 검색범위 sync
			if (typeof(research) != "undefined" && typeof(research_s) != "undefined")
			{
				research.checked = research_s.checked; // 결과 내 재검색 sync
				if (obj == research_s)
				{
					setReSearch(research);
					query_s.value = "";
				}
			}
			return;
		}
	</script>
