<script type="text/javascript">

  //今日报纸 AJAX
  var flag = true;
  var AjaxStart = 1500 //AJAX 触发距离
  $(window).scroll( function() { 
    //神奇的IE6 神奇的补丁
    if ($.browser.version == "6.0") {
      scrollTop =  document.documentElement.scrollTop;
    }else {
      scrollTop = $(document).scrollTop()
    }
    //判断滚动距离 并显示今日报纸
    if (scrollTop > AjaxStart && flag == true) {
      flag = false;
      $.ajax({
        type: "GET",
        url: "http://www.nbd.com.cn" + "/newspapers/today.json",
        success: function(msg){
            console.log(msg.data)
            newspaper_num = Math.ceil(msg.data.sections.length/4);
            sections_num = msg.data.sections.length;
            for (var i = 0; i < newspaper_num; i++) {
              $("#newspaper_items").append("<li></li>")
            };
            $("#newspaper_items li").eq(0).addClass("on");
             var bt = baidu.template;
             var html=bt('hell',msg.data)
             document.getElementById('result').innerHTML=html;
          }
      });
    };
  });
