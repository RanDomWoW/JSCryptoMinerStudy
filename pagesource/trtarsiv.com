<script language="javascript" type="text/javascript">

        function GlobalSearch() {
            var text = $("#globalSearchTxt").val();
            GlobalNavigateTo(text);
        }

        function GlobalNavigateTo(name) {
            window.location.href = window.location.href = "/ara?bul=" + name;
        }


        $(function () {
            $("#globalSearchTxt").autocomplete({
                source: function (request, response) {
                    $.ajax({
                        url: '/api/ArsivApi/AutoComplete',
                        type: "POST",
                        dataType: "json",
                        data: { "": request.term },
                        success: function (event, ui) {
                            response(event.suggestions, ui);
                        },
                    });
                },
                focus: function (event, ui) {
                    $("#globalSearchTxt").val(ui.item.Name);
                    return false;
                },
                minLength: 2,
                select: function (event, ui) {
                    $("#globalSearchTxt").val(ui.item.Name);


                    window.location.href = "/izle/" + ui.item.Id + "/" + ui.item.EncodedName;

                },
            }).data('ui-autocomplete')._renderItem = function (ul, item) {
                if (!ul.hasClass("col-sm-4 col-xs-8")) {
                    ul.addClass("col-sm-4 col-xs-8");
                }

                var text = item.Name + '</br>';
                text = text.replace(new RegExp("(?![^&;]+;)(" + $.ui.autocomplete.escapeRegex(this.term) + ")(?![^&;]+;)", 'gi'), "<strong>$1</strong>");
                return $("<li>")
                        .append("<a href='/izle/" + item.Id + "/" + item.EncodedName + "'>" + text + "</a>")
                        .appendTo(ul);
            };
        });


    </script>
