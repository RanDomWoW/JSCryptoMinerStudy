<script>
        $('.swiper-container').hover(function () {
            $(this).children('.swiper-button-prev').css("display", "block"); $(this).children('.swiper-button-next').css("display", "block");
        }, function () {
            $(this).children('.swiper-button-prev').css("display", "none"); $(this).children('.swiper-button-next').css("display", "none");
