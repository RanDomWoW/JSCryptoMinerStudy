        <script type="text/html" id="recommendations-home-template">
    <![CDATA[
        
        {{ if (4 <= SC.page.products.length) { }}
            
            <div id="recommendations-home" class="recommendations" data-tracking-id="personal-recommendations">
                <div class="recommendations__title recommendations__title--home text-center" data-tracking-slides-title="true" data-tracking-id="hpLandscapeTitle">Unsere Empfehlungen</div>

                <div id="recommendations-home-slider" class="slides__container recommendations__slides-horizontal js_slider">
                    <button class="slides__button slides__button--previous js_prev is-hidden" type="button" data-tracking-id="hpArrow">zurück</button>
                    <button class="slides__button slides__button--next js_next is-hidden" type="button" data-tracking-id="hpArrow">weiter</button>

                    <div class="js_frame">
                        <ul class="js_slides">
                            
                            {{ var i; }}
                            {{ var recommendedProduct; }}
                            {{ var showMsrp; }}
                            {{ var isLesaraProductClass; }}
                            {{ var productImageUrl; }}

                            {{ for (i = 0; i < SC.page.products.length; i++) { }}
                                {{ recommendedProduct = SC.page.products[i]; }}
                                

                                {{ if ('false' === recommendedProduct.available) { }}
                                    {{ continue; }}
                                {{ } }}

                                
                                {{ showMsrp = "1" === recommendedProduct.c_show_msrp; }}
                                {{ productImageUrl = recommendedProduct.c_img_rel_path ? window.Lesara.Config.cdnBaseUrls.product + recommendedProduct.c_img_rel_path + '?w=243&auto=format' : recommendedProduct.image; }}
                                

                                {{ isLesaraProductClass = (recommendedProduct.hasOwnProperty('c_producer_value') && 'Lesara' === recommendedProduct.c_producer_value) ? ' lesara-brand' : ''; }}

                                <li class="js_slide">
                                    <a
                                        class="product product--is-simple"
                                        href="{{= recommendedProduct.link }}"
                                        title="{{= recommendedProduct.title }}"
                                        data-scarabitem="{{= recommendedProduct.id}}"
                                        data-tracking-id="hpProductImage"
                                    >
                                        <div class="product__overflow-container">
                                            <div class="product__image-container">
                                                <img
                                                    class="product__image"
                                                    src="{{= productImageUrl }}"
                                                    width="243"
                                                    height="243"
                                                    alt="{{= recommendedProduct.title }}"
                                                />
                                                {{ if (showMsrp) { }}
                                                    <div class="product__discount">-{{= recommendedProduct.c_discount_de }}%</div>
                                                {{ } }}
                                            </div>

                                            <div class="product__footer">
                                                <div class="product__title">{{= recommendedProduct.title }}</div>

                                                <div class="row collapse">
                                                    {{ if (showMsrp) { }}
                                                        <div class="column text-right">
                                                            <div class="product__msrp{{= isLesaraProductClass }}" data-unformatted-price="{{=recommendedProduct.msrp}}">{{= recommendedProduct.msrp }}</div>
                                                        </div>
                                                    {{ } }}

                                                    <div class="column{{ if (!showMsrp) { }} text-center{{ } }}">
                                                        <div class="product__price" data-unformatted-price="{{=recommendedProduct.price}}">{{= recommendedProduct.price }}</div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </a>
                                </li>
                            
                            {{ } }}
                        </ul>
                    </div>
                </div>
            </div>
        {{ } else { }}}
            {{ var tempContainer = document.getElementById('recommendations-home-container'); }}
            {{ if (tempContainer !== null) { }}
                {{ tempContainer.parentElement.removeChild(tempContainer); }}
            {{ } }}
        {{ } }}
        
    ]]>
</script>

