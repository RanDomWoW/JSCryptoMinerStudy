            return false;
        }
        if(mobilevalue!="" && !mobilereg.test(mobilevalue) ){
            $("#showerrormsg").html("* 手机号码格式不正确!");
            return false;
        }
        if(phonevalue!="" && !phonereg.test(phonevalue) ){
            $("#showerrormsg").html("* 固定电话格式不正确!");
            return false;
        }
        if(phonevalue=="" && mobilevalue==""){
            $("#showerrormsg").html("* 手机号码、固定电话至少选填一项!");
            return false;
        }
        if(subjectvalue==""){
            $("#showerrormsg").html("* 主题是必填项!");
            return false;
        }
        if(contentvalue==""){
            $("#showerrormsg").html("* 内容是必填项!");
            return false;
        }
        if(inputCodevalue==""){
            $("#showerrormsg").html("* 验证码为必填项!");
            return false;
        }		
        return true;
}
