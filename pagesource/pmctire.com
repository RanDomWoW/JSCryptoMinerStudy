    var rimCenteringValues = jQuery('.dimensions-container input[type=radio]:checked').attr('data-wheel-centering-rims-values');
    var magCenteringID = jQuery('.dimensions-container input[type=radio]:checked').attr('data-wheel-centering-mags');
    var magCenteringValues = jQuery('.dimensions-container input[type=radio]:checked').attr('data-wheel-centering-mags-values');
    var wheelNuts = jQuery('.dimensions-container input[type=radio]:checked').attr('data-wheel-nuts');
    var wheelNutsValue = jQuery('.dimensions-container input[type=radio]:checked').attr('data-wheel-nuts-value');
    var wheelBoltCircles = jQuery('.dimensions-container input[type=radio]:checked').attr('data-wheel-bolt-circles');
    var wheelBoltCircleValues = jQuery('.dimensions-container input[type=radio]:checked').attr('data-wheel-bolt-circle-values');
