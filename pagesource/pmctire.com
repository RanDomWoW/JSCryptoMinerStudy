<script type="text/javascript">
//<![CDATA[

var stepsWizardFogentoForm = new VarienForm('stepsWizardFogentoForm', true);

jQuery(document).ready(function($) {
    /* Steps wizard steps binding */
    //jQuery time
    var current_fs, next_fs, previous_fs; //fieldsets
    var left, opacity, scale; //fieldset properties which we will animate
    var animating; //flag to prevent quick multi-click glitches

    $('.next-step').click(function(){

        if(stepsWizardFogentoForm.validator && stepsWizardFogentoForm.validator.validate())    {

            if(animating) return false;
            animating = true;

            current_fs = $(this).parents('fieldset');
            next_fs = $(this).parents('fieldset').next();

            //activate next step on progressbar using the index of next_fs
            $('.steps-wizard-fogento-progressbar li').eq($('fieldset').index(next_fs)).addClass('active');

            // Change the text of the "vehicle-selection" dropdown
            var year  = jQuery('.steps-wizard-fogento-my-vehicle select#year').val();
            var makeText  = jQuery('.steps-wizard-fogento-my-vehicle select#make option:selected').text();
            var modelText = jQuery('.steps-wizard-fogento-my-vehicle select#model option:selected').text();
            var trimText  = jQuery('.steps-wizard-fogento-my-vehicle select#trim option:selected').text();
            $('#vehicle-selection')
                    .empty()
                    .append('<option value="0">Change my vehicule selection</option>')
                    .append('<option value="" selected="selected">' + makeText + ' ' + modelText + ' ' + year + ' ' + trimText + '</option>');

            //set current fieldset position to absolute
            current_fs.css({'position': 'absolute'});
            //show the next fieldset
            next_fs.show();
            //hide the current fieldset with style
            current_fs.animate({opacity: 0}, {
                step: function(now, mx) {
                    //as the opacity of current_fs reduces to 0 - stored in "now"
                    //1. scale current_fs down to 80%
                    scale = 1 - (1 - now) * 0.2;
                    //2. bring next_fs from the right(50%)
                    left = (now * 50)+"%";
                    //3. increase opacity of next_fs to 1 as it moves in
                    opacity = 1 - now;
                    current_fs.css({'transform': 'scale('+scale+')'});
                    next_fs.css({'left': left, 'opacity': opacity});
                },
                duration: 800,
                complete: function(){
                    //set current fieldset position to relative
                    current_fs.css({'position': 'relative'});
                    current_fs.hide();
                    animating = false;
                },
                //this comes from the custom easing plugin
                easing: 'easeInOutBack'
            });
        }

    });

    $('.previous-step').click(function(){

        if(animating) return false;
        animating = true;

        current_fs = $(this).parents('fieldset');
        previous_fs = $(this).parents('fieldset').prev();

        //de-activate current step on progressbar
        $('.steps-wizard-fogento-progressbar li').eq($('fieldset').index(current_fs)).removeClass('active');

        //set previous fieldset position to absolute
        previous_fs.css({'position': 'absolute'});
        //show the previous fieldset
        previous_fs.show();
        //hide the current fieldset with style
        current_fs.animate({opacity: 0}, {
            step: function(now, mx) {
                //as the opacity of current_fs reduces to 0 - stored in "now"
                //1. scale previous_fs from 80% to 100%
                scale = 0.8 + (1 - now) * 0.2;
                //2. take current_fs to the right(50%) - from 0%
                left = ((1-now) * 50)+"%";
                //3. increase opacity of previous_fs to 1 as it moves in
                opacity = 1 - now;
                current_fs.css({'left': left});
                previous_fs.css({'transform': 'scale('+scale+')', 'opacity': opacity});
            },
            duration: 800,
            complete: function(){
                //set previous fieldset position to relative
                previous_fs.css({'position': 'relative'});
                current_fs.hide();
                animating = false;
            },
            //this comes from the custom easing plugin
            easing: 'easeInOutBack'
        });
    });

    $('.submit').click(function(){
        return false;
    });
    /* END -- Steps wizard steps binding */

    /* Bind click on first item of the "Change vehicle selection" dropdown */
    $('#vehicle-selection').on('change', function(){
        if( $(this).val() == 0 )
            $('.previous-step').trigger('click');
    });
    /* END -- Bind click on first item of the "Change vehicle selection" dropdown */

    /* Check if we need to show the steps wizard (show only in the Home page and in the product list pages) */
    if($('body').hasClass('cms-index-index')) {
        // Show the steps wizard
        $('.steps-wizard-fogento-container').show();
        // Set the hover state on the "My Vehicle" link in the top links (header)
        $('.top-line .top-link-steps-wizard-postcode, .header-container .header-steps-wizard-postcode').addClass('link-active');
    }
    else
    {
        // Else, remove the hover state on the "My Vehicle" link in the top links (header)
        $('.top-line .top-link-steps-wizard-postcode, .header-container .header-steps-wizard-postcode').removeClass('link-active');
    }
    /* END -- Check if we need to hide the steps wizard */

    /* Bind click on the "Modify my vehicle" from the "My Vehicle Resume" and from "Header" */
    $('.modify-my-vehicle').live('click', function(){
        var isVisible = $('.steps-wizard-fogento-container').is(':visible');

        if(!isVisible)  {
            $('.top-line .top-link-steps-wizard-postcode, .header-container .header-steps-wizard-postcode').addClass('link-active');
        }

        $('.steps-wizard-fogento-container').slideDown(300, function(){
            $(this).css({'overflow': 'visible'});
        });

        // Hide the megamenu dropdown (open state)
        $('.mega-menu .dropdown').css('clip', 'rect(1px, 1px, 1px, 1px)');
    });
    /* END -- Bind click on the "Modify" button from the "My Vehicle Resume" */

    /* Bind click on the steps wizard close button */
    $('.btn-close-steps-wizard').on('click', function() {
        ga('send', 'event', 'vehicleSelector', 'exit_closeBtn', 'Sprint');

        $('.steps-wizard-fogento-container').slideToggle(300);

        // Show "my-vehicle-tooltip" div and reset his animation (disappeared after 5 seconds)
        $('.my-vehicle-tooltip').css({'display': 'inline-block', 'position': 'absolute'});
        $('#my_vehicle_tooltip').removeClass('my-vehicle-tooltip').animate({'nothing': null}, 1, function () {
            $(this).addClass('my-vehicle-tooltip');
        });

        // Remove the hover state on the "My Vehicle" link in the top links (header)
        $('.top-line .top-link-steps-wizard-postcode, .header-container .header-steps-wizard-postcode').removeClass('link-active');

    });
    /* END -- Bind click on the steps wizard close button */
});

/**
* Save the different cookies
*/
function saveCookies_stepsWizard()
{
    // Date for cookies
    var date = new Date();
    date.setTime(date.getTime() + (1000 * 60 * 60 * 24 * 7));

    // Set variables for cookies
    var postcode = jQuery('.steps-wizard-postcode #postcode-steps_wizard').val();

    var year  = jQuery('.steps-wizard-fogento-my-vehicle select#year').val();

    var make  = jQuery('.steps-wizard-fogento-my-vehicle select#make').val();
    var makeText  = jQuery('.steps-wizard-fogento-my-vehicle select#make option:selected').text();
    var model = jQuery('.steps-wizard-fogento-my-vehicle select#model').val();
    var modelText = jQuery('.steps-wizard-fogento-my-vehicle select#model option:selected').text();
    var trim  = jQuery('.steps-wizard-fogento-my-vehicle select#trim').val();
    var trimText  = jQuery('.steps-wizard-fogento-my-vehicle select#trim option:selected').text();
    var usage = jQuery('.steps-wizard-fogento-my-vehicle input[name="usage"]:checked').val();

    var vehicleVariantInfoId = jQuery('.dimensions-container input[type=radio]:checked').attr('data-variant-info-id');
    var diameter = jQuery('.dimensions-container input[type=radio]:checked').attr('data-wheel-diameter');
    var diameterId = jQuery('.dimensions-container input[type=radio]:checked').attr('data-wheel-diameter-id');
