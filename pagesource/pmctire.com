    var vehicleHub = variant['hubs_for_wheels'][0]['hub'];

    // Save cookies
    if (postcode !== undefined && postcode !== null && postcode !== '') {
        jQuery.cookie("pmc_postcode", postcode, { expires: date, path: '/' });
    }

    jQuery.cookie("pmc_vehicle_year", year, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_make_id", make, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_make", makeText, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_model_id", model, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_model", modelText, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_trim_id", trim, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_trim", trimText, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_hub", vehicleHub, { expires: date, path: '/' });
    jQuery.cookie("pmc_usage", usage, { expires: date, path: '/' });

    jQuery.cookie("pmc_vehicle_variant_id", vehicleVariantInfoId, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_diameter", diameter, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_diameter_id", diameterId, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_centerings_rims", rimCenteringID, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_centerings_rims_values", rimCenteringValues, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_centerings_wheels", magCenteringID, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_centerings_wheels_values", magCenteringValues, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_wheel_nuts", wheelNuts, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_wheel_nuts_value", wheelNutsValue, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_wheel_bolt_circles", wheelBoltCircles, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_wheel_bolt_circle_values", wheelBoltCircleValues, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_wheel_offset", wheelOffset, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_wheel_offset_values", wheelOffsetValues, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_wheel_hardware_type", wheelHardwareType, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_wheel_hardware_size", wheelHardwareSize, { expires: date, path: '/' });
    jQuery.cookie("pmc_vehicle_tiresize_id", tireSizeId, { expires: date, path: '/' });
