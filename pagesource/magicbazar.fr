<script type="text/javascript">

$('.sliderProduct').each(function( index ) {
	$size = 6;
	if($(window).width() <=700){
		$size = 2;
	}
	if($(this).children(".MS-content").children().length > $size){
		$( this ).multislider({
			  // endless scrolling
			  continuous: false,
			  // slide all visible slides, or just one at a time
			  slideAll: true,
			  // autoplay interval
			  // 0 or 'false' prevents auto-sliding
			  interval: false,
			  // duration of slide animation
			  duration: 500,
			  // pause carousel on hover
			  hoverPause: true,
			  // pause above specified screen width
			  pauseAbove: null,
			  // pause below specified screen width
			  pauseBelow: null
		});
	}else{
		$(this).children(".MS-controls").hide();
	}
});

</script>
