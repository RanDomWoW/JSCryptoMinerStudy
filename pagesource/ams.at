$(document).ready(function(){ 

$('.newscontent').each(function(){
var text = $(this).text();
if(text.length > 400) {
text = text.substring(0, 400);
index = text.lastIndexOf(' ');
text = text.substring(0, index);
$(this).html(text+" ...<br />");
}
});
 
$('.news-gallery').hide();
$('.news-gallery.news1').show();
$('.news-gallery.news1').addClass('gallery_active');
$('#galleryMenu').show();
$('#galleryNum li').click(function() {
if( $(this).hasClass('gallery_next') ){
active = $('#galleryNum li.gallery_active').next();
if (active.attr('class') == null || active.attr('class') == 'gallery_next'){
showNews($('li.news1'));
}else{
showNews(active);
}
} else {

showNews($(this));
}
});
});

function showNews(shownews){
$('#galleryNum li').removeClass('gallery_active'); 
news = '.news-gallery.' + shownews.attr('class');
shownews.addClass('gallery_active');
$('.news-gallery').hide();
$(news).show();
}

