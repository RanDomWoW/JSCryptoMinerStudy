                                        <script>
                                        $(function(){
                                            requirejs(["sexpandable", "RangePicker"], function(sexpandable, rangepicker){
                                                var r = rangepicker.create({
                                                    target: $(".finder__price .range-picker"),
                                                    makeList: function (params) {
                                                        var out = [], t, i, factor = 1;

                                                        if ($(".finder [name=type]:checked").val() == 2
                                                            || $(".finder [name=type]:checked").val() == 4) {
                                                            factor = 500;
                                                        }

                                                        for (i = 0; i < 10; i++) {
                                                            t = {};
                                                            t.value = i * 50000 / factor;
                                                            t.text = "&euro; " + t.value.toLocaleString();
                                                            out.push(t);
                                                        }

                                                        return out;
                                                    },
                                                    getString: function(params){
                                                        var from_label = '',
                                                            to_label = '',
                                                            out = '';

                                                        if (params.from.value > 0) {
                                                            from_label = '&euro;';
                                                            if (params.from.value > 1000) {
                                                                from_label += (params.from.value / 1000).toFixed(0).toString() + "k";
                                                            }
                                                            else {
                                                                from_label += params.from.value;
                                                            }
                                                        }
                                                        if (params.to.value > 0) {
                                                            to_label = '&euro;';
                                                            if (params.to.value > 1000) {
                                                                to_label += (params.to.value / 1000).toFixed(0).toString() + "k";
                                                            }
                                                            else {
                                                                to_label += params.to.value;
                                                            }
                                                        }

                                                        if (from_label && to_label) {
                                                            out = from_label + " - " + to_label;
                                                        }
                                                        else if (from_label) {
                                                            out = "от " + from_label;
                                                        }
                                                        else if (to_label) {
                                                            out = "до " + to_label;
                                                        }
                                                        else {
                                                            // nqma izbrano ni6to
                                                        }

                                                        return out;
                                                    }


                                                });

                                                sexpandable.create({
                                                    target: $(".finder__price .expandable"),
                                                    closeOnBlur: true,
                                                    scrollIntoView: false,
                                                    keepLabel: false,
                                                    onExpand: function(pane){
                                                        pane.find(".range-picker__from .js-label").trigger("focus");
                                                    },
                                                    getPreview: function(pane){
                                                        return r.getString();
                                                    }
                                                });
                                            });

                                        });
