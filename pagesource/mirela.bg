            <script>
                (function(){
                    // zarejdaneto e predi expandable init
                    var t;
                    if (t = localStorage.getItem("index-finder")) {
                        t = JSON.parse(t);
                        if (t.hasOwnProperty("ac_s")) {
                            $(".js-form-finder [name=ac_s]").val(t["ac_s"]);
                        }
                        if (t.hasOwnProperty("ac_v")) {
                            $(".js-form-finder [name=ac_v]").val(t["ac_v"]);
                        }
                        if (t.hasOwnProperty("etypes")) {
                            for (x in t.etypes) {
                                $(".js-form-finder [name='etypes[]']").filter("[value=" + t.etypes[x] + "]").prop("checked", true);
                            }
                        }
                        if (t.hasOwnProperty("price_from")) {
                            $(".js-form-finder [name=price_from]").val(t["price_from"]);
                        }
                        if (t.hasOwnProperty("price_to")) {
                            $(".js-form-finder [name=price_to]").val(t["price_to"]);
                        }
                        if (t.hasOwnProperty("type")) {
                            $(".js-form-finder [name=type]")
                                .prop("checked", false)
                                .filter("[value=" + t["type"] + "]")
                                .prop("checked", true);
                        }
                    }

                    $(".js-form-finder").on("submit", function(){
                        var data = {}, self = $(this);
                        data["ac_s"] = self.find("[name=ac_s]").val();
                        data["ac_v"] = self.find("[name=ac_v]").val();
                        data["price_from"] = self.find("[name=price_from]").val();
                        data["price_to"] = self.find("[name=price_to]").val();
                        data["type"] = self.find("[name=type]:checked").val();
                        data["etypes"] = [];
                        self.find("[name='etypes[]']:checked").each(function(){
                            data["etypes"].push($(this).val());
                        });
                        localStorage.setItem("index-finder", JSON.stringify(data));
                    });

                    $(".js-submit-finder").on("click", function(){
