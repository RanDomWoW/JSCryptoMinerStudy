$(function(){
    var attachAllAndNone, applyAllAndNone, visualAllAndNone;

    visualAllAndNone = function(context) {
        context.find("[data-allandnone]").each(function(){
            var self = $(this);
            var target = $(self.data("allandnone"));
            if (target.filter(":checked").size() == 0) {
                self.prop("checked", true);
            }
            else {
                self.prop("checked", false);
            }
        });
    }
    attachAllAndNone = function(context) {
        context.find("[data-allandnone]").each(function(){
            var self = $(this);
            var target = $(self.data("allandnone"));

            if (target.filter(":checked").size() == 0) {
                self.prop("checked", true);
            }

            self.off("click.allandnone").on("click.allandnone", function(e){
                if (self.prop("checked")) {
                    target.prop("checked", false);
                }
                else {
                    e.preventDefault();
                }
            });

            target.off("click.allandnone").on("click.allandnone", function(){
                visualAllAndNone(context);
            });
        });
    };
    applyAllAndNone = function(pane, target) {
        var el = $('<label>' +
                        '<input type="checkbox" value="" />' +
                        ' Всички' +
                    '</label>' +
                    '<br/>');
        el.find("input").attr("data-allandnone", target);
        pane.prepend(el);
        attachAllAndNone(pane);
    }
    applyAllAndNone($(".finder__etype .expandable__list-content"), "[name='etypes[]']");
})
</script>
