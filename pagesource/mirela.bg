        <div class="collapse" id="historyList">
            <script>
            $(function(){
                $(".finder [name=type]").on("change", function(){
                    $(".finder__project").hide();
                    if ($(this).val() == 1) {
                        $(".finder__project").show();
                    }

                    if ([1, 3].indexOf($(this).val()) != -1) {
                        $(".finder__price input[type=text]").prop("list", "dl-price_sell");
                    }
                    else {
                        $(".finder__price input[type=text]").prop("list", "dl-price_rent");
                    }

                });
                requirejs(["History", "handlebars"], function(History, Handlebars){
                    $(".history-offer").collapse({toggle: false});

                    $("#historyList").one("show.bs.collapse", function(){
                        var offerTemplate = Handlebars.compile($(".history-offer .template").html());

                        var offerHistory = new History({
                            key: History.KEY_OFFER,
                            render: function(data){
                                var ids = [];
                                for (x in data) {
                                    ids.push(data[x].id);
                                }
                                ids.reverse();

                                $.getJSON('index.php?p=misc',
                                    {
                                        a: 'getOffer',
                                        o: 'card',
                                        id: ids.join("|")
                                    },
                                    function(data){
                                        $(".history-offer").html(offerTemplate(data));

                                        requirejs(["bookmark"], function(bookmark){
                                            bookmark.process({
                                                toggle: $(".history-offer .bookmark")
                                            });
                                        });
                                    }
                                );
                            }
                        });

                        $(".history-offer").on("click", ".remove", function(e){
                            e.preventDefault();
                            var self = $(this),
                                t;
                            $(".history-offer .remove").remove();
                            t = parseInt(self.data('index'));
                            t = t + 1;
                            t = -t;

                            offerHistory.removeAt(t);
                        });

                        $(".history-offer").on("click", ".l-grid__expand", function(e){
                            e.preventDefault();
                            $(".history-offer .l-grid").toggleClass("is-active");
                        });

                    });


                    var searchTemplate = Handlebars.compile($(".history-search .template").html());

                    var searchHistory = new History({
                        key: History.KEY_SEARCH,
                        render: function(data){
                            var chunked = [];
                            while (data.length) {
                                chunked.push(data.splice(0, 5));
                            }

                            $(".history-search .l-grid").html(searchTemplate(chunked));
                        }
                    });

                    $(".history-search .l-grid").on("click", ".remove", function(e){
                        e.preventDefault();
                        var pane = $(this).closest(".l-grid__item");

                        searchHistory.removeAt(pane.data('index'));
                    })

                    $("#historyList").on("show.bs.collapse hide.bs.collapse", function(e){
                        localStorage.removeItem("search-history-invisible");
                        if (e.type == 'hide') {
                            localStorage.setItem("search-history-invisible", 1);
                        }

                        $(".history-offer").collapse('toggle');
                    });
                    if (localStorage.getItem("search-history-invisible")) {
                    }
                    else {
                        $("#historyList").collapse("show");
                    }
                });

            });
            </script>

            <div class="history-search container-fluid">
                <!-- <div class="h3">История на вашето търсене</div> -->
