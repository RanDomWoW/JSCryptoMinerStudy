        function place(){
            var place = autocomplete.getPlace();
            //como seleccionó pasamos a true
            if(place){
                var col='';
                var mun='';
                var est='';
                var mun_sub = '';
               if(place.types.indexOf('establishment') !== '-1'){
                    col = place.name;
                    for (var x in place.address_components)
                    {

                        if('locality' == place.address_components[x].types[0] && col.length == 0){
                            col = place.address_components[x].long_name;
                        }else if('locality' == place.address_components[x].types[0] && est.length == 0){
                            mun = place.address_components[x].long_name;
                        }else if('administrative_area_level_2' ==  place.address_components[x].types[0]){
                            mun = place.address_components[x].long_name;
                        }else if('administrative_area_level_1' ==  place.address_components[x].types[0]){
                            est = place.address_components[x].long_name;
                        }else if('administrative_area_level_3' ==  place.address_components[x].types[0]){
                                mun_sub = place.address_components[x].long_name;
                            }
                    }
                }else{
                    for (var x in place.address_components)
                    {
                        if( 'neighborhood' ==  place.address_components[x].types[0]){
                            col = place.address_components[x].long_name;
                        }else if('sublocality_level_1' ==  place.address_components[x].types[0] && col.length == 0){
                            col = place.address_components[x].long_name;
                        }else if('locality' == place.address_components[x].types[0] && col.length == 0){
                            col = place.address_components[x].long_name;
                        }else if('locality' == place.address_components[x].types[0] && est.length == 0){
                            mun = place.address_components[x].long_name;
                        }else if('administrative_area_level_2' ==  place.address_components[x].types[0]){
                            mun = place.address_components[x].long_name;
                        }else if('administrative_area_level_1' ==  place.address_components[x].types[0]){
                            est = place.address_components[x].long_name;
                        }else if('administrative_area_level_3' ==  place.address_components[x].types[0]){
                                mun_sub = place.address_components[x].long_name;
                            }
                    }
                }
                if(est == 'Distrito Federal' || est == 'Federal District'){
                    est = 'Df';
                    mun = mun_sub;
                }
                if(est == 'Santiago de Querétaro'){
                    est = 'Querétaro';
                }
                if(mun == 'Santiago de Querétaro'){
                    mun = 'Querétaro';
                }
                if(col == 'Santiago de Querétaro'  || col == 'Culiacán Rosales'){
                    col = '';
                }
                //Checa municipio
                if(col == mun){
                    col='';
                }
                //Envia datos al formulario
                document.getElementById('colonia').value = col;
                document.getElementById('municipio').value = mun;
                document.getElementById('estado').value = est;

                if(place.geometry){
                    document.getElementById('top').value = place.geometry.viewport.getNorthEast().lat();
                    document.getElementById('bott').value = place.geometry.viewport.getSouthWest().lat();
                    document.getElementById('left').value = place.geometry.viewport.getNorthEast().lng();
