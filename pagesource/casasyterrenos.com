        var map;
        function initialize() {
            getPolygon( function () {
                var polygonCoords = this;
                if(polygonCoords.length){
                    var bounds = new google.maps.LatLngBounds();
                    var i;
                    var coord;
                    for (i = 0; i < polygonCoords.length; i++) {
                        if(0 < polygonCoords[i].lat){
                            coord = new google.maps.LatLng(polygonCoords[i].lat, polygonCoords[i].lng);
                        } else {
                            coord = new google.maps.LatLng(polygonCoords[i].lng, polygonCoords[i].lat);
                        }

                        bounds.extend(coord);
                    }
                    var center= bounds.getCenter();

                    var draggable_mode = true;
                    if(window.matchMedia("only screen and (max-width: 760px)")) {
                        draggable_mode = false;
                    }

                    var map = new google.maps.Map(document.getElementById('map-canvas_detalle_desarrollo'), {
                        zoom: 15,
                        center: center,
                        scrollwheel: false,
                        draggable: draggable_mode,
                        mapTypeId: google.maps.MapTypeId.TERRAIN,
                    });

                    var Polygon = new google.maps.Polygon({
                        paths: polygonCoords,
                        geodesic: true,
                        strokeColor: '#FF0000',
                        strokeOpacity: 0.5,
                        strokeWeight: 1,
                        fillColor: '#FF0000',
                        fillOpacity: 0.2
                    });
                    Polygon.setMap(map);
                    getFoursquare(center.lat(),center.lng(), 'http://www.casasyterrenos.com/');
                    getPropiedades(center.lat(),center.lng(), 'http://www.casasyterrenos.com/');
                }else{
                    var map = new google.maps.Map(document.getElementById('map-canvas_detalle_desarrollo'), {
                        zoom: 15,
                        center: {lat: 20.6737919, lng: -103.3354131},
                        scrollwheel: false,
                        mapTypeId: google.maps.MapTypeId.TERRAIN
                    });

                    getFoursquare(20.6737919,-103.3354131,'http://www.casasyterrenos.com/');
                    getPropiedades(20.6737919,-103.3354131,'http://www.casasyterrenos.com/');
                }
            });
        }
        google.maps.event.addDomListener(window, 'load', initialize);
    }

