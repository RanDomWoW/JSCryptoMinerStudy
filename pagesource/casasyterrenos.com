            var autocomplete2 = new google.maps.places.Autocomplete(document.getElementById('buscar_ciudad_small'), { types: ['geocode','establishment'],componentRestrictions: {country : 'mx'} });
            google.maps.event.addListener(autocomplete2, 'place_changed', function(){
                placedos(null,null);
            });

            function placedos(enter, callback){
                var placedos = autocomplete2.getPlace();
                if(placedos){
                    var col='';
                    var mun='';
                    var est='';
                    var mun_sub = '';

                    if(placedos.types.indexOf('establishment') !== '-1'){
                        col = placedos.name;
                        for (var x in placedos.address_components)
                        {

                            if('locality' == placedos.address_components[x].types[0] && col.length == 0){
                                col = placedos.address_components[x].long_name;
                            }else if('locality' == placedos.address_components[x].types[0] && est.length == 0){
                                mun = placedos.address_components[x].long_name;
                            }else if('administrative_area_level_2' ==  placedos.address_components[x].types[0]){
                                mun = placedos.address_components[x].long_name;
                            }else if('administrative_area_level_1' ==  placedos.address_components[x].types[0]){
                                est = placedos.address_components[x].long_name;
                            }else if('administrative_area_level_3' ==  placedos.address_components[x].types[0]){
                                    mun_sub = placedos.address_components[x].long_name;
                                }
                        }
                    }else{
                        for (x in placedos.address_components)
                        {
                            if( 'neighborhood' ==  placedos.address_components[x].types[0]){
                                col = placedos.address_components[x].long_name;
                            }else if('sublocality_level_1' ==  placedos.address_components[x].types[0] && col.length == 0){
                                col = placedos.address_components[x].long_name;
                            }else if('locality' == placedos.address_components[x].types[0] && col.length == 0){
                                col = placedos.address_components[x].long_name;
                            }else if('locality' == placedos.address_components[x].types[0] && est.length == 0){
                                mun = placedos.address_components[x].long_name;
                            }else if('administrative_area_level_2' ==  placedos.address_components[x].types[0]){
                                mun = placedos.address_components[x].long_name;
                            }else if('administrative_area_level_1' ==  placedos.address_components[x].types[0]){
                                est = placedos.address_components[x].long_name;
                            }else if('administrative_area_level_3' ==  placedos.address_components[x].types[0]){
                                mun_sub = placedos.address_components[x].long_name;
                            }
                        }
                    }

                    if(est == 'Distrito Federal' || est == 'Federal District'){
                        est = 'Df';
                        mun = mun_sub;
                    }
                    if(est == 'Santiago de Querétaro'){
                        est = 'Querétaro';
                    }
                    if(mun == 'Santiago de Querétaro'){
                        mun = 'Querétaro';
                    }
                    if(col == 'Santiago de Querétaro' || col == 'Culiacán Rosales'){
                        col = '';
                    }
                    //Checa municipio
                    if(col == mun){
                        col='';
                    }
                    //Envia datos al formulario
                    document.getElementById('colonia_small').value = col;
                    document.getElementById('municipio_small').value = mun;
                    document.getElementById('estado_small').value = est;

                    if(placedos.geometry){
                        document.getElementById('top_small').value = placedos.geometry.viewport.getNorthEast().lat();
                        document.getElementById('bott_small').value = placedos.geometry.viewport.getSouthWest().lat();
                        document.getElementById('left_small').value = placedos.geometry.viewport.getNorthEast().lng();
                        document.getElementById('right_small').value = placedos.geometry.viewport.getSouthWest().lng();
                        document.getElementById('locationlat_small').value = placedos.geometry.location.lat();
                        document.getElementById('locationlng_small').value = placedos.geometry.location.lng();
                    }
                    if(enter){
                        callback();
                    }
                }
            }
