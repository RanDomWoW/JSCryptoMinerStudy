function validatePass(campo) {
    var RegExPattern =  /(?!^[a-zA-Z]*$)^([a-zA-Z0-9]{8,10})$/;
    if ((campo.match(RegExPattern)) && (campo.value!='')) {
        console.log('Bien');
        return true;
    } else {
        console.log('Mal');
        return false;
    }
}


