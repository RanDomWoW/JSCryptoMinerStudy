/* CALCULADORA */
function calcularCreditoHipotecario2(nombre_cal, correo_cal,valor_vivienda, tasa_interes, plazo, anticipo1, codigo_propiedad, telefono )
{
    if(valor_vivienda.length > 0 && tasa_interes.length > 0 && anticipo1.length > 0 && correo_cal.length > 0)
    {
        var valor_vivienda  =valor_vivienda.replace(/,/g, "");
        /*var valor_vivienda  =valor_vivienda.replace(",", "");*/
        var valInm          = number_format(valor_vivienda);

        $('#valInm').html('$' + valInm);


        /// PORCENTAJE DEL CREDITO A SOLICITAR

        var anticipo =  (valor_vivienda * (anticipo1 / 100));
        $('#engpag').html('$' + number_format(anticipo));


        var creditoSol = valor_vivienda - anticipo;
        $('#cresol').html('$' + number_format(creditoSol));

        var comision_aper = (creditoSol * 0.02);
        $('#comaper').html('$' + number_format(comision_aper));


        var gastos_notario = (creditoSol * 0.09);
        $('#gasnot').html('$' + number_format(gastos_notario));

        var seguro = (creditoSol * 0.0043);
        $('#segvid').html('$' + number_format(seguro));


        var seguro_danos = (creditoSol * 0.00325);
        $('#segda').html('$' + number_format(seguro_danos));


        var evaluo_gastos = (creditoSol * 0.003125);
        $('#costoava').html('$' + number_format(evaluo_gastos));

        var tasa1 = (tasa_interes / 100);
        var tasa2 = (tasa1 / 12);
        var monthly_factor  = get_interest_factor(plazo,tasa2);
        var monthly_payment = creditoSol / monthly_factor;

        $('#pagmen').html('$' + number_format(monthly_payment));

        var ingresosMensuales = monthly_payment * 3.5;
        $('#ingrebrucom').html('$' + number_format(ingresosMensuales));
        $('#detalleBoton').show();

        var tabla = "<table class=\"table table-bordered table-striped\" >\n";
            tabla  += "\t<tr valign=\"top\" bgcolor=\"#cccccc\">\n";
            tabla  += "\t\t<td align=\"right\"><b>No.</b></td>\n";
            tabla  += "\t\t<td align=\"right\"><b>Interés</b></td>\n";
            tabla  += "\t\t<td align=\"right\"><b>Pago Principal</b></td>\n";
            tabla  += "\t\t<td align=\"right\"><b>Mensualidades</b></td>\n";
            tabla  += "\t\t<td align=\"right\"><b>Saldo</b></td>\n";
            tabla  += "\t</tr>\n";

            principal     = creditoSol;
            current_month = 1;
            month_term = plazo*12;
            power = -(month_term);
            denom =  Math.pow((1 + tasa2), power);
            monthly_paymentN = principal * (tasa2 / (1 - denom));

            while(current_month <= month_term){

                interest_paid     = principal * tasa2;
                principal_paid    = monthly_paymentN - interest_paid;
                remaining_balance = principal - principal_paid;

                tabla  += "\t<tr valign=\"top\" bgcolor=\"#eeeeee\">\n";
                tabla  += "\t\t<td align=\"right\">" + current_month + "</td>\n";
                tabla  +=  "\t\t<td align=\"right\">\$" + number_format(interest_paid) + "</td>\n";
                tabla  +=  "\t\t<td align=\"right\">\$" + number_format(principal_paid) + "</td>\n";
                tabla  +=  "\t\t<td align=\"right\">\$" + number_format(monthly_payment) + "</td>\n";
                tabla  += "\t\t<td align=\"right\">\$" + number_format(remaining_balance) + "</td>\n";
                tabla  +=   "\t</tr>\n";

                principal = remaining_balance;
                current_month++;
            }

            tabla  += "</table>";

        //$('#detalle_tabla').html(tabla);

        //AQUI HAY QUE PONER AJAX

        $.ajax({
                    type        :   'POST',
                    url         :   base_url + 'calculadora_credito/enviarCalculo',
                    data        :   { 'codigo_propiedad' : codigo_propiedad, 'nombre': nombre_cal, 'correo' : correo_cal,'telefono' : telefono, 'valor_vivienda' : valor_vivienda, 'tasa_interes' : tasa_interes, 'plazo' : plazo, 'anticipo1' : anticipo1},
                    dataType    :   'html',
                    success     :   function( response )
                                    {
                                        ga('send','event','CALCULADORA','CALCULO');

                                    },
                    error       :   function()
                                    {
                                        //alert('Actualize su navegador.');
                                    }
                });

        return tabla;
    }

}

function get_interest_factor(year_term, monthly_interest_rate){

      var factor        = 0;
      var base_rate     = 1 + monthly_interest_rate;
        denominator     = base_rate;
        for (i=0; i < (year_term * 12); i++) {
            factor += (1 / denominator);
            denominator *= base_rate;
        }
        return factor;
}


function number_format (number, decimals, dec_point, thousands_sep) {
    number = (number + '').replace(/[^0-9+\-Ee.]/g, '');
    var n = !isFinite(+number) ? 0 : +number,
        prec = !isFinite(+decimals) ? 0 : Math.abs(decimals),        sep = (typeof thousands_sep === 'undefined') ? ',' : thousands_sep,
        dec = (typeof dec_point === 'undefined') ? '.' : dec_point,
        s = '',
        toFixedFix = function (n, prec) {
            var k = Math.pow(10, prec);            return '' + Math.round(n * k) / k;
        };
    // Fix for IE parseFloat(0.55).toFixed(0) = 0;
    s = (prec ? toFixedFix(n, prec) : '' + Math.round(n)).split('.');
    if (s[0].length > 3) {        s[0] = s[0].replace(/\B(?=(?:\d{3})+(?!\d))/g, sep);
    }
    if ((s[1] || '').length < prec) {
        s[1] = s[1] || '';
        s[1] += new Array(prec - s[1].length + 1).join('0');    }
    return s.join(dec);
}

function isNumberKey(evt)
{
    var charCode = (evt.which) ? evt.which : event.keyCode;
    if (charCode > 31 && (  charCode < 48  || charCode > 57)){

        if(charCode == 45 ){
            return true;
        }

        if(charCode == 46 ){
            return true;
        }

        return false;

    }


    return true;


}



