	function joinDivForm(opt){
		$("#join_form1").hide();
		$("#join_form2").hide();
		$("#join_form3").hide();

		$(".join_form .userTxt").val('');
		$('#j_userId').attr('placeholder','이메일 ID를 입력해 주세요.');
		$('#j_userId1').attr('placeholder','휴대폰번호를 입력해주세요.');

		if( opt == 3 ){
			$("#join_form2").hide();
			var p_num = $("#j_userId1").val();
			p_num += " 로 가입인증번호 전송완료";
			$("#join_form3 div:first-child .userTxt").val( p_num );
			$("#join_form3 #phone_auth").val( p_num );
		}else{
			$(".join_form_p .userTxt").val('');
		}

		$("#join_form"+opt).show();
	}

	function is_number(opt){
		v = opt.value;
		var reg = /^(\s|\d)+$/;
		result = reg.test(v);

		if (result == false){
			$('#j_userId1').val('');
			$('#j_userId1').attr('placeholder','숫자만 입력 가능합니다.');
			$('#j_userId1').focus();
			return false;
		}
		return;
	}

