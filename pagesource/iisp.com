	var t_hide_bridge = -1;
	$(function(){
		t_hide_bridge = setInterval('hideNewBridge()', 1000);
	});
	function hideNewBridge() {
		// 只识别靠左居中的
		var b = $('#newBridge .icon-left-center');
		if(b.length) {
			clearInterval(t_hide_bridge);
			t_hide_bridge = -1;
			var w = b.width(), lft = parseInt(b.css('left')), o = $('.group-text:first', b), color = o.length ? o.css('color') : '#888';
			b.append(
				'<div style="height:26px;opacity:.8;position:absolute;width:'+w+'px;top:0px;right:0;">' +
				'<a style="display:block;width:100%;text-decoration:none;height:26px;line-height:26px;text-align:right;' + 
				'color:'+color+';' +
				'"><img src="/share/ico.png"style="margin: 3px 3px 0 0;" /></a></div>');
			$('a', b).click(function(){
				b.animate({left:'-'+w+'px'}, 300, function() {
					if(!$('.show_bridge_btn', b).length) {
						b.append('<div class="show_bridge_btn" style="position:absolute;width:26px;right:-26px;top:0%;">' +
							'<a style="display:block;width:100%;text-decoration:none;width:26px;line-height:18px;text-align:center;' + 
							'color:#fff;font-size:14px;padding:.5em 0;background-color:#ffffff;background-color:rgba(204,0,0,.9);' +
							'">在线咨询</a></div>');
							$('.show_bridge_btn', b).click(function(){
								$(this).hide();
								b.animate({left:lft+'px'}, 300);
							});
					}
					$('.show_bridge_btn', b).show();
				});
			});
		}
	}
