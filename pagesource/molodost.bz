<script type="text/javascript">
    function getBbmHost() {
        // Для совместимости проверяем налиие конфига
        return JsGlobalConfig && JsGlobalConfig.bbm && JsGlobalConfig.bbm.host
            ? JsGlobalConfig.bbm.host
            : 'bbm.molodost.bz';
    }

    function getCookie(name) {
        var matches = document.cookie.match(new RegExp(
            "(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
        ));
        return matches ? decodeURIComponent(matches[1]) : undefined;
    }

    function SuperBMBanner() {
        var auth = getCookie('molodost_user');
        var uid = getCookie('last_user_id');
        var visitor_id = getCookie('visitor_id');
        if (typeof(uid) == 'undefined') {
            var bbm_luid = '';
        } else {
            var bbm_luid = '&u=' + uid;
        }
        if (typeof(auth) == 'undefined') {
            var bbm_u = '';
        } else {
            var bbm_u = '&a=1' + bbm_luid;
        }
        if (typeof(visitor_id) == 'undefined') {
            var bbm_vid = '';
        } else {
            var bbm_vid = '&v=' + visitor_id;
        }
        if (typeof(pr) == 'undefined') {
            var pr = Math.floor(Math.random() * 1000000);
        }

        this.loadBanner = function(id, s, t, p, b, tagIds) {
            var xhttp = new XMLHttpRequest();
            xhttp.onreadystatechange = function () {
                if (this.readyState == 4 && this.status == 200) {
                    document.getElementById(id).innerHTML = this.responseText;
                    var element = document.getElementById(id).getElementsByTagName('a')[0];
                    data = element.dataset;
                    if (data.id != '0') {
                        dataLayer.push({
                            "event": "promoView",
                            "ecommerce": {
                                "promoView": {
                                    "promotions": [{
                                        "id": data.id,
                                        "name": data.title,
                                        "position": data.position
                                    }]
                                }
                            }
                        });
                    }
                }
            };

            xhttp.open("GET", '//' + getBbmHost() + '/?s=' + s + '&t=' + t + '&p=' + p + '&b=' + b + '&pr=' + pr + bbm_u + bbm_vid + '&tagIds=' + (tagIds || ''), true);
            xhttp.send();
        }
    }

    window.superBMBanner = new SuperBMBanner();
</script>

