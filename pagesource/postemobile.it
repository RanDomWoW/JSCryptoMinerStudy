<script type="text/javascript">
    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + d.toUTCString();
        document.cookie = cname + "=" + cvalue + "; path=/; " + expires;
    }
    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') c = c.substring(1);
            if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
        }
        return "";
    }
    function showPrivacyCookiesMessage() {
        $('#pm15_corp_footer_container')
	        .after('<div id="pm15_cookie_info"></div>');

        $('#pm15_cookie_info')
	        .append('<div class="maincol"></div>').find('.maincol')
		        .append('<div class="text"></div>').find('.text')
			        .append('PosteMobile usa i cookie per offrirti la migliore esperienza possibile.')
			        .append('<br />')
			        .append('Procedendo con la navigazione sul sito, ci autorizzi a memorizzare e ad accedere ai cookie del tuo dispositivo.')
			        .append('<br />')
			        .append('<a href="/privacy/cookie-policy">Cookie policy</a>')
		        .end()
		        .append('<div class="close-btn"></div>').find('.close-btn')
			        .append('<a href="javascript:;">Chiudi</a>')
		        .end()
	        .end();

        $('#pm15_cookie_info .close-btn a').on('click', function (e) {
            $("#pm15_cookie_info").hide();
        });
    }
    function setPrivacyCookiesMessageOff() {
        var cname = 'PCOOK';
        var cvalue = getCookie(cname);
        var exdays = 5;
        var cflags = '';
        if (cvalue.indexOf('|') != -1) {
            cflags = cvalue.substring(cvalue.indexOf('|'))
        }
        cvalue = 'PCOOK-MESSAGE-OFF' + cflags;
        setCookie(cname, cvalue, 5);
    }
    $(document).ready(function () {
        showPrivacyCookiesMessage();
        setPrivacyCookiesMessageOff();
    });
</script>

  
  
  
