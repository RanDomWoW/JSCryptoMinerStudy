        function onPlayerReady(event) {
            // console.log('onPlayerReady', event.target.getIframe().id);
            $(event.target.getIframe()).animate({ 'opacity': 1 }, 500, 'linear');
        }

        function onPlayerStateChange(event) {
            // console.log('onPlayerStateChange', event, this);

            // console.log('event.target.a.id', event.target.a.id);
            if (event.data == YT.PlayerState.PLAYING) {
                var playerId = event.target.a.id;
                stopPlayers(playerId);

                // console.log('this.node.style.backgroundImage', this.node.style.backgroundImage);
                this.node.style.background = 'none';
            }

            if (event.data == YT.PlayerState.ENDED) {
            }
            // console.log('players', players);
        }

        function stopPlayers(exeptionPlayerId) {
            for (var playerId in players) {
                if (playerId !== exeptionPlayerId) {
                    players[playerId].pauseVideo();
                }
            }
        }



        namespace.YTPlayer = function () {
            function YTPlayer(node) {
                this.node = node;
                this.youtubeId = node.dataset.ytid;

                this.link = node.querySelector('a');

                this.link.addEventListener('click', this, false);

                this.loadImage();
            }
            YTPlayer.prototype = {
                handleEvent: function (event) {
                    switch (event.type) {
                        case 'click': this.load(event); break;
                    }
                },

                loadImage: function () {
                    var thumbId = this.node.dataset.ytthumb || this.youtubeId,
                        imageSrc = 'https://img.youtube.com/vi/' + thumbId + '/maxresdefault.jpg';
                    this.node.style.backgroundImage = 'url("' + imageSrc + '")';
