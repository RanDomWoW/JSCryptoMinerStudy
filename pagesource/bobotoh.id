					$('.member_input').val('');
					$('#member_form').slideToggle(500);$('.down_member').toggle();$('.up_member').toggle();
					$('#member_daftar_bt').remove();
					$('#member_code').html(data);
					$('#myModal').modal('show');
				}else{
					alert("Maaf, member dengan nama dan email yang sama sudah terdaftar.");
				}
			});
		}
		
