<script type="text/javascript">
		$(function(){
		    var sframes =$("iframe") ;
		    sframes.each(function(index,element){//index 序号；element节点
		    	var oFrm = element ;
		    	oFrm.onload = oFrm.onreadystatechange = function() {
		            if (this.readyState && this.readyState != 'complete') 
		           	 	return;
		            else {
		            	var detailSrc = $(this).attr("detailPageSrc");
		            	var answerSrc = $(this).attr("answerPageSrc");
		            	if(detailSrc!=undefined){
			            	window.frames[index].document.getElementById("detailPageSrc").value=detailSrc;
		            	}
		            	if(answerSrc!=undefined){
			            	window.frames[index].document.getElementById("answerPageSrc").value=answerSrc;
		            	}
		            }
		         }
		    });
		});
	</script>
