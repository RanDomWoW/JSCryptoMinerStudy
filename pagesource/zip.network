</script>

<script src="js/isotope.pkgd.min.js"></script>
<script>
    $(function () {
        $(window).load(function () {
            var $container = $('.sorting-container').isotope({
                itemSelector: '.sorting-item',
                masonry: {
                    columnWidth: '.grid-sizer'
                },
                getSortData: {
                    category_one: '[data-category_one]',
                    category_two: '[data-category_two]',
                    category_three: '[data-category_three]',
                    category_four: '[data-category_four]',
                    category_five: '[data-category_five]',
                }
            });

            $('.sorting-menu a').click(function () {
                if ($(this).hasClass('active')) return false;
                $(this).parent().find('.active').removeClass('active');
                $(this).addClass('active');
                var sortValue = $(this).attr('data-sort-value');
                $('.sorting-item.disabled').removeClass('disabled');
                $('.sorting-item[data-' + sortValue + '!="1"]').addClass('disabled');
                $container.isotope({sortBy: sortValue});
                if (sortValue == 'original-order') $('.sorting-item.disabled').removeClass('disabled');
            });
        });
    });
</script>
<script>

    $("#lnkSendContact").click(function (event) {
        event.preventDefault();
        var _inputValid = true;
        $('#frmContato input').each(function () {
            if (!$(this).val()) {
                _inputValid = false;
            }
        });

        $('#frmContato textarea').each(function () {
            if (!$(this).val()) {
                _inputValid = false;
            }
        });

        if (_inputValid) {
            $.post(
                '/actions/enviar-email.php',
                $('#frmContato').serialize()
            ).done(function (args) {
                $("#nome_contato").val("");
                $("#email_contato").val("");
                $("#telefone_contato").val("");
                $("#assunto_contato").val("");
                $("#mensagem_contato").val("");
                alert("Enviado com sucesso.");
            });
        } else {
            alert("Preencha todos os campos");
        }
    });
</script>

