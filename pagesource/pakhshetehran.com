            //$('#MainSuggestions .Contant').html("<span><img src='/KWAdmin/images/loaders/circular/007.gif' /></span>");

            $('#MainSuggestions .Contant').html("<div class='loadingImage' style='margin-top: 30px;'><div class='circle'></div><div class='circle1'></div></div>");

            $('#MainSuggestions').fadeIn(); // Show the MainSuggestions box
            $.ajax({
                url: "/Default.aspx/GetMainSearchProduct",
                data: JSON.stringify({ StringText: inputString, GroupID: GroupID }),
                type: "POST",
                contentType: "application/json; charset=utf-8",
                dataType: "JSON",
                processData: true,
                success: function (result) {
                    $('#MainSuggestions').fadeIn();
