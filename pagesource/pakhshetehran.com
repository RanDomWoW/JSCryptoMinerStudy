        clearTimeout($.data(this, 'timer'));
        var wait = setTimeout(lookupMainProductSearch, 500);
        $(this).data('timer', wait);
    });

    $('#txt_Main_Search').on('keypress', function (e) {
        if (e.which === 13) {
