                        setHeight: 430,
                        theme: "dark-thin",
                        autoExpandScrollbar: true
                    });
                },
                error: function (xhr, status) {
                    //alert("error");
                }
            });
        }
    }

    $('#txt_Main_Search').on('keypress', function (e) {
        if (e.which === 13) {
            e.preventDefault();
        }
    });

