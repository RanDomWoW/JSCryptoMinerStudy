                    values: [90, 290],
                    slide: function (event, ui) {
                        $('#rangeval').html(ui.values[0] + " - " + ui.values[1]);
                    }
                });
                $('#defaultslide').slider({
                    max: 1,
                    min: 0,
                    value: 0,
                    slide: function (e, ui) {
                        $('#currentval').html(ui.value);
                    }
                });
            });

        </script>

        <script src="/js/script.js?v=1.0.9" type="text/javascript"></script>
        <script src="/js/inewsticker.js"></script>




        <script>
            $(function () {
                $('.spinner').spinner({
                    min: 0,
                    max: 100,
                    step: 1
                });
            });
        </script>


    </form>
</body>
</html>