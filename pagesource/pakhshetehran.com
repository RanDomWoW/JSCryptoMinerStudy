    function lookupMainProductSearch() {

        inputString = $('#txt_Main_Search').val();
        var GroupID = -1// $(".header .selected").attr("data-id");

        if (inputString.length == 0) {
            $('#MainSuggestions').fadeOut(); // Hide the MainSuggestions box
