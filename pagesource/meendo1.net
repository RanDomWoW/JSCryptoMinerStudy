    {
        if (frmValidatePassword()) frmSuccessMessage(this, _passwordOK);
    });

    $('#User_email').blur(function ()
    {
        if (frmValidateEmail())
        {
            $.get('/?r=users/auth/checkemail', {email: $(this).val()}, function (ret)
