function chsubscribedzw(){
    jQuery.getJSON('http://my.lotour.com/i/u/msg/GetIsReadMsgCount?isRecord=false&random=' + (new Date()) + '&callback=?', function (data) {
    var dataMsg = data.Result.Data;
    if(parseInt(dataMsg.memberid) > 0){
      $('.dy_nw2 .dy,.dy_nw .dy').each(function(i,e){
        var id=$(e).attr('deffer');
        jQuery.ajax({
            url: "http://api.lotour.net/brandhome/Statistics/getSubscribeZL",
            data: { zlid: id, userid: dataMsg.memberid },
            dataType: "jsonp",
            type: "POST",
            success: function (result) {
                if (result.subscribed > 0) {
                    $(e).siblings('.dy_pic').hide();
                    $(e).text("已订阅");
                }
            }
        });
      });
    }
  });
}
function subscribedzw(id){
  jQuery.getJSON('http://my.lotour.com/i/u/msg/GetIsReadMsgCount?isRecord=false&random=' + (new Date()) + '&callback=?', function (data) {
    var dataMsg = data.Result.Data;
    if(parseInt(dataMsg.memberid) > 0){
        jQuery.ajax({
            url: "http://api.lotour.net/brandhome/Statistics/SubscribeZL",
            data: { zlid: id, userid: dataMsg.memberid },
            dataType: "jsonp",
            type: "POST",
            success: function (result) {
                if (result.status > 0) {
					          $('.dy[deffer='+id+']').siblings('.dy_pic').hide();
                    $('.dy[deffer='+id+']').text("已订阅");
                }
            }
        });
    }
    else{
        loginToAction();
    }
  });
}
function Consolesubscribedzw(id) {
