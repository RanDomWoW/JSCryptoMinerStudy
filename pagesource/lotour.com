function validateAccount() {
    setErrMsg("");
    // setAllTextDisable(false);
     var loginNameValue = $.trim($('#'+inputId).val());

     if(inputId=='txtemail'){
         if(loginNameValue==""){
        	 setErrMsg1("请输入您的手机号码！");
             return false;
         }
         if (!isMobile(loginNameValue)) {
             setErrMsg("请输入正确的手机号码！");
             return false;
         }
     }else if(inputId=='out_txtemail'){
         if(loginNameValue==""){
        	 setErrMsg1("请输入您的邮箱账号！");
             return false;
         }
         if (!isEmail(loginNameValue)) {
             setErrMsg("请输入正确的邮箱账号！");
             return false;
         }
     }
     return true;
}

function validateEmail(id) {

	inputId = id;
	var loginNameValue = $.trim($('#'+inputId).val());
    if(!validateAccount()){
    	return false;
    }

   jQuery.ajax({
       url : 'http://passport.lotour.com/reg/judgeMobileStatus',
       type : "get",
       data : {
           "mobile" : loginNameValue
       },
       cache : false,
       dataType : "jsonp",
       contentType : "application/x-www-form-urlencoded; charset=utf-8",
       success : function(data) {
            $("#checking").addClass("sphl");
               if (data.code == 1) {
                   setErrMsg( "该手机号码已被占用！");
               }
               else if (data.code == -1) {
                   setErrMsg("请输入正确的手机号码！");
               }
               else if (data.code == 2) {
                   setErrMsg("该手机号码已被占用！");
               }
               else if (data.code == 9) {
                   setErrMsg("请输入正确的邮箱格式！");
               }
               else if (data.code == 11) {
                   setErrMsg("该邮箱已被占用！");
               }
                   return true;
       },
       error : function() {
           $("#resent").attr("disabled", false);
       }
   });
}

function validateNickname() {
	 setErrMsg("");
	if ($.trim($('#txtname').val()).length <= 0) {
		setErrMsg1("请填写昵称!");
		return false;
	}
	if(stripscript($('#txtname').val())){
		setErrMsg("昵称包含特殊字符");
		return false;
	}
	if ($.trim($('#txtname').val()).length < 2|| $.trim($('#txtname').val()).length > 32) {
		 setErrMsg("昵称应为2-32位字符!");
		return false;
	}
	setErrMsg("");
	return true;
}
function validatePass() {
		 setErrMsg("");
		    if ($('#txtpassword').val()=="") {
		    	setErrMsg1("请填写密码！");
		        return false;
		    }
		if ($('#txtpassword').val().indexOf(' ') != -1) {
			setErrMsg("密码中不能包含空格");
			return false;
		}
		if(ckChinese($('#txtpassword').val())){
			setErrMsg("密码中不能包含汉字");
			return false;
		}
		if ($.trim($('#txtpassword').val()).length < 6||$.trim($('#txtpassword').val()).length>16) {
			 setErrMsg("密码应为6-16位字符!");
			return false;
		}
		 setErrMsg("");
	}
function validateCode() {
	 setErrMsg("");
	if ($.trim($('#ecode').val()).length <= 0) {
		setErrMsg1("请填写验证码！");
		return false;
	}
	if ($.trim($('#ecode').val()).length != 6) {
		setErrMsg("请输入六位验证码");
		return false;
	}
	setErrMsg("");
}
function validateParms() {
    setErrMsg("");

    if(!validateAccount()){
        return false;
    }

    if ($.trim($('#txtname').val()).length <= 0) {
    	setErrMsg1("请填写昵称!");
        return false;
    }
    if(stripscript($('#txtname').val())){
        setErrMsg("昵称包含特殊字符！");
        return false;
    }
    if ($.trim($('#txtname').val()).length < 2
            || $.trim($('#txtname').val()).length > 32) {
        setErrMsg("昵称应为2-32位字符！");
        return false;
    }
    if ($('#txtpassword').val()=="") {
    	setErrMsg1("请填写密码！");
        return false;
    }
    if ($('#txtpassword').val().indexOf(' ') != -1) {
        setErrMsg("密码中不能包含空格！");
        return false;
    }
    if(ckChinese($('#txtpassword').val())){
        setErrMsg("密码中不能包含汉字！");
        return false;
    }
    if ($.trim($('#txtpassword').val()).length < 6||$.trim($('#txtpassword').val()).length>16) {
         setErrMsg("密码应为6-16位字符！");
        return false;
    }
    if ($.trim($('#ecode').val()).length <= 0) {
    	setErrMsg1("请填写验证码！");
        return false;
    }
    setErrMsg("");
    return true;
}

//发送短信计时
	function countDown() {
	 	var st=60;
	 	var tot=setInterval(function(){
	 				st--
	 				if(st==0){
	 					clearInterval(tot);
	 					$("#resent").html('重新获取验证码');
	 					$("#resent").attr("disabled", false);
	 					 $("#resent").css("background", "#F2F2F2");
	 					return false
	 				}else{
	 				$("#resent").html(st+'秒后重发');
	 				$("#resent").css("background","#c0c0c0");
	 				$("#resent").attr("disabled", "disabled");
	 				}
	 		},1000)
	 }
