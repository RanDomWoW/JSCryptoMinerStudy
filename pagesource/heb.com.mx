<script type='text/javascript'>
    jQuery.noConflict();
	jQuery(document).ready(function() {
		jQuery('#closeBtn').click(function(){
			parent.jQuery.fancybox.close();
		});
		jQuery('#txtZp').keyup(function (){
			this.value = (this.value + '').replace(/[^0-9]/g, '');
		});
		jQuery("#redirect-nacional").hide();
		jQuery('#sbmCheckZp').click(function(){
			var zipCode = jQuery('#txtZp').val();
			
			if(zipCode === '' || zipCode === null){
				alert('Ingrese CP válido');
				return;
			}
		
			jQuery('#load_changestore').show();
			jQuery('#sbmStoreSelect').hide();
			jQuery('#selectStore').hide();
			jQuery('#txtZp').hide();
			jQuery('#sbmCheckZp').hide();
			jQuery('.title-message-cp').hide();
			jQuery('#sbmCheckZp').hide();
			var url = 'http://www.heb.com.mx/switchstore/index/redirectZipcode';
			jQuery.ajax({
				type: 'POST',
				url: url,
				data: {postal_code:zipCode, modal: 1, type:'normal_shipping'}
			}).done(function(data) {
				jQuery('#load_changestore').hide();
				jQuery('#sbmCheckZp').show();
				var response = jQuery.parseJSON(data);
				
				if(response.success == true){
					//var switchstore = confirm('¿Desea cambiar de tienda?');
					//if(switchstore){
					if(response.error_messages !== 'CLOSE'){
						top.window.location.href = response.error_messages;
					}
					parent.jQuery.fancybox.close();
					//}else{
						//     window.location.reload();
						//   parent.jQuery.fancybox.close();
					//}
				}else if(response.error_caracter == true){
					alert(response.error_messages);
					jQuery('#sbmStoreSelect').show();
					jQuery('#selectStore').show();
					jQuery('#txtZp').show();
					jQuery('#sbmCheckZp').show();
					jQuery('.title-message-cp').show();
					jQuery('#sbmCheckZp').show();
					return;
				}else if(response.error_close == true){
					parent.jQuery.fancybox.close();
				} else if(response.error_invalid == true){
					alert(response.error_messages);
					jQuery('#sbmStoreSelect').show();
					jQuery('#selectStore').show();
					jQuery('#txtZp').show();
					jQuery('#sbmCheckZp').show();
					jQuery('.title-message-cp').show();
					jQuery('#sbmCheckZp').show();
					return;
				}else if(response.error_redzone == true){
					alert(response.error_messages);
					jQuery('#sbmStoreSelect').show();
					jQuery('#selectStore').show();
					jQuery('#txtZp').show();
					jQuery('#sbmCheckZp').show();
					jQuery('.title-message-cp').show();
					jQuery('#sbmCheckZp').show();
					return;
				}else{
					var storeId = '20';
					if(storeId !== '1' || storeId !== 1){
						jQuery("#redirect-nacional").click(function() {
							location.href = this.href;
						});
						jQuery("#redirect-nacional").click();
					}else{
						parent.jQuery.fancybox.close();
					}
				}
			});
		});
		
		/***** SELECTOR  DE TIENDA EN STOREPICKUP *****/
		jQuery('#sbmStoreSelect').click(function(){
			var singleValues = jQuery('#selectStore').val();
			jQuery('#load-store').show();
			jQuery('#sbmStoreSelect').hide();
			jQuery('#selectStore').hide();
			jQuery('#txtZp').hide();
			jQuery('#sbmCheckZp').hide();
			jQuery('.title-message-cp').hide();
			var url = 'http://www.heb.com.mx/switchstore/index/redirectZipcode';
			jQuery.ajax({
				type: 'POST',
				url: url,
				data: {postal_code:singleValues, modal: 1, type:'storepickup'}
			}).done(function(data) {
				jQuery('#load-store').hide();
				jQuery('#sbmStoreSelect').show();
				var response = jQuery.parseJSON(data);
				if(response.success == true){
					//var switchstore = confirm('¿Desea cambiar de tienda?');
					//if(switchstore){
					if(response.error_messages !== 'CLOSE'){
						top.window.location.href = response.error_messages;
					}
					parent.jQuery.fancybox.close();
				//}else{
					//      window.location.reload();
					//      parent.jQuery.fancybox.close();
					//}
				}else if(response.error == true){
					alert(response.error_messages);
					jQuery('#sbmStoreSelect').show();
					jQuery('#selectStore').show();
					jQuery('#txtZp').show();
					jQuery('#sbmCheckZp').show();
					jQuery('.title-message-cp').show();
					jQuery('#sbmCheckZp').show();
					return;
				}else if(response.error_close == true){
					parent.jQuery.fancybox.close();
				}
			});
		});
		
		if(!GetCookie('telef_agent')){
			var qtyItems = '0';
			if(!GetCookie('modalcart') && qtyItems >= 1){
				jQuery(".fancybox").hide();
				jQuery(".fancybox").fancybox({
					closeBtn 	: false,
					scrolling	: 'no',
					width		: '20%',
					height		: '40%',
					helpers:{
						overlay : {closeClick: false}
					},
					keys: {
						close: [null] // disable escape on fancybox
					}
				});
				jQuery(".fancybox").trigger('click');
			}else{
				jQuery(".fancybox").hide();
			}
		}
	});
    
	function GetCookie(name) {
        var arg=name+"=";
        var alen=arg.length;
        var clen=document.cookie.length;
        var i=0;
        
		while (i<clen) {
            var j=i+alen;
            if (document.cookie.substring(i,j)==arg)
                return "1";
            i=document.cookie.indexOf(" ",i)+1;
            if (i==0)
				break;
        }

        return null;
    }
