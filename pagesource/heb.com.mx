.tien_header, .tien_tiendas { width: 94%; margin-bottom: 20px; margin-right: 0; }
.tien_centrado h1, .tien_centradoh1 { font-size: 30px; line-height: 0; margin-bottom: 25px; padding-left: 0; }
.tien_tiendas{ overflow: auto; }
.tien_telhora { font-weight: bold !important; font-size: 16px !important; }
