		slideSize=465;
		autoslide= function(){
			cicle=setInterval(function(){slide(nowplay, 'autoplay')}, 5000);
		}
		slide=function(id, opt){
			if(opt=='autoplay'){
				nowplay++;
				if(nowplay>cnt){nowplay=1};
				id=nowplay;
			}else{
				if (typeof(cicle)!=='undefined') clearTimeout(cicle);
				nowplay=id;
			}
			nowplay==1?multiplier=0:multiplier=slideSize*(id-1);
			$("#topslider div").animate({'left':'-'+multiplier+'px'},250)
			$(".slidebutton").css("list-style-type", "disc");
			$(".slidebutton").css("color", "rgb(71,71,71)");
			$("#slidebutton_"+nowplay).css("list-style-type", "disc");
			$("#slidebutton_"+nowplay).css("color", "rgb(254, 203, 0)");
		}
		$(document).ready(function(){
			cnt=$("#menu_open > ul > li").size();
			autoslide();
		});
		$(window).resize(function() {
			slideSize=$('#topslider').width();
			$('#topslider .slides').css({'width':$('#topslider').width()+'px'});
		});
</script>
