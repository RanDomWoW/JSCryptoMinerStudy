                    );
                        function expand($elem) {
                            var angle = 0;
                            var t = setInterval(function () {
                                if (angle == 1440) {
                                    clearInterval(t);
                                    return;
                                }
                                angle += 40;
                                $j('.link', $elem).stop().animate({ rotate: '+=-40deg' }, 0);
                            }, 10);
                            $elem.stop().animate({ width: '215px', height: '75px' }, 1000)
                        .find('.item_content').fadeIn(200, function () {
                            $j(this).find('p').stop(true, true).fadeIn(600);
                        });
                        }
                        function collapse($elem) {
                            var angle = 1440;
                            var t = setInterval(function () {
                                if (angle == 0) {
                                    clearInterval(t);
                                    return;
                                }
                                angle -= 40;
                                $j('.link', $elem).stop().animate({ rotate: '+=40deg' }, 0);
                            }, 10);
                            $elem.stop().animate({ width: '35px', height: '35px' }, 1000)
                    .find('.item_content').stop(true, true).fadeOut().find('p').stop(true, true).fadeOut();
                        };
                    });
