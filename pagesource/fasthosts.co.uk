  // Fires reload on back button navigation for iOS - fixes video autoload
  window.onpageshow = function(event) {
      if (event.persisted) {
          window.location.reload() 
      }
  };
  // Autoplay handler and dacform validation
  // Call to object-fit polyfill includes event handling to fire autoplay 
  jQuery(function ($) {
    $(document).ready(function(){
      
      //objectFitVideos();
      var vid = document.getElementById("video");
      function vidLoad () {
        vid.load();
      }
      function getmetadata(){
        vid.play();
      }
      var detectBreakPoint = function (bp) {
        var bps = [319, 1025, 1920],
        w = $(window).width(),
        min, max
        for (var i = 0, l = bps.length; i < l; i++) {
          if (bps[i] === bp) {
            min = bps[i-1] || 0
            max = bps[i]
            break
          }
        }
        return w > min && w <= max
      }
      if (detectBreakPoint(1920)) { 
        var dataSrc = $( ".video__src" ).attr( "datasrc" );
        objectFitVideos();
        $(".video__src").attr("src", dataSrc);
        vidLoad();
        getmetadata();
        vid.addEventListener("loadedmetadata", getmetadata);
        if (vid.readyState >= 2) {
          getmetadata();
        }
      } // Breakpoint between 1025 and 1920

      // Form validation
      $(".dacform-flex").submit(function(){
        var x;
        x = document.getElementById("edit-domain-super-hero").value;
        if (x == "") {
          $(".empty-alert").fadeIn();
          setTimeout(function() { $(".empty-alert").fadeOut(); }, 5000);
          return false;
        };
      });
    });
  });
</script>

