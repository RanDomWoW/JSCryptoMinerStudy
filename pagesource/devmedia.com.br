        function loadBanners(){
            var cache = 16;
            var x = 0;
            $.each($(".alt-banner"), function(){
                var block = $(this);
                if(!block.hasClass('loaded') && !block.hasClass('loading')){
                    var id = x++;
                    if(block.data("id") == undefined){
                        block.attr("data-id", id);
                    }else{
                        id = block.data("id");
                        x = id++;
                    }

                    if(isOnScreen(block)){
                        var url = block.data("url");
                        var html = block.clone();

                        block.addClass("loading");

                        html.load(url + "?" + cache, function(){
                            var link = html.find("link[rel='stylesheet']");
                            if(link.length > 0){
                                var qnt = link.length;
                                var x = 1;

                                $.each(link, function(){
                                    var css = $("<style scoped></style>");
                                    css.load(link.attr("href")+"?"+cache, function(){
                                        css.text(css.text().replace(/\.bg\_banner\_section/gi,"section.alt-banner[data-id='"+ block.data("id") +"'] .bg_banner_section"));
                                        link.remove();
                                        html.prepend(css);
                                        if(x >= qnt){
                                            setTimeout( function(){
                                                block.html(html.html());
                                                block.addClass('loaded');
                                            }, 600);
                                        }else{
                                            x++;
                                        }
                                    });
                                });
                            }else{
                                setTimeout( function(){
                                    block.html(html.html());
                                    block.addClass('loaded');
                                    block.removeClass('loading');
                                }, 600);
                            }                   
                        });
                    }
                }
            });
        }

        function loadImages(){
            $.each($(".lazy-img"), function(){
                var block = $(this);
                var image = block.find("img");

                if(isOnScreen(block)){
                    var url = image.data("url");
                    
                    if(image.attr("src") != url){
                        image.on('load', function(){
                            $(this).addClass("loaded");
                        });
                        image.attr("src",url);
                    };
                }
            });
        }

        function loadImgSlider(){
            var slider = $(".slider-slick, .slider-devcast").find(".slick-active figure");

            $.each(slider, function(i, e){
                var figure = $(e);

                if(isOnScreen(figure)){
                    var img = figure.children("img");               

                    if(img.data("slide-lazy") != undefined){
                        img.on("load", function(){
                            figure.addClass("loaded");
                        });
                        img.attr("src", img.data("slide-lazy"));
                        img.removeAttr("data-slide-lazy");
                    }
                }
            });
        }

