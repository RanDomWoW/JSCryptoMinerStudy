<script type="text/javascript">
	
	$(function(){
				
		/*-- Mobile --*/
		(function Mobile(){
			
			if($.browser.msie && $.browser.version < 9){ return; }
			
			var a = $(window),
					o = $(".nav-item"),
					c = $("#wrapper"),
					onoff = true;

				a.bind("resize",function(){
					var v = $("body").height() > a.height() ? 783 : 800;
					if(a.width() <= v){
						$("body").find("#m-banner").fadeIn(300);
						if(!onoff){ return; }
						c.before("<div id='m-banner'><div></div></div>");
						o.each(function(i){
							$("body").find("#m-banner div").append("<img src='" + $(this).find("img").attr("src") + "' />");
						});
						
						var oImg = new Image();
								oImg.src = o.eq(0).find("img").attr("src");
						
						function fnIscroll(){
							var Focus;
							Focus = new iScroll("m-banner", {
								snap: true,
								momentum: false,
								hScrollbar: false
							});
						}
						
						if(oImg.width > 0){
							fnIscroll();	
						}else{
							oImg.onload = function(){
								fnIscroll();	
							}
						}
						onoff = false;
					}else{
						$("body").find("#m-banner").fadeOut(300);
					}
				}).trigger("resize");
			
		})();
		
	});
	
</script>
