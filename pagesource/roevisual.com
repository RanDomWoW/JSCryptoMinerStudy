<script>
	$(function(){

		//弹出层
		(function pop(){
			var aph = $("[data-rel='aph']"), //半透明层
				box = $("[data-rel='pop']"), //内容层
				off = $("[data-adv='close']"), //关闭
				play = $("[data-adv='play']"), //触发
				init = {};

			//弹出效果
			init = {
				open : function(){
					aph.fadeIn(500, function(){
						box.css({ "display" : "block", top : "55%", opacity : 0 })
						   .animate({ top : "50%", opacity : 1 }, 500);
					});
				},
				close : function(){
					box.animate({ top : "45%", opacity : 0 }, 500, function(){
						aph.fadeOut(500);
						$(this).css({ "display" : "none" });
					});
				}
			}

			init.open();
			off.bind("click", init.close);
			aph.bind("click", init.close);
			play.bind("click", init.open);

		})();

	});
</script>
<script type="text/javascript" src="/themes/roeled-cn/default/Scripts/jquery.browser.js"></script>
