<script>
    $(document).ready(function () {

        // this block for pick language ->
        var lang = 'ru';

        $('.js-target-for-language.active').toggleClass('active');

        if (lang == 'ru') {
            $('.js-target-for-language').each(function () {
                if ($(this).hasClass('Russian')) {
                    $(this).toggleClass('active')
                }
            });
        }

        else {
            $('.js-target-for-language').each(function () {
                if ($(this).hasClass('English')) {
                    $(this).toggleClass('active')
                }
            });
        }

        var searchInput = $('.search-input');

        searchInput.autocomplete({

            minLength: 2,

            appendTo: '.scrollbar-inner.scroll-content',

            source: function (request, response) {

                $('.row-search .loading').show();

                $.ajax({
                    url: '/index.php?route=product/search/findProductByName',
                    type: 'POST',
                    data: {filter_name: request.term}
                })
                    .done(function (data) {

                        var obj = JSON.parse(data);

                        $('.search-list .total-row').remove();

                        $('.search-list').prepend(obj.shift());

                        $('.row-search').addClass('active');

                        $('.row-search .loading').hide();

                        if(obj.length === 0)
                            obj = ['']; // clear items if empty response

                        response(obj);
                    });

            },
            focus: function (event, ui) {

            },
            select: function (event, ui) {
            },
            close: function( event, ui ) {
                $('.row-search').removeClass('active');
            }
        });

        searchInput.autocomplete('instance')._renderItem = function (ul, item) {

            return $(item.html).appendTo(ul);

        };


    })
</script>

