    <script type="text/javascript">
        var lastItemid = 0;
        var Curr = 'USD';
        addItems = function (curr) {
            $("#itemLoader").show();
            addItems.loading = true;
            $.get("asp/items.asp?id=" + lastItemid + "&lang=en-US&curr=" + curr + "&rnd=" + Math.random(), function (data) {
                lastItemid = data.substring(0, data.indexOf('|'));
                $("#itemLoader").before(data.substring(data.indexOf('|') + 1));
                addItems.loading = false;
                $("#itemLoader").hide();
            });
        }
        checkforAdd = function() {
            if  ($(window).scrollTop() + 400 > $(document).height() - $(window).height()) {
                if (lastItemid > 0) addItems(Curr);
            }
        }

        function reloadItems(curr) {
            Curr = curr;
            last_ID = 0;
            lastItemid = 0;
            $("#itemLoader").prevAll().remove();
            addItems(curr);
        }

        $(document).ready(function () {
            lang = 'en-US';
            last_ID = 0; addItems(Curr);
            checkforAdd();
            $(window).scroll(function() {
                if (addItems.loading) return;
                checkforAdd();
            });

            // предзаказы
            $(function () {
                var $container = $('#slider-games');
                $($container).platiSlider({
                    $slider: $('.slider_inner', $container),
                    $pager: $('.slider_controls', $container)
                });
                $('.slider_inner').show();
            });


        });

        function Goods(page, rows, curr) {
            $("#GoodsPager").html('<img style=margin-top:4px src=images/loading.gif />');
            $(".platiru-loader").css("visibility","visible");
            $.get("asp/block_recommended.asp?page=" + page + "&rows=" + rows + "&curr=" + curr + "&lang=en-US&rnd=" + Math.random(), function (result) {
                if (result.length > 10) {
                    $("#RecommendedBlock").html(result);
                    qtip_init();
                }
		    });
		    return false;
        }

    </script>
