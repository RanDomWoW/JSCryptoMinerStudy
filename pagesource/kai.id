			return false;
		}
		
		if($('#destinationm').val()==''){
			alert('Anda belum memilih stasiun tujuan.');
			return false;
		}
		
		if($('#departure_datem').val()==''){
			alert('Anda belum memilih tanggal berangkat.');
			return false;
		}
		
		if($('#adultm').val()=='0'){
			alert('Anda belum menentukan jumlah penumpang dewasa.');
			return false;
		}
		
		if($('#infantm').val()>$('#adultm').val()){
			alert('Jumlah infant tidak boleh lebih besar dari jumlah dewasa.');
			return false;
		}
		
		if($('#originationm').val()==$('#destinationm').val()){
			alert('Lokasi asal dan tujuan tidak boleh sama.');
			return false;
		}
		
	}	
	
	function cek_kodebookingm(){
