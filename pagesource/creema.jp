<script type="text/template" id="tpl-timeline-found">
    <div
        data-id="<%- feed.feed_id %>"
        data-verb-type="<%- feed.verb_type %>"
        data-verb-type-name="<%- feed.verb_type_name %>"
        data-follow-type="<%- feed.follow_type %>"
        data-created-time="<%- feed.created_time %>"
        class="p-home-timeline__item">
        <div class="timeline-item-section p-home-timeline__visual">
            <% if (feed.item.visuals.length > 1) { %>
                <div>
                    <ul class="p-home-timeline-photo-list u-clearfix">
                        <% _.forEach(feed.item.visuals, function(visual, index) { %>
                            <li class="p-home-timeline-photo-list__item">
                                <a href="<%- visual.url %>?et_source=<%- visual.source %>&et_medium=<%- feed.verb_type_name %>&et_campaign=item_visual_4_<%- index + 1 %>" class="p-home-timeline-photo" target="_blank">
                                    <img src="/assets/images/default.jpg" data-original="<%- visual.image %>" class="p-home-timeline-photo">
                                </a>
                                <button
                                    class="c-button c-button--white c-button--sm c-like-button p-home__like js-like-item"
                                    title="お気に入りに追加"
                                    data-id="<%- visual.id %>"
                                    data-active-class="is-active"
                                    data-active-text=""
                                    data-inactive-text="">
                                    <i class="fa fa-heart-o"></i>
                                </button>
                            </li>
                        <% }); %>
                    </ul>
                </div>
            <% } else { %>
                <div>
                    <ul class="p-home-timeline-photo-list u-clearfix">
                        <% _.forEach(feed.item.visuals, function(visual, index) { %>
                            <li class="p-home-timeline-photo-list__one">
                                <a href="<%- visual.url %>?et_source=<%- visual.source %>&et_medium=<%- feed.verb_type_name %>&et_campaign=item_visual" target="_blank">
                                    <img src="/assets/images/default.jpg" data-original="<%- visual.image %>" class="p-home-timeline-photo-list__one-photo">
                                </a>
                                <button
                                    class="c-button c-button--white c-button--sm c-like-button p-home__like js-like-item"
                                    title="お気に入りに追加"
                                    data-id="<%- visual.id %>"
                                    data-active-class="is-active"
                                    data-active-text=""
                                    data-inactive-text="">
                                    <i class="fa fa-heart-o"></i>
                                </button>
                            </li>
                        <% }); %>
                    </ul>
                </div>
            <% } %>
        </div>
        <% if (feed.verb_type == 'recommend-feed') { %>
            <div class="timeline-item-section timeline-item-meta cl">
                <div class="timeline-meta-double-title ellipsis"><a href="<%- feed.feed.shop.onsale_newer_url %>&et_source=<%- feed.source %>&et_medium=<%- feed.verb_type_name %>&et_campaign=sentence" target="_blank"><%- feed.sentence %></a></div>
            </div>
            <div class="timeline-item-section timeline-item-foot recommend-exhibitor cl">
                <div class="timeline-item-foot-layout">
                    <div class="timeline-item-avator">
                        <a href="<%- feed.feed.shop.onsale_url %>?et_source=<%- feed.source %>&et_medium=<%- feed.verb_type_name %>&et_campaign=feed.shop.icon" target="_blank"><img src="/assets/images/default.jpg" data-original="<%- feed.feed.shop.icon %>" /></a>
                    </div>
                    <div class="timeline-item-footer-meta ellipsis">
                        <div class="timeline-meta-title ellipsis"><a href="<%- feed.item.url %>?et_source=<%- feed.source %>&et_medium=<%- feed.verb_type_name %>&et_campaign=item_title" target="_blank"><%- feed.item.title %></a></div>
                        <div class="timeline-meta-info">
                            <span class="timeline-meta-onsale-count">
                                出品 <a href="<%- feed.feed.shop.onsale_url %>?et_source=<%- feed.source %>&et_medium=<%- feed.verb_type_name %>&et_campaign=show_profile_exhibits" target="_blank"><%- feed.item.meta.exhibit_count %></a>
                            </span> /
                            <span class="timeline-meta-rating-count">
                                評価 <a href="<%- feed.feed.shop.rating_url %>?et_source=<%- feed.source %>&et_medium=<%- feed.verb_type_name %>&et_campaign=show_profile_ratings" target="_blank"><%- feed.item.meta.review_count %></a>
                            </span>
                        </div>
                        <div class="timeline-item-action">
                            <button
                                type="button"
                                class="c-button-follow p-home__follow js-like-creator"
                                data-id="<%- feed.shop.id %>"
                                data-count-target=".c-button-follow__letter"
                                data-active-class="is-active"
                                data-active-text=""
                                data-inactive-text="">
                                <i class="c-icon-follow"></i>
                                <span class="c-button-follow__letter" data-id="<%- feed.shop.id %>"><%- feed.item.meta.follow_count %></span>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
