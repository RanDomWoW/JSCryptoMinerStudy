        <div class="c-speech-bordering-bubble c-speech-bordering-bubble--top p-header-bubble--cart__inner">
            <% if (!_.isEmpty(items)) { %>
                <% _.forEach (items, function(item, key) { %>
                    <div class="p-header-bubble__item">
                        <div class="p-header-bubble__photo">
                            <a href="/item/<%- item.id %>/detail"><img src="<%- item.image %>" width="34" height="34" alt="<%- item.title %>" class="c-item-article-image p-header-bubble__image"></a>
                        </div>
                        <div class="p-header-bubble__meta">
                            <div class="u-margin-b-sm"><a href="/item/<%- item.id %>/detail"><%- item.title %></a></div>
                            <div>
                                <span><%- item.price %></span> / <span><%- item.creator_name %></span>
                            </div>
                        </div>
                    </div>
                <% }); %>
                <div class="u-text-center p-header-bubble__buttons">
                    <a href="/cart" class="c-button c-button--sm u-block  p-header-bubble__button">カートを見る（<%- count %>）</a>
                </div>
            <% } else { %>
                <div class="u-text-center p-header-bubble__nothing">
