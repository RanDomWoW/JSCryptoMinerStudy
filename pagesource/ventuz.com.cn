    <script type="text/javascript">
        $(document).ready(function () {

            function openwx() {
                $("#wximg").css("visibility", "visible");
            }
            function hiddenwx() {
                $("#wximg").css("visibility", "hidden");
            }
            var url = "../ajax/getMenuList.ashx?title=" + document.title;
            $.get(url, function (data, status) {
                $("#superfish-1").html(data);
            });

            url = "../ajax/getSmallMenuList.ashx?title=" + document.title;
            $.get(url, function (data, status) {
                $("#leftsidebar_box").html(data);
                opensmallmenus();
            });

            url = "../ajax/getFootMenusList.ashx?title=" + document.title;
            $.get(url, function (data, status) {
                $("#superfish-2").html(data);
            });
           
            url = "../ajax/getBanner.ashx?name=" + document.title;
            $.get(url, function (data, status) {
                if (data != "") {
                    $("#flexslider-1").html(data);
                }
            });
            //
            function opensmallmenus() {
                $('#superfish-1-toggle').click(function () {
                    var s = $("#leftsidebar_box").css("display");
                    if (s == "none") {
                        $("#leftsidebar_box").css("display", "block");

                    }
                    else {
                        $("#leftsidebar_box").css("display", "none");

                    }
                });
                $(".leftsidebar_box dt").css({ "background-color": "#000000" });
                $(".leftsidebar_box dd").hide();
                $(".leftsidebar_box dt").click(function () {
                    $(this).parent().find('dd').removeClass("menu_chioce");
                    $(".leftsidebar_box dt img").attr("src", "images/select_xl01.png");
                    $(this).parent().find('img').attr("src", "images/select_xl.png");
                    $(".menu_chioce").slideUp();
                    $(this).parent().find('dd').slideToggle();
                    $(this).parent().find('dd').addClass("menu_chioce");
                });
                var w = $(window).width();
                myresize(w);
                if ($(document).scrollTop() > 120) {
                    $("#leftsidebar_box").css("width", "100%");
                    $("#leftsidebar_box").css("margin-left", "0%");
                }
                else {
                    $("#leftsidebar_box").css("width", "80%");
                    $("#leftsidebar_box").css("margin-left", "10%");
                }
            }
            //
        });
        function myresize(w) {
            if (w <= 768) {
                //$("#superfish-1").html("");
                $("#superfish-1").css("display", "none");
                $("#superfish-small").css("display", "block");
            }
            else {
                $("#superfish-small").css("display", "none");
                $("#superfish-1").css("display", "block");
                $('#logo').css("margin-left", "0%");
            } 
        }
        $(window).resize(function () {

            var w = $(window).width();
            myresize(w);
        });
    </script>
