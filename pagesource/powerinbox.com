<script>
    var closeDynamicMail = function () {
        jQuery('.dynamicMail-top').addClass('closed');
        jQuery('.dynamicMail-sticky').addClass('closed');
        jQuery('#header').removeClass('dynamicMail-open');
        jQuery('.feature').removeClass('dynamicMail-open');
    };

    jQuery(document).ready(function() {
        jQuery('#header').addClass('dynamicMail-open');
        jQuery('.feature').addClass('dynamicMail-open');

        // on document load
        if (jQuery(document).scrollTop() >= 30) {
            jQuery('.dynamicMail-sticky').addClass('sticky-show');
        }
        else {
            jQuery('.dynamicMail-sticky').removeClass('sticky-show');
        }

        // while user scrolls
        jQuery(window).scroll(function () {
            if (jQuery(window).scrollTop() >= 30) {
                jQuery('.dynamicMail-sticky').addClass('sticky-show');
            }
            else {
                jQuery('.dynamicMail-sticky').removeClass('sticky-show');
            }
        });
    });
</script>