<script>
    $("#full_keyword").keypress(function (event) {
        if (event.which == 13) {
            location.href = '/knowledge?_keyword=' + this.value;
        }
    });
    $('#full_keyword-btn').click(function () {
        location.href = '/knowledge?_keyword=' + $("#full_keyword").val();
    });
    $('#login_btn').click(function () {
        var data = {account: $('#login_account').val(), password: $('#login_password').val()};
        JF.ajax.AjaxHandler('/member/login', data);
    });

    $('.kw-favor').click(function () {
        var data = {id: $(this).attr('data-kw-id')};
        JF.ajax.AjaxHandler('/knowledge-favor', data);
    });

    $('.forum-favor').click(function () {
        var data = {id: $(this).attr('data-id')};
        JF.ajax.AjaxHandler('/forum-favor', data);
    });

    $('.forum-reply-quote').click(function () {
        var dataId = $(this).attr('data-id');
        var account_data = $('#account-' + dataId).html();
        var quote_data = $('#content-' + dataId).html();
        var current_data = CKEDITOR.instances['edit-area'].getData();
        CKEDITOR.instances['edit-area'].setData(current_data + '<p></p><blockquote>引用' + account_data + '：<br />' + quote_data + '</blockquote><p></p>');
    });

    $('.forum-reply-submit').click(function () {
        JF.ckEditor.updateEvent();
        JF.ajax.FormAjaxHandler('/forum-reply', $('#data-form'));
    });

    $('.forum-goto-write').click(function () {
        var data = {cid: $(this).attr('data-cid')};
        JF.ajax.AjaxHandler('/forum-goto-write', data);
    });


    $('.forum-write-submit').click(function () {
        JF.ckEditor.updateEvent();
        JF.ajax.FormAjaxHandler('/forum-write', $('#data-form'));
    });

    $('.forum-report').click(function () {
        if (confirm('您是否確定要檢舉此篇文章？')) {
            var data = {id: $(this).attr('data-id')};
            JF.ajax.AjaxHandler('/forum-report', data);
        }
    });

    $('.btn-vote').click(function () {
        var data = {ctid: $(this).attr('data-id')};
        JF.ajax.AjaxHandler('/showbaby-creative-vote', data);
    });

    $('.btn-add-cart').click(function () {
        var data = {
            id: $(this).attr('data-id'),
            cid: $(this).attr('data-cid')
        };
        JF.ajax.AjaxHandler('/cart-add', data);
    });

    $('.btn-change-amount').on('change', function () {
        var data = {
            id: $(this).attr('data-id'),
            cid: $(this).attr('data-cid'),
            amount: $(this).val()
        };
        JF.ajax.AjaxHandler('/cart-change', data);
    });

    $('.btn-remove-cart').click(function () {
        var data = {
            id: $(this).attr('data-id'),
            cid: $(this).attr('data-cid')
        };
        JF.ajax.AjaxHandler('/cart-remove', data);
    });

    $('#btn-CartRegistrationMail').click(function () {
        var data = {
            status: ($(this).prop('checked') === true) ? 1 : 0
        };
        JF.ajax.AjaxHandler('/cart-registration-mail', data);
    });


    $('#btn-redeem-coupon').click(function () {
        if ($('#coupon-id').val() == null) {
            alert('請輸入優惠代碼');
        }
        var data = {
            code: $('#coupon-id').val()
        };
        JF.ajax.AjaxHandler('/cart-coupon-check', data);
    });

    $('#btn-redeem-coupon-cancel').click(function () {
        var data = {
            code: ''
        };
        JF.ajax.AjaxHandler('/cart-coupon-check', data);
    });


    //    // Alt div
    $('.popup-alt').on('mouseover', function () {
        var altText = $(this).attr('data-alt');
        var divString = '<div class="popup-alt-div" style="z-index: 99999;position: absolute;top: ' + window.event.clientY + ';left: ' + window.event.clientX + ';width: 80px;background:#fffca2;color: #000;border: solid 1px;">' + altText + '</div>';
        $(this).append(divString);
//        setTimeout('$("#' + divId + '").remove();', 3000);
    }).on('mouseout', function () {
        $('.popup-alt-div').remove();
    });

</script>
