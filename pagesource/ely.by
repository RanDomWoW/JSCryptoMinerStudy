<div id="chat-box">
    <script class="hidden" type="text/x-template" id="mustache-chat-template" al-stop>
        <div class="chat-box" id="chat-box-{{id}}">
            <header>
                <img src="{{skin.face_url}}" />
                <span>{{nickname}}</span>
                <div class="chat-box-controls">
                    <div class="button red input-min icon" action="close"><div class="ico-close s26"></div></div>
                </div>
            </header>
            <div class="chat-body">
                <div class="chat-message-box">
                            {{#messages}}
                    <div class="chat-line"><b>{{author.nick}}</b>: {{text}}</div>
                            {{/messages}}
                        </div>
                <form method="POST" action="/messages/add">
                    <textarea class="black-style chat-message-input" placeholder="Enter your message here and press "Enter" to send" name="chatMessage" id="chatMessage"></textarea>
                    <input type="hidden" value="{{id}}" id="chatId" name="chatId" />
                </form>
            </div>
        </div>
    </script>

    <script class="hidden" type="text/x-template" id="mustache-chat-row-template" al-stop>
        {{#messages}}
        <div class="chat-line"><b>{{author.nickname}}</b>: {{text}}</div>
        {{/messages}}
    </script>
</div>
