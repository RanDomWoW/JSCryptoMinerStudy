<script>
    var calendarStartDate = new Date('2016-03-01');
    var calendarEndDate = new Date('2018-04-13');
    var calendarData = {};
    
    function dateFmt(d) {
        var m = d.getMonth() + 1;
        var c = d.getDate();
        return d.getFullYear() + '-' + (m < 10 ? '0' : '') + m + '-' + (c < 10 ? '0' : '') + c;
    }
    $(function() {
    var calendar = $('#calendar').datepicker({
        language: 'ru',
        startDate: calendarStartDate,
        endDate: calendarEndDate,
        beforeShowDay: function (d) {
            var monthStr = dateFmt(d).slice(0, 7);
            if (calendarData[monthStr] === undefined) {
                return false;
            }
            var dateStr = dateFmt(d).slice(0, 10);
            if (calendarData[monthStr][dateStr] === undefined) {
                return false;
            } else {
                return true;
            }
        }
    });
    calendar.on('changeMonth', function(e){
        load_calendar(dateFmt(e.date).slice(0, 7));
    });
    calendar.on('changeDate', function(e) {
        var monthStr = dateFmt(e.date).slice(0, 7);
        if (calendarData[monthStr] === undefined) {
            return false;
        }
        var dateStr = dateFmt(e.date).slice(0, 10);
        if (calendarData[monthStr][dateStr] === undefined) {
            return false;
        } else {
            var events = [];
            for (var event_id in calendarData[monthStr][dateStr]) {
                var title = '';
                if (calendarData[monthStr][dateStr][event_id]['title']) {
                    title = calendarData[monthStr][dateStr][event_id]['title'];
                } else {
                    title = calendarData[monthStr][dateStr][event_id]['d_title'];
                }
                var t = calendarData[monthStr][dateStr][event_id]['ptime'];
                var m = calendarData[monthStr][dateStr][event_id]['mesto'];

                events.push('<table><tr><td><span>' + t + '</span></td><td style="width: 95%; padding-left: 10px; vertical-align: bottom;"><span class=mesto>' + m + '</span></td></tr></table><p>' + title + '</p><br/>');
            }
            var offs = $('#day-' + dateStr).offset();
            $('#active-event')
                    .html(events.join(''))
                    .css({position: 'absolute', left: (offs.left - 520) + 'px', top: offs.top + 'px', width: '480px'})
                    .show();
            $('#day-' + dateStr).on('mouseout.dp', function(e) {
                $(this).off('mouseout.dp');
                $('#active-event').hide();
            });
            return false;
        }
    });
    function load_calendar(d) {
        var monthStr;
        if (d === undefined) {
            monthStr = dateFmt(new Date()).slice(0, 7);
        } else {
            monthStr = d;
        }
        if (calendarData[monthStr] === undefined) {
            var pData = {events_for: monthStr};
            $.ajax('/events/json/', {
