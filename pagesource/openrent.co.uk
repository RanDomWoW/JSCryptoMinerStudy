        var didItTakeAgesToDownloadTheImage = false;

        function getOrUpdateCookieAndReturnValue()
        {
            var randomImage = 13;
            var cookie = getCookie("indexCoverImage");
            if (cookie == null || cookie == "") {
                createCookie("indexCoverImage", randomImage, 30);
            }
            else
            {
                if(randomImage == 0)
                {
                    // to enable updating user onto new test before cookie expiration
                }
                else
                {
                    randomImage = cookie;
                }
            }

            // Let's test cover image
            _gaq.push(['_setCustomVar',
                3,                                              // This custom var is set to slot #1.  Required parameter.
                'HomePageCoverImage',                 // The name acts as a kind of category for the user activity.  Required parameter.
                randomImage,      // This value of the custom variable.  Required parameter.
                1                                               // Sets the scope to visitor-level.  Optional parameter.
            ]);

            return randomImage;
        }

        $(function () {


            var randomImage = getOrUpdateCookieAndReturnValue();

            var css = $('#hackyLateLoadingCSS').html();
            css = css.replace(/REPLACEWITHRANDOMNUMBER/g, randomImage);
            head = document.head || document.getElementsByTagName('head')[0],
            style = document.createElement('style');

            style.type = 'text/css';
            if (style.styleSheet) {
                style.styleSheet.cssText = css;
            } else {
                style.appendChild(document.createTextNode(css));
            }

            head.appendChild(style);

            setTimeout(function () {
                // wait for DOM to use new style, then get the back-ground image URL
                var computedURL = window.getComputedStyle(
                    document.querySelector('.index-block__picture'), ':after'
                ).getPropertyValue('background-image');

                // remove the leading url( and trailing );
                // note IE and Chrome / Firefox handle this differently, so we normalise the string first
                computedURL = computedURL.replace(/\s/g, '').replace(/"/g, '');
                // then we strip it down to the URL itself
                computedURL = computedURL.substring(4, computedURL.length - 1);

                setTimeout(function () { didItTakeAgesToDownloadTheImage = true; }, 100);

                var downloadingImage = new Image;
                downloadingImage.onload = function () {
                    if (didItTakeAgesToDownloadTheImage) {
                        // if it took a while, then we'll fade it in slowly
                        $(".index-block__picture").addClass("afterLoadedFadeInSlow");
                    }
                    else
                    {
                        $(".index-block__picture").addClass("afterLoadedFadeInFast");
                    }

                    $(".index-block__picture").addClass("showCover");
                };
                downloadingImage.src = computedURL;

            }, 10);

            // rubbish, no way to hook into the background-image loaded event.
            // hence why we use this extremely convoluted / complex code above.
            //$(".index-block__picture").ready(function () {
            //    $(".index-block__picture").addClass("afterLoadedOpacity");
            //});
        });
