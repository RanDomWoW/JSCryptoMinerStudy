            {
                clearTimeout(nearEndCheckTimeout);

                if (event.data == YT.PlayerState.PLAYING && !pausedOnce) {
                    // now check we're not 1s away from the end (when we'll pause the video)
                    videoDuration = player1.getDuration();
                    nearEndCheckTimeout = setTimeout(pauseIfNearEndOfVideo, 100);
                }
            }
        }

        function pauseIfNearEndOfVideo()
        {
            if (player1.getCurrentTime() + 1 > videoDuration) {
                player1.pauseVideo();
                pausedOnce = true;
            }
            else {
                setTimeout(pauseIfNearEndOfVideo, 100);
            }
        }

</script>



