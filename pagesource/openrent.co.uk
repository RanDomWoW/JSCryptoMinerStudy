        function launchYoutubePlayer()
        {
            var v = document.getElementsByClassName("youtube-player");
            for (var n = 0; n < v.length; n++) {
                var p = document.createElement("div");
                p.innerHTML = labnolThumb(v[n].getAttribute("data-id"));
                p.onclick = labnolIframe;
                v[n].appendChild(p);

                if (v[n].getAttribute("data-loadimmediately") == "true") {
                    p.onclick();
                }
            }
        }

        function labnolThumb(id) {
            return '<img class="youtube-thumb" src="//i.ytimg.com/vi/' + id + '/maxresdefault.jpg"><div class="play-button"></div>';
        }

        function labnolIframe() {
            var iframe = document.createElement("iframe");
            var autoplay = "&autoplay=1";
            if (this.parentNode.getAttribute("data-autoplay") == "false")
            {
                autoplay = "";
            }
            if (this.parentNode.getAttribute("data-playlistid") != null) {
