		<script type="text/javascript">
		function previous(cmnid)
		{
			var imageWidth1= $("#horiz_container"+cmnid+" > li:first").outerWidth(true);
			if($("#horiz_container"+cmnid).position().left < 0 && !$("#horiz_container"+cmnid).is(":animated")){
				$("#horiz_container"+cmnid).animate({left : "+=" + imageWidth1 + "px"});
			}
		}
		function next(cmnid)
		{
			var totalImages= $("#horiz_container"+cmnid+" > li").length,
			imageWidth= $("#horiz_container"+cmnid+" > li:first").outerWidth(true),
			totalWidth = imageWidth * totalImages,
			visibleImages = Math.round($("#gallery-wrap"+cmnid).width() / imageWidth),
			visibleWidth = visibleImages * imageWidth,
			stopPosition = (visibleWidth - totalWidth);
		  	if($("#horiz_container"+cmnid).position().left > stopPosition && !$("#horiz_container"+cmnid).is(":animated")){
				$("#horiz_container"+cmnid).animate({left : "-=" + imageWidth + "px"});
			}
		}
		$(document).ready(function(){
			for(var i=1;i<=2;i++)
			{
				var totalImages= $("#horiz_container"+i+" > li").length,
				imageWidth= $("#horiz_container"+i+" > li:first").outerWidth(true),
				totalWidth = imageWidth * (totalImages+1),
				visibleImages = Math.round($("#gallery-wrap"+i).width() / imageWidth),
				visibleWidth = (visibleImages+1) * imageWidth,
				stopPosition = (visibleWidth - totalWidth);
				$("#horiz_container"+i).width(totalWidth);
				$("#horiz_container"+i).bind('mousewheel', function(event, delta) {
				if(delta>0)
				{
				    next(1);
				}
				else
				{
				    previous(1);
				} 
				});
				$('.container').bind('mousewheel', function() {
				return false
				});
			}
		});
