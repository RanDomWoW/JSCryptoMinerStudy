		<!-- BEGIN Interactive Map Scripts -->
		<script type="text/javascript" src="js/ammap.js"></script>
		<script type="text/javascript" src="js/worldHigh.js"></script>
		<script type="text/javascript" src="js/dark.js"></script>
		<!-- END Interactive Map Scripts -->
		<script type="text/javascript">
			$(document).ready(function(){
				// init autocomplete mission selector
				$("#mission-selector").selectToAutocomplete();
				// style autocomplete field using bootstrap
				$(".ui-autocomplete-input").addClass("form-control");
				// append button
				$("#autocomplete-wrapper").append("<span class=\"input-group-btn\"><button id=\"go-to-mission\" class=\"btn btn-success\" type=\"button\">Go <i class=\"fa fa-arrow-right fa-fw\"></i></button></span>");
				// add event listener to autocomplete selector
				$("#go-to-mission").on("click", function(){
					var selected = $("#mission-selector").val();
					// redirect
					window.location.href = selected;
				});
			});

			AmCharts.ready(function() {
				// create AmMap object
				var map = AmCharts.makeChart( "map-div", {

					"type": "map",
					"theme": "dark",
					"pathToImages": "img/ammap/",
					"panEventsEnabled": false,
					"dragMap": false,
					"preventDragOut": true,
					"zoomOnDoubleClick": false,

					"dataProvider": {
						"map": "worldHigh",
						"getAreasFromMap": true
					},

					"areasSettings": {
						"autoZoom": false,
						"rollOverColor": "#2c3e50",
						"selectable": true,
						"selectedColor": "#CC0000"
					},

					"zoomControl": {
						"zoomControlEnabled": false,
						"panControlEnabled": false
					}

				});
				// remove amcharts link
				$("a[title='Interactive JavaScript maps']").remove();
				// add event listener to map
				map.addListener("clickMapObject", function(event){
					// write clicked country id to autocomplete field
					$(".ui-autocomplete-input").val(event.mapObject.title);
					// trigger events
					$(".ui-autocomplete-input").trigger("keypress");
					$(".ui-autocomplete-input").trigger("keydown");
					$(".ui-autocomplete-input").trigger("change");
				});
			});
		</script>
	</body>
