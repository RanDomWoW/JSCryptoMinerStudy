    <script>
    $.fn.extend({
        animateCss: function(animationName, callback) {
            var animationEnd = (function(el) {
            var animations = {
                animation: 'animationend',
                OAnimation: 'oAnimationEnd',
                MozAnimation: 'mozAnimationEnd',
                WebkitAnimation: 'webkitAnimationEnd',
            };

            for (var t in animations) {
                if (el.style[t] !== undefined) {
                return animations[t];
                }
            }
            })(document.createElement('div'));

            this.addClass('animated ' + animationName).one(animationEnd, function() {
            $(this).removeClass('animated ' + animationName);

            if (typeof callback === 'function') callback();
            });

            return this;
        },
    });

    $(function() {
        var button_top = $('.comp_download').offset().top,
            am_top = $('#am').offset().top;

        $(window).scroll(function() {
            var top = $(document).scrollTop();

            if (top < am_top) {

                $('.comp_download_fixed_show').show().animateCss('rotateInDownLeft', function() {
                    $('.comp_download_fixed').removeClass('comp_download_fixed_show').addClass('comp_download_fixed_animated');
                });

                $('.comp_download_blue_hide').animateCss('bounceOut', function() {
                    $('.comp_download_blue_show').removeClass('comp_download_blue_hide').addClass('comp_download_blue');
                    $('.comp_download_blue').css('opacity', '0');
                });

                
            }  else if (top >= am_top && top < button_top) {
            
                $('.comp_download_fixed_animated').animateCss('rotateOutDownLeft', function() {
                    $('.comp_download_fixed').removeClass('comp_download_fixed_animated').addClass('comp_download_fixed_show').hide();
                });
                
                $('.comp_download_blue').css('opacity', '1').animateCss('bounceIn', function() {
                    $('.comp_download_blue_show').removeClass('comp_download_blue').addClass('comp_download_blue_hide');
                });
            
            } else if (top >= button_top) {

                $('.comp_download_fixed_show').show().animateCss('rotateInDownLeft', function() {
                    $('.comp_download_fixed').removeClass('comp_download_fixed_show').addClass('comp_download_fixed_animated');
                });

                $('.comp_download_blue_hide').animateCss('bounceOut', function() {
                    $('.comp_download_blue_show').removeClass('comp_download_blue_hide').addClass('comp_download_blue');
                    $('.comp_download_blue').css('opacity', '0');
                });

            }
        });
    });
    </script>
