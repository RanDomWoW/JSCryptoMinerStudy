    <script type="text/javascript">
        function isElementInViewport(el) {

            //special bonus for those using jQuery
            if (typeof jQuery === "function" && el instanceof jQuery) {
                el = el[0];
            }

            var rect = el.getBoundingClientRect();

            return (
                rect.top >= 0 &&
                rect.left >= 0 &&
                rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) &&
                rect.right <= (window.innerWidth || document.documentElement.clientWidth)
            );
        }
    </script>
    <script type="text/javascript">
        function bindUIEvents() {
            $('#main-nav a').on('click', function(event) {
                if ($(this).attr('href') === '#memberarea') {
                    event.preventDefault();
                    window.location = 'https://my.gridinsoft.com/';
                    return true;
                }
                if ($(this).attr('href') === '#lang') {
                    event.preventDefault();
                    $(this).find('i').toggleClass('fa-caret-down fa-caret-up');
                    $(this).next().toggle();
                }
                if ($(this).attr('href') === '#fall') {
                    event.preventDefault();
                    $(this).next().toggle();
                }
                var el = $($(this).attr('href'));
                if (el.length !== 0) {
                    event.preventDefault();
                    $('html, body').animate({
                        scrollTop: el.offset().top - $('#main-header').outerHeight()
                    }, 750);
                    return true;
                }
                return false;
            });

            $(document).on('click', '.drop-down a', function(event) {
                $('.fall .drop-down').hide();
            });

            $(document).on('mouseleave', '.fall', function(event) {
                $('.fall .drop-down').hide();
            });

            $('#lang-switch-ua').on('click', function(event) {
                event.preventDefault();
                var pathname = window.location.pathname;

                if (pathname !== '/') {
                    window.location = 'https://gridinsoft.ua' + window.location.pathname;
                } else {
                    window.location = 'https://gridinsoft.ua/';
                }
            });

            $('#lang-switch-ru').on('click', function(event) {
                event.preventDefault();
                var pathname = window.location.pathname;

                if (pathname !== '/') {
                    window.location = 'https://ru.gridinsoft.com' + window.location.pathname;
                } else {
                    window.location = 'https://ru.gridinsoft.com/';
                }
            });

            $(window).on('scroll', function(event) {
                var navigation = $('#main-nav');
                var scrollPosition = $(document).scrollTop() + $('#main-header').outerHeight();
                var intro = $('#intro');
                var about = $('#about');
                var product = $('#product');
                var meet = $('#meet');
                var team = $('#team');
                var contact = $('#contact');

                switch (true) {
                    case (scrollPosition > intro.offset().top && scrollPosition < intro.offset().top + intro.outerHeight()):
                        navigation.find('a').removeClass('active-sect');
                        navigation.find("a[href='#intro']").addClass('active-sect');
                        break;
                    case (scrollPosition > about.offset().top && scrollPosition < about.offset().top + about.outerHeight()):
                        navigation.find('a').removeClass('active-sect');
                        navigation.find("a[href='#about']").addClass('active-sect');
                        break;
                    case (scrollPosition > product.offset().top && scrollPosition < product.offset().top + product.outerHeight()):
                        navigation.find('a').removeClass('active-sect');
                        navigation.find("a[href='#product']").addClass('active-sect');
                        break;
                    case (scrollPosition > contact.offset().top && scrollPosition < contact.offset().top + contact.outerHeight() || isElementInViewport(contact)):
                        navigation.find('a').removeClass('active-sect');
                        navigation.find("a[href='#contact']").addClass('active-sect');
                        break;
                    case (scrollPosition > meet.offset().top && scrollPosition < meet.offset().top + meet.outerHeight() + team.outerHeight()):
                        navigation.find('a').removeClass('active-sect');
                        navigation.find("a[href='#meet']").addClass('active-sect');
                        break;
                    default:
                        navigation.find('a').removeClass('active-sect');
                        break;
                }
            });
        }
    </script>
