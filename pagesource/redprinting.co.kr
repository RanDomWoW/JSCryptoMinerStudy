<script language="javascript">
	var slider_array = new Array();

	$(document).ready(function() {
	//메인메뉴 open img handle
	$('.bxslide').each(function(i){
		slider_array[i] = $(this).bxSlider({
			speed: 500,
			auto: true,
			controls: true,
			pagerSelector: '#main_slide_'+(i+1)+' >.slide_thumb'
		});

		$('#main_slide_'+(i+1)+' .prev').on('click', function() {
			slider_array[i].goToPrevSlide();
			return false;
		});
		$('#main_slide_'+(i+1)+' .next').on('click', function() {
			slider_array[i].goToNextSlide();
			return false;
		});
	});

		$('.nav-a, .nav-ul').on("mouseover",function(){
			$(".btn_slide").addClass("hide");
		});
		$('.nav-a, .nav-ul').on("mouseleave",function(){
			$(".btn_slide").removeClass("hide");
		});
		//
		$('.nav-li').on("mouseover",function(){
			var obj = $(this).find(".submenu-banner .main_banner_slide");
			if (obj.length==1){
				var obj_id = obj[0].id;
				if (obj_id!='')
				{
					var aId = obj_id.split("_");
					idx = aId[2];
					if (idx!='')
						slider_array[idx-1].reloadSlider();
					$(".btn_slide").addClass("hide");
				}
			}
		});

		// $('.best-menu').on("mouseover",function(){
		// 	$(".best-menu").removeClass("select");
		// 	$(this).addClass("select");
		// });
		// $('.best-menu').on("mouseleave",function(){
		// });

		$(".nav-ul .nav-li").has(".best-menu").each(function (index) {
			$(this).each(function (index) {
				$(this).bind("mouseenter focusin", function () {
					$(".best-menu").removeClass("active");
					$(this).find(".best-menu").addClass("active");
				}).bind("mouseleave focusout", function () {
					$(".best-menu").removeClass("active");
				});
			});
		});

		$('.nav-li').on("mouseleave",function(){
			$(".btn_slide").removeClass("hide");
		});



		$('.main-bottom-link').on("mouseover", function() {
			var btn_id = $(this)[0].id;
			if (btn_id!='')
			{
				$(".main-slide").removeClass("show").addClass("hide");
				var aId = btn_id.split("_");
				idx = aId[2];
				$(".main-slide-" + idx).removeClass("hide").addClass("show");
				var obj_id = $(".main-slide-" + idx)[0].id;
				$('.main-bottom-link').removeClass("active");
				$(this).addClass("active");
				if (obj_id!='')
				{
					var aId = obj_id.split("_");
					idx = aId[2];
					if (idx!='')
						slider_array[idx-1].reloadSlider();
				}
			}
		})
	});
</script>

