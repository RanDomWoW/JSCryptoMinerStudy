    <script type="text/javascript">
        $(document).ready(function () {
            var url = window.location.pathname;
            if (url.length == 1 || url.toLowerCase() == '/default.aspx') {
                $("#popup").animate({ "height": "40px" }, function () {
                    setTimeout(function () {
                        $("#popup").animate({ "height": "0px" })
                    }, 11000);
                });
            }

            $('img').bind('contextmenu', function (e) {
                e.preventDefault();
                return false;
            });

            $("ul").delegate("*", "focus blur", function (e) {

                var elem = $(this);
                setTimeout(function () {
                    elem.toggleClass("focused", elem.is(":focus"));
                }, 0);

            });

            $(document).on('keydown', 'a.focused', function (e) {
                var ev = e || event;
                var t = $(this);
                if (ev.keyCode == 13) {
                    if (t.parent('li').children().length != 1) {
                        t.parent('li').toggleClass('open');
                        $('li.open').length > 1 ? $('li.open').not(t.parent('li')).removeClass('open') : null;
                        e.preventDefault();
                        return false;
                    }
                }

            });
            $(document).on('click', function () { $('li.open').removeClass('open'); })

        });



    </script>



