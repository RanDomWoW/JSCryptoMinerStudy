        if (mode[0] == "GetProgressCount") {
            if (mode[1] != "false") {
                $('#progressdiv').html(mode[1]);
            }
        }
        if (mode[0] == "SendOTP") {
            if (mode[2] == "false") {
                alert("Reference number Not Found");
                return;
            }
            else {
                if (mode[1] == "false") {
                    alert("Valid Mobile Number Not Found");
                    return;
                }
                else {
                    debugger;
                    $('#Step1').css('display', 'none');
                    $('#step2').css('display', '')
                    $('#txtMob').val(mode[3]);
                    $('#txtMob').prop('disabled', true);
                    case_status = mode[4];
                }
            }
        }
        else if (mode[0] == "SendLoginOTP") {
            debugger;
            if (mode[1] == "true") {

                alert("OTP sent successfully on your Mobile");
                $('#txt_OTP').css('display', '');
                $('#txt_OTP').focus();
                $('#btn_SendOtp').css('display', 'none');
                var str = $('#span_tr1').html();
                $('#span_tr1').html('');
                $('#span_tr2').html(str);
                $('#txt_submitStatus').val('2');
                $('#txtUpass').val('');
            }
        }
    }
