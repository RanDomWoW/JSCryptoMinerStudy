                $('.rightSideBar').addClass('stickSideBarfixed').css('top', '0 !important');
              }
              else if (y >= footerTop - winHeight - gap ) {
                var rightSideheight = $('.rightSideBar').height();
                var leftSideheight = $('.leftSideBar').height();
                var mainHeight = $('.main').height(); 
                var mainTop = $('.main').offset().top;
                $('.leftSideBar').removeClass('stickSideBarfixed');
                $('.leftSideBar').addClass('stickMoveUp').css('top', mainTop + mainHeight - leftSideheight +'px');
                $('.rightSideBar').removeClass('stickSideBarfixed');
                $('.rightSideBar').addClass('stickMoveUp').css('top', mainTop + mainHeight - rightSideheight +'px'); 
              } 
              else    
              {
                var mainTop = $('.main').offset().top;
                $('.leftSideBar').removeClass('stickMoveUp');
                $('.leftSideBar').removeClass('stickSideBarfixed').css('top', mainTop +'px');
                $('.rightSideBar').removeClass('stickMoveUp');
                $('.rightSideBar').removeClass('stickSideBarfixed').css('top', mainTop + 'px');
                $('.rightSideBar').css('max-width', rightSidewidth +'px');
              }
            });
          }  
        });
        
        
		  
    });

      </script> 
