<script>
  jQuery(function($) {
    $('.first_digits, .second_digits').placeholder();
  
    $('.zip_code_search_form').submit(function(e) {
      var form = $(this);
      var zipCode = [
        form.find('.first_digits').val(),
        form.find('.second_digits').val()
      ].join('-');
  
      if (zipCode === '-') {
        zipCode = '';
      }
  
      form.find('.zip_code').val(zipCode);
    });
  });
