        tp.experience.execute();
    }

    function doPianoRequestUsername() {
        tags.push("tp_nickForgotten");
        tp.push(["setTags", tags]);
        tp.experience.execute();
    }

    function doPianoCallMyProfile() {
        tags.push("tp_myProfile");
        tp.push(["setTags", tags]);
        tp.experience.execute();
    }


