    <script type="text/javascript">

    // 头部导航颜色切换
    var header_back_transparent = function() {
        $(".header-fixed").css({"background":"rgba(0,0,0,0)"});
        $(".header-fixed").css({"border-bottom":"none"});
        $(".header-fixed li a").css({"color":"white"});
        $("#menu").css({"background":"rgba(0,0,0,0)"})
        $('.ymbLOGO').attr('src','/images/index/LOGO.png');
        $(".header li.active a").css({"border-bottom":"3px solid #FFF"});
        $(".danduLittle").css({"background-color":"#fff"});
        $(".headFreeSY").css({"border":"2px solid #fff"});
        $(".headFreeSY").css({"color":"#fff"});
    }
    var header_back_white       = function() {
        $(".header-fixed").css({"background":"white"});
        $(".header-fixed").css({"border-bottom":"1px solid rgba(43, 35, 35, 0.14)"});
        $(".header-fixed li a").css({"color":"#666"});
        $('.ymbLOGO').attr('src','/images/index/LOGO2.png');
        $(".header li.active a").css({"border-bottom":"3px solid #2b88fe"},{"color":"#2b88fe !important"});
        $(".header li.active a").css({"color":"#2b88fe"});
        $(".danduLittle").css({"background-color":"#333"});
        $(".headFreeSY").css({"border":"2px solid #2b88fe"});
        $(".headFreeSY").css({"color":"#2b88fe"});
    }

    $(function(){
        // 监控鼠标移动事件，控制顶部二级菜单的显示和隐藏
        $(document).mousemove(function (e) {
            var li1    = $('.headQBCP2');
            var li2    = $('.headGHSQ2');
            var li3    = $('.headGYWM2');
            var li4    = $('.headGRZX2');
            var li1_ul = $('.headQBCP2').find('ul');
            var li2_ul = $('.headGHSQ2').find('ul');
            var li3_ul = $('.headGYWM2').find('ul');
            var li4_ul = $('.headGRZX2').find('ul');
            var li1_x  = li1.offset().left - 10; // 位置向左扩展10px
            var li2_x  = li2.offset().left - 2;  // 位置向左扩展2px
            var li3_x  = li3.offset().left - 10;
            var li4_x  = 0;
            var li1_l  = li1.outerWidth() + 10;
            var li2_l  = li2.outerWidth() + 2;
            var li3_l  = li3.outerWidth() + 10;
            var li4_l = 0;
            if (li4.length) {
                var li4_x  = li4.offset().left;
                var li4_l  = li4.outerWidth();
            }
            var head_y = $('.header').outerHeight();
            var menu_y = head_y + $('.headBottom1').outerHeight();
            var back1 = $('.headBottom1');
            var back24  = $('.headBottom2');
            var back3  = $('.headBottom3');
            var x      = e.clientX;
            var y      = e.clientY;
            var isShow = false;
            var isShow1 = false;
            var isShow24 = false;
            var isShow3 = false;

            // 控制二级菜单显示/隐藏
            if (y < head_y) {
                if (x > li1_x && x < (li1_x + li1_l)) {
                    li1_ul.show();
                    li2_ul.hide();
                    li3_ul.hide();
                    li4_ul.hide();
                    isShow1 = true;
                }
                else if (x > li2_x && x < (li2_x + li2_l)) {
                    li1_ul.hide();
                    li2_ul.show();
                    li3_ul.hide();
                    li4_ul.hide();
                    isShow24 = true;
                }
                else if (x > li3_x && x < (li3_x + li3_l)) {
                    li1_ul.hide();
                    li2_ul.hide();
                    li3_ul.show();
                    li4_ul.hide();
                    isShow3 = true;
                }
                else if (x > li4_x && x < (li4_x + li4_l)) {
                    li1_ul.hide();
                    li2_ul.hide();
                    li3_ul.hide();
                    li4_ul.show();
                    isShow24 = true;
                }
                else {
                    li1_ul.hide();
                    li2_ul.hide();
                    li3_ul.hide();
                    li4_ul.hide();
                    back1.hide();
                    back24.hide();
                    back3.hide();
                }
            } else {
                if (y > menu_y) {
                    li1_ul.hide();
                    li2_ul.hide();
                    li3_ul.hide();
                    li4_ul.hide();
                }
            }
            //控制白色背景显示/隐藏
            if (y < menu_y) { // 显示
                if (isShow1) {
                    back1.show();
                    back24.hide();
                    back3.hide();
                }
                else if (isShow24) {
                    back1.hide();
                    back24.show();
                    back3.hide();
                }
                else if (isShow3) {
                    back1.hide();
                    back24.hide();
                    back3.show();
                }
            } else { // 隐藏
                back1.hide();
                back24.hide();
                back3.hide();
            }
        });

        // 顶部菜单栏显示
        var _url = window.location.href.split('/');

        $(window).scroll(function(){
            var height=$("#yc").offset().top-$(window).scrollTop();
            if(height < "2"){ //　往下滚动
                header_back_white();
            }else{　//　顶部
                if ((_url[3] == 'index' && _url[4] == 'index') || _url[3] == '') { // 首页，变透明
                    var block1 = $('.headBottom1').css('display');
                    var block2 = $('.headBottom2').css('display');
                    var block3 = $('.headBottom3').css('display');
                    if (block1 == 'block' || block2 == 'block' || block3 == 'block') {
                        header_back_white();
                    } else {
                        header_back_transparent();
                    }
                }else {
                    header_back_white();
                }
            }
        });

        if ((_url[3] == 'index' && _url[4] == 'index') || _url[3] == '') {
            // 不显示
        } else {
            header_back_white();
        }

        $('#page-container').click(function(){
            $('#menu').collapse('hide');
        });
        // 鼠标移动事件
        $(".header-fixed .navbar-collapse").mouseover(function(){
            header_back_white();
		}).mouseout(function(){
			// $(".header-fixed").css({"background":"rgba(0,0,0,0)"});
            // $(".header-fixed li a").css({"color":"white"});
		});


    })

    </script>

