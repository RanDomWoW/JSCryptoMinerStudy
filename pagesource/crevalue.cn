<script type="text/javascript">
  var currentPgNumCookieName = 'currentPgNum';
  var lastPgNumCookieName = 'lastPgNum';
  // 预计每秒步长
  var preStep = 0.6;

  $(document).ready(function(){
    // 初始化
    $.getJSON("http://api.creprice.cn/v1/rest/comdata/getavmlog?apiKey=e88befb3d3a4d0882c6b957715f52d8f&pagesize=10&datareturn=?",
      function (json) {
        var currentPgNum = json.data.count;
        htmlPgNum = currentPgNum - parseInt(preStep * 10);
        setHtmlPgNum(htmlPgNum);

        document.cookie = lastPgNumCookieName + "=" + htmlPgNum;
        document.cookie = currentPgNumCookieName + "=" + currentPgNum;
      }
    );

    updatePgNum();
  });

  var times = 1;
  // 更新评估数
  function updatePgNum(){

    var time = (times++) % 10;
    // 获取下一秒要增加的评估数
    var nextStep = getNextStep(time);
    // 更新评估数量
    htmlPgNum = getHtmlPgNum();
    setHtmlPgNum(parseInt(htmlPgNum) + parseInt(nextStep));

    setTimeout('updatePgNum()' , 1000);
  }

  // 取下一秒要增加的评估数
  function getNextStep(time){

    var currentPgNum = getCookie(currentPgNumCookieName);
    var lastPgNum = getCookie(lastPgNumCookieName);
    
    if(time){
      
      return enNextStep(currentPgNum , lastPgNum);
    // 10s刷新一次当前评估数
    } else {
      $.getJSON("http://api.creprice.cn/v1/rest/comdata/getavmlog?apiKey=e88befb3d3a4d0882c6b957715f52d8f&pagesize=10&datareturn=?",
        function (json) {

          var currentPgNumNew = json.data.count;
          // console.log("接口评估数量:" + currentPgNumNew);
          document.cookie = lastPgNumCookieName + "=" + currentPgNum;
          document.cookie = currentPgNumCookieName + "=" + currentPgNumNew;
        }
      );
      
      var htmlPgNum = parseInt(getHtmlPgNum() , 10);
      return (currentPgNum - htmlPgNum) >= 0 ? (currentPgNum - htmlPgNum): 0;
    }
  }

  // 根据当前评估数与10s前评估数生成下一秒要增加评估数
  function enNextStep(currentPgNum , lastPgNum){

    var htmlPgNum = getHtmlPgNum();
    // 显示的评估数大于当前评估数，停止
    if(htmlPgNum >= currentPgNum)
      return 0;

    // 评估数量大约3s增加1，将lastPgNum默认为当前减3
    if(0 == lastPgNum) lastPgNum = currentPgNum - preStep * 10;

    var step = parseInt(currentPgNum) - parseInt(lastPgNum);
    // 每秒平均步长
    secondStep  = parseInt(step / 10);
    // 取余
    residueStep = parseInt(step % 10);
    var nextStep = secondStep;
    // 若随机数小于余数则将步长加1
    if(Math.round(Math.random()*10) < residueStep){
      nextStep++;
    }

    return nextStep; 
  } 

  function getCookie(name){
    var strCookie=document.cookie;
    var arrCookie=strCookie.split("; ");
    for(var i=0;i<arrCookie.length;i++){
      var arr=arrCookie[i].split("=");
      if(arr[0]==name)return arr[1];
    }
    // 默认1
    return 0;
  } 
  // 获取页面中的评估数量
  function getHtmlPgNum(){

    var htmlPgNum = '';
    for(i = 8 ; i > -1 ; i--){
      htmlPgNum += $("#pgNum_" + i).text();
    }
    return parseInt(htmlPgNum , 10);
  }
  // 设置页面中的评估数量
  function setHtmlPgNum(htmlPgNum){
    //console.log("设置评估数为：" + htmlPgNum);
    var htmlPgNum = htmlPgNum + "",
      count = htmlPgNum.length;
    for(var i = 0; i < count; i++) {
      $("#pgNum_" + i).text(htmlPgNum.charAt(count -1 - i));
    }
  }
</script>
