<script type="text/javascript">
jQuery.fn.slowEach = function(interval, callback, callback2) {
	var items = this, i = 0;
	if(!items.length) return;
	function next() {
		(callback.call(items[i], i, items[i]) !== false && ++i < items.length) ? setTimeout(next, interval) : callback2 && callback2.call(items, i, items);
	}
	next();
};
(function($){
	$(function(){
		if (typeof($(".icon-love")) !== 'defined') {
			$('.icon-love').click(function(e){
					e.preventDefault();
					var id = $(this).attr('rel');
					var ele = $(this);
					$.get('user.php?act=collect',{id:id},function(data) {
						alert(data.message);
						if (!data.error) {
							ele.addClass('loved');
						}			
					},'json');
			});
		}
		
		if (typeof($(".rating")) !== 'defined') {
			$(".rating").children().not(":radio").hide();	
			// Create stars
			$(".rating").each(function(){
				var ele = $(this);
				var goods_id = ele.attr('data-id');
				var old_avg = ele.attr('data-avg');
				var other = $('.rating[rel='+goods_id+']').not(this);
				ele.stars({
				cancelShow: false,
				callback: function(ui, type, value)
				{
					var arr = ui.$stars.find("a");
					var avg = ele.next().next().find(".avg");
					//var votes = ele.next().find(".votes");
					var loader = ele.next().find(".loader");
					arr.slowEach(100, 
						function(){ $(this).animate({top: "28px"}, 300) }, 
						function()
						{
							loader.fadeIn(function()
							{
								$.post("rating.php", {rate: value,id:goods_id,old_avg: old_avg}, function(db)
								{
									if (db.reviewed) {
										alert('此項商品您已評過了！');
										ui.selectID(db.old_avg-1);
										loader.fadeOut(function(){ 
											arr.slowEach(100, function(){ $(this).animate({top: 0}, 300) });
										});
										return false;
									}
									ui.select(Math.round(db.avg));
									avg.text(db.avg);
									//votes.text(db.votes);
									loader.fadeOut(function(){ 
										arr.slowEach(100, function(){ $(this).animate({top: 0}, 300) });
									});
	if (other.length > 0) {
	other.parent().html(ele.parent().html()).find('.loader').fadeOut();
	}
	alert('感謝您的評分！');
								}, "json");
							});
						});
				}
			});
			});
		}
		
	});
})(jQuery)
