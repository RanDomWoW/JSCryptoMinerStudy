	function fillInputFormats(selectedFormat){
		//add options in the list
		for (var i=0; i < input_formats.length; i++) {
			var optionToAdd = new Option(file_ext[input_formats[i]], input_formats[i]);
			document.forms[0].input_format.options[i] = optionToAdd;
			if (file_ext[input_formats[i]].match(/>>>>$/)) document.forms[0].input_format.options[i].className = "mc-formatin-h";
			if (input_formats[i] == selectedFormat){
				document.forms[0].input_format.options[i].selected = true;
				document.forms[0].storedOpt.value = i;
			}
		}
	}

	function fillOutputFormats(inputFormat){
		//reset the list to empty one
		for (var k = document.forms[0].output_format.options.length; k >=0; k--) {
			document.forms[0].output_format.options[k] = null;
		}
		//add options in the list	
		//alert("hi " + inputFormat);
		var previousFormat = '';
		for (var i=0; i < output_formats[inputFormat].length; i++) {
		//	alert(file_ext[output_formats[inputFormat][i]]);
			var optionToAdd = new Option(file_ext[output_formats[inputFormat][i]], output_formats[inputFormat][i]);
			document.forms[0].output_format.options[i] = optionToAdd;
			if (output_formats[inputFormat][i] == "."){
				document.forms[0].output_format.options[i].selected = true;
			}

			if (file_ext[output_formats[inputFormat][i]].match(/>>>>$/)) document.forms[0].output_format.options[i].className = "mc-formatin-h";
			if (output_formats[inputFormat][i] == ".eao" || output_formats[inputFormat][i] == ".eaw" || output_formats[inputFormat][i] == ".tmb" || output_formats[inputFormat][i] == ".agif" || output_formats[inputFormat][i].match(/^\.ocr_/) ) document.forms[0].output_format.options[i].className = "eao";
		//  }
		}
	}

	function checkExtension(isOnFileUploadChange) {
		var AllowedExtensionFlag = 0;
		var pathname = document.forms[0].uploadedfile.value;
		//alert(pathname);
		var ext;
		if (pathname.match(/([^\/\\]+)(\.tar\.gz)$/) ) {
			ext = RegExp.$2;
			//alert(ext);
		}else{
			if (pathname.match(/([^\/\\]+)(\.\w+)$/) ) ext = RegExp.$2;
		}
		//alert(ext);
		if(ext != undefined) {
			ext = ext.toLowerCase();
			for (var i=0; i < input_formats.length; i++) {
				if (input_formats[i] == ext) AllowedExtensionFlag = 1;
			}
		}
		if ( AllowedExtensionFlag == 0) {
			alert("File type not supported !");
			document.forms[0].uploadedfile.value = "";
			return false;
		}else{
			if (isOnFileUploadChange) fillInputFormats(ext);
			if (isOnFileUploadChange) fillOutputFormats(ext);
			return true;
		}
	}

	function checkExtensionFromURL(isOnURLChange, isOnInputEventMozilla) {
		var AllowedExtensionFlag = 0;
		var pathname = document.forms[0].download_url.value;
		var ext;

		if(input_formats.indexOf(".html") < 0) input_formats.push(".html"); //from html to pdf only

		if (pathname.match(/([^\/\\]+)(\.tar\.gz)$/) ) {
			ext = RegExp.$2;
			//
		}else{
			//if (pathname.match(/([^\/\\]+)(\.\w+)$/) ) ext = RegExp.$2;
			var file_part;
			if (pathname.match(/([^\/]*\.[^\/]*?)$/) ) file_part = RegExp.$1;
	//		alert(file_part);
			if(file_part != undefined && file_part != '') ext = file_part.split('.').pop();
			if(ext != undefined ) {
				ext = ext.split(/\#|\?/g)[0];
				ext = '.' + ext;
			}
		}
		if (pathname.match(/xhamster\.com\/videos\//)) ext = ".flv";
		if (pathname.match(/youjizz\.com\/videos\/.*?\.html/)) ext = ".flv";
		if (pathname.match(/v\.youku\.com\/v_show\/id_.*?\.html/)) ext = ".flv";
		if (pathname.match(/ku6\.com\/.*?\.html/)) ext = ".flv";
		if (pathname.match(/56\.com\/.*?\.html/)) ext = ".flv";
		if (pathname.match(/break\.com\/.+/)) ext = ".flv";
		if (pathname.match(/slutload\.com\/video\/.*?/) ) ext = ".flv";
		if (pathname.match(/empflix\.com\/.*?video\d+/) ) ext = ".flv";
		if (pathname.match(/dailymotion\.com\/.*?video\/.*?\#/) ) ext = ".flv";
		if (pathname.match(/xtube\.com\/.*?watch/) ) ext = ".flv";
		if (pathname.match(/pornhub\.com\/view_video\.php\?viewkey=/) ) ext = ".mp4";

		if (pathname.match(/youtube\.com\/watch\?v=/) || pathname.match(/vimeo\.com\/\d+/)){
			ext = ".mp4";	
			if (isOnInputEventMozilla) output_formats[".mp4"].splice(output_formats[".mp4"].length - 3, 0, ".mp4");
			document.getElementById('video_resolution_list').style.display = 'block';
			document.getElementById('video_resolution').style.display = 'block';
		}else{
			document.getElementById('video_resolution_list').style.display = 'none';
			document.getElementById('video_resolution').style.display = 'none';
			document.forms[0].youtube_mode.value = 'default';
		}

	//	alert(ext);
		if(ext != undefined) {
			ext = ext.toLowerCase();
			for (var i=0; i < input_formats.length; i++) {
				if (input_formats[i] == ext) AllowedExtensionFlag = 1;
			}
		}else{
		//	alert(pathname); spankwire.com/Sonia-Red-Black-Snake-Owns/video189917/   || pathname.match(/empflix\.com\/.*?video\d+/)  || pathname.match(/vbox7\.com\/play:.*?/)
			if (pathname.match(/hulu\.com\/watch\/\d+\/.*/) || pathname.match(/spankwire\.com\/.*?\/video\d+/) || pathname.match(/tnaflix\.com\/.*?video\d+/) || pathname.match(/youporn\.com\/watch\/\d+\//) || pathname.match(/redtube\.com\/\d+/) || pathname.match(/dailymotion\.com\/.*?video\//) || pathname.match(/tube8\.com\//) || pathname.match(/metacafe\.com\/watch\/.*/) ||  pathname.match(/xvideos\.com\/video\d+\//) || pathname.match(/veoh\.com\/.*?watch\/.+?/) || pathname.match(/xnxx\.com\/video-/) || pathname.match(/nicovideo\.jp\/watch\/.*?\d+/) || pathname.match(/liveleak\.com\/view\?i=/) || pathname.match(/hardsextube\.com\/video\/.*?/)  ) { 
				AllowedExtensionFlag = 1;
				ext = ".flv";
			}
			if (pathname.match(/vimeo\.com\/\d+/)) {
				AllowedExtensionFlag = 1;
				ext = ".mp4";
			}	
		}

		//alert("Flag " + AllowedExtensionFlag);
		if ( AllowedExtensionFlag == 0) {
//			alert("File type not supported !");
//			document.forms[0].download_url.value = "";
//			previousValueLength = 0;
//			return false;
			ext = ".html";
			if (isOnURLChange) fillInputFormats(ext);
			if (isOnURLChange) fillOutputFormats(ext);
			return true;
		}else{
			//alert(ext);
			if (isOnURLChange) fillInputFormats(ext);
			if (isOnURLChange) fillOutputFormats(ext);
			return true;
		}
	}

