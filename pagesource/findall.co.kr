<script type="text/javascript">
  function mainNumSlider(param) {
    var param = param;
	if ($(param+' > .cont > .box').length % 2 != 0){
		//alert($(param+' > .cont > .box').length);
		$(param+' > .cont > .box').parent().append('<div class="box"></div>');
	}
    var target = $(param + ' > .cont > .box');
    var targetLength = $(target).length;
    var toggleIndex = 1;
    $(param + ' .paging .page_num span').html(targetLength / 2);
    $(target).eq(toggleIndex - 1).addClass('show');
    $(target).eq(toggleIndex).addClass('show');
    $(param + ' .paging .btn_prev').click(function () {
      $(target).removeClass('show');
      if (toggleIndex == 1) {
        toggleIndex = targetLength / 2;
        $(target).eq(toggleIndex * 2 - 1).addClass('show');
        $(target).eq(toggleIndex * 2 - 2).addClass('show');
        $(param + ' .paging .page_num strong').html(toggleIndex);
      } else {
        toggleIndex = toggleIndex - 1;
        $(target).eq(toggleIndex * 2 - 1).addClass('show');
        $(target).eq(toggleIndex * 2 - 2).addClass('show');
        $(param + ' .paging .page_num strong').html(toggleIndex);
      }
  });
    $(param + ' .paging .btn_next').click(function () {
      $(target).removeClass('show');
      if (toggleIndex == (targetLength / 2)) {
        toggleIndex = 1;
        $(target).eq(0).addClass('show');
        $(target).eq(1).addClass('show');
        $(param + ' .paging .page_num strong').html(toggleIndex);
      } else {
        toggleIndex = toggleIndex + 1;
        $(target).eq(toggleIndex * 2 - 1).addClass('show');
        $(target).eq(toggleIndex * 2 - 2).addClass('show');
        $(param + ' .paging .page_num strong').html(toggleIndex);
      }
    });
  }
</script>
