<script>
/* 자동완성 포커싱 처리용 변수*/
var focustarg1 = 0 ;
var focustarg2 = 0 ;
var focustarg3 = 0 ;

var firstbannerflag = "1";
var firstbannerLink ="http://www.naver.com"
var recentHTML ="" ;	// 최근 검색어 HTML (count 및 )
var tSInterval=0; // 포커싱 처리용

$("#tf_keyword").keyup(function(){
	if(getCookie("autocomplete")=="off"){ $("#autocomp").empty().html("");return false;}
	var searchval;
	var targeturl;
	searchval = $(this).val();
	if(searchval == "" && recentHTML != "" ) { 
		$("#findallSuggestBox").show();
		$("#autocomp").empty().html(recentHTML);
		return false;
	}
	
	var section = $("#totalsection").val();
	if(searchval == ""  ) targeturl ="/ksearch/ajax/recent.asp?section=" + section;
	else targeturl = "/ksearch/ajax/autocomplete.asp?query=" + searchval + "&section=" + section;
	//if(searchval == "" && recentHTML !="" ){$("#autocomp").empty().html(recentHTML);  return false;}
	$.ajax({
  	type: "post",
  	async: false,
  	url: targeturl,
  	dataType: "html",
  	success: function (data) { 
		if($.trim(data)==""){searchpopular();}
		else{
			$("#findallSuggestBox").show();
			$("#autocomp").empty().html(data);
			if(searchval == "" ){ setTimeout("recentCount()",100);}
		}

	}
  });

})

$("#tf_keyword").focus(function(){
	focustarg1 = 1;
	if(firstbannerflag == 1){
		firstbannerflag = 0;
		$("#tf_keyword").val("");
	}
	$("#tf_keyword").keyup();
});



$("#tf_keyword").blur(function(){
focustarg1 = 0;	
});


function recentCount(){
	$(".recent").each(function(){
		var ThisId;
		var param;
		var url;
		ThisId = $(this).attr("id");
		param = $(this).attr("param");
		url  = "/ksearch/ajax/count.asp?" + param
		$(this).show();
			$.ajax({
			type: "post",
			async: true,
			url: url,
			dataType: "html",
			success: function (data) { 
					$("#" + ThisId + "count").html(data);
					recentHTML = $("#autocomp").html();
			}
		  });

	});


}

function searchpopular(){
	var searchval;
	$.ajax({
		type: "post",
		async: false,
		url: "/ksearch/ajax/popular.asp",
		dataType: "html",
		success: function (data) { 
			$("#findallSuggestBox").show();
			$(".box_mysearch").hide();
			$("#box_hotsearch").hide();
			$("#autocomp").empty().html(data);
		}
	  });

}

/* 자동완성??*/
$('#tf_keyword').keydown(function(event) {
	if (event.which == 40) {
		focustarg2 = 1;
		$('#findallSuggestBox').children().children().find('a').eq(0).focus()
		return false;
	}
	else	if (event.which == 13) {
		$("#searchSubmit").click();
	}

});

$("#findallSuggestBox").delegate("a", "keydown", function(){
	if (event.which == 13) {// 엔터키 누를시
		location.href=$(this).attr("href");
		return false;
	}
		focustarg2 = 1;
	if (event.which == 38) {// 상위 화살표 누를시
		var target = $(this).parent().prev('li').children('a');
		if (target.length == 0)	target = $(this).parent().parent().find('a').last();
	}
	if (event.which == 40) {// 하위 화살표 누를시
		var target = $(this).parent().next('li').children('a');
		if (target.length == 0)		target=$(this).parent().parent().find('a').first();
	}
	if (event.which == 38 || event.which == 40 ) {// 상위 화살표 누를시
		$('#tf_keyword').val($(target).attr("keyword"))
		$(target).focus();
		focustarg2 = 1;

		return false;
	}
	if (event.which == 37 || event.which == 39 || event.which == 8) {// 좌우 눌렀을때
		$('#tf_keyword').focus();
		return false;
	}

});


$("#findallSuggestBox, #autocomp").delegate("a", "blur", function(){
	focustarg2 = 0;
});

$("#wrapSearch").blur(function(){
	$("#finda llSuggestBox").hide();
});

function totalSearchFocus(){
	if(tSInterval==0) tSInterval = setInterval(tSsearchChk,200)
}
function tSsearchChk(){
	if(focustarg1 ==0 && focustarg2 ==0){
		clearInterval(tSInterval);
		$("#findallSuggestBox").hide();
		tSInterval = 0 ;
	}
}

$("#autocomp, .box_suggest_opt").delegate("a", "mouseover", function(){
	focustarg2 = 1;
});

$("#autocomp, .box_suggest_opt").delegate("a", "mouseout", function(){
	focustarg2 = 0;
});

function SearchHistoryDel(){
	if(confirm("검색이력을 삭제하시겠습니까?")){
		$.ajax({
		type: "post",
		async: false,
		url: "/ksearch/ajax/ajax_search_user_cookies_del.asp",
		dataType: "html",
		success: function (data) { 
			recentHTML = "";
			$("#tf_keyword").keyup();//팝업 삭제?
		}
	  });
	}
}

function SearchSaveToggle(){
	if (getCookie("searchautosave")=="off")
	{
		setCookie("searchautosave", "on", 365,"findall.co.kr");
	}
	else{
		setCookie("searchautosave", "off", 365,"findall.co.kr");
	}
	setSearchSaveChk();
}

function SearchAutoCompleteToggle(){
	if (getCookie("autocomplete")=="off")
	{
		setCookie("autocomplete", "on", 365,"findall.co.kr")
	}
	else{
		if(confirm("검색어 자동완성, 통합자동검색 기능을 중지합니다.\n사용을 원하실경우 검색창내 ▼버튼을 클릭하세요.")){
			$("#autocomp").html("");
			setCookie("autocomplete", "off", 365,"findall.co.kr");
			$("#findallSuggestBox").hide();
			recentHTML = "";

		}
	}
	$("#tf_keyword").focus();//팝업 삭제?
	setAutocompleteChk();
}

function setAutocompleteChk(){
	if(getCookie("autocomplete")=="off")
		$('#IdSearchAutoCompleteToggle').html("자동완성 켜기")
	else
		$('#IdSearchAutoCompleteToggle').html("자동완성 끄기")
}
function setSearchSaveChk(){
	if(getCookie("searchautosave")=="off")
		$('#IdSearchSaveToggle').html("검색어 저장 켜기")
	else
		$('#IdSearchSaveToggle').html("검색어 저장 끄기")
}
firstbannerflag = 0;
$("#searchSubmit").click(function(){
	if (firstbannerflag==1)
	{
		window.open(firstbannerLink);
//		document.location.href=firstbannerLink;
		return false;
	}
	if($.trim($("#tf_keyword").val()).length<2){
		alert("검색어는 최소 2글자 이상입니다");
		$("#tf_keyword").focus();
		return false;
	}
	document.topsearch.submit();
	return false;

});

function changeSection(val){
	$("#totalsection").val(val);
}
setAutocompleteChk();
setSearchSaveChk();
</script>
