            var less = block.find(".less");
            more.on('click', function (e) {
                e.preventDefault();
                blockH.css({
                    height: "auto"
                })
                more.css({
                    display: 'none'
                })
                less.css({
                    display: 'inline-block'
                })
            })
            less.on('click', function (e) {
                e.preventDefault();
                blockH.css({
                    height: "200px"
                })
                less.css({
                    display: 'none'
                })
                more.css({
                    display: 'inline-block'
                })
            })
        }
        $(document).ready(function () {
            textMore();
        })
