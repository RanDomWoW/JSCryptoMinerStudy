<script type="text/JavaScript">
	function setLayer(type,price,cnt){
		if(type=="card"){
			$("#fPriceC").text(kmSetComma(price)+"원");
			$("#fCntC").text(kmSetComma(cnt)+"명");
		} else{
			var type = $("#fPayType");
			var name = $("#fPayName");
			$("#fCard").hide();
			$("#fBank").show();
			$("#fPriceB").text(kmSetComma($(":radio[name=fPayPrice]:checked").val()));
			$("#fNameB").text(name.val());
		}
		$("#fPercent").css("width",((cnt*1)/1000)*100);
		$("#fPercent2").text(kmSetComma(((cnt*1)/1000)*100)+"%");
	}
	function chkForm(){
		var type = $("#fPayType");
		var name = $("#fPayName");
		if(type.val()==""){
			alert("결제 방법을 선택해주세요");
			type.focus();
		} else if(type.val()=="bank" &&name.val()==""){
			alert("입금자명을 선택해주세요");
			name.focus();
		} else{
			if(type.val()=="card"){
				var myForm = document.funding;
				var url = "/payment/funding/pay.php";
				window.open("" ,"funding", 
				"toolbar=no, width=540, height=467, directories=no, status=no,    scrollorbars=no, resizable=no"); 
				myForm.action =url; 
				myForm.method="post";
				myForm.target="funding";				
				myForm.submit();
			} else{
				var data = "name="+name.val()+"&price="+$(":radio[name=fPayPrice]:checked").val();
				$.ajax({
					type: "POST",
					url: "../process/funding.php",
					data: data,
					success: function(data){
						if(data!="0"){
							setLayer("bank","",data);
							crw(3);
						} else{
							alert("결제 실패하였습니다.");
							crw(2);
						}
					}
				});
			}
		}
	}
	function crw(n) {
		if(""==""){
			alert("후원은 회원만 가능합니다.");			
		} else{
			for(var i = 1; i < 4; i++) {
				obj = document.getElementById('crowd_box'+i);						
				if ( n == i ) {
					obj.style.display = "block";
				} else {
					obj.style.display = "none";
				}
			}
		}
	}
	/////##### 숫자 콤마 붙이기 #####/////
	function kmSetComma(n){
		var reg = /(^[+-]?\d+)(\d{3})/;
		n += '';
		while (reg.test(n)){
			n = n.replace(reg, '$1' + ',' + '$2');
		}
		
		return n;
	}
	function payTypeChk(obj){
		if(obj.value=="bank"){
			$("#fBankName").show();
		} else{
			$("#fBankName").hide();
		}
	}
</script>

