 function shareSrc(){
	 if(""==""){
		 alert("로그인 후 추천해주실 수 있습니다.");
	 } else{
		 if($("#shareUrl").val()==""){
			 alert("공유하실 사이트 URL을 입력해주세요");
			$("#shareUrl").focus();
		 } else{
			 var src = $("#shareSrc").val();
			 var url = $("#shareUrl").val();
			 var data = "sSrc="+src+"&sUrl="+url;
			$.ajax({
				type: "POST",
				url: "process/shareSrc.php",
				data: data,
				success: function(data){
					if(data=="1"){
						alert("정보가 공유되었습니다.");
						$("#shareSrc").val("free");
						$("#shareUrl").val("");
					} else{
						alert("공유 실패하였습니다.");
						$("#shareSrc").val("free");
						$("#shareUrl").val("");
					}
				}
			});
		 }
	 }
 }
