	function toggle_reply_mode(no)
	{
		//comment_reply_0
		document.getElementById("comment_view_" + no).style.display  = "block";
		document.getElementById("comment_modify_" + no).style.display  = "none";
		document.getElementById("comment_delete_" + no).style.display = "none";
		document.getElementById("comment_modify_ask_" + no).style.display  = "none";
		document.getElementById("comment_modify_edit_" + no).style.display  = "none";

		if (document.getElementById("comment_reply_" + no).style.display == "none")
		{
			document.getElementById("comment_reply_" + no).style.display  = "block";
		}
		else
		{
			document.getElementById("comment_reply_" + no).style.display  = "none";
		}
	}

	function toggle_edit_mode(no, mode)
	{
		if (document.getElementById("comment_view_" + no).style.display == "none")
		{
			document.getElementById("comment_view_" + no).style.display  = "block";
			document.getElementById("comment_modify_" + no).style.display  = "none";
			document.getElementById("comment_delete_" + no).style.display = "none";
			document.getElementById("comment_modify_ask_" + no).style.display  = "none";
			document.getElementById("comment_modify_edit_" + no).style.display  = "none";
			document.getElementById("comment_reply_" + no).style.display  = "none";
		}
		else
		{
			document.getElementById("comment_view_" + no).style.display  = "none";
			document.getElementById("comment_modify_" + no).style.display  = "block";
			document.getElementById("comment_delete_" + no).style.display = "none";
			document.getElementById("comment_reply_" + no).style.display  = "none";

			if (mode == "0") // non member
			{
				document.getElementById("comment_modify_ask_" + no).style.display  = "block";
				document.getElementById("comment_modify_edit_" + no).style.display  = "none";
			}
			else
			{
				document.getElementById("comment_modify_ask_" + no).style.display  = "none";
				document.getElementById("comment_modify_edit_" + no).style.display  = "block";
			}

		}
	}

