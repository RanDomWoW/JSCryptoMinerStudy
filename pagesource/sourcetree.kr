	<Script Type = "Text/JavaScript">
		function write_post_search()
		{
			new daum.Postcode({
				oncomplete: function(data) {
					var addr = data.address.replace(/(\s|^)\(.+\)$|\S+~\S+/g, '');
					document.getElementById('cp_post').value = data.postcode1 + "-" + data.postcode2;
					document.getElementById('cp_addr1').value = addr;
					document.getElementById('cp_addr2').focus();
				}
			}).open();

		}

		function write_recruit()
		{
			// Field Vaild chk
			if (document.getElementById('cp_type').value == "")
			{
				alert("업종을 선택해주세요");
				return false;
			}

			if (document.getElementById('cp_type').value == "직접입력" && document.getElementById('wr_direct').value.replace(" ","") == "")
			{
				alert("업종을 입력해주세요");
				return false;
			}

			if (document.getElementById('cp_name').value == "")
			{
				alert("회사명을 입력해주세요");
				return false;
			}


			if (document.getElementById('wr_subject').value == "")
			{
				alert("구인광고 제목을 입력해주세요");
				return false;
			}

			if (document.getElementById('cp_post').value == "" || document.getElementById('cp_addr1').value == "")
			{
				alert("회사위치를 입력해주세요");
				return false;
			}

			
			if (document.getElementById('cp_url').value == "")
			{
				alert("구인정보 URL을 입력해주세요");
				return false;
			}

			/*document.getElementById('no_card').style.display = "none";
			document.getElementById("step1").style.display = "none";
			document.getElementById("step2").style.display = "block";*/

		}

	</Script>


