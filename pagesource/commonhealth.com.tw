<script>
$(document).ready(function(){

	// hide #back-top first
	$("#back-top").hide();
	
	// fade in #back-top
	$(function () {
		$(window).scroll(function () {
			if ($(this).scrollTop() > 100) {
				$('#back-top').fadeIn();
			} else {
				$('#back-top').fadeOut();
			}
		});

		// scroll body to 0px on click
		$('#back-top a').click(function () {
			$('body,html').animate({
				scrollTop: 0
			}, 800);
			return false;
		});
	});

});
</script>

<script type="text/javascript">
	function doFormSubmit(formId){
		$("#" + formId).submit();
	}
	
	var publishDateQueryHtml = $('select[name="publishDateQuery"]').html();
	
	function chgUnitQuery($this){
		
		var unit = $($this).val();
		// 子資料類型條件
		$('#subUnitQueryDiv').html('');
		$('#subUnitQueryDiv').show();
		var optionHtml = "";
		if(unit == 'article'){
			optionHtml+="<option value='A'>全文開放</option>";
			optionHtml+="<option value='B'>訂戶專屬</option>";
		}else if(unit == 'mediaInfo'){
			optionHtml+="<option value='video'>影音</option>";
			optionHtml+="<option value='ppt'>PPT</option>";
		}else if(unit == 'personalHealth'){
			optionHtml+="<option value='consult'>線上諮詢</option>";
			optionHtml+="<option value='testSelf'>自我檢測</option>";
		}else if(unit == 'activityType'){
			optionHtml+="<option value='investigate'>康健大調查</option>";
			optionHtml+="<option value='activity'>活動/講座/讀書會</option>";
		}
		if(optionHtml == false){
			$('#subUnitQueryDiv').hide();
		}else{
			$("#subUnitQueryDiv").removeAttr("class");
			$("#subUnitQueryDiv").html("<select name=\"subUnitQuery\" id=\"drugstoreArea_12\" class=\"width_210\" onchange=\"chgSubUnitQuery(this)\">" + optionHtml + "</select>");
			$("#subUnitQueryDiv").jqTransform();
			$("#subUnitQueryDiv").find('.jqTransformSelectWrapper').css('z-index','87');
		}
		// 日期條件
		$('#publishDateQueryDiv').show();
		$('#publishDateQueryDiv').html('');
		if(unit == 'food'){
			$('#publishDateQueryDiv').hide();
		}else if(unit == 'body'){
			$('#publishDateQueryDiv').hide();
		}else if(unit == 'search'){
			$('#publishDateQueryDiv').hide();
		}else if(unit == 'medical'){
			$('#publishDateQueryDiv').hide();
		}else{
			$("#publishDateQueryDiv").removeAttr("class");
			$("#publishDateQueryDiv").html("<label for=\"drugstoreSubect_10\">日　　期:</label><select name=\"publishDateQuery\" id=\"drugstoreSubect_10\" class=\"width_210\">" + publishDateQueryHtml + "</select>");
			$("#publishDateQueryDiv").jqTransform();
			$("#publishDateQueryDiv").attr("class","messageColumn jqtransformdone");
			$("#publishDateQueryDiv").find('.jqTransformSelectWrapper').css('z-index','86');
		}
	}
	
	function chgSubUnitQuery($this){
		
		var subUnit = $($this).val();
		// 日期條件
		$('#publishDateQueryDiv').show();
		$('#publishDateQueryDiv').html('');
		if(subUnit == 'testSelf'){
			$('#publishDateQueryDiv').hide();
		}if(subUnit == 'activity'){
			$('#publishDateQueryDiv').hide();
		}else{
			$("#publishDateQueryDiv").removeAttr("class");
			$("#publishDateQueryDiv").html("<label for=\"drugstoreSubect_10\">日　　期:</label><select name=\"publishDateQuery\" id=\"drugstoreSubect_10\" class=\"width_210\">" + publishDateQueryHtml + "</select>");
			$("#publishDateQueryDiv").jqTransform();
			$("#publishDateQueryDiv").attr("class","messageColumn jqtransformdone");
			$("#publishDateQueryDiv").find('.jqTransformSelectWrapper').css('z-index','86');
		}
	}
	
	// 還原預設值
	var _select_unit_query_head = $('select[name=unitQuery]');
	$(_select_unit_query_head).val('article');
	
</script>

