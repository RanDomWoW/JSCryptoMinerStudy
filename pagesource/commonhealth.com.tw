			if (xhr.status != 0) {
				alert(xhr.status);
				alert(thrownError);
			}
		}
	});
	
	$.ajax({
		url : "/queryAction_queryNursingType.action",
		async : false,
		dataType : "json",
		contentType : "application/x-www-form-urlencoded; charset=UTF-8",
		type : "POST",
		success : function(jsonData) {
			var division = $("select[name=nursingType]");
			division.append("<option value=''>全部</option>");
			for (var i = 0; i < jsonData.length; i++) {
				division.append("<option value='" + jsonData[i].uuid + "'>" + jsonData[i].name + "</option>");
			}
		},
		error : function(xhr, ajaxOptions, thrownError) {
			if (xhr.status != 0) {
				alert(xhr.status);
				alert(thrownError);
			}
		}
	});

	$.ajax({
		url : "/addr.xml",
		async : false,
		dataType : "xml",
		contentType : "application/x-www-form-urlencoded; charset=UTF-8",
		type : "GET",
		success : function(xml) {
			$(xml).find("city").each(
					function() {
						var cityName = $(this).attr("name");
						var areas = "";
						$(this).find("area").each(function() {
							areas += $(this).attr("name") + "|";
						});
						$("select[name=city]").append(
								"<option value='" + cityName + "' areas='" + areas + "'>"
										+ cityName + "</option>");
					});
		},
		error : function(xhr, ajaxOptions, thrownError) {
			if (xhr.status != 0) {
				alert(xhr.status);
				alert(thrownError);
			}
		}
	});

	$(document).ready(function() {
		$("#doctorCity div.jqTransformSelectWrapper ul li a").click(function() {
			cityChange("doctorCity", "doctorArea");
			return false;
		});
	
		$("#hospitalCity div.jqTransformSelectWrapper ul li a").click(function() {
			cityChange("hospitalCity", "hospitalArea");
			return false;
		});
	
		$("#drugstoreCity div.jqTransformSelectWrapper ul li a").click(function() {
			cityChange("drugstoreCity", "drugstoreArea");
			return false;
		});
		
		$("#nursingCity div.jqTransformSelectWrapper ul li a").click(function() {
			cityChange("nursingCity", "nursingArea");
			return false;
		});
	});

	function cityChange(cityId, areaId) {
		var city = $("#" + cityId + " select[name=city]");
		var areaSelect = $("#" + areaId + " select[name=area]");
		areaSelect.empty();
		areaSelect.append("<option value=''>區域</option>");

		var cityName = city.val();
		var areas = city.find("option:selected").attr("areas").split("|");
		for (var i = 0; i < areas.length; i++) {
			if (areas[i]) {
				areaSelect.append("<option value='" + areas[i] + "'>"
						+ areas[i] + "</option>");
			}
		}
		$("#" + areaId).removeAttr("class");
		$("#" + areaId).html(
				"<select name='area' class='width_85'>" + areaSelect.html()
						+ "</select>");
		$("#" + areaId).jqTransform();
	}
