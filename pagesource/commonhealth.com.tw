<script type="text/javascript">

	var wallFlg = true;
	var categorySelected = "";
	var liSeq = 0;

	function doAddWall(callEtu, etuSampling){
		
		if(wallFlg){
			wallFlg = false;
			$.ajax({
	            url: "queryAction_queryWallArticle.action",
	            data: {type : categorySelected, callEtu : callEtu, eruid : ""},
	            dataType: "json",
	            contentType: "application/x-www-form-urlencoded; charset=UTF-8",
	            type:"POST",
	            success: function(jsonData){
	            	for(var i=0; i<jsonData.length; i++){
        		
	            		var etuProcessed = (jsonData[i].etu === 'true');

	            		var wallImg = $("<img class='wallImg' width='260' alt='" + jsonData[i].title + "' uuid='" + jsonData[i].uuid + "' seq='" + liSeq + "' url='" + jsonData[i].url + "' preface='" + jsonData[i].preface + "' hoards='" + jsonData[i].hoards + "' subTitle='" + jsonData[i].subTitle + "' >");	            		
	            		liSeq++;
	            		//必須等圖先loaded，才能append，不然圖片會疊在一起
	            		wallImg.load(function(){
	            			var seq = parseInt($(this).attr("seq"), 10);
	            			var article = $("<li seq='" + seq + "'></li>");
	            		    var a1 = "";	 	            		  	            		   
	            		    
	            		    if(etuProcessed && callEtu && etuSampling && seq<10)  // GA could send 10 event at most
	            		    {
	            		    	a1 = $("<a href='" + $(this).attr("url") + "&healthwall=all&eturec=1'>");	   
	            		    	//ga('send', 'event', 'recommend', 'eturec', 'healthwall');
	            		    }
	            		    else if(callEtu && !etuProcessed)  //etu failed
	            		    {
	            		    	a1 = $("<a href='" + $(this).attr("url") + "&healthwall=all'>");	    	
	            		    }
	            		    else if(!callEtu && etuSampling && seq<10)   //don't use etu but included for statistics
	            		    {
	            		    	a1 = $("<a href='" + $(this).attr("url") + "&healthwall=all&dbrec=1'>");	
	            		    	//ga('send', 'event', 'recommend', 'dbrec', 'healthwall');
	            		    }
	            		    else
	            		    {
	            		    	a1 = $("<a href='" + $(this).attr("url") + "&healthwall=all'>");	    	
	            		    }

	            			$(this).appendTo(a1);
	            			a1.append("<h3 class='titleGreen'><span class='subTitle'>[" + $(this).attr("subTitle") + "] &nbsp;</span>" + $(this).attr("alt") + "</h3><p>" + $(this).attr("preface") + "</p>");
	            			article.append(a1);
	            			article.append("<span class='addFavorite' id='hoard_" + $(this).attr("uuid") + "'>人氣(" + $(this).attr("hoards") + ")</span>");
	            			$('.wallContent').append(article);
	            			$('.wallContent').isotope('appended', article);
	            			$('.wallContent').isotope({ sortBy: 'wallseq' });
	            		});
	            		wallImg.attr("src", jsonData[i].img);
	            		$('.wallContent').append(wallImg);
	            	}

	            },	             
				error : function(xhr, ajaxOptions, thrownError) {
					if(xhr.status != 0){
						alert(xhr.status);
						alert(thrownError);	
					}
				},
				complete : function (jqXHR, textStatus){
					wallFlg = true;
				}
			});
		}
		
		
	}	
	

	doAddWall(true, true);

    
	$(window).scroll(function() {
        if($(window).scrollTop() + $(window).height() > ($(document).height()/2)) {   
            doAddWall(false, false);
        }
	});
	
	
	function healwallTabChanged(category, element){
		
		categorySelected = category;	
		$(element).parent().siblings().attr("class", "");
		$(element).parent().attr("class", "active");
		reloadWall();
	}
	
	function reloadWall(){
		$('.wallContent').css('min-height', $(window).height());
		$('.wallContent').isotope("remove", $(".wallContent li:not('.wallContentAd')"));	
		liSeq = 0;
		doAddWall(false, false);
	}
	
	$(document).ready(function() {
		
		$(window).on('resize', function(){
			$('.wallContent').isotope({ sortBy: 'wallseq' });
		});
	});
	
	$(window).load(function(){
		$(".wallContentAd").each(function(){
			if($(this).height() == 0){
				$('.wallContent').isotope("remove", $(this));	
			}
		});	
	});
	
</script>



