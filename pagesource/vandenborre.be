 <script type="text/javascript">
 var toggleChatbot = function(){

 if(!$('#btmBarChatbotBtn').hasClass('disabled')){
 if($('.stat_chatbot').length == 0){
 if($('#btmBarChatbot #iChatConversation').is(':visible')){
 $('#btmBarChatbot #iChatConversation').fadeOut(100).parent().prev().find('img#max-on-btn').delay(200).animate({bottom: '-25px'}, 300);
 }else{
 $('#btmBarChatbot #iChatConversation #max').css('bottom','-100px');
 $('#btmBarChatbot #iChatConversation').fadeIn(100).parent().prev().find('img#max-on-btn').delay(200).animate({bottom: '-72px'}, 300,function(){
 $('#btmBarChatbotBtn').removeClass('up');
 $('#btmBarChatbot #iChatConversation #max').animate({bottom: '0'}, 300);
 });

 //ga('send','event','site','chatbot','open');
 }
 }else{
 $('body, html').animate({scrollTop : 0}, 600);

 if(!$("#btmBarChatbot #warning").is(':visible')){
 $('img#max-on-btn').animate({bottom: '0px'}, 300);
 $('#btmBarChatbotBtn').addClass('up');
 $("#btmBarChatbot #warning h3").text("Hallo, ik ben hierboven !");
 $("#btmBarChatbot #warning p").text("Is er nog iets anders waarmee ik u van dienst kan zijn ?");
 $("#btmBarChatbot #warning").fadeIn().delay(5000).fadeOut(function(){
 $('img#max-on-btn').animate({bottom: '-25px'}, 300,function(){
 $('#btmBarChatbotBtn').removeClass('up');
 });
 });

 }
 }
 }
 }

 var closeChatbot = function(){
 //ga('send','event','site','chatbot','close');
 $('#btmBarChatbot #iChatConversation').fadeOut(100);
 $('img#max-on-btn').animate({bottom: '-25px'}, 300);
 return false;
 }

 $(document).ready(function(){
 $('html').click(function() {
 closeChatbot();
 })

 $('#btmBarChatbot').click(function(e){
 e.stopPropagation();
 })

 $('#btmBarChatbotBtn').click(function(e){
 toggleChatbot();
 e.stopPropagation();
 })

