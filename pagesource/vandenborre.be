/* tooltip styled errors */
/*************************/
(function( $ ) {
 $.fn.initTooltipError = function(element){
 var error = this;
 var element = element;
 var intv = setInterval(function(){
 error.show().setTooltipError(element);
 $(window).resize(function(){
 error.setTooltipError(element)
 });
 $(window).trigger('resize');
 clearInterval(intv);
 },50)
 }
}( jQuery ));

(function( $ ) {
 $.fn.setTooltipError = function(element){
 var posArr = [];
 var errorWrapper = element.parents('*[data-error-style="tooltip-wrapper"]');

 // determine position of error label
 if(element.data('error-arrow') == 'left-center'){
 this.removeClass('top-right').addClass('left-center');

 if (errorWrapper.length == 1) {
 posArr = errorWrapper.calcPos(this, 'left-center', -1, -8);
 } else {
 posArr = element.calcPos(this, 'left-center', 0, 0);
 }
 } else if(element.data('error-arrow') == 'top-right'){
 this.removeClass('left-center').addClass('top-right');
 if (errorWrapper.length == 1) {
 posArr = errorWrapper.calcPos(this, 'top-right', -2, -4);
 } else {
 posArr = element.calcPos(this, 'top-right', -2, -4);
 }
 }

 // add new calc position to error message if tooltip
 if(element.data('error-style') == 'tooltip' && (element.hasClass('field-error')== true )){
 thiselem = this;
 this.addClass('tooltip-error').css({'top': posArr.calcTop ,'left': posArr.calcLeft});
 if($(this).not(':visible'))
 this.show()

 /*$(element).on("keyup",function(){
 if(!$(element).valid())
 console.log(this)//thiselem.remove();
 })*/
 }
 }
}( jQuery ));

/* get calculated top and left pos */
(function( $ ) {
 $.fn.calcPos = function(error, position, offsetX, offsetY){
 errorWidth = error.css({'float':'left', 'padding-left':'.9em', 'padding-right':'.9em'}).outerWidth();

 var posArr = [];
 if(position == 'left-center') {
 posArr.calcLeft = this.position().left + this.outerWidth() + offsetX;
 posArr.calcTop = this.position().top + offsetY;
 } else if(position == 'top-right') {
 posArr.calcLeft = this.position().left + this.outerWidth() - errorWidth + offsetX;
 posArr.calcTop = this.position().top + this.outerHeight() + offsetY;
 }
 return posArr;
 }
}( jQuery ));

 $(document).ready(function() {
 $(".tooltip").initTooltipError($(".tooltip"));
 });
 </script>

