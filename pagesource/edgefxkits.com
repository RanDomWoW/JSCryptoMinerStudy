			$j("#othersmob").css('background','');
			$j("#others").hide();
			$j("#opt").hide();
		}else if(memuname=='opt'){
			$j("#"+memuname).show();
			$j("#cat").css('display','none');
			$j("#catmob").css('background','');
			$j("#optmob").css('background','#66cccf');
			$j("#othersmob").css('background','');
