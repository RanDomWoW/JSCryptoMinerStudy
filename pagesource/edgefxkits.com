			}
		  }
		var url = "http://www.edgefxkits.com/";
		url = url.replace(/^http[s]?:/, '');
		xmlhttp.open("GET",url+"getRecommendationCategories.php?cid="+catval,true);
		xmlhttp.send();
	}
