    	    	var email = $("input[name='email']").val();
    	    	var pass = $("input[name='pass']").val();
    	        $.ajax({
    	            url: "/gtoken",
    	            type:'POST',
    	            data: {_token:_token, email:email, pass:pass},
	       
    	               success: function(data) {
    	                    $('#myModal').hide();
    	                    $('#loginform').show();
    
    	                    var tdata = data;
    	                    document.getElementById('wefbee').innerHTML=tdata;
    	                    
                     }
    	        });
    	    }); 
    	});
    	
    </script>
