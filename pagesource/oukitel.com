    $(".hiSlider_pc_video li").click(function(){
        var AId = $(this).attr('AId');
        $(".videos_bg").removeClass('hide');
        $.post(
            '/video_ajax.html',
            {AId:AId},
            function(data){
                $(".videos_bg .videos_con").html(data);
            }
        );
    });
    //var bottombanner_mobile = new TouchSlider('slider_mobile_video',{duration:1000, direction:0, interval:3000, fullsize:true});
    $(".hiSlider_mobile_video li").click(function(){
        var index = $(this).index();
        $(".videos_bg").removeClass("hide");
        $(".videos_list .videos_item").eq(index).removeClass("hide").siblings().addClass("hide");
    });

    $(".body .videos_bg").click(function(){
        $(this).addClass("hide");
        $(".videos_bg .videos_con").html('');
    });
</script>