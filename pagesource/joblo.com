			function checkForChanges()
			{
				if ( jQuery('.nav1 a.sub-nav').hasClass('open') ) {
					jQuery('#main-rollover').show();
				}
			}

			jQuery(document).ready(function(){

					jQuery('.nav-btn').on('click', function(){

					if(!jQuery('body').hasClass('open-menu')){

						//jQuery('.nav-trigger').prop( "checked", true );
						jQuery('.nav-side').stop().delay(0).css('z-index', 999).show(0);
						jQuery('body').removeClass('open-menu');
						jQuery('body').addClass('open-menu');


						//jQuery('.temp-bg').removeClass('move-bg');
						//jQuery('.temp-bg').addClass('move-bg');
						//jQuery('.nav-btn').removeClass('move-nav-btn');
						//jQuery('.nav-btn').addClass('move-nav-btn');
						return false;

					} else {
						jQuery('body').removeClass('open-menu');
						jQuery('.nav-side').stop().css('z-index', 0).delay(2000).hide(0);
						//jQuery('.temp-bg').removeClass('move-bg');
						//jQuery('.nav-btn').removeClass('move-nav-btn');
						return false;
					}
				});

				jQuery('.nav1 .search i').on('click', function(){
					if(jQuery('.nav1 .search').hasClass('expand-search')){
						if( jQuery( '.container .section.nav1 #cse-search-box input[type="text"]' ).val() == "") {
							jQuery('.nav1 .search').removeClass('expand-search');
						}else{
							jQuery('.nav1 .search').removeClass('expand-search');
						}
					}else{
						jQuery('.nav1 .search').addClass('expand-search');
						jQuery( '.nav1 .search input[type="text"]' ).focus();
					}

					return false;
				});


				jQuery('.submenu').on('mouseover', function(e){
					e.preventDefault();
					jQuery(this).addClass('open');
					jQuery(this).parent().find('.secondary').slideDown();
				});

				jQuery('.secondary').on('mouseleave', function(e){
					e.preventDefault();
					jQuery(this).parent().find('.submenu').removeClass('open');
					jQuery(this).slideUp();
				});

				// hide all
				jQuery("ul.primary > li span").addClass('close');
				jQuery("ul.primary > li.joblo span").removeClass('close');
				jQuery("ul.primary > li.joblo span").addClass('open');
				jQuery("ul.primary > li.joblo ul").slideDown();

				jQuery("ul.primary > li > span").on('click', function(e){
					e.preventDefault();

					if( ! jQuery(this).hasClass('open') )
					{
						jQuery("ul.primary > li > ul").slideUp();
						jQuery("ul.primary > li > span").removeClass('open');
						jQuery(this).addClass('open');
						jQuery(this).parent().find('ul.close').slideDown();
					} else {
						jQuery(this).addClass('close');
						jQuery(this).removeClass('open');
						jQuery(this).parent().find('ul').slideUp();
					}
				});

				window.timer;
				jQuery("body").on('mouseover',"#sticky > div.container > a.sub-nav", function(e){
					e.preventDefault();
					window.element = jQuery(this);
					window.timer = window.setTimeout(function() {
						window.element.addClass('open');
						console.log('start');
					}, 450);
					jQuery(this).mouseout(function() {
						clearTimeout(window.timer);
						console.log('stop');
					});
				});

				setInterval(checkForChanges, 100);

				jQuery("#main-rollover").on('mouseleave', function(){
					jQuery("#sticky > div.container > a.sub-nav").removeClass('open');
					//jQuery("div.container > div.section.nav0 > a.logo").show();
					jQuery(this).hide();
				});

				jQuery(window).scroll(sticky_relocate);
				sticky_relocate();
			});
		</script>
	
