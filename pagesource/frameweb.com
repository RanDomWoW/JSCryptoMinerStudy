		<!-- Blurred image loading -->
		<script>
			$(function() {
				$(".blurred-load, .background-blurred-load").each(function(){
					var $image = $(this);
					var $downloadingImage = new Image();

					var responsive = ($image.data("responsive"));

					$downloadingImage.onload = function(){
					console.log("Downloaded: " + $downloadingImage.complete);
						if($image.hasClass("background-blurred-load")) {
							// load image as background-image
							$image.css("background-image", "url('" + $(this).attr("src") + "')");
						} else {
							// load image as src
							$image.attr("src", $(this).attr("src"));
						}
						
						$image.removeClass('blurred');	
						console.log("load successful");
					}

					if(responsive) {
						var $container = $($(this).data("container") || window);
						var width = $container.width();
						var height = $container.height();
						var src = "/scripts/small_image.php?w=" + width + "&h=auto&minw=" + width + "&minh=" + height + "&f=" + $(this).data('src');
					} else {
						var src = $(this).data('src');
					}

					$downloadingImage.src = src;
				});
			});
		</script>

