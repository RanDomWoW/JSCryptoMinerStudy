<script id="template-upload" type="text/x-tmpl">

{% for (var i=0, file; file=o.files[i]; i++) { %}
    <div class="template-upload" id="file">
        <ul>
            <li class="center" style="width: 5%;"><img style="vertical-align: middle;" src="{%=o.getIcon(file.name)%}" alt=""></li>
            <li style="width: 25%;">{%=o.formatFileName(file.name)%}</li>
            <li class="center" style="width: 7%;">{%=o.formatFileSize(file.size)%}</li>
            <li style="width: 35%;">
                <div class="progress progress-warning progress-striped active">
                    <div class="bar">0%</div>
                </div>
            </li>
            <li class="center upload-speed" style="width: 8%;">N/A</li>
            <li class="center remaining-time" style="width: 10%;">N/A</li>
            <li class="center" style="width: 10%;"><button type="button" class="cancel">Annuler</button></li>
            <li class="start" style="display: none;"></li>
        </ul>
    </div>
{% } %}

</script>
<!-- The template to display files available for download -->
<script id="template-download" type="text/x-tmpl">

{% for (var i=0, file; file=o.files[i]; i++) { %}
    <div class="template-download" id="file">
        <ul>
            <li class="center" style="width: 5%;"><img style="vertical-align: middle;" src="{%=o.getIcon(file.name)%}" alt=""></li>
            <li style="width: 25%;">{%=o.formatFileName(file.name)%}</li>
            <li class="center" style="width: 7%;">{%=o.formatFileSize(file.size)%}</li>
            <li style="width: 35%;">
                {% if (file.error) { %}
                    <div class="progress progress-danger">
                        <div class="bar" style="width: 100%">{%=o.formatError(file.error)%}</div>
                    </div>
                {% } else { %}
                    <div class="progress progress-success">
                        <div class="bar" style="width: 100%">Upload réussi</div>
                    </div>
                {% } %}
            </li>
            <li class="center" style="width: 8%;">N/A</li>
            <li class="center" style="width: 10%;">N/A</li>
            <li class="center" style="width: 10%;"><button type="button" class="cancel">Annuler</button></li>
            <li class="start" style="display: none;"></li>
        </ul>
    </div>
{% } %}

</script>

