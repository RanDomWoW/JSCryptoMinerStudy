                function contactuss() {

                        if (document.contact.name.value.length <= 0)
                        {
                                alert('Invalid   Name ');
                                return false;
                        }

                        if (document.contact.email.value.length <= 0)
                        {
                                alert('Enter Email');
                                return false;
                        }
                        if (document.contact.email.value.length > 0)
                        {
                                at = document.contact.email.value.indexOf("@");
                                if (at == -1)
                                {
                                        alert('Invalid Email ID');
                                        return false;
                                }
                                at = document.contact.email.value.indexOf(".");
                                if (at == -1)
                                {
                                        alert('Invalid Email ID');
                                        return false;
                                }
                        }

                        if (document.contact.phone.value.length <= 0)
                        {
                                alert('Invalid  Telephone ');
                                return false;
                        }

                        if (document.contact.description.value.length <= 0)
                        {
                                alert('Invalid  Comments ');
                                return false;
                        }
                        if (document.contact.captche.value.length <= 0)
                        {
                                alert('Enter the captcha text ');
                                return false;
                        }

                }


