					$('.hm-callrecorderft .callrecorderfto-ui').fadeIn(500);
					$('.hm-callrecorder .callrecorder-ui').fadeIn(500);
					$('.hm-printopia .col-4').delay(500).fadeIn(500);
					$('.hm-printopia .col-2').delay(1250).fadeIn(500);
					$('.hm-printopia .wireless').delay(2000).fadeIn(500);			
				}

				$('.hm-callrecorderft .callrecorderfto-ui').delay(350).fadeIn(500);
	
				// home page fade transitions
				$(window).scroll(function() {
		
					var fromTop = $(window).scrollTop();
		
					if (fromTop > 720) {
						$('.hm-callrecorder .callrecorder-ui').fadeIn(500);
					}
		
					if (fromTop > 1200) {
						$('.hm-printopia .col-4').delay(500).fadeIn(500);
						$('.hm-printopia .col-2').delay(1250).fadeIn(500);
						$('.hm-printopia .wireless').delay(2000).fadeIn(500);			
					}
		
				});
			});
			
