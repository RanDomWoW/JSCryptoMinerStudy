			    $('.selectorSecciones').click(function() {

					event.preventDefault();



					var $elemento = $(this).attr('id');

					var $elemPrev = $elementoPrev;



					$('.seccionToggle').toggle();

					toggleSelector($elemento, $elemPrev);

				});



				var toggleSelector = function (param1, param2) {



					$('#' + param1).css('display', 'none');

					$('#' + param1 + 'Blue').css('display', 'block');

					$('#' + param2).css('display', 'block');

					$('#' + param2 + 'Blue').css('display', 'none');



					$elementoPrev = param1;

				};	



