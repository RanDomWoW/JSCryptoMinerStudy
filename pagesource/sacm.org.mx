		<script type="text/JavaScript">
			year = 2018;
			month = 3;
			
			function CargaCartelera(yearValue,monthValue){
				$("#cartelera-calendario").html("<table width='100%' height='100%'><tr><td align='center' valign='middle'><br><br><br><br><br><img src='/images/loading_icon.gif' /> Cargando...</td></tr></table>");
				$("#cartelera-calendario").load("/cargaCarteleraHome.asp?year=" + yearValue + "&month=" + monthValue);
			}
			
			$('#menu-year').click(function(e){
	     		var id = e.target.id;
				
				year	= id.replace("linkAnoCartelera","");
				
				$("ul#menu-year li").removeClass("year-selected");
				
				$("ul#menu-year li#anoCartelera" + year).addClass("year-selected");
				
				CargaCartelera(year,month);
	    	});
			
			 $('#menu-month').click(function(e){
	     		var id = e.target.id;
				
				month	= id.replace("linkMesCartelera","");
				
				$("ul#menu-month li").removeClass("seleccionado");
				
				$("ul#menu-month li#mesCartelera" + month).addClass("seleccionado");
				
				CargaCartelera(year,month);
	    	});
			
			CargaCartelera(year,month);
			
			function ShowEvent(listOfEvents){
				$(".cartelera-eventos section.eventos-cartelera").hide();
				
				var arrayEvents	= listOfEvents.split(",");
				var eventInfo;
				var eventID;
				//var eventCategory;
				
				for (var i=0; i<arrayEvents.length; i++){
					eventInfo		= arrayEvents[i].split("|");
					eventID			= eventInfo[0];
	//				eventCategory	= eventInfo[1];
							
					$("section#eventID" + eventID).show();
				}
			}
		</script>
