    var tm = $.cookie('tm'); if (tm == undefined) tm = '';
    var tmb = $.cookie('tmb'); if (tmb == undefined) tmb = '';

    $(document).ready(function () {

        $('div#topmenu ul.topmenufirst li a').click(function () {
            var level2 = (this.parentNode.id + '').substr(3, 1);

            tmb = '';
            if (level2 == '0') {
                HideSelect('div#topmenu div.topmenubox');
                HideSelect('div#topmenu ul.topmenusecond');
                $('div#topmenu li#' + tm.substr(0, 3) + '0')[0].toggleClass("active");
                if ((tm.substr(3, 1) + 0) > 0)
                    $('div#topmenu li#' + tm)[0].removeClass("click");
                tm = this.parentNode.id;
                $('div#topmenu li#' + tm + ' ul.topmenusecond')[0].show('inline-block');
                $('div#topmenu li#' + tm)[0].toggleClass("active");
            }
            else {
                if ((tm.substr(3, 1) + 0) > 0)
                    $('div#topmenu li#' + tm)[0].removeClass("click");
                tm = this.parentNode.id;
                var temptmb = 'tmb' + tm.substr(2, 2);
                if (document.getElementById(temptmb) != undefined) {
                    var e = $('div#topmenu div#' + temptmb + '')[0];
                    var f = $('div#topmenu div#' + temptmb + '');
                    if (IsVisible(e)) {
                        //Hide(e);
                        f.fadeOut('slow');
                        $('div#topmenu li#' + tm)[0].removeClass("click");
                    }
                    else {
                        HideSelect('div#topmenu div.topmenubox');
                        //Show(e, 'block');
                        f.fadeIn('slow');
                        tmb = temptmb;
                        $('div#topmenu li#' + tm)[0].addClass("click");
                    }
                }
                else {
                    HideSelect('div#topmenu div.topmenubox');
                }
            }
            $.cookie('tm', tm);
            $.cookie('tmb', tmb);
        });

        if (tm == '') {
            tm = 'tm10'; $.cookie('tm', tm);
        }

        if ((tm.substr(3, 1) + 0) > 0)
            $('div#topmenu li#' + tm)[0].addClass("click");
        $('div#topmenu li#' + tm.substr(0, 3) + '0')[0].toggleClass("active");
        $('div#topmenu li#' + tm.substr(0, 3) + '0 ul.topmenusecond')[0].show('inline-block');
        ShowSelect('div#topmenu div.topmenubox', 'none');
        if (tmb != '') {
            var f = $('div#topmenu div#' + tmb + '');
            f.fadeIn('slow');
        }

    });



    /* FUNCTION */

    // Hide elements
    function HideSelect(s) {
        $(s).each(function (i, obj) {
            obj.hide();
        });
    }

    // Show elements
    function ShowSelect(s, display) {
        $(s).each(function (i, obj) {
            obj.show(display);
        });
    }

    // Check visibility
    function IsVisible(e) {
        return e.style.display != 'none';
    }

</script>
