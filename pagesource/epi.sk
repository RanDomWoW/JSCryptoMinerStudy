			//$menu.hide("slide");
			$menu.hide();
		}
		else
		{
			$menu.css('height', '' + $(window).height() + 'px');
			$menu.show();
			//$menu.show("slide");
		}
	});
	$("#header-scroll-menu").mouseleave(function (e) {
		$(this).hide();
	});
	$(".header-scroll-menu-close").click(function (e) {
		var $menu = $("#header-scroll-menu");
		if ($menu.is(":visible")) {
			$menu.hide();
		}
	});

	$(".header-scroll-menu-container h3").click(function (e) {
		var t = $(this);
		var s = $(this).next();
		if (s.is(":visible")) {
			s.hide();
			t.addClass('down');
		}
		else {
			s.show();
			t.removeClass('down');
		}
	});

	$(".header-scroll-menu-container h3:not(:first-child)").each(function (e) {
		var t = $(this);
		t.attr("title", "sbaliť / rozbaliť");
		//if (t.text() != "Moje EPI") {
			var s = t.next();
			s.hide();
			t.addClass('down');
		//}
	});
});
</script>
	

	
	
    
