	<style type="text/css">
		*{margin:0; padding:0;}
		html,body{height:100%;font-family:'Nanum Gothic', "돋움",Dotum,sans-serif; font-size:14px; color:#333; line-height:24px;}
		ul, ol, li{list-style:none;}
		img{border:0; line-height:0; font-size:0; vertical-align:top;}
		a{text-decoration:none;}
		.indent{overflow:hidden; text-align:left; text-indent:-9999em; display:block; height:100%;}
		
		.home_wrap{width:1200px; margin:0 auto; overflow:hidden; padding-bottom:100px;}
		.home_wrap h1{display:block; text-align:center; margin-top:40px;}
		.home_wrap h1 a{display:inline-block; width:380px; height:180px; background:url('images/kangnam.jpg') no-repeat 0 0;}
		.btn_R{text-align:right; margin-top:20px;}
		.btn_group{padding:0 27px; height:31px; line-height:31px; color:#fff; background:#0099ff; border-radius:5px; display:inline-block;}
		.affiliate_list{width:auto; margin:20px 0 0 -20px; overflow:hidden;}
		.affiliate_list > li{width:25%; box-sizing:border-box; padding-left:20px; float:left;}
		.affiliate_list > li .in_wrap h2{border-top:2px solid #002b78; height:60px; line-height:60px; width:100%; display:block; font-size:26px; color:#48515b; font-weight:bold;}
		.affiliate_list > li .in_wrap .thumb{line-height:0; font-size:0;}
		.affiliate_list > li .in_wrap .thumb img{width:100%;}
		.affiliate_list > li ul{margin-top:10px; border-bottom:1px solid #898989;}
		.affiliate_list > li ul li{border-top:1px solid #ccc; min-height:203px;}
		.affiliate_list > li ul li:first-child{border-top:0;}
		.affiliate_list > li ul li .name{font-size:18px; color:#1a99ff; display:block; margin-top:16px;}
		.affiliate_list > li ul li .link{line-height:20px; color:#444; font-weight:bold;}
		.affiliate_list > li ul li .link:hover{text-decoration:underline;}
		.affiliate_list > li ul li .establish{font-size:14px; line-height:32px;display:block; margin-top:6px;}

		@media screen and (min-width:640px) and (max-width:1199px) {
		.home_wrap{width:100%;}
		.affiliate_list > li .in_wrap h2{text-align:center;}
		.affiliate_list > li{width:100%; overflow:hidden; float:none;}
		.affiliate_list > li .in_wrap{padding-bottom:30px;}
		.affiliate_list > li .in_wrap .thumb{text-align:center;}
		.affiliate_list > li .in_wrap .thumb img{max-width:285px;}
		.affiliate_list > li ul{width:100%; overflow:hidden; margin-top:20px; border-bottom:0;}
		.affiliate_list > li ul li{float:left; width:50%; box-sizing:border-box; padding:0 10px;}
		.affiliate_list > li ul li:first-child{border-top:1px solid #ccc;}
		.affiliate_list > li ul li p br{display:none;}
		}

		@media screen and (max-width:640px){
		.home_wrap{width:100%; box-sizing:border-box; padding:0 15px;}
		.home_wrap h1 a{display:inline-block; width:280px; height:137px; background:url('images/kangnammobile.jpg') no-repeat 0 0; background-size:280px 137px;}
		.btn_R{text-align:right; margin-top:20px;}
		.btn_group{padding:0 10px; height:21px; line-height:21px; color:#fff; background:#1a99ff; border-radius:5px; display:inline-block;}
		
		.affiliate_list{margin-left:0; width:100%;}
		.affiliate_list > li .in_wrap h2{text-align:center;}
		.affiliate_list li{padding-left:0; width:100%; float:none;}
		.affiliate_list > li ul li p br{display:none;}
		}

