/* # Basic styles to set the overall theme/mood
================================================== */
html {
  -webkit-font-smoothing: antialiased; }

body {
  color: #000;
  font-family: "Gotham A", "Gotham B", sans-serif; }

/* Header */
.logo {
  max-width: 11em; }

.navbar {
  margin-bottom: 0px;
  font-size: .95em; }

.navbar-default .navbar-nav > li > a {
  color: #fff;
  text-transform: uppercase;
  font-weight: 700;
  letter-spacing: 2px; }

.navbar-default .navbar-nav > li > a:hover {
  color: #1ca5fc; }

.navbar-default .navbar-nav > .open > a,
.navbar-default .navbar-nav > .open > a:hover,
.navbar-default .navbar-nav > .open > a:focus {
  background: #fff;
  color: #000; }

.navbar-brand {
  height: auto;
  /*padding: 2.25em .833333333em;*/ }

.navbar-default {
  border-radius: 0px;
  border-bottom: 0px;
  background: #000;
  border: none;
  box-shadow: 0px 2px 10px 0px rgba(0, 0, 0, 0.3); }

.navbar-default .navbar-nav > li > a {
  font-size: 1em; }

.caret {
  border-top: 4px dotted #ffcd31; }

@media (min-width: 768px) {
  .navbar-nav > li > a {
    padding-top: 21px;
    padding-bottom: 21px; } }

/* End Header */
/* Post excerpts */
.post-snippet.featured {
  background-color: #fff9f0; }

.article-hero {
  margin-bottom: 3em; }

.post-snippet h2.entry-title {
  margin-bottom: .4em; }

.post-snippet p.excerpt {
  font-size: 1em;
  color: #333; }

.post-snippet p.category,
p.category,
.article-hero p.by {
  text-transform: uppercase;
  text-align: center;
  letter-spacing: 1px;
  font-size: 1.25em;
  margin-bottom: .5em; }

.post-snippet p.meta {
  font-size: .9em;
  text-transform: uppercase;
  font-weight: normal;
  font-style: normal;
  letter-spacing: 1px;
  margin-top: 1em;
  margin-bottom: .25em; }

.post-snippet p.meta-comments {
  font-size: .75em;
  margin-bottom: .5em;
  text-transform: uppercase;
  color: #676767; }

.post-snippet p.category a,
p.category a,
.article-hero p.by a {
  -webkit-transition: all 100ms ease-in-out;
  -moz-transition: all 100ms ease-in-out;
  -o-transition: all 100ms ease-in-out;
  transition: all 100ms ease-in-out;
  color: inherit;
  border-bottom: 1px solid white; }

p.category a:hover,
.article-hero p.by a:hover {
  text-decoration: none;
  border-bottom: 4px solid #1ca5fc; }

p.excerpt-date,
p.meta {
  color: #999;
  margin-bottom: .5em;
  font-size: 1em; }

.post-snippet h2,
.single .also-like .post-snippet h2 {
  margin-top: 0px;
  font-family: Industry-Bold;
  font-weight: normal;
  font-style: normal;
  font-size: 1.25em; }

.post-snippet h2 a {
  color: #000;
  line-height: 1.28em; }

.post-snippet p.by a {
  color: inherit; }

/* End Post excerpts */
/* Load More arrow and hover animation */
a.load-more,
.scroll-down {
  display: block;
  width: 9em;
  margin: 0px auto;
  padding: 3em; }
  a.load-more span,
  .scroll-down span {
    display: block;
    background-color: #000;
    margin-bottom: 5px; }
  a.load-more span.one,
  .scroll-down span.one {
    width: 100%;
    margin-left: auto;
    margin-right: auto;
    height: 3px; }
  a.load-more span.two,
  .scroll-down span.two {
    width: 70%;
    margin-left: auto;
    margin-right: auto;
    height: 2px; }
  a.load-more span.three,
  .scroll-down span.three {
    width: 40%;
    margin-left: auto;
    margin-right: auto;
    height: 1px; }

a.load-more:hover span {
  margin-bottom: 8px;
  background-color: #1ca5fc;
  -webkit-transition: all 100ms ease-in-out;
  -moz-transition: all 100ms ease-in-out;
  -o-transition: all 100ms ease-in-out;
  transition: all 100ms ease-in-out;
  position: relative; }

a.load-more:hover span.two {
  transition-duration: .2s;
  transition-delay: 0.1s;
  -webkit-transform: rotate(180deg);
  transform: rotate(180deg); }

/* End Load More arrow and hover animation */
