            {

                if ((form1.model_rating[0].checked == false) && (form1.model_rating[1].checked == false) && (form1.model_rating[2].checked == false) && (form1.model_rating[3].checked == false) && (form1.model_rating[4].checked == false) && (form1.model_rating[5].checked == false) && (form1.model_rating[6].checked == false) && (form1.model_rating[7].checked == false) && (form1.model_rating[8].checked == false) && (form1.model_rating[9].checked == false))
                {
                    if (Trim(document.getElementById("rating_given").value) == 0)
                    {
                        alert("Si prega di dare voto");
