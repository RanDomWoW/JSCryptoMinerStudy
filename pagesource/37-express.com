                        <form action="http://www.37-express.com/cn/rebate/customer/login/" method="post" id="rebate-login-form" onsubmit="return rebateAjaxLogin(this)">
                            <ul class="form-list">
                                <li>
                                    <div class="input-box">
                                        <input id="email" name="username" class="input-text required-entry" title="电话号码或邮箱地址" placeholder="电话号码或邮箱地址" type="text">
