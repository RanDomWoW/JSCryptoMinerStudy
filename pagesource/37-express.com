</div>
<script type="text/javascript">
var loginFormDialogHtml = document.getElementById('rebate-login-form-dialog').innerHTML,discountsAfterLogin = document.getElementById('discounts-after-login').innerHTML,iTimerID = null;
function afterLoggedIn(flag){
    if(flag){
        document.getElementById('discounts-after-login').style.display = 'block';
        document.getElementById('discounts-before-login').style.display = 'none';
    }else{
        document.getElementById('discounts-before-login').style.display = 'block';
        document.getElementById('discounts-after-login').style.display = 'none';
    }
}
function rebateAjaxLogin(form){
    if(document.getElementById('login-message').innerHTML!=='')document.getElementById('login-message').innerHTML = '';

    new Ajax.Request($(form).readAttribute('action'),{"method":$(form).readAttribute('method'),"evalJSON":"force","parameters":$(form).serialize(),"onSuccess":function(transport){
        if(transport.responseJSON){
            if(transport.responseJSON.error_mesage && transport.responseJSON.error_mesage!==''){
                document.getElementById('login-message').innerHTML = transport.responseJSON.error_mesage;
            }else if(typeof(transport.responseJSON.banner_link)=='string' && transport.responseJSON.banner_link!=''){
                document.getElementById('discounts-after-login').innerHTML = discountsAfterLogin.replace(/\{(\w+)\}/g,function($0,$1){
                    return typeof(transport.responseJSON[$1])!='undefined' ? transport.responseJSON[$1] : $0;
                });
                afterLoggedIn(true);
                iTimerID = window.setTimeout('window.location.reload(true)',5000);
            }else{
                document.getElementById('login-message').innerHTML = '登录失败';
            }
        }
    }});

    return false;
}
$$('a[logo]').invoke('observe','click',function(event){
    var bannerId = this.getAttribute('bannerid');
    if(bannerId && bannerId!=''){
        afterLoggedIn(false);
        document.getElementById('disabled-mask').style.display = 'block';
        new Ajax.Request('http://www.37-express.com/cn/rebate/banner/click/id/'+bannerId+'/',{"method":"get","evalJSON":"force","onSuccess":function(transport){
            if(transport.responseJSON && transport.responseJSON.is_logged_in && typeof(transport.responseJSON.banner_link)=='string'){
                window.location.href = transport.responseJSON.banner_link;
            }else if(transport.responseJSON && typeof(transport.responseJSON.banner_id)=='number'){
                document.getElementById('rebate-login-form-dialog').innerHTML = loginFormDialogHtml.replace(/\{(\w+)\}/g,function($0,$1){
                    return typeof(transport.responseJSON[$1])!='undefined' ? transport.responseJSON[$1] : $0;
                });
                document.getElementById('rebate-login-form-dialog').style.display = "block";
                JSExt.documentBody.center('#rebate-login-form-dialog');
            }else{
                document.getElementById('disabled-mask').style.display = 'none';
                console.log('返回结果是错误的');
            }
        }});
        Event.stop(event);
        return false;
    }
    return true;
}).invoke('writeAttribute','href','javascript:;');

$('disabled-mask').observe('click',function(){$(this).hide();document.getElementById('rebate-login-form-dialog').style.display = 'none';if(iTimerID){window.clearTimeout(iTimerID);iTimerID = null;window.location.reload(true);}});
</script>
