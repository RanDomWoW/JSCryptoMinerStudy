    <script type="text/javascript">
/*
	Stock Ticker

	This script uses a JSON file, which is created via an Azure Webjob (CRON job). 
	The Webjob posseses a script that retrieves the Yahoo finance quote,
	and places the contents in the stock_ticker.json file, 
	located at site/wwwroot/assets/json/stock_ticker/stock_ticker.json. 
	
	A copy of the script, uploaded to the Azure Webjob, is also located in this directory, titled stock_ticker_create_json.html.

	If for any reason the stock_ticker_create_json.html file is changed, the file must also be updloaded into the Azure Webjob, titled Stockticker02.
*/

	$(document).ready(function($) {
		$.ajax({
			url: "/assets/json/stock_ticker/stock_ticker.json",
			cache: false,
			dataType: 'json',
			success: function(data) {
				var footerOutput    = "";
				var investorsOutput = "";
				var symbol         = "COHR";
				var name           = "Coherent";
				var lastTradePrice = data[0].last;
				var lastTradeDate  = data[0].timestamp;

			    // EST
			    var offset = -5.0;
			    var clientDate = new Date(lastTradeDate);
			    var utc = clientDate.getTime() + (clientDate.getTimezoneOffset() * 60000);
			    var ets = new Date(utc + (3600000*offset));
			    var dateTime = ets.toLocaleString();

			    var dateTimeResult = dateTime.split(", ");

				// Build footer output
				footerOutput = "<b>(" + symbol + ")</b> $" + lastTradePrice + " " + dateTimeResult[0] + ", " + dateTimeResult[1] + " EST";

				// Build investor's page output
				investorsOutput = "COHERENT: " + symbol + "<table><tr><td>Symbol</td><td>Name</td><td>Last Trade</td><td>Last Trade Date</td><td>Last Trade Time</td></tr><tr><td style='text-align: center;'>" + symbol + "</td><td>" + name + "</td><td>" + lastTradePrice + "</td><td>" + dateTimeResult[0] + "</td><td>" + dateTimeResult[1] + " EST</td></tr></table>";

				// Output footer stock quote.
				$(".share-nav").prepend(footerOutput);

				// Output investor's page stock quote.
				$("#investors-stock-quote").prepend(investorsOutput);
			},
			error: function(data) {
				console.log("Stock Ticker Error:");
				console.log(data);
			}
		});
	});
</script>
    
