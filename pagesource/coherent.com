            var name = $(this).closest('[data-code]').data('name');

            //tell transifex live to translate the page
            //based on user selection
            Transifex.live.translateTo(code, true);
        });
        // End add static links 

        //called when Transifex Live successfully translates the
        //page to a language. In that case lets update the
        //selected language of the widget
        Transifex.live.onTranslatePage(function (language_code) {
            $('#language-current').html(Transifex.live.getLanguageName(language_code));

            // Translate forms
            // var formIframe = document.getElementById('parts');
            var formIframe = document.getElementsByClassName('iframe-contact-form');

            if(formIframe.length){ 
                for(var i=0; i<formIframe.length; i++){
                    formIframeSrc = formIframe[i].src;

                    if(!formIframeSrc.match(/lang=/)){
                        // If there is no language query string, then add it.
                        formIframe[i].src = formIframeSrc + '&lang=' + language_code;
                    }else{
                        // Otherwise replace existing language query string with new language code.
                        var formIframe_src_array = formIframeSrc.split('?');
                        var formIframe_base_url = formIframe_src_array[0];
                        var formIframe_param_array = formIframe_src_array[1].split('&');
                        var proxy_array = [];

                        // Store all the parameters, except the language parameter, so that we
                        // we can reuse them to rebuild the query string. We can then append the 
                        // changed language parameter.
                        if(formIframe_param_array.length){
                            for(var j=0; j<formIframe_param_array.length; j++){
                                if(!formIframe_param_array[j].match('lang=')){proxy_array.push(formIframe_param_array[j]);}
                            }

                            formIframe[i].src = formIframe_base_url + '?' + proxy_array.join('&') + '&lang=' + language_code;
                        }
                    }
                }
            }
        });

        Transifex.live.onTranslatePage(function (lang) {
            if (lang === 'en_US') {
                $('.en').show();
                $('.de').hide();
                $('.fr').hide();
                $('.cn').hide();
                $('.kr').hide();
                $('.sa').hide();
                $('.ja').hide();
            }
            if (lang === 'de') {
                $('.de').show();
                $('.en').hide();
                $('.fr').hide();
                $('.cn').hide();
                $('.kr').hide();
                $('.sa').hide();
                $('.ja').hide();
            }
            if (lang === 'fr_FR') {
                $('.fr').show();
                $('.en').hide();
                $('.de').hide();
                $('.cn').hide();
                $('.kr').hide();
                $('.sa').hide();
                $('.ja').hide();
            }
            if (lang === 'zh') {
                $('.cn').show();
                $('.en').hide();
                $('.de').hide();
                $('.fr').hide();
                $('.kr').hide();
                $('.sa').hide();
                $('.ja').hide();
            }
            if (lang === 'ko') {
                $('.kr').show();
                $('.en').hide();
                $('.de').hide();
                $('.fr').hide();
                $('.cn').hide();
                
                $('.sa').hide();
                $('.ja').hide();
            }
            if (lang === 'es_419') {
                $('.sa').show();
                $('.en').hide();
                $('.de').hide();
                $('.fr').hide();
                $('.cn').hide();
                $('.kr').hide();
                $('.ja').hide();
            }
            if (lang === 'ja') {
                $('.ja').show();
                $('.en').hide();
                $('.de').hide();
                $('.fr').hide();
                $('.cn').hide();
                $('.kr').hide();
                $('.sa').hide();
            }
        });
    });      
</script> 
