  <script type="text/javascript">
    function autoResize4by3(iframe_element)
    {
      iframe_element.width = Math.floor(iframe_element.parentElement.clientWidth - 5) + "px";
      iframe_element.height = Math.floor(iframe_element.parentElement.clientWidth / 4 * 3) + "px";
    }

    function autoResize16by9(iframe_element)
    {
      iframe_element.width = Math.floor(iframe_element.parentElement.clientWidth - 5) + "px";
      iframe_element.height = Math.floor(iframe_element.parentElement.clientWidth / 16 * 9) + "px";
    }

    function resize_iframes()
    {
      var iframe_elements = $('.embed-responsive-iframe-4by3'); // document.getElementsByTagName("iframe");
      for (var j = 0; j < iframe_elements.length; j++)
      {
        autoResize4by3(iframe_elements[j]);
      }

      iframe_elements = $('.embed-responsive-iframe-16by9'); // document.getElementsByTagName("iframe");
      for (var j = 0; j < iframe_elements.length; j++)
      {
        autoResize16by9(iframe_elements[j]);
      }
      $(".lawyers_slider").css("width","100%");
      $(".lawyers-main-layout-middle-right").css("height",($(".lawyers-main-layout-middle-left").height()-56)+"px");
    }

    function resize_iframes_init() {
      $(".article_text").find("iframe").removeClass("embed-responsive-iframe-4by3");
      $(".article_text").find("iframe").removeClass("embed-responsive-iframe-16by9");
      $(".article_text").find("iframe").addClass("embed-responsive-iframe-4by3");
      resize_iframes();
    }
    window.onresize = resize_iframes;
    window.onload = resize_iframes_init;
  </script>

