$(window).scroll(function() {
    
    // calculate the percentage the user has scrolled down the page
    var scrollwin = $(window).scrollTop();
    var articleheight = $('#content_news').outerHeight(true);
    var windowWidth = $(window).width();
    if(scrollwin >= $('#content_news').offset().top){
        if(scrollwin <= ($('#content_news').offset().top + articleheight)){
        $('.bar-long').css('width', ((scrollwin - $('#content_news').offset().top) / articleheight) * 100 + "%"  );
        }else{
            $('.bar-long').css('width',"100%");
        }
    }else{
        $('.bar-long').css('width',"0px");
    }
    
    
});


 
                    
