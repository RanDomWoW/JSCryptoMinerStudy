	$('#myCarousel').on('click', '.banner a', function() {
			clickEvent = true;
			$('.banner div').removeClass('active');
			$(this).parent().addClass('active');		
	}).on('slid.bs.carousel', function(e) {
		if(!clickEvent) {
			var count = $('.banner').children().length -1;
			var current = $('.banner div.active');
			current.removeClass('active').next().addClass('active');
			var id = parseInt(current.data('slide-to'));
			if(count == id) {
				$('.banner div').first().addClass('active');	
			}
		}
		clickEvent = false;
	});
});</script>
