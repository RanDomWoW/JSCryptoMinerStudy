<script>
	document.addEventListener("DOMContentLoaded", function (event) {
		function enableSlider(selector, timeout) {
			var $sliderItems = $(selector);
			var index = -1;
			var moveToNext = function () {
				index++;
				if (index === $sliderItems.length) {
					index = 0;
				}
				$sliderItems.removeClass("active");
				$($sliderItems.get(index)).addClass("active");

				setTimeout(moveToNext, timeout);
			};

			moveToNext();
		}

		enableSlider(".lp-references__customer-list .slider-item", 2000);
		enableSlider(".lp-references__press-list .slider-item", 2000);
	});
</script>
