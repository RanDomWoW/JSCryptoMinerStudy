<script type="application/javascript">
    var currentUserName = null;
    document.addEventListener("DOMContentLoaded", function () {
        (function ($) {
            if (currentUserName) {
                var lastVizzard = null;
                var ratedVizzard = [];
                var lastChangedVizzardKey = currentUserName+ '.LAST_CHANGED_VIZZARD';
                var ratedVizzardKey = currentUserName + '.RATED_VIZZARDS';

                try {
                    lastVizzard = JSON.parse(window.localStorage.getItem(lastChangedVizzardKey));
                } catch (e) {

                }
                try {
                    ratedVizzard = JSON.parse(window.localStorage.getItem(ratedVizzardKey)) || [];
                } catch (e) {

                }

                var dismissDialog = function () {
                    try {
                        window.localStorage.removeItem(lastChangedVizzardKey);
                    } catch (e) {
                    }
                    $('#ratingDialog').removeClass('show');
                };

                window.addEventListener('beforeunload', function(){
                    dismissDialog();
                });

                if (lastVizzard && ratedVizzard.indexOf(lastVizzard.id) === -1) {
                    var ratingForm = document.forms.ratingForm;
                    var closeTimeout = setTimeout(dismissDialog, 5000);
                    $('#ratingDialog').bind('mouseover', function () {
                        closeTimeout && clearTimeout(closeTimeout);
                    });
                    $(ratingForm).bind('submit', function (e) {
                        e.preventDefault();
                        try {
                            ratedVizzard.push(lastVizzard.id);
                            window.localStorage.setItem(ratedVizzardKey, JSON.stringify(ratedVizzard));
                            window.localStorage.removeItem(lastChangedVizzardKey);
                        } catch (e) {

                        }
                        fetch("/api/v1/vizzards/" + lastVizzard.id + "/reviews", {
                            method: 'POST',
                            include: 'credentials',
                            body: JSON.stringify({
                                rating: parseInt(ratingForm.elements.rating.value) || 0,
                                feedback: ratingForm.elements.feedback.value
                            })
                        });
                        $('#ratingContent').removeClass('step-1', 'step-3').addClass('step-2');
                        setTimeout(function () {
                            $('#ratingDialog').removeClass('show');
                        }, 2000);
                    });
                    $('#ratingDialog input[type="radio"]').bind('change', function(){
                        $('#ratingDialog button[type="submit"]').removeAttr("disabled");
                    });
                    $('#ratingDialog *[data-dismiss="modal"]').click(dismissDialog);
                    $('.rating-label-vizzard').html(lastVizzard.title);
                    $('#ratingDialog').addClass('show');
                    $('.rating > label').bind('click mouseover', function () {
                        $('#ratingLabel').html($(this).attr('title'));
                    });
                    $('.rating').bind('mouseleave', function () {
                        var value = ratingForm.elements.rating.value || "3";
                        var title = "Select a rating";
                        if (value) {
                            title = $("label[for='star" + value + "']").attr('title');
                        }
                        $('#ratingLabel').html(title);
                    });
                }
            }
        })(jQuery);
    });
</script>
