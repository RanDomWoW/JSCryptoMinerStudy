				document.addEventListener("DOMContentLoaded", function (event) {
					var isSmallScreen = window.innerWidth <= 768;

					$('#allinone_carousel_charming').allinone_carousel({
						skin: 'sweet',
						width: isSmallScreen ? 800 : 1040,
						height: 450,
						width100Proc: true,
						responsive: true,
						responsiveRelativeToBrowser: false,
						autoPlay: 5,
						target: "_self",
						resizeImages: true,
						autoHideNavArrows: true,
						autoHideBottomNav: true,
						absUrl: "./ext/allinone-carousel/",
						showElementTitle: false,
						showPreviewThumbs: false,
						showCircleTimer: false,
						showNavArrows: !isSmallScreen,
						showBottomNav: !isSmallScreen,
						numberOfVisibleItems: isSmallScreen ? 3 : 5,
						elementsVerticalSpacing: 40,
						elementOrigTop: 480,
						verticalAdjustment: 50,
						animationTime: 0.8,
						circleLeftPositionCorrection: 10,
						circleTopPositionCorrection: 10,
						circleLineWidth: 1,
						circleColor: "#29282B",
						behindCircleColor: "#999999",
						nextPrevMarginTop: -25,
						bottomNavMarginBottom: 0
					});
				});
			</script>

		</div>
	</div>
