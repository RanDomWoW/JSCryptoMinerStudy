<style>
    #ratingDialog {
        top: auto;
        width: 380px;
        margin-top: 100px;
        position: fixed;
        left: 5px;
        bottom: 0;
        transform: translate(0, 600px);
        margin-bottom: 5px;
        transition: transform 0.3s;
        z-index: 10000;
    }

    #ratingDialog .modal-header {
        border-bottom: none;
        padding-bottom: 0;
        padding-top: 20px;
    }

    #ratingDialog .modal-footer{
        border-top: none;
        padding-top: 0;
    }

    #ratingDialog.show {
        transform: translate(0, 0);
    }

    .visible-step-1, .visible-step-2, .visible-step-3 {
        display: none;
    }

    .step-1 .visible-step-1 {
        display: block;
    }

    .step-2 .visible-step-2 {
        display: block;
    }

    .step-3 .visible-step-3 {
        display: block;
    }

     

    .rating {
        border: none;
         
        float: left;
    }

    .rating > input {
        display: none;
    }

    .rating > label:before {
        margin: 5px;
        font-size: 1.25em;
        font-family: FontAwesome;
        display: inline-block;
        content: "\f006";
        cursor: pointer;
    }

    .rating > label {
        color: #FFD700;
        float: right;
        margin-bottom: 0;
    }

    .rating:hover > label:before {
    }

     

    .rating > input:checked ~ label,  
    .rating:not(:checked) > label:hover,  
    .rating:not(:checked) > label:hover ~ label {
        color: #FFD700;
    }

    .rating > input:checked ~ label:before,  
    .rating:not(:checked) > label:hover:before,  
    .rating:not(:checked) > label:hover ~ label:before {
        content: "\f005";
    }

     

    .rating > input:checked + label:hover,  
    .rating > input:checked ~ label:hover,
    .rating > label:hover ~ input:checked ~ label,  
    .rating > input:checked ~ label:hover ~ label {
        color: #FFED85;
    }
</style>
