// api/src/Entity/Organization.php

namespace App\Entity;

use ApiPlatform\Core\Annotation\ApiFilter;
use ApiPlatform\Core\Bridge\Doctrine\Orm\Filter\SearchFilter;

/**
 * ...
 */
class Organization
{
    // ...

    /**
     * @ApiFilter(SearchFilter::class)
     * ...
     */
    public $name;
}</textarea></div>
