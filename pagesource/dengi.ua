  divElement.style.position = 'absolute';
  divElement.style.left = '-99999px';
  divElement.innerHTML = text;
  bodyElement.appendChild(divElement);
  selection.selectAllChildren(divElement);
  setTimeout(function() {
   bodyElement.removeChild(divElement);
  }, 0);
 }; 
 
</script>


