<script type="text/javascript">
var cx=0;
var formData = {"sess_id" : ""};
$('#mfcontrols input, #mfcontrols select').change(function() {
   formData[this.name] = this.value;
   $("#uploadify").uploadify("settings", "formData", formData);
})

$(document).ready(function() {
	$("#uploadify").uploadify({
        'debug'           : false,
    		'swf'             : 'http://imgtown.net/swf/uploadify.swf',
    		'uploader'        : 'http://it1.imgtown.net/cgi-bin/upload_flash.cgi',
    		'cancelImg'       : 'http://imgtown.net/images/cancel.png',
    		//'buttonImg'      : 'http://imgtown.net/images/browse-files.png',
        'buttonText'     : 'SELECT IMAGES',
    		'width'          : 230,
    		'height'         : 40,
    		'queueID'        : 'fileQueue',
    		'auto'           : false,
        'multi'          : true,
        'wmode'          : 'transparent',
        'formData'       : formData,
        'fileTypeExts'   : '*.jpg;*.jpeg;*.gif;*.png;*.bmp',
        'fileTypeDesc'   : 'Select images',
        'queueSizeLimit' : 30,
        'uploadLimit'    : 30,
        'fileSizeLimit'  : '10240KB',
        'onSelect'       : function(){ $('#fileQueue, #mfstart, #mfcontrols').show(); },
        'onUploadStart'  : function(){  },
        'onSWFReady' : function() {
            $('#fileQueue, #mfstart, #mfcontrols').hide();
            $("#uploadify").uploadify('cancel', '*');
        },
        'onQueueComplete': function()
        {
            $('#fileQueue, #mfstart, #mfcontrols').hide();
        },
        'onUploadSuccess' : function(file, data, response){ uploadDone(data) }
    });
});

function uploadDone(response)
{
    $('#ff1_results').show();
    if(response.substr(0,2)!='({'){ alert(response); $('#ff1_results').append('<div class="error">Upload error<\/div>'); return; }

    var res = eval(response);
    if(res.status!='OK'){ $('#ff1_results').append('<div class="error">'+res.name+' : '+res.status+'<\/div>'); return; }
    var code = res.code;
    var fname = res.name;
    var dx = res.dx;
    var link_url = res.link_url;
    var thumb_url = res.thumb_url;
    var hotlink_url = res.hotlink_url;

    forum_code='[URL='+link_url+'][IMG]'+thumb_url+'[/IMG][/URL]';
    html_code='<a href="'+link_url+'" target="_blank"><img src="'+thumb_url+'" border=0><\/a>';

    per_row = $("#advanced1 select[name=per_row]").val();
    cx++;
    if(cx%per_row==0)forum_code=forum_code+'\n';
    if(cx%per_row==0)html_code=html_code+'<br>';

    $('#codes0').show();
    $('#codes0 textarea').val( $('#codes0 textarea').val()+link_url+'\n' );
    
    $('#codes1').show();
    $('#codes1 textarea').val( $('#codes1 textarea').val()+forum_code );
    
    $('#codes2').show();
    $('#codes2 textarea').val( $('#codes2 textarea').val()+html_code+'\n' );

    if(hotlink_url)
    {
        $('#codes3').show();
        $('#codes3 textarea').val( $('#codes3 textarea').val()+hotlink_url+'\n' );
    }

    txt='<table cellspacing=0 cellpadding=2 class="result_slot">'+
      '<tr><td align=right><b>File:<\/b><\/td><td><a href="'+link_url+'"><b>'+fname+'<\/b><\/a><\/td><\/tr>'+
      '<tr><td align=right><b>Preview:<\/b><\/td><td><img src="'+thumb_url+'"><\/td><\/tr>'+
      '<tr><td align=right><b>Direct Link:<\/b><\/td><td><input type="text" onFocus="copy(this);" style="width:99%" value="'+link_url+'"><\/td><\/tr>'+
      '<tr><td align=right width=5% nowrap><b>Forum code:<\/b><\/td><td><input type="text" onFocus="copy(this);" style="width:99%" value="'+forum_code+'"><\/td><\/tr>'+
      '<tr><td align=right><b>HTML code:<\/b><\/td><td><input type="text" onFocus="copy(this);" style="width:99%" value=\''+html_code+'\'><\/td><\/tr><\/table>';

    $('#ff1_results').append(txt);
}
</script>
