<script type="text/javascript">
	$(function(){
		var todaydate = new Date();
		var dd= $('#issue_date').val().split('/');
		var endDate = dd[1] + "/" + dd[0] + "/"+ dd[2];
		$('.cal')
			.datePicker({createButton:false, startDate:'07/09/2002', endDate:todaydate})
			.dpSetSelected(endDate)
			.bind('click', function() {
					$(this).dpDisplay();
					this.blur();
					return false;
				}).bind('dateSelected', function(e, selectedDate, $td, state)
			{
				var dtm = new Date(selectedDate);
				$('#issue_date').val(dtm.asString('mm/dd/yyyy'));
				
				$('#issue_form').submit();
			});
			
		$('.cal').dpSetPosition($.dpConst.POS_TOP+ 25, $.dpConst.POS_RIGHT);
	});
</script>


