	<script type="text/javascript">
		$(document).ready(function()
		{
			$("#writerScrollable").smoothDivScroll({
				manualContinuousScrolling: true,
				autoScrollingMode: "onStart",
				autoScrollingDirection: "endlessLoopRight",
				touchScrolling: true
			});
		});

		$(document ).on('mouseover', '.scrollingHotSpotLeft, .scrollingHotSpotRight', function(e)
		{
			$(this).fadeTo('fast', '.70');
		});

		$(document ).on('mouseout', '.scrollingHotSpotLeft, .scrollingHotSpotRight', function(e)
		{
			$(this).fadeTo('fast', '.35');
		});

		$("#writerScrollable").bind("mouseover", function()
		{
			$(this).smoothDivScroll("stopAutoScrolling");
		});

		// Mouse out
		$("#writerScrollable").bind("mouseout", function()
		{
			$(this).smoothDivScroll("startAutoScrolling");
		});
	</script>


