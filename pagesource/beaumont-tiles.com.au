    <!--******** Count Down Function *****************-->
    <!--CDF(Javascript|/Portals/0/Skins/Beaumont/js/countdown.js)-->
    <!--CDF(Css|/Portals/0/Skins/Beaumont/countdown.css)-->
    
    <script type="text/javascript" charset="utf-8">
    $(function() {
      function reset() {
        var currentDate = new Date();
        $('div.clock').each(function(i) {
          var toDate = $(this).attr("data-countdown");
		  var d, h, m, s;

          
			$(this).countdown(toDate, function(event) {
			var timeFormat = 
				"<div class='timer-block'><div class='number'>%d</div><div class='label'>DAYS</div></div>" + 
				"<div class='timer-block'><div class='number'>%h</div><div class='label'>HOURS</div></div>" + 
				"<div class='timer-block'><div class='number'>%m</div><div class='label'>MINUTES</div></div>" + 
				"<div class='timer-block'><div class='number'>%s</div><div class='label'>SECONDS</div></div>"; //%d day(s) %h:%m:%s
			  switch(event.type) {
			  case "days":
				d = event.value;
				break;
			  case "hours":
				h = event.value;
				break;
			  case "minutes":
				m = event.value;
				break;
			  case "seconds":
				s = event.value;
				break;
			}
			// Assemble time format
			if(d > 0) {
				timeFormat = timeFormat.replace(/\%d/, d);
				timeFormat = timeFormat.replace(/\(s\)/, Number(d) == 1 ? '' : 's');
			} else {
			  	timeFormat = timeFormat.replace(/%d/, '0');
			}
				timeFormat = timeFormat.replace(/\%h/, h);
				timeFormat = timeFormat.replace(/\%m/, m);
				timeFormat = timeFormat.replace(/\%s/, s);
			// Display
			$(this).html(timeFormat);
			});
			});
			}
			reset();
			});
  </script>
    <!--******** End Count Down Function *****************-->
    
    
    
		   
