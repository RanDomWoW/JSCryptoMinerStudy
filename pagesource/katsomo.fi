    state.contentUpdated
      .then(function () {
        console.info('[service-worker] Updated content available.');
      })
    window.addEventListener('load', function() {
      console.info('[service-worker] registering service-worker.js')
      navigator.serviceWorker.register('service-worker.js').then(function(reg) {
        reg.onupdatefound = function() {
          var installingWorker = reg.installing;
          console.info('[service-worker] update for service-worker.js found');
          installingWorker.onstatechange = function() {
            switch (installingWorker.state) {
              case 'installed':
                if (navigator.serviceWorker.controller) {
                  console.debug('[service-worker] New or updated content is available.');
                  resolvePromise()
                } else {
                  console.debug('[service-worker] Content is now available offline!');
                }
                break;

              case 'redundant':
                console.error('[service-worker] The installing service worker became redundant.');
                break;
            }
          };
        };
      }).catch(function(e) {
        console.error('[service-worker] Error during service worker registration:', e);
      });
    });
  }
