    function atualizar_monitoramento() {
        $.ajax({
            type: 'POST',
            url: location.protocol+"//"+location.hostname+"/manualsaude/cet.php",
            dataType: "xml",
            success: function (xml) {
                xml = $(xml).children('transitoagora');

                $('#data').empty().append($(xml).children('data').text() + ' - ' + $(xml).children('hora').text());
                $('#monitorados').empty().append($(xml).children('monitorados').text());
                $('#lentidao').empty().append($(xml).children('lentidaocidade').text());
                $('#percentual').empty().append($(xml).children('percentcidade').text());
                $('#tendencia').empty().append($(xml).children('tendenciacidade').text());


                var lista = '';

                $(xml).children('regiao').each(function () {
                    var id = $(this).attr('ID');
                    var tendencia = $(this).children('tendencia').text();
                    var cor = 'green';
                    if (id.toLowerCase() == 'norte') {
                        cor = 'norte';
                    }
                    else if (id.toLowerCase() == 'oeste') {
                        cor = 'oeste';
                    }
                    else if (id.toLowerCase() == 'centro') {
                        cor = 'centro';
                    }
                    else if (id.toLowerCase() == 'leste') {
                        cor = 'leste';
                    }
                    else if (id.toLowerCase() == 'sul') {
                        cor = 'sul';
                    }
                    lista +=
                    '<div id="regiao"><span class="fonte_cet" style="background: url(/img/layout/' + cor + '.jpg);background-size:20px 20px; ">&nbsp;&nbsp;&nbsp;&nbsp;</span><span class="fonte_cet"><strong> ' + id + ' ' + $(this).children('lentidao').text() + ' km</strong></span> de lentidão</div> ';
                    
                });

                $('#lista_regioes').empty().append(lista);
            },
            error: function (jqXHR, textStatus, errorThrown) {
                //alert(errorThrown);
				if (console) { console.error(errorThrown); }
            }
        });
    }

    $(document).ready(function () {
        jQuery.support.cors = true;
        $('#botao_atualizar').button().bind('click', function () {
            atualizar_monitoramento();
        });
        atualizar_monitoramento();
    });
		</script>

