-moz-box-sizing: border-box;
box-sizing: border-box;
display: block;
position: absolute;
left: 0px;
top: 0px;
height: 100px;
width: 300px;
background-color: rgb(255, 49, 51);
color: white;
padding: 15px;
padding-left: 50px;
padding-right: 50px;
z-index: 10003;
`

const h4Style = `
font-size: 29px;
text-align: center;
padding: 0px;
margin: 0px;
`

const h5Style = `
font-size: 19px;
margin: 0px;
margin-bottom: 7px;
`

const infoStyle = `
-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;
display: block;
position: absolute;
background-color: rgb(233, 233, 233);
left: 0px;
width: 300px;
height: 439px;
z-index: 10002;
padding: 30px;
padding-top: 120px;
`

const iframeStyle = `
position: absolute;
right: -19px;
height: 425px;
border: 0px;
margin-top: 40px;
width: 485px;
clip: rect(0px,466px,399px,19px);
`

const footerText = `
margin-top: 35px;
`
const modalStyle = `
position: fixed;
top: 0;
left: 0;
bottom: 0;
right: 0;
background-color: rgba(17, 17, 17, 0.7);
z-index: 999;
`

const xcirclep = `
margin-top: 8px;
font-weight: 800;`

const xcircle = `
width: 36px;
height: 36px;
background: rgb(244, 243, 244);
-moz-border-radius: 18px;
-webkit-border-radius: 18px;
border-radius: 18px;
float: right;
margin-right: -12px;
margin-top: -12px;
font-size: 18px;
text-align: center;`

const modal =`
  <div id="cintmodal" style="${modalStyle}">
    <div id="cintdiv" style="${style}">
    <div id="cintxdiv">
      <div
      id="cintxcircle"
      style="${xcircle}"
      onMouseOver="this.style.cursor = 'pointer'">
      <p style="${xcirclep}">
      X
      </p>
      </div>
    </div>
    <div style="${overskriktStyle}"><h4 style="${h4Style}">Tjen penger med Spill.no</h4></div>
    <div style="${infoStyle}">
    <h5 style="${h5Style}">1. Registrere deg</h5>
    <h5 style="${h5Style}">2. Svar på undersøkelser</h5>
    <h5 style="${h5Style}">3. Tjen penger</h5>
    <p>Som medlem av Biip.no Brukerpanel får du invitasjoner til enkle brukerundersøkelser på epost. Biip.no er søsterside av Spill.no.
    Svar på disse, så tjener du opp penger som kan veksles inn i produkter eller tas ut som penger.
    </p>
    <p style="${footerText}">
    Brukerundersøkelsene skjer i regi av det anerkjente analyseselskapet CINT AB.
    </p>
    </div>
    <iframe style="${iframeStyle}" src='https://panel.cint.com/c45faf8d-4bb0-464f-abec-27136a79ed27/panelists/new?i=1&c=%2355CAFF'></iframe>
  </div>
  </div>`

const isCookie = name => document.cookie.split(';').filter(x => x.trim().indexOf(name) > -1 ? true : false).length > 0 ? true : false

const setCookie = name => document.cookie = `${name}=true;`

const setListeners = () => {
    const close = () => document.getElementById('cintmodal').style.display = 'none'
    document.getElementById('cintxcircle').addEventListener("click", e => close())
    document.getElementById('cintmodal').addEventListener("click", e => e.target.id === 'cintmodal' ? close() : null)
}

const setModal = () => setTimeout(() => {
    $(document.body).append(modal);
    setCookie('CintABTjenPenger')
    setListeners()
  }, 2500)

const run = () => !isCookie('CintABTjenPenger') ? setModal() : null

run()

})
</script>



