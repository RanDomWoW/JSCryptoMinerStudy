        $(window).load(function() {
            $('.animation').flexslider({
                animation: "slide",
                directionNav: true 
            });
        
        });

        $(function(){
            $("#slides").slides({
                preload: true,
                play: 8000,
                pause: 2500,
                hoverPause: true
            });
        });

        $(document).ready(function() {
            $('#logos').innerFade({
                timeout: 5000,
                speed: 2300
            });
            var indexChart;
            var subindexChart;
            var compareChart;
            var stockChart;

            //This function draws stock chart.
            function drawChart(chartContainer, rangeValue, indexID, indexTitle, type) {
                urlbase = 'http://www.nepalstock.com.np/graphdata/';
                url = urlbase+indexID+'/'+rangeValue;

                if(type == 4) {
                  url = 'http://www.nepalstock.com.np/company/graphdata/'+indexID+'/'+rangeValue;
                };
                $(chartContainer).addClass('loading');
                $.getJSON(url, function(data) { // get data from url

                  $(chartContainer).removeClass('loading');
                  if($(chartContainer).html() == '') { // If there was no chart in the beginning

                    var chartoptions = {
                      chart: {
                        alignTicks: false
                      },
                      rangeSelector : {
                        buttons: {},
                        selected: 6,
                      },
                      navigator: {
                        enabled: true,
                      },
                      legend: {
                          layout: 'horizontal',
                          enabled: true,
                      },
                      credits: {
                        enabled: false
                      },
                      title: {

                      },
                      yAxis: [{
                          title: {
                              
                          }
                      }],         
                      series : [{
                        tooltip: {
                          valueDecimals: 2
                        },
                        type: 'area',
                        threshold: null,
                        
                      }],
                      

                    }; // Default Options for Chart

                    chartoptions.title.text = indexTitle; // set Title of graph
                    chartoptions.series[0].name = indexTitle; // Set name of series
                    chartoptions.series[0].data = data; // set series data

                    if(type == 1) { // Checking if we're dealing with index Chart or Sub Index Chart
                      chartoptions.chart.renderTo = $(chartContainer)[0];
                      indexChart = new Highcharts.StockChart(chartoptions);
                    } else if(type == 2) {
                      chartoptions.chart.renderTo = $(chartContainer)[0];
                      subindexChart = new Highcharts.StockChart(chartoptions);
                    } else if(type == 3) {
                        chartoptions.chart.renderTo = $(chartContainer)[0];
                        chartoptions.series[0].name = 'NEPSE';
                        $.getJSON(urlbase+'57'+'/'+rangeValue, function(data1){

                          chartoptions.yAxis.push({
                              title: {
                                  text: 'Sensitive',
                              },
                              opposite: false,
                          });

                          chartoptions.series[0].yAxis= 0;
                          chartoptions.series[0].type= 'line';
                          chartoptions.series.push({
                              tooltip: {
                                valueDecimals: 2
                              },
                            
                              threshold: null,
                              data: data1,
                              name: 'Sensitive',
                              yAxis: 1,
                            });
                          chartoptions.navigator.enabled= false;
                          console.log(chartoptions);
                          compareChart = new Highcharts.StockChart(chartoptions);
                        });
                        
                    } else if(type ==4) {
                      chartoptions.chart.renderTo = $(chartContainer)[0];
                      stockChart = new Highcharts.StockChart(chartoptions);
                    };

                    return;
                  } else { // If the chart already exists
                    if(type == 1) { // Checking if we're dealing with index Chart or Sub Index Chart
                      indexChart.series[0].setData(data, false);
                      //indexChart.series[0].update({name: indexTitle});
                      indexChart.setTitle({text: indexTitle});
                      // var nav = indexChart.get('navigator');
                      //     nav.setData(data);
                      indexChart.xAxis[0].setExtremes();
                      indexChart.redraw();
                    } else if(type == 2) {
                      subindexChart.series[0].setData(data, false);
                      subindexChart.series[0].name = indexTitle;
                      subindexChart.setTitle({text: indexTitle});
                      subindexChart.xAxis[0].setExtremes();
                      subindexChart.redraw();
                      
                    } else if(type == 3) {
                        $.getJSON(urlbase+'57'+'/'+rangeValue, function(data1){
                          compareChart.series[0].setData(data, false);
                          compareChart.series[1].setData(data1, true);

                          compareChart.xAxis[0].setExtremes();
                          compareChart.redraw();
                        })
                        
                    } else if(type==4){
                      stockChart.series[0].setData(data, false);
                      stockChart.series[0].name = indexTitle;
                      stockChart.setTitle({text: indexTitle});
                      stockChart.xAxis[0].setExtremes();
                      stockChart.redraw();
                    }
                  } 

                });  
            }

            $('.btn-group .btn').on('click', function(e){
              $(this).parent().find('.active').removeClass('active');
              $(this).addClass('active');
            }) 

            $("#indexSelect").on("change", function(e){
              e.preventDefault();
              indexTitle = $('#indexSelect').find('option:selected').html();
              indexID = $('#indexSelect').val();
              rangeValue = $('#indexSelect').parent().find('.btn.active').attr('data-val');
              chartContainer = '#market-watch #option1 .graph-container';

              drawChart(chartContainer, rangeValue, indexID, indexTitle, 1);
            });

            $("#subindexSelect").on("change", function(e){
              e.preventDefault();
              indexTitle = $('#subindexSelect').find('option:selected').html();
              indexID = $('#subindexSelect').val();
              rangeValue = $('#subindexSelect').parent().find('.btn.active').attr('data-val');
              chartContainer = '#market-watch #option2 .graph-container';

              drawChart(chartContainer, rangeValue, indexID, indexTitle, 2);
            })

            $("#indexSelectRange .btn").on("click", function(e){
              
              indexTitle = $('#indexSelect').find('option:selected').html();
              indexID = $('#indexSelect').val();
              rangeValue = $('#indexSelect').parent().find('.btn.active').attr('data-val');
              chartContainer = '#market-watch #option1 .graph-container';

              drawChart(chartContainer, rangeValue, indexID, indexTitle, 1);
            });

            $("#subindexSelectRange .btn").on("click", function(e){
              
              indexTitle = $('#subindexSelect').find('option:selected').html();
              indexID = $('#subindexSelect').val();
              rangeValue = $('#subindexSelect').parent().find('.btn.active').attr('data-val');
              chartContainer = '#market-watch #option2 .graph-container';

              drawChart(chartContainer, rangeValue, indexID, indexTitle, 2);
            });

            $("#compareRange .btn").on("click", function(e){
              
              indexTitle = "NEPSE Vs. Sensitive";
              indexID = 58;
              rangeValue = $('#compareRange').find('.btn.active').attr('data-val');
              chartContainer = '#market-watch #option3 .graph-container';

              drawChart(chartContainer, rangeValue, indexID, indexTitle, 3);
            });

            $("#stockChartSelect").on("change", function(e){
              e.preventDefault();
              indexTitle = $('#stockChartSelect').find('option:selected').html();
              indexID = $('#stockChartSelect').val();
              chartContainer = '#market-watch #option4 .graph-container';
              rangeValue = $('#companyRange').find('.btn.active').attr('data-val');

              drawChart(chartContainer, rangeValue, indexID, indexTitle, 4);
            });

            $("#companyRange .btn").on("click", function(e){
              e.preventDefault();
              indexTitle = $('#stockChartSelect').find('option:selected').html();
              indexID = $('#stockChartSelect').val();
              chartContainer = '#market-watch #option4 .graph-container';
              rangeValue = $('#companyRange').find('.btn.active').attr('data-val');

              drawChart(chartContainer, rangeValue, indexID, indexTitle, 4);
            })

            $("#annual-list").change(function(){
              var option = $("#annual-list").val();
              if(option.toLowerCase() == 'annual'){
                $('#semi-annual').hide();
                $('#annual').show();
              }
              if(option.toLowerCase() == 'semi-annual'){
                $('#annual').hide();
                $('#semi-annual').show();
              }
            })

            $(".nav-tabs li#indexTab").on('click', function(e){
              $("#indexSelect").trigger("change");
            });

            $(".nav-tabs li#subindexTab").on('click', function(e){
              $("#subindexSelect").trigger("change");
            });

            $(".nav-tabs li#compareTab").on('click', function(e){
              $("#compareRange .btn.active").trigger("click");
            });

            $('#annual-list').trigger("change");
            $(".nav-tabs li:first-child").trigger("click");
        });



        
    </script>

