<script type="text/javascript">
  $jq("nav").autoHidingNavbar();
  $jq(window).scroll(function () {
    if ($jq(this).scrollTop() > 0)
    {
      $jq('#fixed-logo').fadeOut();
    }
    else {
      $jq('#fixed-logo').fadeIn();
    }
  });
  var isVisible = false;
  $jq(window).scroll(function () {
    var shouldBeVisible = $jq(window).scrollTop() > 300;
    if (shouldBeVisible && !isVisible) {
      isVisible = true;
      $jq('#GPlus').fadeIn();
    } else if (isVisible && !shouldBeVisible) {
      isVisible = false;
      $jq('#GPlus').fadeOut();
    }
  });
  $jq('#SearchModal').on('shown.bs.modal', function () {
    $jq('#seach_text').focus();
  })
  $jq("#seach_text").keyup(function (event) {
    if (event.keyCode == 13) {
      $jq("#search").click();
    }
  });
  $jq("#search").click(function () {
    var bla = $jq('#seach_text').val();
    var bla2 = bla;
    bla2 = bla2.trim();
    if (bla2 == '') {
      $jq('#seach_text').val("");
      $jq('#seach_text').focus();
    }
    else {
      var url = "http://apfco.com/search:" + bla;
      setTimeout("window.location.href='" + url + "';", 0);
    }
  });
</script>
