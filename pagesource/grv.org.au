<script>

    jQuery(document).ready(function($) {  
		$("#widgetFtDogSearchForm").submit(function() {	 
var dogName= $("#wdogsearch").val();
var dogNameTruncated = dogName.split("(");

if (dogNameTruncated[0] != "") {
	var url="http://www.grv.org.au/racing/dog-search/?dog="+dogNameTruncated[0].trim();
	window.location.href = url;
	}
return false;
});
		$(function() {               

			var searchRequest = null;
			$("#wdogsearch").autocomplete({
			    minLength: 3,
			    source: function(request, response) {
			        if (searchRequest !== null) {
			            searchRequest.abort();
			        }
			        searchRequest = $.ajax({
			            url: 'https://www.grv.org.au/api/api.php?action=searchDogs&dogname='+request.term.trim(),
			            method: 'post',
			            dataType: "json",
			            data: {term: request.term},
			            success: function(data) {
			                searchRequest = null;
			                response($.map(data.items, function(item) {
			                    return {
			                        value: item.name,
			                        label: item.name
			                    };
			                }));
			            }
			        }).fail(function() {
			            searchRequest = null;
			        });
			    }
			});

		});    
	});
</script>



