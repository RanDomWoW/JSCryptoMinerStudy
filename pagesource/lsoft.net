    <script type="text/javascript">
$(function() 
{
	var $slides = $('.banners > .banner'),
	$navigation_container = $('.banner-navigation'),
	$navigation_items = $('li > a', $navigation_container),
	$abs_el = $('#banner_1 > .banner-abs-el_main-box');

	function setActiveTab($menu_link) 
	{
		var $tab = $menu_link.data().tab,
		tab_index = $menu_link.data().i;
		
		if (tab_index == '1') {
			$abs_el.appendTo('.great-block-content_main-page').css(
			{
			'top': '116px',
			'left': '389px',
			'display': 'block'
			});
		} else 
			$abs_el.hide();
				
		$navigation_items.not($menu_link).removeClass('active');
		$menu_link.addClass('active');
		$navigation_container.attr('id', 'active-' + $menu_link.data().i);
		$slides.not($tab).hide();
		$tab.css({
			'display': 'block',
			'opacity': .15
			}).stop(true, true).animate({'opacity': 1}, 230);
	}
		
	$navigation_items.each(function(i, el) 
		{
			var $item = $(el);
			$item.data({
			'i': i + 1,
			'tab': $slides.filter($item.attr('href'))
			});
		}).live('click', function(e) 
			{
				e.preventDefault();
				var $clicked_item = $(this);
				if ($clicked_item.hasClass('active')) return;
				setActiveTab($clicked_item);
			});
	
	
	setActiveTab($navigation_items.first());
	
	if ($.browser.msie && ($.browser.version << 0) < 7) {
		$navigation_container.css('background', 'transparent');
		}

	var slides_len = $navigation_items.length;
	
	function slidingMainSlider() {
		var $active = $('.active', $navigation_container),
		$next = {};
		if (!$active.length) return;
		if ($active.data().i < slides_len) {
			$navigation_items.each(function(i, el) 
				{
					var $item = $(el);
					if ($item.data().i != ($active.data().i + 1)) return;
					$next = $item;
				});
				setActiveTab($next);
		} 
		else 
			setActiveTab($navigation_items.first());
	}
	
	var main_slider_interval = 5000;
	var var_main_slider_interval = setInterval(slidingMainSlider, main_slider_interval);
	$slides.add($navigation_container).hover(
		function() {
			clearInterval(var_main_slider_interval);
		},
		function() {
			var_main_slider_interval = setInterval(slidingMainSlider, main_slider_interval);
		}
	);
});

</script> 	

