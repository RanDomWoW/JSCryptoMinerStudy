<script type="text/javascript">
	$(function() {
		$("#map li").click(function() {
			var num = $("#map li").index(this);
			$(".map").addClass('disnon');
			$(".map").eq(num).removeClass('disnon');
			$("#map li").removeClass('active');
			$(this).addClass('active')
		});
	});
</script>
<script>
$(function(){
	//初期段階にてダミー画像は透明にしておく
	$("#mapImg").css({
		"opacity": "0"
	});
    //areaのマウスオーバーにて画像を置換
    $("area").hover(function(){
        $("#mapImg").stop();
        var areaId = $(this).attr("id");
        var changeareaId = areaId.replace(/(.*)/, "nav-$1" );
        $("#mapImg").attr("src", "/base/pc/img/map/" + areaId + ".png").fadeTo(200, 1);
        $("[id='" + changeareaId + "']").addClass("selected");
	},
    function(){
        $("#mapImg").stop();
        $("#mapImg").attr("src", "/base/pc/img/map/dummy.png").css({
            "opacity": "0"
        });
		$("li").attr("class", "")
	});
	    //li aのマウスオーバーにて画像を置換
    $(".map li a").hover(function(){
        $("#mapImg").stop();
        var listId = $(this).parent("li").attr("id");
        var changeListId = listId.replace(/nav-(\w.)/, "$1");
        $("#mapImg").attr("src", "/base/pc/img/map/" + changeListId + ".png").fadeTo(200, 1);
    },
    function(){
        $("#mapImg").stop();
        $("#mapImg").attr("src", "/base/pc/img/map/dummy.png").css({
            "opacity": "0"
        });
    });
});
</script>

