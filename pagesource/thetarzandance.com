<script>
function getParameterByName(name, url) {
    if (!url) url = window.location.href;
    url = url.toLowerCase(); // This is just to avoid case sensitiveness  
    name = name.replace(/[\[\]]/g, "\\$&").toLowerCase();// This is just to avoid case sensitiveness for query parameter name
    var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
        results = regex.exec(url);
    if (!results) return null;
    if (!results[2]) return '';
    return decodeURIComponent(results[2].replace(/\+/g, " "));
}
 
function init() {
    if (typeof (window.runLandingPageJs) != "undefined" && !window.runLandingPageJs) {
        return;
    }
    var install = getParameterByName("install")
    var uninstall = getParameterByName("uninstall");
	var src = "http://a.etfsrv.com/jbl";
  if (uninstall == "true" || window.location.pathname.indexOf("uninstalled") != -1 ||
		window.location.pathname.indexOf("uninstall-success") != -1)
	{
		src = "http://a.etfsrv.com/jbu";
	}    
	else if (install == "true" || window.location.pathname.indexOf("installed") != -1 ||
        window.location.pathname.indexOf("install-success") != -1) {
        src = "http://a.etfsrv.com/jbs";
    }
    var script = document.createElement("script");
    script.src = src;
    document.body.appendChild(script);
}

init();
  </script>
