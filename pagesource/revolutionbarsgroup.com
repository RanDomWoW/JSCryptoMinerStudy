		<script>
			$(function() {
				var banner = $("#home_banner");

				banner.slick({
					appendArrows: '.slick-arrows',
					cssEase: 'linear',
					dots: false,
					fade: true,
					infinite: true,
					slide: '.home_banner__slide',
					speed: 400,
				});

				banner.on('beforeChange', function(event, slick, currentSlide, nextSlide) {
					$('.slick-current').find('.home_banner__text').fadeOut(200);
					$('.slick-current').find('.home_banner__extra').fadeOut(200);
				});

				banner.on('afterChange', function(event, slick, currentSlide, nextSlide) {
						$('.slick-current').find('.home_banner__text').fadeIn(600);
						$('.slick-current').find('.home_banner__extra').fadeIn(600);
				});
			});
		</script>
