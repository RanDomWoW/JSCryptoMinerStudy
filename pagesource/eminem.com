    aeJS.events.onLogin.addHandler(function(user, type){ require(["IGA.events"], function(events){ events.trigger("ae_login", [user, type]); }); });
    aeJS.events.onLogout.addHandler(function(){ require(["IGA.events"], function(events){ events.trigger("ae_logout"); }); });
    aeJS.events.onWindow.addHandler(function(state){ require(["IGA.events"], function(events){ events.trigger("ae_window", [state]); }); });
