        /* //siteurl.com/sign-in?redir=/path/to/page&timeout=true */
        window.timeOutScr = function () {
            if(window.reqAuth == 'true') {
                var loc = window.location.host;
                var newLoc = window.location.protocol + '//' + loc + '/sign-in' + redir;
                window.location = newLoc;
            } else {
                if (window.location.search.indexOf('timeout') <0) {
                    window.reloadLoc = document.location.pathname + addUrlParam(document.location.search, 'timeout', 'true');
                    window.location = window.reloadLoc;
                } else {
                    window.location.reload();
                }
            }
        }
        if (window.timeOut != 0) {
            setTimeout(window.timeOutScr,window.timeOut);
        }

        try {
            //if(typeof Sys.WebForms == "undefined")  throw new Error('Sys.WebForms is undefined');
            Sys.WebForms.PageRequestManager.getInstance().add_endRequest(endRequestHandler);
        } catch(e){}

        /**
        * Add a URL parameter (or changing it if it already exists)
        * @param {search} string  this is typically document.location.search
        * @param {key}    string  the key to set
        * @param {val}    string  value 
        */
        var addUrlParam = function(search, key, val){
            var newParam = key + '=' + val,
                params = '?' + newParam;

            // If the "search" string exists, then build params from it
            if (search) {
                // Try to replace an existance instance
                params = search.replace(new RegExp('[\?&]' + key + '[^&]*'), '$1' + newParam);

                // If nothing was replaced, then add the new param to the end
                if (params === search) {
                    params += '&' + newParam;
                }
            }

            return params;
        };
