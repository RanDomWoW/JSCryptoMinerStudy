    $('#formConsultaProcessual').find('select[name="consultaProcessualInstancia"]').change(
      function() {

        var instancia = $(this).val();
        var selectComarca = $('select[name="consultaProcessualComarca"]');
        var arraySelecionado = null;

        if (instancia == 'pg') {
          arraySelecionado = primeiroGrauArray;
        } else if (instancia == 'sg') {
          arraySelecionado = segundoGrauArray;
        } else if (instancia == 'jz') {
          arraySelecionado = juizadoArray;
        }

        selectComarca.empty();
        $.each(arraySelecionado,
          function (value, label) {
            selectComarca.append('<option value="' + value + '">' + label + '</option>');
          }
        );
      }

    );

    $('#formConsultaProcessual').submit(function() {

      var url = $(this).find('select[name="consultaProcessualComarca"]').val();
      var parametro = $(this).find('select[name="consultaProcessualParametro"]').val();
      var texto = $(this).find('input[name="consultaProcessualTexto"]').val();
      texto = unescape(encodeURIComponent(texto));

      if (parametro == 'NUMPROC' && texto.length >= 20) {
        var numeroProcesso = texto.replace(/\D+/g , "");
        var numero = numeroProcesso.substr(0, 13);
        var foro = numeroProcesso.substr(16, 4);
        address = '/' + url;
        address += '&conversationId=';
        address += '&cbPesquisa=' + parametro;
        address += '&dadosConsulta.tipoNuProcesso=UNIFICADO';
        address += '&tipoNuProcesso=UNIFICADO';
        address += '&numeroDigitoAnoUnificado=' + numero;
        address += '&foroNumeroUnificado=' + foro;
        address += '&dadosConsulta.valorConsultaNuUnificado=' + numeroProcesso;
        address += '&valorConsultaNuUnificado=' + numeroProcesso;
        address += '&dePesquisaNuUnificado=' + numeroProcesso;
      } else {
        address = '/' + url;
        address += '&cbPesquisa=' + parametro;
        address += '&dePesquisa=' + texto;
        address += '&tipoNuProcesso=SAJ';
      }

      var win = window.open(address, '', 'width=790,height=390,scrollbars=YES,menubar=YES,resizable=YES,toolbar=YES,location=YES');
      win.focus();

      return false;

    });

