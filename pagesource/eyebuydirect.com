<script>
    var buttonInfo = [
            ];
    bannerButton = function(){
        var _picP = $(".index-banners-image").position();
        var _picW = $(".index-banners-image").width();
        var _parentW = $(".index-banners-image").closest('.index-banners').width();
        var offset =  0 - (_picW - _parentW) / 2;
        var _picPercent = (_picW / 1600);
        if (_picW < 768) {
             _picPercent = (_picW / 768);
        }

        if (buttonInfo) {
            for (var i in buttonInfo) {
                var linkConfig = buttonInfo[i];
                var array = {};
                for(var j in linkConfig.bigImagePosition) {// Two point
                    var pos = _picW >= 768 ? linkConfig.bigImagePosition[j] : linkConfig.smallImagePosition[j];
                    array[j] = [pos.x * _picPercent, pos.y * _picPercent];;
                }
                var newWidth = array[1][0] - array[0][0];
                var newHeight = array[1][1] - array[0][1];
                $(linkConfig.contentName).css({'width': newWidth, 'height': newHeight, 'left': array[0][0] + offset, 'top' : array[0][1]});
            }
        }
    }
    bannerButton();
    $(window).resize(function(){
        bannerButton();
    });

