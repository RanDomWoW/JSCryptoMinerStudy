                $.popup($fpop, {'class_name': ['rectangle','event-first-visit']}, function(){
                    if ($.cookie('pop_delay') != 2) {//record popup show for GTM
                        $.cookie('pop_delay', 2, {'expires': expireDays, 'path': '/'});
