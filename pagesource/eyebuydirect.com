<script type="text/javascript">
    $(function() {
    var amazonAuthRequest,
        amazonButtonId = 'amazon-login-btn-head';
        $amazonLoginBtn = $('#' + amazonButtonId);
    OffAmazonPayments.Button(amazonButtonId, "	A3B49UYQ1HEINM", {
        type: "LwA",
        color: "Gold",
        size: "middle",
        authorization: function() {
            if ('desktop' != ebdDeviceType) {
                loginOptions = {scope: "profile postal_code payments:widget payments:shipping_address", popup: false};
                amazonAuthRequest = amazon.Login.authorize(loginOptions, '/user/amazon-login');
            } else {
                var redirect = "/";
                loginOptions = {scope: "profile postal_code payments:widget payments:shipping_address", popup: true};
                amazonAuthRequest = amazon.Login.authorize(loginOptions, function(response) {
                    if (response.error) {
                        $.alert(response.error);
                        return;
                    } else {
                        var token = response.access_token;
                        $amazonLoginBtn.ajaxAuto({
                            'data': {'access_token' : token, 'redirect': redirect},
                            'success': function(response){
                                if (response.status == 'ok') {
                                    if (response.redirect != '') {
                                        window.location = response.redirect;
                                    } else {
                                        window.location.reload();
                                    }
                                } else {
                                    if (response.msg != '') {
                                        $.alert(response.msg);
                                    } else {
                                        window.location.reload();
                                    }
                                }
                            }
                        });
                    }
                });
            }
        },
        onError: function(error) {
        }
    });
    });
    $('#amazon-logout-btn').click(function(){
        amazon.Login.logout();
        $(this).ajaxAuto();
        return false;
    });

</script>
