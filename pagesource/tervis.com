// Offer Drawer
$(document).ready(function() {
  checkout();
  banner();
  drawer();

  function checkout() {
    if ($(".checkout-header").length > 0) {
      $("#offer-drawer").css({ display: "none" });
    } else {
      $("#offer-drawer").css({ display: "block" });
    }
  }

  function banner() {
    if ($('div.promotion-callout:contains("Last Call")').length > 0 ||
      window.location.href.indexOf("last-call") > -1){
      $(".lastcall-banner").css({ display: "block" });
		} else {
      $(".freeshipping-promo-banner").css({ display: "block" });
   }	
  }
  function drawer() {
    $(".swiper-slide").css({ visibility: "visible" });

    setTimeout(function() {
      // Setting variable so we only access the DOM once
      var arrow = $("#offer-arrow");
      var slide = $(".offer-slide");
      var height = $("#swiper-box").height();
      var n = -1;
      var drawerDistance = height * n;

      $(slide).css({ marginBottom: drawerDistance + "px" });

      $(document).on("click", ".offer-button", function() {
        var height2 = $("#swiper-box").height();
        var n2 = -1;
        var drawerDistance2 = height2 * n2;

        if ($(slide).css("marginBottom") != "0px") {
          $(arrow).prop(
            "src",
            "https://www.tervis.com/on/demandware.static/-/Sites-Tervis-Library/default/dwc181ef20/images/interface/down-arrow-white.svg"
          );
          $(slide).animate({ marginBottom: "0px" });
        } else if ($(".offer-slide").css("marginBottom") == "0px") {
          $(arrow).prop(
            "src",
            "https://www.tervis.com/on/demandware.static/-/Sites-Tervis-Library/default/dw9e966792/images/interface/up-arrow-white.svg"
          );
          $(slide).animate({ marginBottom: drawerDistance2 + "px" });
        }
      });
    }, 1000);
  }
	
$(document).on("click", function(){
var checkForInputTimer;
var checkForInput = function () {
		window.clearInterval(checkForInputTimer);
		banner(); 
};

checkForInputTimer = window.setInterval(checkForInput, 1000);
	
		   });
	

  $(window).on("resize", function() {
    checkout();
    banner();
    drawer();
  });
});

  
</script>
