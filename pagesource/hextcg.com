<script type="text/javascript">

// At the top of your script:
if ( ! window.console ) console = { log: function(){} };

$(window).bind("load", function() {
   // code here
   pageLoad();
});

//$(pageLoad);

//window.onload = function () { pageLoad(); };

function pageLoad()
{
	console.log("Activate mail chimp...");
	$failure = false;
	//alert("Debugging...");
	
	//append message
	$widgetTitle = $('.widgettitle');
	//console.log($widgetTitle.html());
	$widgetTitle.after("<p id='popMessage'>Enter your email below and we'll let you know when the HEX open beta launches.</p>");
	
	
	$betaButton = $("#menu-item-2266");
	$window = $(window);
	
	$betaButton.click(loadDataCapture);
	
	
	//sign up click
	$signUpBtn = $("#ns_widget_mailchimp_form-2 input.button");
	$signUpBtn.click(onSignUpClick);
}

function onSignUpClick()
	{
		// "console" in ie7 throws an error use the example below instead 
		// REF: http://stackoverflow.com/questions/7585351/testing-for-console-log-statements-in-ie
		console.log("Sign up clicked...");
		
		$emailInput = $("#ns_widget_mailchimp-email-2");
		$emailValue = $emailInput.val();
		
		if( !isValidEmailAddress( $emailValue ) ) 
		{ 
			/* do stuff here */ 
			//REF - http://stackoverflow.com/questions/2855865/jquery-regex-validation-of-e-mail-address
			
			$failure = true;		
		}
		else
		{	
			//console.log("valid email...");
			$popMessage = $('#popMessage');
			console.log($popMessage.html());
			$popMessage.fadeOut(500);
			
		}
	}
	
	function isValidEmailAddress(emailAddress) {
		var pattern = new RegExp(/^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);
		return pattern.test(emailAddress);
	};
	
	function loadDataCapture()
	{
		//console.log("Beta clicked....");
		//console.log("Body height: " + $("body").height());
		
		$bodyHeight =  $("body").height();
		
		betaPopUp = $(".betaPopup");
		$betaForm = $("#ns_widget_mailchimp-2");
		$betaForm.hide(10).fadeIn(500);
		
		betaPopUp.fadeIn(200);
		betaPopUp.css({
					  	"width" : "100%",
						"height" : $bodyHeight + "px",
						"z-index" : 999999
					  });
		
		//close button
		$closeBtn = $("#closeBtn");
		$closeBtn.click(closeBetaPopUp);
		
		
		$window.scroll(onScroll);
		
		console.log("Failure..." + $failure);
	}
	
	function closeBetaPopUp()
	{
		if($failure == true)
		{
			$('.error').hide();
			$('#ns_widget_mailchimp-email-2').val('');
			
			console.log("Clear error...");
		}
		
		betaPopUp = $(".betaPopup");
		betaPopUp.fadeOut(200, hideBetaPopUp);
	}
	
	function hideBetaPopUp()
	{
		betaPopUp = $(".betaPopup");
		betaPopUp.css({
					  	"width" : "0px",
						"height" : "0px",
						"z-index" : 0
					  });
		console.log("hide beta pop up...");
	}
	
	function onScroll()
	{
		//console.log("Page scrolling...");
		$windowHeight = $window.height();
		$scrolledSpaceHeight = $window.scrollTop();
		
		betaPopUp = $(".betaPopup");
		betaPopUp.css({
					  	"width" : "100%",
						"height" : "100%"
					  });
	}
</script>

