/* @Main Menu Drop Down Menus
===================================== */
#NavContainer {
    overflow: visible;
}

#Nav {
    position: relative;
    z-index: 5;
	margin-top: 5px;
}

#Nav ul,
#Nav li {
	display: block;
    position: relative;
}

#Nav a {
    padding: 1.1em 13px;
}

#Nav li ul{
	position:absolute;
	z-index: 1500;
	clear:both;
	
	width:auto;
	height:0;
	overflow: hidden;
	margin:0 0 0 0;
	border-top: none;
	
	opacity:0;
	transition: opacity .25s ease-in-out; 
	-moz-transition: opacity .25s ease-in-out;
	-webkit-transition: opacity .25s ease-in-out;
}

#Nav li:hover ul{
	height:auto;
	overflow: visible;
	padding:0;
	opacity:1;
	
	left:0;
	
	-webkit-box-shadow: 0px 2px 8px 2px rgba(50, 50, 50, 0.25);
	-moz-box-shadow:    0px 2px 8px 2px rgba(50, 50, 50, 0.25);
	box-shadow:         0px 2px 8px 2px rgba(50, 50, 50, 0.25);
}

#Nav li ul.sub-menu {
	background: #f5f5f5;
}

#Nav li ul.sub-menu:after {
	bottom: 100%;
	left: 10%;
	border: solid transparent;
	content: " ";
	height: 0;
	width: 0;
	position: absolute;
	pointer-events: none;
	border-color: rgba(245, 245, 245, 0);
	border-bottom-color: #f5f5f5;
	border-width: 8px;
	margin-left: -8px;
}

#Nav li li ul{	
	width:auto;
	height:0;
	overflow: hidden;
	opacity:0 !Important;
	transition: opacity .25s ease-in-out; 
	-moz-transition: opacity .25s ease-in-out;
	-webkit-transition: opacity .25s ease-in-out;
	
	left:250px;
	top: 0;
}

#Nav li li:hover ul{
	height:auto;
	overflow: visible;
	padding:0;
	opacity: 1 !Important;
	
	-webkit-box-shadow: 5px 5px 10px -6px rgba(0, 0, 0, 0.5);
	-moz-box-shadow: 5px 5px 10px -6px rgba(0, 0, 0, 0.5);
	box-shadow: 5px 5px 10px -6px rgba(0, 0, 0, 0.5);
	
	z-index:1000;
}

#Nav li li{
	float: none;
	padding:0;
	margin:0;
	height: auto;
	font-size: 0.8em;
	text-align:left;
	min-width: 250px;
	top: 0;
}


#Nav li.current_page_ancestor li a,
#Nav li.current_page_item li a,
#Nav li li a{
	display: block;
	padding: 0.75em 2em 0.75em 1.25em;
	margin:0;
	color:#535353 !important;
	background:#f5f5f5 !important;
	line-height: 1.25em;
	
	transition: all .25s ease-in-out; 
	-moz-transition: all .25s ease-in-out;
	-webkit-transition: all .25s ease-in-out;
	border-bottom: 1px solid rgba(180, 180, 180, 0.25);
	
	-moz-box-shadow: none;
    -webkit-box-shadow:none;
    box-shadow: none;
}

#Nav li li:last-child a{
	border-bottom: none;
}

#Nav li li:last-child li a{
	border-bottom: none;
}

#Nav li li.current_page_item a,
#Nav li li a:hover{
	color:#535353;
	background:#e8e8e8 !important;
	
	-moz-box-shadow: none;
    -webkit-box-shadow:none;
    box-shadow: none;
}

#Nav li.current-page-ancestor a, #Nav  {
    -moz-box-shadow: inset 0px 0px 0px rgba(0,0,0,0);
