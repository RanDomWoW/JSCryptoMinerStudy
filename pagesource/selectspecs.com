<script type="text/javascript">
    $('button.search').click(function() {
        var form = $(this).parent('form');
        var mob = (form.attr('id') == 'searchformMob' ? 'Mob' : '');
        var searchInput = form.find('#inputString' + mob).eq(0);
        var searchAll = form.find('#all').prop('checked');
        var str = searchInput.val();
        if (str.length) {
            var str = str.replace('/', ' ').replace(/\s+/g, ' ');
            if(str.length == 0) {
                return false;
            }
            document.location.href = CoreVars.base_url + '/catalogue/index/term/' + encodeURI(str) + '?q=' + encodeURI(str) + '&all=' + (searchAll ? 1 : 0)
                + '&back=' + document.location.protocol + '//' + document.location.host + document.location.pathname;
        } else {
            return false;
        }
        return false;
    });
</script>
