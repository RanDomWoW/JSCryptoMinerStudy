                        }
                    }
                    });

                    $('#wayblazer-query-textfield').focus(function() {
                        while (timer) {
                            window.clearTimeout(timer); // will do nothing if no timeout with id is present
                            timer--;
                        }

                    $('#wayblazer-query-textfield').val('');
