                        var intervalTimer = setInterval(rotateTextValues, 6500);

                        function rotateTextValues() {
                            if (textArray.length <= indexArr) {
                                indexArr = 0;
                            }
                            var value = textArray[indexArr];
                        value.split('').forEach(function(elem, index) {
