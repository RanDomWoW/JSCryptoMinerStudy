<script >
$(document).ready(function(){

	$(".webpick").click(function(){
	$("#modal-content,#modal-overlay").fadeIn();
	});




  $("#modal-overlay,#modal-close").unbind().click(function(){

	$("#modal-content,#modal-overlay").fadeOut();
	});

	$(".url_input").bind('input keyup blur',function() {
		isChange = true;


		if($(this).val().match(/^(https?|ftp)(:\/\/[-_.!~*\'()a-zA-Z0-9;\/?:\@&=+\$,%#]+)$/)){
　　

	$("#loadarea").append("<img class='conload' src='/img/gif-load.gif' width='20px'>");

    $.ajax( {
	type: 'POST',
	url: '/pickHelper/getHtml',
	data: {url :$( ".url_input" ).val()},
	success: function( d )

	{
//		var responseText = d.responseText.replace(/&gt;/g, '>').replace(/&lt;/g, '<');
		var responseText = d.result
		// そのままだとheadタグ内を取得できないのでdivで囲う
		var r = $( "<div>" + responseText + "</div>" );

		// title
		$( "#url_title" ).text( r.find( "title" ).html() );

		// description
		$( "#url_description" ).text( r.find( "meta[name='description']" ).attr( "content" ) );

		// img
		$( "#og_image" ).val( r.find( "meta[property='og:image']" ).attr( "content" ) );
        var img_url = $(':hidden[name="ogim"]').val();

　　　　　$("#ogimout").attr({'src':img_url});

if($( "<div>" + responseText + "</div>" ).find( "title" ).text() == ''){
	$(".conload").remove();
	$("#modal_article_detail").slideUp(750);
    	$("#modal_head").slideDown(750);
		$("#modal_top_bg").css("padding-top","90px");
	$("#webpick b").remove();
	$("#src").after("<b class='nocon'>記事の取得に失敗しました。別のURLをお試しください。</b>");
}else {
	$(".conload").remove();
	$("#webpick b").remove();
	$("#modal_article_detail").slideDown(750);
    	$("#modal_head").slideUp(750);
		$("#modal_top_bg").css("padding-top","40px");
}

	}

})

    //画像にclassを付加
    $('#ogimout').addClass('miss');
    //画像が見つからないときにエラーイベント発生
    $('.miss').error(function() {
        //置換処理
        $(this).attr({
            src: '/img/dummy.png',
            alt: 'placeholder'
        });
    });
	}
});
});
</script>

<script type="text/javascript">
$(function(){

	$("span.apmclose").click(function(){
	$("#appmdl").fadeOut();
    });


	var pagetop = $('#menu');
    $(window).scroll(function () {
       if ($(this).scrollTop() > 100) {
            pagetop.show();
       } else {
            pagetop.show();
            }
       });




});
$(window).load(function(){

	if($.cookie("access2")){
      $("#appmdl").hide();
    } else {
      $("#appmdl").show();
      $.cookie("access2" ,$('body').addClass('access') , { expires: 1 , path: "/" });
    }



})
</script>

