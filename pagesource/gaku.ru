<script language="JavaScript" type="text/javascript">
    function sikenCalc(m, y, v){
        var time = new Date();
        var Year = time.getFullYear() + y;
        m=m-1;

        var SikenSch = 0;
        for (var i = 1; i < 31; i++) {
                var Siken_test = new Date(Year, m, i);
                var TekDay = Siken_test.getDay();
                if (TekDay == 0) {
                    SikenSch += 1;
                    if (SikenSch == v) {
                        return new Date(Year, m, i);
                    }
                }
        }

        return 0;
    }

    function fulltime() {
        var time = new Date();
        var TekTime = time.getTime();
        var TekYear = time.getFullYear();
        var NextYear = time.getFullYear()+1;

        // 7 -месяц, 0 - текущий год (1 - след. год), 1 - первое воскресенье

        // Норёку
        var siken_ns1 = sikenCalc(7,0,1);
        var siken_ns2 = sikenCalc(12,0,1);
        var siken_ns3 = sikenCalc(7,1,1);

        //console.log('Siken date: ' + siken_ns1.toString());
        //console.log('Siken date: ' + siken_ns2.toString());
        //console.log('Siken date: ' + siken_ns3.toString());

        if (siken_ns3.getTime() - TekTime > 1) {
                SikenTime = siken_ns3.getTime();
        }

        if (siken_ns2.getTime() - TekTime > 1) {
                SikenTime = siken_ns2.getTime();
        }

        if (siken_ns1.getTime() - TekTime > 1) {
                SikenTime = siken_ns1.getTime();
        }

        OstTime = SikenTime - TekTime;

        // Дни
        OstDay = parseInt(OstTime/(24*60*60*1000));

        // Часы
        OstChas = parseInt((OstTime-(OstDay*24*60*60*1000))/(60*60*1000));

        // Минуты
        OstMin = parseInt((OstTime-(OstDay*24*60*60*1000)-(OstChas*60*60*1000))/(60*1000));

        // Секунды
        OstSec = parseInt((OstTime-(OstDay*24*60*60*1000)-(OstChas*60*60*1000)-(OstMin*60*1000))/(1000));

        document.getElementById("siken_day").innerHTML = OstDay;

        setTimeout(fulltime, 10000);
    }
</script>

