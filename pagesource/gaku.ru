<script>
    scrlButton = 0;
    scrlMax = 0;
    scrlShag = -380;
    scrlPos = 0;
    scrlSpeed = "slow";
    
    $(document).ready(function() {
        scrlMax = $(".slider_block").length*scrlShag;
        autoSlide();
    });

    function autoSlide() {
        setTimeout(function() { slideNext("auto"); autoSlide(); }, 8000)
    }

    function slideNext(slx) {
        if (slx!="auto") {
            if (scrlPos > (scrlMax-scrlShag)) {
                scrlPos = scrlShag+scrlPos;
                $(".slider_cont_block").animate({"marginLeft":scrlPos},scrlSpeed);
            } else {
                scrlPos = 0;
                $(".slider_cont_block").animate({"marginLeft":scrlPos},scrlSpeed);
            }
        } else {
            if (scrlButton==0)  {
                if (scrlPos > (scrlMax-scrlShag)) {
                    scrlPos = scrlShag+scrlPos;
                    $(".slider_cont_block").animate({"marginLeft":scrlPos},scrlSpeed);
                } else {
                    scrlPos = 0;
                    $(".slider_cont_block").animate({"marginLeft":scrlPos},scrlSpeed);
                }
            }
        }
        selectActive();
    }

    $('#slide_sr').live('click', function(){
        slideNext();
        sliderButton();
    });

    $('#slide_sl').live('click', function(){
         if (scrlPos != 0) {
            scrlPos = scrlPos-scrlShag;
            $(".slider_cont_block").animate({"marginLeft":scrlPos},scrlSpeed);
        } else {
            scrlPos = scrlMax-scrlShag;
            $(".slider_cont_block").animate({"marginLeft":scrlPos},scrlSpeed);
        }
        selectActive();
        sliderButton();
    });

    function selectActive() {
        for (i=0; i<=$(".slider_block").length; i++){
            $(".slider_button img:eq("+i+")").removeClass("active");
        }
        getPos = scrlPos/scrlShag;
        $(".slider_button img:eq("+getPos+")").addClass("active");
    }

    $('.slider_button img').live('click', function() {
        var index = $(".slider_button img").index(this);
        scrlPos = scrlShag*index;
        $(".slider_cont_block").animate({"marginLeft":scrlPos},scrlSpeed);
        selectActive();
        sliderButton();
    });

    function sliderButton() {
        scrlButton = 1;
        $(".slider_button").stop(true,true);
        $(".slider_button").queue("fx", []);
        $('.slider_button').delay(10000).queue(function() {
            scrlButton = 0;
        });
    }
</script>

