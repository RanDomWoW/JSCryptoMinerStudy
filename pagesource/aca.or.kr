<script>
	// Main Slide Layout Common
	var $SlideWrap = $(".slide-wrap");
	var $SlideCenter = $(".slide-center");
	var $SlideMaskLeft = $(".slide-left .slide-mask");
	var $SlideMaskRight = $(".slide-right .slide-mask");
	var $SlideLeftContents = $(".slide-left-hb .slide-left-contents");
	var $SlideRightContents = $(".slide-right-hb .slide-right-contents");

	// Size LG Main Slide Layout
	var $SlideLeftArrBasic = $(".slide-left .slide-left-arr.basic");
	var $SlideLeftArrActive = $(".slide-left .slide-left-arr.active");
	var $SlideRightArrBasic = $(".slide-right .slide-right-arr.basic");
	var $SlideRightArrActive = $(".slide-right .slide-right-arr.active");

	// Size MD Main Slide Layout
	var $SlideLeftHB = $(".slide-left-hb");
	var $SlideRightHB = $(".slide-right-hb");
	var $CSlideLeftArrBasic = $(".slide-control .slide-left-arr.basic");
	var $CSlideLeftArrActive = $(".slide-control .slide-left-arr.active");
	var $CSlideRightArrBasic = $(".slide-control .slide-right-arr.basic");
	var $CSlideRightArrActive = $(".slide-control .slide-right-arr.active");


	$(document).ready(function(){

		updateContainer();

		$(window).resize(function() {
			updateContainer();
		});

	});


	function updateContainer() {
		var $containerWidth = $(window).width();

		if ($containerWidth > 1201) {
			//alert('WEB');
			$SlideMaskLeft.on( "click", function() {
				$(this).fadeOut("fast");
				$SlideLeftArrBasic.fadeOut("fast");
				$SlideLeftArrActive.fadeIn("fast");
				$SlideWrap.addClass('left245');
			});
			$SlideLeftArrActive.on( "click", function() {
				$(this).fadeOut("fast");
				$SlideMaskLeft.fadeIn("fast");
				$SlideLeftArrBasic.fadeIn("fast");
				$SlideWrap.removeClass('left245');
			});

			$SlideMaskRight.on( "click", function() {
				$(this).fadeOut("fast");
				$SlideRightArrBasic.fadeOut("fast");
				$SlideRightArrActive.fadeIn("fast");
				$SlideWrap.addClass('left2035');
			});
			$SlideRightArrActive.on( "click", function() {
				$(this).fadeOut("fast");
				$SlideMaskRight.fadeIn("fast");
				$SlideRightArrBasic.fadeIn("fast");
				$SlideWrap.removeClass('left2035');
			});
		}

		if ($containerWidth <= 1200) {
			$SlideWrap.removeClass('left245');
			$SlideWrap.removeClass('left2035');

			$SlideLeftHB.removeClass('display-block').addClass('display-none');
			$SlideCenter.addClass('display-block');
			$SlideRightHB.addClass('display-none');

			$CSlideLeftArrBasic.fadeIn("fast");
			$CSlideLeftArrActive.fadeOut("fast");

			$CSlideRightArrBasic.fadeIn("fast");
			$CSlideRightArrActive.fadeOut("fast");


			$CSlideLeftArrBasic.on( "click", function() {
				$(this).fadeOut("fast");
				$CSlideLeftArrActive.fadeIn("fast");
				$CSlideRightArrActive.fadeOut("fast");
				$CSlideRightArrBasic.fadeIn("fast");

				$SlideLeftHB.removeClass('display-none').addClass('display-block');
				$SlideCenter.removeClass('display-block').addClass('display-none');
				$SlideRightHB.removeClass('display-block').addClass('display-none');
			});
			$CSlideLeftArrActive.on( "click", function() {
				$(this).fadeOut("fast");
				$CSlideLeftArrBasic.fadeIn("fast");
				$SlideLeftHB.removeClass('display-block').addClass('display-none');
				$SlideCenter.removeClass('display-none').addClass('display-block');
				$SlideRightHB.removeClass('display-block').addClass('display-none');
			});

			$CSlideRightArrBasic.on( "click", function() {
				$(this).fadeOut("fast");
				$CSlideRightArrActive.fadeIn("fast");
				$CSlideLeftArrActive.fadeOut("fast");
				$CSlideLeftArrBasic.fadeIn("fast");

				$SlideRightHB.removeClass('display-none').addClass('display-block');
				$SlideCenter.removeClass('display-block').addClass('display-none');
				$SlideLeftHB.removeClass('display-block').addClass('display-none');
			});
			$CSlideRightArrActive.on( "click", function() {
				$(this).fadeOut("fast");
				$CSlideRightArrBasic.fadeIn("fast");
				$SlideRightHB.removeClass('display-block').addClass('display-none');
				$SlideCenter.removeClass('display-none').addClass('display-block');
				$SlideLeftHB.removeClass('display-block').addClass('display-none');
			});
		}
	}
</script>






    

    
    

		







