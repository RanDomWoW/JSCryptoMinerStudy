<script type="text/javascript">
    var openid, oAuthType,oauthAccessToken;
    var loginname = '';

    function showImg() {
        $("#ShowTitleImg").slideDown();
        $(".close").css("display", "none");
        $(".show").css("display", "");


    }
    function closeImg() {
        $("#ShowTitleImg").slideUp();
        $(".show").css("display", "none");
        $(".close").css("display", "");

    }

    function TitleImg() {
        strurl = window.location.href;
        if (strurl == DomainUrl || strurl == (DomainUrl + "public_forward.aspx??url=home.aspx?dtag=aa_1") || strurl == (DomainUrl + "public_forward.aspx")) {
            $(".close").css("display", "");
            $(".show").css("display", "");
        } else {
            $("#Imgshow").css("display", "none");
            $("#ShowTitleImg").css("display", "none");
        }
    }

    function occurs(souce, target) {
        return souce.split(target).length - 1;
    }

    $(document).ready(function () {

        //获取OAuth授权access_token
        oAuthType = getCookie("OAUTHTYPE");
        if (oAuthType == "qq") {
              oauthAccessToken = $.query.get("access_token");
            if (oauthAccessToken && !loginname) {
                qc_app.getUserOpenId(oauthAccessToken, setCheckOAuth);
            }
        } else if (oAuthType == "sina") {
            var code = $.query.get("code");
            if (!code)return;
            sina_app.getAccessToken(code, function (r) {
                if (!r) {
                    alert("授权失败，请稍后再试");
                    return;
                }
                if (r.error_code == 21325) {
                    return;
                }
                oauthAccessToken = r.access_token;
                openid = r.uid;
                validateRegister(openid, oAuthType, validateReg);
            });
        }
        delCookie("OAUTHTYPE");

        function setCheckOAuth(data) {
            openid = data.openid;
            validateRegister(data.openid, oAuthType, validateReg);
        }
        function validateReg(data) {
            if (data.register) {
                OAuthLogin(data.userId,oauthAccessToken ,openid, authLoginCallback);
            } else {
                parent.window.tipsWindown("iframe:" + "OAuthBind.aspx", "361", "260", "true", "", "true", "id");
            }
        }
        function authLoginCallback(r) {
            if (r && !loginname) {
                location.reload();
            } else {
                parent.window.tipsWindown("iframe:" + "OAuthBind.aspx", "361", "260", "true", "", "true", "id");
            }
        }

        //if (document.domain != "www.whrt.gov.cn" && document.domain != "www.wuhanrt.cn") {
        //    top.location.href = "http://www.whrt.gov.cn";
        //};

        TitleImg();
        setTimeout(closeImg, 3000);

        try {
            var str = window.location.href.split("??")[1];
            if (parseInt(occurs(str, '.')) > 1) {
                //top.location.href = "www.whrt.gov.cn";
                top.location.href = DomainUrl + "public_forward.aspx??url=home.aspx?dtag=aa_1";
            }

            if (str.indexOf("http") > -1 || str.indexOf("https") > -1 || str.indexOf("www") > -1) {
                //top.location.href = "www.whrt.gov.cn";
                top.location.href = DomainUrl + "public_forward.aspx??url=home.aspx?dtag=aa_1";
            }
            str = str.substring(4, str.length);
        }
        catch (e) {
            str = "home.aspx?dtag=aa_1";
        }
        window.frames["mainFrame"].location.href = str;

        var cssClass = "";
        var oldClass = "";
        if (str.indexOf("dtag") > 0) {
            cssClass = str.substring(str.indexOf("dtag") + 5, str.length);
            oldClass = cssClass.substring(0, cssClass.length - 2);


            $("li[name='group']").each(function () {
                if ($(this).attr("class").indexOf("_1") > 0) {
                    var i = $(this).attr("class").length - 2;
                    var tempclass = $(this).attr("class").substring(0, i);
                    $(this).removeClass().addClass(tempclass);
                }
            });
            $("li[name='group']").each(function () {
                if ($(this).attr("class") == oldClass) {
                    $(this).removeClass().addClass(cssClass);
                }
            });
        }


        var countNum = "49084011";
        var html = "";
        for (var i = 0; i < countNum.length; i++) {
            html += "<div class=\"countNum_bg\">" + countNum.substring(i, i + 1) + "</div>";
        }
        $("#dynNum").html(html);

        var theme = '/Css/theme.css';

        $("#lnkTheme").attr("href", theme);

    });

    function OpenLogin() {
        parent.window.conultingreplyReguserBox("Login.aspx");
    }
    function OpenRegister() {
        parent.window.conultingreplyReguserBox("RegisterUser.aspx");
    }

    function UpdaUser() {
        document.location.href = "public_forward.aspx?isout=2";
    }

    function LoginOut() {
        if (confirm("您确认退出登录吗？")) {
            document.location.href = "public_forward.aspx?isout=2";
        }
    }

    function changeZY(obj) {
        stopFunc(event);
        window.open("http://zy.wuhanrt.cn/MainPage/Index.aspx?ID=Index");
    }
    function stopFunc(e) {
        e.stopPropagation ? e.stopPropagation() : e.cancelBubble = true;
    }
</script>
