<script>
//////////////////////////////MG Tag Manager PLUG-IN////////////////////////////////////////////////

//Detect Device
//var deviceType ='desktop';

//Detect UserName/ID
//var userName = 'public';

//Detect Page
var pageAlias ='home';

//Detect User Status
var userStatus = 'logged out';
var loginSuccess = 'False';

//Detect BTag
var userBtag = '';

//Detect Domain
var domain = 'www.sportpesa.it';

//Detect BTag
function Btag() {
    if (userBtag !='') {
        return userBtag
    } 
    else{
        return undefined
    }
}
//Detect Enviroment
function stagingEnviroment() {
    if (domain.indexOf("staging") >= 0) {
        return 'staging'
    } 
    else{
        return 'live'
    }
}

//Detect BreakpointType
//Definisco i breakpoint del sito responsive

var xs = window.matchMedia("(max-width: 767px)");
var md = window.matchMedia("(min-width: 768px) and (max-width: 1199px)");
var lg = window.matchMedia("(min-width: 1200px)");

function breakpointType() {
    if (xs.matches) {
        return 'mobile'
    } 

    if (md.matches)  {
        return 'tablet'
    } 

    if (lg.matches)  {
        return 'desktop'
    } 
}

//Detect Market
function productType() {
	switch(pageAlias){
        case "casino":
        case "casino-livebet":
        case "casino-live-evo":
                return 'casino';
        case "scommesse":
        case "scommesse-pro":
        case "scommesse-mobile":
				return 'sports';
		default:
			return undefined
	}
};

//Se la pagina in uso è la pagina di registrazione 
function registrationLoaded() {
    if(pageAlias=="registrazione" && !registrationConfirmMessage){
         sendEvent('registrationLoaded');
    }
    else{}
};

//Se la pagina in uso è specifica per mobile (solo il modulo Scommesse)
function platformType() {
    if(pageAlias=="scommesse-mobile"){
         return 'mobile';
    }
    else{
        return 'desktop';
    }
};

//Se è attiva la pagina di Registrazione

    //Verifico se è lo STEP01 della registrazione (Dati anagrafici)
    function checkPersonalFields() {
            var element = document.getElementById("BaseAccountFields").querySelectorAll("input,select");
            for(var i=0; i<element.length; i++){ 
                element[i].onfocus = function(){
                    //if(_sendedEvent[this.attributes.id.value] == undefined){ 
                    //	_sendedEvent[this.attributes.id.value] = new Date();
                        sendEvent('registrationPersonal');				
                    //}
                } 
            };
    };
    //Verifico se è lo STEP02 della registrazione (Residenza)
    function checkResidenceFields() {
            var element = document.getElementById("ResidenceAccountFields").querySelectorAll("input,select");
            for(var i=0; i<element.length; i++){ 
                element[i].onfocus = function(){
                    //if(_sendedEvent[this.attributes.id.value] == undefined){ 
                    //	_sendedEvent[this.attributes.id.value] = new Date();
                        sendEvent('registrationResidence');				
                    //}
                } 
            };
    };
    //Verifico se è lo STEP03 della registrazione (Contatti)
    function checkContactFields() {
            var element = document.getElementById("ContactAccountFields").querySelectorAll("input,select");
            for(var i=0; i<element.length; i++){ 
                element[i].onfocus = function(){
                    //if(_sendedEvent[this.attributes.id.value] == undefined){ 
                    //	_sendedEvent[this.attributes.id.value] = new Date();
                        sendEvent('registrationContacts');				
                    //}
                } 
            };
    };
    //Verifico se è lo STEP04 della registrazione (Dati accesso)
    function checkLoginDataFields() {
            var element = document.getElementById("LoginDataAccountFields").querySelectorAll("input,select");
            for(var i=0; i<element.length; i++){ 
                element[i].onfocus = function(){
                    //if(_sendedEvent[this.attributes.id.value] == undefined){ 
                    //	_sendedEvent[this.attributes.id.value] = new Date();
                        sendEvent('registrationAccess');				
                    //}
                } 
            };
    };
    //Verifico se è lo STEP05 della registrazione (Recupero Account)
    function checkRestoreFields() {
            var element = document.getElementById("RestoreAccountFields").querySelectorAll("input,select");
            for(var i=0; i<element.length; i++){ 
                element[i].onfocus = function(){
                    //if(_sendedEvent[this.attributes.id.value] == undefined){ 
                    //	_sendedEvent[this.attributes.id.value] = new Date();
                        sendEvent('registrationRecovery');				
                    //}
                } 
            };
    };
    //Verifico se è lo STEP06 della registrazione (Limitazioni sul Deposito)
    function checkDepositLimitationFields() {
            var element = document.getElementById("DepositLimitationAccountFields").querySelectorAll("input,select");
            for(var i=0; i<element.length; i++){ 
                element[i].onfocus = function(){
                    //if(_sendedEvent[this.attributes.id.value] == undefined){ 
                    //	_sendedEvent[this.attributes.id.value] = new Date();
                        sendEvent('registrationRestrictions');				
                    //}
                } 
            };
    };
    //Verifico se è lo STEP07 della registrazione (Promozioni)
    function checkPromotionsFields() {
            var element = document.getElementById("PromotionsAccountFields").querySelectorAll("input,select");
            for(var i=0; i<element.length; i++){ 
                element[i].onfocus = function(){
                    //if(_sendedEvent[this.attributes.id.value] == undefined){ 
                    //	_sendedEvent[this.attributes.id.value] = new Date();
                        sendEvent('registrationPromotions');				
                    //}
                } 
            };
    };
    //Verifico se è lo STEP08 della registrazione (Condizioni)
    function checkConditionsFields() {
            var element = document.getElementById("GeneralRulesAccountFields").querySelectorAll("input,select");
            for(var i=0; i<element.length; i++){ 
                element[i].onfocus = function(){
                    //if(_sendedEvent[this.attributes.id.value] == undefined){ 
                    //	_sendedEvent[this.attributes.id.value] = new Date();
                        sendEvent('registrationsConditions');				
                    //}
                } 
            };
    };
    //Verifico se la registrazione è avvenuta con successo
    function checkRegistrationSuccess() {
                if(registrationConfirmMessage){
                    sendEvent('registrationSuccess');
                }       
    };

//Se è stata inviata la mail tramite WP HelpDesk
function emailSupportSent() {
	if(pageAlias=="helpdesk" && helpdeskConfirmMessage){
			sendEvent('emailSupportSent');
	}
};
//Se è stata inviata la mail tramite la WP ResetUsername
function usernamePasswordSent() {
	if(pageAlias=="recuperousername" && usernameConfirmMessage){
			sendEvent('forgottenUsernameSent');
	}
};
//Se è stata inviata la mail tramite la WP ResetPassword
function forgottenPasswordSent() {
	if(pageAlias=="recuperopassword" && passwordConfirmMessage){
			sendEvent('forgottenPasswordSent');
	}
};
//////////////////////////SPORT BETTING/////////////////////////////////

function gtmBettingData(data) {
    console.log(data.bet.amount);
    console.log(data.bet.betId);
}

/////////////////////CHAT///////////////////////////////////////////////

function checkChat() {
    //console.log()
     sendEvent('liveChatShown');
}

//DataLayer Parametri
var dataLayerParams = {
		
        'event': "",
        
        'page' : {
                    'platformType': platformType(),
                    'product': productType(),
                    'currency': 'EUR',
                    'breakpoint': breakpointType(),
                    'stagingEnviroment': stagingEnviroment(),
                    'country': 'italy'
    
                },
        'user' : {
                     //'playerId': userName,
                    'loggedIn': userStatus,
                    'keyword': undefined,
                    'Btag': Btag(),
                    'NPR': undefined
                },
        'deposit' : {
                    //'currency': 'EUR',
                },
        'bet':  {
                    'value': undefined,
                    'currency': 'EUR',
                    'betId': undefined,
                    'type': undefined,
                    'numSelections': undefined			
                },
        'odd':  {
                    'quota': undefined,
                    'categoryName':undefined,
                    'categoryId': undefined,
                    'outcome': undefined,
                },

        'sportEvent':  {
                    'type': undefined,
                    'eventName':undefined,
                    'eventId': undefined,
                    'sportName': undefined,
                    'sportId': undefined,
                    'competitionName': undefined,
                    'competitionId': undefined,
                },
        'search':  {
                    'keyword': undefined
                }
        };

try	{
/////////////////////////////////Google Data Layer//////////////////////////////////////////////////////////

function sendEvent(eventType){
    dataLayerParams.event = eventType;
	window.dataLayer = window.dataLayer || [];
	window.dataLayer.push(dataLayerParams);
}

var registrationConfirmMessage = false;
var helpdeskConfirmMessage = false;
var passwordConfirmMessage = false;
var usernameConfirmMessage = false;

//Init Event 	
var initEvent = function (){

        //Messaggi di avvenuta e conferma operazione
        registrationConfirmMessage = document.getElementsByClassName('box-registration-message-ok-area').length > 0;
        helpdeskConfirmMessage = document.getElementsByClassName('resultInformation').length > 0;    
        passwordConfirmMessage = document.getElementsByClassName('resultInformation').length > 0;   
        usernameConfirmMessage = document.getElementsByClassName('resultInformation').length > 0; 
        stagingEnviroment(); 
        registrationLoaded();
		emailSupportSent();
        usernamePasswordSent();
        forgottenPasswordSent();

		if(pageAlias=="registrazione" && !registrationConfirmMessage){
			checkPersonalFields();
			checkResidenceFields();
			checkContactFields();
			checkLoginDataFields();
			checkRestoreFields();
			checkDepositLimitationFields();
			checkPromotionsFields();
			checkConditionsFields();
		}
        if(pageAlias=="registrazione"){
            checkRegistrationSuccess();
        }
	};
	//Al caricamento della pagina
	window.onload = initEvent;	
}
catch(err) {}
</script>

