for (var i in locationData) {
if(locationData[i].title) { 
  if(locationData[i].title.indexOf('Test Short') == -1 && locationData[i].title.indexOf('not at') == -1) {
     var latlng = eval("new google.maps.LatLng(" + locationData[i].latlng + ")");
    var marker_title = locationData[i].title.replace(/&#39;/g, "'").replace(/&rsquo;/g,"'").replace(/&amp;/g,"&");
    var content = "<a class='map-location-title-link' href='"+locationData[i].url+"'><h4>"+marker_title+"</h4></a>";
    if (locationData[i].street) {
      content += "<p class='map-location-address'>"+locationData[i].street+"</p>";
    }
    locationData[i].street2 = locationData[i].street2.replace(/\s/g, '');
    if (locationData[i].street2 && locationData[i].street2 != '') {
      content += "<p class='map-location-address'>"+locationData[i].street2+"</p>";
    }
    if (locationData[i].city && locationData[i].state && locationData[i].zip) {
      content += "<p class='map-location-address'>"+locationData[i].city+", "+locationData[i].state+" "+locationData[i].zip+"</p>";
    }    
    var marker = new google.maps.Marker({
      map: map,
      position: latlng,
      icon: locationIcon,
      title: marker_title,
      optimized: false,
      content: content
    });
    bounds.extend(marker.getPosition());

    google.maps.event.addListener(marker, 'click', (function(marker, i) {
      return function() {
        infowindow.close();
        infowindow.setContent(this.content);
        infowindow.open(map, marker);
      };
    })(marker, i));
    google.maps.event.addListener(marker, 'mouseover', function(event) {
      this.setIcon(locationIconHover);
    });
    google.maps.event.addListener(marker, 'mouseout', function(event) {
      this.setIcon(locationIcon);
    });
   gmarkers[i] = marker;
 }
}
