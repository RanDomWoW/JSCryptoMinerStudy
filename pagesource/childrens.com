$(document).ready(function() {
  $('input[value="Hospital"]').data('type', 'Hospitals');
  $('input[value="ER"]').data('type', 'Emergency%20Departments/ER');
  $('input[value="Specialty"]').data('type', 'Specialty%20Centers');
 $('input[value="Imaging"]').data('type', 'Imaging%20Centers');
  $('input[value="Pediatric"]').data('type', 'Primary%20Care');
  $('input[value="Admin"]').data('type', 'Administrative%20Offices');

  $('input[name="locations"]').click(function() {
     var value = $(this).prop("value");
     var checked = $(this).prop("checked");
     //console.log(value);
     //console.log(checked);

     if (value == "all" && checked == true) {
        $('input[name="locations"]').not("[value='all']").prop('checked','');
        for (var i=0;i<locationData.num;i++) {
           if (gmarkers[i]) {
              gmarkers[i].setVisible(true);
           }
        }
     } else if (value == "all" && checked == false) {
        for (var i=0;i<locationData.num;i++) {
           if (gmarkers[i]) {
              gmarkers[i].setVisible(false);
           }
        }
     } else if (value != "all") {
        $('input[value="all"]').prop('checked','');
     
     
     var filter = '';     

     $('input[name="locations"]').each(function() {
       if($(this).prop('checked') == true) {
         if (filter == '') {
             filter += $(this).val();
         } else {
             filter += ',' + $(this).val();
         }
        }
     });
     
     if (filter == '') {
        for (var i=0;i<locationData.num;i++) {
           if (gmarkers[i]) {
              gmarkers[i].setVisible(false);
           }
        }
     } else {
       var filters = filter.split(',');
       //console.log(filters);

       for (var i = 0; i<locationData.num; i++) {
         if(gmarkers[i]){
           gmarkers[i].setVisible(false);
           var showMarker = false;
           var locType = locationData[i].type;
         
           for(var j=0;j<filters.length;j++) {
             if (locType.indexOf(filters[j]) > -1) {
                showMarker = true;
             }
           }
         
          if (showMarker == true) {
           gmarkers[i].setVisible(true);
          }
        }
      }
     }
    }
  });


  $('#filter-loc-search').click(function(e) {
    e.preventDefault();

    var filter = '';
    $('input[name="locations"]').each(function() {
      if($(this).prop('checked') == true) {
        if ($(this).prop("value") != 'all') {
          if (filter == '') {
            filter += $(this).data('type');
          } else {
            filter += '+' + $(this).data('type');
          }
        }
      }
    });

    var url = '/search?q=*&amp;ct=ls&amp;start=0&amp;rows=10&amp;sort=relv';
    if (filter != '') {
       url += '&fq=type:'+ filter;
    }

    window.location.href = url;
  });

  $('#all-count').text(locationData.num);
  $('#hospital-count').text(locationData.hospitalCount);
  $('#er-count').text(locationData.ERCount);
  $('#specialty-count').text(locationData.specialtyCount);
  $('#imaging-count').text(locationData.imagingCount);
  $('#pediatric-count').text(locationData.pediatricCount);
  $('#admin-count').text(locationData.adminCount);

  $('input[name="locations"]').prop('checked','');
  $('input[name="locations"][value="all"]').prop('checked', true);
  
});

</script>


