    specialtyCnt++;
  }

  if (tmpType.indexOf('Imaging') > -1) {
    imagingCnt++;
  }

  if (tmpType.indexOf('Pediatric') > -1) {
    pediatricCnt++;
  }

  if (tmpType.indexOf('Admin') > -1) {
