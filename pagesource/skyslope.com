    function checkBlank(el){
      if (jQuery(el).val() != ''){
        jQuery(el).css('borderColor', '#27ae60');
        return true;
      } else {
        jQuery(el).css('borderColor', '#cc0000');
      }
      return false;
    }
    
    function checkSelectBlank(el){
      if (jQuery(el).find('option:selected').val() != ''){
        jQuery(el).css('borderColor', '#27ae60');
        return true;
      } else {
        jQuery(el).css('borderColor', '#cc0000');
      }
      return false;
    }
    
    function checkNum(el){
      var reg =  /^[0-9\(\) -]+$/;
      if (reg.test(jQuery(el).val())){
        jQuery(el).css('borderColor', '#27ae60');
        return true;
      } else {
        jQuery(el).css('borderColor', '#cc0000');
      }
      return false;
    }
    
    function checkEmail(el){
      var reg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
      if (reg.test(jQuery(el).val()) && jQuery(el).val() != ''){
        jQuery(el).css('borderColor', '#27ae60');
        return true;
      } else {
        jQuery(el).css('borderColor', '#cc0000');
      }
      return false;
    }
