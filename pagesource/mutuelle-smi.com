<script>
    $j = jQuery.noConflict();
    $j('#callme-plugin-widget h5').click(function() {
        $j(this).parent().toggleClass('expanded');
    });

    // resize menu font-size to fit
    function adjust_menu() {
        var menu_max_size = ($j('#menu').width() - $j('#logo').width() - $j('.main-search').width() - 10 );
        var menu_items = $j('#menu').find('a');
        var i = 0;
        while ($j('#menu li').width() >= menu_max_size && i <= 200) {
            menu_items.css('font-size', (parseInt(menu_items.css('font-size')) - 1) + 'px');
            menu_items.css('padding', '7px' );
            $j('#logo').css('margin-right', '20px');
            i++;
        }
    }

    if ($j(window).width() >= 1024) {
        $j(window).resize(adjust_menu);
        $j(document).ready(adjust_menu);
    }
</script>