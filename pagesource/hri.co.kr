		nUrl = arguments[0];
		nWidth = arguments[1];
		nHeight = arguments[2];
		nScroll = (arguments.length > 3 ? arguments[3] : "no");
		nLeft = (arguments.length > 4 ? arguments[4] : (screen.width/2 - nWidth/2));
		nTop = (arguments.length > 5 ? arguments[5] : (screen.height/2 - nHeight/2));
		
