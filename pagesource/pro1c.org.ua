	var headerHeight = $('#elementForSetUserLinks').offset().top + 10; // высота шапки
	var userLinksIndent = 0;    // отступ когда шапка уже не видна
	var userLinksHideTop = -40; // когда позиционируемся на странице, то прячем меню на столько
	
	var hasAnchor = (window.location.hash.substring(1) !== "");
	var alreadyScroll = false;
	var scrollingUserLinks = true;
	
	var userLinksJQueryElem;
	
	var setUserLinksFunction = function(){
		
			if(!scrollingUserLinks)
				return;
		
			headerHeight = $('#elementForSetUserLinks').offset().top + 10; // высота шапки
	
			var currentTopPosition = $(this).scrollTop();
		
			if(hasAnchor && !alreadyScroll){
				alreadyScroll = true;
				userLinksJQueryElem.css('top', userLinksHideTop);
				return;
			}
		
			alreadyScroll = true;
	
			var userLinksTop = 0;
			if (currentTopPosition + userLinksIndent < headerHeight) {
				userLinksTop = headerHeight - currentTopPosition;
			} else {
				userLinksTop = userLinksIndent;
			}
			
			if(userLinksJQueryElem.css('top') == userLinksHideTop + 'px') {
				scrollingUserLinks = false;
				userLinksJQueryElem.animate(
					{top: userLinksTop}, 
					400,
					function(){ scrollingUserLinks = true; });
			}
			else {
				userLinksJQueryElem.css('top', userLinksTop);
			}
		 }
	
	$(function(){
		$(window).scroll(setUserLinksFunction);
		$(window).resize(setUserLinksFunction);
		
		userLinksJQueryElem = $('#userlinks');
		userLinksJQueryElem.css('position', 'fixed');
		
		// для FireFox-а
		if(hasAnchor)
			userLinksJQueryElem.css('top', userLinksHideTop);
		else
			$(window).trigger('scroll');
			
	});
	
</script>

