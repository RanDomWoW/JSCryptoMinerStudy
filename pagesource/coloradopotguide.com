    <script type="text/javascript">
        jQuery(document).ready(function () {
            App.init();
            OwlCarousel.initOwlCarousel();
            NewsletterSignup.init(); 
            EventNewsletterSignup.init();			
        });

        var selectedStates = ['ak', 'az', 'ca', 'co', 'fl', 'hi', 'il', 'ma', 'me', 'mi', 'nv', 'nm', 'or', 'wa'];

        $(document).ready(function () {
            $('#state-map-home').vectorMap({
                map: 'usa_en',
                backgroundColor: '#fff',
                borderColor: '#999',
                borderOpacity: 0.4,
                borderWidth: 1,
                color: '#f4f3f0',
                enableZoom: false,
                hoverColor: '#72C02C',
                hoverOpacity: null,
                selectedColor: '#588033',
                selectedRegions: selectedStates,
                showTooltip: true,
                onRegionOver: function (event, code) {
                    var skip_index = selectedStates.indexOf(code);
                    if (skip_index == -1) {
                        event.preventDefault();
                        return false;
                    }
                    $(this).css("cursor", "pointer");

                    $(".state-list-home li").removeClass("map-overlay");
                    $(".state-list-home li[data-stateabbr='" + code + "']").addClass("map-overlay");
                },
                onRegionOut: function (event, code, region) {
                    $(this).css("cursor", "");
                    $(".state-list-home li").removeClass("map-overlay");
                },
                onRegionClick: function (element, code, region) {
                    var skip_index = selectedStates.indexOf(code);
                    if (skip_index == -1) {
                        event.preventDefault();
                        return false;
                    }

                    var redirect_url;

                    switch (code) {
                        case 'ak':
                            redirect_url = 'https://potguide.com/alaska/'
                            break;
                        case 'az':
                            redirect_url = 'https://potguide.com/arizona/'
                            break;
                        case 'ca':
                            redirect_url = 'https://potguide.com/california/'
                            break;
                        case 'co':
                            redirect_url = '/'
                            break;
                        case 'fl':
                            redirect_url = 'https://potguide.com/florida/'
                            break;
                        case 'hi':
                            redirect_url = 'https://potguide.com/hawaii/'
                            break;
                        case 'il':
                            redirect_url = 'https://potguide.com/illinois/'
                            break;
                       case 'me':
                            redirect_url = 'https://potguide.com/maine/'
                            break;
                        case 'ma':
                            redirect_url = 'https://potguide.com/massachusetts/'
                            break;
                        case 'mi':
                            redirect_url = 'https://potguide.com/michigan/'
                            break;
                        case 'nv':
                            redirect_url = 'https://potguide.com/nevada/'
                            break;
                       case 'nm':
                            redirect_url = 'https://potguide.com/new-mexico/'
                            break;
                        case 'or':
                            redirect_url = 'https://potguide.com/oregon/'
                            break;
                        case 'wa':
                            redirect_url = 'https://potguide.com/washington/'
                            break;
                    }
                    window.location = redirect_url;
                }
            });
        });
    </script>
