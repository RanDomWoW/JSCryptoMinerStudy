	<!-- isotope用　※isotopeで必須 -->
	<script>
	$(window).load(function(){

		var $container = $('#isotope_container:not(#isotope_filters + #isotope_container)');

		// autoheight設定
		var windowWidth = $(window).width();
		function doAutoHeight() {
			console.log( windowWidth );
			if( windowWidth > 751 ) { //3列
				$container.find( '.item' ).autoHeight({column:3});
			} else if( windowWidth > 403 && windowWidth <= 751 ) { //2列
				$container.find( '.item' ).autoHeight({column:2});
			} else { //1列
				$container.find( '.item' ).each( function(){
					$( this ).css({ 'min-height' : 'initial' });
				});	
			}	
		}
		doAutoHeight();
		$(window).resize(function(){
			windowWidth = $(window).width();
			doAutoHeight();
		});
		// autoheight設定ここまで

		//isotope設定
		$('#isotope_container').isotope({
		  // options
		  itemSelector: '.item',
	      layoutMode : 'fitRows',
	      animationOptions: {
		      duration:500,
		      easing:'liner'
	      }
		});


		//ページ内リンク無効化
		$("#isotope_filters a").removeAttr("href");

		//タブ処理
		$("#isotope_filters ul li a").click(function(){
			var $selector = "."+$(this).attr('data-filter');
			if ($selector ==".*"){$selector="*";}
		  	$('#isotope_container').isotope({ filter: $selector });

			$("#isotope_filters ul li").removeClass("selected");
			$(this).parent().addClass("selected");

		});

	});
	</script>

