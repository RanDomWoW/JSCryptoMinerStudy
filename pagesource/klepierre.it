<script type="text/javascript">
    $(document).ready(function() {

        var $form_magnifier_btn = $( ".findAMall_form .icon-magnifying-glass" );

        $form_magnifier_btn.on('click', function(){
            $(this).closest(".findAMall_form").trigger('submit');
        });

        $('#ourMalls-seeMapBtn').attr('href', $('#our-malls-link').attr('href'));

        $(document).on('submit', '.findAMall_form', function( event ) {
            event.preventDefault();
            var url_route = Routing.generate('find_a_mall_form');
            var postCode = $(this).find('input[name="postCode"]').val();

            if (postCode == '' || $.isEmptyObject(postCode)) {
                document.location.href = $('#our-malls-link').attr('href');
            } else {
                $.ajax({
                    type: 'POST',
                    url: url_route,
                    data: new FormData($(this)[0]),
                    processData: false,
                    contentType: false,
                    cache: false,
                    success: function(data)
                    {
                        if (data != "" || !$.isEmptyObject(data)) {
                            window.location.href = Routing.generate('find_a_mall', {'lat':data.latitude, 'lng':data.longitude});
                            return false;
                        } else {
                            if ($(".findAMall_form p.error").lenght) {
                                $('.findAMall_form p.error').html('CAP non valido');
                            }
                            else {
                                $('.findAMall_form input[type="search"]').after('<p class="error">CAP non valido</p>');
                            }

                        }
                    }
                });
            }
            
            return false;
        });


    });

</script>
