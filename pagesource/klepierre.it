<script type="text/javascript">
    var $overbeau = $('#overbeau');
    var $mainNav = $(".mainNav");

    var searchTerm = "Recherche";

    
    jQuery(document).ready(function() {


        $("#globalSearch a").click(function(e) {


            $("#globalSearch").toggleClass("active");
            e.preventDefault();
            e.stopPropagation();
        });

        $("#searchTextTags").on("input", function(){
            if($("#searchTextTags").val() != '')
                $("#autoCompletionSubmit").css('z-index', '10');

            else
                $("#autoCompletionSubmit").css('z-index', '0');
        });

        
    	var searchTextTags = jQuery("#searchTextTags"),
        autoCompletion = jQuery("#autoCompletion"),
        autoCompletionSubmit = jQuery("#autoCompletionSubmit"),
        globalSearch = jQuery("#globalSearch"),
        availableTags = [
                    ];

    searchTextTags.autocomplete({
        source: availableTags,
        appendTo:"#autoCompletion"
    });

    searchTextTags.on( "focus", function( event, ui ) {
        if (this.value == searchTerm) {
            this.value = '';
        }

        if ($('ul', autoCompletion).css('display') != 'none') {
            autoCompletion.show();
        } else {
            autoCompletion.hide();
        }
    } );

    searchTextTags.on( "blur", function( event, ui ) {
        if (this.value == '') {
            this.value = searchTerm;
        }

        if ($('ul', autoCompletion).css('display') != 'none') {
            autoCompletion.show();
        } else {
            autoCompletion.hide();
        }
    } );

    searchTextTags.on( "autocompletesearch", function( event, ui ) {
        if ($('ul', autoCompletion).css('display') != 'none') {
            autoCompletion.show();
        } else {
            autoCompletion.hide();
        }
    } );

    searchTextTags.on( "autocompleteclose", function( event, ui ) {
        //searchTextTags.val('');
        autoCompletion.hide();
    });
    searchTextTags.on( "autocompleteselect", function( event, ui ) {
        event.preventDefault();
        event.stopPropagation();
        var sUrl = ui.item.value;
        searchTextTags.val('');
        searchTextTags.autocomplete('close');
        window.location = sUrl;
        return false;
    } );

    globalSearch.on( "submit", function( event, ui ) {

        var text = searchTextTags.val();
        var url_route = Routing.generate('search', { text: text});
        jQuery(location).attr("href", url_route);
        return false;

    } );


        
