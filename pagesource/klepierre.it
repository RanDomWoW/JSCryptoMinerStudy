    function getGMTSpecificDataLayerForValue(params){
        var ret = {};
        for (var key in params) {
            var value = params[key];
            if (key === 'retailer_name'){
                ret.pageRetailerName = value;
            }else if (key === 'shop_name'){
                ret.pageRetailerName = value;
            }else if (key === 'page_contest'){
                ret.pageContest = value;
            }else if (key === 'search_count'){
                ret.pageSearchResults = value.toString(); // pour être sur d'avoir une string cf Excel      'pageSearchResults': '3'
            }
        }
        //console.log('getGMTSpecificDataLayerForValue = '+JSON.stringify(ret));
        return ret;
    }

