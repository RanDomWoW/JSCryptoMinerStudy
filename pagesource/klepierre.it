    var isAndroid = ua.indexOf("android") > -1 && ua.indexOf("mobile");
    if(isAndroid) {

    }

    var androidLink = '';
    var iOSLink     = '';

    jQuery(document).ready(function() {
        if (isMobileDesktop) {
            jQuery(".mobileLinkDisabled").click(function () {
                if (jQuery(this).parent("li").hasClass("active")) {
                    return true;
                }
                jQuery(this).parents("ul").find('.active').removeClass("active");
                jQuery(this).parent("li").addClass("active");
                return false;
            });
        } else {
            //alert("pas mobile");
        }

        var tokenExpiry = cookieManagerLocal.readCookie('tokenExpiry');
        if (null != tokenExpiry) {
            if (tokenExpiry == 'check') {
                getTokenExpirationDate();
            } else {
                var today = new Date();
                var expiry = new Date(tokenExpiry);

                if (expiry < today) {
                    magnificP("LA tua sessione è scaduta.");
                    setTimeout(function(){
                        location.replace('/logout');
                    }, 2000);
                }
            }
        } else {
            // anonymous
            var cookieExpires = new Date(new Date().getTime() + (30 * 86400 * 1000)).toString();
            document.cookie = "tokenExpiry=anon; expires="+cookieExpires+"; path=/";
        }
    });

    function getTokenExpirationDate() {
        var url_route = Routing.generate('getTokenExpirationDate');
        $.ajax({
            type: 'POST',
            url: url_route,
            success: function(data)
            {
                if (data != false) {
                    var cookieExpires = new Date(new Date().getTime() + (30 * 86400 * 1000)).toString();
                    document.cookie = "tokenExpiry="+data+"; expires="+cookieExpires+"; path=/";
                } else {
                    var cookieExpires = new Date(new Date().getTime() + (30 * 86400 * 1000)).toString();
                    document.cookie = "tokenExpiry=anon; expires="+cookieExpires+"; path=/";
                    magnificP("LA tua sessione è scaduta.");
                    setTimeout(function(){
                        location.replace('/logout');
                    }, 2000);
                }
            }
        });
    }

    function getOsMobile() {
        var userAgent = navigator.userAgent || navigator.vendor || window.opera;

        if( userAgent.match( /Android/i ) ) {
            return 'Android';
        } else if(userAgent.match(/iPhone/i) || userAgent.match(/iPad/i) || userAgent.match( /iPod/i ) ) {
            return 'iOS';
        } else {
            //PC, blackberry, ...
            return 'others';
        }
    }

