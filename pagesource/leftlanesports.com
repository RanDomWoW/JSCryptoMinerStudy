    <script>
        window.fbAsyncInit = function () {
            FB.init({
                appId: '144947542206818',
                cookie: true,  // enable cookies to allow the server to access
                // the session
                xfbml: true,  // parse social plugins on this page
                version: 'v2.9' // use version 2.9
            });
        };

        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) {
                return;
            }
            js = d.createElement(s);
            js.id = id;
            js.src = "//connect.facebook.net/en_US/sdk.js";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));

        function loginFbUser() {
            FB.api('/me', function (response) {
                window.response = response;
                if (response.email) {
                    window.location = "fbLogin/Default.aspx?email=" + response.email +
                        "&first_name=" + response.first_name +
                        "&last_name=" + response.last_name +
                        "&gender=" + response.gender +
                        "&ReturnURL=" + location.pathname.substring(1) + location.search;
                } else {
                    if (sessionStorage.RetriedFbLogin !== '1') {
                        var source = $('#templateFbEmailPermission').html(),
                            template = Handlebars.compile(source),
                            context = response,
                            html = template(context);
                        $('#FbEmailPermissionPopup').html(html);
                        $('a[href="#FbEmailPermissionPopup"]').click();
                        $('#FbReAsk').on('click', function () {
                            FB.login(
                                function (response) {
                                    if (response.status === 'connected') {
                                        $.fancybox.close();
                                        sessionStorage.RetriedFbLogin = '1';
                                        loginFbUser();
                                    } else {
                                        $('#fancybox-close').click();
                                        sessionStorage.RetriedFbLogin = '1';
                                    }
                                },
                                {
                                    scope: 'email',
                                    auth_type: 'rerequest'
                                }
                            );
                        });
                        $('#FbEmailPermissionPopup').parent().siblings('#fancybox-close').on('click', function () {
                            sessionStorage.RetriedFbLogin = '1';
                        });
                    } else {
                        //Hide the button maybe?
                    }
                }
            });
        }

        function fbLogin() {
            FB.login(
                function (response) {
                    if (response.authResponse) {
                        loginFbUser();
                    }

                    $.fancybox.close();
                },
                { scope: 'public_profile,email' }
            );
        }

        function statusChangeCallback(response) {
            if (response.status === 'connected') {
                // Logged into your app and Facebook.
                loginFbUser();
            } else if (response.status === 'not_authorized') {
                // The person is logged into Facebook, but not the app.
                loginFbUser();
            } else {
                // The person is not logged into Facebook, so we can't do much on our side.
            }
        }

        function checkLoginState() {
            FB.getLoginStatus(function (response) {
                statusChangeCallback(response);
            });
        }

        $('body').on('click', '#FBLoginMemberButton, #FBLoginLoginButton, #FBLoginJoinBannerButton', function () {
            fbLogin();
        });

        $('body').on('click', '.share-bar.facebook', function () {
            FB.ui({
                method: 'share',
                href: window.location.href
            });
        });
    </script>
