        //For simplicity's sake, sortify only works on siblings
        $.Sortify = function (el, options) {
            var $el = $(el);
            var $elPrev = $el.prev();
            var list = $el.nextUntil(options.onlySameType ? ":not(" + options.selector + ")" : "").andSelf().sort(function (a, b) {
                var upA = $(a).data('sort');
                var upB = $(b).data('sort');
                return (upA < upB) ? -1 : (upA > upB) ? 1 : 0;
            });
            options.flat ? $(list).insertAfter($elPrev) : $(list).appendTo($el.parent());
        };
        $.fn.sortify = function (options) {
            var settings = $.extend({
                switchCase: 'siblings',
                onlySameType: true,
                selector: this.selector,
                flat: true
            }, options);
            return this.each(function () {
                var plugin = new $.Sortify(this, settings);
            });
        };
        $('a[data-sort]').sortify();

