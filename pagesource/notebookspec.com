<script>
var nbFilter = 0;
var nbFlexSlider;
var tlFilter = 0;
var tlFlexSlider;
$().ready(function(){


		jQuery('#notebook_flexslide').flexslider({
			animation: "slide",
			controlsContainer: '#notebook_flexslide_control',
			directionNav: false,
			animationLoop:false,
			slideshowSpeed : 20000 ,
			start:function(){
					nbFlexSlider = jQuery('#notebook_flexslide').data('flexslider');
					jQuery('#notebook_flexslide').css('display','');
			},
			end:function(slider){
				setTimeout("$('#notebook_flexslide').data('flexslider').flexAnimate(0,true,true);",7000);
			}
	});
	
	
	$('.nb_attr').click(function(){
										
										if($(this).hasClass('selected')){
											$(this).removeClass('selected');
											nbFilter =0;
										}else{
											$('.nb_attr').removeClass('selected');
											$(this).addClass('selected');
											nbFilter = parseInt($(this).data('value'));
										} 
					
										$.getJSON('/web/v2/cover_new_notebook_big.php?filter='+nbFilter,function(data){
											$('#notebook_flexslide li:not(.clone)').each(function(){ 
												nbFlexSlider.removeSlide(this); 
											});
											for(i = 0; i < data.length;i++){
												nbFlexSlider.addSlide('<li>'+data[i]+'</li>');
											}
											$('#notebook_flexslide').flexslider(0);
										});
									});
	
					/* Notebook */
				
				
												

								  $('#tablet_flexslide').flexslider({
									animation: "slide",
									controlsContainer: '#tablet_flexslide_control',
									directionNav: false,
									slideshow:false,
									start:function(){
										tlFlexSlider = $('#tablet_flexslide').data('flexslider');
										$('#tablet_flexslide').css('display','');
									}
								  });
								  
								  $('.tl_attr').click(function(){
										
										if($(this).hasClass('selected')){
											$(this).removeClass('selected');
											tlFilter ='';
										}else{
											$('.tl_attr').removeClass('selected');
											$(this).addClass('selected');
											tlFilter = $(this).data('value');
										} 
					
										$.getJSON('/web/v2/index_new_tablet_big.php?filter='+tlFilter,function(data){
											$('#tablet_flexslide li:not(.clone)').each(function(){ 
												tlFlexSlider.removeSlide(this); 
											});
											for(i = 0;i<data.length;i++){
												tlFlexSlider.addSlide('<li>'+data[i]+'</li>');
											}


											$('#tablet_flexslide').flexslider(0);
										});
									});

				/* Tablet */ 
});
</script>


