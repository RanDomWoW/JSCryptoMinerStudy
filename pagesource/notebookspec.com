<script>
function myLoadToDiv(selector,url,callback){
	$(selector).html("Loading");
	$(selector).load(url,callback);
}	

var NewNotebookType = -1;
var NewNotebookPage = 1;
function selectNewNotebookType(t,obj){
	NewNotebookType = t;
	NewNotebookPage = 0;

	myLoadToDiv('#new_notebook','/web/v2/index_new_notebook.php?type='+NewNotebookType+'&page='+NewNotebookPage,function(){
		$($('.cover_new_left_new_notebook')[0]).find('.newsbox2').css('box-shadow','');
		$(obj).parent().css('box-shadow','0 0 5px rgba(0, 153, 255, 0.5) ');
	});
	NewNotebookPage++;
}
function NewNotebookPrevPage(){
	//$('#new_notebook').load('/web/v2/index_new_notebook.php?type='+NewNotebookType+'&page='+NewNotebookPage);
	myLoadToDiv('#new_notebook','/web/v2/index_new_notebook.php?type='+NewNotebookType+'&page='+NewNotebookPage);
	NewNotebookPage++;
}
function NewNotebookNextPage(){
	NewNotebookPage-=2;
	if(NewNotebookPage<0) NewNotebookPage= 0;
	myLoadToDiv('#new_notebook','/web/v2/index_new_notebook.php?type='+NewNotebookType+'&page='+NewNotebookPage);
	NewNotebookPage++;
}
/* END Notebook */


