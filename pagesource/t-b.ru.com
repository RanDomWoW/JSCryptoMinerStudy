    <script>
        $(function () {
            var sessid = '23071d8b4475e5aacc5f9b2afe07cc5c';
            var show_pane = '0';
            if (show_pane != '0') {
                if (show_pane == "login") {
                    show_auth(0);
                } else if (show_pane == "register") {
                    show_reg(0);
                }
                show_pupover(show_pane);
            }

            function show_pupover(show_pane) {
                var parent = '';
                if (show_pane == "login") {
                    parent = '#loginWindow .auth_cont.topconts';
                } else if (show_pane == "register") {
                    parent = '#loginWindow .reg_cont.topconts';
                }

                $(parent + ' .input-block-level').each(function () {
                    if ($(this).attr('error_txt')) {
                        $(this).popover({
                            content: $(this).attr('error_txt'),
                            delay: {show: 0, hide: 0},
                            trigger: 'manual'
                        });
                        $(this).popover('show');
                    }
                });

                if ($('#capcha_el').length && show_pane != '0') {
                    var src = $('#capcha_el').attr('imgsrc');
                    $('#capcha_el').popover({
                        content: '<span class="capinclude"><img alt="" src="' + src + '" width="222" height="49" alt="CAPTCHA" /></span>',
                        html: true,
                        trigger: 'manual'
                    });
                    $('#capcha_el').popover('show');

                }
            }

            function show_auth(speed) {

                if (!$('#loginWindow').hasClass('opened')) {
                    $('#loginWindow .topconts').addClass('hide');
                    $('#loginWindow .auth_cont.topconts').removeClass('hide');
                    toggle_window(speed);
                }
                else {
                    if ($('#loginWindow .auth_cont.topconts').hasClass('hide')) {
                        $('#loginWindow .topconts').addClass('hide');
                        $('#loginWindow .auth_cont.topconts').removeClass('hide');
                    }
                    else {
                        toggle_window(speed);
                    }
                }
            }

            function show_reg(speed) {

                if (!$('#loginWindow').hasClass('opened')) {
                    $('#loginWindow .topconts').addClass('hide');
                    $('#loginWindow .reg_cont.topconts').removeClass('hide');
                    toggle_window(speed);
                }
                else {
                    if ($('#loginWindow .reg_cont.topconts').hasClass('hide')) {
                        $('#loginWindow .topconts').addClass('hide');
                        $('#loginWindow .reg_cont.topconts').removeClass('hide');
                    }
                    else {
                        toggle_window(speed);
                    }
                }
            }

            function toggle_reg() {
                $('#loginWindow .topconts').toggleClass('hide');
            }

            function toggle_window(speed) {
                $('#loginWindow').slideToggle(speed);
                if ($('#loginWindow').hasClass('opened')) {
                    if ($('.goog-te-banner-frame .skiptranslate').length > 0) {
                        $('#loginWindow').css('margin-top', '40px');
                        $('.cloneheader').css('top', '40px');
                    } else {
                        $('#loginWindow').css('margin-top', '0px');
                        $('.cloneheader').css('top', '0px');
                    }
                    $('#loginWindow .close').fadeOut(speed);
                    $('#loginWindow').removeClass('opened');
                    $('.mymodal-backdrop').fadeOut(speed, function () {
                        $('.mymodal-backdrop').remove();
                        $('.cloneheader').remove();
                    });
                }
                else {
                    if ($('.goog-te-banner-frame .skiptranslate').length > 0) {
                        $('#loginWindow').css('margin-top', '40px');
                        $('.cloneheader').css('top', '40px');
                    } else {
                        $('#loginWindow').css('margin-top', '0px');
                        $('.cloneheader').css('top', '0px');
                    }
                    $('#loginWindow').addClass('opened');
                    $('#loginWindow .close').fadeIn(speed, function () {
                        if ($('#capcha_el').length) {
                            var src = $('#capcha_el').attr('imgsrc');
                            $('#capcha_el').popover('destroy');
                            $('#capcha_el').popover({
                                content: '<span class="capinclude"><img src="' + src + '" width="222" height="49" alt="CAPTCHA" /></span>',
                                html: true,
                                trigger: 'manual'
                            });
                            $('#capcha_el').popover('show');
                        }
                    });
                    if ($('.cloneheader').length == 0) {
                        var new_head = $('.headerclass .topmenu').clone();
                        new_head.addClass('fixedcl');
                        new_head.addClass('cloneheader');
                        $('.headerclass').append(new_head);
                    }
                    if ($('.mymodal-backdrop').length == 0) {
                        $('body').append('<div style="display:none" class="mymodal-backdrop">');
                        $('.mymodal-backdrop').fadeIn(speed);
                    }
                }
            }

            $(document).on('click', '.mymodal-backdrop', function () {
                toggle_window('fast');
            });

            $(document).on('click', '.registrationlink', function () {
                show_reg('fast');

            });
            $(document).on('click', '.loginlink', function () {
                show_auth('fast');
            });

            $(document).on('click', '#loginWindow .close', function () {
                toggle_window('fast');
            });

            $(document).on('click', '#loginWindow .trigger_conts', function () {
                toggle_reg();
            });

            //Если не надо запускать, то комментим строчку!
            //$('#loginWindow').modal('show');


            $('#regform').submit(function () {
                if (!$('#iagree').is(':checked')) {
                    alert('Необходимо согласиться с условиями договора!');
                    return false;
                }
            });

            if ($(window).width() < 768) {
                validate(sessid, '/auth/check.php', 'inline', 'loginWindow');
            }
            else {
                validate(sessid, '/auth/check.php', 'popup', 'loginWindow');
            }

        });
    </script>
