        return false;
    });
    function left_carusel_pvh(carusel){
        var block_width = $(carusel).find('.carousel-block-pvh').outerWidth();
        $(carusel).find(".carousel-items-pvh .carousel-block-pvh").eq(-1).clone().prependTo($(carusel).find(".carousel-items-pvh"));
        $(carusel).find(".carousel-items-pvh").css({"left":"-"+block_width+"px"});
        $(carusel).find(".carousel-items-pvh .carousel-block-pvh").eq(-1).remove();
        $(carusel).find(".carousel-items-pvh").animate({left: "0px"}, 200);

    }
    function right_carusel_pvh(carusel){
        var block_width = $(carusel).find('.carousel-block-pvh').outerWidth();
        $(carusel).find(".carousel-items-pvh").animate({left: "-"+ block_width +"px"}, 200, function(){
            $(carusel).find(".carousel-items-pvh .carousel-block-pvh").eq(0).clone().appendTo($(carusel).find(".carousel-items-pvh"));
            $(carusel).find(".carousel-items-pvh .carousel-block-pvh").eq(0).remove();
            $(carusel).find(".carousel-items-pvh").css({"left":"0px"});
        });
    }

</script>




