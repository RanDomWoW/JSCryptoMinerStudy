<div id="ruspostmodal" class="modal hide fade" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                        <div style="font-size: 14px;font-weight: 400;line-height: 18px;margin: 0;padding: 10px 0 0;" class="inputincluded">Расчет доставки товара до города
        <span class="city_change_panel">
            <span class="text_inner"><span class="text_inner_val bold text_inner_val_city">Москва</span> <a
                    id="change_city_link_small" class="change_city_link_small">изменить</a></span>
	        <input style="display:none" class="input_inner" name="t" type="text" id="toruspost" value="Москва"/>
        </span>
        </div>
    </div>
    <div class="modal-body">
        <script>
    $(function () {
        var url = '/bitrix/components/taorucom/tao.ruspost/ajax/json_result.php';
        var c = '0';
        var f = 'Благовещенск';
        var form = $('#ruspostform');
        var input_to = $('#toruspost');

        function is_int(input) {
            return !isNaN(input) && parseInt(input) == input && input > 0;
        }

        form.submit(function () {
            send_ajax(2, $('#post_data_cont'), $('#weghtruspostsubmit'), $('#weghtruspost').val());
            return false;
        });

        $('.get_price_post').click(function () {
            var btn = $(this);
            var cont = $('#' + btn.data('cont_id'));
            var weight = btn.data('weight');

            send_ajax(1, cont, btn, weight);
        });

        function send_ajax(param, cont, btn, weight) {

            if ($('#ruspostmodal').data('price_delive')) {
                c = $('#ruspostmodal').data('price_delive');
            }

            var val = weight;
            var val_to = input_to.val();

            if ((val != '') && is_int(val) && (val_to != '')) {

                $.ajax({
                    url: url,
                    type: 'POST',
                    dataType: 'JSON',
                    data: {t: val_to, c: c, f: f, w: val, p: param},
                    beforeSend: function () {
                        cont.html('<div class="loader"></div>');
                        cont.fadeIn();
                        btn.button('loading');
                    },
                    success: function (data) {
                        btn.button('reset');
                        if (data.type == 'success') {
                            cont.html(data.text);
                        }
                        else {
                            alert(data.text);
                            cont.fadeOut();
                        }
                    },
                    error: function () {
                        btn.button('reset');
                        cont.fadeOut();
                        alert('Нет соединения с сервером!');
                    }
                });
            }
            else {
                alert('Ваш город и вес товара - обязательные параметры для расчета!');
            }
        }
    });
</script>

