            target_element = layer.getElementsByTagName("input")[i];
        }
    }
    
    if(link) {
        link = link.replace(/^\//, '');
        if (tar == "NEW") {
            var linkurl = "http://www.fairplay142.com/"+link;
            window.open(linkurl);
            if (win == "CLOSE") {
                document.getElementById(layername).style.display = "none";
                document.getElementById(layername).innerHTML='';
            }
        } else {
            document.location.href = "http://www.fairplay142.com/"+link;
        }
        return;
    }
    
    if(target_element) { 
        if(temp == 0) {
            if(target_element.getAttribute("checked") === true || target_element.checked == true) {
                setCookie(eventname, enddate, day, '.fairplay142.com');
