function fbLogin(){
    var dialog = jQuery('#modal-fb-password');
    var response = fbResponse
    jQuery.post('/facebook/ajax/connect', response, function(json, textStatus) {
        jQuery('#loading-mask').hide();
        var html = '';
        switch(json.code){
            case -1:
            //报错信息
                html = '<div style="font-size:20px;"><strong>'+json.msg+'</strong></div>'
                dialog.find('.modal-body').html(html);

                dialog.modal('show');
                break;
            case 0:
                setLocation(json.url);
                break;
            case 2://补充邮箱信息
                showBindEmail(json.msg);
                break;
            case 3://邮箱已存在 ，选择输入密码（绑定该邮箱）或者输入其他邮箱（重复2）
                showConfirmPassword(json.msg);
                break;
            default:
                jQuery('#loading-mask').hide();

        }
    })
    .error(function(){
        jQuery('#loading-mask').hide();
    });
