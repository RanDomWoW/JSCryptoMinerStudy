var body = null;
jQuery('#modal-simple').on('show.bs.modal', function (e) {
    var target = jQuery(e.relatedTarget);
    var id = target.data('id');
    var modal = jQuery(this);
    var content = null;
    if(id == "video") {
        jQuery('#product-video iframe').attr('src', target.data('video'));
        content = jQuery('#product-video').html();
    }else{
        var temp = target.data('body');
        if(temp) {
            body = jQuery(temp);
            content = body.contents();
        }else{
            temp = modal.data('body');
            if(temp instanceof jQuery) {
                body = temp;
                content = body.contents();
            }else{
                body = temp;
                content = temp;
            }
        }
    }
    jQuery('#loading-mask').hide();
    modal.find('.modal-body').html(content);
});
jQuery('#modal-simple').on('hidden.bs.modal', function () {
    jQuery(this).find('iframe').removeAttr('src');
    if(!(body instanceof jQuery)){ 
        return false;
    }
    var modalBody = jQuery(this).find('.modal-body');
    modalBody.removeClass().addClass('modal-body');
    var content = modalBody.contents();
    body.html(content); 
});
