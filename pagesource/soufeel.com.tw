function lazyCss(url) {
    jQuery("<link>").attr({
        rel: "stylesheet",
        type: "text/css",
        href: url
    }).appendTo("head");
}
function lazyRun(domElement) {
    var jdom =  jQuery(domElement);
    if(jdom.data("lazyclass") != undefined) {
        jdom.addClass(function(n){
            var lazyclass = jdom.data("lazyclass");
            jQuery(this).removeAttr('data-lazyclass');
            jQuery(this).next('.swiper-lazy-preloader').remove();
            return lazyclass;
        });
    }
    if(jdom.data("lazysrc") != undefined) {
        jdom.attr('src',function(){
            var lazysrc = jdom.data("lazysrc");
            jQuery(this).removeAttr('data-lazysrc');
            jQuery(this).next('.swiper-lazy-preloader').remove();
            return lazysrc;
        })
    }
    if(jdom.data("lazystyle") != undefined) {
        var style = jdom.attr('style');
        if( style == undefined) {
            style = ''
        }else{
            style = style + ';'
        }
        jdom.attr('style', function(index, oldvalue){
            var lazystyle = jdom.data("lazystyle");
            jQuery(this).next('.swiper-lazy-preloader').remove();
            jQuery(this).removeAttr('data-lazystyle');
            return style +  lazystyle;
        })
    }
}
function resizeDoor() {
    var doors = jQuery('div.door');
    if(doors.length == 0) {
        return false;
    }
    var parent = doors.parents('.standard-product-list');
    if(parent.length == 0) {
        return false;
    }
    var elements = parent.find('div.element');
    if(elements.length == 0) {
        return false;
    }
    var element = elements.last();
    if(element.length == 0) {
        return false;
    }
    var outerHeight = element.outerHeight();
    doors.each(function(){
        var door = jQuery(this);
        var widthRatio = door.data('widthRatio') ? door.data('widthRatio') : 1;
        var heightRatio = door.data('heightRatio') ? door.data('heightRatio') : 1;
        door.width(element.outerWidth() * widthRatio);
        door.height(element.outerHeight() * heightRatio);
    });
    elements.slice(0, -1).outerHeight(outerHeight);
}


