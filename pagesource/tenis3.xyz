		}else {
			$this.addClass('active02');
		}
		$(this).next().slideToggle("fast");
	});

	$(".over").hover(function() {
		$(this).stop().animate({'opacity':'1'},5);
	}, function() {
		$(this).stop().animate({'opacity':'0'},5000);
	});
});
</script>
