<script type="text/javascript">
    curFTemplate = 1;

    $(document).ready(function () {
        $("#clText").click(function () {
            $('#sourceText').val('').focus();
            clearLastTranslation();
            clearSelectionInfoResult();
            $(".selResult").hide();
            $('#editResult_test').val('');
            $("#trResult").hide();
            $("#adviseIconDiv").hide();
            $("#topSamplesBlock").hide();
            $("#topSamplesSelSourceBlock").hide();
            $("#trResult4Selection").html('');
            $("#trResult4Selection").hide();
            $("#topPhrasesBlock").hide();
            $("#topExplanatoryBlock").hide();
            $(".socResult").hide();
            $("#autoInfo").hide();
            $('#sourceText').css('height', '145px');
            setEqualHeights();
        });

        $("#sourceText").attr('placeholder', placeholderText);

        if (($('#sLang').val() == 'ar') || ($('#sLang').val() == 'he'))
            $('#sourceText').css('direction', 'rtl');

        $("#sourceText").keydown(checkForEnter);
        $("#sourceText").focus(function () {
            $(this).parent().addClass('focus-text');
        }).blur(function () {
            $(this).parent().removeClass('focus-text');
        });

        $('#infoBttn').popover();

        $("#editResult_test").focus(function () {
            $(this).parent().parent().addClass('focus-text');
        }).blur(function () {
            $(this).parent().parent().removeClass('focus-text');
            $(this).css('color', '#000')
        });

        $("#bTranslation").on("click", function (e) {
            e.stopPropagation();
            getTranslation();
            $("#blogRSS").hide();
        });

        var checkLT = JSON.parse(window.sessionStorage.getItem('myLastTranslation'));
        if (checkLT) {
            if ($('#sourceText').val() == "") {
                $('#sourceText').val(checkLT.source);
                if (getDir() != checkLT.dir)
                    revertDir('sLang', 'rLang');

                $('#result').html(checkLT.result);
                trPtsDirCode = checkLT.dir;
                trDirCode = trPtsDirCode;

                if (checkLT.isWord) {
                    $("#topPhrases").html(checkLT.topPhrases);
                    $("#topSamples").html(checkLT.topSamples);
                    $('#topExplanatory').html(checkLT.dictDef);
                    $('#topPhrasesBlock').show();
                    $('#PhrasesBlock').show();
                    $('#topPhrases').show();
                    $('#topSamplesBlock').show();
                    $('#SamplesBlock').show();
                    $('#topSamples').show();

                    $('#linkMorePhrases').attr('href', checkLT.morePhrasesLink);
                    $('#linkMoreSamples').attr('href', checkLT.moreSamplesLink);
                    $('#morePhrases').show();
                    $('#moreSamples').show();
                    $('#topExplanatoryBlock').show();
                    $("#dictTab a").attr('href', window.sessionStorage.getItem('myLastFWord'));
                    $("#samplTab a").attr('href', window.sessionStorage.getItem('myLastSamples'));
                    $("#grammTab a").attr('href', window.sessionStorage.getItem('myLastGrammar'));
                    uTrType = "word";
                } else {
                    if (!checkLT.isURL) {
                        $('#editResult_test').val(checkLT.trresult);
                        $("#trResult").css('color', 'transparent');
                        $("#trResult").css('display', 'none');

                        setEqualHeights();
                        showEditTranslationWin(0);

                        if ($("#sourceText").val().length > $("#editResult_test").val().length)
                            $("#sourceText").val($("#sourceText").val() + "");

                        setEqualHeights();
                    }
                }
            } else {
                if ((window.location.pathname.indexOf('links') == -1) && (window.location.pathname.indexOf('-') == -1)) {
                    $('#sourceText').val(checkLT.source);
                    $('#result').html(checkLT.result);
                    trPtsDirCode = checkLT.dir;
                    trDirCode = trPtsDirCode;
                    if (blogPos != "right") {
                        if (uiLang != 'de')
                            $("#blogRSSRight").html($("#blogRSS").html());

                        $("#blogRSS").html('');
                        blogPos = "right";
                        $("#moreBlog a").width(20).height(20);
                        $("#moreBlog a").css('margin-left', '14px');
                        $("#moreBlog").addClass('rightCol');

                        if (uiLang != 'de')
                            $("#blogRSSRight").show();
                    }

                    if (!checkLT.isWord) {
                        $('#editResult_test').html(checkLT.trresult);
                        setEqualHeights();
                        $('#copyResBttn').attr('title', copyText);
                        $('#printResBttn').attr('title', printText);
                        $('#clResult').attr('title', closeText);
                    }

                    if (checkLT.isWord) {
                        $("#topPhrases").html(checkLT.topPhrases);
                        $("#topSamples").html(checkLT.topSamples);
                        $('#topExplanatory').html(checkLT.dictDef);
                        $('#topPhrasesBlock').show();
                        $('#PhrasesBlock').show();
                        $('#topPhrases').show();
                        $('#topSamplesBlock').show();
                        $('#SamplesBlock').show();
                        $('#topSamples').show();
                        $('#linkMorePhrases').attr('href', checkLT.morePhrasesLink);
                        $('#linkMoreSamples').attr('href', checkLT.moreSamplesLink);
                        $('#morePhrases').show();
                        $('#moreSamples').show();
                        $('#topExplanatoryBlock').show();
                        $("#dictTab a").attr('href', window.sessionStorage.getItem('myLastFWord'));
                        $("#samplTab a").attr('href', window.sessionStorage.getItem('myLastSamples'));
                        $("#grammTab a").attr('href', window.sessionStorage.getItem('myLastGrammar'));
                    }
                } else {
                    setTimeout('getTranslation();', 10);
                }
            }
        } else {
            if ($('#sourceText').val() != "")
                setTimeout('getTranslation();', 10);
            else
                $('#sourceText').focus();
        }

        $("#vkb_f2").dialog({
            title: "Virtual keyboard",
            width: 490,
            resizable: false,
            draggable: true,
            close: function () {
                hideAllKBs();
                Show_vkb = false;
                $('#Vkeyboard').append($("#vkb_f1"));
            },
            autoOpen: false
        });

        $("#megaFullInfo").dialog({
            title: "Грамматическая справка",
            width: 680,
            height: 500,
            resizable: false,
            draggable: false,
            autoOpen: false,
            modal: true,
            zIndex: 110000,
            close: function () {
                fullWordInfo = 0;
                $(".trsl").hide();
                $("#mfi_grammInfo").html("");
            }
        });

        $("#spellInfo").dialog({
            title: "Spell checking by ORFO",
            width: 680,
            height: 520,
            resizable: false,
            draggable: false,
            autoOpen: false,
            modal: true,
            zIndex: 110000,
            close: function () {
                $("#okSpellDescr").hide();
                $("#errSpellDescr").hide();
                $("#spellEditor").html('');
                $("#spellLoadDiv").show();
            }
        });

        $('#sourceText').bind('input propertychange', function () {
            var textA = $(this).val();
            var textB = $('#sourceText').val();
            if ((textA.length - $('#sourceText').val().length) != 1 && (textA.length - textB.length) != -1) {
                if ((textA.length - textB.length) > 1)
                    pasteBuffer('Source');
                else if (textB.indexOf(textA) == -1)
                    pasteBuffer('Source');
            }

            textB = $('#sourceText').val();
        });

        $('#editResult_test').bind('input propertychange', function () {
            var textAR = $(this).val();
            var textBR = $('#editResult_test').val();
            if ((textAR.length - textBR.length) != 1 && (textAR.length - textBR.length) != -1) {
                if ((textAR.length - textBR.length) > 1)
                    pasteBuffer('Result');
                else if (textBR.indexOf(textAR) == -1)
                    pasteBuffer('Result');
            }

            textBR = $('#editResult_test').val();
        });

        if (navigator.userAgent.indexOf('Opera/9.80') != -1) {
            $("#loading").html('<img src="/img/loading.gif" />');
            $("#spellLoad").html('<img src="/img/loading.gif" />');
        }

        var clipboard = new Clipboard('.copyBttn');
        clipboard.on('success', function (e) {
            e.clearSelection();
        });

        clipboard.on('error', function (e) {
            var msgCopy = "Your browser does not allow access to your computer\'s clipboard.\r\nPlease use Ctrl+C for Copy, or use Edit menu above the browser toolbar.";
            msgCopy = msgCopy.replace("Ctrl", "Command");
            alert(msgCopy);
        });
    });    
</script>

