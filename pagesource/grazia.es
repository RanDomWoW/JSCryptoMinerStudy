<script>
(function($){
    $(document).ready(function(){
        var existe_rotador_desfiles=$('#rotador_desfiles').length;
        if(existe_rotador_desfiles>0){
            initSlider('#rotador_desfiles',-1,'desfiles');
        }
        var existe_rotador_videos=$('#rotador_videos').length;
        if(existe_rotador_videos>0){
            $('#rotador_videos ul').css('left',-21);
            initSlider('#rotador_videos',3,'videos');
        }
        var existe_rotador_armario=$('#rotador_armario').length;
        if(existe_rotador_armario>0){
            initSlider('#rotador_armario',1,'armario');
        }
        var existe_rotador_desfiles_2=$('#rotador_desfiles_zoom').length;
        if(existe_rotador_desfiles_2>0){
            if($('#rotador_desfiles_zoom ul li').length>1){
                $('#rotador_desfiles_zoom ul li').eq(1).addClass('destacado');
            }
            var mida=$('#rotador_desfiles_zoom ul li').length;
            if(mida<4){
                if(mida<3){
                    var html=$('#rotador_desfiles_zoom ul li').eq(0);
                    $('#rotador_desfiles_zoom ul').append(html);
                    $('#rotador_desfiles_zoom ul').css('left',70);
                    if(mida==1)$('#rotador_desfiles_zoom ul li').eq(0).addClass('destacado');
                }
                $('#rotador_desfiles_zoom').find('.anterior').hide();
                $('#rotador_desfiles_zoom').find('.siguiente').hide();
            }else{
                initSlider('#rotador_desfiles_zoom',1,'desfiles_zoom');
            }
        }
    });
    function initSlider(obj,i_per_pagina,tipus_rotador){
        var items=$(obj).find('ul li').length;
        var ul=$(obj).find('ul');
        var moure=0;
        var tipus=tipus_rotador;
        var sentit='next';
        var canvi_sentit=false;
        var mides=getMida(tipus,i_per_pagina);
        var mida_ul=mides['ul'];
        var mida_li_gran=mides['li_gran'];
        var mida_li_petit=mides['li_petit'];
        var items_per_pagina=mides['items_per_pagina'];
        if(items>items_per_pagina){
            $(obj).find('.anterior').show();
            $(obj).find('.siguiente').show();
        }else{
            $(obj).find('.anterior').hide();
            $(obj).find('.siguiente').hide();
        }
        initActius();
        $(obj).find('.anterior').click(function(){
            if(!$(this).hasClass('disabled')){
                $(this).addClass('disabled');
                if(sentit=='next'){
                    canvi_sentit=true;
                    sentit='prev';
                }else{
                    canvi_sentit=false;
                }
                posicionaLisAbans();
                var primer_actiu=getPrimerActiu();
                var inici=primer_actiu-items_per_pagina;
                var final=primer_actiu;canviaActius(inici,final);
                zoom('prev');
                $(obj).find('ul').animate({left:'+='+mida_ul},600,function(){$(ul).find('li.remove').remove();$(obj).find('.anterior').removeClass('disabled');});
            }
            return false;
        });
        $(obj).find('.siguiente').click(function(){
            if(!$(this).hasClass('disabled')){
                $(this).addClass('disabled');
                if(sentit=='prev'){
                    canvi_sentit=true;
                    sentit='next';
                }else{
                    canvi_sentit=false;
                }
                posicionaLisDespres();
                var primer_actiu=getPrimerActiu();
                var inici=primer_actiu+items_per_pagina;
                var final=inici+items_per_pagina;canviaActius(inici,final);
                zoom('next');
                $(obj).find('ul').animate({left:'-='+mida_ul},600,function(){$(ul).find('li.remove').remove();var no_moure=['desfiles_zoom','armario','desfiles'];if(canvi_sentit&&$.inArray(tipus,no_moure))moure=0;$(ul).css('left','+='+mida_li_petit*moure);$(obj).find('.siguiente').removeClass('disabled');});
            }
            return false;
        });
        $(window).resize(function(){
            var mides=getMida(tipus,i_per_pagina);
            mida_ul=mides['ul'];
            mida_li_petit=mides['li_petit'];
            mida_li_gran=mides['li_gran'];
            items_per_pagina=mides['items_per_pagina'];
            $(ul).css('left',0);
            initActius();
        });
        function posicionaLisAbans(){
            var counter=getNoVisibles('abans');
            var diferencia=items_per_pagina;
            if(tipus=='desfiles_zoom')diferencia=items_per_pagina+1;
            if(counter<diferencia){
                var abans='';
                var inici=items-items_per_pagina;
                var final=items;
                for(var i=inici;i<final;i++){
                    var last=i;
                    abans+='<li>'+$(ul).find('li').eq(last).html()+'</li>';
                    $(ul).find('li').eq(last).addClass('remove');
                }
                $(ul).prepend(abans);
                $(ul).css('left','-='+mida_ul);
            }
        }

        function posicionaLisDespres(){
            var counter=getNoVisibles('despres');
            var diferencia=items_per_pagina;
            if(tipus=='desfiles_zoom')diferencia=items_per_pagina+2;
            if(counter<diferencia){
                moure=diferencia-counter;
                var despres='';
                var inici=0;
                var final=inici+moure;
                for(var i=inici;i<final;i++){
                    var first=i;despres+='<li>'+$(ul).find('li').eq(first).html()+'</li>';$(ul).find('li').eq(first).addClass('remove');
                }
                $(ul).append(despres);
            }
        }
        function zoom(tipus_zoom){
            if(mida_li_gran!=0){
                $(obj).find('ul li.destacado').animate({'width':mida_li_petit},function(){$(this).removeClass('destacado');});
                var obj_zoom=$(obj).find('ul li.destacado').next();
                if(tipus_zoom=='prev')obj_zoom=$(obj).find('ul li.destacado').prev();
                $(obj_zoom).animate({'width':mida_li_gran},function(){$(this).addClass('destacado');});
            }
        }
        function getPrimerActiu(){
            var primer_actiu=0;
            $(ul).find('li').each(function(i){
                if($(this).hasClass('activo')){
                    primer_actiu=i;return false;
                }
            });
            return primer_actiu;
        }
        function initActius(){
            var inici=0;
            var final=items_per_pagina;
            if(tipus=='desfiles_zoom'){
                inici++;
                final++;
            }
            canviaActius(inici,final);
        }
        function canviaActius(inici,final){
            $(ul).find('li').removeClass('activo');
            for(var i=inici;i<final;i++){
                $(ul).find('li').eq(i).addClass('activo');
            }
        }
        function getNoVisibles(mode){
            var counter=0;
            var trobat=false;$(ul).find('li').each(function(){
                if($(this).hasClass('activo')){
                    trobat=true;
                }else{
                    if((!trobat&&mode=='abans')||trobat&&mode=='despres'){
                        counter++;
                    }
                }
            });
            return counter;
        }
        function getMida(tipus,items_per_pagina){
            var mida_ul=mida_li_gran=mida_li_petit=0;
            if($(obj).find('li').length>0){
                switch(tipus){
                    case'desfiles':
                        var margin=$(obj).find('.visible li').css('margin-right').split('px');
                        margin=parseInt(margin[0]);
                        var mida_li_petit=$(obj).find('.visible li').outerWidth()+margin;
                        if(mida_li_petit==75||mida_li_petit==106)
                            items_per_pagina=3;
                        else if(mida_li_petit==107)
                            items_per_pagina=5;
                        else if(mida_li_petit==92||mida_li_petit>=150)
                            items_per_pagina=4;
                        mida_ul=mida_li_petit*items_per_pagina;
                        break;
                    case'armario':
                        mida_ul=$(obj).find('.visible').css('width').split('px');
                        mida_ul=parseInt(mida_ul[0]);
                        mida_li_petit=mida_ul/items_per_pagina;
                        break;
                    case'videos':
                        var margin=$(obj).find('.visible li').css('margin-left').split('px');
                        margin=parseInt(margin[0]);
                        var mida_li_petit=$(obj).find('.visible li').outerWidth()+margin;
                        mida_ul=mida_li_petit*items_per_pagina;
                        break;
                    case'desfiles_zoom':
                        if($('#rotador_desfiles_zoom ul li.destacado').length>0){
                            mida_li_gran=$(obj).find('li.destacado').css('width').split('px');
                            mida_li_gran=parseInt(mida_li_gran[0]);
                        }
                        mida_li_petit=$(obj).find('li').eq(0).css('width').split('px');
                        mida_ul=mida_li_petit=parseInt(mida_li_petit[0]);
                        break;
                }
            }
            var retorn={'ul':mida_ul,'li_gran':mida_li_gran,'li_petit':mida_li_petit,'items_per_pagina':items_per_pagina};
            return retorn;
        }
    }
})(jQuery);
</script>
