(function($){
    $(document).ready(function(){
        var hide=true;
        $('.show-social').hoverIntent(showSocial,hideSocial);
        function showSocial(){
            var h_pico=$('#hover-social span').outerHeight();
            var red=$(this).attr('red');
            $('#hover-social #hover-'+red).show();
            $('#hover-social').show();
            var item_pos=$(this).offset();
            var item_x=item_pos.left;
            var item_w=$(this).outerWidth();
            var box_w=$('#hover-social').outerWidth();
            var box_x=item_x+(item_w/2)-(box_w/2);
            var item_y=item_pos.top-$(document).scrollTop();
            var item_h=$(this).outerHeight();
            var box_h=$('#hover-social').outerHeight();
            var box_y=item_y-(item_h/2)-box_h-4;
            if(box_y<0){
                $('#hover-social span.down').hide();
                $('#hover-social span.up').show();
                box_y=item_y+item_h+15;
            } else {
                $('#hover-social span.down').show();
                $('#hover-social span.up').hide();
            }
            $('#hover-social').css({'left':box_x,'top':box_y});
        }
        function hideSocial(){
            if(hide){
                $('#hover-social').hide();
                $('.hover-red').hide();
            }
        }
        $('#hover-social').hover(function(){ hide=false; },function(){
            hide=true;
            hideSocial();
        });
    });
})(jQuery);
