                <script>
            function bind_links_events() {
                $('#grp_pills a[data-toggle="tab"]').click(function() {
                    var id_ope = $('#ope_pills .active a').attr('href').substring(1);
                    var id_grp = $(this).attr('href').substring(1).split('#')[1];
                    var level_from = $('#grp_pills').attr('data-level-from');
                    load_links(id_ope, id_grp, level_from);
                });
            }
            
            function load_links(id_ope, id_grp, level_from) {
                var id_grp_ppt = id_grp || ''; 
                var level = level_from || 'province';
                $.ajax({
                    type: 'POST',
                    url: 'include/main/ajax.php',
                    data: {action: 'get_links', id_ope: id_ope, id_grp: id_grp_ppt, level_from: level},
                    success: function(data) {
                        $('#ope_content').html(data);
                        bind_links_events();
                    }
                });
            }
            
            $(document).ready(function(){
                img_lazy_load();
                $('#grpproptyp').data('focused',true);
                
                //scroll a la opción seleccionada
                $('#grpproptyp').animate({
                    scrollTop: $(this).find(":selected").offset().top
                }, 0);
                
                var $location_input = $("#location_input");
                var items = 10;
                $location_input.typeahead({
                    autoSelect: true,
                    minLength: 2,
                    items: items,
                    source: function(query, process) {
                        $.ajax({
                            type: 'POST',
                            url: 'include/main/ajax.php',
                            data: {action: 'query_combo', query: query, limit: items},
                            success: function(data) {
                                return process(JSON.parse(data));
                            },
                            error: function() {
                                show_msg('Ocurrió un error al obtener las localizaciones', 'error');
                            }
                        });
                    }
                });
                $location_input.change(function() {
                    var current = $location_input.typeahead("getActive");
                    if (current) {
                        // Some item from your model is active!
                        if (current.name.toLowerCase() == $location_input.val().toLowerCase()) {
                            // This means the exact match is found. Use toLowerCase() if you want case insensitive match.
                        } else {
                            // This means it is only a partial match, you can either add a new item
                            // or take the active if you don't want new items
                            $location_input.val('');
                        }
                    } else {
                        // Nothing is active so it is a new value (or maybe empty value)
                        $location_input.val('');
                    }
                });
                
                $('#btn_search_location').click(function(e) {
                    e.preventDefault();
                    if ($location_input.val().trim() != '') {
                        var optyp = $('#form_location input[name="op_type"]:checked').val();
                        var grpproptyp = $('#form_location select[name="grpproptyp"]').val();
                        var url = '';
                        var prv = '';
                        var zne = '';
                        var sbzne = '';
                        var dis = '';
                        switch ($location_input.typeahead("getActive").type) {
                            case 'province':
                                url = $location_input.typeahead("getActive").slug;
                                prv = $location_input.typeahead("getActive").slug;
                                break;
                            case 'zone':
                                url = $location_input.typeahead("getActive").slug_province+'/'+$location_input.typeahead("getActive").slug;
                                prv = $location_input.typeahead("getActive").slug_province;
                                zne = $location_input.typeahead("getActive").slug;
                                break;
                            case 'subzone':
                                url = $location_input.typeahead("getActive").slug_province+'/'+$location_input.typeahead("getActive").slug_zone+'/'+$location_input.typeahead("getActive").slug;
                                prv = $location_input.typeahead("getActive").slug_province;
                                zne = $location_input.typeahead("getActive").slug_zone;
                                sbzne = $location_input.typeahead("getActive").slug;
                                break;
                            case 'district':
                                url = $location_input.typeahead("getActive").slug_province+'/'+$location_input.typeahead("getActive").slug_zone+'/'+$location_input.typeahead("getActive").slug_subzone+'/'+$location_input.typeahead("getActive").slug;
                                prv = $location_input.typeahead("getActive").slug_province;
                                zne = $location_input.typeahead("getActive").slug_zone;
                                sbzne = $location_input.typeahead("getActive").slug_subzone;
                                dis = $location_input.typeahead("getActive").slug;
                                break; 
                        }
                        if (optyp != '' && grpproptyp != '' && url != '') {
                            url = 'https://www.redpiso.es/'+optyp+'-'+grpproptyp+'/'+url;
                            $.ajax({
                                type: 'POST',
                                url: 'include/main/ajax.php',
                                async: false,
                                data: {action: 'save_search', optyp: optyp, grpproptyp: grpproptyp, url: url, prv: prv, zne: zne, sbzne: sbzne, dis: dis},
                                success: function() {
                                    
                                },
                                error: function() {
                                    
                                }
                            });
                            window.location.href = url;
                        }
                    } else {
                        show_msg('Debe seleccionar una localización', 'info');
                    }
                });
                
                $('#ope_pills a[data-toggle="tab"]').click(function() {
                    var id_ope = $(this).attr('href').substring(1);
                    var id_grp = $('#grp_pills .active a').attr('href').substring(1).split('#')[1];
                    load_links(id_ope);
                });
                bind_links_events();
           });
        </script>
    </body>
