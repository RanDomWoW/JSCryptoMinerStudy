<script type="text/javascript">
$('#top_rodape').corner('bottom 15px');
$('#wrap').corner('top 15px');
$(function() {

	$('.titulo_verde,#eventos_home,#ensino_home,#banner_lateral_home,h1.verde_claro,h1.verde_escuro,h1.verde_musgo,#enquete_home,#conteudo_esquerda').corner("5px");
	$('.nav_internas').corner();
	$('#conteudo_centro.internas #menu_lateral li.topo').corner('cc:#8cb72e top 8px');
	$('#conteudo_centro.internas #menu_lateral li.final').corner('bottom 8px');
	$('#filtro_noticias').SelectImg({imagem:BaseURL+'/imagens/filtro_noticias.gif', Redirect: true});

	//$('#conteudo_centro a[href$=.pdf]').each(function() { $(this).addClass('link_icone pdf'); $('<br clear="all" />').insertBefore($(this)); })
	//$('#conteudo_centro a[href$=.doc],a[href$=.docx]').each(function() { $(this).addClass('link_icone doc'); $('<br clear="all" />').insertBefore($(this)); })
	
	
	//myMenu.Init();
	
	$('#tam_maior').click(function() {
		Container = $($(this).attr('rel'));
		Size = parseInt(Container.css('fontSize'));
		if(Size < 16) {
			Container.css({'fontSize':Size+1+'px'});
		}
		return false;
	});
	
	$('#tam_menor').click(function() {
		Container = $($(this).attr('rel'));
		Size = parseInt(Container.css('fontSize'));
		if(Size > 11) {
			Container.css({'fontSize':Size-1+'px'});
		}
		
		return false;
	});
	
	rH = $('#direita_home').innerHeight();
	cH = $('#conteudo_centro').innerHeight();
	if(rH > cH) {
		rH = rH + 36;
		$('#conteudo_centro').css({height:rH+'px',height:'auto',minHeight:rH+'px'});
	}

	$('#menu_topo li').each(function() {
		sW = $(this).children('.submenus').css('width');
		$(this).find('.has_sub').children('a').css({backgroundImage:'url('+BaseURL+'/imagens/bg/setasub.gif)',backgroundPosition:'right center',backgroundRepeat:'no-repeat'});
		$(this).find('.has_sub').children('.sub2').css({marginLeft:sW});
		
	}).mouseover(function() {
		//Link = $(this).find('a:first-child');
		//Submenu = $(this).find('.submenus');
		//if(!Link.is(':visible')) {
			$(this).children('a').addClass('hover');
			$(this).children('.submenus').css({visibility:'visible',display:'block'});
		//}
	}).mouseout(function() {
		$(this).children('a').removeClass('hover');
		$(this).children('.submenus').css({visibility:'hidden',display:'none'});
	});
	$('div.has_sub').mouseover(function() { $(this).addClass('hover'); }).mouseout(function() { $(this).removeClass('hover'); });

	$('#votar_enquete').click(function() {
		EnviaEnquete();
		return false;
	});
	
	$('input[prompt]').each(function() {
		$(this).val($(this).attr('prompt')); 
	}).focus(function() { 
		if($(this).val()==$(this).attr('prompt'))$(this).val('');
	}).blur(function() { 
		if($(this).val() == '')
			$(this).val($(this).attr('prompt'));
	});
	
	$('#ver_resultado').click(function() {
		$('#dados_enquete').html('');
		$('.load_enquete').css({display:'block'});
		$('.resultado_enquete').css('display','block');
		$.post(BaseURL+'/enquete/resultado/codenquete/'+$('input[name=enquete]').val(), { enquete: $('input[name=enquete]').val() },
			function(response) { 
				$('.load_enquete').css({display:'none'});
				$('#enquete_home .erro').html('');
				if(response.indexOf('0,,Erro:')>-1) {
					erro = response.split('0,,Erro:');
					$('#enquete_home .erro').html(erro[1]);
				}
				else {
					
					$('#dados_enquete').html(response);
				} 
			}
		);
		return false;
	});
	
	$('.resultado_enquete .fechar').click(function() {
		$('.resultado_enquete').css('display','none');
	});
	
	$('#lista_projetos div:nth-child(odd)').each(function() {
		$(this).css({backgroundColor:'#f9f9f9',margin:'5px 0'});
	});

	$('#busca_topo form').submit(function() {
		Termo = $('#termo_busca');
		$.post(BaseURL+'/busca/geratermo/', { termo: Termo.val() },function(response) { if(response.indexOf('0,,')==0) { TrataRespostaAjax(response); } else { window.location = BaseURL+'/busca/listagem/termo/'+response; } });
		return false;
	});
});
function EnviaEnquete() {
	Enquete = $('input[name=enquete]').val();
	Voto = $('#form_enquete input[name=voto]:checked').val();
	if(Voto!=undefined)
		$.post(BaseURL+'/enquete/votar',{ voto: Voto, enquete: Enquete },function(response) { TrataRespostaAjax(response); });
	else
		alert('É necessário escolher alguma das opções para votar');
	return false;
}

function TrataRespostaAjax(Str) {
	
	if(Str == 1 || Str.indexOf('1,,')==0) {
		if(Str.indexOf('1,,')==0) {
			Str = Str.split('1,,');
			alert(Str[1]);
		}
		else
			alert('Informação processada com sucesso.')
	}
	else if(Str.indexOf('0,,')==0) { 
		Str = Str.split('0,,');
		alert(Str[1]);
	}
	else { alert('Falha ao processar a informação. Por favor tente mais tarde.') }
}
</script>
