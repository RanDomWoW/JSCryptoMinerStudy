<div class="deg_top_home">
    <div id="left_home">
        <h1 class="titulo_branco">NOTÍCIAS</h1>
        <h1 class="verde_claro titulos_geral home">ÚLTIMAS NOTÍCIAS UFTM</h1>
        <script>
    pag1 = pag2 = pag3 = '';
    imgLoaded = new Array();
    pagDestaques = 1;
    isLooping = true;
    Loop = '';

    $(function() {
        destaqueHomeActivate($('#noticias_home li:has(.conteudo_noticia):eq(0)'));
        destaqueAtual = 1;
        $('#noticias_home li:has(.conteudo_noticia)').live('mouseover',function() {
            stopLooping();
            Debug(destaqueAtual = $('#noticias_home li:has(.conteudo_noticia)').index(this));
            
            destaqueHomeActivate($(this));
        }).mouseout(function() {
            startLooping();
        });
        $('.pag_destaque_home a').live('click',function() {
            pagDestaques = parseInt($(this).text())-1;
            loadPageDestaques();
            
            return false;
        });
        startLooping();
    });

    function stopLooping() {
        if(Loop != '') {
            isLooping = false;
            window.clearInterval(Loop);
        }
    }

    function startLooping() {
        isLooping = true;
        Loop = window.setInterval("loopDestaques()",8000);
    }

    function loopDestaques() {
        Debug(isLooping);
        if(isLooping) {
            if(destaqueAtual < 3) {
                destaqueHomeActivate($('#noticias_home li:has(.conteudo_noticia):eq('+destaqueAtual+')'));
                destaqueAtual = destaqueAtual+1;
            }
            else {
                
                loadPageDestaques(pagDestaques);
            }
        }
    }

    function destaqueHomeActivate(This) {
        
        This.parents('ul').find('li').removeClass('hover');
        This.addClass('hover');
        if(This.attr('id') != 'noticia_destaque') {

            Cod = This.attr('rel');
            imgName = This.attr('img');
            imgName = (imgName==undefined || imgName == '') ? 'sem_img.jpg' : imgName;
            Debug(imgName);
            try {
                isLoaded = imgLoaded[Cod] != undefined ? true : false;
            } catch (e) {
                isLoaded = false;
            }

            changeImgDest(isLoaded);
        }
    }

    function changeImgDest(isLoaded) {
        if(!isLoaded) {
            $('#loadNotHome').show();
            var Img = new Image();
            imgSrc = BaseURL+'/upload/noticias/'+imgName;
            $(Img).load(function() {
                $('#imgNotDest').attr({src:imgSrc});
                imgLoaded[Cod] = imgSrc;
                 $('#loadNotHome').hide();
            }).attr({src:imgSrc});
        }
        else {
            $('#imgNotDest').attr({src:imgLoaded[Cod]});
        }
    }

    function loadPageDestaques() {
        pagDestaques = (pagDestaques+1) > 3 ? 1 : pagDestaques+1;
        stopLooping();
        $('#loadNotPag').show();
        $('#noticias_home').fadeTo('fast',0.2);
