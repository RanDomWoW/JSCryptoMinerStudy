    <script type="text/javascript">
        $(document).ready(function() {
            $(".threeclick-banner-btn").on("click", function (e) {
                e.preventDefault();
                UM.init({"id":"E7CA6586-B2BB-ED9C-AD88-D04EF46AA728"});
                $('body').append('<div id="popupOverlay"></div><div id="popup"><div id="popup-i" class="popup-i2"><div id="popup-close"></div><div id="popup-title2">Заполните заявку<br>на бесплатный дизайн-проект<br>в три клика!</div><div id="popup-text"><div data-um-id="E7CA6586-B2BB-ED9C-AD88-D04EF46AA728"></div></div></div></div>');
                $('#popup').css('margin-top', ($(window).height() - $('#popup').height())/6 );
                /*$('#popup i, #popup-corner, #popup-question').click( function(){
                 location.href = $('a', '#popup').attr('href');
                 });*/
                $('#popup-close').click( function(){
                    $('#popup, #popupOverlay').remove();
                });

                keypressButton();

                //Клик по кнопке “Попробовать”
                function keypressButton() {
                    dataLayer.push({
                        'event': 'addEvents_makeActions',
                        'event_id': 'd-v6-e1',
                        'action_cat': 'Button',
                        'action_name': 'click',
                        'action_param': 'Попробовать'
                    });
                    //console.log('0');
                }

                //Показ поп-апа заявки
                function showPopUp() {
                    dataLayer.push({
                        'event': 'addEvents_makeActions',
                        'event_id': 'd-v6-e2',
                        'action_cat': 'popup',
                        'action_name': 'show',
                        'action_param': 'Заявка на дизайн-проект'
                    });
                    //console.log('01');
                }

                //Начало ввода данных в форму
                function keypressForm() {
                    dataLayer.push({
                        'event': 'addEvents_makeConversions',
                        'event_id': 'd-v6-e3',
                        'conversion_name': 'Order_form_popup',
                        'conversion_step': 'step_1_interaction',
                        'conversion_param': '(not set)'
                    });
                    //console.log('1');
                }

                //Ошибка при клике по кнопке “Отправить заявку”
                function errorSendForm() {
                    dataLayer.push({
                        'event': 'addEvents_makeConversions',
                        'event_id': 'd-v6-e4',
                        'conversion_name': 'Order_form_popup',
                        'conversion_step': 'step_1_error',
                        'conversion_param': 'Не введены обязательные поля'
                    });
                    //console.log('2');
                }

                //Отображение окна подтверждения номера по СМС
                function showPhoneFormForm() {
                    dataLayer.push({
                        'event': 'addEvents_makeConversions',
                        'event_id': 'd-v6-e5',
                        'conversion_name': 'Order_form_popup',
                        'conversion_step': 'step_2_show',
                        'conversion_param': '(not set)'
                    });
                    //console.log('3');
                }

                //Ошибка при вводе подтверждения номера по СМС
                function errorSmsForm() {
                    dataLayer.push({
                        'event': 'addEvents_makeConversions',
                        'event_id': 'd-v6-e6',
                        'conversion_name': 'Order_form_popup',
                        'conversion_step': 'step_2_error',
                        'conversion_param': 'Неверно введен код с SMS'
                    });
                    //console.log('4');
                }

                //Клик на отправку СМС повторно
                function repeatCodeForm() {
                    dataLayer.push({
                        'event': 'addEvents_makeConversions',
                        'event_id': 'd-v6-e7',
                        'conversion_name': 'Order_form_popup',
                        'conversion_step': 'step_2_sendSMS',
                        'conversion_param': '(not set)'
                    });
                    //console.log('5');
                }

                //Успех при подтверждении номера по СМС
                function shownConfirmForm() {
                    dataLayer.push({
                        'event': 'addEvents_makeConversions',
                        'event_id': 'd-v6-e8',
                        'conversion_name': 'Order_form_popup',
                        'conversion_step': 'success',
                        'conversion_param': '(not set)'
                    });
                    //console.log('6');
                }

                //Клик на отправку СМС повторно
                function repeatCodeDom() {
                    setTimeout(function(){
                        $("input#repeatCode").on("click", function () {
                            var disabled = $(this).attr('disabled');
                            if (disabled != 'disabled') {
                                repeatCodeForm(); //вызов функции
                            }
                        });
                    }, 3000);
                }


                UM.vent.on('layout:shownForm', function(id){
                    //if ((id == "E7CA6586-B2BB-ED9C-AD88-D04EF46AA728")) {

                    //форма показана
                    showPopUp();

                    //Начало ввода данных в форму
                    setTimeout(function(){
                        var i=0;
                        $("form.um-form input").keypress(function() {
                            if (i==0){//вызываем функцию только 1 раз
                                keypressForm(); //вызов функции
                                i++;
                            }
                        });
                    }, 1000);
                    //}
                });

                UM.vent.on('layout:showPhoneForm', function(id){
                    if ((id == "E7CA6586-B2BB-ED9C-AD88-D04EF46AA728")) {
                        showPhoneFormForm();
                        //т.к. это ajax, и несущесвует класса для повт. отправки, мы вызываем функцмю события после отображения формы с смс
                        repeatCodeDom();
                    }
                });
                UM.vent.on('layout:showConfirm', function(id){
                    if ((id == "E7CA6586-B2BB-ED9C-AD88-D04EF46AA728")){
                        shownConfirmForm();
                    }
                });

                UM.vent.on("layout:errorSaving", function (errors) {
                    errorSendForm();
                });
                UM.vent.on("layout:invalidCode", function (model) {
                    errorSmsForm();
                });

            });

            UM.vent.on('layout:shownForm', function(){
                setTimeout(function(){
                    //Поднимаем в Москве студию Варшавское ш., д. 82 наверх
                    $('.um-form-group-shop').click(function() {
                        for (var i=0;i<=$('.um-body').length-1;i++){
                            if($('.um-body:eq('+i+') .um-city-list li.active').text() == 'Москва') {
                                $('.um-body:eq('+i+') .um-shop-list li span:contains("Варшавское ш., д. 82")').parent('li').insertBefore('.um-body:eq('+i+') .um-shop-list li:eq(1)');
                            }
                        }
                    });

                    //Вставляем текст "Варшавское ш., д. 82 — Самая большая студия Марии!"
                    $('.um-form-group-city').click(function() {
                        for (var i=0;i<=$('.um-body').length-1;i++){
                            if($('.um-body:eq('+i+') .um-city-list li.active').text() == 'Москва') {
                                if (!($(".um-header").next().hasClass("add-title-2"))) {
                                    $('.um-header:eq('+i+')').after('<div class="add-title-2" style="text-align:center"><span style="margin: 0; padding: 10px 0; font-family: \'OfficinaSansMedium\', sans-serif; letter-spacing: 0.2px; color: #d50000; display: inline-block; font-size: 14px;">Варшавское ш., д. 82 — Самая большая студия Марии!</span><div>');
                                }
                            } else {
                                $('.add-title-2').remove();
                            }
                        }
                    });
                }, 1000);
            });
        });

        
