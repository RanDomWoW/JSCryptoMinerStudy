    $('#footerShopsSliderCityName').text($('.studioContacts .city').first().text());
    (function () {
        function studioDeclension(counts) {
            switch(parseInt(counts)) {
                case 1:
                    return " студия";
                    break;
                case 2:
                case 3:
                case 4:
                    return " студии";
                    break;
                default: 
                    return " студий";
                    break;
            }
        }

        var countsShops = $('.studioContacts .city').length;
        if (countsShops < 10) {
            $('#footerShopsSliderCounts').text(countsShops + studioDeclension(countsShops));
        } else if (countsShops < 15) {
            $('#footerShopsSliderCounts').text(countsShops + studioDeclension(countsShops));
        } else { 
            var counts = countsShops.toString().split('').pop();
            $('#footerShopsSliderCounts').text(countsShops + studioDeclension(counts));
        }
        
    })();

    // slider uikit затраивает элементы слайдера, если студия одна
    // убираем дубликаты после добавления слайдера uikit'ом в DOM
    UIkit.domObserve('#shopSlider', function(element) { 
        var idShop = 0;
        $(element).find('.shopSlide').each(function() {
            if (idShop === $(this).data('idshop')) {
                $(this).hide();
            } else {
                idShop = $(this).data('idshop');
            }
        });
    })
