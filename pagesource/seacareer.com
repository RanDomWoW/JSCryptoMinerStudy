<script type="text/javascript">
$(document).ready(function() {
	
	
	$('#categories-search .tabs').show();
	
	function switch_tabs(tab) {
		$('#categories-search .tabs .i1 a').removeClass('active');
		$('#categories-search .tabs .i2 a').removeClass('active');
		$('#categories-search .tabs .i3 a').removeClass('active');
		$('#categories-search .tabs .i' + tab + ' a').addClass('active');
	}
	
	var tab_active = $.cookie("sector");
	
	if (tab_active=='shoreside') {
		$('#categories-search .tab1-content').hide();
		$('#categories-search .tab2-content').show();
		$('#categories-search .tab3-content').hide();
		switch_tabs(2);
	} else if (tab_active=='offshore') {
		$('#categories-search .tab1-content').hide();
		$('#categories-search .tab2-content').hide();
		$('#categories-search .tab3-content').show();
		switch_tabs(3);
	} else {
		$('#categories-search .tab1-content').show();
		$('#categories-search .tab2-content').hide();
		$('#categories-search .tab3-content').hide();
		switch_tabs(1);
	}
	
	$('#categories-search .tabs .i1').click(function() {
		switch_tabs(1);
		$('#categories-search .tab1-content').fadeIn(200);
		$('#categories-search .tab2-content').hide();
		$('#categories-search .tab3-content').hide();
		$.cookie("sector", "seagoing", { expires:30, path:'/' });
		return false;
	});
	
	$('#categories-search .tabs .i2').click(function() {
		switch_tabs(2);
		$('#categories-search .tab1-content').hide();
		$('#categories-search .tab2-content').fadeIn(200);
		$('#categories-search .tab3-content').hide();
		$.cookie("sector", "shoreside", { expires:30, path:'/' });
		return false;
	});
	
	$('#categories-search .tabs .i3').click(function() {
		switch_tabs(3);
		$('#categories-search .tab1-content').hide();
		$('#categories-search .tab2-content').hide();
		$('#categories-search .tab3-content').fadeIn(200);
		$.cookie("sector", "offshore", { expires:30, path:'/' });
		return false;
	});
	
	$('#categories-search .inner3').removeClass("hidden");
	
	
	
});
</script>
