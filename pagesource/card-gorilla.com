    	function cast_nextpage(page) {
      		if (total_page > 0) {
        		if ((page + 1) > total_page) {
          		cast_page(1);
        		}
        		else {
          		cast_page(page + 1);
        		}
      		}
    	}

    	function cast_prevpage(page) {
      		if (total_page > 0) {
        		if ((page - 1) < 1) {
          		cast_page(total_page);
        	}
        		else {
          		cast_page(page - 1);
        		}
      		}
    	}

    	function cast_page(page) {
      		for(i=1;i<=total_page;i++) {
        		document.getElementById("cast_list"+i).style.display = "none";
      		}
      		document.getElementById("cast_list"+page).style.display = "block";
