    var top_margin = 1270;
    var bottom_margin = 1186;

    if ($(window).scrollTop() > top_margin) {
      if ($(document).height() - $(window).scrollTop() < bottom_margin) {
        var new_top = $(document).height() - (top_margin+bottom_margin) + 90 + "px";
        $cache.css({'position':'relative', 'top':new_top});
      }
      else {
        $cache.css({'position':'fixed', 'top':'100px'});
      }
    }
    else {
      $cache.css({'position':'relative', 'top':'100px'});
    }
  }
  $(window).scroll(fixDiv);
  fixDiv();
</script>
