		<script type="text/javascript">
			$(document).ready(function() {
				var email_submit = function() {
					var user_name = $('#webmail_user').val();
					var user_name_array = user_name.split("@");
					if (user_name_array[1] && user_name_array[1] == 'netmadeira.com')
						user_name = user_name_array[0];
					$('#webmail_input_user').val(user_name);
					$('#loginform').submit();
					$('#webmail_input_password').val('');
				}
				$('#email_submit').click(function(){ email_submit(); });
				$('#webmail_input').keypress(function(event) {
		            var code = (event.keyCode ? event.keyCode : event.which);
		            if (code == 13) email_submit();
		        });
				$('#webmail_input_password').keypress(function(event) {
		            var code = (event.keyCode ? event.keyCode : event.which);
		            if (code == 13) email_submit();
		        });
			});
		</script>
