<script>
var indexUrl=window.location.href;
var itemIndex=0;
if(indexUrl.indexOf("#page")!=-1){
	var pageID=indexUrl.substring(indexUrl.lastIndexOf('#page')+5,indexUrl.length);
	pageID=parseInt(pageID)-1;
	if(pageID<10){
		itemIndex=pageID;
		if(pageID > 1){
			$('.pf_con').show();
		}else{
			$('.pf_con').hide();
		}		
	}	
}
var runPage;
runPage = new FullPage({
	id: 'pageContain',
	slideTime: 800,
	effect: {
		transform: {
			translate : 'Y',          // 'X'|'Y'|'XY'|'none'
 		    scale : [.1, 1],           // [scalefrom, scaleto]
      		rotate : [0, 0]
		},
		opacity: [0, 1]
	},
	pageNow:itemIndex,
	callback:function(index){
		if(index >= 1){
			$('#sy_pfcon').animate({bottom:'-114px'},1000);
			$('#sy_pfcon').css('z-index','10');
			$('.pf_con').show();
		}else{
			$('#sy_pfcon').animate({bottom:'32px'},1000);
			$('.pf_con').hide();
		}
	},
  	continuous:true,
	mode: 'wheel, touch, nav:navBar',
	easing: 'ease'
});


</script>


