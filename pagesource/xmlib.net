function contentPosition(){
	var con_height =$(window).height(); //获取页面可见高度
	var con_height_half=(con_height-114-32-498)/2+114;
	if (con_height <= 700){
	   $('.content').css('top',con_height_half+'px').css('margin-top','0');		   
	}else{		   
	   $('.content').css('top','50%').css('margin-top','-234px');
	}
}
$(window).resize(function() {
	contentPosition();
 }) 
