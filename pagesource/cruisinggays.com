        function loadCities() {
            $.ajax({
                url: "/api/geo/gettopcities",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                data: {
                    count: 50
                },
                success: function (data) {
                    //deleteOverlays();
                    
                    if (data) {
                        markers = new L.featureGroup().addTo(map);

                        $.map(data, function (city, idx) {

                            var latlng = city.LatLong.split(',');
                            var cityName = city.Name.replace(' ', '-').toLowerCase();

                            L.marker([latlng[1], latlng[0]], { icon: purpleIcon }).addTo(markers)
                                .bindPopup("<b>Visit our <a href=\"http://www.cruisinggays.com/" + cityName + "\">" + city.Name + " gay guide</a></b></div>")
                                .on("click", function () { window.location.href = "http://www.cruisinggays.com/" + cityName ; });
                        });
                        markers.on('mouseover', function (e) {
                            e.layer.openPopup();
                        });
                        markers.on('mouseout', function (e) {
                            e.layer.closePopup();
                        });

                        //var bounds = new L.latLngBounds(markersArray);
                        //map.fitBounds(bounds);
                    }
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    alert(errorThrown);
                }
            });
        }

