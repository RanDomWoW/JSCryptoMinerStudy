	<script>
	$(function(){
		var host= location.host.indexOf('t.fu.be') != -1 ? "test.kid.be-xx.com" : location.host;
		var ishttps = 'https:' == document.location.protocol ? true: false;
        if(ishttps){
                host = 'https://' + host;
        }else{
                host = 'http://' + host;
        }
		try{
			$.get(host+'/ra/', function(data){
				var header = data.replace(/[\r\n]/g,'').match(/<!-- header start -->(.*?)<!-- header end -->/)[0];
				var footer = data.replace(/[\r\n]/g,'').match(/<!-- footer start -->(.*?)<!-- footer end -->/)[0];
				$("#frame-head").html(header);
				$("#frame-foot").html(footer);					
			},'html');
		}catch(e){};
		
		$.ajax({
			url: host+'/news/getTop',
			type: 'POST', //GET
			timeout: 5000,    //超时时间
			dataType: 'json',    //返回的数据格式：json/xml/html/script/jsonp/text
			success: function (data, textStatus, jqXHR) {
				window.console && console.log(data);
				if (data.errcode == '0') {
					var html = '';
					for(var i=0; i<data.result.length;i++){
						html+='<li><a href="'+host+'/news/'+data.result[i].urlCode+'"><span>'+data.result[i].date+'</span>'+data.result[i].title+'</a></li>';
					}
					$('#newsTops').html(html);
				} else {					
					fn.alert(data.errmsg);
				};
			}
		})
	})
	</script>
