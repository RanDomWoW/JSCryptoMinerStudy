/*---------------------------------------------------
    共通フッター (footer-xxx)
---------------------------------------------------*/
.footer {
  margin-top: 100px;
}

.footer ul, .footer ol{
  list-style: none;
  padding-left: 0;
}

.footer li {
  padding-left: 15px;
}

.footer .badge {
  display: initial;
}

/* footer top */
.footer-top {
  background-color: #efefef;
  padding-bottom: 10px;
}

.footer-top h2.page-header {
  border-color: #ddd;
}

.footer-top a:hover {
  background-color: #ddd !important;
}

/* blog archives */
.BlogArchive {
  text-align: left
}

.BlogArchive a {
  text-decoration: none;
  position: relative;
  display: block;
  padding: 10px 15px;
  border-radius: 4px;
}

.BlogArchive a:hover {
  /*background-color: #eee;*/
}

.BlogArchive .toggle {
  cursor: pointer;
  font-family: Arial, sans-serif
}

.BlogArchive .toggle-open {
  _font-size: 1.7em;
  line-height: .6em
}

.BlogArchive a.post-count-link {
  /*display: inline-block;*/
  margin-left: 3em;
}

.BlogArchive a.toggle {
  color: inherit;
  /*display: inline-block;*/
  position: absolute;
}

.BlogArchive ul {
  margin: 0;
  padding: 0;
  list-style: none;
  list-style-image: none;
  border-width: 0
  position: relative;
}

.BlogArchive ul li {
  background: none;
  list-style: none;
  list-style-image: none;
  list-style-position: outside;
  border-width: 0;
  padding-left: 15px;
  background-image: none
}

.BlogArchive ul ul li {
  padding-left: 1.2em
}

.BlogArchive .collapsed ul {
  display: none
}

/* footer bottom */
.footer-bottom {
  background-color: #333333;
  color: #dddddd;
  padding-top: 10px;
  padding-bottom: 10px;
}

.footer-bottom a {
  color: #dddddd;
}

.footer-bottom .logo {
  display: inline-block;
  margin-left: 10px;
  margin-right: 10px;
}

.footer-bottom .logo>img {
  width: 23px;
  height: 23px;
  padding: 1px;
}

