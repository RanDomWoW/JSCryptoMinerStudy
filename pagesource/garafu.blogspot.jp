    garafu.lib.initGaNavbar = function () {
        $('#header a.navbar-brand').on('click', function (event) { ga('send', 'event', 'navbar', 'click', this.href, 1); });
        $('#header a.navbar-item').on('click', function (event) { ga('send', 'event', 'navbar', 'click', this.href, 1); });
        $('#frmNavbarSearch button').on('click', function (event) { ga('send', 'event', 'navbar', 'click', '/search=' + $('#frmNavbarSearch #s').val(), 1); });
    };

    garafu.lib.initGaShare = function () {
        $('a.socialbutton-link').on('click', function (event) { ga('send', 'event', 'share', 'click', this.href, 1); });
    };
    
    garafu.lib.initGaPopularPosts = function () {
        $('#PopularPosts1 a.item-content').on('click', function (event) { ga('send', 'event', 'popularposts', 'click', this.href, 1); });
    };

    // ドキュメント読み込み後のエントリーポイント
    $(document).ready(function (event) {
        garafu.nav.init();
        garafu.idx.init();
        garafu.itm.init();
        garafu.lib.init();
    });
})();
