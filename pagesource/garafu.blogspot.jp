}

/* popular posts
--------------------------*/

.PopularPosts .item-content {
  background: #fff;
  margin: 0 0 20px;
  padding: 5px 10px;
  position: relative;
  border-radius: 4px;
}

.PopularPosts .item-thumbnail {
  float: left;
  margin: 0 5px 5px 0;
}

.PopularPosts .item-title {
  font-weight: bold;
}

