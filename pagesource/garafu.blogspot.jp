  height: 100%;
}

.jumbotron .bg-cover {
  background-color: #000;
  opacity: 0.4;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.index-message {
  margin-top: 50px;
}

.index-post-item {
  background: #fff;
  margin: 0 0 20px;
  position: relative;
  border: 1px solid #ddd;
  border-radius: 4px;
}

.index-post-item a {
  color: #222;
  text-decoration: none;
  display: inline-block;
  width: 100%;
}

.index-post-item a:hover {
  color: #444;
  background-color: #efefef;
  border-color: #adadad;
}

.index-post-title {
  margin: 0;
