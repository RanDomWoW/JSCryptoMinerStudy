    共通サイドメニュー
---------------------------------------------------*/

/* related posts
--------------------------*/
.RelatedPosts {
}

.RelatedPosts i {
  text-align: center;
}

.RelatedPosts ul {
  list-style-type: none;
  margin: 0 !important;
  padding: 0;
}

.RelatedPosts .item-content {
  margin-top: 2px;
}

.RelatedPosts .item-content>a {
  padding: 10px 15px;
  border-radius: 4px;
  text-decoration: none;
  display: block;
}

.RelatedPosts .item-content>a:hover {
  background-color: #eee;
}

.RelatedPosts .item-thumbnail {
  float: left;
