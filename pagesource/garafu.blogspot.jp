---------------------------------------------------*/
/* Blogger style overwrite */
.jumbotron {
  color: #efefef;
  position: relative;
  background: transparent;
}

.jumbotron .bg-container {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}

.jumbotron .bg-image {
  background-image: url("//2.bp.blogspot.com/-a0sVOUkRCE4/WGXPo2UvkXI/AAAAAAAADt8/6m3lwX7KQ5g5Gi2t0VJpwhExkFeRkOHbQCLcB/s1600/jumbotron-wood.jpg");
