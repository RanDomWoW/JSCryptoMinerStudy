<script src="/js/fancybox/jquery.fancybox.pack.js"></script>

<script>
    $(document).ready(function(){
        $("a.custLogin").fancybox({ helpers: { title: null }, padding:35 });

        /* Exit topbar company/search overlay */
        $('a.exit-top-overlay').on('click', function(e) {
            $('#header').toggleClass('popup-active');
            if($('.top-overlay.popup').css('display') == 'block') {
                $('.top-overlay.popup').css('display', 'none');
            }
            e.preventDefault();
        });

        /* Add background fill to fixed navbar beyond video splash,
         trigger using Waypoint js */

        $('.intro-block').waypoint(function(dir) {
            if (dir === 'down') {
                $('#wrapper').removeClass('video-on-scr');
                $('#wrapper .logo img').attr('src', '/images/logo29.svg');
            }
        }, {
            offset : $('#header').outerHeight(), triggerOnce: false
        })

        $('.intro-block,.videoContent').waypoint(function(dir) {
            if (dir === 'up') {
                $('#wrapper').addClass('video-on-scr');
                $('#wrapper .logo img').attr('src', '/images/icp_logo_2.svg');
            }
        }, {
            offset : -$('#header').outerHeight() - 50, triggerOnce: false
        });

        $(".custLogin#gsub").prop("disabled", true);

        $("#icp_name").keyup(function(){
            if ($("#icp_name").val().length < 3) {
                $(".custLogin#gsub").prop("disabled", true);
            } else {
                $(".custLogin#gsub").prop("disabled", false);
            }
        });

    });
</script>
<script>
    $(document).ready(function(ev){$("a.play-btn").fancybox({});
    });
</script>



