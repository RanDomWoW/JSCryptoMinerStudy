            sdkLoaded();
        } else {
            //this script loaded before FB sdk loaded;
            //when the sdk is loaded it looks for window.fbAsyncInit;
            //point Facebook's fbAsyncInit to our sdkLoaded method
            window.fbAsyncInit = sdkLoaded;
