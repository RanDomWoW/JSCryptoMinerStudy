<script>
    //** YouTube **

    setTimeout(function () {
        if (typeof (player) == 'undefined') {
            onYouTubeIframeAPIReady();
        }
    }, 3000)

    var players = {};

    window.onYouTubeIframeAPIReady = function () {

        $('iframe[src*="youtube.com"]').each(function () {
            var playerid = GetYouTubeID($(this).attr("src"));
            $(this).attr('id', playerid);
            if ($(this).attr("src").indexOf('?') != -1) {
                if ($(this).attr("src").indexOf("enablejsapi=1") == -1) {
                    $(this).attr('src', $(this).attr("src") + "&enablejsapi=1");
                }
            } else {
                $(this).attr('src', $(this).attr("src") + "?enablejsapi=1");
            }
            var eventname = "onPlayerStateChange" + playerid
            players[playerid] = new YT.Player(playerid, {
                height: 346,
                width: 536,
                videoId: playerid,
                events: {
                    'onStateChange': onPlayerStateChange
                }
            });
        });
    };

    function GetYouTubeID(url) {
        var ID = '';
        url = url.replace(/(>|<)/gi, '').split(/(vi\/|v=|\/v\/|youtu\.be\/|\/embed\/)/);
        if (url[2] !== undefined) {
            ID = url[2].split(/[^0-9a-z_\-]/i);
            ID = ID[0];
        }
        else {
            ID = url.toString();
        } 
        return ID;
    }

    function onPlayerStateChange(event) {
        if (event.target.getPlayerState() == 1) {
            mixpanel.register({'Watched Video':true});
            mixpanel.track("watched video", {
                "Video Title": event.target.getVideoData().title
            });
            
        }
    }
	

    //** Vimeo **

    function GetVimeoID(url) {
        var ID = '';
        url = url.replace(/(>|<)/gi, '').split(/(vi\/|v=|\/v\/|player.vimeo.com\.be\/|\/video\/)/);
        if (url[2] !== undefined) {
            ID = url[2].split(/[^0-9a-z_\-]/i);
            ID = ID[0];
        }
        else {
            ID = url.toString();
        }
        return ID;
    }

    $(function () {

        $('iframe[src*="player.vimeo.com"]').each(function () {
            var playerid = GetVimeoID($(this).attr("src"));
            $(this).attr('id', playerid);

            if ($(this).attr("src").indexOf('?') != -1) {
                if ($(this).attr("src").indexOf("api=1") == -1) {
                    $(this).attr('src', $(this).attr("src") + "&api=1");
                }
                if ($(this).attr("src").indexOf("player_id") == -1) {
                    $(this).attr('src', $(this).attr("src") + "&player_id=" + playerid);
                }
            } else {
                $(this).attr('src', $(this).attr("src") + "?api=1&player_id=" + playerid);
            }

            var iframe = $(this)[0];
            var player = $f(iframe);
            //var status = $('.status');

            // When the player is ready, add listeners for play, pause, finish, and playProgress
            player.addEvent('ready', function () {
                //status.text('ready');

                player.addEvent('play', play);
                //player.addEvent('pause', onPause);
                //player.addEvent('finish', onFinish);
                //player.addEvent('playProgress', onPlayProgress);
            });
        });



        //// Call the API when a button is pressed
        //$('button').bind('click', function () {
        //    player.api($(this).text().toLowerCase());
        //});


        function play(data) {
            var player_id = JSON.parse(event.data).player_id;
            $.getJSON('http://www.vimeo.com/api/v2/video/' + player_id + '.json?callback=?', {
                format: "json"
            }, function (data) {
                mixpanel.register({'Watched Video':true});
                mixpanel.track("watched video", {
                    "Video Title": data[0].title
                });
                
            });
        }

        function onPause() {
            alert(event.data);
            alert("Pause");
            //  status.text('paused');
        }

        function onFinish() {
            alert("Finished");
            alert(event.data);
        }

        function onPlayProgress(data) {
            alert("Finished");
            alert(data.seconds + 's played');
        }
    });
</script>



