<script type="text/javascript">
    var myTime     = new Date;
    
    //var timeDiff   = myTime.getTime() - 1521815766000 - (myTime.getTimezoneOffset() + 180) * 60000;
    var london_time = myTime.getTime() + (myTime.getTimezoneOffset()) * 60000;
    var timeDiff = 180 * 60000;

    function lz(i) {
        return i.toString().length == 1 ? "0" + i.toString() : i;
    };
    
    function timeTick(){
        var time = new Date;
        
        var london_time = time.getTime() + (time.getTimezoneOffset()) * 60000;
        
        //console.log(time);
        time = new Date( london_time + timeDiff );
        
        $('.clock_text').html(
            lz(time.getHours())
            + (time.getSeconds() % 2 == 0 ? ':' : '&nbsp;')
            + lz(time.getMinutes())
            + "<span>&nbsp;&nbsp;&nbsp;"
            + lz(time.getDate())
            + '.'
            + lz(time.getMonth() + 1)
            + '.'
            + time.getFullYear()
            + "</span>"
        );
    }
    
    function startTimeTick(){
        var time = new Date;
        //console.log(time);
        time = new Date( london_time + timeDiff );
        
        $('.clock_text').html(
            lz(time.getHours())
            + ':'
            + lz(time.getMinutes())
            + "<span>&nbsp;&nbsp;&nbsp;"
            + lz(time.getDate())
            + '.'
            + lz(time.getMonth() + 1)
            + '.'
            + time.getFullYear()
            + "</span>"
        );
    }
    
    setInterval(timeTick, 1000);
</script>


