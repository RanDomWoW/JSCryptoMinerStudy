<script>
    // set to debug if the theme is unpublished or on staging domain to prevent sending pixels from development theme.
    var debug = false;
    if (Shopify && (Shopify.theme.role == 'unpublished' || Shopify.shop.indexOf("staging") != -1 )) {
        debug = true;
    }
    // on document ready watch for email signups
    $(function() {
        // This is the signup in the footer
        $( "#mc-embedded-subscribe-form" ).submit(function(e) {
            firePixels('Footer');
        });
        // This is the signup in the modal #ep_1 > div > div.ep-modal.stage1 > form
        $( "#ep_1 > div > div.ep-modal.stage1 > form" ).submit(function(e) {
            firePixels('Modal');
        });
    });
    // Depending on environment either console log the signup or submit the pixels if live
    function firePixels(eventLabel) {
        if (debug) {
            console.log('signup in ' + eventLabel + ' submitted');
            // e.preventDefault();
        } else {
            sendSignupPixelGoogle(eventLabel);
            sendSignupPixelFacebook(eventLabel);
        }
    }
    // Here's the Google code to fire:
    function sendSignupPixelGoogle(eventLabel) {
        ga('send', {
        hitType: 'event',
        eventCategory: 'All',
        eventAction: 'Email-Capture',
        eventLabel: eventLabel
        });
    }
    // Here's the Facebook code to fire:
    function sendSignupPixelFacebook(eventLabel) {
        fbq('init', '1582249872056994');
        fbq('track', 'CompleteRegistration');
        fbq('trackCustom', eventLabel, {
            Type: 'All'
        });
    }
</script>

    
  
  
  
<!-- Initialise Cart.js once the page has loaded -->
<script type="text/javascript">
    jQuery(function() {
        CartJS.init({"token":"03930255c976aee1217aee91a0a5556f","note":null,"attributes":{},"original_total_price":0,"total_price":0,"total_discount":0,"total_weight":0,"item_count":0,"items":[],"requires_shipping":false});
    });

    // Once all cart requests are complete, update the item counter
    $(document).on('cart.requestComplete', function(event, cart) {
      if (cart.item_count > 0) {
        $(".mobile-header .cart").addClass("active");
        $("#cart_link").html('<div class="cart-count">' + cart.item_count + '</div>');
        $("#slideout-toggle").html('<div class="icon-cart"></div> <div class="cart-count">' + cart.item_count + '</div>');
      } else {
        $(".mobile-header .cart").removeClass("active");
        $("#cart_link").html('');
        $("#slideout-toggle").html('<div class="icon-cart-blue"></div>');
      }
    });

    // General AJAX success handler (add, remove, change qty)
    $(document).ajaxSuccess(function(event, jqxhr, settings) {
        if(settings.url == "/cart/add.js") {
            rebuildMiniCart();
        }
        if(settings.url == "/cart/change.js") {
            rebuildMiniCart();
        }
    });

    // Handle errors when adding to cart
    $(document).ajaxError(function(event, jqxhr, settings, thrownError) {
        if(settings.url == "/cart/add.js") {
            var response = JSON.parse(jqxhr.responseText);
            alert(response.description);
        }
    });

    // Reload mini cart by calling the cart with a view of "minicart"
    // Replace the current form with the form from the response
    function rebuildMiniCart(open) {
        $.get("/cart?view=minicart", function(data) {
            $(".cart-slideout .cart-items-container").replaceWith($(data).find(".cart-items-container"));
            if (!$(".cart-slideout").hasClass("is-open")) {
                $("#slideout-toggle").trigger("click");
            }
        });
    }
</script>
  

