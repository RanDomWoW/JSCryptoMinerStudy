        if (!$(this).hasClass("disabled")) {
            var currentVal = parseInt($(target).val());
            if (!isNaN(currentVal)) {
                $(target).val(currentVal + 1).trigger("change");
            } else {
                $(target).val(0).trigger("change");
            }
        } else {

            alert("Sorry, only " + $(target).data("max") + " " + $(target).data("title") + " available");
		}
    });
    // This button will decrement the value till 0
    $(document).on("touchstart click", ".qtyminus", function(e) {
        e.preventDefault();
        var target = $(this).parent().find('.qty');
        var currentVal = parseInt($(target).val());
        if (!isNaN(currentVal) && currentVal > 0) {
            $(target).val(currentVal - 1).trigger("change");
        } else {
            $(target).val(0).trigger("change");
        }
    });

    $(document).on("touchstart click", ".slideout-close, p.continue a", function(e) {
    	e.preventDefault();
    	$(".cart-slideout").animate({
                right: '-=425px'
            }, 'fast').removeClass("is-open");
            y = 0;
        $(".page-overlay").fadeOut("fast");
        $("body, html").removeClass("noscroll");
    });

	$(document).on("change", ".cart-slideout .qty", function() {
	    var qty = $(this).val();
		CartJS.updateItem($(this).attr('data-update-quantity'), qty);
	});

    var y = 0;
	$("#slideout-toggle, #cart_link").click(function (e) {
		e.preventDefault();
		if (y == 0) {
			$(".cart-slideout").animate({
				right: 0
			}, 'fast').addClass("is-open");

			y = 1;
		} else {
			$(".cart-slideout").animate({
				right: "-425px"
			}, 'fast').removeClass("is-open")
			y = 0;
		}

		$(".page-overlay").fadeIn("fast");
		$("body, html, .cart-slideout").addClass("noscroll");
	});



    $(".page-overlay").click(function () {
    	$(".cart-slideout").animate({
                right: '-=425px'
            }, 'fast').removeClass("is-open");
            y = 0;
        $(this).fadeOut("fast");
        $("body, html, .cart-slideout").removeClass("noscroll");
    });


    $(".cart-slideout.noscroll, body.noscroll, html.noscroll").on('touchmove', function(e) {

	    e.preventDefault();
    });


    $(".mini-view-cart").click(function() {
       window.location = "/cart";
	});

	if ($(window).height() < 150) {
	        $('.slideout-nosto').addClass('hide');
	    } else {
	    	$('.slideout-nosto').removeClass('hide');
	    }

	$(window).resize(function() {
	    if ($(window).height() < 150) {
	        $('.slideout-nosto').addClass('hide');
	    } else {
	    	$('.slideout-nosto').removeClass('hide');
	    }
	}); 
      
   
});

