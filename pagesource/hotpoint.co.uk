		    var dataRep = data.replace(/^.*\/\*/m,'').replace(/\*\/.*$/m,'');
		    var deleteObj = jQuery.parseJSON(dataRep);
		    console.log(deleteObj);
		    
		    if(deleteObj.result && deleteObj.result == 'success'){	   		 
		        console.log('USER DELETED, logging off...');
		        window.location.href = 'https://www.hotpoint.co.uk/webapp/wcs/stores/servlet/Logoff?catalogId=10585&langId=-11&URL=https%3A%2F%2Fwww.hotpoint.co.uk%2F&storeId=10236';
	        }
	        else if (deleteObj.result && deleteObj.result == 'error') {
	       		 $('#deleteUserAreaMessage').html(deleteObj.reason);
	         }
	        
	    },
	    error: function (data, status, error) {
		    var dataRep = data.replace(/^.*\/\*/m,'').replace(/\*\/.*$/m,'');
		    var deleteObj = jQuery.parseJSON(dataRep);
		    console.log(deleteObj);
