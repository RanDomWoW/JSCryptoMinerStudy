<script type="text/javascript">
        $(function(){
            getBanner();
        });
        // 获取轮播图
        function getBanner() {
            $.common.get('/home/getBanner.aspx', function (data) {
                var html = '', li = '';
                for(var i = 0; i < data.length; i++) {
                    var obj = data[i];
                    html += '<li data-opacity="0.1" style="background:url('+obj.picUrl+') 50% 50% no-repeat; ">' +
                        '<a href="'+obj.url+'" target="_blank">'+obj.title+'</a>' +
                        '</li>';
                    li += '<li class=""></li>';
                }
                $("#thumb-li").html(li);
                $("#slideBox").html(html);
                jQuery(".tdw-banner").slide({ mainCell: ".bd ul", effect: "fold", autoPlay: true, interTime: 5000, delayTime: 1000 });//banner SuperSlide 调用
                $('#slideBox').css({"width":"100%"});
                $('#slideBox').find("li").css("width","100%");
            }, function (err) {
                console.error(err);
            });
        }

        //底部右侧广告位的点击量统计
        function lowerRightAdCount(clickSign) {
            if (typeof (nwbi_api) != 'undefined') {
                var userName = "\u672A\u767B\u9646";
                var now = new Date();
                var clickDate = now.getFullYear()+"-"+(now.getMonth()+1)+"-"+now.getDate()+" "+(now.getHours()<10?"0"+now.getHours():now.getHours())+":"+(now.getMinutes()<10?("0"+now.getMinutes()):now.getMinutes())+":"+now.getSeconds();
                var clickLoc = clickSign==1?"收起位置":"展开位置"
                nwbi_api.nwbi_logWebEvent(
                    userName,//团贷网用户名
                    clickDate,//时间
                    "首页底部广告",//事件类别
                    "点击广告",    //事件标签
                    clickLoc, //事件值
                    1,  //事件达成状态，1：成功；0：失败
                    "", // 失败原因类型
                    "", //失败原因
                    "TDW"); //入口站点
            }
        }


