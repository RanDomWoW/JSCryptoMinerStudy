                        image[read_i] = AD_xmlData.getElementsByTagName("IndexImage")[AD_i].firstChild.nodeValue;
                        images[AD_i] = new Image();
                        images[AD_i].src = AD_xmlData.getElementsByTagName("IndexImage")[AD_i].firstChild.nodeValue;
                        read_i++;
                    }

