    <!--create ig script-->
    <script>        
        function crate_ig_div(link) {
            var blockquote = document.createElement("blockquote");
            blockquote.setAttribute("class", "instagram-media");
            blockquote.setAttribute("data-instgrm-captioned", "");
            blockquote.setAttribute("data-instgrm-version", "7");
            blockquote.setAttribute("style", "background:#FFF; border:0; border-radius:3px; box-shadow:0 0 1px 0 rgba(0,0,0,0.5),0 1px 10px 0 rgba(0,0,0,0.15); margin: 1px; max-width:100%; padding:0; width:99.375%; width:-webkit-calc(100% - 2px); width:calc(100% - 2px);");

            var blockquote_div1 = document.createElement("div");
            blockquote_div1.setAttribute("style", "padding:8px;");
            blockquote.appendChild(blockquote_div1);

            var blockquote_div2 = document.createElement("div");
            blockquote_div2.setAttribute("style", "background:#F8F8F8; line-height:0; margin-top:40px; padding:50.0% 0; text-align:center; width:100%;");
            blockquote_div1.appendChild(blockquote_div2);

            var blockquote_div3 = document.createElement("div");
            blockquote_div3.setAttribute("style", "background:url(data:image/png;base64,); display:block; height:44px; margin:0 auto -44px; position:relative; top:-22px; width:44px;");
            blockquote_div2.appendChild(blockquote_div3);

            var blockquote_p1 = document.createElement("p");
            blockquote_p1.setAttribute("style", " margin:8px 0 0 0; padding:0 4px;");
            blockquote_div1.appendChild(blockquote_p1);

            var ig_a = document.createElement("a");
            ig_a.setAttribute("href", link);
            ig_a.setAttribute("style", " color:#000; font-family:Arial,sans-serif; font-size:12px; font-style:normal; font-weight:normal; line-height:17px; text-decoration:none; word-wrap:break-word;");
            ig_a.setAttribute("target", "_blank");
            blockquote_p1.appendChild(ig_a);

            var blockquote_p2 = document.createElement("p");
            blockquote_p2.setAttribute("style", " color:#c9c8cd; font-family:Arial,sans-serif; font-size:14px; line-height:17px; margin-bottom:0; margin-top:8px; overflow:hidden; padding:8px 0 7px; text-align:center; text-overflow:ellipsis; white-space:nowrap;");
            blockquote_div1.appendChild(blockquote_p2);

            var ig_script = document.createElement("script");
            ig_script.setAttribute("async", "");
            ig_script.setAttribute("defer", "");
            ig_script.setAttribute("src", "//platform.instagram.com/en_US/embeds.js");


            try{
                document.getElementById("ig").appendChild(blockquote);
                document.getElementById("ig").appendChild(ig_script);
            }
            catch(e){
                try {
                    document.getElementById("ig").appendChild(blockquote);
                    document.getElementById("ig").appendChild(ig_script);
                }
                catch (e) {
                }
            }
        }
    </script>
