                jQuery("#BtSelectTop").removeClass('Positif');
                jQuery('#pastille').removeClass('bgOrange');
            }
        }

        // Fonction obligatoire lorsqu'on créer un objet Basket. => L'objet Basket lance un event "triggered" lorsqu'il est actualisé.
        function triggered() {
            try {
                if ((bs.xmlhttp.readyState == 4) && (bs.xmlhttp.status == 200)) {
                    bs.unserialize(bs.xmlhttp.responseText);

                    // Mise a jour du nombre de selections dans l'onglet "selection" du Header
                    majBookmarksInHeader(bs);

                    // Fonction bsRefresh a définir dans chaque page qui a un traitement supplémentaire pour les favoris. (ex: MAJ des boutons d'ajout aux favoris...)
                    if(typeof(bsRefresh) === 'function')
                        bsRefresh();
                }
            }
            catch (e) {
            }
        }

        /**
