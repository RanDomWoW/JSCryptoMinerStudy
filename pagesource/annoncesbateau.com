<script>
$(document).ready(function () {
    
    // Gestion des clicks sur les checkbox des categories 
    
    var $catChoiceTab          = $(".catChoice");
    var $catChoiceTabChecked   = $(".catChoice:checked");
    var $catChoiceLabel        = $(".catChoice").next('label');
    
            $(".lienCategorie").click(function(){
                if($(".lienCategorie").next('.js_category').hasClass('hide')) {
                    $("#help_container").addClass("hide");
                    $(".js_category").removeClass("hide");
                } else {
                    $("#help_container").removeClass("hide");
                    $(".js_category").addClass("hide");
                }
            });
            
            $("#allCategories").click(function(){
                $('.lienCategorie').html('Cat&eacute;gorie');
                if($(this).attr("checked")){
                    $(this).next('label').removeClass('txtGrey');
                    $catChoiceTab.removeAttr("checked");
                    $catChoiceLabel.addClass('txtGrey');
                }else{
                    $catChoiceLabel.removeClass('txtGrey');
                }
            });
            
            $(".catChoice").click(function(){
                    $("#allCategories").removeAttr("checked");
                    $("#allCategories").next('label').addClass('txtGrey');
                    $catChoiceLabel.removeClass('txtGrey');
                
                if($catChoiceTabChecked.length() < 1){
                    $("#allCategories").attr("checked","checked");
                    $("#allCategories").next('label').removeClass('txtGrey');
                    $catChoiceLabel.addClass('txtGrey');
                }else{
                    $("#allCategories").removeAttr("checked");
                    $("#allCategories").next('label').addClass('txtGrey');
                    $catChoiceLabel.removeClass('txtGrey');
                }
            });
            
            $("#BRAND_MODEL").click(function(){
                $(".js_category").addClass("hide");
            });
            
            $("#BRAND_MODEL").keyup(function(){
                $(".js_category").addClass("hide");
                $("#help_container").removeClass("hide");
                critob.endEvent(this.value);
            });

            

        function modifyAction(event){
            if($("#searchType option:selected").val() == '2'){
                event.preventDefault();
                // Seule methode qui fonctionne avec IE pour redefinir l'action du formulaire
                if($("#BRAND_MODEL").val() == '' && $("#prix_max").val() == '' && $("#region").val() == ''){
                    $("#formulaire").prop('action', '/location-bateau-moteur/annonces.html');
                }else{
                    $("#formulaire").prop('action', '/recherche_divers.php?&CATEGORIE=divers');
                    $("#CATEGORIE").val('divers');
                    $("#SS_CATEGORIE").val('Bm');
                }
                $("#CategorieZeLink").removeClass("block").hide();
            }else{
                $("#formulaire").prop('action', '/bateau-moteur/listing_bateaux.php');
            }
        }
        
        // Obligatoire pour changer l'action avec IE
        $("#Valider").click(function(event){
            modifyAction(event);
        });
        
        
            $(".catChoice").change(
                function(){
                                    SelectToHidden('','#SS_CATEGORIE');
                                }
            );

// script place ici parce qu'il concerne aussi les categories
            $("#searchType").change(
                function(event){
 
                    modifyAction(event);
                  if($("#searchType option:selected").val() == '1'){
                        SelectToHidden('#searchType','#new');
                    }

                }
            );
            
            function SelectToHidden(fromField,toField){

                if(fromField == ''){
                    var oSelect = $(".catChoice:checked").toArray();
                }else{
                    var oSelect = $(".selectChoice").toArray();
                }

                var aEndVal = new Array();
                var aEndLib = new Array();
                    
                $.each(oSelect, function(){
                                       
                    var sRemoteVal = $(this).val();
                    aEndVal.push(sRemoteVal);

                    if(typeof $(this) != 'select'){
                        var sRemoteLib = $(this).next("label").html();
                        aEndLib.push(sRemoteLib);
                    
                        // array sous forme de string pour utilisation par populateField
                        sEndLib = aEndLib.join(",");
                        
                        populateField(sEndLib);
                    }
                    
                });

                if(oSelect.length < 1){
                    $(".lienCategorie").html('Cat&eacute;gorie');
                }

                $(toField).val(aEndVal);
            }

            // gere le libelle du champs categories
            function populateField(What){
                if(What != ''){
                    $(".lienCategorie").html(What);
                }else{
                    $(".lienCategorie").html('Cat&eacute;gorie');
                }
            }
            
            $("body").click(function(event){
                var targClick = event.target;

                if(!$(targClick).parents('.contCat').find(".js_category").is(':visible') && typeof targClick != 'checkbox'){
                    $(".js_category, #help_container").addClass("hide"); 
                }
            });
            
}); 
