<script>

    
        // Tracking de comptabilisation des internautes utilisant un adBlock - Limiter à la Homepage Auto pour l'instant
        $(document).on('appnexusLoaded', function(event, data){
            var imgTracking = new Image();
            imgTracking.src = 'http://logi11.xiti.com/hit.xiti?s=256335&s2=18&p=detection_pub::' + (data.isLoaded ? '' : 'non_') + 'chargee&clic=A&timestamp=' + new Date().getTime();
        });

    
            // On lance le redimentionnement automatique de la pub lors des redimensionnement de la fenêtre du navigateur
        throwDrawResponsiveBanner();
    
    /**
     * drawResponsiveBanner : Gestion du redimensionnement automatique de la pub "BANDEAU" quand la taille de l'écran change
     * SUPPORT Tablette ou Desktop uniquement
     * => Pour rappel, sur le support mobile, un test cote serveur nous affiche une banniere speciale mobile.
     */
    function drawResponsiveBanner(){

        var bannerWidth     = 728,
            $banner       = $('.bannerContainer');

        if($banner.size()) {
            var scaleBanner = $(window).width() / bannerWidth;

            if (scaleBanner <= 1) {
                $banner.css({
                    transform: 'scale(' + scaleBanner + ')',
                    transformOrigin: 'left top 0',
                    whiteSpace: 'nowrap'
                });
            } else {
                $banner.css({
                    transform: 'scale(1)',
                    transformOrigin: 'left top 0',
                    whiteSpace: 'normal'
                });
            }
        }

    }

    /**
     * throwDrawResponsiveBanner : Appel de la fonction de redimensionnement auto de la pub BANDEAU à l'ouverture et au redimensionnement de la page
     */
    function throwDrawResponsiveBanner(){

        $(document).ready(function(){
            var nbPass                      = 0,
                delayToRefresh              = 200,
                maxPass                     = 10000 / delayToRefresh, // Duree max du setInterval 10 secondes (protection contre boucles infinies)
                responsiveBannerInterval    = setInterval(function(){

                    // On teste si la bannière est chargée ou si ça fait deja plus de 10 secondes qu'on attend
                    if($('.bannerContainer').size() || nbPass > maxPass) {
                        drawResponsiveBanner();
                        clearInterval(responsiveBannerInterval);
                    }
                    nbPass++;

                }, delayToRefresh);

            drawResponsiveBanner();
        });

        $(window).on('resize', function(){
            drawResponsiveBanner();
        });

    }

</script>

