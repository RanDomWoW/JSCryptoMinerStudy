<script type="text/javascript">
$(function() {
	
//menu
var headerH = $("#header").height();
var secMenu = $('#header_top');
var menuSpeed = "easeOutQuad";
$(window).scroll(function () {
	if ($(this).scrollTop() > headerH) {
		secMenu.stop().animate({'top' : '0px'}, menuSpeed); 
	}else{
		secMenu.stop().animate({'top' : '-150px'}, menuSpeed); 
	}
});
$(window).resize(function(){
	headerH = $("#header").height();
});
	
	
//YouTube
$('document').ready(function() {
	var options = { videoId: 'gZItyr1SNjU'};
	$('#wrap').tubular(options);
});

//sound
$("#s_btn").click(function(){
	if($(this).attr("class")=="off_btn"){
		$(this).removeClass("off_btn").addClass("on_btn");
	} else {
		$(this).removeClass("on_btn").addClass("off_btn");
	}
});



});
</script>

<script type="text/javascript">
$(function() {
    var topBtn = $('.slide_btn');
    $(window).scroll(function () {
        if ($(this).scrollTop() > 50) {
            topBtn.fadeOut();
        } else {
            topBtn.fadeIn();
        }
    });
	
	
});
</script>

