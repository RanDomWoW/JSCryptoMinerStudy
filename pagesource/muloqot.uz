<script type="text/javascript">
window.addEvent('domready', function () {
	if (typeof('document.body.scrollTop') != 'undefined') {
		var panel = new Fx.Tween($('top_panel'), {
			duration: 'normal',
			property: 'top',
			unit: 'px'
		});
		
		var header = new Fx.Tween($('global_header'), {
			duration: 'normal',
			property: 'margin-top',
			unit: 'px'
		});

		var active = true;
		
		window.addEvent('scroll', function (event) {
			if (!active && document.body.scrollTop > 80) return;
			if (document.body.scrollTop > 45) {
				panel.cancel();
				panel.start(-41);

				header.cancel();
				header.start(0);
				
				active = false;
			}
			else {
				header.cancel();
				header.start(40);
				
				panel.cancel();
				panel.start(0);

				active = true;
			}
		});
	}
});
isPage = false;
</script>




