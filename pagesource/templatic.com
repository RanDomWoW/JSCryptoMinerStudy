<script>

jQuery('.close').click( function(){
    jQuery('#myModal').removeClass('in');
    jQuery('#myModal').css('display', 'block');
    jQuery('#myModal').css('padding-right', '17px');
    jQuery("body").removeClass("modal-open");
    jQuery("body").css('','');
});
jQuery('#userlike-offline-message-submit').click(function() {

        var name = jQuery('#userlike-offline-name').val();
        var email = jQuery('#userlike-offline-email').val();
        var message = jQuery('#userlike-offline-textarea').val();
        var filter = /^[\w\-\.\+]+\@[a-zA-Z0-9\.\-]+\.[a-zA-z0-9]{2,4}$/;
        if(name!= '' && filter.test(email) && email != '' && message != '')
        {
            jQuery.ajax({url: "//templatic.com/ajax.php?name="+name+"&email="+email+"&message="+message,
            type:'POST',
            success: function(result){
                jQuery('#userlike-message').show();
                jQuery('#userlike-offline-name').val('');
                jQuery('#userlike-offline-email').val('');
                jQuery('#userlike-offline-textarea').val('');
            }});
        }
        //jQuery('#userlike-offline-message').hide();
    });
</script>


