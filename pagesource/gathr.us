<script>
$(function(){

	var initialUser = $.gathr.currentUser.get();
	$.gathr.register.setLoginAction($('.login-tooltip-form'));
	$.gathr.currentUser.watch(drawLoggedInElements);

	function drawLoggedInElements(){
		var user = $.gathr.currentUser.get();
		$('.nav #register-menu-item').remove();
		$('.nav #register-menu-item-separator').remove();
		$('.nav #login-menu-item').remove();
		$('.nav #fb-menu-item').remove();
		$('.nav #dashboard-menu-item').remove();
		$('.nav #logout-menu-item').remove();

		if(user.isLoggedIn || !!user.id){

			$('<li>', { id: 'dashboard-menu-item' }).append($('<a>', { href: '/dashboard', 'class': 'username'}).text('DASHBOARD')).appendTo($('.gathr-navbar ul.nav'));
			$('<li>', { id: 'logout-menu-item' }).append($('<a>', { href: "/login/logout?referer=&redirect_url="	}).text('LOGOUT')).appendTo($('.gathr-navbar ul.nav'));
		}else{
			$('#no-registered-navigation-template').children().clone(true, true).appendTo('.nav');
		}
		$('.gathr-nav').collapsableMenu('reRender');
		$(window).trigger('resize');
	}


	$('.dropdown a:not(.ico-facebook)').click(function(e) {
		e.preventDefault();
		setTimeout($.proxy(function() {
			if ('ontouchstart' in document.documentElement) {
				$(this).siblings('.dropdown-backdrop').off().remove();
			}
		}, this), 0);
	});

	
});


	var windowWidth = 0;
	$(window).load(function(){
		windowWidth = $(window).width();
	});
	$('document').ready(function(){
		$('.gathr-nav').collapsableMenu({wrapper: $('.gathr-navbar'), more_text: "MORE"});
		$.gathr.widgets.setOverlay();
	});
	</script>
