    const VP_CHANGE_MARKET = '/change_market';

    function showLangPopoverIfNeeded() {
        var locales          = ['es', 'en', 'fr', 'de', 'pt', 'it'];
        var acceptLanguage   = 'en';
        var browserLocale    = (locales.includes(acceptLanguage)) ? acceptLanguage : 'en';
        var appLocale        = 'es';
        var langPopover      = $("[data-toggle=lang-popover]");
        var langOverlay      = $('div.language-popover-overlay');

        var closePopover = function() {
            var dateExpire = ($.cookie('cc_cookie_accept')) ? 1 / 24 / 2 : 0;
            $.cookie('language_change_decline', 'language_change_decline', { expires: dateExpire, path: '/' });
            langOverlay.removeClass("on");
            langPopover.popover('hide');
        };

        if (langPopover.length !== 0 && browserLocale !== appLocale && !$.cookie('language_change_decline') && $.cookie('cc_cookie_accept')) {
            var options  = {
                placement: 'bottom',
                html: true
            };

            langPopover.popover(options);
            langPopover.popover('show')
                .data('bs.popover')
                .tip()
                .addClass('language-change-popover')
                .removeAttr('style')
            ;

            langPopover.on('click', closePopover);
            langOverlay.addClass("on");
            $(".close-lang-popover").on('click', closePopover);

            ga('send', {
                hitType: 'pageview',
                page: VP_CHANGE_MARKET
            });
        }
    }

    $('.cc-cookie-accept').on('click', function (e) {
        showLangPopoverIfNeeded();
    });

    showLangPopoverIfNeeded();
