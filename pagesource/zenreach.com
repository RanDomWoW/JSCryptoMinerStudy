<div class="lead-form">
  <div class="wrap">
    <div class="intro">
      <div class="intro-wrap">
        
          <div class="h1">Let's get started</div>
          
            <h3>It's risk-free.</h3>
            <p>Once you've submitted your info, we will reach out to you with a few questions about your business and goals.</p>
          
        
      </div>
    </div>
    <div class="form form-with-slider-labels">
      <!--[if lte IE 8]>
      <script charset="utf-8" type="text/javascript" src="//js.hsforms.net/forms/v2-legacy.js"></script>
      <![endif]-->
      <script charset="utf-8" type="text/javascript" src="//js.hsforms.net/forms/v2.js"></script>
      <script>
        $(function(){

          var checkFocus = function() {
            $(".form-with-slider-labels .hs-input").each(function() {
              var el = $(this);
              if (el.is('textarea')) {
                return false;
              }
              if ($(this).val()) {
                $(this).closest(".hs-form-field").addClass("has-value");
                $(this).closest(".hs-form-field").addClass("has-focus");
              }
            });
          };

          var zenreachCreateForm = function() {
            hbspt.forms.create({
              portalId: '576279',
              formId:   'afcdf206-ba18-4de3-b263-9c4c951300bb',
              css:      '',
              onFormSubmit: function($form) {
                fbq('track', 'Lead');
              },
              onFormReady: function() {
                var doc = document.documentElement;
                doc.setAttribute('data-useragent', navigator.userAgent);
                $('form').attr('autocomplete','off');
                $('input').attr('autocomplete','off');
                $('html[data-useragent*="Chrome"] form').attr('autocomplete','false');
                $('html[data-useragent*="Chrome"] form input').attr('autocomplete','false');

                $(".form-with-slider-labels .hs-input").focus(function(){
                  var el = $(this);
                  if (el.is('textarea')) {
                    return false;
                  }
                  el.closest(".hs-form-field").removeClass("not-focused");
                  el.closest(".hs-form-field").addClass("has-focus");
                }).blur(function(){
                  var el = $(this);
                  if (el.is('textarea')) {
                    return false;
                  }
                  if (!el.val()) {
                    el.closest(".hs-form-field").removeClass("has-focus");
                    el.closest(".hs-form-field").addClass("not-focused");
                  } else {
                    el.closest(".hs-form-field").addClass("has-value");
                  }
                });

                $(".form-with-slider-labels .hs-input").on("change paste keyup", function() {
                  var el = $(this);
                  if (el.is('textarea')) {
                    return false;
                  }
                  if (!$(this).val()) {
                    $(this).closest(".hs-form-field").removeClass("has-value");
                  }
                });

                checkFocus();
              }
            });
          };
          if (location.pathname == "/signup/" || location.pathname == "/dealersignup/" || location.pathname == "/blackfriday/") {
            zenreachCreateForm();
          }
          $(window).on('zenreachCreateForm', function(){
            zenreachCreateForm();
          });
        });
      </script>
    </div>
  </div>
