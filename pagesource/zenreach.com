<script>
  $(function() {
    var resizeOutlines = function() {
      $('.content-block-outlined').each(function(){
        var el      = $(this);
        var outline = el.find('.outline');
        var padding = 20;
        if (zenreach.breakpoint == "desktop-wide") {
          padding = 68;
        }
        if (zenreach.breakpoint == "mobile") {
          outline.css('height', el.find('h2').outerHeight() + el.find('.img.mobile').outerHeight() - (padding * 2))
          el.find('.text-wrap').css('height', 'auto');
        } else {
          outline.css('height', el.find('.img.desktop').outerHeight() - (padding * 2))
          el.find('.text-wrap').css('height', el.find('.img.desktop').outerHeight());
        }
      });
    };
    resizeOutlines();
    $(window).on('resize', function(){
      resizeOutlines();
    });
    $(window).on('load', function(){
      resizeOutlines();
    });
  });
</script>

    
