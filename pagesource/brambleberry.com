		// Fixed menu, logo resize code
		var scrollTop = 0,
			currentLogoHeight = 112,
			logoElement = $('.storeLogo img');
		var checkScrollPosition = function() {
			var headerHeight = $('#header').height();
			window.requestAnimationFrame(function() {
