    function openVideoPopup() {
        var $that = $(this);
        var videoPopup = $('#videoPopup');
        var videoLink = $that.data('src');
        var videoTitle = $that.data('title');

        if (videoPopup.length) {
            videoPopup = $(videoPopup[0]);
            var videoContainer = videoPopup.find('.video-container');

            videoPopup.find('.modal-title').text(videoTitle);

            if (_validateValidUrl(videoLink)) {    
                videoLink = videoLink.replace('watch?v=', 'embed/');
                var videoPlayer = '<iframe width="100%" height="500" frameborder="0" allowFullScreen class="video-player" src="' + videoLink + '?autoplay=true"></iframe > ';
                videoContainer.html(videoPlayer);
            } else {
                videoContainer.append('<p>We are sorry, the video is currently not available. Please check back later.</p>');
            }

            videoPopup.modal('show');

            videoPopup.on('hide.bs.modal', function () {
                videoContainer.html('');
            });
        }
    } 

    function _validateValidUrl(url) {
        if (!url || typeof (url) !== 'string') { return false; }

        return url.indexOf('youtube.com') > -1;
    }
</script>
