<script type="text/javascript">
    (function ($) {
        'use strict';

        // listrak callback function to render listrak item correctly based on their number
        window.listrakCallback = function (recommendations, callback) {
            if (!recommendations) return;

            var UId = recommendations.MerchandiseBlockUID;

            if (!UId) {
                console.error('No recommendations');
                return;
            }

            var length = 0 || recommendations.length;
            var $block = $('[data-ltk-merchandiseblock="' + UId + '"]');
            var $inner = $block.find('.listrak-block-track');
            var $itemTemplate = $block.find('.js-listrak-item-template');
            var templateString = $itemTemplate.text();

            if (!length) {
                // if there is no recommendation found, hide the listrak block
                $block.css({
                    'display': none
                });
            } else {
                var slider = new ListrakSlider($inner, length);
                var grid = slider.calcGrid(length);
                var width = slider.calcWidth($block, grid);
                templateString = templateString.replace(/{Width}/g, width + 'px');
                templateString = templateString.replace(/{Height}/g, width - 30 + 'px');
                $itemTemplate.text(templateString);

                slider.init();

                // update recommendations price to 2 digit price format
                recommendations.forEach(function (recommendation) {
                    if (recommendation.Price) {
                        recommendation.Price = Number(recommendation.Price).toFixed(2);
                    }
                });

                callback(recommendations);
            }
        }

        function ListrakSlider(element, totalItems) {
            this.element = element;
            this.totalItems = totalItems;
            this.itemWidth = 0;
            this.current = 0;
            this.slides = 0;
            this.slideWidth = 0;
            this.offset = 0;
            this.maxOffset = 0;
            this.grid = 0;
            this.indicators = [];
            this.nextBtn = $('<button class="btn next-btn" aria-hidden="true"><i class="arrow-left"></i><span class="sr-only">next</span></button>');
            this.prevBtn = $('<button class="btn prev-btn" aria-hidden="true"><i class="arrow-right"></i><span class="sr-only">prev</span></button>');
        }

        ListrakSlider.prototype.init = function () {
            if (!this.element) return;

            this.element.wrap('<div class="listrak-block-inner"></div>');
            this.update();
            this.initNavButtons();
            this.initIndicators();

            this.onResize();
            this.onTouch();
        }

        ListrakSlider.prototype.update = function () {
            this.grid = this.calcGrid(this.totalItems);
            this.slides = Math.ceil(this.totalItems / this.grid);
            this.itemWidth = this.calcWidth(this.element.parent(), this.grid);
            this.slideWidth = this.grid * this.itemWidth;
            this.maxOffset = this.calcMaxOffset();

            this.element.css({ width: this.itemWidth * this.totalItems + 'px' });
        }

        ListrakSlider.prototype.calcMaxOffset = function () {
            var modula = this.totalItems % this.grid;
            var panes = Math.max(0, this.slides - (modula ? 2 : 1));

            return this.itemWidth * (panes * this.grid + modula);
        }

        ListrakSlider.prototype.calcGrid = function (length) {
            var vw = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
            var grid = length;
            var index = 1;

            while (grid > 6) {
                index += 1;
                grid = Math.ceil(length / index);
            }

            if (vw < 480) {
                return 1;
            } else if (vw < 768) {
                return Math.min(2, grid);
            } else if (vw < 1024) {
                return Math.min(grid, 4);
            } else {
                return grid;
            }
        }

        ListrakSlider.prototype.calcWidth = function (container, grid) {
            if (!container || grid < 1) return null;

            return (container.width() + 30) / grid;
        }

        ListrakSlider.prototype.next = function (event) {
            if (event) {
                event.preventDefault();
            }

            if (this.current < this.slides - 1) {
                this.current += 1;
                this.slide(event, 'next');
            }
        }

        ListrakSlider.prototype.prev = function (event) {
            if (event) {
                event.preventDefault();
            }

            if (this.current > 0) {
                this.current -= 1;
                this.slide(event, 'prev');
            }
        }


        ListrakSlider.prototype.slide = function (event, direction) {
            if (event) {
                event.preventDefault();
            }

            // ensure that the current index will never go below 0 and above slides number
            this.current = Math.max(Math.min(this.current, this.slides - 1), 0);

            if (direction === 'prev') {
                this.offset = this.offset - this.slideWidth;
            } else if (direction === 'next') {
                this.offset = this.offset + this.slideWidth;
            } else {
                this.offset = this.current * this.slideWidth;
            }

            this.offset = this.calOffset(this.offset, 0);

            this.element.css({
                transform: 'translateX(' + (0 - this.offset) + 'px)'
            });

            this.updateBtnState();
            this.updateIndicators();
        }

        ListrakSlider.prototype.calOffset = function (origin, offset) {
            var result = Math.max(0, offset + origin);

            return Math.min(result, this.maxOffset);
        }

        ListrakSlider.prototype.initNavButtons = function () {
            var that = this;

            this.prevBtn.css({ height: this.itemWidth - 30 + 'px' });
            this.nextBtn.css({ height: this.itemWidth - 30 + 'px' });

            this.prevBtn.on('click', function (event) {
                that.prev(event);
            });

            this.nextBtn.on('click', function (event) {
                that.next(event);
            });

            this.element.parent().before(this.prevBtn);
            this.element.parent().after(this.nextBtn);

            this.updateBtnState();
        }

        ListrakSlider.prototype.updateBtnState = function () {
            if (this.current === 0) {
                this.prevBtn.addClass('disabled');
            } else {
                this.prevBtn.removeClass('disabled');
            }

            if (this.current === this.slides - 1) {
                this.nextBtn.addClass('disabled');
            } else {
                this.nextBtn.removeClass('disabled');
            }
        }

        ListrakSlider.prototype.initIndicators = function () {
            var that = this;
            var $indicators = this.$indicators ? this.$indicators : $('<ol class="slide-indicators"></ol>');

            if (!this.$indicators) {
                this.element.parent().after($indicators);
                this.$indicators = $indicators;
            }

            if (this.slides > this.indicators.length) {
                for (var i = this.indicators.length; i < this.slides; i++) {
                    var indicatorTemplate = '<li class="slide-indicator"><a role="button">' + i + '</a></li>';
                    var $indicator = $(indicatorTemplate);

                    $indicator.data('slide-to', i);
                    this.indicators.push($indicator);
                    $indicators.append($indicator);

                    $indicator.on('click', 'a', function (event) {
                        that.current = $(event.target).parent().data('slide-to');
                        that.slide(event);
                    });
                }
            } else {
                this.indicators = this.indicators.filter(function (indicator, index) {
                    if (index >= that.slides) {
                        indicator.remove();
                        return false;
                    }

                    return true;
                });
            }

            if (this.slides > 1) {
                $indicators.css({
                    display: 'block'
                });
            } else {
                $indicators.css({
                    display: 'none'
                });
            }
            
            this.updateIndicators();
        }

        ListrakSlider.prototype.updateIndicators = function () {
            var that = this;
            this.indicators.forEach(function (indicator, index) {
                if (index === that.current) {
                    indicator.addClass('active');
                } else {
                    indicator.removeClass('active');
                }
            });
        }

        ListrakSlider.prototype.onResize = function () {
            var that = this;

            window.addEventListener('resize', function () {
                that.offset = 0;
                that.current = 0;

                that.update();

                // Find items if it does not exist
                var items = that.element.find('li');
                items.css({ 'width': that.itemWidth + 'px' });

                that.slide();
                that.initIndicators();
            });
        }

        ListrakSlider.prototype.onTouch = function () {
            var that = this;
            var deltaX = 0;
            var hm = new Hammer.Manager(that.element[0]);
            hm.add(new Hammer.Pan({ threshold: 10, pointers: 0 }));

            hm.on('pan', function (e) {
                e.preventDefault();

                deltaX = e.deltaX;

                var offset = that.calOffset(that.offset, e.deltaX * -1);

                that.element.css({
                    transform: 'translateX(' + (0 - offset) + 'px)'
                });
            });

            hm.on('panend pancancel', function (e) {
                e.preventDefault();

                var moved = 0;

                // if touch moves more than 100px,
                // we will change the current slide
                if (Math.abs(e.deltaX) > 100) {
                    moved = Math.abs(e.deltaX) / that.slideWidth;

                    // if it is moved less than a slide, move entire slide
                    moved = moved > 1 ? Math.floor(moved) : Math.ceil(moved); 

                    that.current = e.deltaX < 0 ? that.current + moved : that.current - moved;
                }

                deltaX = 0;
                that.slide(e);
            });

            this.element.on('mouseleave', function (e) {
                var moved = 0;

                if (!deltaX) { return; }

                // if touch moves more than 100px,
                // we will change the current slide
                if (Math.abs(deltaX) > 100) {
                    moved = Math.abs(deltaX) / that.slideWidth;

                    // if it is moved less than a slide, move entire slide
                    moved = moved > 1 ? Math.floor(moved) : Math.ceil(moved);

                    that.current = deltaX < 0 ? that.current + moved : that.current - moved;
                }

                that.slide(e);
            });
        }
    })(jQuery);
</script>
