				if (scrollTop == 0 && currentLogoHeight != 112) {
					currentLogoHeight = 112;
					logoElement.css('height', currentLogoHeight + 'px');
				}
				if (scrollTop > 0 && currentLogoHeight != 63) {
					currentLogoHeight = headerHeight || 68;
					logoElement.css('height', currentLogoHeight + 'px');
				}
			});
		}
		if (logoElement && $(window).innerWidth() > 1025) {
			logoElement.css('height', currentLogoHeight + 'px');
			var scrollInterval = setInterval(checkScrollPosition, 10);
		}
		// End Fixed Menu
	});
</script>
