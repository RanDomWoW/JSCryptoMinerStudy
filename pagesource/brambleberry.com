<script type="text/javascript">
    $(function ()
    {
        var defaultWidth = "";
        $("#hiddenwidth").val($(".searchControl input[type='text']").width());
        defaultWidth = $("#hiddenwidth").val();
        $('.searchControl > select:first-child').width(39);
        $(".searchControl > select:first-child").val("0");
        //$(".searchPhrase").autocomplete({
        $('[id$=SearchPhrase]').autocomplete({
            source: function (request, response)
            {
                $.ajax({
                    url: "/Search.aspx/Suggest",
                    data: "{ 'term': '" + request.term + "' }",
                    dataType: "json",
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    dataFilter: function (data) { return data; },
                    success: function (data)
                    {
                        response($.map(data.d, function (item)
                        {
                            return {
                                value: item
                            }
                        }))
                    }
                });
            },
            minLength: 2
        });

        $('.searchControl > input').each(function ()
        {
            $.data(this, 'default', this.value);
        }).css("color", "#d1d4d4")
    .focus(function ()
    {
        if (!$.data(this, 'edited'))
        {
            this.value = "";
            $(this).css("color", "#6E234F");
        }
    }).change(function ()
    {
        $.data(this, 'edited', this.value != "");
    }).blur(function ()
    {
        if (!$.data(this, 'edited'))
        {
            //this.value = $.data(this, '');
            $(this).css("color", "#d1d4d4");
        }
    });

        $('.searchControl > select:first-child').change(function ()
        {
            $(this).next().next().width(defaultWidth);
            $("#width_tmp").html("");
            $("#width_tmp").html($(this).children('option:selected').text());
            $(this).width($("#width_tmp").width() + 25); // 30 : the size of the down arrow of the select box
            var newWidth = $(this).next().next().width() - $("#width_tmp").width();
            $(this).next().next().width(newWidth + 25);
        });

    });
</script>
