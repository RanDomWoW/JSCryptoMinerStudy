<script type="text/javascript">
  var cartStatus = {
    curValue: 0,
    levels: [ {min: '0.0100', max: '125.0000', quantity: '1'}, {min: '125.0100', max: '249.9900', quantity: '2'}, {min: '250.0000', max: '0.0000', quantity: '3'} ]
  };

  // TODO: move this to common.js
  function openShoppingCart(priceTotal, itemCount, newItems) {
    // if there is missing veriables, we are not going to show the cart
    if (!itemCount || !priceTotal || !newItems) { return; }

    if (itemCount) {
      // update the current cart total
      cartStatus.curValue = priceTotal;
    }
    // update free gift status on footer
    getFreeGiftStatus();
    _updateCartSummary(priceTotal, itemCount);

    _updateAddedProductInfo(newItems);
    var shoppingcart = $('#bramShoppingCart');
    shoppingcart.modal('show');
  }

  function closeShoppingCart() {
    var shoppingcart = $('#bramShoppingCart');
    shoppingcart.modal('hide');
  }

  function getFreeGiftStatus() {
    var textHolder = $('#freeGiftStatus');
    var levels = cartStatus.levels;

    if (levels.length) {
      var nextGiftLevel = _getNextGiftLevel();
      var giftStatus;

      if (nextGiftLevel) {
        giftStatus = _getGiftStatusText(nextGiftLevel, false);
      } else {
        giftStatus = _getGiftStatusText(levels[levels.length - 1], true);
      }

      textHolder.text(giftStatus);
    } else {
      textHolder.text('');
    }
  }

  function _updateCartSummary(cartTotal, itemCount, newItems) {
    $('#cartTotal').text('$' + Number(cartTotal).toFixed(2));
    $('#itemCountDisplay').text(itemCount > 1 ? itemCount + ' Items' : itemCount + ' Item');
    $('#itemCount').text(itemCount);
  }

  function _getNextGiftLevel() {
    var levels = cartStatus.levels;
    var level = levels.find(function(level) {
      return cartStatus.curValue < level.min;
    });

    return level;
  }

  function _getGiftStatusText(level, isMax) {
      var text = ['One', 'Two', 'Three', 'Four', 'Five', 'Six', 'Seven', 'Eight', 'Nine', 'Ten'];
      var numberInText = text[level.quantity - 1] ? text[level.quantity - 1] : level.quantity; // if more then ten, just number instead

    if (!numberInText) {
        return '';
    }

    if (isMax) {
      return 'Congratulations, you\'ve earned ' + numberInText + ' Free Gifts!';
    } else {
      return 'You\'re $' + Number(level.min - cartStatus.curValue).toFixed(2) + ' away from ' + numberInText + ' Free Gifts.';
    }
  }

  function _updateAddedProductInfo(productInfo) {
    if (productInfo) {
      $('.js-product-img').attr('src', productInfo.imageUrl);
      $('.js-product-price').html('<b>$' + Number(productInfo.price).toFixed(2) +'</b>');
      $('.js-product-link').attr('href', productInfo.navigationUrl).text(productInfo.name);
      $('.js-product-quantity').html('Quantity: ' + (productInfo.quantity ? productInfo.quantity : 1));

      if (productInfo.items && productInfo.items.length) {
          productInfo.items.forEach(function (item) {
              $('.js-product-items').append('<p>' + item + '</p>');
         });
      } else {
        $('.js-product-items').empty();  
      }
    }
  }
</script>
