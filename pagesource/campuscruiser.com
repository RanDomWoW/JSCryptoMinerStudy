            jQuery(whichElement).width('auto');
        }
}


function autoHeight (whichElement) {

    jQuery(whichElement).each(function() {        
        jQuery(this).height('auto');
  //      console.log($(this))
    });

}


function equalHeights() {
    console.log(window.innerWidth);
    
    setTimeout(function() {
    
        if (window.innerWidth >= 991) {

            
            if ($('.row').length > 0) {
                $('.row').each(function() {
                
                    matches = $('#introduction .orange-features .wpb_column');
                    
                   // console.log(" test " + matches);
                    autoHeight(matches);
                    resizeMe(matches);
                });
            }
        }
        else {

            if ($('.row').length > 0) {
                $('.row').each(function() {
                    matches = $('#introduction .orange-features .wpb_column');
                    
                    
                    autoHeight(matches);
                    resizeMe(matches);
                });
            }
        }
    }, 1000); // wait 1 second
    
}
$= jQuery;

jQuery(document).ready(function ($) {
    equalHeights(); });


jQuery(window).resize(function($) {
    equalHeights();
});

</script>

