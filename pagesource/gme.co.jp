$(window).load(function(){
	function updateSlideCount(current_, total_) {
		var dotClear = "<span class='dot-clear'>●</span>",
			dotFull = "<span class='dot-full'>●</span>";

		var dots = [];
		for(var position=1; position<=total_; ++position) {
			if(position==current_) {
				dots.push(dotFull);
			}
			else {
				dots.push(dotClear);
			}
		}
		$('#slides-dots').html(dots.join(" "));
	}
	$('.flexslider').flexslider({
		animation: "slide",
		pauseOnAction: true,
		controlNav: "thumbnails",
		//start: function(slider){
		//  $('body').removeClass('loading');
		//}
		start: function(slider) {
			//$('#total-slides').text(slider.count);
			updateSlideCount(1, slider.count);
		},
		after: function(slider) {
			//$('#current-slide').text(slider.currentSlide+1);
			updateSlideCount(slider.currentSlide+1, slider.count);
		}
	});
});
