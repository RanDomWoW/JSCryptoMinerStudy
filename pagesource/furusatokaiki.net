			}
		});
	});

	function createDateStr(date) {
		var year = date.getYear();
		var month = date.getMonth() + 1;
		var day = date.getDate();
		if (year < 2000) { year += 1900 };
		if (month < 10)  { month = "0" + month };
		if (day < 10)    { day = "0" + day };
		var dateStr = year +"/"+ month +"/"+ day;
		return dateStr;
	}
