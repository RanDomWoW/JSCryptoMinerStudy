<script language="javascript">
function loginSubmit() {
    var use_ssl = jQuery('#securityLoin').val();

    if(jQuery('#userid').val() == '') {
            alert('아이디를 입력해주세요.');
            return ;
    } else if(jQuery('#userpw').val() == '') {
            alert('패스워드를 입력해주세요.');
            return ;
    } else if(jQuery('#val_webs').val() != '') {
            jQuery('#login_form').submit();
            return ;
    } else {
        if(use_ssl == 1) {
            $.ajax({
                type: "POST",
                url: "https://ssl.sharebox.co.kr/login/",
                crossDomain: true,
                xhrFields: {
                    withCredentials: true
                },
                data: $('form[name=login_form]').serialize(),
                cache : false,
                dataType :'json',
                success : function(res)
                {
                    return LoginSuccessCallback(res);
                },
                error : function(request,status,error)
                {
                    //alert('해당 네트워크는 보안로그인을 할 수 없는 환경입니다.\n일반로그인으로 접속하였습니다.');

                    jQuery('#login_form').prop('action', '/login/');
                    jQuery('#login_form').submit();
                    return 0;
                }
            });
        } else {
            jQuery('#login_form').prop('action', '/login/');
            jQuery('#login_form').submit();
            return 0;
        }
    }
}

function LoginSuccessCallback(res) {
    var status = res.status;
    var msg = res.msg;
    var login_event = res.login_event;
    var return_url = res.return_url;
    var caller = res.caller;

    if(msg) alert(msg);
    if(login_event) alert(login_event);

    switch(caller) {
        case 'view' :
            try {
                parent.opener.location.reload(); //뷰창호출부모창
                parent.location.reload(); //뷰창
            } catch (e) {
                alert(e.message);
                parent.location.reload(); //뷰창
            }
            break;
        case 'page' :
        case 'main' :
        default :
            try {
                if(return_url) {
                    parent.location.href = return_url;
                } else {
                    if(status != 'Alert') parent.location.reload();
                }
            } catch (e) {
                alert(e.message);
                parent.location.reload();
            }
            break;
    }
}

function loginPasswdDown(key) {
    if(key == 13) {
        loginSubmit();
    }
}
</script>
