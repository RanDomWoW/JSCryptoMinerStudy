function get_info_of_IE () { 

        var word; 
        var agent = navigator.userAgent.toLowerCase(); 

        var info = {  name: "N/A" , version: -1  }; 

        if ( navigator.appName == "Microsoft Internet Explorer" ){    // IE old version ( IE 10 or Lower ) 
                word = "msie "; 
        } 
        else if ( agent.search( "trident" ) > -1 ) word = "trident/.*rv:";    // IE 11 
        else if ( agent.search( "edge/" ) > -1  ) word = "edge/";        // Microsoft Edge 
        else  { 
                return info;    // etc. ( If it's not IE or Edge ) 
        } 


        var reg = new RegExp( word + "([0-9]{1,})(\\.{0,}[0-9]{0,1})" ); 

        if (  reg.exec( agent ) != null  ){ 

                info.version = parseFloat( RegExp.$1 + RegExp.$2 ); 

                info.name = ( word == "edge/" ) ? "Edge" : "IE"; 
        } 

        return info; 
} 


function mfBannerChange() {
	// 이미지 태그부분
	// mfBanners[mfBannerIndex][0] : 이동되는 페이지
	// mfBanners[mfBannerIndex][1] : 이미지 경로
	// mfBanners[mfBannerIndex][2] :  z = 새탭, p 또는 l 또는 x 또는 기타 문자열은 정해진 크기의 팝업 형태로 오픈
	var htmlString;

	if(mfBanners[mfBannerIndex][2] == 'x') {
		htmlString = '<a HREF="#" onclick="window.open(\''+mfBanners[mfBannerIndex][0]+'\',\'\',\'toolbar=no,location=no,status=no,menubar=no,scrollbars=yes,resizable=no,width=920,height=1100\');"><img src="'+mfBanners[mfBannerIndex][1]+'" width="460" height="120" border="0"></a>';
	} else if(mfBanners[mfBannerIndex][2] == 'p') {
		htmlString = '<a HREF="#" onclick="window.open(\''+mfBanners[mfBannerIndex][0]+'\',\'\',\'toolbar=no,location=no,status=no,menubar=no,scrollbars=yes,resizable=no,width=740,height=700\');"><img src="'+mfBanners[mfBannerIndex][1]+'" width="460" height="120" border="0"></a>';
	} else if(mfBanners[mfBannerIndex][2] == 'z') {
		htmlString = '<a HREF="' + mfBanners[mfBannerIndex][0] + '" target="_blank" > <img src="'+mfBanners[mfBannerIndex][1]+'" width="460" height="120" border="0"></a>';
	} else {
		//htmlString = '<a href="'+mfBanners[mfBannerIndex][0]+'" target="_blank"><img src="'+mfBanners[mfBannerIndex][1]+'" width="400" height="93" border="0" style="border:solid 1px #c8c8c8"></a>';
//		htmlString = '<a HREF="#" onclick="window.open(\''+mfBanners[mfBannerIndex][0]+'\',\'\',\'toolbar=no,location=no,status=no,menubar=no,scrollbars=yes,resizable=no,width=800,height=800\');"><img src="'+mfBanners[mfBannerIndex][1]+'" width="400" height="93" border="0"></a>';
		
		htmlString = '<a href="'+mfBanners[mfBannerIndex][0]+'", target="_blank"><img src="'+mfBanners[mfBannerIndex][1]+'" width="460" height="120" border="0"></a>';
	}
	
	if( mfIe) {
		document.all.banner.innerHTML = htmlString;
	}else {
		document.getElementById('banner').innerHTML = htmlString;
	}
	if(mfBannerIndex < mfBanners.length - 1)
		mfBannerIndex++;
	else
		mfBannerIndex = 0;
}
