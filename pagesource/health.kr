//안전성시한
function getInfo() {
	$.ajax({
		type:"GET",
		url:"http://localapi.health.kr:8090/RsInfo.localapi?",
		dataType:"JSONP",
		success:function(data){
			$("#RtInfo").empty();
				var val = 0;
				var res = data.length;
				var dates, datesmain;

				var htmlData = "";
				while (val < res)
				{	
