//PharmReview
function getPharmReview() {
	$.ajax({
		type:"GET",
		url:"http://localapi.health.kr:8090/RsReview.localapi?",
		dataType:"JSONP",
		success:function(data){
			$("#RtReview").empty();
				var val = 0;
				var res = data.length;
				
				var htmlData = "";
				while (val < res)
				{	
					htmlData = htmlData + "<li><a style='cursor:pointer' onclick=window.open('http://www.health.kr/Menu.PharmReview/View.asp?PharmReview_IDX="+data[val].pharmReview_IDX+"&PharmReview_Referrer=','PHARM_REVIEW_"+data[val].pharmReview_IDX+"','scrollbars=yes,status=yes,toolbar=no,resizable=1,location=no,menu=yes,width=800,height=800');>&nbsp;"+data[val].pharmReview_Title+"</a><span class='date'>"+data[val].pharmReview_ViewDate+"</span></li>";
					val++;
				}
				$("#RtReview").append(htmlData).show();
		},
		error:function(request,status,error){
			console.log("code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error);
		}
	});
}
