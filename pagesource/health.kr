function idfy_setting() {
	$("#color ul li").each(function() {
		if($(this).find("[type=checkbox]").is(":checked") == true) {
				$(this).addClass("selected");
		}
	});

	$("#shape ul li").each(function() {
		if($(this).find("[type=checkbox]").is(":checked") == true) {
			var temp = $(this).prop("id");
			var val = temp.substr(1,2);

			if (val == "al") {
				$("#shape_select a").text("모양 전체");
			} else if (val=="tc") {
				$("#shape_select a").text("모양 기타");
			} else {
				$("#shape_select a").html("<img src='images/pills/shape"+val+".jpg'/>");
			}
		}
	});

	$("#type ul li").each(function() {
		if($(this).find("[type=checkbox]").is(":checked") == true) {
			var temp = $(this).prop("id");
			var val = temp.substr(1,2);

			if (val == "al") {
				$("#form_select a").text("제형 전체");
			} else if (val=="tc") {
				$("#form_select a").text("제형 기타");
			} else {
				$("#form_select a").html("<img src='images/pills/type"+val+".jpg'/>");
			}
		}
	});

	$("#line ul li").each(function() {
		if($(this).find("[type=checkbox]").is(":checked") == true) {
			var temp = $(this).prop("id");
			var val = temp.substr(1,2);

			if (val == "al") {
				$("#line_select a").text("분할선 전체");
			} else if (val=="tc") {
				$("#line_select a").text("분할선 기타");
			} else {
				$("#line_select a").html("<img src='images/pills/line"+val+".jpg'/>");
			}
		}
	});

}
