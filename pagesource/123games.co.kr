<script type="text/javascript">
	$(function () {
		//게임타일 리스트
		setGameList();
	})
	var _arrGameProperty = ['all_game', 'client_game', 'web_game', 'mobile_game'];

	function setGameList() {

		$.ajax({
			url: "/Main/GameList"
				, type: "post"
				, success: function (data) {
					var htmlGameList = '';
					var htmlGnbClientList = '';
					var htmlGnbWebList = '';
					var htmlGnbMobileList = '';
					var _arrGamePropertyClass = '';

					for (var i = 0; i < data.length; i++) {

						switch (data[i].Type) {
							case 0:
								_arrGamePropertyClass = '';
								break;
							case 1:
								//- GNB 클라이언트 게임 H
								htmlGnbClientList += '<li>';
								htmlGnbClientList += '<a href="' + data[i].Url + '">' + data[i].Title + '</a>';
								htmlGnbClientList += '</li>';
								_arrGamePropertyClass = _arrGameProperty[1];
								break;
							case 2:
								//- GNB 웹 게임 H
								htmlGnbWebList += '<li>';
								htmlGnbWebList += '<a href="' + data[i].Url + '">' + data[i].Title + '</a>';
								htmlGnbWebList += '</li>';
								_arrGamePropertyClass = _arrGameProperty[2];
								break;
							case 3:
								//- GNB 모바일 게임 H
								htmlGnbMobileList += '<li>';
								htmlGnbMobileList += '<a href="' + data[i].Url + '">' + data[i].Title + '</a>';
								htmlGnbMobileList += '</li>';
								_arrGamePropertyClass = _arrGameProperty[3];
								break;
						}

						//- GNB 전체게임목록
						$('.box_client ul').html(htmlGnbClientList);
						$('.box_web ul').html(htmlGnbWebList);
						$('.box_mobile ul').html(htmlGnbMobileList);

						htmlGameList += '<li class= "all_game ' + _arrGamePropertyClass + '" "style=display:none">';
						htmlGameList += '<a href="' + data[i].Url + '" >';
						htmlGameList += '<img src= "' + data[i].ImageUrl + '">';
						htmlGameList += '<input type = "hidden" id="BiURL"  value="' + data[i].BiURL + '">';

						switch (data[i].Status) {
							case "H":
								htmlGameList += '<span class = "stChk hot">';
								break;
							case "N":
								htmlGameList += '<span class = "stChk new">';
								break;
							case "C":
								htmlGameList += '<span class = "stChk cbt">';
								break;
							case "U":
								htmlGameList += '<span class = "mobile_game">';
								break;
						}

						htmlGameList += '</span>';
						htmlGameList += '</a>';
						htmlGameList += '<span class = "tileName">' + data[i].Title;
						htmlGameList += '</span>'
						htmlGameList += '</li>'
					}

					//게임타일목록
					$('.gameList_all ul').html(htmlGameList);
					setGameInfoList_Init(0);

					$(function () {
						//게임타일 마우스오버 
						$('.gameList_all li a').mouseenter(function () {
							var biImg = $(this).find('input').val();
							$(this).append('<span class="overEffect blind_bg"></span><span class="overEffect linkTxt">바로가기</span><img class="overEffect tile_bi" src="' + biImg + '">');
							$(this).find('.overEffect').animate({ 'opacity': '0' }, 0);
							$(this).find('.overEffect').animate({ 'opacity': '1' }, 300);
						});
						$('.gameList_all li a').mouseleave(function () {
							$('.overEffect').remove();
						});
					});

				}
				, error: function (request, status, error) {
					alert("code:" + request.status + "\n" + "message:" + request.responseText + "\n" + "error:" + error);
				}
		})
	}

	function setGameInfoList_Init(idx) {
		$('.tileTab').find('a').removeClass('on').eq(idx).addClass('on');
		$('.gameList_all ul li').hide();

//		if ($('.' + _arrGameProperty[idx]).length > 6) {
//			$('.gameList_all ul').animate({ 'height': '420px' });
//			$('.moreListBtn a').addClass("on");
//		} else {
//			$('.gameList_all ul').animate({ 'height': '210px' });
//			$('.moreListBtn a').removeClass("on");
//		}

//		if ($('.' + _arrGameProperty[idx]).length >= 12) {
//			$('.gameList_all ul').animate({ 'height': '840px' });
//			$('.moreListBtn a').addClass("on");
//		} else {
//			$('.gameList_all ul').animate({ 'height': '420px' });
//			$('.moreListBtn a').removeClass("on");
//		}

		$('.' + _arrGameProperty[idx]).show();
	}

