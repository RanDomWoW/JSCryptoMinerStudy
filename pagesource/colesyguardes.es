<script id='districtTemplate' type='text/html'>
    <div>Elige distrito(s)</div>
    <div class="comic"></div>
    {{each districts()}}
      {{if ($index%7 == 0) && ($index > 0)}}
        </ul><ul>
      {{/if}}
      {{if ($index == 0)}}
        <ul>
      {{/if}}
      <li><input type="checkbox" value="${$value.Id}" id="locality" name="locality">${$value.Name}</li>
      {{if ($index == districts.length -1)}}
        </ul>
      {{/if}}
    {{/each}}

</script>

