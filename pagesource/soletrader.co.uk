                $.ajax({ 
                type: "POST", 
                url: data.Url, 
                data: data, 
                dataType: "json",
                success: function (o) { 
                    $(container).html(o.Markup);
                    if(data.F != undefined && data.F === "GetPriceBox" && o.Price !== undefined){
                        UpdatePriceMetaTag(o.Price);
                    }
                },
                complete: function(){
                    // Handle the complete event
                    if (data.Callback != undefined) { window[data.Callback](); }
                }
            }); 
        }
    }

    function SlickLoad() {
        $('.altViews').slick({
            infinite: true,
            slidesToShow: 3,
            slidesToScroll: 3
        });
    }

