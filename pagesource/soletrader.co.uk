<script>
    
    $(document).ready(function () {
        checkEmailSignup();
    });
    
    function checkEmailSignup() {
        if (window.location.href.indexOf("/checkout/") == -1)
        {
            try {
                if (getCookie('ST_emailSignupPopup') === null || getCookie('ST_emailSignupPopup') == "" || getCookie('ST_emailSignupPopup') == '0') {
                    setCookie('ST_emailSignupPopup', '0', 1000);
                
                    // Only show popup if persistent cookies are enabled.
                    if (getCookie('ST_emailSignupPopup') != null) {
                        setTimeout(showEmailSignup, 1000);
                    }
                }
            } catch (err) {}
        }
    }
    
function showEmailSignup() {
    $('#overlay-smallemail').overlay({
        api: true,
        fixed: true,
        top: 'center',
        closeOnClick: true,
        oneInstance: false,
        onStart: function () {
            ($.browser.msie && $.browser.version == 6.0) ? "" : $("#overlay-smallemail").css({ 'position': 'fixed' });
        },
        expose: {
            color: '#000',
            loadSpeed: 0,
            closeSpeed: 0,
            opacity: 0.80
        }
    }).load();

    $(".siteHeader").zIndex(9997);

    // Save info that popup was shown in cookie.
    setCookie('ST_emailSignupPopup', '1', 1000);

    $('#overlay-smallemail').find('.overlayClose').click(function () { $('#overlay-smallemail').overlay().close(); });
    $('#overlay-smallemail').bgIframe().find('.overlayClose').click(function () {
        $('#overlay-smallemail').overlay().close();
        $(".siteHeader").zIndex(999999);
    });
}
    
function doJoinEmail(GlobalSiteId) {
    var gender = "Female";
    
    if ($("#rbMens").prop("checked") == true) {
        gender = "Male";
    }

    $.getJSON("/ajax.aspx?F=JoinEmailList&SiteId=" + GlobalSiteId + "&Email=" + $("#txtJoinEmail").val() + "&Gender=" + gender, function (o) {
        if (!o.Success) {
            if (o.Error) {
                showMessage(o.Error, 'PLEASE REVIEW');
            }
            return;
        }
    });

    //insertEmailSignUpTrackingPixel();
    $('#overlay-smallemail').overlay().close();
    smallEmailPopupClose();
    $(".siteHeader").zIndex(999999);
    //showMessage(o.Message, 'THANK YOU FOR SIGNING UP');
}

function smallEmailPopupClose() {
    // Save info that popup was shown in cookie.
    setCookie('ST_emailSignupPopup', '1', 1000);
    $(".siteHeader").zIndex(999999);
}

//HELPER METHODS
function setCookie(name, value, days) {
    
    var expires;
    if (days) {
        var date = new Date();
        date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
        expires = "; expires=" + date.toGMTString();
    }
    else {
        expires = "";
    }
    document.cookie = name + "=" + value + expires + "; path=/";
}

function getCookie(c_name) {
    
    if (document.cookie.length > 0) {
        c_start = document.cookie.indexOf(c_name + "=");
        if (c_start != -1) {
            c_start = c_start + c_name.length + 1;
            c_end = document.cookie.indexOf(";", c_start);
            if (c_end == -1) {
                c_end = document.cookie.length;
            }
            return unescape(document.cookie.substring(c_start, c_end));
        }
    }
    return "";
}

function showMessage(msg, title) {
    $("#overlay-message #divPopupMessage").html(msg);
    $("#overlay-message .messageHdr").html(title);
    $('#overlay-message').overlay({
        api: true,
        top: 'center',
        closeOnClick: true,
        oneInstance: false,
        expose: {
            color: '#fff',
            loadSpeed: 0,
            closeSpeed: 0,
            opacity: 0.80
        }
    }).load();
    $('#overlay-message').find('.overlayClose').click(function() { $('#overlay-message').overlay().close(); });
    $('#overlay-message').find('.close').click(function() { $('#overlay-message').overlay().close(); });
    $('#overlay-message').bgIframe().find('.overlayClose').click(function() { $('#overlay-message').overlay().close(); });
}

