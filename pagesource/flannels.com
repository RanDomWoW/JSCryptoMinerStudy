	$(document).ready(function(){
		$('#bx-pager a').click(function(e){
			e.preventDefault();
		});			
		var imageRoot = '//images.flannels.com',
			pageLoadDone = false;
		var deferredSliderImages = $('.bxslider-wrap img[data-src]'),
			deferredSliderImageCount = deferredSliderImages.length,
			deferredSliderImagesLoadedCount = 0,
			sliderStarted = false;
		
		deferredSliderImages.load(function(){
			deferredSliderImagesLoadedCount++;
			checkSlider();
		});	
		$(window).load(function(){
			pageLoadDone = true;
			checkSlider();
		});	
		deferredSliderImages.each(function(){
			$this = $(this);
			$this.attr('src', imageRoot + $this.data('src'));
		});	
		setTimeout(startSlider, 10000);	
		function checkSlider() {
			if(!pageLoadDone)
				return;
			if(deferredSliderImageCount > deferredSliderImagesLoadedCount)
				return;
			startSlider();
		}		
		function startSlider(){
			try{
				if(sliderStarted)
					return;				
				sliderStarted = true;				
				var autoStartSliders = window.location.search.indexOf("dontstartsliders") == -1;				
