<!-- Scroll to Top -->
<script type="text/javascript">

    $k(document).ready(function(){
 
        $k(window).scroll(function(){
            if ($k(this).scrollTop() > 500) {
                $k('.scrollup').fadeIn();
            } else {
                $k('.scrollup').fadeOut();
            }
        });
 
        $k('.scrollup').click(function(){
            $k("html, body").animate({ scrollTop: 0 }, 900);
            return false;
        });
 
    });
</script> 

