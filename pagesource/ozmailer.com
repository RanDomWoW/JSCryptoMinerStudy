		if (i == args_value) {
			document.getElementById("func_img_0" + String(i)).src = SHARE_HOST_IMG_URL + "/home_i1705/index/func/fn_" + String(i) + "_on.jpg";
		} else {
			document.getElementById("func_img_0" + String(i)).src = SHARE_HOST_IMG_URL + "/pub/blank.png";
		}
	}



	fx_rolling_marked();
}



/*
** 롤링메뉴 선택된 내용에 마크
*/
function fx_rolling_marked() {
	var obj_rolling = document.getElementById("rolling");
	var arrowDivOuter = document.getElementById("div_arrow_outer");
	var arrowDivImage = document.getElementById("div_arrow_image");
	var frameDiv = document.getElementById("div_frame");
	var selected_obj = false;



	if (frameDiv.style.display != "block") return;



	/*
	** 화면에 출력
	*/
	arrowDivOuter.style.display = "block";



	/*
	** 선택된 메뉴 확인
	*/
	for (var i=1; i<=7; i++) {
		var str = document.getElementById("func_img_0" + String(i)).src;
		if (str.indexOf("_on.jpg") > -1) {
			var selected_obj = document.getElementById("func_img_" + String(i));
		}
	}



	/*
	** 화살표 위치 잡기
	*/
	if (selected_obj != false) {
		var L = selected_obj.offsetLeft;
		var M = document.getElementById("rolling").offsetLeft;
		var N = L - M;

		if (N == 0) {
			var MM = 0;
		} else if (N == 137) {
			var MM = 137 * 1;
		} else if (N == 274) {
			var MM = 137 * 2;
		} else if (N == 411) {
			var MM = 137 * 3;
		} else if (N == 548) {
			var MM = 137 * 4;
		} else if (N == 685) {
			var MM = 137 * 5;
		} else if (N == 822) {
			var MM = 137 * 6;
		}

		MM = MM - obj_rolling.scrollLeft;

		arrowDivImage.style.marginLeft = String(MM) + "px";
	}
}



