	var lockDivA = document.getElementById("div_lock_a");
	var N = document.body.scrollHeight;
	lockDivA.style.height = String(N) + "px";
	lockDivA.style.backgroundColor = '#000000';
	lockDivA.style.display = "block";



	/*
	** opcity가 없는 경우 낮은 버전의 IE라고 생각하고 처리
	*/
	if (is_opacity == 'undefined') {
		lockDivA.style.filter = "alpha(opacity=70)";
