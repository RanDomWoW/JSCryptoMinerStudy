	*/
	ROLLING_SELECTED = false;
}



/*
** 롤링메뉴 자동 롤링
*/
function fx_time_rolling() {
	/*
	** 선택 상태인 경우
	*/
	if (ROLLING_SELECTED) return;



	/*
	** sleep인 경우 처리
	*/
	if (ROLLING_SLEEP > 0) {
		ROLLING_SLEEP--;
		return;
	}



	var obj_rolling = document.getElementById("rolling");



	if (ROLLING_TYPE == "L") {
		var N = obj_rolling.scrollLeft + 1;

		if (N == 137) ROLLING_SLEEP = 100;

		obj_rolling.scrollLeft = N;

		if (N >= 274) {
			ROLLING_TYPE = "R";
			ROLLING_SLEEP = 100;
		}



	} else if (ROLLING_TYPE == "R") {
		var N = obj_rolling.scrollLeft - 1;

		if (N == 137) ROLLING_SLEEP = 100;

		obj_rolling.scrollLeft = N;

		if (N <= 0) {
			ROLLING_TYPE = "L";
			ROLLING_SLEEP = 100;
		}
	}



	fx_rolling_disabled_arrow();
}



function fx_rolling_disabled_arrow() {
	var obj_rolling = document.getElementById("rolling");
	var obj_left = document.getElementById("func_arrow_left");
	var obj_right = document.getElementById("func_arrow_right");



	if (obj_rolling.scrollLeft == 0) {
		obj_left.style.backgroundImage = "url('" + SHARE_HOST_IMG_URL + "/home_i1705/index/btn/arrow_left_off.jpg')";
		obj_left.style.cursor = "default";
		obj_right.style.backgroundImage = "url('" + SHARE_HOST_IMG_URL + "/home_i1705/index/btn/arrow_right_on.jpg')";
		obj_right.style.cursor = "pointer";
	} else if (obj_rolling.scrollLeft == 274) {
		obj_left.style.backgroundImage = "url('" + SHARE_HOST_IMG_URL + "/home_i1705/index/btn/arrow_left_on.jpg')";
		obj_left.style.cursor = "pointer";
		obj_right.style.backgroundImage = "url('" + SHARE_HOST_IMG_URL + "/home_i1705/index/btn/arrow_right_off.jpg')";
		obj_right.style.cursor = "default";
	} else {
		obj_left.style.backgroundImage = "url('" + SHARE_HOST_IMG_URL + "/home_i1705/index/btn/arrow_left_on.jpg')";
		obj_left.style.cursor = "pointer";
		obj_right.style.backgroundImage = "url('" + SHARE_HOST_IMG_URL + "/home_i1705/index/btn/arrow_right_on.jpg')";
		obj_right.style.cursor = "pointer";
	}
}



/*
** login
*/
function fx_check_login() {
	var f = document.login;



	if (f.user_id.value == "") {
		alert("아이디를 입력하시기 바랍니다.");
		f.user_id.focus();
		return false;
	}



	if (/^[0-9a-zA-Z]{3,10}$/.test(f.user_id.value) == false) {
		alert("\n이용자ID는 3~10 자리의 영문자, 숫자이어야 합니다\n\n특수문자, 기호, 한글은 사용하실 수 없읍니다.!\n");
		f.user_id.focus();
		return false;
	}



	if (f.password.value == "") {
		alert("비밀번호를 입력해주시기 바랍니다.");
		f.password.focus();
		return false;
	}



	document.login.action = OZM_HOST_SECURE_URL + fx_get_url("login");



	return true;
}



