/*
** 롤링메뉴 좌우측 화살표버튼으로 롤링
*/
function fx_rolling_arrow(args_type) {
	/*
	** 빠른 롤링을 진행하는 동안 자동롤링은 중단
	*/
	ROLLING_SLEEP = 100;



	var obj_rolling = document.getElementById("rolling");

	if (args_type == "left") {
		if (obj_rolling.scrollLeft <= 137) fx_rolling_fast_call(args_type, 0);
		else if (obj_rolling.scrollLeft <= 274) fx_rolling_fast_call(args_type, 137);
	} else {
		if (obj_rolling.scrollLeft < 137) fx_rolling_fast_call(args_type, 137);
		else if (obj_rolling.scrollLeft < 274) fx_rolling_fast_call(args_type, 274);
	}
}



/*
** 빠른 롤링 호출
*/
function fx_rolling_fast_call(args_type, args_num) {
	FR = setTimeout("fx_fast_rolling('" + args_type + "','" + String(args_num) + "')", 1);
}



/*
** 빠른 롤링
*/
function fx_fast_rolling(args_type, args_num) {
	var N = parseInt(args_num);
	var obj_rolling = document.getElementById("rolling");



	if (args_type == "right") {
		obj_rolling.scrollLeft += 1;
	} else if (args_type == "left") {
		obj_rolling.scrollLeft -= 1;
	}



	if (obj_rolling.scrollLeft == N) {
		fx_rolling_disabled_arrow();
		clearTimeout(FR);
		ROLLING_SLEEP = 50;
	} else if (obj_rolling.scrollLeft <= 0 || obj_rolling.scrollLeft >= 274) {
		fx_rolling_disabled_arrow();
		clearTimeout(FR);
		ROLLING_SLEEP = 50;
	} else {
		fx_rolling_fast_call(args_type, args_num);
	}



	fx_rolling_marked();
}



/*
** 롤링메뉴 선택
*/
function fx_rolling_selected(args_obj, args_value) {
	/*
	** 롤링을 멈추기 위한 변수
	*/
	ROLLING_SELECTED = true;



	/*
	** 스타일에 opacity가 있는가 확인
	*/
	var is_opacity = typeof(document.body.style.opacity);



	/* *********************************************************************************************
	** 화면을 가린다.
	*/
	var lockDivTop = document.getElementById("div_lock_top");
	lockDivTop.style.display = "block";



	var lockDivBottom = document.getElementById("div_lock_bottom");
	var N = document.body.scrollHeight - 426;
	lockDivBottom.style.height = String(N) + "px";
	lockDivBottom.style.display = "block";



	/*
	** opcity가 없는 경우 낮은 버전의 IE라고 생각하고 처리
	*/
	if (is_opacity == 'undefined') {
		lockDivBottom.style.filter = "alpha(opacity=70)";
		lockDivTop.style.filter = "alpha(opacity=70)";
