    document.onkeydown = function(evt) {
      var event = evt || window.event;

      if (event.defaultPrevented) {
        return;
      }

      var handled = false;
      var isEscape = false;
      var isLeft = false;
      var isright = false;

      if (event.key !== undefined) {
        handled = true;
        isEscape = (event.key === "Escape" || event.key == "Esc");
        isLeft = (event.key === "ArrowLeft" || event.key === "Left");
        isRight = (event.key === "ArrowRight" || event.key === "Right");
      } else if (event.keyCode !== undefined) {
        handled = true;
        isEscape = (event.keyCode === 27);
        isLeft = (event.keyCode === 37);
        isRight = (event.keyCode === 39);
      }

      handled = (isEscape || isLeft || isRight);

      if (handled) {
        // Suppress "double action" if event handled
        event.preventDefault();
      }

      if (window.location.hash.length > 2) {
        if (isEscape) {
          window.location.hash="_";
        } else {
          var currentIndex = Number(window.location.hash.replace(/[^0-9]/g, ''));
          if (isLeft && currentIndex > 1) {
            window.location.replace("#img" + (currentIndex - 1));
          } else if (isRight && document.getElementById('img' + (currentIndex + 1))) {
            window.location.replace("#img" + (currentIndex + 1));
          }
        }
      }
    };
    </script>
  </body>
</html>