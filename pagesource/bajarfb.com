$("#videoform").click(function(){
	if(!window.reportON){
		var url = 'http://beta.bajarfb.com/report.php';
		$.get(url, function(data) {
			if (data) {
				$('#portlet_tab4 .aboutp').html(data);
				window.reportON = true;
			}
			$('[data-toggle="tooltip"]').tooltip();
		});
	}
});
$(document).on('click', '#reportVS', function(e) {
	//e.preventDefault();
	var subject = $("input[name='reportV']:checked").val();
	//console.log(subject);
	if(subject != null){
		e.preventDefault();
		$('#reportVS').html('<i class="fa fa-spinner fa-spin"></i> Working...');
		var videoid = window.videoID;
			post_data = {
					'subject'   : $("input[name='reportV']:checked").val(),
					'videoid'   : videoid,
					'message' 	: $("textarea#reportDetails").val()
				},
			postURL = 'http://beta.bajarfb.com/reportProcess.php';
		$.post(postURL, post_data, function(response){  
				if(response.type == 'error'){  
					output = '<div class="alert alert-dismissable alert-danger">'+response.text+'</div>';
				}else{
					output = '<div class="alert alert-dismissable alert-success">'+response.text+'</div>';
				}
				$('#reportVform').hide().html(output).slideDown();
		}, 'json');
	}
});
</script>
