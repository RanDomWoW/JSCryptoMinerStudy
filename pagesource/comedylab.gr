<script type="text/javascript">
	function positionSkin() {
		var winwidth = jQuery(window).width();
		var winheight = jQuery(window).height();
		var topmargin = (winheight/2) - ((winheight * 0.7)/2);
		
		var skin_width = jQuery('#skin-banner img:first').attr('width');
		if (skin_width > 0) {
			jQuery('#skin-banner').css('width', skin_width).css('margin-left', (winwidth-skin_width)/2 +'px').css('visibility', 'visible');
		}
		
	}
	
	jQuery(document).ready(function() {		
		if(jQuery('#skin-banner img:first').length > 0) {			
			positionSkin();
		}
		
		jQuery( window ).resize(function() {		
			if(jQuery('#skin-banner img:first').length > 0) {			
				positionSkin();
			}
		});
	});
</script>
