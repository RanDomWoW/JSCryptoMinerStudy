      console.log(maxscroll);
      
      var scroll = 0;
      
      $('.sigProContainer ul').scrollLeft(0);
      
  $(".next").click(function(){
    if(scroll < maxscroll){
     scroll = scroll + 300;
      $('.sigProContainer ul').animate({scrollLeft: scroll}, 800);
      console.log('success' + scroll);
      return false;
    }else{
      console.log('failed' + scroll);
     return false;
    
    }
    
   }); 
      
  $(".prev").click(function(){
