                                <label for="password-input" class="hidden"></label>
                                <input type="password" required="required" name="password" class="form-control" id="password-input" placeholder="пароль">
                            </div>
                            <div id="two-factor" class="form-group row hidden">
                                <label for="code-input" class="hidden"></label>
                                <input type="text" name="code" class="form-control" id="code-input" placeholder="код подтверждения">
                            </div>
                            <div class="form-group space-between">
                                <span class="checkbox">
                                    <input id="remember-check" name="remember" type="checkbox"/>
                                    <label for="remember-check">
                                        запомнить меня
                                    </label>
                                </span>
                                <span><a href="https://cp.beget.com/password_recovery" target="_blank">Забыли пароль?</a></span>
                            </div>
                            <div class="form-group space-between">
                                <span>
                                    <input id="login-submit" class="btn btn-primary submit" type="submit" value="Войти" data-loading-text="Подождите">
                                </span>
                                <span>
                                    <a href="/ru/order" class="btn btn-success">Заказать</a>
                                </span>


                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
    <!-- /.modal -->
