		// Funci&oacute;n para igualar el alto de columnas
		function equalHeight(group) { 
			tallest = 0;
			if(window.innerWidth > 769){
				group.each(function() {           
					thisHeight = $(this).height();
					if(thisHeight > tallest) {
						tallest = thisHeight;
					}
				});
				group.height(tallest); 
			}
		};							  			



</script>
