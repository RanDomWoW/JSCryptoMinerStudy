    <script type="text/javascript">
    function sendCode(obj) {
        var phone = $("#bind_phone").val();
        if(phone.length < 11) {
            alert("请填写正确手机号");
            return false;
        }
        if(!/^1[3|4|5|8|7][0-9]\d{8}$/.test(phone)) {
            alert("手机号格式不正确");
            return false;
        } 
        $.post('/passport/sendcode', {'phone':phone}, function(data) {
            if(data === 'suc') {
                return console.log(data);
            }
            alert(data);
        });
        settime(obj);
    }
    var countdown=60; 
    function settime(obj) { 
        if (countdown == 0) { 
            obj.removeAttribute("disabled");    
            obj.value="获取验证码";
            countdown = 60; 
            return;
        } else { 
            obj.setAttribute("disabled", true); 
            obj.value="重新发送(" + countdown + ")"; 
            countdown--; 
        } 
    setTimeout(function() { 
        settime(obj) }
        ,1000) 
    }
    var state = $('#bind-mobile-state').val();
    if(state == 1) {
        $().ready(function(){
            $('#report-finance-succeed').modal('show');
        });
    }

    function submitCode() {
        var phone = $("#bind_phone").val();
        if(phone.length < 11) {
            alert("请填写正确手机号");
            return false;
        }
         var code = $("#checkcode").val();
        if(code.length = 0) {
            alert("验证码为空");
            return false;
        }
        var topurl = $("#topurl").val();
        if(code.length = 0) {
            topurl = '/';
        }
        $.post('/passport/default/login', {
                'mobile':phone,
                'code':code,

            }, function(data) {
                if(data == 'ok') {
                    window.location.href = topurl;
                }else {
                    alert(data);
                }
            });
    }
</script>
