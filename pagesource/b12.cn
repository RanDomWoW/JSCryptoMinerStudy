      $(".js-stars").on({
  "click": function() {
    console.log($(this).data('score'));
    if ($(this).data('score') < 7) {
        $('#comment-js').attr("data-sub", $(this).data('score'))
      return $(".comment-contain").css({
        "display": "block"
      });
    } else {
        $('#comment-js').attr("data-sub", $(this).data('score'))
      return $(".comment-contain").css({
        "display": "none"
      });
    }
  }
});
function comment() {
    var score = $("#comment-js").data('sub');
    if(score < 4) {
        var content = $("#comment_content").val();
    }else {
        var content = '';
    }
    var cid = $("#classid").val();
    var str='';
    $("input[type=checkbox]:checked").each(function(i){
        str = str + $(this).data("value") + ',';
    })
    $.ajax({
        type : 'post',
        url : '/course/comment',
        dataType: "json",
        data : 'score=' + score + '&cid=' + cid +'&content=' + content + '&tags=' + str,
        success : function(d) {
            $("#rate-box").modal("hide");
             $("#cmtd-js").css({
                "display": "none"
              });
            $(".cmt-js").css({
                "display": "block"
              });
        },
        error: function(e) {
            $("#rate-box").modal("hide");
             $("#cmtd-js").css({
                "display": "none"
              });
            $(".cmt-js").css({
                "display": ""
              });

        }
    });

}

