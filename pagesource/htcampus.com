    <script>
    $(document).ready(function(){

        // tab start
        var tab = $(".nos-tab");
        $("#tabs").html(""); // empty tab html content
        var TabUl = $("<ul/>",{
        class:"f_tab"
        }).appendTo("#tabs"); // ul in tab container
        var TabDiv = $("<div/>",{
        class:"f_tab_content_wrap"
        }).appendTo("#tabs"); // tab data in tab container
        var tabActive = 0;
        tab.each(function(index, value){
            var li = $("<li/>"); // create empty li
            if(index == tabActive){ // check for active classs
            li.addClass("f_active"); // add active class
            }
            $(this).find(".tabLink").appendTo(li).removeClass("tabLink"); // tab link in tab and remove custom class
            $(li).appendTo(TabUl); // append in tab li
            $(this).children("div").appendTo(TabDiv).removeClass("clr2"); // append in tab data container and remove custom class
        });
        // tab end
        $(".f_tab_content").filter(function(i,v){
        return i != 0;
        }).hide();
        $(".f_tab li").on("click", function(e){
                $(".f_tab li").removeClass('f_active');
                $(".f_tab_content").hide();
                $(this).addClass('f_active');
                $(".f_tab_content").eq($(this).index()).show();
                e.preventDefault();

        });
    });
    </script>

