    <div class="dlp-popup" style="display: none;">
        <div class="feature-col-container">
            <div class="feature-col-head">
                <button class="close-btn dlp-close"><i class="icon-close"></i></button>
            </div>
            <div class="feature-col-body feature-col-get feature-col-all">
                <h4>Are you Interested in Distance Learning MBA Course?</h4>
                <form action="">
                    <div class="feature-col-box">
                        <label class="radio-inline  active">
                            <input type="radio" id="dlp_add_category_yes" name="dlp_add_category" checked="">Yes
                        </label>
                        <label class="radio-inline">
                            <input type="radio" name="dlp_add_category">No
                        </label>
                        <button type="button" class="green-btn btn-lg dlp-submit">Submit</button>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <script>
        $(function(){
        var MBA = '3', DCP = '19', key_cookie = '';

        function set_cookie(value){
            if(value){
                $.cookie(key_cookie, value, { expires : 1 , path : '/'});
            }
            else{
                $.cookie(key_cookie, 0, { expires : 1 , path : '/'});
            }
        }

        function close_dlp_popup(){
            $('.dlp-popup').toggleClass('feature-col-popup');
            $('.dlp-popup').hide();
            set_cookie(1);
        }

        $('.dlp-close').on('click', function(){
            close_dlp_popup();
        });

        $('.dlp-submit').on('click', function(){
            close_dlp_popup();
            var dlp_add_category_yes_val = $('#dlp_add_category_yes:checked').val();

            if(dlp_add_category_yes_val){
                var all_user_cat = User.get("leaddetail").user_categories
                var _final_cat_str = all_user_cat ? all_user_cat+','+DCP : DCP
                User.updateCategories(_final_cat_str, true);
                User.get("leaddetail").user_categories = _final_cat_str;
            }
        });

        function dlp_popup(user_categories){
            var cat_array = user_categories.split(',');
            if (!($.inArray(DCP, cat_array) > -1)){
                $('.dlp-popup').show();
                $('.dlp-popup').addClass('feature-col-popup');
            }
            else{
                set_cookie(1);
            }
        }

        User.onInit(function(){
              var leadData = User.get("leaddetail");
              if(!leadData){return};
              var lead_id = leadData.lead_id;
              key_cookie = 'dlp_key_'+lead_id
              var isDlpChecked = $.cookie(key_cookie);

              if(leadData.category==MBA && lead_id){
                  if(isDlpChecked==0){
                    dlp_popup(leadData.user_categories);
                  }
                  else if(typeof $.cookie(key_cookie) === 'undefined'){
                    set_cookie(0);
                    dlp_popup(leadData.user_categories);
                  }
              }
        });

        });
    </script>
