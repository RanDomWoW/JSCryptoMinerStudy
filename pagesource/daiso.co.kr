<script type="text/javascript">
	var _removeProperty = function(eDOM, propName) {
		if (eDOM.style.removeProperty) { eDOM.style.removeProperty(propName); }
		else { eDOM.style.removeAttribute(propName); }
	};

	$(document).ready(function() {
		if($(".growth").parent("li").hasClass("active")){
			$(".sub-menu2-bar").css("display", "block");
		} else {
			$(".growth").parent('li').on('focusin mouseenter',function(){
				$(".sub-menu2-bar").stop().slideDown();
			})
			$(".growth").parent('li').on('focusout mouseleave',function(){
				$(".sub-menu2-bar").stop().slideUp();
			});
		}

		var $header = $('#header'),
			$headerMenu = $('#header-menu'),
			hMenuTop = $headerMenu[0].offsetTop;

		$(window).bind('scroll', function() {
			if ($(window).scrollTop() > hMenuTop) {
				$header.addClass('scrolling')[0].style.paddingBottom = $headerMenu[0].offsetHeight + 'px';
			}
			else {
				_removeProperty($header.removeClass('scrolling')[0], 'padding-bottom');
			}
		});

		var $menuItems = $headerMenu.find('.menu ul >.item-menu');
		$.each($menuItems,function() {
			$(this).on('focusin mouseenter',function() {
				$(this).addClass('hover').siblings().removeClass('hover');
				$(this).siblings('.active').find('.item-submenu').css({display:'none'})
				$('#header-menu').stop().animate({height:134},400)
			}).on('focusout mouseleave',function() {
				$(this).removeClass('hover');
				$(this).siblings('.active').find('.item-submenu').css({display:'block'})
				if (!$('body').is('.sub-body'))	{
					$('#header-menu').stop().animate({height:74},400)
				}
			})
		})


	});
</script>

