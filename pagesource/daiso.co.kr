<script type="text/javascript">
	$(document).ready(function() {
		var $body = $('body'),
			$subMenu, isOpened;

		$body.append($('#mobile-menu'));

		$('#content-wrapper').bind('click', function(e) {
			if ($body.hasClass('mobile-menu-opened')) {
				$body.removeClass('mobile-menu-opened').addClass('mobile-menu-closed');
			}
		});

		$('#mobile-menu-opener').bind('click', function(e) {
			e.preventDefault();

			if ($body.hasClass('mobile-menu-opened')) {
				$body.removeClass('mobile-menu-opened').addClass('mobile-menu-closed');
			}
			else {
				$body.removeClass('mobile-menu-closed').addClass('mobile-menu-opened');
			}

			return false;
		});

		$('.navigation-lists .link-item').each(function(i, elt) {
			$subMenu = $(elt).find('.sub-menu');

			$subMenu[0].style.height = 'auto';
			$subMenu.data('origH', $subMenu[0].offsetHeight);
			$subMenu[0].style.height = 0;

			$(elt).find('.level-1').bind('click', function(e) {
				e.preventDefault();
				$(this).next().css("height", "auto");
				$subMenu = $(elt).find('.sub-menu');
				isOpened = $(this).hasClass('active');
				$subMenu.animate({
					'height' : (isOpened ? 0 : $subMenu.data('origH')) + 'px'
				}, (isOpened ? 100 : 200));
				isOpened ? $(this).removeClass('active') : $(this).addClass('active');
				isOpened = !isOpened;
				return false;
			});
		});

		$('.navigation-lists .link-item2').each(function(i, elt) {
			$subMenu = $(elt).find('.sub-menu2');

			$subMenu[0].style.height = 'auto';
			$subMenu.data('origH', $subMenu[0].offsetHeight);
			$subMenu[0].style.height = 0;

			$('.growth').bind('click', function(e) {
				e.preventDefault();
				$(this).parent().parent().css("height", "auto");
				$subMenu = $(elt).find('.sub-menu2');
				isOpened = $(this).hasClass('active');
				$subMenu.animate({
					'height' : (isOpened ? 0 : $subMenu.data('origH')) + 'px'
				}, (isOpened ? 100 : 200));
				isOpened ? $(this).removeClass('active') : $(this).addClass('active');
				isOpened = !isOpened;

				return false;
			});
		});
	});
</script>
