    <div class="basket-overlay"></div>
    <div id="omnibasket" class="sideBasket js-omnibasket" data-ng-app="cart" data-ng-controller="CartManageCtrl" data-ng-cloak="">
        <div class="sideBasket-contents">
            <span class="sideBasket__toggle"><a href="#" class="remove-link js-sideBasket__toggle-link js-ga_side-basket-close">close</a></span>
            <h4 class="js-sideBasket__toggle-link">Basket summary</h4>
            <p class="sideBasket__notification"></p>

            <div data-ng-repeat="purchaseType in purchaseTypes" class="sideBasket__productWrap" data-ng-if="cart.Products">
                <p class="sideBasket__purchaseType" data-ng-show="(cart.Products | filter:{IsSubscription : purchaseType.IsSubscription}).length">{{purchaseType.name}}</p>

                <ul class="sideBasket__products" data-ng-show="(cart.Products | filter:{IsSubscription : purchaseType.IsSubscription}).length">
                    <li data-ng-repeat="product in cart.Products | filter:{IsSubscription : purchaseType.IsSubscription}" class="sideBasket__product" data-sku="{{product.SKU}}">
                        <div class="sideBasket__thumb sideBasket-roundel-wrapper">
                            
                            <div data-ng-if="product.RoundelUrl" class="product-roundel {{product.RoundelPostion}}">
                                <img data-ng-src="{{product.RoundelUrl}}" alt="{{product.RoundelAlt}}" />
                            </div>

                            <a class="thumb" data-ng-href="{{product.Url}}">
                                <img data-ng-src="{{product.ImageSrc}}" alt="{{ product.Name }}" />
                            </a>
                        </div>
                        <div class="sideBasket__desc">
                            <p><a class="js-ga_side-basket-product-details" data-ng-href="{{product.Url}}">{{product.Name}}</a></p>
                        </div>
                        <div class="sideBasket__amt">
                            <p data-ng-class="{discount: product.WasPrice}">{{product.Total}}</p>
                        </div>
                        <div class="sideBasket__desc">
                            <p> <span class="sideBasket__quantity">Quantity: {{product.Quantity}}</span></p>
                        </div>
                        <a class="sideBasket__remove js-remove-product js-ga_side-basket-remove-product" data-ng-click="removeProduct(product)">x</a>
                    </li>
                </ul>
            </div>

            <div data-ng-show="cart.MissedPromotions" class="promotion basket-promotion">
                <p data-ng-repeat="missedPromo in cart.MissedPromotions">
                    Missed Promotion: {{missedPromo.DisplayText}}
                </p>
            </div>

            <div class="sideBasket__basket-totals" data-ng-if="cart.TotalCount > 0">

                <div data-ng-if="cart.TotalCount > 0">
                    <!-- Total items: {{cart.TotalCount}} -->
                    <div data-ng-repeat="total in cart.Totals" class="row">
                        <p class="sideBasket__desc">{{total.Description}}</p>
                        <p class="sideBasket__amt">{{total.Cost}}</p>
                    </div>
                    <div class="sideBasket__final-total">
                        <p class="sideBasket__desc">Total</p>
                        <p class="sideBasket__amt">{{cart.CartTotal}}</p>
                    </div>
                </div>
                

                <p class="sideBasket__price-list-discount" data-ng-if="cart.TotalPriceListDiscount > 0">
                    (Basket total above includes promotional prices.
You have SAVED £{{cart.TotalPriceListDiscount| number : 2}} today.)
                </p>

                <p data-ng-if="cart.TotalCount > 0"><a href="/basket" rel="nofollow" class="bttn js-ga_side-basket-my-basket">Your basket</a></p>

            </div>
            
            


            <div class="cart-empty" data-ng-if="cart.TotalCount == 0">
                <h4>Your basket is currently empty</h4>
            </div>
        </div>
        <!-- sideBasket-contents-->
    </div>
    <!-- sideBasket-->
</div>
