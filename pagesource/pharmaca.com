	function changeCountry(url)
	{
		// Add referrer

		var country = getCountryFromUrl(url);
		var referrer = getReferrer(country);
		window.location.href = url + "?uenc=" + Base64.mageEncode(referrer);
	}

	/**
	 * Switch to a new geo country
	 *
	 * If the geo country has been identified change the user to that country / currency
	 **/
	function switchGeoCountry(geoCountry)
	{
		// Redirect to the geo country if it isn't the default country
		if (geoCountry && (geoCountry != currentCountry) && (geoCountry != defaultCountry)) {
			var geoOption = getCountryOption(geoCountry, false);
			if (geoOption) {
				window.location.href = geoOption.val() + "?uenc=" + Base64.mageEncode(getReferrer(geoCountry));
			}
		}

	}

	/**
	 * Get the country from a selector option url
	 **/
	function getCountryFromUrl(url)
	{
		// get currency from URL
		var pos = url.search(/__country\/([A-Z0-9]+)\//);
		var country = url.substring(pos+10, pos+12);
		return country;
	}

	/**
	 * Get the referer for the current page.
	 */
	function getReferrer(country)
	{
		var encUrl = document.URL;
		encUrl = encUrl.replace(/&?__country=([A-Z0-9]{2})/g, '');
		if (encUrl.indexOf("?") >= 0) {
			encUrl = encUrl + "&";
		} else {
			encUrl = encUrl + "?";
		}
		encUrl = encUrl + "__country=" + country;
		return encUrl;
	}

