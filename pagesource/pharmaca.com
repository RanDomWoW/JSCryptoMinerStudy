<script type="application/javascript">
    var affiliateParamNames = ["j\r","e\r","l\r","u\r","jb\r","mid"];

    function getURLParam(paramName)
    {
        var uri = window.location.search.substring(1);
        var uriVars = uri.split('&');

        for (var i = 0; i < uriVars.length; i++) {
            var param = uriVars[i].split('=');
            if (param[0] == paramName) {
                return param[1];
            }
        }

        return false;
    }

    function getAffiliateCookie(name)
    {
        var cookiename = "affiliate"+name;
        if(document.cookie.length >0)
        {
            startC = document.cookie.indexOf(cookiename+"=");
            if (startC != -1)
            {
                startC += cookiename.length+1;
                endC = document.cookie.indexOf(";",startC);
                if(endC ==-1) endC = document.cookie.length;
                return decodeURI(document.cookie.substring(startC,  endC));                               }
        }
        return null;
    }


    for (var i = 0; i < affiliateParamNames.length; i++) {
        var paramName = affiliateParamNames[i];
        var paramValue = getURLParam(paramName);

        if (paramValue !== false) {
            document.cookie="affiliate" + paramName + "=" + encodeURI(paramValue);
        }
    }

</script>


