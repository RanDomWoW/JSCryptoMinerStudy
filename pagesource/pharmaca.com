<script type="text/javascript">
    function shouldShow() {
        var declined = Number(Mage.Cookies.get('newsletter-declined'));
        var subscribed = Number(Mage.Cookies.get('newsletter-subscribed'));

        if (!declined && !subscribed && window.innerWidth > 1024) {
            showModal();
        }
    }

    function showModal() {
        jQuery('#newsletter-modal').modal({
            onHidden: function() {
                /* time is in milliseconds */
                Mage.Cookies.set('newsletter-declined', 1, new Date(new Date().getTime() + (1000 * 3600 * 24 * 30 )));
            }
        }).modal('show');
    }

    Varnish.LoaderInstance.loaderDeferred.then(function () {
        shouldShow();
    });

    jQuery( document ).ready(function( $ ) {
        /*on.click added for tabbing to close and pressing enter.*/
        jQuery('#sv-signup-popup-close').on('click', function(){
            jQuery('#newsletter-modal').hide();
            jQuery('#newsletter-modal').removeClass('transition');
            jQuery('#newsletter-modal').removeClass('visible');
            jQuery('#newsletter-modal').removeClass('active');
            jQuery('.modals').removeClass('transition');
            jQuery('.modals').removeClass('visible');
            jQuery('.modals').removeClass('active');
            jQuery('.modals').removeClass('dimmer');
            jQuery('.cms-home').removeClass('dimmed');
            Mage.Cookies.set('newsletter-declined', 1, new Date(new Date().getTime() + (1000 * 3600 * 24 * 30 )));
        });
        /*redirect last tab to first input*/
        jQuery('#sv-signup-popup-close').on('keydown', function (e) {
            if ((e.which === 9 && !e.shiftKey)) {
                jQuery('#modal-newsletter').focus();
                e.preventDefault();
            }
        });

        /*redirect first shift+tab to last input*/
        jQuery('#modal-newsletter').on('keydown', function (e) {
            if ((e.which === 9 && e.shiftKey)) {
                jQuery('#sv-signup-popup-close').focus();
                e.preventDefault();
            }
        });
    });
</script>




