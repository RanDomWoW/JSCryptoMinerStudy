	<script>

	jQuery(document).ready(function() {
		//HOME SLIDER
		$('.carousel').carousel({
			interval: 5000 //changes the speed
		})

		//CLOCK
		var optionsQATAR = {
			utc: true,
			utcOffset: +3
		}
		$('.timehere').jclock(optionsQATAR);

		//WEATHER
		$.simpleWeather({
				location: 'Doha, Qatar',
				unit: 'c',
				success: function(weather) {
						/*html = ''+weather.currently+' '+weather.temp+'&deg; '+weather.units.temp+' ('+weather.alt.temp+'&deg; F)<img style="height: 35px; vertical-align:middle;" src=
						"'+weather.thumbnail+'" />';*/
						html = ''+weather.temp+'&deg; '+weather.units.temp+' ('+weather.alt.temp+'&deg; F)';
						$(".weather").html(html);
				},
				error: function(error) {
						$(".weather").html("<p>"+error+"</p>");
				}
		});

		function scaleToFill() {
			$('video.ems-video-background').each(function(index, videoTag) {
			   var $video = $(videoTag),
				   videoRatio = videoTag.videoWidth / videoTag.videoHeight,
				   tagRatio = $video.width() / $video.height(),
				   val;

			   if (videoRatio < tagRatio) {
				   val = tagRatio / videoRatio * 1.02; <!-- size increased by 2% because value is not fine enough and sometimes leaves a couple of white pixels at the edges -->
			   } else if (tagRatio < videoRatio) {
				   val = videoRatio / tagRatio * 1.02;
			   }

			   $video.css('transform','scale(' + val  + ',' + val + ')');

			});
		}

		scaleToFill();

		$('.ems-video-background').on('loadeddata', scaleToFill);

		$(window).resize(function() {
			scaleToFill();
		});

		$(window).scroll(function () {
			if ($(this).scrollTop() > 50) {
				$('#back-to-top').fadeIn();
			} else {
				$('#back-to-top').fadeOut();
			}
		});
		// scroll body to 0px on click
		$('#back-to-top').click(function () {
			$('#back-to-top').tooltip('hide');
			$('body,html').animate({
				scrollTop: 0
			}, 800);
			return false;
		});

		$('#back-to-top').tooltip('show');
	});
	$(document).on('click', '.yamm .dropdown-menu', function(e) {
		e.stopPropagation()
	})

	</script>
