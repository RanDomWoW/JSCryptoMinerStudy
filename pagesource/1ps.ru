<script>
        
    var opened = false;
    
    window.addEventListener('load', function () {
        
        if ($(document).width() <= 768) return;
        
        var time_start = new Date();
        var mouseY_last = null;
        var mpmodal_final = false;
        
        $(document).mousemove(function(e) {
            var time_current = new Date();
            if(mouseY_last != null && (mouseY_last - e.pageY > 0) && (e.pageY - $(document).scrollTop() <= 5) && !opened && (time_current - time_start > 2000)) {
                //console.log(time_current - time_start);
                showModalOnExit();
            }
            mouseY_last = e.pageY;
        });
    	
    	$('.mainpage-modal').on('click', '.mp-modal-button.mp-next', function(event){
    	    event.preventDefault();
    	    var next = $(this).data('next');
    		var mpmodal = $(this).parents('.mainpage-modal');
    		mpmodal.find('.mp-modal-slide').removeClass('mp-modal-active');
    		mpmodal.find('.mp-modal-slide[data-slide="' + next + '"]').addClass('mp-modal-active');
    		if (!mpmodal_final && next == mpmodal.find('.mp-modal-slide').length) {
    		    yaCounter36235.reachGoal('mpmodal2_final');
    		    mpmodal_final = true;
    		}
    		mpmodal.find('.mp-modal-dots a').removeClass('mp-modal-active');
    		mpmodal.find('.mp-modal-dots a:eq(' + (next - 1) + ')').addClass('mp-modal-active');
    	});
    	
    	$('.mainpage-modal').on('click', '.mp-modal-dots a', function(event){
    	    event.preventDefault();
    	    var index = $(this).index() + 1;
    		var mpmodal = $(this).parents('.mainpage-modal');
    		mpmodal.find('.mp-modal-dots a').removeClass('mp-modal-active');
    		$(this).addClass('mp-modal-active');
    		mpmodal.find('.mp-modal-slide').removeClass('mp-modal-active');
    		mpmodal.find('.mp-modal-slide[data-slide="' + index + '"]').addClass('mp-modal-active');
    		if (!mpmodal_final && index == mpmodal.find('.mp-modal-slide').length) {
    		    yaCounter36235.reachGoal('mpmodal2_final');
    		    mpmodal_final = true;
    		}
    	});
    	
    	$('.mainpage-modal').on('click', '.mp-link-close', function(){
    	    $.magnificPopup.close();
		    yaCounter36235.reachGoal('mpmodal2_regstart');
		    setCookiesSubs(360);
    	});
    	
    });
    
    function showModalOnExit() {
        if(!opened) {
        	$.magnificPopup.open({
        		items: {
        			src:'#mainpage_modal2',
        			type:'inline'
        		},
        		closeOnBgClick:false
        	});
        	yaCounter36235.reachGoal('mpmodal2_show');
            opened = true;
        	setCookiesSubs(30);
        }
    }
    
    function setCookiesSubs(days) {
    	days = parseInt(days);
    	if (days < 1) days = 15;
    	var expires = new Date(); 
    	expires.setTime(expires.getTime() + days*24*60*60*1000);
    	expires = expires.toGMTString();
    	document.cookie = "mainmodal=1; expires="+expires+"; path=/; domain=1ps.ru;";
    }
</script>

