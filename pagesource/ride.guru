<div ng-cloak class="fare card text-center" ng-class="{filtered : r.filtered}" ng-repeat="r in fareResults">
    <div class="service-banner" style="height: 5px; background: {{ r.entity.service.color }}"></div>
    <div class="feature-banner-wrapper">
        <div class="feature-banner" ng-if="r.entity.service.features.featured.length > 0">
            <span class="feature" ng-repeat="feature in r.entity.service.features.featured">
                {{ feature }}
                <span ng-if="!$last">,</span>
            </span>
        </div>
    </div>
    <div class="content-block mobile-content show-for-small-only text-left noscroll">
        
        
        
        
        
        
        <div class="grid-block align-justify noscroll">
            <div class="mobile-card-left shrink">
                <div>
                    <a ng-if="r.entity.service.company.logo_url"
                       href="/content/resources/rideshares-worldwide#{{ r.entity.service.company.handle }}"
                       target="_blank">
                        <img class="company-logo" alt="{{ r.entity.service.name }}" title="{{ r.entity.service.name }}"
                             ng-src="{{ r.entity.service.company.logo_url }}"/>
                    </a>
                </div>
            </div>
            <div class="mobile-card-middle grid-block vertical noscroll">
                <div>
                    <div class="service-name">{{ r.entity.service.name }}</div>
                    <div class="service-detail" ng-if="r.entity.service.tagline">{{ r.entity.service.tagline }}</div>
                    
                    <div class="service-detail">
                        
<div class="subtle inline-block" ng-repeat="feature in r.entity.service.features.all">
                        <span ng-if="!feature[1]">
                            {{ feature[0] }}
                        </span>
    <span ng-if="feature[1]" ba-action-sheet="">
                            <span class="hand" ba-as-button="">{{ feature[0] }}</span>
                            <span ba-as-content="" ba-swipe-close="down" class="info-sheet text-left">
                                {{ feature[1] }}
                            </span>
                        </span>
    <span class="bullet" ng-show="!$last">•</span>
</div>

                    </div>
                </div>
                <div class="grid-block noscroll">
                    
                    <a href="" ba-open="fare-info-{{ r.entity.service.type }}">
                        <strong>More info...</strong>
                    </a>
                </div>
            </div>
            <div class="mobile-card-right fare text-right align-right">
