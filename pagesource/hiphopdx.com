<script>
                $(document).ready(function() {

                    var overlay = $('#newsletterSignUpOverlay');
                    overlay.popup({
                        onopen: function () {
                            window.location.hash = '#signup'
                        },
                        onclose: function() {
                            Cookies.set('newsletterSignUpOverlay', 'showed');
                            window.location.hash = ''
                        }
                    });

                    $('#newsletterSignUpTab').click(function () {
                        overlay.popup('show');
                    });

                    if (window.location.hash == '#signup') {
                        overlay.popup('show');
                    } else if (Cookies.get('newsletterSignUpOverlay') !== "showed") {
                        setTimeout(function () {
                            overlay.popup('show');
                        }, 5000);
                    }

                    var spinner = overlay.find('.loading');
                    var form = $('#newsletterSignUpOverlay form')
                    var errorContainer = form.find('.error-container');

                    form.submit(function (e) {
                        e.preventDefault();

                        var email = form.find('input').val();
                        console.log(email);
                        spinner.show();
                        form.hide();
                        errorContainer.removeClass('show-errors');

                        $.post(
                                "/newsletter/join",
                                {email: email},
                                function(data){
                                    spinner.hide();

                                    if(data.status == "0") {
                                        overlay.find('.greeting').hide();
                                        overlay.find('.thanks').show();
                                        return;
                                    }
                                    form.show();
                                    errorContainer.addClass('show-errors');
                                });
                    });
                });
            </script>
