	<script>
		function loadPage(pageid, modal) {
		    modal = modal? modal: '#mymodal';
			$("#content").load("/assets/ajax.php",{action:"getContent",id:pageid}, function(response){
				//alert(123);
				if (response) {
				$('.socials').hide();
				  var data = JSON.parse(response);
				  $(modal+' #content').html(data.content);
				  $(modal+' #modal-title').html(data.pagetitle);
				  $(modal+' #modal-date').html(data.date);
				  $(modal+' #modal-data').html(data.description);
				  $(modal).modal('show');
				  var description = $('.galeryimgnav:first').data('description');
				  $('.photo_description').remove();
				  //alert(description);
				  if(description) {
						$('#carousel-modal').append('<p class="photo_description">'+description+'</p>');
				  }
				}
			  });

		}
		$(document).on('click','.read-more', function (e) {
			var url = $(e.target).attr('href'), pageid = $(e.target).data('id'),modal,pageid;
			url = url ? url : $(e.target).parents('.read-more').attr('href');
			pageid = pageid ? pageid : $(e.target).parents('.read-more').data('id');
			modal = modal ? modal : $(e.target).parents('.read-more').data('modal');
			modal = modal ? modal : $(e.target).data('modal');
			if(url) {
				window.history.pushState({id:pageid,modal:modal}, 'Title', url);
			}
		});
		$('#mymodal,#mymodalold').on('hidden.bs.modal', function () {
			window.history.pushState('0', 'Title', '/');
		});
		window.onpopstate =  function(e) {
			$('#mymodal').modal('hide');
			var d = e.state || {};
			console.log(d,'d')
			if(d.id) {
			setTimeout(function () {
				loadPage(d.id);

			}, 100)

			}
		};
		function inWindow(s){
		  var scrollLeft = $(window).scrollLeft();
		  var windowWidth = $(window).width();
		  var currentEls = $(s);
		  var result = [];
		  currentEls.each(function(){
			var el = $(this);
			var offset = el.offset();
			//console.log(el.width() + offset.left, scrollLeft + windowWidth);
			//return offset.left && (el.width() + offset.left) - (scrollLeft + windowWidth);
			result.push((el.width() + offset.left) - (scrollLeft + windowWidth));
			/*if(scrollLeft <= offset.left && (el.width() + offset.left) < (scrollLeft + windowWidth))
			  result.push((el.width() + offset.left) - (scrollLeft + windowWidth));*/
		  });
		  return $(result);
		}
		

		$(document).ready(function () {

		  $(document).on('mouseenter','.isevent',function () {
				$('.event[style*=block]').removeClass('margiright');
			  setTimeout(function () {
			  //alert(inWindow('.event[style*=block]').length);
			  console.log(inWindow('.event[style*=block]')[0]);
			  if(inWindow('.event[style*=block]')[0] > 0) {
				setTimeout(function () {
					//$('.event[style*=block]').addClass('margiright');
					//$('.event[style*=block]').css('left', Math.abs(inWindow('.event[style*=block]')[0])+"px");
					$('.event[style*=block]').css('right', "0px");
				}, 1);
			  }
			  }, 1);

		  });
		  $(document).on('mouseleave','.isevent',function () {
			  $('.event[style*=block]').removeClass('margiright');
		  });
		});
	</script>
