<div class="search__aside" id="filter">
  <div class="hide_on--tablet">
    <div class="search-tabs" ng-show="searchVm.total > -1 &amp;&amp; !searchVm.trial_expired &amp;&amp; !searchVm.quota_exceeded">
    <div class="search__menu" ng-class="{'hidden' : searchVm.autocomplete_open == true && searchVm.autocomplete_array.length > 0}">
       <div ng-class="{ 'search__menu__tab--selected': searchVm.type_of_results == 'arret' , 'search__menu__tab': (searchVm.type_of_results !== 'arret') }"  ng-click="searchVm.typeOfResultChanged('arret')">
          Décisions
          <span class="search__menu__tab__number"  ng-show="searchVm.total_show" ng-bind-html="searchVm.tab_count_arret"></span>
        </div>

        <hr class="hide_on--tablet">

        <div ng-class="{ 'search__menu__tab--selected': searchVm.type_of_results == 'commentaire' , 'search__menu__tab': (searchVm.type_of_results !== 'commentaire') }" ng-click="searchVm.typeOfResultChanged('commentaire')">
          Commentaires
          <span class="search__menu__tab__number"  ng-show="searchVm.total_show" ng-bind-html="searchVm.tab_count_comment"></span>
        </div>
        
        <hr class="hide_on--tablet">

        <div ng-class="{ 'search__menu__tab--selected': searchVm.type_of_results === 'law' , 'search__menu__tab': searchVm.type_of_results !== 'law' }" ng-click="searchVm.typeOfResultChanged('law')">
          Lois et règlements
          <span class="search__menu__tab__number"  ng-show="searchVm.total_show" ng-bind-html="searchVm.tab_count_law"></span>
        </div>

        <hr class="hide_on--tablet">

        <div ng-class="{ 'search__menu__tab--selected': searchVm.type_of_results == 'lawyer' , 'search__menu__tab': (searchVm.type_of_results !== 'lawyer') }" ng-click="searchVm.typeOfResultChanged('lawyer')">
          Avocats
          <span class="search__menu__tab__number"  ng-show="searchVm.total_show" ng-bind-html="searchVm.tab_count_lawyer"></span>
        </div>

        <hr class="hide_on--tablet">

        <div ng-class="{ 'search__menu__tab--selected': searchVm.type_of_results == 'enterprise' , 'search__menu__tab': (searchVm.type_of_results !== 'enterprise') }" ng-click="searchVm.typeOfResultChanged('enterprise')">
          Entreprises
          <span class="search__menu__tab__number"  ng-show="searchVm.total_show" ng-bind-html="searchVm.tab_count_enterprise"></span>
        </div>
    </div>
  </div>
  </div>

  <i class="filter__close" aria-hidden="true" ng-click="searchVm.closeFilter()">.</i>
<div class="search__aside__list" ng-show="searchVm.show_filters === true">

  <search-filters-sort-list></search-filters-sort-list>

  <div class="filter__block" >

    <div class="filter__list" ng-repeat="aggregation in searchVm.aggregations | orderBy:'order'" >
      <span class="filter__title" ng-show=" searchVm.type_of_results == 'arret' || aggregation.values.length > 0 " ng-bind-html="aggregation.name | formatAggregationName">
      </span>
      <search-filters-dates ng-if="aggregation.name === 'dates'"></search-filters-dates> 
      <search-filters-parties ng-if="aggregation.name === 'parties' && searchVm.type_of_results == 'arret'">
      </search-filters-parties>
      <search-filters-exclude ng-if="aggregation.name === 'exclude' && searchVm.type_of_results == 'arret'"></search-filters-exclude>
      <search-filters-categories ng-if="['dates', 'parties', 'exclude'].indexOf(aggregation.name) === -1"></search-filters-categories> 
    </div>
  </div>
</div>

  <footer class="filter__footer" id="filter_footer" >
  <ul>
    <li class="filter__footer__list">
      <a class="filter__footer__link" href="/aide?source=footer_aside">Aide</a>
    </li>
    <li class="filter__footer__list">
      <a class="filter__footer__link" href="/a-propos?source=footer_aside">À propos</a>
    </li>
    <li class="filter__footer__list">
      <a class="filter__footer__link" href="/cgu?source=footer_aside">Mentions légales</a>
    </li>
  </ul>
</footer>
  
</div>

<!-- Only on mobile -->
<div class="filter__block__header">
  <i class="material-icons decision__back__content__icon" title="Retour aux résultats" aria-hidden="true" ng-click="searchVm.closeFilter('cross')">close</i>
  <a class="filter__block__header__title">
    Filtres
    <span class="filter__block__header__title__number">
    {{ searchVm.getLengthSelectedFilter() }}
    </span>
  </a>
  <a class="filter__block__header__clear" ng-click="searchVm.resetFilter()" ng-show="searchVm.active_filter_number > 0">
    Tout effacer
  </a>
</div>

<!-- Mask when the side menu is opened -->
<div class="search__aside__overlay"></div>


 
			
<div class="search__main" id="main">

  <nav class="search__nav--mobile">
    <div class="search__nav__item--result"  ng-show="!searchVm.total_show" > &nbsp; </div>
    <div class="search__nav__item--result"  ng-show="searchVm.total_show" ng-bind-html="searchVm.total_show + '&nbsp;'"></div>
  </nav>

  <!-- nav on desktop -->
  <nav class="search__nav--result hide_on--tablet">

    <span class="search__nav__item--result" ng-show="searchVm.total_show" ng-bind-html="searchVm.total_show + '&nbsp;'"></span>

    <search-filters-sort-dropdown ng-if="(searchVm.type_of_results !== 'lawyer') && (searchVm.type_of_results !== 'enterprise') && (searchVm.type_of_results !== 'law')"></search-filters-sort-dropdown>

    <add-watcher-button 
      class="search__add-watcher"  
      label="Créer une alerte"
      watcher-label="searchVm.watcherLabel" 
      on-click="searchVm.addSearchWatchClicked('search-results')" 
      query-params="searchVm.watcherQuery" 
      tags="searchVm.watcherTags" 
      ng-show="searchVm.type_of_results == 'arret'"></add-watcher-button>

  </nav>

  <div id="search__advice" class="search__advice" ng-show="searchVm.advice_action.text_begin" >
  <span class="search__advice__text" ng-show="searchVm.advice_action.text_begin" ng-bind-html="searchVm.advice_action.text_begin"></span><a class="link" ng-show="searchVm.advice_action.query_formatted_html" ng-click="searchVm.clickAdvice()" ng-bind-html="searchVm.advice_action.query_formatted_html"></a><span class="search__advice__text" ng-show="searchVm.advice_action.text_end" ng-bind-html="searchVm.advice_action.text_end"></span>
</div>
 <!-- show search advice when typing -->
  <div ng-show="searchVm.type_of_results === 'arret' || searchVm.type_of_results === 'commentaire'">
    <div class="search__lawyer-snippet" ng-show="searchVm.professional &amp;&amp; searchVm.professional.title">
   <div class="search_results_lawyer__separator"></div>
    <div id="search-lawyer-snippet" class="search_results_lawyer"></div> 
</div>

    <div class="search__main-column--fixed" ng-show="searchVm.fiche &amp;&amp; searchVm.fiche.title" >
	
	<div class="search_card" >
		
		<i class="material-icons search_card__title__icon">explore</i>
  		<a class="search_card__title--fiche" target="_blank" rel="noreferrer noopener" ng-href="{{searchVm.fiche.url}}" ng-click="searchVm.ficheClicked($event)" ng-bind="searchVm.fiche.title"></a>

		<!-- INDEMNITY (dommages et intérêts)  -->
	  <div class="search_card__block" ng-show="searchVm.fiche && searchVm.fiche.type == 'indemnity'">
			<div class="search_card__block__fiche" ng-class="{'search_card__block__fiche--full' : searchVm.fiche.show_all}"> 
				<span ng-bind-html="searchVm.fiche.text"></span>
				<a target="_blank" rel="noreferrer noopener" ng-href="{{searchVm.fiche.url}}" ng-click="searchVm.ficheClicked($event)"> Source : ENM [PDF]</a>
			</div>

			<div class="search_card__block__more" ng-hide="searchVm.fiche.show_all">
				<a class="search_card__block__more__link"  ng-click="searchVm.fullIndemnityFicheClicked()">Afficher la suite<i class="material-icons search_card__block__more--arrow">arrow_drop_down</i></a>
			</div>
			<div class="search_card__block__more" ng-show="searchVm.fiche.show_all">
				<a class="search_card__block__more__link" ng-click="searchVm.fullIndemnityFicheClicked()">Réduire<i class="material-icons search_card__block__more--arrow">arrow_drop_up</i></a>
			</div>
		</div>


		<!-- DEFINITION (toutes les fiches) -->
		<div class="search_card__block" ng-show="searchVm.fiche && searchVm.fiche.type == 'definition'"> 
			<div class="search_card__block__text"> 
				<span ng-bind-html="searchVm.fiche.text"></span>
			</div>

  			<a class="search_card__block__text__url" target="_blank" rel="noreferrer noopener" ng-href="{{searchVm.fiche.url}}" ng-click="searchVm.ficheClicked($event)">Lire la suite...</a>
			
			<div ng-show="searchVm.fiche.decisions.length > 0">
				<div class="search_card__block__title">
				Décision<span ng-show="searchVm.fiche.decisions.length > 1">s</span>
				de référence</div>
				<ul ng-repeat="decision in searchVm.fiche.decisions">
					<li class="search_card__block__li" >
						
						<span class="search_card__block__li__title" ng-bind = "decision.title" ng-click="searchVm.modal.openRestrictedModal('search_fiche_decision')"></span>
						
					</li>
				</ul>
			</div>
		</div>
		
	</div>

</div>

    <div class="search_card search__main-column--fixed" ng-show="searchVm.law &amp;&amp; searchVm.law.text_short">
	
  <i class="material-icons search_card__title__icon">account_balance</i>
	
  <a class="search_card__title--law" target="_blank" ng-show="searchVm.law.url" ng-href="{{searchVm.law.url}}" ng-click="searchVm.lawClicked($event)">
	<span ng-bind="searchVm.law.title"></span>
  </a>

  <div ng-show="searchVm.law_other_versions.length > 1">
    <select class="search_card__select" ng-model="searchVm.law_version_doc_id" ng-change="searchVm.changeLawVersionAndSearch()" >
    	<option ng-repeat="version in searchVm.law_other_versions" value="{{version.doc_id}}" ng-bind-html="searchVm.convertDateLawVersion(version)"> . </option>
    </select>
		
	
    	<div ng-show="searchVm.type_of_results == 'arret'" class="search_card__restrict" ng-click="searchVm.modal.openRestrictedModal('restrict_law_version')">
    	  
	    <i class="material-icons filter__trigger__check " ng-class="{'fa-filter-border' : searchVm.filter_on_specific_article_version == true, 'fa-filter-border-empty' : searchVm.filter_on_specific_article_version != true}">check</i>  
			Restreindre la recherche <span class="hide_on--mobile">à cette version</span>
		</div>

  </div> 

  <div ng-hide="searchVm.law_other_versions.length > 1" ng-bind-html="searchVm.convertDateLawVersion(searchVm.law_other_versions[0])"></div>
	
	<div class="search_card__text" >
		<span ng-bind-html="searchVm.law.expanded ? searchVm.law.text : searchVm.law.text_short"></span>

		
    	<span class="search_card__display" ng-click="searchVm.modal.openRestrictedModal('search_law')" ng-show="! searchVm.law.expanded &amp;&amp; searchVm.law.text_short != searchVm.law.text">
    	
  			Afficher la suite
  		</span>
  		<span class="search_card__display" ng-click="searchVm.lawLimitToClicked()" ng-show="searchVm.law.expanded">
  			Afficher moins
  		</span>
	</div>
	
	<div class="search_card__decisions" ng-show="searchVm.law_qpc_decisions.length > 0">
		<div class="search_card__block__title">
			Décision<span ng-show="searchVm.law_qpc_decisions.length > 1">s</span> liée<span ng-show="searchVm.law_qpc_decisions.length > 1">s</span> à une QPC
		</div>
		<ul class="search_card__block__ul"  ng-repeat="decision in searchVm.law_qpc_decisions">
			<li class="search_card__block__li" >
				
				<span class="search_card__block__li__qpc__title" ng-bind = "decision.titre" ng-click="searchVm.modal.openRestrictedModal('search_law_decision')"></span>
				
				<!-- checker if it works -->
				<span class="search_card__block__li__qpc__solution" ng-show="decision.solution_cc" > (<span ng-bind = "decision.solution_cc"></span>)</span>
			</li>
		</ul>
	</div>

	<div class="search_card__decisions" ng-show="searchVm.law_decisions.length > 0">
		<div class="search_card__block__title">
			Décision<span ng-show="searchVm.law_decisions.length > 1">s</span> de référence
		</div>
		<ul  class="search_card__block__ul" ng-repeat="decision in searchVm.law_decisions">
			<li class="search_card__block__li" >
				
				<doctrine-popover popover-template="<decision-hover free=true callback='searchVm.modal.openRestrictedModal()' decision='decision'></decision-hover>">
					<span class="search_card__block__li__title" ng-bind-html="decision.titre" ng-click="searchVm.modal.openRestrictedModal('search_law_decision')"></span>
				</doctrine-popover>
				
			</li>
		</ul>
	</div>

</div>

    <div class="search__main-column--fixed" ng-show="searchVm.enterprise &amp;&amp; searchVm.enterprise.raison_sociale">
    <div class="search_card">
        <enterprise-details can-follow="searchVm.type_of_results == 'arret'" enterprise="searchVm.enterprise" source="search_enterprise"></enterprise-details>
    </div>
</div>
  </div>

  <!-- if decision -->
  <div ng-show="searchVm.total > 0" class="search__results">
    
    <!-- search results (arret not commentaire) -->
    <div ng-show=" searchVm.type_of_results == 'arret' ">
      <div ng-repeat="result in searchVm.results" >
        <span class="search__hit" ng-attr-id="hit{{result.position}}" ng-if="$index > 0"></span>
          <div class="excerpt" ng-class="{'excerpt--visited': result.visited}">
  
  <span ng-show="result.score != null" ng-bind="result.score"></span>
  
  <!-- Hit position -->
  <div class="excerpt__separator" ng-class="{'excerpt__separator--margin' : $index > 0}">
    <span class="excerpt__separator__text" ng-bind-html="result.position"> </span>
    <hr class="excerpt__separator__line" >
  </div>
  
  <!-- Title -->
<div class="excerpt__block">
  
  <a class="excerpt__title" ng-attr-id="excerpt_title_{{$index}}" ng-bind-html="result.titre" ng-click="commonVm.modal.openRestrictedModal('excerpt_results')" ></a>
  
</div>



<!-- Subtitle-->
<div class="excerpt__subtitle" ng-show="{{result.arret_dec_att | non_empty}} || {{result.solution | non_empty}}">
  
  <!-- decisions attaqueée -->
  <span ng-show="result.arret_dec_att | non_empty">
    <span class="excerpt__subtitle__title">Décision attaquée&nbsp;: </span>
    <span class="excerpt__subtitle__tag" ng-bind-html="result.arret_dec_att"></span>
  </span>
  <!-- dispositif -->
  <span ng-show="result.solution | non_empty">
    <span class="excerpt__subtitle__tag__point" ng-show="result.arret_dec_att | non_empty">|</span>
    <span class="excerpt__subtitle__title">Dispositif&nbsp;: </span>
    <span class="excerpt__subtitle__tag" ng-bind-html="result.solution"></span>
  </span>
  <!-- solution -->
  <span ng-show="result.validation">
    <span class="excerpt__subtitle__tag__point" ng-show="(result.arret_dec_att | non_empty) || (result.solution | non_empty)">|</span>

    <span ng-bind="result.validation.juridiction"></span>&nbsp;:
    <a ng-href="{{result.validation.url}}" class="has-tip" data-tooltip-placement="bottom" data-tooltip-popup-delay='100' ng-class="result.validation.validated ? 'excerpt__subtitle__background__validated' : 'excerpt__subtitle__background__notvalidated'" data-tooltip="Lire la décision {{result.validation.of_juridiction}}">{{result.validation.solution}}</a>

  </span>
  <!-- automatic_metadata -->
  <span ng-repeat="(field, value) in result.automatic_metadata">
    <span class="excerpt__subtitle__tag__point" ng-show="(result.solution | non_empty) || (result.arret_dec_att | non_empty)|| (result.validation)">|</span>
    <span class="excerpt__subtitle__title"><span ng-bind="field"></span>&nbsp;: </span>
    <span class="excerpt__subtitle__tag" ng-bind-html="value"></span>
  </span>
</div>


  <!-- Keywords -->
<ul ng-show="result.raw_keywords.length > 0" class="excerpt__subtitle__inner" >
    <li class="excerpt__subtitle__item" ng-repeat="raw_keyword in result.raw_keywords track by $index">
    <a class="excerpt__subtitle__item__text" ng-bind-html="result.html_keywords[$index]" ng-href="{{raw_keyword | formatQueryUrl}}"></a><span class="excerpt__subtitle__item__separator" ng-hide="$last">·</span>
    </li>
</ul>  <div class="excerpt__block__text">
<!-- text -->

  <p class="excerpt__block__text__content" ng-bind-html="result.sommaire_ana" ng-show="result.sommaire_ana | non_empty"></p> 


<!-- see more -->

<a class="excerpt__more" ng-class="{'excerpt__more--visited': result.visited}" ng-click="commonVm.modal.openRestrictedModal('excerpt_results')">

Lire la suite...
</a>
</div>

  <div class="excerpt__subtitle__comments" ng-show="result.number_of_comments > 0">

    
        <div class="excerpt__subtitle__comments__relative"
             ng-click="commonVm.modal.openRestrictedModal('excerpt_comments')">
            
            <i class="material-icons excerpt__actionbar__comments__icon">comment</i>
            <ul>
                <li ng-repeat="author_name in result.comment_authors track by $index"><span
                            class="excerpt__subtitle__comments__text"
                            ng-show="$last && result.number_of_comments_remaining < 1 && result.comment_authors.length > 1"> et </span>{{author_name
                    | limitTo: 40}}{{author_name.length > 40 ? '…' : ''}}<span
                            class="excerpt__subtitle__comments__text"
                            ng-hide="$last || (result.number_of_comments_remaining < 1 &&
                            result.comment_authors.length > 1 &&
                            result.comment_authors.length - 2 === $index)">, </span>
                </li>
            </ul>
            <span class="excerpt__subtitle__comments__text"
                  ng-show="result.number_of_comments_remaining > 0">
      et {{result.number_of_comments_remaining}}
      <span class="excerpt__subtitle__comments__text"
            ng-show="result.number_of_comments_remaining == 1">autre auteur</span>
      <span class="excerpt__subtitle__comments__text"
            ng-show="result.number_of_comments_remaining > 1">autres auteurs</span>
    </span>
            <span class="excerpt__subtitle__comments__text"
                  ng-show="result.number_of_comments == 1">
    a commenté
    </span>
            <span class="excerpt__subtitle__comments__text" ng-show="result.number_of_comments > 1">
    ont commenté
    </span>
            <span class="excerpt__subtitle__comments__text">
    cette décision
    </span>
        </div>
    </div>

    <!-- Folders ADD  -->
    <div class="excerpt__actionbar">
        
            <span class="excerpt__actionbar__bookmark"
                  ng-click="commonVm.modal.openRestrictedModal('AddToFolderClicked', $event)">
                
                <span ng-show="result.in_folders.length == 0">
      <i class="material-icons excerpt__actionbar__bookmark__picto">add</i>
      <span class="excerpt__actionbar__bookmark__text">Ajouter
      <span class="hide_on--mobile">à mes dossiers</span>
      </span>
    </span>
    <span ng-show="result.in_folders.length > 0">
      <i class="material-icons excerpt__actionbar__bookmark__picto">add</i>
      <span class="excerpt__actionbar__bookmark__text">Ajouter
      <span class="hide_on--mobile">à mes dossiers</span>
      </span>
    </span>
  </span>

  <span ng-repeat="folder in result.in_folders">
      
      <span class="excerpt__actionbar__tag"
            ng-click="commonVm.modal.openRestrictedModal('AddToFolderClicked', $event)"> {{folder.title}}
    </span>
      
  </span>
    </div> 
</div>

      </div>
    </div>

    <!-- search results (commentaire not arret) -->
    <div ng-show="searchVm.type_of_results == 'commentaire' ">
      <div class="excerpt_comment" ng-repeat="comment in searchVm.results" >
        <div class="excerpt__separator" ng-class="{'excerpt__separator--margin' : $index > 0}">
  <span class="excerpt__separator__text ng-binding" ng-bind="$index+1"></span>
  <hr class="excerpt__separator__line">
</div>

<!-- Comment -->
<div  class="excerpt_comment__comment" >
  
  
    <a class="excerpt_comment__comment__title" ng-click="commonVm.modal.openRestrictedModal('excerpt_results_comment')" ng-bind-html="comment.title">
    </a>
  
  <div class="excerpt_comment__comment__author">
    <span ng-show="comment.date">
      <span class="home__comments__right__date" ng-bind-html="comment.date"></span>
    </span>
    <span ng-show="comment.author">
      <span ng-show="comment.date"> ·</span>
      <span ng-bind-html="comment.author"></span>
    </span>
    <span ng-show="comment.nice_source">
      <span ng-show="comment.author || comment.date"> ·</span>
      <span ng-bind-html="comment.nice_source"></span>
    </span>
    <span ng-show="comment.score">
      <span ng-show="comment.author || comment.date || comment.nice_source"> ·</span>
      <span ng-bind-html="comment.score"></span>
    </span>
  </div>

  <div class="excerpt_comment__comment__excerpt">
  <!-- text -->
  
    <p class="excerpt_comment__comment__excerpt__content" ng-bind-html="comment.text_short"></p> 
  

  <!-- see more -->
  
  <a ng-show="comment.text_short" class="excerpt_comment__comment__more" ng-click="commonVm.modal.openRestrictedModal('excerpt_results_comment')">
  
  Lire la suite...
  </a>
  </div>
</div>

<!-- decision -->
<div class="excerpt_comment__arret" ng-show="comment.arret.search_title">
  <div class="excerpt_comment__arret__subtitle">
    <span class="excerpt_comment__arret__link__span">Décision commentée&nbsp;:</span>
    
      <a class="excerpt_comment__arret__link" ng-click="commonVm.modal.openRestrictedModal('excerpt_results_comment_arret')" >
    
      <div class="excerpt_comment__arret__link__title" ng-class="{'excerpt_comment__arret__link__title--visited': comment.visited}" ng-click="commonVm.clickResult(comment)" >
      {{ comment.arret.search_title | limitTo: 125 }}
      {{ comment.arret.search_title.length > 125 ? '...' : ''}}
      </div>
    </a>
  </div>
</div>

      </div>
    </div>

    <!-- search results (laws and rules) -->
    <div ng-show="searchVm.type_of_results === 'law' ">
      <search-results-law laws="searchVm.results" last-search-response="searchVm.last_search_response"></search-results-law>
    </div>

    <!-- search results (avocats) -->
    <div ng-show=" searchVm.type_of_results == 'lawyer' ">
      <div ng-repeat="lawyer in searchVm.results">
        <div class="search_card__lawyer">
  
  <a ng-click="searchVm.modal.openRestrictedModal('lawyer_search')">	
  
    <div class="search_card__lawyer__right">
      <div>
        <span class="search_card__lawyer__right__title">
          <span ng-bind="lawyer.name"></span>
        </span>
      </div>
      <div class="search_card__lawyer__right__verify" ng-show="lawyer.is_verified">
        <i class="material-icons search_card__lawyer__right__verify__icon green">verified_user</i>
