    <script>
  function LoadingFailureDetector() {
    var COOKIE_NAME = 'doctrine_loading_retries'
    var already_reported = false
    var loadingPageTimestamp = getNowTimestamp()
    var waitingTimeBeforeRetry = 3000

    function getNowTimestamp() {
      return Math.round(new Date().getTime())
    }
    
    function setCookie(cname, cvalue, maxAge) {
      var d = new Date();
      d.setTime(d.getTime() + maxAge);
      var expires = "expires="+ d.toUTCString();
      document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }
    
    function getCookie(cname) {
      var name = cname + "=";
      var decodedCookie = decodeURIComponent(document.cookie);
      var ca = decodedCookie.split(';');
      for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
      }
      return "";
    }
    
    function reportLoadingError(message, extra) {
      if (window.location.hostname === "www.doctrine.fr") {
        console.log('reportLoadingError: ' + message, extra)
        var loadingErrorTimestamp = getNowTimestamp()
        extra.duration = Math.abs(loadingErrorTimestamp - loadingPageTimestamp)
        if (_rollbarConfig && !already_reported) {
          already_reported = true
          Rollbar.critical('reportLoadingError: ' + message, extra)
        }
      } else {
        console.log('Not reporting reportLoadingError: ' + message)
      }
    }
    
    function reportLoadingWarning(message, extra) {
      if (window.location.hostname === "www.doctrine.fr") {
        console.log('reportLoadingWarning: ' + message, extra)
        if (_rollbarConfig && !already_reported) {
          already_reported = true
          Rollbar.warning('reportLoadingWarning: ' + message, extra)
        }
      } else {
        console.log('Not reporting reportLoadingWarning: ' + message)
      }
    }

    this.loadingFailedFor = function(what) {
    
      var already_retried = parseInt(getCookie(COOKIE_NAME))
      if (isNaN(already_retried)) {
        already_retried = 0
      }
      if (already_retried >= 1) {
        reportLoadingError(what + ' failed to load', {
          retry: already_retried,
        })
      } else {
        already_retried++
        reportLoadingWarning(what + ' failed to load but retrying', {
          retry: already_retried,
        })
        setTimeout(function() {
          setCookie(COOKIE_NAME, already_retried, 60000)
          location.reload()
        }, waitingTimeBeforeRetry)
      }
    }

    this.loadingLinkFailed = function (el, what) {
      if(el.sheet.cssRules && !el.sheet.cssRules.length) {
        this.loadingFailedFor(what);
      }
    }
  }
  
  var loadingFailureDetector = new LoadingFailureDetector()
</script>    <!-- Google fonts -->
