		var filex = (file.endWith(".jpg") || file.endWith(".jpeg") || file.endWith(".png") || file.endWith(".gif"));
		if (file != "" && !filex) {
			alert("上传文件类型不支持,仅支持jpg、jpeg、png、gif文件类型");
			return false;
		}
		if(content==""){
			alert("请填写裁判文书正文！");
			return false;
		}
		$("#subn").attr("disabled", true);

		alert("请您等待管理员审核！");
	    return true;
	}
	String.prototype.endWith=function(str){
		if(str==null||str==""||this.length==0||str.length>this.length)
 			return false;
		if(this.substring(this.length-str.length)==str)
  			return true;
		else
  			return false;
		return true;
	}
	
