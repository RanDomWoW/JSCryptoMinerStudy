$(function() {
	var page = 1;
	var i = 4; //每版放4个图片
	var len = $(".list_d .list_e li").length;
	var page_count = Math.ceil(len / i);   //只要不是整数，就往大的方向取最小的整数
	var none_unit_width = $(".list_d").width(); //获取框架内容的宽度,不带单位
	var $parent = $(".cntList_fw ");
	//向右 按钮

	//往左 按钮

	$(".leftFw").click(function() {
		var step = 100;
		var u_margin = $("#lest_e").css("margin-left");
		var left = parseInt(u_margin.substring(0, u_margin.length - 2));
		//alert(left);
		if (left == 0) {
			//	left-=step;
			//	alert(left);
			$("#lest_e").animate({'margin-left': '-100px'});
			//alert('haha');	
		} else if (left > -760) {
			left -= step;
			if (left <= -760) {
				left = -760;
			}
			$("#lest_e").animate({'margin-left': left + 'px'});
		}
	});

	//向右翻页
	$(".rightFw").click(function() {
		var step = 100;
		var u_margin = $("#lest_e").css("margin-left");
		var left = parseInt(u_margin.substring(0, u_margin.length - 2));
		if (left == 0) {
			$("#lest_e").animate({'margin-left': '0px'});
		} else if (left <= 0) {
			left += step;
			$("#lest_e").animate({'margin-left': left + 'px'});
		}
	});
});
</script>
