<script>
  var banners = document.getElementsByClassName('em-banner');

  for (b in banners) {
    if (banners.hasOwnProperty(b)) {
      var banner = banners[b];
      if (banner.dataset.hasOwnProperty('banner')) {
        var data = banner.dataset.banner;

        $.get('/banners/'+data, function (data) {
          if (data && data.status == 'success') {
            var a = document.createElement('a');
            a.href = data.data.url;
            var i = document.createElement('img');
            i.src = data.data.img;
            a.appendChild(i);
            banner.appendChild(a);
            banner.style.display = 'block';
          } else {
            banner.remove();
          }
        });
      }
    }
  }
</script>

