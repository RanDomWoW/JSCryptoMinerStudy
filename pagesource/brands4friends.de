        var appearTrackingEvents = {
            
            "#scroll-1": ['Navigation', {action: 'Gescrollt bis', stringValue: 'Unsere Vorschläge für dein erstes Mal', nonInteraction: true, numValue: 29}],
            
            "#scroll-2": ['Navigation', {action: 'Gescrollt bis', stringValue: 'Wir haben jeden Tag neue Aktionen', nonInteraction: true}],
            "#scroll-2-2": ['Navigation', {action: 'Gescrollt bis', stringValue: 'Und morgen? Da gibt es', nonInteraction: true}],
            "#scroll-3": ['Navigation', {action: 'Gescrollt bis', stringValue: 'Angebote exklusiv für Mitglieder.', nonInteraction: true}],
            "#scroll-4": ['Navigation', {action: 'Gescrollt bis', stringValue: 'Wir wurden ausgezeichnet', nonInteraction: true}],
            "#scroll-5": ['Navigation', {action: 'Gescrollt bis', stringValue: 'brands4friends: Deutschlands No. 1 Online-Shopping-Club für Designermode und Lifestyle', nonInteraction: true}],
            "#scroll-6": ['Navigation', {action: 'Gescrollt bis', stringValue: 'Unser Sortiment: ein kleiner Einblick', nonInteraction: true}]
        };

        $.each(clickTrackingEvents, function (key, value) {
            $(key).click(function () {
                _eno.push(clickTrackingEvents[key]);
            });
        });

        $.each(appearTrackingEvents, function (key, value) {
            $(key).appear().one('appear', function () {
                _eno.push(appearTrackingEvents[key]);
            });
        });

        var loginForm = $('#form-login'),
                loginButton = $('#js-login'),
                $username = $('#registration-email'),
                $newPassword = $('#new-password'),
                $confirmNewPassword = $('#confirm-new-password'),
                passwordStrengthIndicator = $('#box-password'),
                passwordStrengthLabel = $('#t-password-strength-label'),
                passwordConfirmMismatchMsg = $('#error-message-passwords-do-not-match'),
                passwordConfirmEmptyMsg = $('#error-message-registration-password-repeat'),
                activeClass = 'validation-error-active';

        var passwordStrengthClass = {
            'strong': 'password-security-high',
            'medium': 'password-security-medium',
            'weak': 'password-security-low',
            'invalid': 'password-security-invalid'
        };

        var passwordStrengthLabelText = {
            'strong': 'stark',
            'medium': 'mittel',
            'weak': 'schwach',
            'invalid': 'ungültig'
        };

        var loginError = {};
        

        loginForm.submit(function (event) {
            var loginForm = $('#form-login'),
                    stateInput = loginForm.find('[name="state"]'),
                    rememberMeEnabled = loginForm.find('[name="rm"]').prop('checked'),
                    targetUrl = loginError.targetUrl !== undefined ? loginError.targetUrl : '';

            event.preventDefault();

            if (loginCredentialsNotSet()) {
                return;
            }
            disableButton(loginButton);

            var formData = _.reduce(loginForm.serializeArray(), function (acc, item) {
                acc[item.name] = item.value;
                return acc;
            }, {});
            var sendData = _.merge(formData, {
                authType: 'usernamePassword',
                rememberMe: rememberMeEnabled,
                targetUrl: targetUrl
            });

            $.post({
                url: '/webshop/login/preauth',
                data: sendData,
                timeout: 10000
            }).done(function (responseData) {
                if (responseData.state) {
                    stateInput.val(responseData.state);
                }
            }).always(function () {
                loginForm.find('[name="csrftkn"]').attr("disabled", "true");
                loginForm.find('[name="basket_info_id"]').attr("disabled", "true");
                loginForm.find('[name="rm"]').attr("disabled", "true");

                loginForm.off('submit');
                loginForm.submit();
            });

            return false;
        });

        var registerButton = $('#js-register');
        registerButton.click(function (event) {
            event.preventDefault();

            if (registrationCredentialsNotSet()) {
                return;
            }
            disableButton(registerButton);

            var sendData = $('#form-register').serialize();
            $.post('/webshop/register', sendData, {withCredentials: true})
                    .done(function (data) {
                        enableButton(registerButton);
                        clearAllErrors();

                        if (data.optInId) {
                            var obfuscatedOptInId = data.optInId;
                            var redirectUrl = '/webshop/registration/acceptance?opt=' + obfuscatedOptInId;
                            redirect(redirectUrl);
                        } else if (data.errors) {
                            handleRegistrationErrors(data.errors);
                            trackErrors(data.errors);
                        }
                    });
        });

        function handleRegistrationErrors(errors) {
            var inlineErrors = $.grep(errors, function (error) {
                return error in errorCodeToElement;
            });

            $.each(inlineErrors, function (index, error) {
                showError(error);
            });
        }

        function doPasswordsMatch() {
            return $newPassword.val() !== '' && $newPassword.val() === $confirmNewPassword.val();
        }

        function hasPasswordCorrectLength() {
            var passwordLength = $newPassword.val() && $newPassword.val().length;

            return passwordLength >= 6;
        }

        function showPasswordStrengthIndicator(strength, tooShort) {
            passwordStrengthIndicator.removeClass('password-security-invalid');
            passwordStrengthIndicator.removeClass('password-security-low');
            passwordStrengthIndicator.removeClass('password-security-medium');
            passwordStrengthIndicator.removeClass('password-security-high');

            passwordStrengthIndicator.addClass(passwordStrengthClass[strength]);
            passwordStrengthLabel.text(tooShort ? 'zu kurz' : passwordStrengthLabelText[strength]);
        }

        function styleValidatedPasswordStrength(strength, errors) {
            clearPasswordErrors();
            showErrors(errors);
            showPasswordStrengthIndicator(strength, false);
            checkConfirmedPassword(false);
        }

        function styleTooShortPasswordStrength() {
            clearPasswordErrors();
            showErrors([]);
            showPasswordStrengthIndicator("invalid", true);
            checkConfirmedPassword(false);
        }

        function checkConfirmedPassword(showEmptyError) {
            clearPasswordConfirmErrors();

            if ($confirmNewPassword.val() === '' && showEmptyError) {
                passwordConfirmEmptyMsg.addClass(activeClass);
            } else if ($confirmNewPassword.val() !== '' && !doPasswordsMatch()) {
                passwordConfirmMismatchMsg.addClass(activeClass);
            }
        }

        $('#confirm-new-password, #new-password').on('keyup', function () {
            if (doPasswordsMatch()) {
                $confirmNewPassword.addClass('passwords-match');
                passwordConfirmMismatchMsg.removeClass(activeClass);
            } else {
                $confirmNewPassword.removeClass('passwords-match');
            }
        });

        $newPassword.focus(function () {
            var hasErrors = false;
            $('span[data-error-for="password"]').each(function () {
                hasErrors = hasErrors || $(this).is(':visible');
            });
        });

        var ajaxPasswordRequest = null;
        $newPassword.on('input', _.debounce(function () {
            if (hasPasswordCorrectLength()) {
                if (ajaxPasswordRequest !== null) {
                    ajaxPasswordRequest.abort();
                    ajaxPasswordRequest = null;
                }

                ajaxPasswordRequest = $.post("/webshop/account/passwordvalidation", {
                            username: $username.val(),
                            password: $newPassword.val()
                        },
                        function (data) {
                            styleValidatedPasswordStrength(data.strength.toLowerCase(), data.errors);
                        }
                );
            } else {
                styleTooShortPasswordStrength();
            }
        }, 350));

        $confirmNewPassword.blur(function () {
            checkConfirmedPassword($newPassword.val !== '');
        });

        $username.blur(function () {
            var email = $username.val();
            if (email === '') {
                return;
            }

            if (!/.+@.+\..+/i.test(email)) {
                handleRegistrationErrors(['account.register.form.error.email']);
            } else if (email.length < 7 || email.length > 150) {
                handleRegistrationErrors(['account.register.form.error.email.length']);
            } else {
                clearError('account.register.form.error.email');
                clearError('account.register.form.error.email.length');
            }
        });

        $('.overlay-close').click(function (event) {
            clearAllErrors();
        });

        function redirect(redirectUrl) {
            window.location.replace(redirectUrl);
        }

        function loginCredentialsNotSet() {
            var username = $('#j_username').val();
            var password = $('#j_password').val();

            return !(username && password);
        }

        function registrationCredentialsNotSet() {
            var username = $username.val();
            var password = $newPassword.val();
            var confirmPassword = $confirmNewPassword.val();

            return !(username && password && confirmPassword);
        }

        function enableButton(button) {
            button.prop('disabled', false);
        }

        function disableButton(button) {
            button.prop('disabled', true);
        }

        function trackErrors(errors) {
            $.each(errors, function (index, error) {
                _eno.push(['Fehler', {
                    action: error,
                    stringValue: 'Homepage'
                }]);
            });
        }

        function clearAllErrors() {
            clearUsernameErrors();
            clearPasswordErrors();
            clearPasswordConfirmErrors();
        }

        function clearUsernameErrors() {
            $('span[data-error-for="email"]').each(function () {
                $(this).removeClass(activeClass);
            });
        }

        function clearPasswordErrors() {
            $('span[data-error-for="password"]').each(function () {
                $(this).removeClass(activeClass);
            });
        }

        function clearPasswordConfirmErrors() {
            $('span[data-error-for="passwordConfirm"]').each(function () {
                $(this).removeClass(activeClass);
            });
        }

        function clearError(error) {
            var errorElement = $('#' + errorCodeToElement[error]);
            if (errorElement) {
                errorElement.removeClass(activeClass);
            }
        }

        function showErrors(errors) {
            errors.forEach(function (error) {
                showError(error);
            });
        }

        function showError(error) {
            var errorElement = $('#' + errorCodeToElement[error]);
            if (errorElement) {
                errorElement.addClass(activeClass);
            }
        }

