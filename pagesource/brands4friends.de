        $('[id^=js-register-]').click(function (e) {
            removeProductImagesFromRegistrationLayer();
            removeTargetingInfoFields();
        });

        $('.js_register_overlay_with_products').click(function (e) {
            var masterId = $(this).data('masterid');
            var renderModel = brandCarpetRenderModel[masterId];
            $('#overlay_register').find('.overlay-content').addClass('has-product-images');
            $('#overlay_register').find('.product-img-wrapper').html($.mustache(template, renderModel));

            addTargetingInfoFieldsToRegistrationForm('b4f_br', renderModel.brandName);
        });
        

        $('.js-register-campaign').click(function (e) {
            removeProductImagesFromRegistrationLayer();

            var productSetId = $(this).data('productsetid');
            addTargetingInfoFieldsToRegistrationForm('b4f_ps', productSetId);
        });

        $('.stage-1st .button').click(function (event) {
            var value = 'Mein erstes Mal Club-Shoppen Slide ' + getStageSlideIndexValue() + ' von 6';
            var buttonText = $(event.target).text().trim();
            _eno.push(['Navigation', {action: buttonText, stringValue: value}]);
        });

        $('#js-register-mobile').click(function (event) {
            var targetInfo = $('input#mobileTargetInfo');
            if (targetInfo && targetInfo.attr('value')) {
                var targetInfoSplit = targetInfo.attr('value').split('=');
                addTargetingInfoFieldsToRegistrationForm(targetInfoSplit[0], targetInfoSplit[1]);
            }

            var buttonText = $(event.target).text().trim();
            _eno.push(['Navigation', {action: buttonText, stringValue: 'Sticky Mobile Footer'}]);
        });

        $('.stage-crm .button').click(function (event) {
            removeProductImagesFromRegistrationLayer();

            var targetInfo = $('section.stage-crm div.slick-current input#targetInfo');
            if (targetInfo && targetInfo.attr('value')) {
                var targetInfoSplit = targetInfo.attr('value').split('=');
                addTargetingInfoFieldsToRegistrationForm(targetInfoSplit[0], targetInfoSplit[1]);
            }

            var value = 'Promo Slide ' + getCrmStageSlideIndexValue() + ' von ' + getCrmStageLastSlideIndexValue();
            var buttonText = $(event.target).text().trim();
            _eno.push(['Navigation', {action: buttonText, stringValue: value}]);
        });

        function getStageSlideIndexValue() {
            return $('section.stage div.slick-current').data('slick-index') + 1;
        }

        function getCrmStageSlideIndexValue() {
            return $('section.stage-crm div.slick-current').data('slick-index') + 1;
        }

        function getCrmStageLastSlideIndexValue() {
            return $('section.stage-crm .slick-track > div:last-child').data('slick-index');
        }

        function removeProductImagesFromRegistrationLayer() {
            $('#overlay_register').find('.overlay-content').removeClass('has-product-images');
            $('#overlay_register').find('.product-img-wrapper').html("");
        }

        function removeTargetingInfoFields() {
            $('input[name=b4f_br]').remove();
            $('input[name=b4f_ps]').remove();
        }
    });

    jQuery(function ($) {
