<script type="text/x-handlebars-template" id="js-delivery-options-template">
	<div class="mdo-modal-store-container">
		<h2 class="mdo-modal-strong">Your delivery options for <span class="mdo-modal-highlight-text">{{address.Suburb}}, {{address.State}} {{address.Postcode}}</span>*</h2>
	</div>
		
	<div class="mdo-modal-delivery-breakdown">
		<table>
			<thead>
				<tr>
					<th><div class="mdo-modal-strong">Delivery Time Frames *</div></th>
					<th><div class="mdo-modal-strong">Price</div></th>
				</tr>
			</thead>
			<tbody>
				{{#each deliveryOptions}}
					{{#if this.Available}}
						<tr class="mdo-modal-delivery-option">
							<td>
								<div class="mdo-modal-strong">{{this.Name}}</div>
								<div class="mdo-modal-delivery-detail">{{this.Description}}</div>
							</td>
							<td>
								<div class="mdo-modal-price">${{this.Price}}</div>
							</td>
						</tr>
					{{else}}
						<tr class="mdo-modal-delivery-option mdo-is-not-available">
							<td>
								<div class="mdo-modal-strong">{{this.Name}}</div>
								<div class="mdo-modal-delivery-detail">{{this.Description}}</div>
							</td>
							<td>
								<div class="mdo-modal-price">${{this.Price}}</div>
							</td>
						</tr>
					{{/if}}
				{{/each}}
			</tbody>
		</table>
	</div>

	<div class="mdo-modal-disclaimer">* Delivery cannot occur on a gazetted public holiday</div>
</script>


