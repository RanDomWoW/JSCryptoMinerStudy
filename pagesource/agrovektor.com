<script type="text/javascript">

//SetHints('qsearch_form');

function shAutocomplete()
{
    $("#search_index_input1").autocomplete({
        source: function( request, response ) {
            $.ajax({
                url: "/lib/sh/php/search_index_autocomplete.php",
                dataType: "json",
                data:
                {
                    term: request.term,
                    country_id: $("#shCountryID").val(),
                    search_type: $("#shSearchType").val()
                },
                success: function(data)
                {
                    response($.map(data, function(item)
                    {
                        return {
                            label: item.title,
                            id: item.title
                        };
                    }));
                }
            });
        },
        minLength: 3
    });

}


$(document).ready(function(){
    shAutocomplete();

    $("form#qsearch_form_id").submit(function(event){
        event.preventDefault();
        
        //ClearHint("search_index_input1");
        var txt = $("#search_index_input1").val().trim();
        txt = FreeText(txt);
        $("#search_index_input1").val(txt);
        if (txt.length < 3)
        {
            alert("Для поиска введите более 2-х символов!");
            return false;
        }
        var href = qsearch_form.action;
        if ($("#search-region").val() != 0) href += "/region/"+$("#search-region").val();
        window.location.href = href + "/free_text/"+FreeTextURL(txt)+".html";
    });

});
