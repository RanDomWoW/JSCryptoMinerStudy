<script type="text/javascript">

var timeout_get_price;
function ShopGetPrice(input)
{
    clearTimeout(timeout_get_price);
    timeout_get_price = setTimeout(function () { input.change() }, 1000);
}

function browserDetectNav(chrAfterPoint)
{
var
    UA=window.navigator.userAgent,       // содержит переданный браузером юзерагент
    //--------------------------------------------------------------------------------
    OperaB = /Opera[ \/]+\w+\.\w+/i,     //
    OperaV = /Version[ \/]+\w+\.\w+/i,   //
    FirefoxB = /Firefox\/\w+\.\w+/i,     // шаблоны для распарсивания юзерагента
    ChromeB = /Chrome\/\w+\.\w+/i,       //
    SafariB = /Version\/\w+\.\w+/i,      //
    IEB = /MSIE *\d+\.\w+/i,             //
    SafariV = /Safari\/\w+\.\w+/i,       //
        //--------------------------------------------------------------------------------
    browser = new Array(),               //массив с данными о браузере
    browserSplit = /[ \/\.]/i,           //шаблон для разбивки данных о браузере из строки
    OperaV = UA.match(OperaV),
    Firefox = UA.match(FirefoxB),
    Chrome = UA.match(ChromeB),
    Safari = UA.match(SafariB),
    SafariV = UA.match(SafariV),
    IE = UA.match(IEB),
    Opera = UA.match(OperaB);
         
        //----- Opera ----
        if ((!Opera=="")&(!OperaV=="")) browser[0]=OperaV[0].replace(/Version/, "Opera")
                else
                    if (!Opera=="") browser[0]=Opera[0]
                        else
                            //----- IE -----
                            if (!IE=="") browser[0] = IE[0]
                                else
                                    //----- Firefox ----
                                    if (!Firefox=="") browser[0]=Firefox[0]
                                        else
                                            //----- Chrom ----
                                            if (!Chrome=="") browser[0] = Chrome[0]
                                                else
                                                    //----- Safari ----
                                                    if ((!Safari=="")&&(!SafariV=="")) browser[0] = Safari[0].replace("Version", "Safari");
//------------ Разбивка версии -----------
 
    var
            outputData;                                      // возвращаемый функцией массив значений
                                                             // [0] - имя браузера, [1] - целая часть версии
                                                             // [2] - дробная часть версии
    if (browser[0] != null) outputData = browser[0].split(browserSplit);
    if ((chrAfterPoint==null)&&(outputData != null))
        {
            chrAfterPoint=outputData[2].length;
            outputData[2] = outputData[2].substring(0, chrAfterPoint); // берем нужное ко-во знаков
            return(outputData);
        }
            else return(false);
}

var shCloseAll = true;
var brdata = browserDetectNav();
var shShopCount = 0;
var shSiteURL = "http://agrovektor.com/";

ShopEdit = function(id_type, count)
{
    $.post(
        "http://"+document.domain+"/lib/sh/php/shop.php",
        {
            "mode" : "edit",
            "id_type" : id_type,
            "count" : count
        },
        function(data)
        {
            $('#rowShop_'+id_type+' .shop-price').text(data);
        }
    );    
}
ShopDelete = function(id_type)
{
    if (brdata[0] == "MSIE") shCloseAll = false;
    
    $(window).unbind('focus');
    if (confirm("Удалить позицию?"))
    {
        $.post(
            "http://"+document.domain+"/lib/sh/php/shop.php",
            {
                "mode" : "delete",
                "id_type" : id_type
            },
            function(data)
            {
                shShopCount = data;
                $('#aShopButton').html("КОРЗИНА ЗАКАЗОВ ("+shShopCount+")");
                $("#rowShop_"+id_type).remove();
            }
        );
    }    
    $(window).focus(function(e) { shWindowFocus(); });
}
ShopData = function()
{
    $.post(
        "http://"+document.domain+"/lib/sh/php/shop.php",
        {"mode" : "data"},
        function(data)
        {
            $('#divShopContent').html(data);
        }
    );    
}
ShopClear = function()
{
    if (brdata[0] == "MSIE") shCloseAll = false;
    if (!confirm("Очистить корзину полностью?")) return;
    
    $.post(
        "http://"+document.domain+"/lib/sh/php/shop.php",
        {"mode" : "clear"},
        function(data)
        {
            shShopCount = data;
            $('#aShopButton').html("КОРЗИНА ЗАКАЗОВ ("+shShopCount+")");
            ShopClose();
        }
    );    
}
ShopCount = function()
{
    $.post(
        "http://"+document.domain+"/lib/sh/php/shop.php",
        {"mode" : "count"},
        function(data)
        {
            if (data != shShopCount && brdata[0] == "Opera")
            {
                ShopMailClose();
                ShopClose();
            }

            shShopCount = data;
            $('#aShopButton').html("КОРЗИНА ЗАКАЗОВ ("+shShopCount+")");
        }
    );    
}
ShopOpen = function()
{
    //Закрываем recall me
    $("#sh-recallme-div-par").hide();
    $(".sh-popup2").hide();
    
    if ($("#divShop").is(':visible'))
    {
         ShopClose();
    }
    else
    {
        ShopData();
        $("#divShop").fadeIn("normal");
    }

    if (brdata[0] == "MSIE") shCloseAll = false;
    ShopMailClose();
    
    //alert($("#divShop_in").height());
    //alert($("#divShop_in").outerHeight());
}
ShopClose = function()
{
    $("#divShop").fadeOut("normal", shShopInWorkCheck);
}
ShopAdd = function(content_id, content_type, title, price, comp_name, comp_id, count, detail_id)
{
    $("#divShop").hide();
                    
    if (count == null) count = 1;
    if (detail_id == null) detail_id = 0;
    
    $(document.body).addClass('wait');
    
    $.post(
        "http://"+document.domain+"/lib/sh/php/shop.php",
        {
            "mode" : "add",
            "content_id" : content_id,
            "content_type" : content_type,
            "count" : count,
            "title" : title,
            "price" : price,
            "comp_name" : comp_name,
            "comp_id" : comp_id,
            "detail_id" : detail_id
        },
        function(data) {
            if (data == "alredy exists")
                alert("Позиция уже добавлена в корзину!")
            else {
                $('#aShopButton').fadeOut("normal");
                shShopCount = data;
                $('#aShopButton').html("КОРЗИНА ЗАКАЗОВ ("+shShopCount+")");
                $('#aShopButton').fadeIn("normal");
                ShopOpen();
            }
        }
    )
    .always(function() {
        $(document.body).removeClass('wait');
    });
    
    //ShopMailClose();
    //ShopClose();
    return false;
}

ShopMailOpen = function()
{
    ShopMailData();
    $("#divShopMail").fadeIn("normal", shShopInWorkCheck);

    if (brdata[0] == "MSIE") shCloseAll = false;
    ShopClose();
}
ShopMailClose = function()
{
    $("#divShopMail").fadeOut("normal", shShopInWorkCheck);
}
ShopMailData = function()
{
    $.post(
        "http://"+document.domain+"/lib/sh/php/shop.php",
        {
            "mode" : "data_mail_header"
        },
        function(data)
        {
            $("#divShopMailHeader").html(data);
            
            $.post(
                "http://"+document.domain+"/lib/sh/php/shop.php",
                {
                    "mode" : "data_mail_content"
                },
                function(data)
                {
                    $("#divShopMailContent").html(data);
                }
            );    
        }
    );    
}
ShopMailEdit = function(mode, type, val)
{
    $.post(
        "http://"+document.domain+"/lib/sh/php/shop.php",
        {
            "mode" : mode,
            "type" : type,
            "val" : val
        },
        function(data)
        {
        }
    );    
}

ShopSend = function()
{
    $(".sh-shop-send-alert").hide();
    $.post(
        "http://"+document.domain+"/lib/sh/php/shop.php",
        {
            "mode" : "send"
        },
        function(data)
        {
            if (data == "mail_is_send")
            {
                alert("Ваш заказ отправлен!..");
                //Закроется и перечитается
            }
            else
            {
                if (data.indexOf("-fio") !== -1)
                    $("#sh-shop-send-alert-fio").show();
                if (data.indexOf("-email") !== -1)
                    $("#sh-shop-send-alert-email").show();
                if (data.indexOf("-phone") !== -1)
                    $("#sh-shop-send-alert-phone").show();
                /*
                if (brdata[0] == "MSIE") shCloseAll = false;
                $(window).unbind('focus');
                alert(data);
                $(window).focus(function(e) { shWindowFocus(); });
                */
            }
        }
    );
    
    return false;
}

shShopInWorkCheck = function()
{
    if ($("#divShop").is(':visible') || $("#divShopMail").is(':visible'))
    {
        $('html, body').css({
            'overflow': 'hidden',
            'height': '100%'
        });
    }
    else
    {
        $('html, body').css({
            'overflow': '',
            'height': ''
        });
    }
    $(window).resize();
}


ShopCount();

shWindowFocus = function()
{
    if (shCloseAll && brdata[0] != "Opera")
    {        
        ShopMailClose();
        ShopClose();
    }
    ShopCount();
    shCloseAll = true;
}

$(window).focus(function(e){
    shWindowFocus();
});

$(window).resize(function() {
    var H = $(window).height();
    var h1 = $("#divShop_in").outerHeight();
    var h2 = $("#divShopMail_in").outerHeight();
    var h26 = 26;
    
    if (h1 > H-h26)
        $("#divShop_in").parent("div.divBottomContainer").css("top", "0px");
    else if (h1 < H-h26)
        $("#divShop_in").parent("div.divBottomContainer").css("top", "");
  
    if (h2 > H-h26)
        $("#divShopMail_in").parent("div.divBottomContainer").css("top", "0px");
    else if (h2 < H-h26)
        $("#divShopMail_in").parent("div.divBottomContainer").css("top", "");
});

</script>

