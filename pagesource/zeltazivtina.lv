<script type="text/javascript">
    $(document).ready(function() {
        $('.zzmi_fog, .naczz_pop .close').click(function() {
            $('.naczz_pop_elem_nac').hide();
        });
        $('#naczz').click(function() {
            //ga('send', 'event', 'NacZZ', 'OpenPopup');
        });
        $('#sidepopform').submit(function(){
            $('#sidepopform .cinput_error_msg').hide();

            $('#sidepopres2').hide();
            $('#nac_done2').hide();
            $('#nac_loader').hide();

            var v=$('#contpop_tel').val();
            v=v.replace(' ','');
            if (v.substring(0,1)=='+') {
                v=v.substring(1);
                $('#contpop_tel').val(v);
            }
            if (v.substring(0,3)=='371') {
                v= v.substring(3);
                $('#contpop_tel').val(v);
            }

            if (!v) {
                $('#contpop_tel').parent().parent().find(".error_noinfo").show();
                return false;
            }

            var cok=true;
            if (v.length>8 || v.length<8) {
                cok = false;
            }
            var v2=v.replace(/[^\d]/,'');
            if (v2!=v) {
                cok = false;
            }
            if (!cok) {
                $('#contpop_tel').parent().parent().find(".error_errinfo").show();
                return false;
            }

            $('#contpop_send').hide();
            $('#nac_loader').show();
            var ok=true;
            $.ajax({
                dataType: "json",
                url: '?cmd=get&cid=4009&check_number='+$('#contpop_tel').val(),
                beforeSend: function(ob){
                    $('.nac_vispiet').hide();
                    $('#nac_loader').show();
                },
                success: function(data) {
                    $('#nac_loader').hide();
                    $('.nac_vispiet').show();
                    if (data.error) {
                        if (data.final) {
                            $('.nac_vispiet').hide();
                            $('#sidepopres2').show();
                            $('#sidepopres2').html(data.error);
                        } else {
                            $('#sidepopres2').show();
                            $('#contpop_send').show();
                            $('#sidepopres2').html(data.error);
                        }
                        ok = false;
                        return false;
                    }
                    if (data.t2) {
                        $('.nac_vispiet').hide();
                        $('#nac_done2').show();
                        ok = false;
                        return false;
                    }

                    var urlv = 'index.php?cmd=popup&cid=4009';
                    urlv = urlv+'&contpop_send=CH987sa';
                    urlv = urlv+'&name='+encodeURIComponent($('#contpop_name').val());
                    urlv = urlv+'&tel='+encodeURIComponent($('#contpop_tel').val());

                    $.ajax({url:urlv,dataType:'json',beforeSend: function(ob){
                        $('.nac_vispiet').hide();
                        $('#nac_loader').show();
                    }}).done(function(data) {
                        $('.nac_vispiet').show();
                        $('#nac_loader').hide();
                        if(data.ok==1) {
                            $('.nac_vispiet').hide();
                            $('#nac_done').show(function(){$.fancybox.resize();});
                            //ga('send', 'event', 'NacZZ', 'ClickOk');
                            dataLayer.push({'event': 'success_msg','alert_msg': 'NacZZ success'});
                        } else {
                            $('.nac_vispiet').hide();
                            $('#sidepopres2').show();
                            //$('#contpop_send').show();
                            $('#sidepopres2').html(data.err);
                        }
                    })
                        .fail(function() {
                            $('#nac_loader').hide();
                            $('.nac_vispiet').show();
