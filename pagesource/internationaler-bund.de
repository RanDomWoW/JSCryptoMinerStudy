				ga('send', 'event', trackingdata[0], trackingdata[1], trackingdata[2]);
			});

			/*
			 * use class="ib-tracking-on-load" (once per page) and set
			 * data-ibtrackingdata = "category,action,label"
			 */

			ibTrackOnLoad();

			function ibTrackOnLoad() {
				var ibReferrer = ['ibrelaunch2016.rmsdev.de', 'ibt3.local.mac', 'internationaler-bund.de', 'www.internationaler-bund.de', 'www.ib-kitas.de', 'ib-kitas.de', 'ib-freiwilligendienste.de', 'www.freiwilligendienste.de'];

				var checkElement = $('.ib-tracking-on-load');
				if (checkElement.length) {
					var trackingdata = $('.ib-tracking-on-load').data('ibtrackingdata');
					trackingdata = trackingdata.split(',');
					if ($.inArray(document.referrer.split( '/' )[2], ibReferrer) == -1) {
						trackingdata[2] = 'Direktaufruf';
						if (getParameterByName('t') != null) {
							trackingdata[2] = 'Standortsuche';
						}

					} else if (getParameterByName('t') != null) {
						trackingdata[2] = 'Standortsuche';
					}
					ga('send', 'event', trackingdata[0], trackingdata[1], trackingdata[2]);

				}
			}

			function getParameterByName(name, url) {
				if (!url) {
					url = window.location.href;
				}
				name = name.replace(/[\[\]]/g, "\\$&");
				var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
				    results = regex.exec(url);
				if (!results)
					return null;
				if (!results[2])
					return '';
				return decodeURIComponent(results[2].replace(/\+/g, " "));
			}

