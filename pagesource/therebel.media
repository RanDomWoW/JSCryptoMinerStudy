<script type="text/javascript">

  // Returns true if the specified element has been scrolled into the viewport.
function isElementInViewport(elem) {
    var $elem = $(elem);

    // Get the scroll position of the page.
    var scrollElem = ((navigator.userAgent.toLowerCase().indexOf('webkit') != -1) ? 'body' : 'html');
    var viewportTop = $(scrollElem).scrollTop();
    var viewportBottom = viewportTop + $(window).height();

    // Get the position of the element on the page.
    var elemTop = Math.round( $elem.offset().top );
    var elemBottom = elemTop + $elem.height();

    return ((elemTop < viewportBottom) && (elemBottom > viewportTop));
}
// Check if it's time to start the animation.
function checkAnimation() {

   var $elem = $( ".bar .level" );

   if ( isElementInViewport( $elem ) ) {
     
      // If the animation has already been started
      if ( $elem.hasClass( "start" ) ) {
         return
      };
     
      var maxValue = $( ".maxvalue" ).text();
      var curValue = $( ".curvalue" ).text();
      //alert(maxValue + " - "+ curValue);
   
      // Start the animation
      $elem.addClass( "start" );
      $elem.each( function() {
         $( this )
            .prop( "Counter", 0 )
            .animate( {
               Counter: curValue
            }, {
               duration: 3500,
               easing: "swing",
               step: function( now ) {
                  //console.log( this.Counter );
                  
                 
                  var percentVal = Math.ceil( now ) / maxValue * 100;
                  var ordinalVal = Math.ceil( now / 25 ) * 25;
                 
                  if ( Math.ceil( now ) >= maxValue ) {
                     percentVal = 100;
                  }
                 
                  if (Math.ceil( now ) > ordinalVal - 200){
                      ordinalVal =  Math.ceil( now );
                  }
                 
                  if (percentVal < 50){
                      $(".ordinal").addClass("leftward");
                  } else{
                      $(".ordinal").removeClass("leftward");
                  }
                  
                 
                  $( this )
                     .css( "width", percentVal + "%" )
                     .find( ".ordinal" )
                     .text( Math.ceil( ordinalVal ) );
               }
            } );
      } );
   } else {   
      // Start the animation
      $elem.removeClass( "start" );
   }
}

// Capture scroll events
$(window).scroll(function(){
  if ($(".bar").length){
    checkAnimation();
  }
});

  
  checkAnimation();
  
 </script>




