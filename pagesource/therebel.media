<script type="text/javascript">
//Start after window load so not messy jumping effect.
// else menu appear top 0 left 0 than jump calculated position
// TODO : If page loads when scrolled way down , menu placed wrong position.So check position for it.

  //if there is at least one header then continue
if (  $("#content h4.nav").length == 40) {

	//add side menu 
	$( "#layout-content-container" ).before( " <div class='side-menu'>&nbsp;</div>" );
	
	//add padding to content
	

	//hide page sharing tools
	//$("#share-widget-buttons li").remove();
	//$("#share-widget-buttons").remove();
	$("#share-widget-buttons").detach().prependTo("#subpage-sidebar");
	$("#subpage-image").remove();
	$("#share-buttons").remove();

  // set menu content
  var menuHTML = "<ul>";
  $("#content h4.nav[id]").each(function() {
    menuHTML += "<li><a href='#" + $(this).attr('id') + "'> " + $(this).text() + "</a></li>";
  });
  $(".side-menu").html(menuHTML + "</ul>");

  var container = $("#content"), // Container Div
    containerPos = container.offset(), // Container Offset
    containerWidth = container.width(), //Container Width
    margin = 10, // Menu Margin to Container
    menu = $(".side-menu"),
    menuWidth = menu.width();
  // Lets Set Menu left location
  menu.css({
    // Menu top position must be container offset top.
    top: 0,
    // Menu left position Container Width + Container Offset Left + Margin
    left: -menuWidth -margin
  });

  //onScroll
  $(window).scroll(function() {
    //if scroll between top and bottom of container, then set the margintop of the menu
    if (
      $(window).scrollTop() >= containerPos.top && ($(window).scrollTop() <= (containerPos.top + container.height() - menu.height())) ) {
      menu.css({
        "margin-top": $(window).scrollTop() - containerPos.top - 20,
        "opacity": .5
      });
    } //if past the top but not less than the bottom
    else if ($(window).scrollTop() >= containerPos.top) {
      menu.css({
        "margin-top": container.height() - menu.height(),
        "opacity": .5
      });
    } //reset to top of container
    else {
      //Dont ask
      menu.css({
        "margin-top": 0,
        "opacity": 1
      });
    }
  });
  } else {
    //hide sidewrapper
    $(".side-menu").hide();
  }

 //smooth scroll
  $(function() {
  $('.side-menu a').click(function(event) {
   if (location.pathname.replace(/^\//, '') == this.pathname.replace(/^\//, '') && location.hostname == this.hostname) {
      var target = $(this.hash);
      target = target.length ? target : $('[name=' + this.hash.slice(1) + ']');
      if (target.length) {
        $('html,body').animate({
          scrollTop: target.offset().top
        }, 1000); // The number here represents the speed of the scroll in milliseconds
        return false;
      }
    }
  });
});


</script>
