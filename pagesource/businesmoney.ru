<script>
window.current_tab=1;
function switch_tab(tabn,stop){
window.current_tab=tabn;
if(stop==1)clearInterval(window.tab_interval);
$('#tabs #s1').removeClass('active');
$('#tabs #s2').removeClass('active');
$('#tabs #s3').removeClass('active');
$('#tabs #s4').removeClass('active');

$('#tab-content #d1').hide();
$('#tab-content #d2').hide();
$('#tab-content #d3').hide();
$('#tab-content #d4').hide();

$('#tab-content #d'+tabn).fadeIn('slow');

$('#tabs #s'+tabn).addClass('active');

}


function go_tabs(){
if(window.current_tab<4)
switch_tab(window.current_tab+1,0);else switch_tab(1);
}
window.tab_interval=setInterval('go_tabs()',3000);
