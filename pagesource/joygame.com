                if (e["data"] == YT.PlayerState.PAUSED) {
                    dataLayer.push({
                        event: "youtube",
                        action: "pause",
                        label: label
                    });
                    YT.gtmLastAction = "p";
                }
            }

            function onPlayerError(e) {
                dataLayer.push({
                    event: "error",
                    action: "GTM",
                    label: "youtube:" + e
                })
                console.info(dataLayer)

            }

            function onPlayerPercent(e) {

                if (e["j"]["playerState"] == YT.PlayerState.PLAYING && e["j"]["duration"] < e["j"]["currentTimeLastUpdated_"]) {

