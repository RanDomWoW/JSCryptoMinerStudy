                    var label = video_data.video_id + ':' + video_data.title;

                    e["j"]["currentTimeLastUpdated_"] = t;

                    dataLayer.push({
                        event: "youtube",
                        action: t * 100 + "%",
                        label: label
                    });
                    gaVideoEvents.video_time = t * 100 + "%";

                    e["j"]["currentTimeLastUpdated_"] != 1 && setTimeout(onPlayerPercent, 1000, e);
                }


            }

            window.onbeforeunload = function (e) {
                var e = e || window.event;
                if (e)
