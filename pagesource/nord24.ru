<script type="text/javascript">
var logoSlider = (function(totalItems, blockWidth, sliderId, visibleItems, speed){

    var delay; // setInterval instance
    var counter = 0;
    return {
        'init': function() {
            this.setWidth();
            this.setEvents();
            this.startDelay();
            this.checkControls();
        },
        'setWidth': function() {
            var el = document.getElementById(sliderId);
            el.style.width = (blockWidth * totalItems) + 'px';
        },
        'scrollRight': function() {
            //console.log('totalItems=' + totalItems + ' visibleItems=' + visibleItems + ' counter=' + counter);
            if ((counter + visibleItems) == totalItems) { // scroll at start
                logoSlider.stopDelay();
                console.log('logo slider stop delay');
                return;
            }
            logoSlider.disableEvents();
            $("#"+sliderId+'Container').animate({ scrollLeft: Math.max(blockWidth, $("#"+sliderId+'Container').scrollLeft() + blockWidth)}, {
                    duration: speed,
                    complete: function() {
                        logoSlider.setEvents();
                    }
                });
            ++counter;
            logoSlider.checkControls();
            googleAnalyticsShow('#' + sliderId, counter + visibleItems - 1);
        },
        'clickRight': function() {
            //console.log('totalItems=' + totalItems + ' visibleItems=' + visibleItems + ' counter=' + counter);
            if ((counter + visibleItems) == totalItems) { // scroll at start
                return;
            }
            $("#"+sliderId+'Container').animate({ scrollLeft: Math.max(blockWidth, $("#"+sliderId+'Container').scrollLeft() + blockWidth)}, {
                duration: speed,
                complete: function() {

                }
            });
            ++counter;
            window[sliderId].checkControls();
            googleAnalyticsShow('#' + sliderId, counter + visibleItems - 1);
        },
        'scrollLeft': function() {
            if (counter == 0) { // scroll to the end
                return;
            }
            logoSlider.disableEvents();
            $("#"+sliderId+'Container').animate({ scrollLeft: Math.max(0, $("#"+sliderId+'Container').scrollLeft() - blockWidth)}, {
                    duration: speed,
                    complete: function() {
                        logoSlider.setEvents();
                    }
                });
            --counter;
            logoSlider.checkControls();
        },
        'clickLeft': function() {
            //console.log('totalItems=' + totalItems + ' visibleItems=' + visibleItems + ' counter=' + counter);
            if (counter == 0) { // scroll to the end
                return;
            }
            $("#"+sliderId+'Container').animate({ scrollLeft: Math.max(0, $("#"+sliderId+'Container').scrollLeft() - blockWidth)}, {
                duration: speed,
                complete: function() {

                }
            });
            --counter;
            window[sliderId].checkControls();
            googleAnalyticsShow('#' + sliderId, counter);
        },
        'checkControls': function() {
            $('#'+sliderId+'Left-icon').attr('class', 'sprite sprite-logoSliderLeft');
            $('#'+sliderId+'Right-icon').attr('class', 'sprite sprite-logoSliderRight');
            if (counter == 0) {
                $('#'+sliderId+'Left-icon').attr('class', 'sprite sprite-logoSliderLeft-gray');
            }
            if ((counter + visibleItems) == totalItems) {
                $('#'+sliderId+'Right-icon').attr('class', 'sprite sprite-logoSliderRight-gray');
            }
        },
        'setEvents': function() {
            $('#'+sliderId+'Container').hover(this.stopDelay);
            $('#'+sliderId+'Right-icon').hover(this.stopDelay);
            $('#'+sliderId+'Left-icon').hover(this.stopDelay);

            $('#'+sliderId+'Right').click(this.clickRight);
            $('#'+sliderId+'Left').click(this.clickLeft);
        },
        'disableEvents': function() {
            $('#'+sliderId+'Right').off();
            $('#'+sliderId+'Left').off();
        },
        'startDelay': function() {
            window.logo_slider_delay = setInterval(logoSlider.scrollRight, 3500);
        },
        'stopDelay': function() {
            clearInterval(window.logo_slider_delay);
        }
    }
})(5, 162, 'logoSlider', 5, 500);
logoSlider.init();
</script>



