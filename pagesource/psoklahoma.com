<script type="text/javascript">
    var OPCOName = 'PSO';
    var originalWelcomeName = '';
    var hasTruncatedName = false;
    var logoWidth = 190;
    $(document).ready(function(){
        if ($('[id*="LiWelcome"]').length > 0) {
            originalWelcomeName = $('[id*="LiWelcome"]').text();
            // determine offset based on OPCO logo size
            switch (OPCOName) {
                case 'AEP':
                    logoWidth = 170; break;
                case 'AEP Ohio':
                    logoWidth = 120; break;
                case 'AEP Texas':
                    logoWidth = 135; break;
                case 'APCO':
                    logoWidth = 200; break;
                case 'I&M':
                    logoWidth = 170; break;
                case 'Kentucky Power':
                    logoWidth = 175; break;
                case 'PSO':
                    logoWidth = 215; break;
                case 'SWEPCO':
                    logoWidth = 225; break;
            }
            truncateWelcomeName();
            $(window).on('resize', Foundation.utils.debounce(function (e) {
                if ($('[id*="LiWelcome"]').is(":visible")) {
                    truncateWelcomeName();
                }
            }, 300));
        }
    });
    function truncateWelcomeName() {
        // get width of viewport
        var welcomeName = $('[id*="LiWelcome"]').text();
        var utilityNavWidth = $('.utility-nav ul.inline-list').width();
        var documentWidth = $(document).width();
        var hasEllipsis = welcomeName.indexOf('...') >= 0;
       
        var counter = 0;
        // check if name is too close or overlaps logo
        if ((utilityNavWidth + logoWidth) > documentWidth) {
            while ((utilityNavWidth + logoWidth) > documentWidth && counter < originalWelcomeName.length) {
                hasTruncatedName = true;
                counter++;
                // do not display name if less than 5 characters unless their full name is less than 5 characters
                if (welcomeName.length < 8 && originalWelcomeName.length >= 5) {
                    $('[id*="LiWelcome"]').text("");
                    utilityNavWidth = $('.utility-nav ul.inline-list').width();
                } else {
                    // remove last character from name
                    if (hasEllipsis) {
                        welcomeName = welcomeName.substring(0, welcomeName.length - 4) + '...';
                        $('[id*="LiWelcome"]').text(welcomeName);
                        utilityNavWidth = $('.utility-nav ul.inline-list').width();
                    } else {
                        welcomeName = welcomeName.substring(0, welcomeName.length - 2) + '...';
                        hasEllipsis = true;
                        $('[id*="LiWelcome"]').text(welcomeName);
                        utilityNavWidth = $('.utility-nav ul.inline-list').width();
                    }
                }
            };
        } else if(hasTruncatedName) {
            // check to see if we can add letters to name, 15 is max width of 'W'
            while ((utilityNavWidth + logoWidth + 15) < documentWidth && welcomeName != originalWelcomeName && counter < originalWelcomeName.length) {
                counter++;
                if (welcomeName.length >= originalWelcomeName.length) {
                    welcomeName = originalWelcomeName;
                    hasTruncatedName = false;
                } else {
                    // remove ellipsis
                    var truncated = welcomeName.substring(0, welcomeName.length - 3);
                    welcomeName = originalWelcomeName.substring(0, truncated.length + 1) + "...";
                }
                $('[id*="LiWelcome"]').text(welcomeName);
                utilityNavWidth = $('.utility-nav ul.inline-list').width();
            }
            // do not display name if less than 5 characters unless their full name is less than 5 characters
            if (welcomeName.length < 8 && originalWelcomeName.length >= 5) {
                $('[id*="LiWelcome"]').text("");
            }
        }
       
    }
     
</script>
</span>
