		    //!preg_match("/^[a-zA-Z ]*$/",x)
			document.getElementById("error1"+fname).innerHTML = "Invalid Name";
		    str =false;
		}
		if(p !== '') {
			if( l > 255 ){
				document.getElementById("error3"+fname).innerHTML = "Invalid Number";
				str =false;
			}
		}
		if(y == '')
		{
		    document.getElementById("error2"+fname).innerHTML = "Required Field";
		    str =false;
		}
		else if (!re.test(y)) {
