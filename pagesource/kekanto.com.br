        var xhr = new XMLHttpRequest();

        xhr.open('POST',
                 'https://3549nhhxx4.execute-api.us-east-1.amazonaws.com/prod/loganalytics',
                 true);

        xhr.setRequestHeader('Content-Type', 'application/json');
        xhr.setRequestHeader('X-Api-Key', X_API_KEY);

        xhr.onload = function() {

            if (xhr.status === 200) {
                console.log('success log');
            }
            else if (xhr.status !== 200) {
                console.log('failed log');
            }
        };

        // send the collected data as JSON
        xhr.send(JSON.stringify(data));
    }

    function loganalytics_pageview(append_pageview) {

        var data = get_data_log('pageview' + append_pageview, {});
        send_log(data);
    }

    function loganalytics_event(action_1, action_2, action_3) {

        var actions = {'action_1':action_1};

        if (typeof action_2 != 'undefined' && action_2 !== null) {
            actions['action_2'] = action_2;
        }

        if (typeof action_3 != 'undefined' && action_3 !== null) {
            actions['action_3'] = action_3;
        }

        var data = get_data_log('event', actions);
        send_log(data);
    }

        // chama pageview pro loganalytics
    loganalytics_pageview('');
            
