<script type="text/javascript">
    function AJAXInteraction(div, url, callback) {
        var req = init();
        req.onreadystatechange = processRequest;
        function init() {
            if (window.XMLHttpRequest) {
                return new XMLHttpRequest()
            } else if (window.ActiveXObject) {
                return new ActiveXObject("Microsoft.XMLHTTP")
            }
        }

        ;
        function processRequest() {
            if (req.readyState == 4) {
                if (req.status == 200) {
                    if (callback) {
                        if (document.getElementById(div) != null) {
                            callback(document.getElementById(div).innerHTML = req.responseText);
                            overlayopen(url, "overlaycontentsDIV", "blackoverlay")
                        }
                    }
                }
            }
        }

        ;
        this.doGet = function () {
            req.open("GET", url, true);
            req.send(null)
        };
        this.doPost = function (body) {
            req.open("POST", url, true);
            req.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
            req.send(body)
        }
    }
    function makeRequest(url, div) {
        var ai = new AJAXInteraction(div, url, function () {
        });
        ai.doGet()
    }
    var rnd = Math.floor(Math.random() * 100000000);
    function IsImageOk(img) {
        if (!img.complete) {
            return false;
        }
        if (typeof img.naturalWidth != "undefined" && img.naturalWidth == 0) {
            return false;
        }
        if (img.width == 1) {
            return false;
        }
        return true;
    }
    function chkimg(id, imgsrc, link) {
        var obj = document.getElementById(id);
        if (obj) {
            var obj_img = obj.getElementsByTagName("img")[0];
            if (typeof obj_img == "undefined") {
//                makeRequest("include-home/hitcounter.php?rnd=" + rnd + "&b=" + id);
                obj.innerHTML = "<a target='_blank' href='" + link + "'><img alt='' src='" + imgsrc + "'/></a>";
            } else {
                if (!IsImageOk(obj_img)) {
//                    makeRequest("include-home/hitcounter.php?rnd=" + rnd + "&b=" + id);
                    obj.innerHTML = "<a target='_blank' href='" + link + "'><img alt='' src='" + imgsrc + "'/></a>";
                }
            }
        }
    }

    setTimeout(function () {
//        chkimg('mmt-logo','http://img.mmtcdn.com/mi8/images/header_footer/logo.jpg','http://www.makemytrip.com/');
