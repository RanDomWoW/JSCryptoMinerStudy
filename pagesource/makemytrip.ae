<script type="text/javascript">
    var allTabAction = function(lobCode){
        try {
            //var /lobCode = $(this).data("code");
            if (lobCode == "M") {
                lobCode = "F";
            }
//                $(".via .value_" + lobCode).click();
            $('.via').dropbox('select',lobCode);
            $("a.all_menuitem").removeClass("active");
            $(".all_menudropdown").hide();
            $("div.fadeOverlay").remove();

//                mmt.omniture.event($(this).attr("class").replace("_active", "_top_header_click"));
        } catch (e) {
        }
        return false;
    }
    $(document).ready(function(e)
    {
        $("ul.all_menuoptionsList > li > a").hover(function(e)
        {
            var $this = $(this);
            $("ul.all_menuoptionsList > li > a").not($this).parent("li").removeClass("active");
            $this.parent("li").addClass("active");
            var gethref = $this.attr("rel");
            $(".allMenu_data").not($(gethref)).stop(false,true).fadeOut(0);
            $(gethref).stop(false,true).fadeIn(0);
        },function(){ });
        var allmenu = function(e)
        {
            $("a.all_menuitem").addClass('active');
            if($(".fadeOverlay")){
                $(".fadeOverlay").remove();
            }
            var createOverlayDiv = $("<div class='fadeOverlay'/>");
            createOverlayDiv.css({"height":$(document).height(), "width":$(document).width(),"background":"#000000", "opacity":0.8, "z-index":99,"position":"absolute","left":0,"top":0 });
            $("body").append(createOverlayDiv);

            // click for hide "All menu opened" and remove "Overlay"
            $("div.fadeOverlay").live("click", function(){
                $("a.all_menuitem").removeClass("active");
                $(".all_menudropdown").hide();
                $("a.all_menuitem").removeClass('active');
                $(this).remove();
                $("a.all_menuitem").bind('click',allmenu);
            });

            if($(".all_menudropdown").is(":visible") == false)
            {
                $(".all_menudropdown").show();
            }
            $("a.all_menuitem").unbind('click',allmenu);
        }

        $("a.all_menuitem").bind('click',allmenu);

    });
</script>

