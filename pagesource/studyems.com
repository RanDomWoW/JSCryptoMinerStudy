<script src="http://www.studyems.com/statics/js/layerout.js"></script><script>
layui.use('layer');	//引入弹出层模块

//判断用户是否已登录
//@param  bool auto_alert 是否自动弹出登录提示
//@return bool
function check_login(auto_alert){
	var bool;
	$.ajax({
		async : false,
		url : 'http://www.studyems.com/home/api/action/check_login.html',
		success : function(data){
			if(data == '1'){
				bool = true;
			}else{
				bool = false;
				if(auto_alert){	//弹出登录提示
					layer.confirm('请登录后操作', {icon:0, btn:['登录', '注册']}, function(index){
						window.open('http://user.studyems.com/');
						layer.close(index);
					}, function(index){
						window.open('http://user.studyems.com/member/reg.html');
					});
				}
			}
		}
	});
	return bool;
}

//异步显示顶部用户登录状态（优先执行）
$.get(
	'http://www.studyems.com/home/api/action/userlogin.html',
	function(data){
		$('#jq_userlogin').html(data);
	}
);

//百度自动提交 此程序需要在上线后加入 上线前绝对不可以加
(function(){
    var bp = document.createElement('script');
    var curProtocol = window.location.protocol.split(':')[0];
    if (curProtocol === 'https') {
        bp.src = 'https://zz.bdstatic.com/linksubmit/push.js';        
    }
    else {
        bp.src = 'http://push.zhanzhang.baidu.com/push.js';
    }
    var s = document.getElementsByTagName("script")[0];
    s.parentNode.insertBefore(bp, s);
})();

$(function(){
	//切换城市
	$(".map-outer").hover(function(){
		$(".float-map").show();
	},function(){
		$(".float-map").hide();
		$(".city-search").hide();
	})
	//城市异步搜索
	$(".city-keyword").keyup(function(){
		var val = $(this).val();
		var phd = $(this).attr('placeholder');
		if($.trim(val) != '' || val != phd){
			//搜索结果
			$.post(
				'http://www.studyems.com/home/station_api/action/ajax_city_search.html',
				{'keyword':val},
				function(data){
					$(".city-search").html(data).slideDown('fast');
				}
			);
		}else{
			$(".city-search").slideUp('fast');
		}
	})
	
	//输入其他侵权理由
	$('.jqOtherDescription').blur(function(){
		var str = $(this).val();
		$(this).prev('input').val(str);
	});
	//投诉举报表单
	$('.jqComplaintForm').submit(function(){
		//判断用户是否已登录
		if(!check_login(true)) return false;
		//提交表单
		var $this = $(this);
		$.ajax({
			'type':'POST',		//注意服务器是否根据提交类型执行不同操作
			'url':$this.attr('action'),
			'data':$this.serialize(),
			'dataType':'json',	//预期服务器返回的数据类型，如果不符合则执行error
			'beforeSend':function(xhr){	//发送请求前
				$('button[type=submit]', $this).text('正在提交...').attr('disabled', true);
			},
			'success':function(data){
				if(data.status == 1){
					layer.close(layer.index);
					layer.open({
						title: false, 
						type: 1, 
						closeBtn: 0,
						area: ['350px', ''],
						content: $('.layer_sueess')
					});
				}else{
					layer.alert(data.info, {icon:0});
				}
			},
			'error':function(){
				layer.alert('操作失败，请重试，错误代码：500', {icon:2});
			},
			'complete':function(){	//请求完成后回调函数 (请求成功或失败之后均调用)
				$('button[type=submit]', $this).text('确认举报').attr('disabled', false);
			}
		});
		return false;
	});
});
</script>
