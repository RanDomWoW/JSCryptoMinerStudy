//公用选项卡额外功能（品牌机构、本周推荐异步加载列表）
$(".tab-head-title a").mouseover(function(){
	var big_catid = $(this).data('catid');
	if(!big_catid) return false;
	$(this).data('catid', '');	//保证只执行一次
	var action = $(this).parent().data('action');
	action = 'ajax_index_'+action;
	var $_this = $(this);
	//加载数据
	$.post(
		'http://www.studyems.com/home/index_api.html',
		{
			'action':action,
			'big_catid':big_catid
		},
		function(data){
			var index = $_this.index();
			var $tab_con = $_this.parent().parent().next().children().eq(index);
			var school = $('.r-text-l ul', data).html();
			var course = $('.r-text-r ul', data).html();
			$('.r-text-l ul', $tab_con).html(school);
			$('.r-text-r ul', $tab_con).html(course);
		}
	);
})


//网站公告
void function(){
$(".notice-wp").show();
var view = document.getElementById("viewport");
var demo1 = document.getElementById("demo1");
var demo2 = document.getElementById("demo2");
demo2.innerHTML = document.getElementById("demo1").innerHTML;
function Marquee(){
    if(view.scrollLeft-demo2.offsetWidth>=0){
        view.scrollLeft-=demo1.offsetWidth;
    }else{
        view.scrollLeft++;
    }
}
var myvar=setInterval(Marquee,30);
view.onmouseout=function (){myvar=setInterval(Marquee,30);}
view.onmouseover=function(){clearInterval(myvar);}
}()
</script>
