    <script>
        $(function(){
            var clientY=document.documentElement.clientHeight;
            $(".adr_banner").height(clientY);
            setTimeout(function(){
                $(".ban_yuan_img").addClass("active");
            },500);

            setTimeout(function(){
                $(".ban_title_one").addClass("active");
            },800);

            setTimeout(function(){
                $(".ban_one_text").addClass("active");
            },1300);

            var bodyHeight=document.body.clientHeight;
            var wLength=$(".w_1200"),winEl=$(window);

            winEl.scroll(function(){
                var thisTop=winEl.scrollTop();
                //控制各个板块的显示
                for(var i=1;i<wLength.length;i++){
                    if(thisTop>=wLength[i].offsetTop-(2/wLength[i].clientHeight+(6/10*clientY))){
                        $(wLength[i]).addClass("showDom")
                    }
                }
                if(thisTop>=bodyHeight-(clientY+100)){
                    $(".home_footer").css({bottom:0,opacity:1});
                    $(this).unbind("scroll");//全部模块加载完毕解绑滚动事件
                }
            });

            $app.API.banner(".adr_banner",8000);//banner轮播


            var container=$(".partner_list_container"),detail_container=$(".partner_detail_container"),info=$(".partner_info");
            container.width(info.length*100+"%");
            detail_container.width(100/info.length+"%").eq(0).addClass("active");
            var elFirst=detail_container.eq(0);
            var elLast=detail_container.last();
            var marginLeft=0;
            var elIndex=0;
            $(".prev_partner").click(function(){
                if(elFirst.hasClass("active")){
                    $(this).addClass("disabled_btn");
                    return;
                }
                $(this).siblings().removeClass("disabled_btn");
                elIndex--;
                $(".partner_detail_container").eq(elIndex).addClass("active").siblings().removeClass("active");
                if(elFirst.hasClass("active")){
                    $(this).addClass("disabled_btn");
                }
                marginLeft+=100;
                container.animate({marginLeft:marginLeft+"%"},400);
            });

            $(".next_partner").click(function(){
                if(elLast.hasClass("active")){
                    return;
                }
                $(this).siblings().removeClass("disabled_btn");
                elIndex++;
                $(".partner_detail_container").eq(elIndex).addClass("active").siblings().removeClass("active");
                if(elLast.hasClass("active")){
                    $(this).addClass("disabled_btn");
                }
                marginLeft-=100;
                container.animate({marginLeft:marginLeft+"%"},400);
            });
        })
    </script>

