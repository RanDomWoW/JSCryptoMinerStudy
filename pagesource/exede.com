	<!-- carousel-v2 scripts -->
	<script>
		/*** Toggle Arrow Tips ***/

		// toggle arrow tip for desktop view
		$( '.hero-navigation-desktop .active-box').on( 'click', function(evt) {
			console.log( $('.hero-navigation').length );
			// remove old active class and append new active to currently selected div
			$('.hero-navigation-desktop .active-box').find( 'button').removeAttr( 'class', '');
			$(this).find( 'button').attr( 'class', 'active');

			// remove old arrow tip and append tip to currently selected div
			$( '.arrow-tip-right').remove();
			$(this).prepend( '<div class="arrow-tip-right"></div>');
		});

		// toggle arrow tip for tablet view
		$( '.hero-navigation-tablet .active-box').on( 'click', function(evt) {
			// remove old active class and append new active to currently selected div
			$( '.hero-navigation-tablet').find( 'button').removeAttr( 'class', '');
			$(this).find( 'button').attr( 'class', 'active');

			// remove old arrow tip and append tip to currently selected div
			$( '.arrow-tip-up').remove();
			$(this).prepend( '<div class="arrow-tip-up"></div>');
		});

		// toggle navigation for mobile view
		$( '.hero-navigation-mobile .active-box').on( 'click', function(evt) {
			// remove old active class and append new active to currently selected div
			$( '.hero-navigation-mobile').find( 'button').removeAttr( 'class', '');
			$(this).find( 'button').attr( 'class', 'active');
		});


		/*** Toggle Slider Images ***/
		$( '.active-box').on( 'click', function(evt) {
			var imagePath = 'wp-content/themes/exede/images/home/';
			var selectedImage =  $(this).find( 'button').attr( 'id');

			// show/hide texts on the images
			if (selectedImage === 'hero-front-img-2') {
				$( '.image-panel').show(50);
			} else {
				$( '.image-panel').hide(50);
			}

			// toggle current main images
			$( '.slider-images').attr( 'src', '');
			$( '.slider-images').attr( 'src', imagePath + selectedImage + '.jpg');
			// mobile images
			$( '.slider-images-mobile').attr( 'src', '');
			$( '.slider-images-mobile').attr( 'src', imagePath + selectedImage + '-mobile.jpg');
		});


	</script>

