<script type="text/template" id="playlist-ld-template">
	
<%
	if ( ! data || ! data.songs ) {
	return;
}

	if ( opts && opts.subtype === 'player' ) {
		var playlist_class = ' c-song--playlist';
	} else if ( opts && opts.subtype === 'music' ) {
		var playlist_class = ' c-song--with-time';
	} else {
		var playlist_class = null;
}

	if ( opts && opts.limit !== '' ) {
		data.songs = data.songs.slice(0, opts.limit );
	}
%>

<% _.each( data.songs, function( song ) { %>
	<li class="l-list__item">
		<div class="c-song<%= playlist_class %>" itemscope itemtype="http://schema.org/MusicRecording">
			<% if ( 'music' === opts.subtype ) { %>
				<time class="c-song__time">
					<%= song.date %>
				</time>
			<% } %>

	<figure class="c-song__cover" itemprop="image" itemscope itemtype="http://schema.org/ImageObject">
				<% if ( song.image !== '' && song.song !== '' ) { %>
					<% if ( song.image.indexOf( '60x60' ) !== -1 ) { %>
						<% song.image = song.image.replace( '60x60', '100x100' ); %>
					<% } %>
					<img src="<%= song.image %>" alt="<%= song.song %>" itemprop="contentUrl">
				<% } else { %>
					<img src="https://edge1021.files.wordpress.com/2017/12/400x400_edge.png?h=100&#038;w=100" alt="<%= song.song %>" itemprop="contentUrl">
				<% } %>
	</figure>

	<div class="c-song__info">
		<div class="c-song__artist" itemprop="byArtist" itemscope itemtype="http://schema.org/MusicGroup">
					<% if ( song.artist !== '' ) { %>
						<span itemprop="name"><%= song.artist %></span>
					<% } %>
		</div>

				<% if ( song.song !== '' ) { %>
					<div class="c-song__title" itemprop="name" data-song-title><%= song.song %></div>
				<% } %>

				<% if ( 'music' !== opts.subtype ) { %>
					<time class="c-song__timeago">
						<%= song.date %>
					</time>
				<% } %>

				<% if ( song.itunes !== '' ) { %>
					<a href="<%= song.itunes %>" class="c-song__buy c-song__buy--itunes" rel="noopener noreferrer external payment" target="_blank" itemprop="offers">
						<span class="screen-reader-text">Get it on iTunes</span>
			</a>
				<% } %>
	</div>
</div>
	</li>
<% }); %>
	<a href="/music">View Full Playlist &rarr;</a>
</script>			</div>
