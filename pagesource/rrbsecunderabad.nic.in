  $(document).ready(function(){

	var defaultH1 = parseInt($('h1').css('font-size')); 
	var defaultP = parseInt($('p').css('font-size')); 
	var defaultA = parseInt($('a').css('font-size')); 
	var defaultH4 = parseInt($('h4').css('font-size')); 
	var defaultFont = parseInt($('font').css('font-size')); 
	var defaultH2 = parseInt($('h2').css('font-size')); 	
	var defaultDd = parseInt($('dd').css('font-size')); 	
	var defaultB = parseInt($('b').css('font-size')); 
	var defaultH6 = parseInt($('h6').css('font-size')); 	
			
	var count = 0;
	
	var elements = ['p', 'h1', 'a', 'h4', 'font', 'h2', 'dd', 'b', 'h6']; 
		
	$('.minus').click(function(){
		if ( count >= -1 ) {
			$(elements).each(function(key, val) {
				$(val).css('font-size', parseInt($(val).css('font-size'))-2);
			});
			count--;
		};
		//alert( "Currently at " + count );
		alert( "Current size " + currentsize );
	});
		
	$('.plus').click(function(){
		if ( count <= 1 ) {
			$(elements).each(function(key, val) {
				$(val).css('font-size', parseInt($(val).css('font-size'))+2);
			});
			count++;
		};
		//alert( "Currently at " + count );
	});
 		
	$('.reset').click(function(){
		$('h1').css('font-size', defaultH1);
		$('p').css('font-size', defaultP);
		$('a').css('font-size', defaultA);
		$('h4').css('font-size', defaultH4);
		$('font').css('font-size', defaultFont);		
		$('h2').css('font-size', defaultH2);				
		$('dd').css('font-size', defaultDd);						
		$('b').css('font-size', defaultB);
		$('h6').css('font-size', defaultH6);								
		count = 0;
	});	

});
</script>
