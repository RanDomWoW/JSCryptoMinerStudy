  <script>  
  /*
     * Recurring Orders Customization
     * Last Updated: May 25th, 2017
     * Purpose: Replace Build a Box quantity inputs with Add to Bag / Remove from Bag buttons
     */

  const addText = "Add to Bag";
  const removeText = "Remove from Bag";
  const fullText = "Bag Full";

  //Hide and quantity text fields and replace them with a button and checkbox
  $('.bold-product__quantity')
  .hide()
  .after('<button type="button" class="atb-btn btn button">Add to Bag</button>')
  .after('<input style="display:none;" type="checkbox" value="0" class="bold-product__quantity-checkbox" />');

  /*
       * On change of label checkbox
       * Set the quantity of that label to 1 if checked, or 0 if unchecked
       * If there is no remaining spots for labels after, disable all unchecked
       * boxes until there is a free spot.
       */

  $(document).on('click', '.atb-btn', function(){

    var $parent = $(this).closest('.bold-product__quantity-wrapper');
    var $checkbox = $parent.find('.bold-product__quantity-checkbox');

    if($checkbox.prop('checked')){
      $(this).html(addText);
    }
    else{
      $(this).html(removeText);
    }

    $checkbox.trigger('click');

  });


  $(document).on('change', '.bold-product__quantity-checkbox', function(){

    var remaining = parseInt($('.bold-ro__choice-progress-value').html());
    var quantity = (Number(this.checked));
    var $parent = $(this).closest('.bold-product__quantity-wrapper');
    var $button = $parent.find('.atb-btn');

    //if this is the last available slot, disable the other options after
    if(remaining == 1 && quantity){
      $('.bold-product__quantity-checkbox:not(:checked)').prop('disabled', true);
      $('.atb-btn').filter(function(){

        var $btnparent = $(this).closest('.bold-product__quantity-wrapper');
        if($btnparent.find('.bold-product__quantity-checkbox').prop('checked')){
          return false;
        }
        else{
          return true;
        }

      }).prop('disabled', true).html(fullText);
    }
    else if(remaining == 1 && !quantity){
      $('.bold-product__quantity-checkbox:not(:checked)').prop('disabled', false);
      $('.atb-btn').filter(function(){

        var $btnparent = $(this).closest('.bold-product__quantity-wrapper');
        if($btnparent.find('.bold-product__quantity-checkbox').prop('checked')){
          return false;
        }
        else{
          return true;
        }

      }).prop('disabled', false).html(addText);
    }
    else{
      $('.bold-product__quantity-checkbox:not(:checked)').prop('disabled', false);
      $('.atb-btn').filter(function(){

        var $btnparent = $(this).closest('.bold-product__quantity-wrapper');
        if($btnparent.find('.bold-product__quantity-checkbox').prop('checked')){
          return false;
        }
        else{
          return true;
        }

      }).prop('disabled', false).html(addText);
    }

    $parent.find('input[type=text]').val(quantity).trigger('change');

  });

  $('.bold-product__quantity-val').each(function(){
    if($(this).val() == 1){
      $(this).closest('.bold-product__quantity-wrapper').find('.atb-btn').trigger('click');
    }
  });
</script>
