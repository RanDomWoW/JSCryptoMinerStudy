    function showForgetLogin(errmsg) {
        if (arguments.length > 0) {
            alert(errmsg);
        }
        if ($(document).height() < $(window).height() + 10) {
            $("body").css("overflow", "hidden");
        }
        $.colorbox({
            inline: true,
            scrolling: false,
            href: '#loginContainer',
            opacity: 0.4,
            onComplete: function () {
                forgetNow();
                $("body").css("overflow", "auto");
                $("#resetEmail").focus();
            },
            onClosed: function () {
                showing = false;
            }
        });
    }

    function login() {
        $("#colorbox, #cboxOverlay").appendTo('#validateloginform');
        if ($('#validateloginform').valid()) {
            $.colorbox.close();

            var email = $("#loginEmail").val();
            var password = $("#loginPassword").val();
            var checked = $("#CheckBox_rmb_me").attr("checked");

            $("#TextBoxLoginEmail").val(email);
            $("#TextBoxLoginPassword").val(password);
            $("#CheckBoxRmbMe").attr("checked", checked);

            $("#ButtonLogin").click();
        }
    }

    function register() {
        $("#colorbox, #cboxOverlay").appendTo('#validateloginform');
        var validregister = $('#validateloginform').valid();
        if ($("#regEmail").val() != $("#regConfirmEmail").val()) {
            alert(MESSAGE_REGEMAIL);
            validregister = false;
        }
        if ($("#knowUs").val() == 0) {
            alert(MESSAGE_KNOWUS);
            $("#knowUsError").show();
            validregister = false;
        }
        if (!$("#regAccount").valid()) {
            alert(MESSAGE_REGACCOUNT);
            validregister = false;
        }
        if (!$("#regUserName").valid()) {
            alert(MESSAGE_REGUSERNAME);
            validregister = false;
        }


        if (!$("#regPassword").valid()) {
            alert(MESSAGE_REGPASSWORD);
            validregister = false;
        }
        if ($("#regPassword").val() != $("#regConfirmPassword").val()) {
            alert(MESSAGE_REGCONFIRMPASSWORD);
            validregister = false;
        }

        if (validregister) {
            $.colorbox.close();

            var account = $("#regAccount").val();
            var username = $("#regUserName").val();
            var email = $("#regEmail").val();
            var password = $("#regPassword").val();
            var opt_out = $("#opt_out").attr("checked") ? 0 : 1;

            $("#TextBoxRegAccount").val(account);
            $("#TextBoxRegUserName").val(username);
            $("#TextBoxRegEmail").val(email);
            $("#TextBoxRegPassword").val(password);
            $("#TextBoxOptOut").val(opt_out);
            $("#ButtonRegister").click();
        }
    }

    function resetPassword() {
        var hfProcessing = document.getElementById("hfProcessing");
        var resetEmail = $("#resetEmail").val();

        if (hfProcessing.value == "") {
            $("#TextBoxForgetEmail").val(resetEmail);
            $("#ButtonForgetPassword").click();
            hfProcessing.value = "1";
        }
    }

