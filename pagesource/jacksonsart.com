var validator = new Validation($('login-form-popup'));//Bind form validation for when login button is clicked

jQuery(function ($) {


  jQuery('button.login-button').click(function(){

      jQuery('#login-form-popup ul.messages').html('');

      if(validator.validate()){//Check the standard magento validation for username/password

          var email    = jQuery('#login-form-popup input#email').val();
          var password = jQuery('#login-form-popup input#pass').val();
          var form_key = jQuery('#login-form-popup input#login-popup-key').val();

          new Ajax.Request('/dynamic/index/postloginpopup', {
              parameters: {form_key: form_key,
                           username: email,
                           password: password,
                           method:   'POST'},
              onLoading: function(){
                  var pleaseWait = $('compare-list-please-wait');
                  if (pleaseWait) {
                      pleaseWait.show();
                  }
              },
              onSuccess: function(transport) {
                  var pleaseWait = $('compare-list-please-wait');
                  if (pleaseWait) {
                      pleaseWait.hide();
                  }

                  var response = transport.responseJSON;

                  if (response.logged_in){
                    localStorage.clear();
                    window.location.reload();

                  } else {

                    jQuery('#login-form-popup ul.messages').html('<li class="error-msg"><ul><li><span>'+response.message+'</span></li></ul></li>');
                    jQuery('#login-form-popup ul.messages').focus();
                    jQuery('#login-form-popup ul.messages').blur();

                  }

                  //window.location.reload();
                  //window.opener.location.reload();
              }
          });

          return false;

      }

  });

});
</script>

