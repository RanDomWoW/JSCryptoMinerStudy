<script>
jQuery(document).ready(function($) {

  if($('.wi-columnists').next().hasClass('wi-clickInvestigate')) {
    $('.wi-columnists').addClass('first');
  } else {
    $('.wi-clickInvestigate').addClass('first');
  }

  var wi = window.innerWidth;

  var count = 0;

  $(window).resize(resize);

  function resize() {

    wi = window.innerWidth;

    if(wi >= 768) {

      if (count === 1) {

        count = 0;

        $('.js-carouselBlogs').cycle('destroy');
        $('.js-pagerBlogs').removeClass('c-pager--active');

        $('.js-carouselMovies').cycle('destroy');
        $('.js-pagerMovies').removeClass('c-pager--active');

        $('.js-carouselCulture').cycle('destroy');
        $('.js-pagerCulture').removeClass('c-pager--active');

      }

    } else {

      if (count === 0) {

        count = 1;

        $('.js-carouselBlogs').cycle({
          fx: 'fade',
          timeout: 0,
          slides: '> article',
          autoHeight: false,
          pagerActiveClass: 'c-pager__item--active',
          swipe: true,
          swipeFx: 'scrollHorz',
          pager: '.js-pagerBlogs',
          pagerTemplate: '<div class="c-pager__item"></div>'
        });

        $('.js-pagerBlogs').addClass('c-pager--active');

        $('.js-carouselMovies').cycle({
          fx: 'fade',
          timeout: 0,
          slides: '> article',
          autoHeight: false,
          pagerActiveClass: 'c-pager__item--active',
          swipe: true,
          swipeFx: 'scrollHorz',
          pager: '.js-pagerMovies',
          pagerTemplate: '<div class="c-pager__item"></div>'
        });

        $('.js-pagerMovies').addClass('c-pager--active');

        $('.js-carouselCulture').cycle({
          fx: 'fade',
          timeout: 0,
          slides: '> div',
          autoHeight: false,
          pagerActiveClass: 'c-pager__item--active',
          swipe: true,
          swipeFx: 'scrollHorz',
          pager: '.js-pagerCulture',
          pagerTemplate: '<div class="c-pager__item"></div>'
        });

        $('.js-pagerCulture').addClass('c-pager--active');

      }

    }

  }

  resize();

});
</script>



