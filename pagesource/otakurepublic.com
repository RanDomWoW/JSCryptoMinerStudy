	<script>
	DEFINE_GLOBAL_ONCE("loadDrawerContent", function(content_name) {
		var url = '/product/product_scrolling_list_ajax.html?block_id=drawer_list_'+content_name+'&full_info=1&scrolling_ui=1&ref=drawer';
		switch(content_name) {
			case "history":
			$.ajax({
				async: true,
				url: url + '&list_type=history',
				success: function(received, text_status) {
					$("#drawer_content_history").html(received);
					$(".drawer_content_history_noresults").clone(true).appendTo("#drawer_content_history .noresult_container");
					$(".noresult_container .drawer_content_history_noresults").show();
				}
			});
			break;
			case "favorite":
			$.ajax({
				async: true,
				url: url + '&list_type=favorite',
				success: function(received, text_status) {
					$("#drawer_content_favorite").html(received);
					$(".drawer_content_favorite_noresults").clone(true).appendTo("#drawer_content_favorite .noresult_container");
					$(".noresult_container .drawer_content_favorite_noresults").show();
				}
			});
			break;
			case "cart":
			$.ajax({
				async: true,
				url: url + '&list_type=cart',
				success: function(received, text_status) {
					$("#drawer_content_cart").html(received);
					$(".drawer_content_cart_noresults").clone(true).appendTo("#drawer_content_cart .noresult_container");
					$(".noresult_container .drawer_content_cart_noresults").show();
				}
			});
			break;
		}
	}, undefined);

	DEFINE_GLOBAL_ONCE("resetDrawerContentStatus", function(content_name) {
		$(".drawer_handle li[content_name='"+content_name+"']").removeAttr("content_loaded");
	});

	DEFINE_GLOBAL_ONCE("init_drawer_handler", true, function() {
		$(".drawer_handle li").hover(function() {
			$(".drawer_close_background").show();

			$(".drawer_handle li").removeClass("active");
			$(this).addClass("active");

			var content_name = $(this).attr("content_name");
			$(".drawer_content ul").attr("pos", content_name);
			if (!$(this).attr("content_loaded")) {
				$(this).attr("content_loaded", true);
				loadDrawerContent(content_name);
			}
		});
		$(".drawer").hover(undefined, function() {
			$(".drawer_close_background").hide();
			$(".drawer_handle li").removeClass("active");
		});
	});
	</script>
