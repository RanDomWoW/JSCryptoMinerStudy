DEFINE_GLOBAL_ONCE("setIntervalImmidiate", function(callback, timeout) {
	callback();
	return setInterval(callback, timeout);
});

DEFINE_GLOBAL_ONCE("getParamString", function(params) {
	var result = "";
	for (var p in params) {
		if (params[p] === undefined) continue;
		if (params[p] === "") continue;

		if (typeof(params[p]) == "object") {
			for (var p2 in params[p]) {
				if (params[p][p2] === undefined) continue;
				if (params[p][p2] === "") continue;

				result += (result === "") ? "?" : "&";
				result += p + "." + p2 + "=" + params[p][p2];
			}
		} else {
			result += (result === "") ? "?" : "&";
			result += p + "=" + params[p];
		}
	}
	return result;
});

DEFINE_GLOBAL_ONCE("atScrollBegin", function(elem) {
	var left = elem.scrollLeft;
	return (left <= 10);
});

DEFINE_GLOBAL_ONCE("atScrollEnd", function(elem) {
	var element_width = elem.offsetWidth;
	var left = elem.scrollLeft;
	var width = elem.scrollWidth;
	return (element_width + left >= width - 10);
});

DEFINE_GLOBAL_ONCE("installSwipeScrolling", function(list_id) {
	var selector = "#" + list_id + " .with_scrolling";
	var elem = $(selector);
	if (elem === undefined) return;

	elem.mousedown(function(e) {
		mousedown_flag_list[selector] = true;
		mouse_pos[selector] = e.clientX;
		mouse_diff_sum[selector] = 0;
		e.preventDefault();
	});

	elem.mouseup(function(e) {
		mousedown_flag_list[selector] = false;

		/* skip event when it was swipe */
		e.preventDefault();
	});

	elem.mouseleave(function(e) {
		mousedown_flag_list[selector] = false;
	});

	elem.click(function(e) {
		/* skip event when it was swipe */
		if (mouse_diff_sum[selector] > swipe_threshold) {
			e.preventDefault();
		}
	})

	elem.mousemove(function(e) {
		e.preventDefault();
		if (!mousedown_flag_list[selector]) return;
		var diff = mouse_pos[selector] - e.clientX;

		mouse_diff_sum[selector] += Math.abs(diff);

		elem.scrollLeft(elem.scrollLeft() + diff);
		mouse_pos[selector] = e.clientX;
	});
});


DEFINE_GLOBAL_ONCE("initScrollUI", function(list_id) {
	var selector = "#" + list_id + " .with_scrolling";
	var elem = $(selector).get(0);

	/* initial view */
	displayScrollUI(list_id, "left", false);
	if (elem.offsetWidth < elem.scrollWidth) {
		displayScrollUI(list_id, "right", true);
	} else {
		displayScrollUI(list_id, "right", false);
	}

	/* scrolling left */
	var scroll_left_timer = false;
	$("#" + list_id + " .scrolling_ui_left").mouseenter(function() {
		scroll_left_timer = setInterval(function() {
			$(selector).animate({scrollLeft: elem.scrollLeft - 210}, 100);
		}, 400);
	}).mouseleave(function() {
		if (scroll_left_timer) clearInterval(scroll_left_timer);
	}).click(function() {
		if (scroll_left_timer) clearInterval(scroll_left_timer);
		$(selector).animate({scrollLeft: elem.scrollLeft - 210}, 100);
	});

	/* scrolling right */
	var scroll_right_timer = false;
	$("#" + list_id + " .scrolling_ui_right").mouseenter(function() {
		scroll_right_timer = setInterval(function() {
			$(selector).animate({scrollLeft: elem.scrollLeft + 210}, 100);
		}, 400);
	}).mouseleave(function() {
		if (scroll_right_timer) clearInterval(scroll_right_timer);
	}).click(function() {
		if (scroll_right_timer) clearInterval(scroll_right_timer);
		$(selector).animate({scrollLeft: elem.scrollLeft + 210}, 100);
	});

	$(selector).on("scroll", function() {
		reloadScrollUI(list_id);
	});
	$(selector).on("mouseenter", function() {
		reloadScrollUI(list_id);
	});
	reloadScrollUI(list_id);
});

DEFINE_GLOBAL_ONCE("displayScrollUI", function(list_id, which, show) {
	var selector = "#" + list_id + " .scrolling_ui_" + which;
	if (show) {
		$(selector).show();
	} else {
		$(selector).hide();
	}
});

DEFINE_GLOBAL_ONCE("reloadScrollUI", function(list_id) {
	/* if scroll position is begin or end, hide scrolling UI */
	var elem = $("#" + list_id + " .with_scrolling").get(0);
	if (atScrollEnd(elem)) {
		displayScrollUI(list_id, "right", false);
	} else {
		displayScrollUI(list_id, "right", true);
	}
	if (atScrollBegin(elem)) {
		displayScrollUI(list_id, "left", false);
	} else {
		displayScrollUI(list_id, "left", true);
	}
});


