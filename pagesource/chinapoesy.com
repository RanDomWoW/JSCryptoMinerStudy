                alert("请输入用户名和密码");
                return;
            }
            
		    $("#spanLoginwait").css("visibility","visible");
              $.ajax({
            async: true, // 默认true(异步请求)
            cache: false, // 默认true,设置为 false 将不会从浏览器缓存中加载请求信息。
            type: "POST", // 默认:GET 请求方式:[POST/GET]
            dataType: "text/html", //默认["xml"/"html"] 返回数据类型:["xml" / "html" / "script" / "json" / "jsonp"]
            url: "Handle/HandleLogin.aspx",
            data: 't='+ t +'&p='+p +'&o='+o ,
            error: function(xml) { window.location="http://www.chinapoesy.com/userlogin.aspx";}, // 请求失败时调用
            timeout: 8000, // 设置请求超时时间
             success: function(xml) { 
              if(xml !=0) $('#DivTopMenu').html(xml);
             else { window.location="http://www.chinapoesy.com/userlogin.aspx";}
              $("#spanLoginwait").css("visibility","hidden");
            }
        });
        
		 }
</script>
        </td>
      </tr>
    </table>
