  <script>
    $(document).ready(function() {
      var imageviewer = {"copyright":"","description":"Bilder aus der Seite: ","enableDownloads":false,"enableOverview":false,"enableSlider":false,"enableSlideshow":false,"localisation":{"viewOverview":"zur \u00dcbersicht","close":"schlie\u00dfen","closeGallery":"schlie\u00dfen","download":"Bild herunterladen","nextImage":"n\u00e4chstes Bild","previousImage":"vorheriges Bild","startSlideshow":"Slideshow starten","stopSlideshow":"Slideshow stoppen"},"link":null,"images":[]};

      $('#SP-content').spImageViewer(imageviewer, {
        api: true,
        download: false,
        overview: false,
        slider: false,
        slideshow: false      });

      var spImageViewer = $('#SP-content').data('SP_UI_ImageViewer');
      $('#SP-content').find('.SP-image').each(function (index, item) {
        if ($(item).find('.SPi-zoom').length > 0) {
          var timing = 0;
          $(item).on('mousedown', function (e) {
            timing = new Date();
          });
          $(item).on('click', function (e) {
            if ($(this).closest(".SP-snip-slide").length == 0 || (new Date() - timing) < 100) {
              spImageViewer.showImage(SP.Website.determineIndex($(item), '#SP-content', '.SP-image:has(.SPi-zoom)'));
            }
            e.preventDefault();
          });
        }
      });
    });
  </script>

