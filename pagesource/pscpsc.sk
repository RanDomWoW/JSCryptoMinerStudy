    <script src="/public/assets/js/script.min.js?v=2"></script>
    <script>
        window.config = {
            gtHandler: "\/?do=gt",
            bnrHandler: 'https://www.pscpsc.sk/?do=banner-click',
            productionMode: true,
            isBot: true,
            city: null,
            street: null,
            psc: null,
            firstResult: null,
            counter: 0,
            searchResults: null,

            searchUrl: "\/search",
            searchLimit: 30,
            dynamicLimit: 5

        };
    </script>


<script>
</script>
