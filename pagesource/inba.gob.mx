<script>
	function send3()
	{
		var data=$("#inba-newsletter-form").serialize();
		
		$.ajax({
			type: 'POST',
			url: '/Site/index',
			data:data,
			dataType:'json',
			success:function(data){
				if(data == "validate"){
					$("#InbaNewsletterForm_name").val("");
					$("#InbaNewsletterForm_name").placeholder='Nombre';
					$("#InbaNewsletterForm_email").val("");
					$("#InbaNewsletterForm_email").placeholder='correo@dominio';
					$("#InbaNewsletterForm_day").val("");
					$("#InbaNewsletterForm_day").placeholder='Día';
					$("#InbaNewsletterForm_month").val("");
					$("#InbaNewsletterForm_month").placeholder='Mes';
					$("#InbaNewsletterForm_year").val("");
					$("#InbaNewsletterForm_year").placeholder='Año';
					$("#InbaNewsletterForm_day").val("");
					$("#InbaNewsletterForm_day").placeholder='none';
					
					grecaptcha.reset();
					$("#mailsus").hide();
					$("#yeah").show();
					window.location.href = "#registroExitoso";
				}else if(data == "active"){
					// alert("validate web");
					$("#InbaNewsletterForm_name").val("");
					$("#InbaNewsletterForm_name").placeholder='Nombre';
					$("#InbaNewsletterForm_email").val("");
					$("#InbaNewsletterForm_email").placeholder='correo@dominio';
					$("#InbaNewsletterForm_day").val("");
					$("#InbaNewsletterForm_day").placeholder='Día';
					$("#InbaNewsletterForm_month").val("");
					$("#InbaNewsletterForm_month").placeholder='Mes';
					$("#InbaNewsletterForm_year").val("");
					$("#InbaNewsletterForm_year").placeholder='Año';
					$("#InbaNewsletterForm_day").val("");
					$("#InbaNewsletterForm_day").placeholder='none';
					// $("#InbaNewsletterForm_verifyCode").val("");
					
					grecaptcha.reset();
					$("#mailsus").hide();
					// $("#yeah").show();
					$("#active").show();
					window.location.href = "#registroExitoso";
				}else{
					grecaptcha.reset();
					$("div.errr").remove();
					jQuery.each( data, function( i, val ) {
						var msg = "<div class='errr' style='color:red;'>"+val+"</div>";
						$("#"+i).append().after(msg);
					});
				}
			},
			error: function(data) {
				$("#errorFormulario").show();
				window.location.href = "#errorForm";
			},
		});
	} 
</script>
<!-- Fin script validación del formulario suscripción al boletín -->

