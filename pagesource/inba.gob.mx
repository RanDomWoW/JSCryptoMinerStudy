<!-- INICIO del script para el Modal de actividades por día -->
<script>
	// Get the modal
	var modal = document.getElementById('modalAct');

	// Get the <span> element that closes the modal
	var span = document.getElementsByClassName("closeModal")[0];

	// When the user clicks on <span> (x), close the modal
	span.onclick = function() {
		modal.style.display = "none";
		document.getElementById('modalImg').setAttribute("src", "");
	}

	// When the user clicks anywhere outside of the modal, close it
	window.onclick = function(event) {
		if (event.target == modal) {
			modal.style.display = "none";
			document.getElementById('modalImg').setAttribute("src", "");
		}
	}
	
	function openModal(act) {
		
		document.getElementById('modalTit').innerHTML = $("#"+act.id+"tit").text();
		document.getElementById('modalSubt').innerHTML = $("#"+act.id+"subt").text();
		document.getElementById('modalRec').innerHTML = $("#"+act.id+"rec").text();
		document.getElementById('modalFchs').innerHTML = $("#"+act.id+"fechas").html();
		
		if($("#"+act.id+"gratis").text() == '1'){
			document.getElementById('modalPrecios').innerHTML = 'Entrada libre';
		}else{
			document.getElementById('modalPrecios').innerHTML = $("#"+act.id+"precios").text();
		}
		
		document.getElementById('modalDisc').innerHTML = $("#"+act.id+"disc").text();
		
		if($("#"+act.id+"linkBoletos").text() == ''){
			$("#modalLinkBoletos").hide();
			document.getElementById('modalBtns').style.textAlign = "center";
		}else{
			$("#modalLinkBoletos").show();
			document.getElementById('modalLinkAct').style.margin = "0px 15px 0px 0px";
		}
		
		switch($("#"+act.id+"id_disc").text()){
			case '1':
					document.getElementById('modalDisc').setAttribute("class", "grd-teatro");
					document.getElementById('modalLinkAct').setAttribute("class", "grd-teatro btnModalLinkAct");
					document.getElementById('modalLinkBoletos').setAttribute("class", "grd-teatro btnModalBoletos");
					break;
			case '2': document.getElementById('modalDisc').setAttribute("class", "grd-danza");
					document.getElementById('modalLinkAct').setAttribute("class", "grd-danza btnModalLinkAct");
					document.getElementById('modalLinkBoletos').setAttribute("class", "grd-danza btnModalBoletos");
					break;
			case '3': document.getElementById('modalDisc').setAttribute("class", "grd-musica");
					document.getElementById('modalLinkAct').setAttribute("class", "grd-musica btnModalLinkAct");
					document.getElementById('modalLinkBoletos').setAttribute("class", "grd-musica btnModalBoletos");
					break;
			case '4': document.getElementById('modalDisc').setAttribute("class", "grd-artes-visuales");
					document.getElementById('modalLinkAct').setAttribute("class", "grd-artes-visuales btnModalLinkAct");
					document.getElementById('modalLinkBoletos').setAttribute("class", "grd-artes-visuales btnModalBoletos");
					break;
			case '5': document.getElementById('modalDisc').setAttribute("class", "grd-literatura");
					document.getElementById('modalLinkAct').setAttribute("class", "grd-literatura btnModalLinkAct");
					document.getElementById('modalLinkBoletos').setAttribute("class", "grd-literatura btnModalBoletos");
					break;
			case '6': document.getElementById('modalDisc').setAttribute("class", "grd-opera");
					document.getElementById('modalLinkAct').setAttribute("class", "grd-opera btnModalLinkAct");
					document.getElementById('modalLinkBoletos').setAttribute("class", "grd-opera btnModalBoletos");
					break;
		}
		
		document.getElementById('modalLinkAct').setAttribute("href", $("#"+act.id+"linkAct").text());
		document.getElementById('modalImg').setAttribute("src", $("#"+act.id+"img").text());
		document.getElementById('modalLinkBoletos').setAttribute("href", $("#"+act.id+"linkBoletos").text());
		
		$("#modalAct").fadeIn(1000);
	}
</script>
<!-- FIN del script para el Modal de actividades por día -->
