        btns.appendTo(wrap);
        wrap.appendTo(this.overlay);
        this.menu = $('#topmenu .main-menu');
        this.new_menu = this.menu.clone(true).appendTo(this.overlay);
        this.menu.addClass(this.cloned_classname);
        this.new_menu.find('li:not(.menu-premium)').addClass('faded');
        this.new_menu.find('a').removeClass('item-active');
        this.new_menu.find('.menu-premium a').addClass('item-active');
        this.orangebtn.attr('href', this.new_menu.find('.menu-premium a').attr('href') + this.orange_event);
        this.greenbtn.attr('href', greenURL);
        this.closer.attr('href', greenURL);
        this.prepareEvents();
        this.calcPosition();
    },
    calcPosition: function() {
        if (this.menu && this.new_menu) {
            m_offset = this.menu.offset();
            m_width = this.menu.width();
            this.new_menu.offset(m_offset);
            this.new_menu.width(m_width);
        }
    },
    prepareEvents: function() {
        var self = this;
        window.addEventListener('resize', function(event){
            ps_highlight.calcPosition()
        });
        if (self.orangebtn && self.greenbtn && self.closer) {
            self.orangebtn.on('click', function(e) {
                self.orangebtn_click(e)
            });
            self.greenbtn.on('click', function(e){
                self.greenbtn_click(e);
            });
            self.closer.on('click', function(e){
                self.greenbtn_click(e);
            });
        }
    },
    orangebtn_click: function(e) {
        this.close(true);
    },
    greenbtn_click: function(e) {
        //e.preventDefault();
        this.close(false);
    },
    close: function(long_cookie) {
        if (long_cookie) {
            this.setCookie(this.cookie_name, 1, {expires: 2*365*24*60*60});
        } else {
            this.setCookie(this.cookie_name, 1, {expires: 40*60}); // 30 minutes
        }
        this.overlay.remove();
        this.menu.removeClass(this.cloned_classname);
        this.menu = null;
        this.orangebtn = null;
        this.greenbtn = null;
        this.new_menu = null;
        this.overlay = null;
        this.closer = null;
        //return false;
    },
    setCookie: function(name, value, options) {
        options = options || {};

        var expires = options.expires;

        if (typeof expires == "number" && expires) {
            var d = new Date();
            d.setTime(d.getTime() + expires * 1000);
            expires = options.expires = d;
        }

        if (c_prefix) {
            name = c_prefix + '_' + name;
        }

        value = encodeURIComponent(value);

        var updatedCookie = name + "=" + value;
        if (c_domain) {
            updatedCookie += "; domain=" + c_domain;
        }

        if (expires && expires.toUTCString()) {
            options.expires = expires.toUTCString();
            //updatedCookie += "; expires=" + expires.toUTCString();
        }

        for (var propName in options) {
            updatedCookie += "; " + propName;
            var propValue = options[propName];
            if (propValue !== true) {
                updatedCookie += "=" + propValue;
            }
        }
        console.log(updatedCookie);
        document.cookie = updatedCookie;
    },
    deleteCookie: function(name) {
        this.setCookie(name, "", {
            expires: -1
        })
    }
};
document.addEventListener('DOMContentLoaded', function(){
    setTimeout(function(){
        ps_highlight.init();
    }, 10000);
});
