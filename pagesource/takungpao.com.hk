<script type="text/javascript" src="http://r.takungpao.com/lib/mediaelement/mediaelement-and-player.min.js"></script>
<script type="text/javascript" src="http://r.takungpao.com/lib/idangerous/idangerous.swiper.min.js"></script>
<script type="text/javascript">
$(function() {
	slideInit();
	$(window).resize(function(){slideInit(); mySwiper.resizeFix(); mySwiper2.resizeFix();});
	
	var c = $('<div class="content"><ul class="pic"><a target="_blank"><img/><span class="ico"></span></a></ul><ul class="txt"><li class="title"><a target="_blank"/></li><li class="time"><span></span></li></ul></div>');
            $.getJSON("http://hkapi.takungpao.com/api.php?op=get_index_news&mk=show&jp=?")
            .done(function (d) {
                var l = $('#content_left'), r = $('#content_right'), m = 0, o;
                for (var i = 0; i < d.length; i++) {
                    o = c.clone();
                    di = d[i];
                    o.find('a').attr('href', di.url);
                    o.find('img').attr('src', di.thumb).load(function(){
						var pic = new Image()
						pic.src = $(this).attr('src');
						if( pic.width <= pic.height ){
							$(this).addClass('shutu');
						}
					});
                    o.find('.title a').text(di.title);
                    o.find('.time').html('大公網<span>' + (new Date(parseInt(di.updatetime) * 1000)).Format("yyyy-MM-dd HH:mm") + '</span>');
					o.find('.ico').text(di.shorttitle);
                    i % 2 == 0 ? l.append(o) : r.append(o);
                }
            })
            .fail(function (d) { });
	
	//焦点图
	var mySwiper = new Swiper('#focusPhotos', {
		pagination: '#pagePhotos',
		paginationClickable: true,
		loop: true,
		autoplay:5000,
		speed:500,
		autoResize: false,
		slidesPerView: 'auto'
	});
	$('.arrow_left_focus').on('click', function(e){
		e.preventDefault();
		mySwiper.swipePrev();
	});
	$('.arrow_right_focus').on('click', function(e){
		e.preventDefault();
		mySwiper.swipeNext();
	});	
	$('a.focus_control_pause').on('click', function() {
		if($(this).hasClass('play')){
			$(this).toggleClass('play');
			mySwiper.startAutoplay();
		}else{
			mySwiper.stopAutoplay();
			$(this).toggleClass('play');
		}
		return false;
	});
	/**
	$('.focus_photos').mouseover(function(){
		$('.focus_control').show();	
	});	
	$('.focus_photos').mouseout(function(){
		$('.focus_control').hide();	
	});	*/
	
	//滚动图片
	var mySwiper2 = new Swiper('#picsScroll', {
		pagination: '#pageScroll',
		paginationClickable: true,
		loop: true,
		speed:1000,
		autoplay: 6000,
		autoplayDisableOnInteraction: false,
		autoResize: false,
		slidesPerView: 'auto'
	});
	$('.arrow_left_scroll').on('click', function(e){
		e.preventDefault();
		mySwiper2.swipePrev();
	});
	$('.arrow_right_scroll').on('click', function(e){
		e.preventDefault();
		mySwiper2.swipeNext();
	});
	//视频播放
	$('video').mediaelementplayer();
	$('.list_tuwen').mouseover(function(){
		//$(this).addClass('first').siblings().removeClass('first');	
	});
	
	//导航更多
	$('.btn_nav_more').on('mouseover',function(){
		$(this).addClass('on');
		$('.list_nav_more').show();
	});
	$('.nav_more_wrap').on('mouseleave',function(){
		$('.btn_nav_more').removeClass('on');
		$('.list_nav_more').hide();
	});
	//产品展开与收起
	var proList = $('.product_float .list');
	var iconW = proList.find('li').height();
	proList.height(iconW*2+2).css('overflow','hidden');
	proList.find('.list-down').on('click',function(){
		$(this).hide();
		$(this).siblings('.list-up').show();
		$(this).parent().height('auto').css('overflow','visible');
	});
	proList.find('.list-up').on('click',function(){
		$(this).hide();
		$(this).siblings('.list-down').show();
		$(this).parent().height(iconW*2+2).css('overflow','hidden');
	});
	
	window.onscroll = function(){ 
		var t = document.documentElement.scrollTop || document.body.scrollTop;
		var s = $('.fixed_flag').offset().top;
		if( t > s ) { 
			$('.product_float').addClass('fixed');			
		} else { 
			$('.product_float').removeClass('fixed');			
		}
	}
	var ww = $(window).width();
	if(ww>768){
		setTimeout(function(){$("#ad_popup").fadeIn();},0);
		setTimeout(function(){$("#ad_popup").fadeOut();},3000);
	}
	$(".sideRight").load("http://static.takungpao.com/lib/tkpToolBar/toolbar.html", function () {
		tkp.toolBar.init(false);
		$('#btn_comment,#btn_tuji').hide();
	});
	
});
function slideInit(){
	if($(window).width() < 768){ $('.swiper-slide').css('width',$(window).width()-20);}	
}
</script>
