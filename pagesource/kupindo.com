    <script>
$(document).ready(function() {

        var w = $(window).width(),
            h = $(window).height(),
            popup_on_load = $('.popup_on_load'),
            on_load_content = $('.on_load_content');

        popup_on_load.hide();
        on_load_content.hide();

        var date = new Date();
        var dateEnd = new Date("2017/01/14 00:00:00");
        var dateStart = new Date("2016/12/31 23:00:00");

        if (dateEnd.getTime() >= date.getTime() && date.getTime() >= dateStart.getTime()) {
            if (document.cookie.indexOf("visited=yes") == -1) {
                $(window).load(function () {
                    setTimeout(
                        function () {
                            popup_on_load.fadeIn(500);
                            on_load_content.fadeIn(1000);

                            var expiry = new Date();
                            expiry.setHours(date.getHours() + 720);
                            document.cookie = "visited=yes; expires=" + expiry.toGMTString();
                        }, 1000);
                });
            }

            $('.popup_on_load_bck, .on_load_close').click(function () {
                on_load_content.fadeOut(500, function () {
                    popup_on_load.fadeOut(500);
                });
            });
        }


        $('#liquid').liquidcarousel({
            height: 220,
            duration: 300,
            hidearrows: true
        });
    });
