    <script type="text/javascript">
        
          
        $(document).ready(function(){

            var playerInstance = jwplayer("playerMetaData");
            if (document.cookie.indexOf("playedfm=") >= 0) {
                // They've pressed play before.
                playerInstance.setup({
                    file: 'http://streaming.fabrik.fm/yfm/echocast/hls/live.m3u8',
                    hlshtml: true,
                    height: 0,
                    width: 0,
                    autostart:true
                });
                $(".theplayer .fa-play").hide();
                $(".theplayer").html('<i class="fa fa-pause"></i>');
                $(".theplayer .fa-play").hide();
                $(".theplayer").html('<i class="fa fa-pause"></i>');
                $(".theplayer").addClass('active');
            }
            else {
                playerInstance.setup({
                    file: 'http://streaming.fabrik.fm/yfm/echocast/hls/live.m3u8',
                    hlshtml: true,
                    height: 0,
                    width: 0
                });
            }

            //load player instance
            $(".theplayer").on('click', function(){

                console.log('Trompies');
                var $this = $(this);
                $this.toggleClass('active');
                if($this.hasClass('active')){
                    //$this.text('pause'); 
                    $(".theplayer .fa-play").hide();
                    $(".theplayer").html('<i class="fa fa-pause"></i>');
                    console.log("Streaming");
                    jwplayer("playerMetaData").play();  
                    // set a new cookie
                expiry = new Date();
                expiry.setTime(expiry.getTime()+(2*60*1000)); // Two minutes

                // Date()'s toGMTSting() method will format the date correctly for a cookie
                document.cookie = "playedfm=yes; expires=" + expiry.toGMTString();      
                } else{
                    $(".theplayer .fa-pause").show();
                    $(".theplayer").html('<i class="fa fa-play"></i>');
                    jwplayer("playerMetaData").pause();
                    // set a new cookie
                expirys = new Date();
                expirys.setTime(expiry.getTime()-(10*60*1000)); // Ten minutes

                // Date()'s toGMTSting() method will format the date correctly for a cookie
                document.cookie = "playedfm=; expires=Thu, 01 Jan 1970 00:00:01 GMT";

                }
            });
        });

        

        var createCORSRequest = function(method, url) {
            var xhr = new XMLHttpRequest();
            if ("withCredentials" in xhr) {
                    // Most browsers.
                xhr.open(method, url, true);
            } else if (typeof XDomainRequest != "undefined") {
                // IE8 & IE9
                xhr = new XDomainRequest();
                xhr.open(method, url);
            } else {
                // CORS not supported.
                xhr = null;
            }
            return xhr;
            console.log('XHRR '+ xhr);
        };

        function corsRequest(){
            var url = 'http://streaming.fabrik.fm/yfm/echocast/hls/live.m3u8';
            var method = 'GET';
            var xhr = createCORSRequest(method, url);
            xhr.onload = function() {
            // Success code goes here.
                console.log('stream...');
            };
            xhr.onerror = function() {
