function processUrl(url_request, url_destination)
{
	$('#nds_load').css('display', 'block');

	$.ajax({

		url: url_request,

		jsonp: 'callback',

		dataType: "jsonp",

        cache: true,

		success: function(count_listings) {

			// console.log('countListings success');

			if(count_listings > 0) {
				
