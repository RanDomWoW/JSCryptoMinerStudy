<script type="text/javascript">
$(document).ready(function()
{
	var video = document.getElementById('home_video');
	var video_volume = 1;
	$('#home_video').on('click', function()
	{
		if(video.paused) {
			video.play();
			$('#play-pause').css('background', 'url(/images/icons/pause-button.png) no-repeat center');
		} else {
			video.pause();
			$('#play-pause').css('background', 'url(/images/icons/play-button.png) no-repeat center');
		}
	});
	$('#play-pause').on('click', function(e)
	{
		if(video.paused) {
			video.play();
			$('#play-pause').css('background', 'url(/images/icons/pause-button.png) no-repeat center');
		} else {
			video.pause();
			$('#play-pause').css('background', 'url(/images/icons/play-button.png) no-repeat center');
		}
		// $('#play-pause').focus();
		e.preventDefault();
	});
	$('#volume').on('click', function(e)
	{
		console.log(video.volume);
		if(!video.muted) {
			video.muted = true;
			$('#volume-bar').val(0);
			$('#volume').css('background', 'url(/images/icons/volume-0.png) no-repeat center');
		} else {
			video.muted = false;
			$('#volume').css('background', 'url(/images/icons/volume-10.png) no-repeat center');
			$('#volume-bar').val(video.volume);
		}
		e.preventDefault();
	});
	$('#volume-bar').on('change', function(e)
	{
		video.volume = $('#volume-bar').val();
		if(video.volume == 0) {
			$('#volume').css('background', 'url(/images/icons/volume-0.png) no-repeat center');
		} else {
			$('#volume').css('background', 'url(/images/icons/volume-10.png) no-repeat center');
		}
	});
	$('#full-screen').on('click', function(e)
	{
		if(video.requestFullScreen) {
			video.requestFullScreen();
		} else if(video.mozRequestFullScreen) {
			video.mozRequestFullScreen();
		} else if(video.webkitRequestFullScreen) {
			video.webkitRequestFullScreen();
		}
		e.preventDefault();
	});
	$('#seek-bar').on('change', function(e)
	{
		var time = video.duration * ($('#seek-bar').val() / 100);
		video.currentTime = time;
	});
	$('#home_video').on('timeupdate', function()
	{
		var value = (100 / video.duration) * video.currentTime;
		$('#seek-bar').val(value);
	});
	$('#seek-bar').on('mousedown', function()
	{
		video.pause();
	});
	$('#seek-bar').on('mouseup', function()
	{
		video.play();
	});
});


</script>
