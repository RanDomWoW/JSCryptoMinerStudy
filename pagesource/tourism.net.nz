<script type="text/javascript">

	// Callback when any ajax request completes
	$( document ).ajaxComplete(function(e) {
		$.each($('#tabs > ul > li > a'), function(index, value) {
			$(value).removeAttr('tabindex');
		});
		
		$.each($('li'), function(index, value) {
			$(value).removeAttr('tabindex');
		});
	});

	$(document).ready(function()
	{

		//responsive menu
		$('body').on('click', '#responsive-menu > ul > li', function(evt) {
			$(this).animate({
				backgroundColor: '#b5de16'
			}, 300, function(){

			});
		});

		// Select Categories Tab by default.
		var index = $('#tabs a[href="#categories"]').parent().index();
		if(index !== -1) {
			$('#tabs').tabs({ active: index });
			$('#tabs a[href="#categories"]').trigger('click');
		}

		$.each($('#tabs > ul > li > a'), function(index, value) {
			$(value).removeAttr('tabindex');
		});
		$.each($('li'), function(index, value) {
			$(value).removeAttr('tabindex');
		});

		// 
		keyboardMode = 0;
		count = 0;

		//Keydown event
		$('body').on('keydown', function(e)
		{
			resetFocusElements();
		});

		//Keyup event
		$("body").on('keyup', function(e) {

			var keyCode = e.keyCode || e.which; 

			initializeMapAreaElements();

			//trigger mouseover if we're browsing a map
			if($(e.target).is('area')) {
				$(e.target).trigger('mouseover');
			}

			//TAB key
			if (keyCode == 9) {

				//if TAB key is pressed, we activate keyboard mode.
			  	keyboardMode = 1;
			  	
			  	//Sibling of current element is the regions list (Select Region dropdown in Home page)
			  	if($($(e.target).siblings()[0]).attr('id') == 'regions-list') {
			  		//display the list
			  		$('#regions-list').css('display', 'block');
			  		//add class to the list so it is displayed on the left side of the map
			  		$('#regions-list').addClass('regions-list-keyboard');
			  		//remove the small triangle at the top of the list
			  		$($('#regions-list').children()[0]).removeClass('regions-list-before');

			  	} else { //If current element is not regions list...
			  		var found = false;
			  		//... we loop through the parents...
					$.each($(e.target).parents(), function(index, value)
					{
						//... if one of the parent is the regions list, it means we're going through the links of regions list ...
						if(value.id == 'regions-list') {
							found = true;
						}
					});
					//... if we are not, we hide and reset the list.
					if(!found) {
						$('#regions-list').css('display', 'none');
						$('#regions-list').removeClass('regions-list-keyboard');
			  			$($('#regions-list').children()[0]).addClass('regions-list-before');
					}

			  	}

			  	//Browsing through top-menu
			  	//Reinitialize top menu items
		  		$('#top-menu > li > a').removeClass('top-menu-a-hover');
		  		//If current element has a parent with an id && current element parent id is 'top-menu-%'
		  		if($($(e.target).parent()[0]).attr('id') !== undefined && $($(e.target).parent()[0]).attr('id').indexOf('top-menu-') !== -1) {
		  			$(e.target).addClass('top-menu-a-hover');
		  		}
		  		//Browsig through main-menu
		  		//If current element has a parent with an id && current element parent id is 'main-menu-%'
		  		if($($(e.target).parent()[0]).attr('id') !== undefined && $($(e.target).parent()[0]).attr('id').indexOf('main-menu-') !== -1) {
		  			//ul element of the main-menu
		  			$mainNav = $("#site-menu");
		  			
					var $magicLine = $("#magic-line");
					//if the magic-line element is not in the DOM yet we add it for visual aid when TABING (for pages outside of the main menu categories. )
					if($magicLine.length == 0) {
						$mainNav.append("<li id='magic-line'></li>");
						$magicLine = $("#magic-line");
					}
					//Calculate magic-line new position.
					leftPos = $($($(e.target)).parent()[0]).position().left;
						newWidth = $($($(e.target)).parent()[0]).width();
						$magicLine.stop().animate({
							left: leftPos,
							width: newWidth
						});
				} else { //otherwise we check if we're not browsing through the main-menu sub items
					
					found = false;
					//for each parent, we check if one has 'dropdown' class (meaning it is the header of the main-menu megamenu)
					$.each($(e.target).parents(), function(index, parent)
					{
						if($(parent).hasClass('dropdown')) {
							found = true;
						}
					});
					//if not, we remove the magic-line.
					if(!found) {
						$("#magic-line").remove();	
					}
					
				}

				//If current element has a sibling
			  	if($(e.target).siblings()[0] !== undefined) {

			  		//if browsing social links, we hide dropdown menus
					if($($(e.target).parents()[0]).attr('id') == 'social')
					{
						$.each($('.dropdown'), function(index, value)
						{
							$(this).css('display', 'none');
						});
					}

			  		//If current element sibling has 'dropdown' class, meaning we're browsing mega menu header, we display the mega menu
					if($($(e.target).siblings()[0]).hasClass('dropdown')) {
						//first we hide all other mega menus
						$.each($('.dropdown'), function(index, value)
						{
							$(this).css('display', 'none');
						});
						//we remove all the mega menu skiplinks...
						$('.skiplink-megamenu').remove();

						//...and we display the corresponding current element's mega menu
						$($(e.target).siblings()[0]).css('display', 'block');
						
						//if current element's mega menu does not have 'skiplink-megamenu', we add it.
						if(!$($(e.target).siblings()[0]).has('.skiplink-megamenu').length) {
							$($(e.target).siblings()[0]).first('a').css('display', 'block');
							$($(e.target).siblings()[0]).prepend('<div class="skiplink-megamenu"><a href="#" id="skiplink_megamenu_'+count+'" class="skip-mega-menu">Skip</a></div>');
						}
					}
				} else { //if current element does not have siblings (chances are we're browsing top or main menus)

					var found = false;
					//loop through current element's parents
					$.each($(e.target).parents(), function(index, value)
					{
						//if parent is top-menu or site-menu
						if(value.id == 'top-menu') {
							found = true;
						} else if(value.id == 'site-menu') {
							found = true;
						}
					});
					//if none of the parents is top or site menu OR parent 2 levels up is top or site menu (meaning we're browsing the header menu items)
					if(!found || $($(e.target).parent()).parent().attr('id') == 'top-menu' || $($(e.target).parent()).parent().attr('id') == 'site-menu') {
						//we hide all mega menus
						$.each($('.dropdown'), function(index, value)
						{
							$(this).css('display', 'none');
						});
					}
				}
				//display skiplink for main content
			    $('.skiplink').css('opacity', '1');

			    //if focus on skip google ads link
			    if($(e.target).attr('id') == 'skip_ga') {
			    	$(e.target).html('Skip Google Ads');
			    	$(e.target).focus();
			    	$(e.target).addClass('focus');
			    } else {
			    	$('#skip_ga').html('&nbsp;');
			    }

			    //if focus on skip map link
			    if($(e.target).attr('id') == 'skip_map') {
			    	$(e.target).html('Skip Map');
			    	$(e.target).focus();
			    	$(e.target).addClass('focus');
			    } else {
			    	$('#skip_map').html('&nbsp;');
			    }

			    //if focus on skip map region link (Region info page)
			    if($(e.target).attr('id') == 'skip_map_regions') {
			    	$(e.target).html('Skip Map');
			    	$(e.target).focus();
			    	$(e.target).addClass('focus');
			    } else {
			    	$('#skip_map_regions').html('&nbsp;');
			    }

			    //if focus on skip map region link (Videos Region info page)
			    if($(e.target).attr('id') == 'skip_map_regions_video') {
			    	$(e.target).html('Skip Map');
			    	$(e.target).focus();
			    	$(e.target).addClass('focus');
			    } else {
			    	$('#skip_map_regions_video').html('&nbsp;');
			    }

			    //if focus on skip category map link
			    if($(e.target).attr('id') == 'skip_category_map') {
			    	$(e.target).html('Skip Map');
			    	$(e.target).focus();
			    	$(e.target).addClass('focus');
			    } else {
			    	$('#skip_category_map').html('&nbsp;');
			    }

			    //if focus on skip tabs links
			    if($(e.target).attr('id') == 'skip_tabs') {
			    	$(e.target).html('Skip Tabs');
			    	$(e.target).focus();
			    	$(e.target).addClass('focus');
			    } else {
			    	$('#skip_tabs').html('&nbsp;');
			    }

			    //if focus on skip client ads links
			    if($(e.target).attr('id') == 'skip_ads') {
			    	$(e.target).html('Skip Ads');
			    	$(e.target).focus();
			    	$(e.target).addClass('focus');
			    } else {
			    	$('#skip_ads').html('&nbsp;');
			    }

			    //if focus on Map tab
			    if($(e.target).attr('href') == '#map') {
			    	console.log('#map');
			    	$('#map_region_div').css('display', 'block');
			    }
			} else if(keyCode == 13) {
				// if selecting tabs "All Results" or "Gallery"
				if($(e.target).attr('href') == '#listings-list' || $(e.target).attr('href') == '#listings-grid') {
					$(e.target).trigger('click');
				}
			}
		});

		//Click on mega menu skip link
		$('body').on('click', '.skiplink-megamenu', function(evt)
		{
			//hide the current mega menu
			$($(this).parent()[0]).css('display', 'none');
			//remove 'focus' class from all element
			$.each($('.focus'), function(index, value)
			{
				$(this).removeClass('focus');
			});
			//check if the skiplink-megamenu is in the main menu or top menu
			if($.isNumeric($($($(this).parent()[0]).parent()[0]).attr('id').replace('main-menu-',''))) {
				menu = 'main-menu-';
				id = $($($(this).parent()[0]).parent()[0]).attr('id').replace('main-menu-','');	
			} else {
				id = $($($(this).parent()[0]).parent()[0]).attr('id').replace('top-menu-','');	
				menu = 'top-menu-';
			}
			//remove skiplink
			$('.skiplink-megamenu').remove();
			//check if next menu header item exists.
			if($('#'+menu+''+(parseInt(id)+1)).attr('id') !== undefined) {

				//if we're browsing main menu
				if(menu == 'main-menu-') {
					var $magicLine = $("#magic-line");
					leftPos = $('#main-menu-'+(parseInt(id)+1)).position().left;
					newWidth = $('#main-menu-'+(parseInt(id)+1)).width();
					$magicLine.stop().animate({
						left: leftPos,
						width: newWidth
					});
				} else { //if browsing top menu
					$('#top-menu > li > a').removeClass('top-menu-a-hover');
					$('#'+menu+''+(parseInt(id)+1)+' > a').addClass('top-menu-a-hover');
				}

				$('#'+menu+''+(parseInt(id)+1)+' > div.dropdown').css('display', 'block');
				if(!$('#'+menu+''+(parseInt(id)+1)+' > div.dropdown').has('.skiplink-megamenu').length) {
					$('#'+menu+''+(parseInt(id)+1)+' > div.dropdown').prepend('<div class="skiplink-megamenu"><a href="#" id="skiplink_megamenu_'+count+'" class="skip-mega-menu">Skip</a></div>');
				}

				// If willing to highlight Mega menu header
				$('#'+menu+''+(parseInt(id)+1)+' > a').focus();
				$('#'+menu+''+(parseInt(id)+1)+' > a').addClass('focus');

				// If willing to highlight SKIP link in mega menu
				// $('#skiplink_megamenu_'+count).focus();
				// $('#skiplink_megamenu_'+count).addClass('focus');
				count++;
			} else {
				$('#top-menu > li > a').removeClass('top-menu-a-hover');
			}
			
		}).on('mouseup', function(e) 
		{
			$('.skiplink').css('opacity', '0');
			$('.skiplink-megamenu').remove();
			$('#skip_ga').html('&nbsp;');
			$('#skip_map').html('&nbsp;');
			$('#skip_map_regions').html('&nbsp;');
			$('#skip_map_regions_video').html('&nbsp;');
			$('#skip_tabs').html('&nbsp;');
			$('#skip_ads').html('&nbsp;');
			keyboardMode = 0;
		}).on('mousemove', function(e)
		{
			$('.skiplink').css('opacity', '0');
			$('.skiplink-megamenu').remove();
			$('#skip_ga').html('&nbsp;');
			$('#skip_map').html('&nbsp;');
			$('#skip_map_regions').html('&nbsp;');
			$('#skip_map_regions_video').html('&nbsp;');
			$('#skip_tabs').html('&nbsp;');
			$('#skip_ads').html('&nbsp;');
			$('#regions-list').removeClass('regions-list-keyboard');
		  	$($('#regions-list').children()[0]).addClass('regions-list-before');
		  	resetFocusElements();
			keyboardMode = 0;
		});

		$('body').on('click', '#skiplink', function()
		{
			$('#main_content').children().find('h1').first().parent().find('a').first().focus();	
			if($('#inner').length) {
		        var skipTo="#"+this.href.split('#')[1];
		        $(skipTo).attr('tabindex', -1).on('blur focusout', function () {
		            $(this).removeAttr('tabindex');
		        }).focus();
			} else {
				$('#main_content').children().find('h1').first().parent().find('a').first().focus();	
			}
			
		});

		$('body').on('click', '#skip_ga', function()
		{
			$('#aboutus').focus();
			$('#aboutus').addClass('focus');
		});

		$('body').on('click', '#skip_map', function()
		{
			$('#makebooking').focus();
			$('#makebooking').addClass('focus');
		});

		$('body').on('click', '#skip_map_regions', function()
		{
			$('#northland_region_info').focus();
			$('#northland_region_info').addClass('focus');
		});

		$('body').on('click', '#skip_map_regions_video', function()
		{
			$('#new_zealand_video_page_link').focus();
			$('#new_zealand_video_page_link').addClass('focus');
		});

		$('body').on('click', '#skip_category_map', function()
		{
			$('#skip_ga').focus();
			$('#skip_ga').addClass('focus');
			$('#skip_ga').html('Skip Google Ads');
		});

		$('body').on('click', '#skip_tabs', function()
		{
			$('#skip_ads').focus();
			$('#skip_ads').addClass('focus');
			$('#skip_ads').html('Skip Ads');
		});

		$('body').on('click', '#skip_ads', function()
		{
			$('#skip_ga').focus();
			$('#skip_ga').addClass('focus');
			$('#skip_ga').html('Skip Google Ads');
		});

	});

	
	

	//Initialize page for keyboard-only use by looping through element with 'focus' class and removing the class.
	function resetFocusElements()
	{
		$.each($('.focus'), function(index, value)
		{
			$(this).removeClass('focus');
		});
	}

	//Init area elements
	function initializeMapAreaElements()
	{
		$.each($('area'), function(index, value)
		{
			$(value).trigger('mouseout');
		});
	}
	
	

	
</script>


