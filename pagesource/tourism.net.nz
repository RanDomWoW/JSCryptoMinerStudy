	$('body').on('mousedown', 'a, select, input, textarea', function(evt)
	{
		$(this).attr('data-pressed', 'true');
	}).on('mouseup', 'a, select, input, textarea', function(evt)
	{
		$(this).attr('data-pressed', 'false');
		$(this).removeClass('focus');
	}).on('focus', 'a, select, input, textarea', function(evt)
	{
		if($(this).attr('data-pressed') != 'true') {
			$(this).addClass('focus');
		}
	}).on('blur', 'a, select, input, textarea', function(evt)
	{
		$(this).removeClass('focus');
	});
});
