<script>
jQuery.noConflict();
jQuery(window).load(function() {
	// For Mobile Menu
	jQuery("#mmobile").removeClass('mm-remove');
	// For Urgent News
	jQuery.ajax({
		type: "GET",
		url: "http://www.ariffino.net/wp-admin/admin-ajax.php",
		dataType: 'html',
		data: ({ action: 'load_urgent_news'}),
		success: function(data){
			jQuery(".auto-hide").append(jQuery(data));
		}
	});
	jQuery(".urgent").slideDown(9000);
	jQuery("body").on('click', '.urgent span.close-urgent', function() {
		jQuery(".urgent").slideUp(500);
		return true;
	});
	 
	jQuery(".auto-hide").delay(30000).fadeOut("slow");
	});
</script>
