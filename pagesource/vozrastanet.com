<script>
    function sendNewSubscriber(sender, operation) {
        $(sender).prop("disabled", true);
        var form = $(sender).parents("form:first");
        form.find(".error").remove();
        $.post("/subscribe", form.serialize() + "&operation=" + operation + "&locale=ru-RU", function (data) {
            $(sender).prop("disabled", false);
            if (data.status === "Ok") {
                $('.jsModalSubscribe').modal('hide');
                $('.jsModalSubscribeSuccess').modal('show');
                form.find("input[type=text], input[type=email]").val("");
                if (operation === 'Etap2PodpiskaNaRassylkuSprava') {
                    dataLayer.push({
                        'event':'event',
                        'event_category':'vozrastanet',
                        'event_action':'right_menu',
                        'event_label':'subscribe_appy'
                    });
                } else if (operation === 'Etap2OsnovnayaFormaPodpiski') {
                    dataLayer.push({
                        'event':'event',
                        'event_category':'vozrastanet',
                        'event_action':'subscribe',
                        'event_label':'click'
                    });
                }
            } else {
                for (var field in data.message) {
                    for (var i = 0; i < data.message[field].length; i++) {
                        var error = '<div class="error" style="display:block">' + data.message[field][i] + '</div>';
                        form.find("[name=" + field + "]").after(error);
                    }
                }
            }
        });
    }
</script>
