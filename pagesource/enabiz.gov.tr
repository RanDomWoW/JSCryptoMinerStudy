                function SMSGonder() {
                    var tc = $("#inlineFormInputGroupUsername2").val();
                    var cep = $("#exampleInputPassword2").val();
                    if (tc === "" || cep === "") {
                        $("#lbSonuc").html("* T.C. kimlik numarası ve cep telefonu bilgisi boş olamaz.");
                    }
                    else {
                        $.ajax({
                            /**/
                            /**/
                            /**/
                            url: "/Account/SMSOnayi",
                /**/
                /**/
                /**/
                data: { tc, cep },
                beforeSend: function (xhr) {
                    xhr.setRequestHeader("XSRF-TOKEN",
                        $('input:hidden[name="__RequestVerificationToken"]').val());
                },
                type: "POST",
                success: function (data) {
                    if (data === "1") {
                        $("#dvsmsOnayi").css("display", "block");
                        $(".dvsmsOnayi-bg").css("display", "block");
                        $("#ah-giris").css("display", "none");
                        $("#sifremi-unuttum").css("display", "none");
                        $(".sifremi-unuttum-bg").css("display", "none");
                        $('#ah-giris-btn').removeClass('ah-giris-active');
                        $('.ah-giris-active-x').css("display", "none");
                    }
                    else if (data === "2") {
                        $("#lbSonuc").html("* SMS gönderimi sırasında bir hata alındı tekrar deneyiniz.");
                    }
                    else if (data === "3") {
                        $("#lbSonuc").html("* Girilen bilgiler ile eşleşen bir kullanıcı bulunamadı , e-Devlet üzerinden giriş yaparak şifrenizi değiştirebilirsiniz.");
                    }
                    else if (data === "4") {
                        $("#lbSonuc").html("* T.C. kimlik numarası ve cep telefonu bilgisi boş olamaz.");
                    }

                },
                error: function () {

                }
            });
        }

    }
    function SifremiUnuttum() {

        var tc = $("#iSMSKodu").val();
        var kod = $("#ismsOnay").val();
        if (tc === "" || kod === "") {
            $("#lbSmsSonuc").html("* T.C. kimlik numarası ve güvenlik kodu bilgisi boş olamaz.");
        }
        else {
            $.ajax({
                /**/
                url: "/Account/SifremiUnuttum",
                /**/
                data: { tc, kod },
                type: "POST",
                beforeSend: function (xhr) {
                    xhr.setRequestHeader("XSRF-TOKEN",
                        $('input:hidden[name="__RequestVerificationToken"]').val());
                },
                success: function (data) {
                    if (data === "1") {
                        $("#lbSmsSonuc2").html("* Şifreniz cep telefonunuza gönderilmiştir , giriş yaptığınızda şifrenizi değiştiriniz.");
                        $("#enabiz-giris").css("display", "block");
                        $("#dvsmsOnayi").css("display", "none");
                        $(".dvsmsOnayi-bg").css("display", "none");
                        $("#ah-giris").css("display", "none");
                        $("#sifremi-unuttum").css("display", "none");
                        $(".sifremi-unuttum-bg").css("display", "none");
                        $('#ah-giris-btn').removeClass('ah-giris-active');
                        $('.ah-giris-active-x').css("display", "none");
                    }
                    else {
                        $("#lbSmsSonuc").html("* Şifre gönderimi sırasında hata alındı.");
                    }
                },
                error: function () {

                }
            });
        }

    }
