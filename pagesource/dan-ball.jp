	else if(r<11)	{h='m';						s='mobile2';	}
	else if(r<12)	{h='javagame\/bit';			s='bit';		}
	else if(r<13)	{h='javagame\/dust2';		s='dust2';		}
	else if(r<14)	{h='javagame\/monsterbox';	s='monsterbox';	}
	else if(r<15)	{h='javagame\/lim';			s='lim';		}
	else			{h='javagame\/hamrace2';	s='ham2';		}

	if(tbi==0){
		document.getElementById("top_banner_a").href=u+h+'\/';
		document.getElementById("top_banner_i").src='\/images\/top_'+s+'.png';
	}

	setTimeout("top_banner('"+u+"')",5000);
}
