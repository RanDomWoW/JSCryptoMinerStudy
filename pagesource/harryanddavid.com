<script type="text/javascript" data-content="HD_contGlobalPriceFilter">
    $(function () {
        if (urlParam('priceFilter').length && !$('#clubsEspot').length && !$('#deptFeatA').length) {

            if ($('.Co-Products').length == 200) {
                $('.pagination-row1').hide();
            }

            var range = UX.urlParam('priceFilter').split('-'),
                low = parseInt(range[0]),
                high = parseInt(range[1]);

            $('.Co-Products').each(function () {
                if ($(this).find('.blueProductScrollPrice').length) {
                    var cost = parseFloat($(this).find('.blueProductScrollPrice').html().split('$')[1])
                    if (cost < low || cost > high)
                        $(this).remove();
                }
                if ($(this).find('.discountPrice').length) {
                    var cost = parseFloat($(this).find('.discountPrice').html().split('$')[1]);
                    if (cost < low || cost > high)
                        $(this).remove();
                }
            })
            if ($('#Sort').length) {
                $('#Sort option').each(function () {
                    $(this).attr('value', $(this).attr('value') + '&priceFilter=' + UX.urlParam('priceFilter'));
                })
            }
            updateCount();
        }
        function updateCount(){
            $('.nbr-items strong').html($('.Co-Products').length);
        }
        function urlParam(name) {
            name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
            var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
                results = regex.exec(location.search);
            return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
        }
    });
</script><!--HD_contB2BCMS-->
