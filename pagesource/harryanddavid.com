            $('#membershipPromo').css({
                background: '#ffc'
            }).attr('placeholder', 'Membership ID');
        },
        applyTmoBanner: function() {
            console.log('applying TMO banner');
            var $error = $('#errorMsg'),
                errorMsg = 'PleaseenteravalidmembershipID.<br><br>',
                bannerSpot = $('body').hasClass('co-cart') ? '#updateOrderItemForm' : '#showcheckoutPpBanner';
            if ($error != undefined && $error.html() != undefined)
                    if ($error.html().replace(/\s/g, '') == errorMsg)
                        $error.remove()


            $('<div>').css({
                background: '#e20074 url(data:image/png;base64,) no-repeat 5px center',
                backgroundSize: '25%',
                color: '#fff',
            }).addClass('tMoBanner').insertBefore(bannerSpot);
            $('<p style="margin-left: 28%;padding: 0.5%;font-size: 12px;"><strong>- On Payment Page -</strong><br>1: Expand Promotional Code section <br>2: Enter promo code <strong>MOB</strong><br>3: Enter 10-digit pin in Membership field</p>').appendTo('.tMoBanner');
        },
        getCookie: function(cookieName) {
            var name = cookieName + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1);
                if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
            }
            return "";
        }
    }

    $(function() {
        if (TMO.getCookie('ADV_ATTR_COOKIE') === 'tmobilehd')
            TMO.init();
    })
