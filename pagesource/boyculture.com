      }
      if ($(this).hasClass($(elem).attr('name')+"_")) {
        $(this).addClass('current');
        $(this).addClass($(elem).attr('name')+"_current");
        $(this).removeClass($(elem).attr('name')+"_");
      }
    });
    $('#category_scroll').animate({'scrollLeft':elem.offsetLeft},500, function() {
