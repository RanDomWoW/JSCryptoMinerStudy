<script>
    jQuery(document).ready(function($) {
            if (location.hash == '#product') {
                $('#head_nav li').each(function(i,v) {
                    $(v).removeClass('on');
                    $('#nav_product').addClass('on');
                });
            }
            $('#nav_product').click(function(){
                $('#head_nav li').each(function(i,v) {
                    $(v).removeClass('on');
                    $('#nav_product').addClass('on');
                });
            });

            function hashChangeFire(){
                if(window.location.hash==="#product"){
                    var timer = setInterval(function() {  
                        var body_top = (document.body.scrollTop||document.documentElement.scrollTop);
                        if(body_top == $(".banner").height()){
                            clearInterval(timer);
                        }else{
                            document.body.scrollTop = $(".banner").height();
                            document.documentElement.scrollTop = $(".banner").height();
                        }
                        
                    }, 10);  
                }
            }
            
            hashChangeFire();
            
            var yixiaObj = new yxObj();
            yixiaObj.homepage();
        });

</script>
