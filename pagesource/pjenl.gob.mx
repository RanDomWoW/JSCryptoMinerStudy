    <script type="text/javascript">
        function showMsg(id,msg)
        {
            var modal = $('#' + id);
            $(modal).find('.modal-body').html('<p>' + msg +'</p>');
            $(modal).modal('show');
        }

        function Init() {
            $('#Buscador_lnkBuscar').on('click', function (e) {
                if ($("#Buscador_txtBuscar").val().length == 0)  {
                    e.preventDefault();
                    showMsg('Buscador_Mensaje', 'Escriba en el cuadro de texto para buscar');
                }
            });
            $('#Buscador_sectionResultado').on('show.bs.tab', function (e) {
                if ($("#Buscador_txtBuscar").val().length == 0) {
                    e.preventDefault();
                    showMsg('Buscador_Mensaje', 'Escriba en el cuadro de texto para buscar');
                }
            });
            $('#Buscador_sectionResultado').on('shown.bs.tab', 'a[data-toggle="tab"]', function (e) {
                if ($('#Buscador_hdnBuscarEn').val() != $(e.target).attr('aria-controls')) {
                    $('#Buscador_hdnBuscarEn').val($(e.target).attr('aria-controls'));
                    if ($("#Buscador_txtBuscar").val().length > 0) {
                        __doPostBack('Buscador$lnkBuscar', '');
                    }                    
                }
            })

            //Autocompletado
            $("#Buscador_txtBuscar").autocomplete({
                source: function (request, response) {
                    $.ajax({
                        type: "POST",
                        url: "/Buscar/Index.aspx" + "/Buscar",
                        dataType: "json",
                        contentType: 'application/json; charset=utf-8',
                        data: '{texto: "' + request.term + '"}',
                        success: function (data) {
                            response($.map(JSON.parse(data.d), function (item) {
                                return {
                                    label: item.label,
                                    value: item.value
                                };
                            }));
                        },
                        error: function (xhr, status, error) {
                            alert(error);
                        }
                    });
                },
                minLength: 3,
                select: function (event, ui) {
                    event.preventDefault();
                    //$(this).val(ui.item.label);
                    window.location.href = ui.item.value;
                    //window.open(ui.item.value, '_blank');
                },
                open: function () {
                    $(this).removeClass("ui-corner-all").addClass("ui-corner-top");
                },
                close: function () {
                    $(this).removeClass("ui-corner-top").addClass("ui-corner-all");
                },
                focus: function (event, ui) {
                    event.preventDefault();
                    $(this).val(ui.item.label);
                }
            });
        }

    </script>
