<script>

  function Showcase(){
    this.$container = $('.grids-wrapper');
    this.$filter = $('.category-filter');
    this.$filterSelect = $('.category-filter-dropdown select');
    this.initEvents();

    if(!(window.location.hash && this.show(window.location.hash.toString().replace(/^#/, '')))){
      this.initClick = true;
      this.$filter.find('[data-f-capacity]:first').click();
    }

  }

  Showcase.prototype.initEvents = function(){
    $(document).on('click','.category-filter [data-f-capacity]', this.onChangeCapacity.bind(this));
    $(document).on('change','.category-filter-dropdown select', this.onChangeCapacity.bind(this));
  }

  Showcase.prototype.onChangeCapacity = function(event){
    var cc = $(event.currentTarget).data('f-capacity') || $(event.currentTarget).val();
    this.show(cc);
  }

  Showcase.prototype.show = function(cc){

    if(this.$container.find('[data-capacity="'+cc+'"]').length == 0){
      return false
    }

    this.$container.find('[data-capacity]').toggleClass('hide', true);
    this.$container.find('[data-capacity="'+cc+'"]').toggleClass('hide', false);

    this.$filter.find('[data-f-capacity]').toggleClass('active', false);
    this.$filter.find('[data-f-capacity="'+cc+'"]').toggleClass('active', true);

    this.$filterSelect.prop('value', cc);

    if(!this.initClick){
      this.scrollToShowcase();
    }else{
      delete this.initClick;
    }

    return true;
  }

  Showcase.prototype.scrollToShowcase = function(){
    $(function(){
      $('body, html').stop().animate({
        scrollTop: $('.motor-styles-showcase').offset().top - (window.innerWidth <= 768 ? $('.top-nav').outerHeight() : 0)
      }, 500);
    });
  }

  new Showcase();

</script>

