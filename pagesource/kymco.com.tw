  "use strict";

  $(document).one('lazybeforeunveil', function(){
    window.carousel.trigger('refresh.owl.carousel');
  });

  $(document).ready(function(){

    $('.banner-slider [data-due]').filter(function(){
      return new Date().getTime() > new Date(this.getAttribute('data-due')).getTime();
    }).remove();

    window.carousel = $('.banner-slider').owlCarousel({
      loop: true,
      navigation : true,
      slideSpeed : 300,
      paginationSpeed : 400,
      singleItem: true,
      items: 1,
      autoHeight: true,
      autoplay: true,
      autoplayTimeout: 5000
    });
  });

</script>
