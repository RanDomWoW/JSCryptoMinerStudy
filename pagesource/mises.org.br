        // editoriais
        var editorialsOpen = false;
        function EditorialToggle() {
            var e$ = $('#editorials'),
                m$ = $('#editorials_menu .glyphicon');
            if (editorialsOpen) {
                e$.slideUp();
                m$.addClass('glyphicon-menu-down').removeClass('glyphicon-menu-up');
            }
            else {
                e$.slideDown();
                m$.addClass('glyphicon-menu-up').removeClass('glyphicon-menu-down');
            }
            editorialsOpen = !editorialsOpen;
        }

