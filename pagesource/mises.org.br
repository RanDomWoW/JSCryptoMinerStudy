    <script type="text/javascript">
        // eventos
        var eventId = 0;
        function GetEvent(p) {
            $.ajax('AjaxWs.aspx/GetEvent' + p, {
                dataType: "json",
                type: 'POST',
                contentType: "application/json",
                cache: false,
                async: true,
                data: JSON.stringify({ eventId: eventId }),
                success: function (response) {
                    DisplayEvent(response);
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    r = false;
                    if (window.console) {
                        console.log(jqXHR.status + ': ' + jqXHR.statusText);
                        console.log(jqXHR.responseText);
                    }
                }
            });
        }
        function DisplayEvent(r) {
            var d = JSON.parse(r.d);
            console.log(d);
            eventId = d.EventId;
            if (eventId > 0) {
                $('#calendar').show();
                $('#calendar_day').html(d.DateDay);
                $('#calendar_month').html(d.DateMonth);
                $('#calendar_title').html(d.Name);
                $('#calendar_subtitle').html(d.DateLong);
                $('#calendar_link').prop('href', 'Event.aspx?id=' + d.EventId);
            }
            else
                $('#calendar').hide();
        }

        // podcasts
        var podcastId = 0;
        var playAdded = false;
        function GetPodcast(p) {
            $.ajax('AjaxWs.aspx/GetPodcast' + p, {
                dataType: "json",
                type: 'POST',
                contentType: "application/json",
                cache: false,
                async: true,
                data: JSON.stringify({ podcastId: podcastId }),
                success: function (response) {
                    DisplayPodcast(response);
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    r = false;
                    if (window.console) {
                        console.log(jqXHR.status + ': ' + jqXHR.statusText);
                        console.log(jqXHR.responseText);
                    }
                }
            });
        }
        function DisplayPodcast(r) {
            var d = JSON.parse(r.d);
            //console.log(d);
            if (d.FileUpId > 0) {
                podcastId = d.FileUpId;
                $('#podcast_author').html(d.AuthorName);
                $('#podcast_name').html(d.Description);
                $('#podcast_audio').prop('src', 'https://cdn.mises.org.br/audio/' + d.FileUpId + '.mp3');
                $('#podcast_facebook').prop('href', d.FacebookShare);
                $('#podcast_twitter').prop('href', d.TwitterShare);
                playAdded = false;
            }
        }

        function GetCitationRand() {
            $.ajax('AjaxWs.aspx/GetCitationRand', {
                dataType: "json",
                type: 'POST',
                contentType: "application/json",
                cache: false,
                async: true,
                success: function (response) {
                    var d = JSON.parse(response.d);
                    console.log(d);
                    if (d.CitationId > 0) {
                        $('#citation_author').html(d.AuthorName);
                        $('#citation_text').html(d.Text);
                        $('#citation_image').css('background-image', 'url(https://cdn.mises.org.br/images/authors_thumbs/' + d.AuthorId + '.png)');
                        $('#citation_url').prop('href', 'Citation.aspx?id=' + d.CitationId);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    r = false;
                    if (window.console) {
                        console.log(jqXHR.status + ': ' + jqXHR.statusText);
                        console.log(jqXHR.responseText);
                    }
                }
            });
        }

