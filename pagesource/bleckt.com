		function reopen_video(video, n, flag) {
			close_tool();
			setTimeout("open_video('"+video+"',"+ n +")",400);
		}
		function open_video(video, n, flag) // открывает попап с видео. файл - n-ный
		{
			//close_tool();
			$('#prohibit_downloading').css('visibility','hidden');
			$('#prohibit_downloading').css('display','none');
			$.post('http://bleckt.com:80/t/public.php',{action:'load_video', id:video, n:n, flag:flag}, function(e)
			{
				$('#standartTool').html(e);
				open_tool();
			});
			if(flag=='N') {

				$('#prohibit_downloading').css('visibility','visible');
			}
		}
		
		function open_tool()
		{
			if (window.tool_open){
				close_tool();
				return_tool_to_default_param();
			}

			$('#toolCont, #tool').customFadeIn(500);
			window.tool_open = 1;
			
			h_inner = $('.main').height();
			w_inner = $('.main').width();
			$('.container').height(h_inner);
			$('#toolCont').height(h_inner);
			
			w_w = $(window).width();
			
			if (w_w>w_inner)
				mar_left = (w_w-w_inner)/2;
			else
				mar_left = 0;
			
			window.iScrolled = $(window).scrollTop();
			$('.main').css('position', 'fixed').css('margin', '-'+iScrolled+'px auto 0 '+mar_left+'px');
			$(window).scrollTop(0);
		}	 // function open_tool(type)

		function close_tool() 
		{
			$('#toolCont, #tool').customFadeOut(500);
			setTimeout("return_tool_to_default_param()",400);
		}
		function return_tool_to_default_param()
		{
			window.tool_open = 0;
			
			$('#toolCont, #toolClose').css('height','100%');
			$('#standartTool').html('');
			
			$('.main').css('margin-top', '0px').css('position', 'relative').css('margin','0 auto');
			$(window).scrollTop(iScrolled);	
		}		
		function isValidEmailAddress(emailAddress) 
		{
			var pattern = new RegExp(/^(("[\w-\s]+")|([\w-]+(?:\.[\w-]+)*)|("[\w-\s]+")([\w-]+(?:\.[\w-]+)*))(@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][0-9]\.|1[0-9]{2}\.|[0-9]{1,2}\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\]?$)/i);
			return pattern.test(emailAddress);
		};
		function add_new_comment(id)
		{
			var n = $('#commentary_box input[name$="name"]').val();
			var e = $('#commentary_box input[name$="email"]').val();
			var t = $('#commentary_box textarea[name$="msg"]').val();
			var m_city = $('#commentary_box input[name$="city"]').val();
			var m_country = $('#commentary_box input[name$="country"]').val();
			var error = 0;
			if (n == '' || n == '* Имя:')
			{
				error = 1;
				$('#commentary_box input[name$="name"]').addClass('inputError');
			}
			else
				$('#commentary_box input[name$="name"]').removeClass('inputError');
			

/*==================================================================================================*/
			
			if (m_city == '' || m_city == '* Город:')
			{
				error = 1;
				$('#commentary_box input[name$="city"]').addClass('inputError');
			}
			else
				$('#commentary_box input[name$="city"]').removeClass('inputError');	
				
			if (m_country == '' || m_country == '* Страна:')
			{
				error = 1;
				$('#commentary_box input[name$="country"]').addClass('inputError');
			}
			else
				$('#commentary_box input[name$="country"]').removeClass('inputError');
			if (e == 'E-mail: (не публикуется)')
			{
				e='';
			}
			/*	
			if (e == '' || e == 'E-mail: (не публикуется)' || !isValidEmailAddress(e))
			{
				error = 1;
				$('#commentary_box input[name$="email"]').addClass('inputError');
			}
			else
				$('#commentary_box input[name$="email"]').removeClass('inputError');
			*/
			
			if (t == '' || t == 'Добавить комментарий:')
			{
				error = 1;
				$('#commentary_box textarea[name$="msg"]').addClass('inputError');
			}
			else
				$('#commentary_box textarea[name$="msg"]').removeClass('inputError');
				
			if (error == 0)
			{
				if (window.comment == 'no')
				{
					window.comment = 'send';
					$.post('http://bleckt.com:80/t/public-script.php',{action:'new_comment', id:id, name:n, email:e, text:t, city:m_city, country:m_country}, function(e)
					{
						$('#script').html(e);
						window.comment = 'add';
					});
				}
				else if (window.comment == 'send')
				{
					//al('Ваш комментарий добавлен на сервер.');
					$('#add_comment_box').fadeIn(10).delay(7500).fadeOut(200);
				}
				else if (window.comment == 'add')
				{
					//al('Ваш комментарий добавлен на сервер.');
					$('#add_comment_box').fadeIn(10).delay(7500).fadeOut(200);
				}
			}
			
		}
