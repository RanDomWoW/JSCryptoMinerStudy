                    var selectedImage = pressImages[index];
                    $(selectedImage).addClass("Press-image--selected");
                }
            });
        });
    </script>

    <script>
        // Hero carousel
        (function($) {
            $.fn.carousel = function() {

                var _carousel = $(this);
                var _carouselItems = $(".Carousel-item");
                var _currentItem = 0;
                var _rotateSpeed = 3000; // milliseconds
                var _rotate = null;

                function centerItem() {
                    var currentItem = $(_carouselItems[_currentItem]).addClass("Carousel-item--current");

                    _carouselItems.removeClass("Carousel-item--prev Carousel-item--next Carousel-item--current");
                    _carouselItems.addClass("Carousel-item--hidden");

                    var nextItem = $(_carouselItems[_currentItem+1]),
                        prevItem = $(_carouselItems[_currentItem-1]);

                    if ( _currentItem === 0 ) {
                        prevItem = _carouselItems.last();
                    }
                    else if ( _currentItem === _carouselItems.length-1 ) {
                        nextItem = _carouselItems.first();
                    }

                    nextItem.removeClass("Carousel-item--hidden").addClass("Carousel-item--next");
                    prevItem.removeClass("Carousel-item--hidden").addClass("Carousel-item--prev");

                    currentItem
                        .addClass("Carousel-item--current")
                        .removeClass("Carousel-item--prev Carousel-item--next Carousel-item--hidden");
                }

                function moveTo(index) {
                    _currentItem = index;
                    centerItem();
                }

                function moveNext() {
                    if (_currentItem < _carouselItems.length - 1) {
                        _currentItem++;
                    }
                    else {
                        _currentItem = 0;
                    }
                    centerItem();
                }

                function startRotate() {
                    _rotate = setInterval( function() {
                        moveNext();
                    }, _rotateSpeed);
                }

                function init() {
                    // Fade in
                    _carousel.hide().css("visibility","visible").fadeIn(400,function(){ centerItem(); });

                    // Move on click
                    _carouselItems.on("click", function() {
                        moveTo(_carouselItems.index(this));
                    });

                    // Start auto rotate
                    startRotate();

                    // Hold rotateon hover
                    _carouselItems.hover(function() {
                        clearInterval(_rotate);
                    }, function() {
                        startRotate();
                    });
                }

                init()
            };
        })( jQuery );
    </script>



        
