			$(document).ready(function () {
				$("#topmenu .metis").metisMenu();

				$("#topmenu .dropdown .dropdown-menu a").on("click", function (e) {
					let keepMenuOpen = $(this).data("ko");

					$(this).parents(".dropdown").data("ko", keepMenuOpen);
				});

				$("#topmenu .dropdown").on("hide.bs.dropdown", function (e) {
					let ko = $(this).data("ko");
					$(this).removeData("ko");

					return ko !== true;
				});

				$(document).on("click", function (e) {
					let clickover = $(e.target);
					let navbar = $("#topmenu .collapse");

					if (navbar.hasClass("in") && !clickover.hasClass("has-arrow")) {
						navbar.collapse('hide').attr("aria-expanded", "false");
						navbar.closest('li').removeClass("active");
						navbar.prev("a.has-arrow").attr("aria-expanded", "false");
					}
				});
			});
		</script>
	
