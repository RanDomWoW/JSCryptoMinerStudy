 function hideWhenClickedOnBody(elm,ns){
    if(elm.is(':visible'))
        {$(document).on('click.'+ns,function(){
            elm.hide();
            $(document).off('click.'+ns);
        });
    
    }else{
        $(document).off('click.'+ns);
    }
}

