<script type="text/javascript">
	var nextProductUrl = null;
	
	(function($) {
				
		$("#frm-comment").submit(function(e) {
			var error = false,
				score = jQuery("#frm-comment-rating").raty('score');

			e.preventDefault;

			jQuery("#error", this).parents("tr").remove();
			jQuery(".error-terms", this).parents("tr").remove();

			if (typeof score === "undefined") {
				error = true;
				jQuery("#frm-comment-rating").parents("tr").after('<tr><td><div id="error"><p class="vykricnik">Prosím, ohodnoťte produkt.</p></div></td></tr>');
			}
			
			if ($("#frm-comment-title").val().length > 250) {
				error = true;
				jQuery("#frm-comment-title").parents("tr").after('<tr><td><div id="error"><p class="vykricnik">Titulok je príliš dlhý, maximálna dĺžka je 250 znakov.</p></div></td></tr>');
			}
			
			if ($("#frm-comment-comment").val().length > 250) {
				error = true;
				jQuery("#frm-comment-comment").parents("tr").after('<tr><td><div id="error"><p class="vykricnik">Komentár je príliš dlhý, maximálna dĺžka je 250 znakov.</p></div></td></tr>');
			}
			
			if($('#agree_terms').prop("checked")==false) {
				error = true;
				jQuery("#agree_terms").parents("tr").after('<tr><td><div id="error" class="error-terms"><p class="vykricnik">Musíte akceptovať zásady ochrany osobných údajov.</p></div></td></tr>');
			}

			if (error ){
				return false;
			}
			
			var data = $(this).serialize();
			
			$.post(location.href, data, function(payload) {
				if (payload.hasOwnProperty("nextProductUrl")) {
					nextProductUrl = payload.nextProductUrl;
				}
				
				$("#commentFormDialog").hide();
				
				$("#commentText").text($("#frm-comment-comment").val());
				$("#ratingStars").data('rating', score);
				setupRating($("#ratingStars"), "white");
				
				if ($("#frm-comment-showName").is(":checked")) {
					$("#showNameText").hide();
				} else {
					$("#showNameText").show();
				}
				
				$("#ratingText").text(ratyHints[score - 1]);
				
				if (nextProductUrl === null) {
					$(".commentContinue").hide();
				}
				$("#commentFormThanksDialog").show();
			});
			
			return false;
		});
		
		$("#frm-comment-comment").on("keyup", function() {
			var val = $(this).val(),
				len = val.length,
				maxChar = 250,
				remain = maxChar - len;
			
			if (len >= maxChar) {
				$(this).val(val.substring(0, maxChar));
			}
			
			if (remain < 0) {
				remain = 0;
			}
			
			$('#commentsCharNum').text(remain);
		});
	})(jQuery);
	
	function nextProductForRating() {
		if (nextProductUrl !== null) {
			location.href = nextProductUrl;
		} else {
			hidecommentthanks();
		}
	}
	
	function showRatingForm(){
		$("#commentFormDialogBack").show();
		$("#commentFormDialog").show();
	}
</script>

