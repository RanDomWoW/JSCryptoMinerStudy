    <script type="text/javascript">
        $(document).ready(function () {
            $('#tabs div').hide();
            //$('#tabs div:first').show();
            //$('#tabs ul li:first').addClass('active');
            $('#tabs #aTitle li a').click(function () {
                $('#tabs #aTitle li').removeClass('active');
                $(this).parent().addClass('active');
                var currentTab = $(this).attr('href');
                $('#tabs div').hide();
                $(currentTab).show();
                return false;
            });
            i = 0;
            $('#tabs #aTitle li').each(function (i) {
                if ($(this).is('.active')) {

                    i = i + 2;

                    $('#tabs div').hide();
                    $('#tabs div:nth-child(' + i + ')').show();
                    //alert(i);



                }
            });


        });
        
    </script>
