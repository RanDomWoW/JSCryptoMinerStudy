        $(".fullPage").css("height",h+"px");
        var i=1,f=0;
        var scrollTime;
        function scrollBody(f) {
            $(document.body).stop(true,true).animate({scrollTop:f*h+'px'},1000,"easeInOutExpo",function () {
                //i=1;
            });
            $(document.documentElement).stop(true,true).animate({scrollTop:f*h+'px'},1000,"easeInOutExpo",function () {
                //i=1;
            });
        }
        //锚点
        $(".dots ul li").eq(0).addClass("dlActive");
        $(".dots ul li").on("click",function(){
            var index=$(this).index();
            f=index;
            $("body,html").animate({scrollTop:f*h+'px'},500);
        });
        var scrollFunc=function(e){
            var e=e || window.event;
            e.preventDefault();
            if(e.wheelDelta && i){
                i=0;
                if(e.wheelDelta<0){
                    f=f==5 ? 5 : f+1;
                    //f++;
                    scrollBody(f);
                }else {
                    f=f==0 ? 0 : f-1;
                    //f--;
                    scrollBody(f);
                }
            }else if(e.detail && i){
                i=0;
                if(e.detail>0){
                    f=f==5 ? 5 : f+1;
                    scrollBody(f);
                }else {
                    f=f==0 ? 0 : f-1;
                    scrollBody(f);
                }
            }
            scrollTime=window.setTimeout(function () {
                i=1;
                window.clearTimeout(scrollTime);
            },500)
        };
        //事件监听及兼容
        function addEventHandler(type,func){
            if(document.addEventListener){
                //监听IE9，谷歌和火狐
                document.addEventListener(type, func, false);
            }else if(document.attachEvent){
                document.attachEvent("on" + type, func);
            }else{
                document["on" + type] = func;
            }
        }
        addEventHandler('DOMMouseScroll',scrollFunc);
        addEventHandler('mousewheel',scrollFunc);
        addEventHandler('keydown',function (e) {
            var e=e||window.event;
            var i=e.keyCode;
            if(i==40 || i==34 || i==32){
                e.preventDefault();
                f=f==5 ? 5 : f+1;
                scrollBody(f);
            }else if(i==38 || i==33){
                e.preventDefault();
                f=f==0 ? 0 : f-1;
                scrollBody(f);
            }else if(i==36){
                e.preventDefault();
                f=0;
                scrollBody(f);
            }else if(i==35){
                e.preventDefault();
                f=5;
                scrollBody(f);
            }
        });
