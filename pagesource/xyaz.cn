        function bannerAnimate() {
            if(parseInt($("body").css("width"))>=1366){
                $(".strip-l").css({"width":1070*data.length+"px","margin-right":-((data.length-1)*535+187)+"px"});
                $(".strip-r").css({"width":1070*data.length+"px","margin-left":"-185px"});
                c=parseInt($(".project-detail .project").css("width"));
                l=$(".strip-l").css("margin-right");
                r=$(".strip-r").css("margin-left");
                setTimeA();
            }else{
                $(".strip-l").css({"width":1030*data.length+"px","margin-right":-((data.length-1)*535+218)+"px"});
                $(".strip-r").css({"width":1030*data.length+"px","margin-left":"-216px"});
                c=parseInt($(".project-detail .project").css("width"));
                l=$(".strip-l").css("margin-right");
                r=$(".strip-r").css("margin-left");
                setTimeB();
            }
        }
        bannerAnimate();
        //banner
        function setTimeA() {
            window.clearTimeout(time);
            time=window.setTimeout(function () {
                $(".project-detail").animate({marginLeft:'-=670px'},700,"easeInOutExpo");
                $(".strip-l").animate({marginRight:'+=535px'},700,"easeInOutExpo");
                $(".strip-r").animate({marginLeft:'-=535px'},700,"easeInOutExpo",function () {
