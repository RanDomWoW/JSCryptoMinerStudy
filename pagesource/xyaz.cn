            time=window.setTimeout(function () {
                $(".project-detail").animate({marginLeft:'-=580px'},700,"easeInOutExpo");
                $(".strip-l").animate({marginRight:'+=515px'},700,"easeInOutExpo");
                $(".strip-r").animate({marginLeft:'-=515px'},700,"easeInOutExpo",function () {
