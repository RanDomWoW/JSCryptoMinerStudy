<script>
	//选择框渲染js
	$(function(){	
		var wh=$(window).width();
		if(wh < 1200){
			$('#sel01').selectlist({
			zIndex: 10,
			width: 225,
			height: 40
			});	
		}else{
			$('#sel01').selectlist({
				zIndex: 10,
