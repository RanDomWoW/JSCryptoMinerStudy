        function animateUpackLineUp() {

          var $lineUp = $('#upack-line-up-led-con');

          $lineUp.mouseover(function() {

                          event.preventDefault();


                            var $upackNoLED = $('#upack-no-led');
                            var $upackLED = $('#upack-led');
                          
                          $upackNoLED.fadeTo('fast', 0);
                          $upackLED.fadeTo('fast', 1);


          });

          $lineUp.mouseleave(function() {

                          event.preventDefault();


                            var $upackNoLED = $('#upack-no-led');
                            var $upackLED = $('#upack-led');
            
                          $upackNoLED.fadeTo('fast', 1);
                          $upackLED.fadeTo('fast', 0);
            

          });


        }
        animateUpackLineUp()
