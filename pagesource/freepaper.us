<script>
var myf;
function suggest(inputString){
		if(inputString.length == 0) {
			$('#suggestions').fadeOut();
		} else {
		$('#country').addClass('load');
			$.post("livesearch.php", {queryString: ""+inputString+""}, function(data){
				if(data.length >0) {
					$('#suggestions').fadeIn();
					$('#suggestionsList').html(data);
					$('#country').removeClass('load');
					myf="keyword";
				}
			});
		}
	}

	
	function fill(thisValue) {
		//submit form!
	if(myf == "keyword"){	
		$('#country').val(thisValue);
		$("#form").submit();
	}
	if(myf == "title"){	
		$('#paperurl').val(thisValue);
		$("#form_title").submit();
	}
		setTimeout("$('#suggestions').fadeOut();", 600);
		setTimeout("$('#suggestions_title').fadeOut();", 600);
	}

	
function suggest_title(inputString){
		if(inputString.length == 0) {
			$('#suggestions_title').fadeOut();
		} else {
		$('#paperurl').addClass('load');
			$.post("livesearch_title.php", {queryString: ""+inputString+""}, function(data){
				if(data.length >0) {
					$('#suggestions_title').fadeIn();
					$('#suggestionsList_title').html(data);
					$('#paperurl').removeClass('load');
					myf="title";
				}
			});
		}
	}	
	

	
	
	
</script>

