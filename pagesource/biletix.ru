<script>
    $(function(){
        var popularFROM = "Москва|MOW.Санкт-Петербург|LED.Симферополь|SIP.Сочи|AER.Ереван|EVN".split(".");
        var popularTO = "Москва|MOW.Санкт-Петербург|LED.Екатеринбург|SVX.Краснодар|KRR.Ростов|ROV".split(".");
        function getStaticCities(arr, cssclass, style) {
            var L = '';
        	if (cMode == "OJ") {
        		$(".ac_results").addClass("long");
        		var L = 'long';
        	}

            var str = '<div style="'+style+'" class="ac_results '+cssclass+" "+L+'"><ul style="max-height: 268px; overflow: auto;">';
            for (var i = 0; i < arr.length; i++) {
                str+="<li><strong>"+arr[i].split("|")[0]+" <span>("+arr[i].split("|")[1]+")</span></strong></li>";
            }
            str+="</ul></div>";
            return str;
        }

        //формируем строки(2) популярных направлений
        var topac = 218;
        if ($("body").hasClass("authorized")) {
            topac+=40;
        }

        var strFROM = getStaticCities(popularFROM, "ac_results_staticf", "position: absolute;");
        var strTO = getStaticCities(popularTO, "ac_results_statict", "position: absolute;");


        var capsule;
        function mostPopularSet(inp, str, cssclass, idinp){
            inp.bind("focus", function(){
                capsule = inp;
                $(".ac_results_staticf, .ac_results_statict").remove();
                $(cssclass).remove();
                if (capsule){
                    setTimeout(function(){
                    	if ($(cssclass).css("display") == 'block') {return;}
                        var val = $.trim(capsule.val());
                        if (val.length == 0) {
                            $("body").append(str);
                            $(cssclass).position({
                                my: "left top",
                                at: "left bottom",
                                of: idinp
                            });

                            if (cMode == "OJ") {
                                $(".ac_results").addClass("long");
                            } else {
                            	$(".ac_results").removeClass("long");
                            }

                            setTimeout(function(){
                                $(cssclass+" li").on("click", function(){
                                    inp.val($("strong", $(this)).text());

                                    if (!(capsule.attr("name").indexOf("depart") < 0)) {
                                        var num = $.trim(capsule.attr("id")).split("depart")[1];
                                    	if (cMode == "OJ") {
                                    		$(".multi-route #arrival"+num).focus();
                                    	} else {
                                    		$("#arrival"+num).focus();
                                    	}
                                    } else if (!(capsule.attr("name").indexOf("arrival") < 0)) {
                                    	var num = $.trim(capsule.attr("id")).split("arrival")[1];
                                    	if (cMode == "OJ") {
                                    		$(".multi-route .dateto"+num).focus();
                                    	} else {
                                    		$("#dateto"+num).focus();
                                    	}
                                    }
                                    inp.change();
                                })
                            }, 200);
                        } else {
                            $(cssclass).remove();
                        }
                    }, 200);
                }
            })
        }

        $("body").keyup(function(event){
        	//$(".ac_results").focus();
        	if (capsule && ($(".ac_results").css("display") == "block")) {
        		var inpid = capsule.attr("id");
                var k = event.keyCode;

                if (k == 40) {
                    if ($(".ac_results ul li.ac_over").index() == -1 ){
                        $(".ac_results ul li:first").addClass("ac_over");
                    } else {
                        $(".ac_results ul li.ac_over").next().addClass("ac_over");
                        $(".ac_results ul li.ac_over").prev().removeClass("ac_over");
                    }
                    capsule.val($(".ac_results ul li.ac_over strong").text()).blur();
                    $("#miracle").focus();
                    event.preventDefault();
                    return false;
                }

                if (k == 38) {
                	$(".ac_results li.ac_over").prev().addClass("ac_over");
                    $(".ac_results li.ac_over").next().removeClass("ac_over");
                    capsule.val($(".ac_results ul li.ac_over strong").text()).blur();
                    $("#miracle").focus();
                    event.preventDefault();
                    return false;
                }

                if (k == 13) {
                	if (!(capsule.attr("name").indexOf("depart") < 0)) {
                        var num = $.trim(capsule.attr("id")).split("depart")[1];
                    	if (cMode == "OJ") {
                    		$(".multi-route #arrival"+num).focus();
                    	} else {
                    		$("#arrival"+num).focus();
                    	}
                    } else if (!(capsule.attr("name").indexOf("arrival") < 0)) {
                    	var num = $.trim(capsule.attr("id")).split("arrival")[1];
                    	if (cMode == "OJ") {
                    		$(".multi-route .dateto"+num).focus();
                    	} else {
                    		$("#dateto"+num).focus();
                    	}
                    }

                	$(".ac_results_staticf, .ac_results_statict").remove();
                	capsule.blur();
                	event.preventDefault();
                    return false;
                }
        	}
        })

        $("body").click(function(e){
            var target = e.srcElement || e.target;
            if (!$(target).parents().hasClass('.ac_results')){
            	$(".ac_results_staticf, .ac_results_statict").remove();
            }
        })

        //назначаем логику работы для подсказок городов
        mostPopularSet($("input[name=depart]"), strFROM, ".ac_results_staticf", $(".default-route .dest").eq(0));
        mostPopularSet($("input[name=arrival]"), strTO, ".ac_results_statict", $(".default-route .dest").eq(1));

        mostPopularSet($("input[name='multi_depart[]']", $(".multi-route .direction").eq(0)), strFROM, ".ac_results_staticf", $(".multi-route .dest").eq(0));
        mostPopularSet($("input[name='multi_depart[]']", $(".multi-route .direction").eq(1)), strFROM, ".ac_results_staticf", $(".multi-route .dest").eq(2));
        mostPopularSet($("input[name='multi_depart[]']", $(".multi-route .direction").eq(2)), strFROM, ".ac_results_staticf", $(".multi-route .dest").eq(4));
        mostPopularSet($("input[name='multi_depart[]']", $(".multi-route .direction").eq(3)), strFROM, ".ac_results_staticf", $(".multi-route .dest").eq(6));

        mostPopularSet($("input[name='multi_arrival[]']", $(".multi-route .direction").eq(0)), strTO, ".ac_results_statict", $(".multi-route .dest").eq(1));
        mostPopularSet($("input[name='multi_arrival[]']", $(".multi-route .direction").eq(1)), strTO, ".ac_results_statict", $(".multi-route .dest").eq(3));
