            if ( typeof( $.oAjaxSteps ) != 'undefined' ) {
              var bDisableScroll = _this.error_string !== false;
              if ( $(".show-message").length && $(".show-message").parent().css("display") == "block" ) {
                bDisableScroll = true;
              }
              $.oAjaxSteps.on_after_post_ajax( bDisableScroll );
