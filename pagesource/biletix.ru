function dAuthSuccess(dauth){
    if(typeof dauth !== "undefined" && dauth != ""){
    var dAuthResult = false;
    if(typeof dauth.code !== "undefined" && dauth.code !== null){
    if (dauth.code > 0){
      $(function() {

        $( "#dialog-message" ).dialog({
          resizable: false,
          height:160,
          width:350,
          modal: true,
          open: function( event, ui ) {
              $(".ui-dialog-buttonset").hide();
              $( "#dialog-message" ).css("height", "64px");
              $.ajax({
                  url: "/tools/auth/attempts.php?user_id="+dauth.user_id+"&lacheck",
                  async: false,
                  success: function(data){
                      if (data == "deny"){
                          $("#dialog-message .dauth-text").css("color", "red");
                          $("#dialog-message .dauth-text").html("Ваш аккаунт заблокирован, обратитесь по телефону 8-966-037-87-18");
                          $("input[name=dauth-code]").hide();
                      } else {
                          $(".ui-dialog-buttonset").show();
                          $("#dialog-message .dauth-text").css("color", "black");
                          $("#dialog-message .dauth-text").html("Введите код авторизации:");
                          $("input[name=dauth-code][val != '']").val("");
                          $("input[name=dauth-code]").show();
                      }
                  }
              });
          },
          buttons: {
            "Подтвердить": function() {
                    if (latryCounter >= 9) {
                        $.ajax({
                            url: "/tools/auth/attempts.php?user_id="+dauth.user_id+"&laset",
                            async: false,
                            success: function(data){
                                if (data == "ok"){
                                    $("#dialog-message .dauth-text").css("color", "red");
                                    $("#dialog-message .dauth-text").html("Ваш аккаунт заблокирован, обратитесь по телефону 8-966-037-87-18");
                                    $("input[name=dauth-code]").hide();
                                    $(".ui-dialog-buttonset").hide();
                                    latryCounter = 0;
                                }
                            }
                        });
                    } else {
                        $.ajax({
                            type: "POST",
                            url: "/tools/auth/check_user_bill_code.php",
                            data: "user_id=" + dauth.user_id + "&code=" + $("input[name=dauth-code][value != '']").val(),
                            async: false,
                            success: function (answer) {
                                if (typeof answer !== "undefined") {

                                    if (answer === "1") {
                                        dAuthResult = true;
                                        $("#auth_form_line").submit();
                                        console.log("correct");

                                    } else {
                                        dAuthResult = false;
                                        $("#dialog-message .dauth-text").css("color", "red");
                                        if (latryCounter>=6){
                                            $("#dialog-message .dauth-text").html("У вас осталось "+(9-latryCounter)+" попытки. В случае неправильного ввода ваша учетная запись будет заблокирована.");
                                        } else {
                                            $("#dialog-message .dauth-text").html("Введен некорректный код.");
                                        }
                                        console.log(latryCounter);
                                        console.log("incorrect");
                                    }
                                    console.log("answer - " + answer);
                                }
                            }
                        });
                        if (dAuthResult) $(this).dialog("close");
                    }
                latryCounter++;
            }
          }
        });
        $(".ui-dialog-title").hide();
      });
    }}
    return dAuthResult;
  }
}

