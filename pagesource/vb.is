        <script>
        var screenState = 'desktop';
        $(window).scroll(function() {
            $('#scrolledMenu').each(function(){
                var itemPos = $(this).offset().top;
                var topOfWindow = $(window).scrollTop();
                if (topOfWindow < 201) {
                    $('#scrolledMenu .active').removeClass('active');
                }
            });
            moveSpons();
        });
        $(window).ready(function(){
            $('#nav-toggle').click(function(){
                $(this).toggleClass('active');
                $('#hero-menu').toggleClass('active');
                $('#search-menu').removeClass('active');
                $('#search-toggle-scroll').removeClass('active');
                return false;
            });
            $('#search-toggle-scroll, #search-toggle').click(function(){
                $('#search-toggle-scroll').toggleClass('active');
                $('#scrolledMenu').removeClass('affix-top').addClass('affix');

                $('#search-menu').toggleClass('active');
                $('#hero-menu').removeClass('active');
                $('#nav-toggle').removeClass('active');

                $('#hero-search-input').focus();

                return false;
            });
            moveSpons();
        });
        function moveSpons(type){
            var w = $(window).width();

            if (w < 767 && screenState != 'mobile') {
                screenState = 'mobile';
                var u = $("#spons_upper .spons" ).detach();
                $("#spons_upper_mobile").html(u);
                var m = $("#spons_mid .spons" ).detach();
                $("#spons_mid_mobile").html(m);
                var l = $("#spons_lower .spons" ).detach();
                $("#spons_lower_mobile").html(l);
            } else if(w >= 767 && screenState != 'desktop') {
                screenState = 'desktop';
                var u = $("#spons_upper_mobile .spons" ).detach();
                $("#spons_upper").html(u);
                var m = $("#spons_mid_mobile .spons" ).detach();
                $("#spons_mid").html(m);
                var l = $("#spons_lower_mobile .spons" ).detach();
                $("#spons_lower").html(l);
            }
        }
        </script>
