<script type="text/javascript">
//function to clean Marketo injected canvas styled forms.
function cleanMarketoForm(formId,exception,formClass,formShow,cSelect1,cSelect2){
    //Set up form css clean/remove marketo css
    if((formShow) == true){
    	$("#mktoForm_"+formId).attr("style","display: block;");
    }else{
    	$("#mktoForm_"+formId).attr("style","display: none;");
    }
    $("#mktoForm_"+formId).attr("class",formClass);
    $("#mktoForm_"+formId+" > style").remove();
    //function for populating hidden values from LS
    function getLastValueFromStringAndAppend(value,name){
        if((value) == null){
            // console.log("getLastValueFromStringAndAppend null");
        }
        else{
            // console.log("!!: "+value);
            var tempArrayFromString = value.split('&');
            var tempArrayFromStringVal = tempArrayFromString[tempArrayFromString.length - 1];
            // console.log(tempArrayFromStringVal);
            $("."+formClass+" input[name='"+name+"']").val(tempArrayFromStringVal);
        }
    }
    //function to clean the css of just fields
    function cleanAddedFeilds(){
        $(".mktoButtonWrap").attr("class","form-submit");
        $(".mktoButton").attr("class","btn");
        $(".mktoOffset").attr("class","mktoOffsetClean");
        $(".mktoFieldWrap").attr("class","mktoFieldWrapClean");
        $(".mktoHasWidth").attr("class","mktoHasWidthClean");
        $(".mktoGutter").attr("class","mktoGutterClean");
        $("#mktoForm_"+formId+" input").attr("style","width: 100%");
        if (typeof cSelect1 !== 'undefined') {
			$("#mktoForm_"+formId+" select").addClass(cSelect1);
		}
        if (typeof cSelect2 !== 'undefined') {
			$("#mktoForm_"+formId+" select").addClass(cSelect2);
		}
        $(".mktoHasWidthClean").attr("style","width: 100%");
        $(".mktoFieldWrapClean").attr("class","mktoFieldWrapClean marketo-panda-2");
        $(".form-submit").attr("style","margin-left: 10px;");
        $("#mktoForm_"+formId+" input").prop('required',true);
        //Hidden fields/Global location/refferrer vars
        $("."+formClass+" input[name='Country']").val(country);
        if(country !== "United States"){
            state = "";
        }
        $("."+formClass+" input[name='City']").val(city);
        $("."+formClass+" input[name='State']").val(state);
        $("."+formClass+" input[name='PostalCode']").val(postalCode);
        getLastValueFromStringAndAppend(localStorage.INST_LeadSource,"LeadSource");
        getLastValueFromStringAndAppend(localStorage.INST_Lead_Source_Description__c,"Lead_Source_Description__c");
        getLastValueFromStringAndAppend(localStorage.INST_last18CharacterCampaignID,"last18CharacterCampaignID");
        //this can be resutructured if more exceptions come up in the future.
        if(exception != "holdLastOffer"){
            getLastValueFromStringAndAppend(localStorage.INST_Last_Offer__c,"Last_Offer__c");
        }
        //set current user locale
        getLastValueFromStringAndAppend(INST_Contact.locale ,"locale");
        //set marUID
	    if (marUID !== null) {
	        $(".MARUID").attr('value', marUID);
	        $("."+formClass+" input[name='MARUID__c']").val(marUID);
	        INST_Contact.MARUID__c = marUID;
	    }
	    else{
	        $(".MARUID").attr('value', "not_set");
	        $("."+formClass+" input[name='MARUID__c']").val("not_set");
	        INST_Contact.MARUID__c = "not_set";
	    }
    }
    //initial clean
    cleanAddedFeilds();
    //listen to select changes for newley injected fields from Marketo if logic
    $('select').on('change', function() {
        var selectedName= $(this).attr('name');
        var selectedNameRaw = selectedName;
        selectedName = selectedName.toLowerCase();
        var selectedValue = $(this).val();
        INST_Contact[selectedNameRaw] = selectedValue;
        cleanAddedFeilds();
    });
    $('input').on('change', function() {
        var selectedName= $(this).attr('name');
        var selectedNameRaw = selectedName;
        selectedName = selectedName.toLowerCase();
        var selectedValue = $(this).val();
        INST_Contact[selectedNameRaw] = selectedValue;
        cleanAddedFeilds();
    });
}
