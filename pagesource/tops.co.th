            $("#form-search-keyword-input").blur(function (e) {
                $("#drop-search").delay(500).fadeOut(100);
            });

            /////////////////// Start SEARCH ////////////////////////
            $.widget("custom.catcomplete", $.ui.autocomplete, {
                _renderMenu: function (ul, items) {
                    var that = this,
                        currentCategory = "";
                    var loop = 0;
                    $.each(items, function (index, item) {
                        var li;
                        if (item.value == "" && loop <= 0) {
                            currentCategory = item.value;
                            loop++;
                        }
                        li = that._renderItemData(ul, item);
                    });
                },
                _renderItem: function (ul, item) {
                    return $("<li>")
                        .addClass(item.value)
                        .attr("data-value", item.value)
                        .append($('<div class="ui-menu-item-wrapper">')
                            .append($('<a>').text(item.label)))
                        .appendTo(ul);
                }
            });

            $("#form-search-keyword-input").catcomplete({
                source: function (request, response) {
                    $.ajax({
                        url: '/suggest',
                        dataType: "json",
                        data: { term: request.term },
                        success: function (data) {
                            response($.map(data.depts, function (item) {
                                return {
                                    label: item.name,
                                    value: item.rootDept
                                }
                            }));
                        }
                    });
                },
                minLength: 2,
                select: function (event, ui) {
                    if (ui.item.value == "") {
                        $("#form-search-keyword-input").val(ui.item.label);
                    }
                    else {
                        $("#form-search-keyword-input").val(ui.item.value);
                        ui.item.value = $("#form-search-keyword-input").val();
                    }
                    $("#search-dept").val('');
                    $("input[name='brand']").val('');
                    $("form[name='searchQuery']").submit();
                },
                focus: function (event, ui) {
                }
            }); // end function catcomplete


            $('#search_product').click(function (event) {
                var key = $('#form-search-keyword-input').val();
                if (key == "") {
                    popup("#msgNoKeyword", 399, 200);
                    event.preventDefault();
                }
            });

            /////////////////// End SEARCH ////////////////////////

            /////////////////// EMAIL Subscribe ///////////////////
            $('#EMail').keypress(function (e) {
                var code = (e.keyCode ? e.keyCode : e.which);
                if (code == 13) {
                    var Email = $('#EMail').val();
                    if (Email == '') {
                        e.preventDefault();
                    }
                }
            });

            $('#formSubscribe').submit(function (e) {
                e.preventDefault();
                var _email = $('#EMail').val();
                var title = "Message Box";
                if (_email != '') {
                    $.ajax({
                        url: '/Subscribe/Add.aspx',
                                type: "POST",
                                data: { EMail: _email, IsActive: true },
                                success: function (data) {
                                    var IsSuccess = data["IsMemberTops"];
                                    if (IsSuccess == "1") {
                                        popup("#SubscribeSuccess", 250, 100, "", "auto");
                                    }
                                    else {
                                        popup("#SubscribeUnsuccess", 250, 100, "", "auto");
                                    }

                                },
                                cache: false
                            });
                        }
                    });
                    /////////////////// End Email Subscribe ///////////////////

                    /////////////////// Mini Cart ////////////////////////////
                    cart = function (cartType) {
                        if (!$("#MiniCart").is(":visible")) {
                            $.ajax({
                                url: '/cart/minicartnew.aspx',
                                type: "GET",
                                data: { valueType: cartType },
                                contentType: 'application/json; charset=utf-8',
                                success: function (data) {
                                    $('#MiniCart').html(data);
                                },
                                cache: false
                            });
                        }
                    }


                });      // end ready

