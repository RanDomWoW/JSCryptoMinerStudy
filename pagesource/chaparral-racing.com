    function updateCartCount() {
        $.getJSON('/Cart/CartSvc.aspx?action=ITEM_COUNT&un=' + new Date().getTime(), function (data) {
            if (!handleJsonException(data)) {
                $('#cartCount').html(data.Count);
                $('#mobileCartCount').html(data.Count);
                showHideCartCount();
            }
        });
    }

    function showHideCartCount() {
        if ($('#cartCount').html() == '') {
            $('#siteHdrCartCount').hide();
            $('#mobileCartCount').hide();
        }
        else {
            $('#siteHdrCartCount').show();
            $('#mobileCartCount').show();
            var cnt = $('#mobileCartCount').html();
            if (cnt.indexOf('(') < 0) {
                $('#mobileCartCount').html('(' + cnt + ')');
            }
        }
    }
    
