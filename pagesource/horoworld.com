$(document).ready(function(){
	var lastScrollTop = 0;
    $(window).scroll(function()
    {
        var st = $(this).scrollTop();
        if (st > lastScrollTop){
            console.log('down');
            // scrolling down
            if($('#social-fixed').data('size') === 'show')
            {
                $('#social-fixed').data('size','hide');
                $('#social-fixed').stop().animate({
                    bottom:'0'
                },100);
            }
        }
        else
        {
            // scrolling up
            if($('#social-fixed').data('size') === 'hide')
            {
                            console.log('up');
                $('#social-fixed').data('size','show');
                $('#social-fixed').stop().animate({
                    bottom:'-100%'
                },50);
            }  
        }
        lastScrollTop = st;
    });
	if($('#social-fixed').length > 0) 
	{
		var top8 = $('#social-fixed').offset().top - parseFloat($('#social-fixed').css('marginTop').replace(/auto/, 0));
		$(window).scroll(function (event) {
		var y8 = $(this).scrollTop();
		if (y8 >= top8) {$('#social-fixed').addClass('fixed');} else {$('#social-fixed').removeClass('fixed');}
		});
	}
});
</script>
