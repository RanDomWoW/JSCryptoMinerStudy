<script>
var regis_password = document.getElementById("regis_password");
var cregis_password = document.getElementById("cregis_password");

function newPassword() 
{
   if (regis_password.value.search(new RegExp(regis_password.getAttribute('pattern'))) >= 0) 
   {
        regis_password.setCustomValidity('');
    } else {
        regis_password.setCustomValidity('ต้องมีตัวอักษรผสมกับตัวเลข และมีความยาว 8 ตัวอักษรขึ้นไป.');
    }
}

function validatePassword(){
  if(regis_password.value != cregis_password.value) 
  {
    cregis_password.setCustomValidity("กรุณากรอกรหัสผ่านให้ตรงกัน.");
  } else {
    cregis_password.setCustomValidity('');
  }
}
regis_password.oninvalid = newPassword;
regis_password.oninput = newPassword;
regis_password.onchange = validatePassword;
cregis_password.onkeyup = validatePassword;
cregis_password.oninput = validatePassword;
</script>
