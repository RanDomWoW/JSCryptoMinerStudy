			var t;
	
		function next_slide(stop_timeout) {
			if ($('#slideshow').find('li').length==1)
					return;
					
					
			var $active = $('#slideshow li.active');
			var $active_a = $('#nav a.active');
			
			 if ( $active.length == 0 ) $active = $('#slideshow li:last');

			 var $next =  $active.next().length ? $active.next()
				: $('#slideshow li:first');
			 var $next_a =  $active.next().length ? $active_a.next()
				: $('#nav a:first');

			$next.find('.title').addClass("title");
				
			$active.animate({opacity: 0}, 1000,function() {
					$active.removeClass('active');
				$active_a.removeClass("active");
				});
				
			$next.css({opacity: 0.0})
					.addClass('active')
					.animate({opacity: 1.0}, 1000,function() {
						$next.addClass('active');
						$next_a.addClass('active');
					});
			
			if (!stop_timeout)	
				t = setTimeout("next_slide()", 6000);

		}
		
		function change_slide(id) {
				
			if ($('#nav a:eq('+id+')').hasClass("active"))
				return false;
				
			$('#nav a').removeClass("active");
			$('#nav a:eq('+id+')').addClass("active");
			
			var $active = $('#slideshow li.active');
			var $next = $('#slideshow li:eq('+id+')');
			
			$active.animate({opacity: 0}, 1000,function() {
					$active.removeClass('active');
				});
				
			$next.css({opacity: 0.0})
					.addClass('active')
					.animate({opacity: 1.0}, 1000,function() {
						$next.addClass('active');
					});
			
			clearTimeout(t);
			t = setTimeout("next_slide()", 6000);
			
			return false;
		}
		
				
