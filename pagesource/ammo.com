    <script type="text/javascript">
    //<![CDATA[
    var algoliaLiveSearch = new AlgoliaLiveSearch({
        placeholder: 'Search Products Here',
        applicationID: 'KHD88LQOBC',
        apiKey: '659dab259ab09407463619097a12798e',
        indexName: 'prod_ammonet',
        searchDelay: 400,
        minLength: 2,
        resultLinks: '#search_autocomplete a',
        queryOptions: {
            hitsPerPage: 5,
            tagFilters: '(product,category)',
            attributesToRetrieve: ["name","url","thumbnail_url","categories","name","url","image_url","_tags"],
            attributesToHighlight: 'name'
        },
        renderResults: function (results) {
            if ( ! results.hits) {
                return;
            }

            $('search_autocomplete').show();

            var hits = results.hits,
                nbSuggestions = this.options.queryOptions.hitsPerPage,
                nbHits = results.nbHits,
                allProducts,
                products = [],
                categories = [],
                html, i;
            hits.each(function(hit) {
                if (typeof(hit) === 'object' && typeof(hit._tags) === 'object') {
                    if (hit._tags.indexOf('product') > -1) {
                        products.push(hit);
                    } else if (hit._tags.indexOf('category') > -1) {
                        categories.push(hit);
                    }
                }
            });
            var displayCategories = (categories.length > 0) || (products.length < nbSuggestions);
            var displayProducts = (products.length > 0) || (categories.length < nbSuggestions);

            // Display categories
            if (displayCategories) {
                html = '<div class="algoliasearch-autocomplete-title">Category Results:</div>';
                if (categories.length > 0) {
                    for (i = 0; i < categories.length; ++i) {
                        hit = categories[i];
                        html += '<a class="algoliasearch-autocomplete-hit" href="' + hit.url + '">' +
                        (hit.image_url ? '<div class="thumb"><img src="' + hit.image_url + '" /></div>' : '') +
                        '<div class="info">' +
                        hit._highlightResult.name.value +
                                                '</div>' +
                        '<div class="clearfix"></div>' +
                        '</a>';
                    }
                } else {
                    html += '<div class="algoliasearch-autocomplete-empty">No results.</div>'
                }
            } else {
                html = '';
            }
            $('search_autocomplete_categories').update(html);

            // Display products
            if (displayProducts) {
                html = '<div class="algoliasearch-autocomplete-title">Product Results:</div>';
                if (products.length > 0) {
                    var searchQuery = algoliaLiveSearch.searchForm.field.getValue();
                    for (i = 0; i < products.length; ++i) {
                        var hit = products[i];
                        var productName = hit._highlightResult.name.value;
                        var parts = productName.split('|');
                        if (parts.length > 1) {
                            if (searchQuery.length <= 5 || parts[0].indexOf(searchQuery) !== -1 || parts[0].indexOf('<em>') !== -1) {
                                productName = parts[0] + '<br/><small class="hint">Grouped product. Your query matches one of its variants.</small>';
                            } else {
                                productName = parts[0];
                                var children = [];
                                parts.each(function(name, index){
                                    if (index > 0) { // skip the configurable/grouped product name
                                        if (parts[index].indexOf(searchQuery) !== -1 || parts[index].indexOf('<em>') !== -1) {
                                            children.push(parts[index]);
                                            if (children.length >= 3) {
                                                throw $break;
                                            }
                                        }
                                    }
                                });
                                if (children.length) {
                                    productName += '<br/><small class="hint">options: ' + children.join(' | ') + '</small>';
                                }
                            }
                        }
                        html += '<a class="algoliasearch-autocomplete-hit" href="' + hit.url + '">' +
                        (hit.thumbnail_url ? '<div class="thumb"><img src="' + hit.thumbnail_url + '" /></div>' : '') +
                        '<div class="info">' +
                        productName +
                        (hit.categories && hit.categories.length > 0 ? '<div class="algoliasearch-autocomplete-category">in ' + hit.categories.join(', ') + '</div>' : '') +
                        '</div>' +
                        '<div class="clearfix"></div>' +
                        '</a>';
                    }
                } else {
                    html += '<div class="algoliasearch-autocomplete-empty">No results.</div>'
                }
                if (products.length > 0 && (allProducts = nbHits - categories.length) > 0) {
                    html += '<div class="algoliasearch-autocomplete-all"><a><span>'+'View All %d Product Results'.replace('%d', allProducts.toString())+'</span></a></div>';
                }
            } else {
                html = '';
            }
            $('search_autocomplete_products').update(html);
            if($$('.section-header ~ .algoliasearch-backdrop').length == 0) {
                $$('.section-header').first().insert({after:'<div class="algoliasearch-backdrop" />'});
            }

            if (products.length > 0) {
                $('search_autocomplete_products').select('.algoliasearch-autocomplete-all a').first().observe('click', function(e){
                    e.stop();
                    $('search_mini_form').submit();
                });
            }

            $('search_autocomplete').select('a').invoke('observe', 'click', function(e){
                var searchQuery = algoliaLiveSearch.searchForm.field.getValue();
                if(searchQuery){
                    Mage.Cookies.set('lastSearchQuery', searchQuery);
                }
            });
        },
        clearResults: function() {
            setTimeout(function(){
                $('search_autocomplete').hide();
                $('search_autocomplete_categories').update('');
                $('search_autocomplete_products').update('');
                $$('.section-header ~ .algoliasearch-backdrop').invoke('remove');
            }, 500);
        }
    });
    $('search').observe('keydown', function(e) {
        if (e.keyCode == 13) { // Enter
            var element = Event.element(e);
            if (element.value != '' && element.tagName == 'INPUT' && $$('.algoliasearch-autocomplete-hit.marked').length == 0) {
                e.stop();
                $('search_mini_form').submit();
            }
        }
    });
    //]]>
</script><script>
    ga('send', 'pageview');
</script>    </div>
