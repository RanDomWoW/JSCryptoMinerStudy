<script type="text/javascript">
$(window).load(function(){
	$(document).ready(function()
	{
		// Hide all the DETAIL div elements
		$( "div[id^='DETAILS_']").hide() ;
		$( "#keyword_details").hide() ;

		$( "#moreAboutCategoriesDiv").hide() ;
		$( 'img' ).click( function (){
			$( "div[id^='DETAILS_']").hide(500) ;
			var divToShow = "DETAILS_"+ $(this).attr( "id") ;
			var isVisible = $( "#"+divToShow ).is( ':visible' ) ;
			if( isVisible )
			{
				$( "#"+divToShow ).hide( 500 ) ;
				return ;
			}
			$( "#"+divToShow ).show( 500 ) ; 
		}) ;


    $('#emailBTN').click(function (e) {
        e.preventDefault() ;
        $("body").append('<div class="modalOverlay">');
		$( "div[id^='DETAILS_']").hide(500) ;
        $('html').addClass('overlay');
        //var activePopup = $(this).attr('data-popup-target');
        $('#email-details-popup').addClass('visible');
		
		var divToShow = document.getElementById('email-details-popup');
		divToShow.setAttribute('tabIndex', '100');
		divToShow.focus();

		
    });

    $('#show_keyword_details').click(function (e) {
        e.preventDefault() ;
		$( "div[id^='DETAILS_']").hide(500) ;
        var isVisible = $( "#keyword_details").is( ':visible' ) ;
		if( isVisible )
		{
			$( "#keyword_details").hide( 500 ) ;
			return ;
		}
		$( "#keyword_details").show( 500 ) ;
    });


    $('#close_keyword').click(function (e) {
        e.preventDefault() ;
		$( "#keyword_details").hide( 500 ) ;
    });

    $(document).keyup(function (e) {
        if (e.keyCode == 27 && $('html').hasClass('overlay')) {
        	$("div").remove(".modalOverlay");
        	
        	var linkToShow = document.getElementById('pubs');
			linkToShow.setAttribute('tabIndex', '109');
			linkToShow.focus();
		
            clearPopup();
        }
    });

    $('.pExit').click(function () {  
    	//$("body").remove('<div class="modalOverlay">');
    	$("div").remove(".modalOverlay");
		
		var linkToShow = document.getElementById('pubs');
		linkToShow.setAttribute('tabIndex', '109');
		linkToShow.focus();
		
        clearPopup();
	
    });

    $('.simulateModal').click(function () {
        clearPopup();
    });

    function clearPopup() {
        $('.popup.visible').addClass('transitioning').removeClass('visible');
        $('html').removeClass('overlay');

        setTimeout(function () {
            $('.popup').removeClass('transitioning');
        }, 200);
    }
    
  // this used to keep the focus in the modal  
  $(function() {
	  $('#modal_close').focusout(function() {  
	    var divToShow = document.getElementById('email-details-popup');
	    divToShow.setAttribute('tabIndex', '100');
			divToShow.focus();
	  });
	})

});

}) ;


</script>
	
