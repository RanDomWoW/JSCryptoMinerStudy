<script>
    function trimWords() {
        var titles = document.getElementsByClassName("hyperLinkTitle");
        var contentWidth = $(document).width();
        var maxLength = 0;
        var trimmedString = "";
        for (var i = 0; i < titles.length; i++) {
            var theString = titles[i].text;
            var originalLength = theString.length;
            if (contentWidth < 400) {
                maxLength = 40;
            } else if (contentWidth > 400 && contentWidth < 500) {
                maxLength = 50;
            } else if (contentWidth > 500 && contentWidth < 600) {
                maxLength = 60;
            } else if (contentWidth > 600 && contentWidth < 700) {
                maxLength = 70;
            } else if (contentWidth > 700 && contentWidth < 800) {
                maxLength = 80;
            } else if (contentWidth > 800 && contentWidth < 900) {
                maxLength = 90;
            } else if (contentWidth > 900) {
                maxLength = 100;
            }
            if (originalLength < maxLength) {
                trimmedString = theString;
            } else {
                var trimmedString = theString.substr(0, maxLength);
                trimmedString = trimmedString.substr(0, trimmedString.lastIndexOf(" "));
                trimmedString = trimmedString + " ...";
            }
            titles[i].text = trimmedString;
        }
    }
