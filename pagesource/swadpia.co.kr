	function popup_div(v, pop_id, pop_cookie) {
		var pnl_div_l;
		var pnl_div_t;
		
		var arr_pop_id = ['popup_main'];

		var pnl_popup_div ;
		var popupDivCookie ;
		pnl_popup_div = document.getElementById(pop_id);
		new_year = document.getElementById("new_year");
		if (v=="show")
		{

			pnl_div_l = (document.body.scrollWidth/82); //좌측으로 이동시  나누기값을 크게...
			pnl_div_t = (document.body.scrollHeight/6.2) +"px"   ;
			
			
 
			for(i=0;i< arr_pop_id.length ; i++)
			{
				pnl_popup_div = document.getElementById(arr_pop_id[i]);
				popupDivCookie = getCookie(arr_pop_id[i]);  // 쿠키네임 지정

				if (popupDivCookie != "no"){
					pnl_popup_div.style.display = "";
					pnl_popup_div.style.top = pnl_div_t   ;
					
					new_year.play();
					pnl_popup_div.style.left = pnl_div_l + "px";
					pnl_div_l = pnl_div_l + pnl_popup_div.scrollWidth + 5;;
				}else
				{
					pnl_popup_div.style.display = "none";
					new_year.pause();
				}
			}

		}else if (v=="close_today") {
			document.getElementById("new_year").pause();
			setCookie(pop_cookie,"no",1);
			pnl_popup_div.style.display = "none";
		}else {
			pnl_popup_div.style.display = "none";
		}
	} 
	
