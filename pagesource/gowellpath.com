      url: '/newsletter_signup',
      type: 'POST',
      data: data,
      error: function (data) {
        data = JSON.parse(data.responseText);
        $generalError.html(data.message);
        $generalError.addClass('show');
        setTimeout( function () {
          $generalError.removeClass('show');
