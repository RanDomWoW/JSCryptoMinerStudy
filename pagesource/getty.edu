    <script type="text/javascript">
    if (!ie8()) $(document).foundation();
    $(document).foundation('interchange', {
        named_queries: {
            sm: 'only screen',
            med: 'only screen and (min-width: 481px)',
            midmed: 'only screen and (min-width: 599px)',
            lrg: 'only screen and (min-width: 769px)',
            xlrg: 'only screen and (min-width: 1280px)',
            print: 'print'
        }
    });
    $(document).ready(function() {

        // initialize fit text
        $('#home-promos .visit figure .overlay h3').fitText(1.5, {
            minFontSize: '16px',
            maxFontSize: '30px'
        });

        // function to set the carousel counter values
        var setCount = function() {
            var figs = $('#carousel').find('figure');
            var total = $(figs).length;
            $(figs).each(function(i, e) {
                $(e).find('.carousel-count').text(i + 1);
            });
            $('.carousel-total').text(total);
        };

        // function to set the creditline text "see more" buttons
        var credits = function() {
            var seeMore = $(document.createElement('span'));
            $(seeMore).addClass('hide-for-medium-up');
            $(seeMore).append('<span class="show-more">&hellip;<a href="#">See more</a></span>');
            $(seeMore).append('<span class="show-less hide"><a href="#">See less</a></span>');

            var carousel = $('#carousel');
            var figures = $('#carousel figure');
            if ($(window).width() <= 481) {
                $(figures).each(function(i, e) {
                    var credit = $(e).find('.credit');
                    if (!$(credit).hasClass('less')) {
                        if ($(credit).height() > 25) {
                            $(credit).addClass('less');
                            $(credit).after($(seeMore).clone());
                            var more = $(e).find('.show-more');
                            var less = $(e).find('.show-less');
                            var toggleClasses = function() {
                                $(credit).toggleClass('less');
                                $(more).toggleClass('hide');
                                $(less).toggleClass('hide');
                                $(carousel).triggerHandler('updateSizes');
                            };
                            $(more).click(function() {
                                toggleClasses();
                            });
                            $(less).click(function() {
                                toggleClasses();
                            });
                        }
                    }
                });
            }
        };

        // function runs on carousel creation
        var doCreate = function() {
            if ($('html').hasClass('ie9')) {
                $('#carousel').triggerHandler('removeItem', ['.load']);
            }
            setCount();
            credits();
        };

        // carousel options
        var opts = {
            responsive: true,
            items: {
                visible: 1,
                height: "variable",
                filter: "figure"
            },
            scroll: {
                duration: 500,
                fx: "scroll",
                easing: "swing",
                pauseOnHover: "immediate-resume"
            },
            auto: {
                play: true,
                timeoutDuration: 6000
            },
            swipe: {
                onTouch: false,
                options: {
                    excludedElements: "button, input, select, textarea, .noSwipe"
                }
            },
            next: {
                button: '.carousel-next'
            },
            prev: {
                button: '.carousel-prev'
            },
            onCreate: doCreate
        };

        // initialize carousel
        $('#carousel').carouFredSel(opts);

        // run the credits() function on window resize
        // this runs 250ms after user stops resizing the window
        $(window).resize($.debounce(250, credits));

        // update carousel size on interchange img replacement
        var updateCarouselSize = function() {
            $('#carousel').triggerHandler('updateSizes');
        };
        $('.carousel-img').load($.debounce(250, updateCarouselSize));

        // function to resize promo containers
        var resizePromos = function() {
            if (!$('html').hasClass('ie9')) {
                var promos = $('.promos');
                promos.children().each(function(i, e) {
                    $(e).find('.promo-wrap').height('auto');
                });
                var heights = promos.children().map(function(i, e) {
                    return $(e).height();
                }).get();
                promos.each(function(i, e) {
                    $(e).find('.promo-wrap').height(Math.max.apply(Math, heights));
                });
            }
        };

        // resize promo containers on load and on resize
        // waits until 250ms after user stops resizing the window
        $(window).on('load resize', $.debounce(250, resizePromos));

        // check to see if an element is in view.
        // http://stackoverflow.com/questions/487073/check-if-element-is-visible-after-scrolling
        var isScrolledIntoView = function(elem) {
            var docViewTop = $(window).scrollTop();
            var docViewBottom = docViewTop + $(window).height();

            var elemTop = $(elem).offset().top;
            var elemBottom = elemTop + $(elem).height();

            return ((elemBottom <= docViewBottom) && (elemTop >= docViewTop));
        };

        // check the rotator to see if its on screen.
        // if it is, play. else, stop.
        var checkRotator = function() {
            if (!$('html').hasClass('ie9')) {
                if (isScrolledIntoView($('#carousel'))) {
                    if ($('#carousel').triggerHandler('isStopped')) {
                        $('#carousel').triggerHandler('play', [true]);
                    }
                } else {
                    if (!$('#carousel').triggerHandler('isStopped')) {
                        $('#carousel').triggerHandler('stop');
                    }
                }
            }
        };

        // check the if the rotator's in view on scroll
        $(window).scroll($.debounce(250, checkRotator));

        // remove loader window on page load
        $(window).load(function() {
            if (!$('html').hasClass('ie9')) {
                $('#loader').fadeOut(1000);
            }
        });

        // if ie9, don't load youtube iframe. load img with link instead.
        $(window).load(function() {
            // Edit these links to change the featured video
            // Its a good idea to just replace the video ID.
            var ytLink = " https://www.youtube.com/watch?v=an1Ppk0cMcs ";
            var ytEmbedLink = "https://www.youtube.com/embed/an1Ppk0cMcs?showinfo=0 ";
            // The following image should be 480x290
            var ytImage = "/images/yt_inspired.jpg";
            var altTag = "Featured Video on YouTube";

            if ($('html').hasClass('ie9')) {
                var nonCompatible = $('<figure><a href="' + ytLink + '"><img src="' + ytImage + '" alt="' + altTag + '"></a></figure>');
                $('#yt').append(nonCompatible);
            } else {
                var compatible = $('<figure class="flex-video aspect-5x3"><iframe width="560" height="315" src="' + ytEmbedLink + '" frameborder="0" allowfullscreen></iframe></figure>');
                $('#yt').append(compatible);
            }
        });
    });
	getty_chrome_includes();
    </script>
