<script type="text/template" id="resellerCertificateDlgTmpl">
    <div class="CustomDialogWindow certificate-dialog">
        <div class="CustomDialogHeader">Add a Certificate</div>
        <div class="CustomDialogData">
            <div class="form-block">
                <div class="error-message"></div>
                  <div class="file-upload-form">
                    <fieldset>
                        <div class="row">
                            <label>Country</label>
                            <select id="countryId" name="countryId">
                                <option <%= currentCountryId == null ? "selected" : "" %> >Select Country</option>
                                <% _.each(countries, function(country){ %>
                                <option value="<%= country.id %>" <%= currentCountryId == country.id ? "selected" : "" %> ><%= country.name %></option>
                                <% }); %>
                            </select>
                        </div>

                        <div class="row">
                            <label>State</label>
                            <select id="stateId" name="stateId">
                                <option selected>Select State</option>
                            </select>
                        </div>
                        <div class="row">
                            <div>
                                <label>Expiration Date</label>
                            </div>
                            <select name="month" class="certificate-expiry-month">
                                <option>Month</option>
                                <option value='1'>01 (Janaury)</option>
                                <option value='2'>02 (February)</option>
                                <option value='3'>03 (March)</option>
                                <option value='4'>04 (April)</option>
                                <option value='5'>05 (May)</option>
                                <option value='6'>06 (June)</option>
                                <option value='7'>07 (July)</option>
                                <option value='8'>08 (August)</option>
                                <option value='9'>09 (September)</option>
                                <option value='10'>10 (October)</option>
                                <option value='11'>11 (November)</option>
                                <option value='12'>12 (December)</option>
                            </select>
                            <select name="year" class="certificate-expiry-year">
                                <option>Year</option>
                                <% var currentYear = new Date().getFullYear();
                                for (var year = currentYear; year < currentYear + 40; year++) { %>
                                <option value="<%= year %>"><%= year %></option>
                                <% } %>
                            </select>
                            <span class="btn-orange-hollow file-upload-btn">
                                <input type="file" name="files" class="file-data" />
                                <span class="files">Choose a file</span>
                            </span>
                        </div>

                        <div class="row">
                            <input type="button" class="btn-orange-hollow certificate-save-btn" value="Submit" />
                        </div>
                    </fieldset>
                </div>
            </div>
        </div>
    </div>
</script>
