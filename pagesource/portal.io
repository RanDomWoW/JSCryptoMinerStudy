<script type="text/template" id="supplierTmpl">
    <%
        var showPrice = supplier.isAuthorizedSupplier ? (supplier.salePrice != null ? formatPrice(supplier.salePrice) : (supplier.price != null ? formatPrice(supplier.price) : "0.00")) : "0.00";
    %>
    <article class="side-block order-block" supplierid="<%= supplier.supplierId %>" groupid="<%= supplier.groupId %>" suppliername="<%= supplier.name %>" hideuntilauthorized="<%= supplier.hideUntilAuthorized %>" price="<%= showPrice %>" isinstock="<%= supplier.isInStock %>" isdefaultsupplier="<%= supplier.isDefault %>">
        <input type="hidden" id="defaultCost" value="<%= supplier.defaultCost %>" />
        <input type="hidden" id="defaultModifiedDate" value="<%= supplier.defaultModifiedDate %>" />
        <div class="row" style="padding: 20px;">
            <div class="product-logo-block">
                <!-- logo block -->
                <div>
                    <a class="supplier-name" href="/Parts/Profile/CompanyProfile/<%= supplier.supplierId %>"><%= supplier.name %></a>
                </div>
                <div class="supplier-part-number">
                    <% if (supplier.supplierPartNumber && supplier.supplierPartNumber !== "") { %>
                        <%= supplier.supplierPartNumber %>
                    <% } %>
                </div>
                <div style="bottom: 0; position: absolute;">
                    <% if (isAdministrator) { %>
                        <div style="height: 25px;">
                            <button type="button" class="btn-grey-v-hollow edit-logo">Edit Supplier</button>
                        </div>
                    <% } if (isAuthenticated) { %>
                        <div class="supplier-default" style="margin-top: 10px;">
                            <button type="button" class="btn-grey-v-hollow supplier-default-btn">Set as Default</button>
                        </div>
                    <% } %>
                </div>
            </div>
            <div class="product-cost-list cf">
                <!-- List of costs block -->
                <% if (isAdministrator) {
                    var priceClass = supplier.hasCustomPrice || supplier.isSupplierDealerCustomPrice ? "custom-price" : "";
                    priceClass = supplier.salePrice != null ? (priceClass + " sale-price") : priceClass; %>
                    <ul class="price-list" style="text-align: right;">
                        <% _.each(supplier.Prices, function(priceModel){ %>
                        <li class="cf">
                            <div><%= priceModel.priceString %></div>
                            <div class="supplier-updated-date" style="<%= priceModel.priceModifiedDate == null ? 'display: none;' : '' %>">updated <span class="supplier-updated-date-value"><%= priceModel.priceModifiedDate %></span></div>
                        </li>
                        <% if (supplier.hasCustomPrice || supplier.isSupplierDealerCustomPrice) { %>
                            <li class="<%= priceClass %> cf">
                                <div><%= supplier.customPriceString %></div>
                                <div class="supplier-updated-date" style="<%= supplier.customPriceModifiedDate == null ? 'display: none;' : '' %>">updated <span class="supplier-updated-date-value"><%= supplier.customPriceModifiedDate %></span></div>
                            </li>
                        <% }}); %>
                    </ul>
                <% } %>
            </div>
            <div class="product-dealer-cost">
                <!-- Main cost block -->
                <% if (supplier.isAuthorizedSupplier) {
                    var addPriceDisplay = supplier.isAuthorizedSupplier && !(supplier.price > 0) ? "display: block;" : "";
                    var rowStyle = supplier.price != null && supplier.price > 0 ? "display: block;" : "";
                    var price = supplier.price != null && supplier.price > 0 ? ("$" + formatPrice(supplier.price)) : "";
                    var priceClass = supplier.hasCustomPrice || supplier.isSupplierDealerCustomPrice ? "custom-price" : "";
                    priceClass = supplier.salePrice != null ? (priceClass + " sale-price") : priceClass; 
                    var priceLevelDisplay = supplier.priceName == null || supplier.priceName == "" ? "display: none;" : "";
                %>
                    <div class="cf price-row">
                        <div class="cf">
                            <input class="custom-price-text floatright" type="text" style="<%= addPriceDisplay %>" data-a-sign="$" placeholder="$" />
                        </div>
                        <div class="form-col price-block" style="<%= rowStyle %>">
                            <div class="price-txt <%= priceClass %>"><%= price %></div>
                            <% if (supplier.salePrice != null) { %>
                                <div class="sale-price-txt <%= priceClass %>">$<%= formatPrice(supplier.salePrice) %></div>
                            <% } %>
                            <div class="note <%= priceClass %>"><%= supplier.costNote %><span class="note-unit"><%= supplier.supplierUnitOfMeasure %></span></div>
                            <% if (supplier.discountPercentage != null) { %>
                                <div class="price-info-note" style="<%= priceLevelDisplay %>">Price Level - <%= supplier.priceName %></div>
                                <div class="price-info-note">$<%= formatPrice(supplier.price / ((100 - supplier.discountPercentage) / 100)) + " Less " + supplier.discountPercentage + "%" %></div>
                            <% } if (supplier.salePrice != null) { %>
                                <div class="sale-note <%= priceClass %>"><%= supplier.saleEnd %></div>
                            <% } %>
                        </div>
                    </div>
                <% } else {
                    if (supplier.isAuthorizedOnlySupplier) { %>
                        <section style="color:black">
                            <div style="float: left; margin-left:10px; margin-right: 7px; margin-top: 2px;">
                                <img style="margin-left: 3px;" width="30" src="/Content/ImagesBlack/Attention_Icon.png" />
                            </div>
                            <div style="float: left; width: 210px">
                                <div style="font-size: 18px; font-weight: bold;">Additional Approval <br /> Required For this Item</div>
                                <div style="font-size: 12px;">
                                    You are unlocked for <%= supplier.Name %>, but this item requires a secondary level of authorization.
                                </div>
                            </div>
                            <div style="clear:both;"></div>
                        </section>
                    <% } else {
                    var addPriceDisplay = "display: " + (supplier.isAuthorizedSupplier && !(supplier.price > 0) ? "block;" : "none;"); %>
                    <div class="cf price-row">
                        <div class="cf">
                            <input class="custom-price-text floatright" type="text" style="<%= addPriceDisplay %>" data-a-sign="$" placeholder="$" />
                        </div>
                        <div class="form-col price-block">
                            <div class="price-txt"></div>
                            <div class="note"><span class="note-unit"></span></div>
                        </div>
                    </div>
                    <div class="row cf authorized-block">
                        <div class="form-col price-block">
                            <img src="/Areas/Parts/Content/ImagesBlack/Lock-DealerCost.png" class="floatright" />
                            <div class="note">Dealer Cost Locked</div>
                        </div>
                    </div>
                <% }} %>
                <div class="price-info-block cf">
                    <% if (isAuthenticated) {
                        var saveCostStyle = supplier.isAuthorizedSupplier && !(supplier.price > 0) ? "display: block;" : "";
                        var addCustomCostStyle = "display: " + (supplier.hasCustomPrice || supplier.isAuthorizedSupplier && !(supplier.price > 0) ? "none;" : "block;");
                        var editCostCaption = !supplier.isAuthorizedSupplier ? "Enter Your Cost" : "Edit Cost"; %>
                        <div class="edit-custom-container">
                            <button class="btn-grey-v-hollow load-custom-link" style="<%= addCustomCostStyle %>"><%= editCostCaption %></button>
                            <button type="button" class="btn-grey-v-hollow remove-custom-price-link" style="display: <%= (supplier.hasCustomPrice ? 'block' : 'none') %>">Remove Custom $</button>
                            <div class="custom-price-edit" style="<%= saveCostStyle %>">
                                <div class="cf">
                                    <button type="button" class="btn-orange custom-price-submit">Submit</button>
                                    <button type="button" class="btn-cancel custom-price-cancel">Cancel</button>
                                </div>
                            </div>
                        </div>
                    <% } %>
                    <div class="supplier-updated-date" style="<%= supplier.modifiedDate == null ? 'display: none;' : '' %>">updated<br /><span class="supplier-updated-date-value"><%= supplier.modifiedDate %></span></div>
                </div>
            </div>
            <div class="product-purchase-block cf">
                <!-- Purchase block -->
                <% if (supplier.isInStock) { %>
                    <div class="part-stock-ind"><span>In Stock</span></div>
                <% } if (isAuthenticated) {
                    var showOrderForm = (supplier.price > 0 && supplier.isAuthorizedSupplier);
                    var orderDisplay = "display: " + (showOrderForm ? "block;" : "none;"); %>
                    <form action="#" class="purchase" style="<%= orderDisplay %>">
                        <div style="bottom: 0; position: absolute;">
                            <div class="row price-quantity-block cf">
                                <input type="text" class="qty" value="1" />
                                <div class="arrows">
                                    <a href="#" dir="up">Up</a>
                                    <a href="#" dir="down">Down</a>
                                </div>
                            </div>
                            <div>
                                <button type="button" class="btn-orange order-btn" style="color: #ffffff; float: none; height: 47px; margin: 0; width: 250px;">Add to Purchase Order</button>
                            </div>
                        </div>
                    </form>
                <% if (isAdministrator || supplier.hasCustomPrice) { %>
                <form action="/Parts/ProductDetails/SupplierUpdatePrice" method="GET" class="update-supplier-price-form" style="display:none">
                    <input type="hidden" id="PartId" name="PartId" value="<%= supplier.partId %>" />
                    <input type="hidden" id="PartMSRP" name="PartMSRP" value="<%= supplier.partMSRP %>" />
                    <input type="hidden" id="SupplierId" name="SupplierId" value="<%= supplier.supplierId %>" />
                    <input type="hidden" id="hasCustomPrice" name="hasCustomPrice" value="<%= supplier.hasCustomPrice %>" />
                    <div style="bottom: 0; position: absolute;">
                        <% if ((isAdministrator || isRep) && supplier.supplierWarehouses.length > 0){ %>
                            <div class="row">
                                <ul>
                                    <% var i = -1;
                                    _.each (supplier.supplierWarehouses, function(supplierWarehouse){
                                        i++;
                                        var itemClass = supplierWarehouse.onHand > 0 ? "supplier-warehouse-item" : "supplier-warehouse-empty-item"; %>
                                        <li class="<%= itemClass %>" warehouseid="<%= supplierWarehouse.warehouseId %>">
                                            <input type="hidden" name="SupplierWarehouses[<%= i %>].WarehouseId" id="SupplierWarehouses_<%= i %>__WarehouseId" value="<%= supplierWarehouse.warehouseId %>" />
                                            <span><%= supplierWarehouse.warehouseName %></span>
                                            <input type="text" class="supplier-warehouse-item-count floatright" name="SupplierWarehouses[<%= i %>]__OnHand" id="SupplierWarehouses_<%= i %>_OnHand" value="<%= supplierWarehouse.onHand %>" originalValue="<%= supplierWarehouse.onHand %>" />
                                        </li>
                                    <% }); %>
                                </ul>
                            </div>
                        <% } %>
                        <div class="row enter-cost price-quantity-block">
                            <div class="form-col">
                                <label>Dealer Cost</label>
                                <input type="text" id="priceTextBox" class="price" placeholder="$123.45" originalValue="<%= supplier.price %>" data_a_sign = "" data_a_sep = "" value="<%= supplier.price %>" />
                            </div>
                            <div class="form-col">
                                <label>Qty</label>
                                <input type="text" id="supplierUnitOfMeasureTextBox" placeholder="Each" originalValue="<%= supplier.supplierUnitOfMeasure %>" value="<%= supplier.supplierUnitOfMeasure %>" />
                            </div>
                        </div>
                        <div class="row">
                            <input type="button" value="Cancel" class="btn-grey" id="cancel-edit" style="margin-left: 5px; width: 110px;">
                            <input type="submit" value="Submit" class="btn-blue" style="width: 110px;">
                        </div>
                    </div>
                </form>
                 <% }} if (isAuthenticated && !supplier.isAuthorizedSupplier) { %>
                    <div style="bottom: 0; position: absolute;">
                        <div class="row authorized-block">
                            <button class="btn-grey authorized-btn" style="float: none; height: 47px; margin-left: 0; width: 250px;">GET AUTHORIZED</button>
                        </div>
                        <div class="row price-quantity-block custom-order-block cf">
                            <input type="text" class="qty" value="1" />
                            <div class="arrows">
                                <a href="#" dir="up">Up</a>
                                <a href="#" dir="down">Down</a>
                            </div>
                        </div>
                        <div class="custom-order-block">
                            <button type="button" class="btn-orange order-btn" style="color: #ffffff; float: none; height: 47px; margin: 0; width: 250px;">Add to Purchase Order</button>
                        </div>
                    </div>
                <% } else if (!isAuthenticated) { %>
                    <div style="bottom: 0; position: absolute;">
                        <button type="button" id="signin-pricing" href="/Authorization/Authorize" class="btn-orange login-btn" style="color: #ffffff; float: none; height: 47px; margin: 0; width: 250px;">LOGIN for Details</button>
                    </div>
                <% } %>
            </div>
        </div>
        <% if ((isAdministrator || isRep) && supplier.supplierWarehouses.length > 0){ %>
            <div class="cf">
                <!-- Warehouse block -->
                <div class="floatright" style="border-top: 2px solid #f3f3f3; padding: 20px; width: 520px;">
                    <% var listStyle = supplier.supplierWarehouses.length == 1 ? "-webkit-column-count: 1; -moz-column-count: 1; column-count: 1;" : ""; %>
                    <ul class="supplier-warehouse-list cf" style="<%= listStyle %>">
                        <% _.each (supplier.supplierWarehouses, function(supplierWarehouse) {
                            var itemClass = supplierWarehouse.onHand > 0 ? "supplier-warehouse-item" : "supplier-warehouse-empty-item";
                            itemClass += supplier.supplierWarehouses.length == 1 ? " floatright" : ""; %>
                            <li class="<%= itemClass %>" warehouseid="<%= supplierWarehouse.warehouseId %>">
                                <div class="cf">
                                    <div class="supplier-warehouse-item-count floatright"><%= supplierWarehouse.onHand %></div>
                                    <div class="floatleft"><%= supplierWarehouse.warehouseName %></div>
                                </div>
                                <div class="supplier-warehouse-updated-date"><%= supplierWarehouse.modifiedDate %></div>
                            </li>
                        <% }); %>
                    </ul>
                </div>
            </div>
        <% } %>
    </article>
</script>

<script type="text/template" id="supplierMainTmpl">
    <%
    var showPrice = supplier.isAuthorizedSupplier ? (supplier.salePrice != null ? formatPrice(supplier.salePrice) : (supplier.price != null ? formatPrice(supplier.price) : "0.00")) : "0.00";
    var title = isAuthenticated ? (supplier.IsWhiteLabel ? "Order" : "My Default Supplier") : "Sold By";
    %>
    <article class="side-block order-block" supplierid="<%= supplier.supplierId %>" suppliername="<%= supplier.name %>" hideuntilauthorized="<%= supplier.hideUntilAuthorized %>" price="<%= showPrice %>" isinstock="<%= supplier.isInStock %>" isdefaultsupplier="<%= supplier.isDefault %>" isAuthorizedSupplier="<%= supplier.isAuthorizedSupplier %>">
        <input type="hidden" id="defaultCost" value="<%= supplier.defaultCost %>" />
        <input type="hidden" id="defaultModifiedDate" value="<%= supplier.defaultModifiedDate %>" />
        <div class="row cf" style="padding-right: 11px;">
            <h2 class="order-block-title"><%= title %></h2>
            <% if (isAuthenticated && !supplier.isWhiteLabel) { %>
            <span class="supplier-default">
                <span class="supplier-default-type"><%= supplier.isDefault ? "set by dealer" : "calculated" %></span>
            </span>
            <% } %>
        </div>
        <% if (!supplier.isWhiteLabel) { %>
        <div class="row" style="position: relative;">
            <a class="supplier-name" href="/Parts/Profile/CompanyProfile/<%= supplier.supplierId %>"><%= supplier.name %></a>
        </div>
        <% } if (isAuthenticated) { %>
        <div class="price-info-block">
            <% if (isAdministrator) { %>
            <div class="edit-supplier"></div>
            <% } if (supplier.isInStock) { %>
            <div class="row">
                <div class="part-stock-ind"><span>In Stock</span></div>
            </div>
            <% } if (supplier.isAuthorizedSupplier) {
            var rowStyle = supplier.price != null && supplier.price > 0 ? "display: block;" : "";
            var price = supplier.price != null && supplier.price > 0 ? "$" + formatPrice(supplier.price) : "";
            var priceClass = supplier.hasCustomPrice || supplier.isSupplierDealerCustomPrice ? "custom-price" : "";
            priceClass = supplier.salePrice != null ? "sale-price" : priceClass;
            var priceLevelDisplay = supplier.priceName == null || supplier.priceName == "" ? "display: none;" : ""; %>
            <div class="row cf price-row" style="<%= rowStyle %>">
                <div class="form-col price-block">
                    <div class="price-txt <%= priceClass %>"><%= price %></div>
                    <% if (supplier.salePrice != null) { %>
                    <div class="sale-price-txt <%= priceClass %>">$<%= formatPrice(supplier.salePrice) %></div>
                    <% } %>
                    <div class="note <%= priceClass %>"><%= supplier.costNote %><span class="note-unit"><%= supplier.supplierUnitOfMeasure %></span></div>
                    <% if (supplier.discountPercentage != null) { %>
                    <div class="price-info-note" style="<%= priceLevelDisplay %>">Price Level - <%= supplier.priceName %></div>
                    <div class="price-info-note"><%= "$" + formatPrice(supplier.price.Value / ((100 - supplier.discountPercentage) / 100)) + " Less " + supplier.discountPercentage + "%" %></div>
                    <% } if (supplier.salePrice != null) { %>
                    <div class="sale-note"><%= supplier.saleEnd %></div>
                    <% } %>
                </div>
                <ul class="price-list">
                    <% if (isAdministrator || isRep) {
                    _.each (supplier.prices, function(priceModel){ %>
                    <li class="cf">
                        <div><%= priceModel.priceString %></div>
                        <div class="supplier-updated-date" style="<%= priceModel.priceModifiedDate == null ? 'display: none;' : '' %>">updated <span class="supplier-updated-date-value"><%= priceModel.priceModifiedDate %></span></div>
                    </li>
                    <% });} if (supplier.hasCustomPrice || supplier.isSupplierDealerCustomPrice){ %>
                    <li class="<%= priceClass %> cf">
                        <div><%= supplier.customPriceString %></div>
                        <div class="supplier-updated-date" style="<%= supplier.customPriceModifiedDate == null ? 'display: none;' : '' %>">updated <span class="supplier-updated-date-value"><%= supplier.customPriceModifiedDate %></span></div>
                    </li>
                    <% } %>
                </ul>
            </div>
            <div class="row cf authorized-block" style="display: none;">
                <div class="form-col price-block">
                    <img src="/Areas/Parts/Content/ImagesBlack/Lock-DealerCost.png" class="floatright" />
                    <div class="note">Dealer Cost Locked</div>
                </div>
            </div>
            <% } else { if (supplier.issAuthorizedOnlySupplier) { %>
                <section style="color:black;margin-top:10px;">
                    <div style="float: left; margin-left:10px; margin-right: 7px; margin-top: 2px;">
                        <img style="margin-left: 3px;" width="30" src="/Content/ImagesBlack/Attention_Icon.png" />
                    </div>
                    <div style="float: left; width: 210px">
                        <div style="font-size: 18px; font-weight: bold;">Additional Approval <br /> Required For this Item</div>
                        <div style="font-size: 12px;">
                            You are unlocked for <%= supplier.name %>, but this item requires a secondary level of authorization.
                        </div>
                    </div>
                    <div style="clear:both;"></div>
                </section>
           <% } else { %>
                <div class="row cf price-row">
                    <div class="form-col price-block">
                        <div class="price-txt custom-price"></div>
                        <div class="note custom-price"> / <span class="note-unit"></span></div>
                    </div>
                    <ul class="price-list"></ul>
                </div>
                <div class="row cf authorized-block">
                    <div class="form-col price-block">
                        <img src="/Areas/Parts/Content/ImagesBlack/Lock-DealerCost.png" class="floatright" />
                        <div class="note">Dealer Cost Locked</div>
                    </div>
                </div>
            <% }} %>
            <div class="row" style="text-align: right;">
                <% if (supplier.supplierPartNumber != null && supplier.supplierPartNumber != "") { %>
                <span>sku </span>
                <span><%= supplier.supplierPartNumber %></span>
                <% } %>
                <div class="supplier-updated-date" style="style="<%= supplier.modifiedDate == null ? 'display: none;' : 'display: block;' %>" margin: 0;">updated <span class="supplier-updated-date-value"><%= supplier.modifiedDate %></span></div>
            </div>
            <div class="row" style="padding-top:0; text-align: right;">
                <%
                    var saveCostStyle = "display: " + (supplier.isAuthorizedSupplier && !(supplier.price > 0) ? "block;" : "none;");
                    var addCustomCostStyle = "display: " + (supplier.hasCustomPrice || supplier.isAuthorizedSupplier && !(supplier.price > 0) ? "none;" : "block;");
                    var editCostCaption = !supplier.isAuthorizedSupplier ? "Enter Your Cost" : "Edit Cost";
                %>
                <div class="edit-custom-container">
                    <button type="button" class="btn-orange custom-price-submit" style="<%= saveCostStyle %>">Save Cost</button>
                    <button class="btn-grey-v-hollow load-custom-link" style="<%= addCustomCostStyle %>"><%= editCostCaption %></button>
                    <button type="button" class="btn-grey-v-hollow remove-custom-price-link" style="display: <%= (supplier.hasCustomPrice ? 'block;' : 'none;') %>">Remove Custom $</button>
                    <div class="custom-price-edit">
                        <input class="custom-price-text" type="text" data-a-sign="$" placeholder="$" />
                        <div class="cf" style="margin-top: 10px;">
                            <button type="button" class="btn-orange custom-price-submit">Submit</button>
                            <button type="button" class="btn-cancel custom-price-cancel">Cancel</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <% } %>
        <div>
            <% if (isAuthenticated) {
            var showOrderForm = supplier.price > 0 && supplier.isAuthorizedSupplier;
            var orderDisplay = "display: " + (showOrderForm ? "block;" : "none;"); %>
            <form action="#" class="purchase cf" style="<%= orderDisplay %>">
                <fieldset>
                    <% if ((isAdministrator || isRep) && supplier.supplierWarehouses.length > 0) { %>
                    <div class="row">
                        <ul class="supplier-warehouse-list cf">
                            <% _.each (supplier.supplierWarehouses, function(supplierWarehouse) {
                            var itemClass = supplierWarehouse.onHand > 0 ? "supplier-warehouse-item" : "supplier-warehouse-empty-item"; %>
                            <li class="<%= itemClass %> cf" warehouseid="<%= supplierWarehouse.warehouseId %>">
                                <span><%= supplierWarehouse.warehouseName %></span><span class="supplier-warehouse-item-count floatright"><%= supplierWarehouse.onHand %></span>
                                <div><span class="supplier-warehouse-updated-date floatright"><%= supplierWarehouse.modifiedDate %></span></div>
                            </li>
                            <% }); %>
                        </ul>
                    </div>
                    <% } if (supplier.isAuthorizedSupplier) {
                    var displayStyle = supplier.profit != null ? "display: block;" : "display: none;";
                    var profitValue = supplier.profit != null ? formatPrice(supplier.profit) : ""; %>
                    <div class="row profit-row" style="text-align: right; <%= displayStyle %>">
                        <div class="price-profit-percent"><span class="price-profit-percent-value<%= supplier.profitPercent %></span>%</div>
                        <div class="price-profit">Profit :$<span class="price-profit-value"><%= profitValue %></span></div>
                    </div>
                    <% } %>
                    <div class="row price-quantity-block cf">
                        <input type="text" class="qty" value="1" />
                        <div class="arrows">
                            <a href="#" dir="up">Up</a>
                            <a href="#" dir="down">Down</a>
                        </div>
                    </div>
                    <div class="row floatleft" style="padding: 11px 15px;">
                        <input type="button" value="Add to Purchase Order" class="btn-orange order-btn" style="color: #ffffff; float: none; height:47px; margin: 0; width: 270px;" />
                    </div>
                </fieldset>
            </form>
            <% if (isAdministrator || supplier.hasCustomPrice) { %>
            <form class="update-supplier-price-form" action="/Parts/ProductDetails/SupplierUpdatePrice" method="GET" style="display:none">
                <input type="hidden" name="PartId" id="PartId" value="<%= supplier.partId %>" />
                <input type="hidden" name="PartIMSRP" id="PartMSRP" value="<%= supplier.partMSRP %>" />
                <input type="hidden" name="SupplierId" id="SupplierId" value="<%= supplier.supplierId %>" />
                <input type="hidden" name="HasCustomPrice" id="HasCustomPrice" value="<%= supplier.hasCustomPrice %>" />
                <fieldset>
                    <% if ((isAdministrator || isRep) && supplier.supplierWarehouses.length > 0) { %>
                    <div class="row">
                        <ul>
                            <% var i = -1;
                            _.each (supplier.supplierWarehouses, function(supplierWarehouse){
                            i++;
                            var itemClass = supplierWarehouse.onHand > 0 ? "supplier-warehouse-item" : "supplier-warehouse-empty-item"; %>
                            <li class="<%= itemClass %>" warehouseid="<%= supplierWarehouse.warehouseId %>">
                                <input type="hidden" name="SupplierWarehouses[<%= i %>].WarehouseId" id="SupplierWarehouses_<%= i %>__WarehouseId" value="<%= supplierWarehouse.warehouseId %>" />
                                <span><%= supplierWarehouse.warehouseName %></span>
                                <input type="text" class="supplier-warehouse-item-count floatright" name="SupplierWarehouses[<%= i %>]__OnHand" id="SupplierWarehouses_<%= i %>_OnHand" value="<%= supplierWarehouse.onHand %>" originalValue="<%= supplierWarehouse.onHand %>" />
                            </li>
                            <% }); %>
                        </ul>
                    </div>
                    <% } %>
                    <div class="row enter-cost price-quantity-block">
                        <div class="form-col">
                            <label>Dealer Cost</label>
                            <input type="text" class="price" name="Price" id="priceTextBox" placeholder="$123.45" originalValue="<%= supplier.price %>" data-a-sign="" data-a-sep="" value="<%= supplier.price %>" />
                        </div>
                        <div class="form-col">
                            <label>Qty</label>
                            <input type="text" name="SupplierUnitOfMeasure" id="supplierUnitOfMeasureTextBox" placeholder="Each" originalValue="<%= supplier.supplierUnitOfMeasure %>" value="<%= supplier.supplierUnitOfMeasure %>" />
                        </div>
                    </div>
                    <div class="row">
                        <input type="button" value="Cancel" class="btn-grey" id="cancel-edit" style="margin-left: 5px; width: 110px;">
                        <input type="submit" value="Submit" class="btn-blue" style="width: 110px;">
                    </div>
                </fieldset>
            </form>
            <% } } if (isAuthenticated && !supplier.isAuthorizedSupplier) { %>
            <div class="row authorized-block">
                <button class="btn-grey authorized-btn" style="float: none; height: 47px; margin-left: 0; width: 270px;">GET AUTHORIZED</button>
            </div>
            <div class="row custom-order-block">
                <div class="arrows">
                    <a href="#" dir="up">Up</a>
                    <a href="#" dir="down">Down</a>
                </div>
            </div>
            <div class="row custom-order-block" style="padding: 11px 15px;">
                <button type="button" class="btn-orange order-btn" style="color: #ffffff; float: none; height: 47px; margin: 0; width: 270px;">Add to Purchase Order</button>
            </div>
            <% } else if (!isAuthenticated) {
            if (supplier.SupplierWarehouses.length > 0) { %>
            <div class="row">
                <div class="part-stock-teaser">
                    <span class="grey-square"></span>
                    <span>Login for stock status</span>
                </div>
            </div>
            <% } %>
            <div class="row" style="margin: 10px 0; padding: 11px 15px; text-align: center;">
                <a id="signin-pricing" href="/Authorization/Authorize" class="btn-grey" style="height: 47px; padding: 10px 32px; width: 116px;">LOGIN <span>to see pricing</span></a>
            </div>
            <% } %>
        </div>
    </article>
</script>
