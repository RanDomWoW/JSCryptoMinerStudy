<script>
    $(function () {
        var arr = [];
        $('.dropdown a').removeClass('selected');
        var lnks = $('.dropdown a');
        var thisLocation = window.location.href;
        for (var i = 0; i < lnks.length; i++) {
            var a = lnks[i].href;
            if (thisLocation == a) {
                lnks[i].className += " selected";
            }
        }
        //console.log('lnks', lnks);
        $('.nav > li')
            .mouseenter(function () {
                $('.dropdown').removeClass('open').css('background-color','inherit');
                $(this).addClass('open');
                setLocationColor();
            })
            .mouseleave(function () {
                $(this).removeClass('open').css('background-color', 'inherit');
                setLocationColor();

            });
        function setLocationColor() {
            var _location = window.location.href;
            if (_location.indexOf('/our-company') > -1) {
                $('.our-company').addClass('section-selected');
            }
            if (_location.indexOf('/restaurants') > -1) {
                $('.our-brands').addClass('section-selected');
            }
            if (_location.indexOf('/citizenship') > -1) {
                $('.dropdown.citizenship').addClass('section-selected');
            }
            if (_location.indexOf('/blog') > -1) {
                $('.darden-digest-link').addClass('section-selected');
            }
            if (_location.indexOf('/careers') > -1) {
                $('.career-center').addClass('section-selected');
            }
            if (_location.indexOf('/gift-cards') > -1) {
                $('.gift-cards').addClass('section-selected');
            }
        }
        setLocationColor();
    })
</script>

