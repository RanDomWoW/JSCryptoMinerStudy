	$(document).ready(function() {
		$("a.popup").click(function () {
			var w = $(window).width();
			var h = $(window).height();
			var _w = w * .85; // popup width, %75 of window
			var _h = h * .85; // popup height, %75 of window
			if (_h < 500)// Height cant be less than 500px
				_h = 500;
			var left = (w - _w) / 2;
			var top = (h - _h) / 2;
			$('<div class="popup_overlay">&nbsp;</div>').appendTo('body');
			$('<div class="popup_window" style="width:' + _w + 'px;height:' + _h + 'px;top:' + top + 'px;left:' + left + 'px"><iframe src="' + $(this).data('url') + '" style="width:100%;height:100%"></iframe></div>').appendTo('body');
			return false;
		});
	});

	function closePopup(refresh) {
		$(".popup_overlay, .popup_window").remove();
		if (refresh)
			location.reload();
	}
</script>
