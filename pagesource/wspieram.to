        });

//  Otwieranie aktywnej zakładki
        setTimeout(function() {
            
            if(getUrlParameterByName('active_tab'))
            {
                $('.fun_tab .options li[data-typ="'+getUrlParameterByName('active_tab')+'"]').addClass('active').click();
                setTimeout(function() {
                    if(location.hash){
                        $('html, body').animate({
