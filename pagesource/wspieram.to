                        }, 300);
                    }
                }, 700);
            }
        }, 700)

        // Tooltips
        $('.jbox-tooltip').each(function () {
            $(this).jBox('Tooltip', {
                getTitle: 'data-jbox-title',
                getContent: 'data-jbox-content',
                animation: 'flip',
                maxWidth: 500,
            });
        });

        $(".stick_in_parent").stick_in_parent();
        if ($(window).width() <= 736 ) {
            $(".rwd_stick_in_parent").stick_in_parent();
        }

            });
