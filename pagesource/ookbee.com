        $('.boxgrid.captionfull').hover(function () {
            $(".cover.skilllane-caption", this).stop().animate({ top: '20px' }, { queue: false, duration: 200 });
        }, function () {
            $(".cover.skilllane-caption", this).stop().animate({ top: '350px' }, { queue: false, duration: 200 });
        });
    });
    $(document).ready(function () {
        $('.boxgridsmall.captionfullsmall').hover(function () {
            $(".coversmall", this).stop().animate({ top: '100px' }, { queue: false, duration: 100 });
        }, function () {
            $(".coversmall", this).stop().animate({ top: '173px' }, { queue: false, duration: 100 });
        });

        $('.boxgridsmall.captionfullsmall').hover(function () {
            $(".coversmall.skilllane-caption", this).stop().animate({ top: '40px' }, { queue: false, duration: 100 });
        }, function () {
            $(".coversmall.skilllane-caption", this).stop().animate({ top: '173px' }, { queue: false, duration: 100 });
        });
    });

    function mouseover(path) {

        $("#imgPlaybutton").attr('src', path);
