        box-shadow: 0 0 10px #888;
        border-radius: 1px;
    }

    .boxcaption {
        float: left;
        position: absolute;
        background: white;
        height: 200px;
        width: 100%;
        opacity: .8;
        /* For IE 5-7 */
        filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=80);
        /* For IE 8 */
        -MS-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
    }

    .captionfull .boxcaption {
        top: 350px;
        left: 0;
        text-align: center;
    }
    /*small*/
    .boxgridsmall {
        width: 135px;
        height: 173px;
        overflow: hidden;
        position: relative;
        -moz-box-shadow: 0 0 5px #888;
        box-shadow: 0 0 10px #888;
        border-radius: 1px;
    }

    .boxcaptionsmall {
        float: left;
        position: absolute;
        background: white;
        height: 200px;
        width: 100%;
        opacity: .8;
        /* For IE 5-7 */
        filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=80);
        /* For IE 8 */
        -MS-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
    }

    .captionfullsmall .boxcaptionsmall {
        top: 173px;
        left: 0;
        text-align: center;
    }

    .fontSize-PromoteVDO {
        color: black;
        font-size: 35px;
        font-family: "DB Helvethaica X 57 Cond";
        margin: 0 !important;
    }
</style>
<script type="text/javascript">
    $(document).ready(function () {
        $('.boxgrid.captionfull').hover(function () {
