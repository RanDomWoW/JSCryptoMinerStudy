    <script>
        function ToggleFollow(itemcode, type, url, self) {
            $.ajax({
                url: url,
                type: 'POST',
                data: { itemcode: itemcode, type: type }
            })
                .success(function (response) {
                    if (response.isSuccess) {
                        if (response.isFollow) {
                            $(self).text("UnFollow");
                        } else {
                            $(self).text("Follow");
                        }
                        $(self).attr("onclick", "ToggleFollow('" + itemcode + "','" + type + "','" + response.actionName + "',this)");
                    } else {
                        if (!response.isLogin) {
                            window.location.href = response.actionName;
                        }
                    }
                });
        }
    </script>
