(adsbygoogle = window.adsbygoogle || []).push({});

var valid = 0;
var invalid = 0;

$(function() {
	$('.loading').show();
	$('.kviz').hide();
	$.post("../../wp-admin/admin-ajax.php",{
			action: 'GetRandomQuestion'
		},
		function(data){
			var questions = jQuery.parseJSON(data);
			$('#chapter').text(questions.Chapter);
			$('ul').find("[data-option=1]").find('div').text(questions.Answer1);
			$('ul').find("[data-option=2]").find('div').text(questions.Answer2);
			$('ul').find("[data-option=3]").find('div').text(questions.Answer3);
			$('#question').text(questions.Question);
			$('#question').data('id',questions.Id);
			$('.loading').hide();
			$('.kviz').show();
		});
});

$("ul.options li").on('click', function(){
	$('.loading').show();
	$('.kviz').hide();
	var option = $(this).data('option');
	$.post("../../wp-admin/admin-ajax.php",{
			action: 'GetAnswer',
			id: $('#question').data('id')
		},
		function(data){

			if(jQuery.parseJSON(data) == option){
				$('ul').find("[data-option="+option+"]").find('div').addClass('valid');
				valid += 1;
				$('.nextButton').css( "display", "block" );
			}
			else{
				$('ul').find("[data-option="+jQuery.parseJSON(data)+"]").find('div').addClass('valid');
				$('ul').find("[data-option="+option+"]").find('div').addClass('invalid')
				invalid += 1;
				$('.nextButton').css( "display", "block" );
			}
			$('#validCounter').text(valid);
			$('#invalidCounter').text(invalid);
			$('.loading').hide();
			$('.kviz').show();
	});
});

$('.nextButton').on('click', function(){
	$('.loading').show();
	$('.kviz').hide();
	$('li').find('div').removeClass('valid').removeClass('invalid');
	$.post("../../wp-admin/admin-ajax.php",{
			action: 'GetRandomQuestion',
		},
		function(data){
			var questions = jQuery.parseJSON(data);
			$('#chapter').text(questions.Chapter);
			$('ul').find("[data-option=1]").find('div').text(questions.Answer1);
			$('ul').find("[data-option=2]").find('div').text(questions.Answer2);
			$('ul').find("[data-option=3]").find('div').text(questions.Answer3);
			$('#question').text(questions.Question);
			$('#question').data('id',questions.Id);
			$('.loading').hide();
			$('.kviz').show();
		});
})

</script>
 

