    <script>
    $(document).ready(function() {
        // bxSlider init and options
        // Do not auto start slider (auto = false) --> use bxSlider's callbacks and methods to custom control slide transition times and slider start/stop/advance.
        slider = $('.bxslider').bxSlider({
            mode: 'fade',
            pause: $('.bx-slide').eq(0).data('duration'), // Grab initial slide's duration from its HTML5 data attr.
            infiniteLoop: true,
            pager: true,
            controls: false,
            auto: false,
            autoStart: false,
            touchEnabled: false,
            wrapperClass: 'bx-container',
            slideSelector: '.bx-slide',
            preloadImages: 'all',
            // Executed once after slider is fully loaded...
            onSliderLoad: function() {
                // Use callback to unhide homepage slider once bxSlider loads
                $('section.homepage-slider').css('visibility', 'visible');
                // Trigger any HTML5 <video> slides to reload/start playing
                $('video').trigger('load');
            },
            // Executed after each slide change
            onSlideAfter: function() {
                // Force stop "auto next" slide change
                slider.stopAuto();
                // Get current slide index number (zero indexed)
                var current_slide_index = slider.getCurrentSlide();
                // Get slide duration from HTML data attr.
                var current_slide_duration = $('.bx-slide').eq(current_slide_index).data('duration');
                // Wait "x" ms before going to next slide -- time taken from HTML data attr.
                sliderWaiting = setTimeout(function() {
                    // After setTimeout advance to the next slide in the list
                    slider.goToNextSlide();
                    // Trigger any HTML5 <video> slides to reload/start playing
                    $('video').trigger('load');
                }, current_slide_duration);
            }
        });
        // Start the bxSlider auto slide show once slider loaded
        slider.startAuto();
    });
    </script>
    
