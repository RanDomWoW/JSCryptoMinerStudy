<script>
	jQuery(document).ready(function($){

		if($.cookie('newsletter_signup') === null) {
			$.cookie("newsletter_signup", 1);
			$('.overlay').fadeIn(400,function(){
				$("#newsletter_signup").slideDown();
			});
		}

		$("body").on('submit', '#submitContact', function(e){
			e.preventDefault();
			$.ajax({
				type: 'POST',
				url: '/ajax/ccAdd.php',
				data: $("#submitContact").serialize(),
				beforeSend:function(){
					// this is where we append a loading image
					$('#ajax-panel').html('<div class="loading"><img src="/images/loading.gif" alt="Loading..." /></div>');
				},
				success:function(data){
					// successful request; do something with the data
					$.cookie("newsletter_signup", 1, { expires : 60 });
					$('#ajax-panel').empty();
					$(data.msg).find('item').each(function(i){
						$('#ajax-panel').append('<h4>' + $(this).find('title').text() + '</h4><p>' + $(this).find('link').text() + '</p>');
					});
					$('#newsletter_signup form').fadeOut(400,function(){
						$('#newsletter_signup').addClass('thankYou');
						setTimeout(function() {
							$('#newsletter_signup').slideUp( "slow", function() {
								$(".overlay").hide();
							});
						}, 3000);
					});
				},
				error:function(){
					// failed request; give feedback to user
					$('#ajax-panel').html('<p class="error"><strong>Oops!</strong> Try that again in a few moments.</p>');
				}
			});

		});
		
		$("body").on('submit', '#submitContactFooter', function(e){
			e.preventDefault();
			$.ajax({
				type: 'POST',
				url: '/ajax/ccAdd.php',
				data: $("#submitContactFooter").serialize(),
				beforeSend:function(){
					// this is where we append a loading image
					$('#ajax-panel').html('<div class="loading"><img src="/images/loading.gif" alt="Loading..." /></div>');
				},
				success:function(data){
					// successful request; do something with the data
					if(data.error!=false){
						window.location.replace("https://mattandnat.com/?thank-you-subscribe");
					}
				},
				error:function(){
					// failed request; give feedback to user
					$('#ajax-panel').html('<p class="error"><strong>Oops!</strong> Try that again in a few moments.</p>');
				}
			});

		});
	});
</script>



