		jQuery('.filter-price button').click(function(e){

			e.preventDefault();

			var cur_class = jQuery(this).attr('class');
			var base = "https://mattandnat.com/";
			var n = base.search("price");
			var cat = base.search("catalogsearch");

			var now = new Date();
			var time = now.getTime();
			var expireTime = time + 1000*36000;
			now.setTime(expireTime);
			document.cookie = 'sort='+cur_class+';expires='+now.toGMTString()+';path=/';

			if(cat){
				if(cat > 1){
					if( cur_class === 'low-to-high' ){
