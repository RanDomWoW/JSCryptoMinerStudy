	jQuery(document).ready(function(){

		jQuery('#options > li').click(function(){

			setCookie('redirect',this.id);

			var currenturl = window.location.pathname;

			if (currenturl.toLowerCase().indexOf('/fr') == 0) {
				location.href = "/";
			}
			else if (this.id == 'french' && currenturl.toLowerCase().indexOf('/shop/') != 0) {
				location.href = '/fr';
			}
			else {
				location.reload(true);
			}
		});


	});

</script>



