<script type="text/javascript">
	// This allows for Javascript setting of cookie for store selector dropdown menu in header.phtml - AB.
	function setCookie(name, value, expires, path, domain, secure) {
		var cookieStr = name + "=" + escape(value) + "; ";
		if( expires ){
			var expiresDate = new Date(new Date().getTime() + expires * 24 * 60 * 60 * 1000);
			cookieStr += "expires=" + expiresDate.toGMTString() + "; ";
		}

		cookieStr += "path=/" + "; ";

		if( domain ){
			cookieStr += "domain=" + domain + "; ";
		}

		if( secure ){
			cookieStr += "secure; ";
		}

		document.cookie = cookieStr;
	}

	
	
</script>

