		if(cart > 0){
			$.map( obj, function( val, i ){		
				jQuery('.cart .sub-menu').append(
					'<div id="products-cart">\
						<a href="'+val.url+'" class="product">\
						 	<div class="image">\
						 		<img width="45" height="45" src="'+val.image+'" alt="">\
						 	</div>\
						 	<div class="text">\
						 		<span class="name">'+val.name+'</span>\
						 		<span class="color">CLR:'+val.color+'</span>\
						 		<span class="qty">QTY: '+val.qty+'</span>\
						 		<span class="price">$'+val.price+'</span>\
						 	</div>\
						</a>\
					</div>'
				);
			});
			jQuery('.inner .top-right .item.cart > span').text('('+cart+')');
			jQuery('.cart .sub-menu').append(
				'<div class="total">\
					<span>Total: $'+total_cart+'</span>\
				</div>\
				<a href="http://mattandnat.com/shop/checkout/cart/" id="go-to-cart" class="buttons">View shopping cart</a>\
				<a href="http://mattandnat.com/shop/onepagecheckout/" id="go-to-checkout" class="buttons">Proceed to checkout</a>'
			);
			jQuery('.empty').remove();
		}
		else{
			jQuery('.inner .top-right .item.cart > span').text('(0)');
		}
	});
</script>
