					}
					else{
						var rep2 = base.replace(/[&?=]/g,'');
						rep2 = rep2.replace('dirascorderprice','');
						rep2 = rep2.replace('dirdescorderprice','');
						window.location.href = ''+rep2+'?dir=desc&order=price';
					}
				}
				if(n < 1 && cat < 1){
					if(cur_class === 'low-to-high' ){
						//console.log('yes');
						window.location.href = ''+base+'?dir=asc&order=price';
					}
					else{
						window.location.href = ''+base+'?dir=desc&order=price';
					}
				}
			}
		});
	});
