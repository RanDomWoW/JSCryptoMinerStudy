<script >
    function CE(e) {
      

        var selectedText = (
        window.getSelection
        ?
            window.getSelection()
        :
            document.getSelection
            ?
                document.getSelection()
            :
                document.selection.createRange().text
     );
        if (e.keyCode == 13 && e.ctrlKey) {
           
            $('#Xato').val(selectedText);
            $('#Tugri').val(selectedText);
            //alert();
            $('#xato1').show();
            $('#over').show();
        }
    }
    function Close_Xato() {
      
        $('#xato1').hide();
        $('#over').hide();
    }
    function CtrlEnter(elem) {
      
        var element = $(elem);
        var form = element.parents("form");
        var fields = form.serializeArray();
        var data = new FormData();
        for (var j = 0; j < fields.length; j++) {
            data.append(fields[j].name, fields[j].value);
        }
        $.ajax({
            type: 'POST',
            url: form.attr("action"),
            data: data,
            cache: false,
            contentType: false,
            processData: false,
            success: function (data) {
                if (data.msg != undefined) alert(data.msg);
            }
        });
    }

</script>


