<script id="participantListTop" type="text/template">
	<h4>Personen</h4>	    
	<ul>
        <li class="grid-row">
            <div class="grid-col col1-3"><strong>Personen</strong></div>
            <div class="grid-col col2-3">
               <select class="participantCount" data-role="participantCount">					
					[% for(var index=1; index <= maxParticipantCount; index++) { %]
						<option value="[%=index%]" [%= (!showTenPlusMessage && index == participants.length) ?  "selected='selected'" : ""%]>[%=index%]</option>
					[% }%]
				   <option value="10+" [%= showTenPlusMessage  ?  "selected='selected'" : ""%]>10+</option>
				</select>
                <div class="help-text-container">
                    <div class="tooltip-text hoverable">Geef het aantal personen van uw reisgezelschap op</div>
                </div>
            </div>
        </li>
		<li [%= showTenPlusMessage  ? "" : "class='invisible'"%]>
			<div class="pk-large-group-notice">
				<h4>Bel ons. Onze specialisten helpen u graag bij het uitzoeken van de perfecte reis! <br />Voor groepen vanaf 20 personen kunt u ook direct bellen met: 023 530 75 83</h4>
				<div class="popup-contactcenter-info grid-row">
					<div class="grid-col col1-2">
						<a class="call-info" href="tel:+0237510606" >
							<i class="iconfont iconfont-phone-right"></i>
							<span class="call-text">023 751 06 06</span>
						</a>
					</div>
					
						<div class="grid-col col1-2 chat-info">
							<a class="chat-info">
								<i class="iconfont iconfont-bubble"></i>
								<span class="chat-text">Kunnen wij u helpen?<strong> Live chat</strong></span>
							</a>
						</div>
					
				</div>
			</div>
		<li>
		[% if (showTenPlusMessage === false) {%]
		[%_.each(participants, function(item, index){ %]
			<li class="grid-row">
				
						[% if (loggedIntoMyCorendon) { %]
							<div class="grid-col col1-3">
								<select class="traveller" data-id="[%= item.id %]">
									<option value="-1" [%= item.selectedContactTravellerId == -1 ? "selected='selected'" : "" %]>
										Persoon [%= index + 1 %]
									</option>
									[% _.each(item.contactTravellerList, function (contactTraveller, index) { %]
										<option value="[%= contactTraveller.id %]" [%= item.selectedContactTravellerId == contactTraveller.id ? "selected='selected'" : "" %]>
											[%= contactTraveller.fullName %]
										</option>
									[% }) %]
								</select>
							</div>
						[% } else { %]
							<div class="grid-col col1-3">Persoon [%= index + 1 %]</div>
						[% }%]
				
			 
				<div class="grid-col col2-3">
					<input type="text" name="birthdate_[%=item.id%]" data-id="[%=item.id%]" class="birthdate" value="[%=item.birthdate%]" />
					[% if (index >= 0 && participants.length>1) {%]
					<span class="icon delete remove-participant" data-id="[%=item.id%]"></span>						
					[%}%]
					<div class="help-text-container">
						<div class="tooltip-text error">Geboortedatum vereist</div>
					</div>					
				</div>
			</li>			
		[%})%]
		[%}%]       
    </ul>
	 [% if(showTenPlusMessage === false) { %]
	<div class="add-item">
		[% if(participants.length < maxParticipantCount) { %]
			<i class="icon add-item add-participant"></i>
			<a href="#" class="add-participant">Voeg een persoon toe</a>
		[%}%]
		<p [%= participants.length < maxParticipantCount  ? "" : "class='more-persons-message'"%]><i>Reist u met een groep van meer dan 9 personen? <a id="add10">klik hier</a></i><p>
	</div>	
	[%}%]
</script>

<script id="participantsRooms" type="text/template">
	<h4>Aantal (familie)kamers</h4>
	 <ul>
        <li class="grid-row">
            <div class="grid-col col1-3"><strong>Aantal (familie)kamers</strong></div>
            <div class="grid-col col2-3">
                <select class="roomCount">					
					[% for(var index=1; index <= maxRoomCountCount; index++) { %]
			 			<option [%= index == roomCount ?  "selected='selected'" : ""%]>[%=index%]</option>
					[% }%]
				</select>
            </div>
        </li>
    </ul>
	<ul class="room-assignment">
        <li class="labels column">
            <div class="row"></div>
            [%_.each(participants, function(item, index){ %]
				<div class="row">
					
						[% if (loggedIntoMyCorendon) { %]
							[% if (item.fullName) { %]
								<span class="label">[%= item.fullName %]</span>
							[% } else { %]
								<span class="label">Persoon [%=index + 1%]</span>
							[% } %]
						[% } else { %]
							<span class="label">Persoon [%=index + 1%]</span>
						[% } %]
					
					<span class="label birthdate" data-id="[%=item.id%]">[%=item.birthdate%]</span>
				</div>				
			[%})%]			                  
        </li>
		[% for(var i=0; i < roomList.length; i++) { %]
        <li class="room column [%=roomList[i].isRoomInError ? 'highlighted' : ''%]">
            <div class="row"><span class="label">Kamer<br />[%=roomList[i].roomNr%]</span></div>
            [%_.each(participants, function(item, index){ %]
			<div class="row">
                <input type="radio" name="roomAssign[%=item.id%]" class="roomAssign"
					[%= item.roomNr == roomList[i].roomNr ? "checked='checked'" : ""%]
					data-id="[%=item.id%]" data-room-nr="[%=roomList[i].roomNr%]" />						
            </div>
			[%})%]			                 
        </li>
		[% }%]                
    </ul>
	[% if(existsRoomInError) { %]
	<div class="help-text-container">
        <div class="tooltip-text error">Minimaal 1 persoon per kamer. Let op: minderjarigen mogen alleen onder begeleiding van tenminste 1 volwassene verblijven op een kamer.</div>
    </div>
	[% }%] 	
</script>

