<script>
	$(function () {
		$('.showSignUpModal').click(function(e) {
			e.preventDefault();

			// redirect logged in user to dashboard
			var isLoggedIn = false;
			if (isLoggedIn) {
				if ($(this).data('page') === 'pricing') {
					window.location.href = "https:\/\/account.smartsupp.com\/subscription\/";
				} else {
					window.location.href = "https:\/\/dashboard.smartsupp.com\/";
				}
				return false;
			}

			var loading = $('.modal-loading');
			var formContainer = $('.modal-form-container');
			$(document).on('ajaxStart.signUpModal', function () {
				formContainer.hide();
				loading.fadeIn();
			}).on('ajaxStop.signUpModal', function () {
				loading.hide();
				formContainer.fadeIn();
			});

			var signUpForm = $('#signUpForm');
			signUpForm.find('input[name="source"]').val($(this).data('source'));
			signUpForm.find('input[name="package"]').val($(this).data('package'));
			signUpForm.find('input[name="timezone"]').val(jstz.determine().name());
			$('#signUpModal').modal('show');
			return false;
		});
	});
</script>

