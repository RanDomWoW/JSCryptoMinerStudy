<!-- バイク輸送・陸送ならBAS -->
<div id="pagetop"><a href="#"><img src="/common/img/foot/btn_pagetop.png" alt="バイク輸送・陸送ならBASのトップへ"></a></div>
<script type="text/javascript">
$(function() {
    var topBtn = $('#pagetop');    
    topBtn.hide();
    //スクロールが100に達したらボタン表示
    $(window).scroll(function () {
        if ($(this).scrollTop() > 100) {
            topBtn.fadeIn();
        } else {
            topBtn.fadeOut();
        }
    });
    //スクロールしてトップ
    topBtn.click(function () {
        $('body,html').animate({
            scrollTop: 0
        }, 500);
        return false;
    });
});
</script>

