<script id="checkMobile" languague="JavaScript" type="text/javascript">

function closeAppBanner() {
	var expires = new Date();
	expires.setTime(expires.getTime() + (24 * 60 * 60 * 1000));
	document.cookie = "app=banner; expires=" + expires.toUTCString() + "; path=/";
	
	document.getElementById("appBanner").style.display = "none";
	document.getElementById("androidText").style.display = "none";
	document.getElementById("appleText").style.display = "none";
	document.getElementById("androidImage").style.display = "none";
	document.getElementById("appleImage").style.display = "none";
}

function checkMobileOperatingSystem() {
	var os = getMobileOperatingSystem();
	
	var showBanner = false;
	
	var bannerCookie = document.cookie;
	if(bannerCookie != null) {
		showBanner = bannerCookie.indexOf("app=banner") < 0;		
	}
	
	document.getElementById("appBanner").style.display = "none";
	document.getElementById("androidText").style.display = "none";
	document.getElementById("appleText").style.display = "none";
	document.getElementById("androidImage").style.display = "none";
	document.getElementById("appleImage").style.display = "none";
	
	if(showBanner == true) {
		if(os == "android") {
			document.getElementById("appBanner").style.display = "block";
			document.getElementById("androidText").style.display = "block";
			document.getElementById("androidImage").style.display = "block";
		} else if(os == "apple") {
			document.getElementById("appBanner").style.display = "block";
			document.getElementById("appleText").style.display = "block";
			document.getElementById("appleImage").style.display = "block";
		}
	}
	
}

function getMobileOperatingSystem() {
  var userAgent = navigator.userAgent || navigator.vendor || window.opera;

  if (userAgent.match( /iPad/i ) || userAgent.match( /iPhone/i ) || userAgent.match( /iPod/i ) ) {
    return 'apple';
  } else if( userAgent.match( /Android/i ) ) {
    return 'android';
  } else {
    return 'other';
  }
}

checkMobileOperatingSystem();

</script>
