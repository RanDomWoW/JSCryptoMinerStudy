<script>
    $(document).ready(function () {
        $(document).on('click', '.js-ow-form-submit-charity', function (e) {
            e.preventDefault();
            var numberCert = $('.js_number_charity').val();
            if(numberCert.length > 0) {
                $('.js_error_block').empty();
                $('.js_number_charity').removeClass('error');
                var nameCharity = $('.js_name_charity').children().data('name_charity');
                $('.js_input_path').val("/");
                $('.js_input_name').val(nameCharity);
                var data = $('.js-ow-form-charity').serialize();
                $.ajax({
                    data: data,
                    type: 'post',
                    url: '/local/ajax/charity_form.php',
                    dataType: 'html',
                    success: function (answer) {
                        $('#send-certif').addClass('m-ty');
                        $('#send-certif').append(answer);
                    }
                })
            } else {
                $('.js_error_block').text('Введите номер сертификата');
                $('.js_number_charity').addClass('error');
            }
        })
    })
