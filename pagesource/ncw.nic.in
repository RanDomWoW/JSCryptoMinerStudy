			var slideShowSpeed = 5000
			var crossFadeDuration = 100
			var Pic = new Array()
			Pic[0] = 'http://ncw.nic.in/Slide/1.JPG'
			Pic[1] = 'http://ncw.nic.in/Slide/2.JPG'
