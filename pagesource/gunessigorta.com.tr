    window.location.replace('/arama.aspx?sk=' + escape(kelime) + '&k=' + escape(kelime));
  } else {
    alert("Arama yapmak için lütfen kelime giriniz");
    return;
  }
}

function kvkkGonder() {
	var t = new Date();
	$('#divKvkkLayout').fadeOut();

	$.ajax({
		url: "/kvkk-tamam.ashx?d=" + t.getTime(),
		type: 'POST',
		async: true,
		beforeSend: function () { console.log("İşlem gönderiliyor"); },
		complete: function () { console.log("İşlem tamamlandı"); },
		error: function (XMLHttpRequest, textStatus, errorThrown) {
			alert("Beklenmeyen bir hata ile karşılaşıldı. Lütfen daha sonra tekrar deneyiniz.");
			window.location.reload();
		},
		success: function (json) {
			console.log("İşlem başarılı");
		}
	});
}

