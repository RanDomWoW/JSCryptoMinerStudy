function fnMnuLinkMv(perBizDiv, mainMnuId, subMnuId){
	
	var hForm = document.gnbForm;
	
	hForm.mainMnuId.value  = mainMnuId;
	hForm.subMnuId.value   = subMnuId;
	hForm.perBizDiv.value  = perBizDiv;
	//개인
	if(perBizDiv == '01'){
		// 마이핀
		if(mainMnuId == '1'){
			
			// 마이핀
			switch (subMnuId)
			{
			
			case "1": 
				hForm.tc.value = 'kcb.acs.non.mn.cmd.PerMypinCertIntroCmd';
				break;
				
			case "2": 
				hForm.tc.value = 'kcb.acs.non.mn.cmd.PerMypinCertRegiCmd';
				break;

			case "3": 
				hForm.tc.value = 'kcb.acs.non.mn.cmd.PerMypinCertMngCmd';
				break;

				
			}
				
		}else if(mainMnuId == '2'){
		 //아이핀
		 
			switch (subMnuId)
			{
			
			case "1": 
				hForm.tc.value = 'kcb.acs.non.mn.cmd.PerIpinCertIntroCmd';
				break;
				
			case "2": 
				hForm.tc.value = 'kcb.acs.non.mn.cmd.PerIpinCertRegiCmd';
				break;

			case "3": 
				hForm.tc.value = 'kcb.acs.non.mn.cmd.PerIpinCertMngCmd';
				break;

				
			}

		}else if(mainMnuId == '3'){
		 // 실명등록	

			switch (subMnuId) {
			
				case "1": 
					hForm.tc.value = 'kcb.acs.non.mn.cmd.PerRealCertMnCmd';
					break;
					
			}
		}
		 else{
			
			switch (subMnuId) {
			
				case "1": 
					hForm.tc.value = 'kcb.acs.non.mn.cmd.PerCusQuesCmd';
					break;
					
				case "2": 
					hForm.tc.value = 'kcb.acs.non.mn.cmd.PerCusBoardCmd';
					break;
	
				case "3": 
					hForm.tc.value = 'kcb.acs.non.mn.cmd.PerCusTelInfoCmd';
					break;

			
			}
		
		}
	
		hForm.target="_self";
		hForm.submit();
	
	}
	
	//기업
	if(perBizDiv == '02'){
		// 마이핀
		if(mainMnuId == '1'){
			
			// 마이핀
			switch (subMnuId)
			{
			
			case "1": 
				hForm.tc.value = 'kcb.acs.non.mn.cmd.BizMypinCertIntroCmd';
				break;
				
			case "2": 
				hForm.tc.value = 'kcb.acs.non.mn.cmd.BizMypinCertAdvCmd';
				break;

			case "3": 
				hForm.tc.value = 'kcb.acs.non.mn.cmd.BizMypinCertReqCmd';
				break;

				
			}
				
		}else if(mainMnuId == '2'){
		 //아이핀
		 
			switch (subMnuId)
			{
			
			case "1": 
				hForm.tc.value = 'kcb.acs.non.mn.cmd.BizIpinCertIntroCmd';
				break;
				
			case "2": 
				hForm.tc.value = 'kcb.acs.non.mn.cmd.BizIpinCertAdvCmd';
				break;

			case "3": 
				hForm.tc.value = 'kcb.acs.non.mn.cmd.BizIpinCertReqCmd';
				break;
				
			case "4": 
				hForm.tc.value = 'kcb.acs.non.mn.cmd.BizIpinPubIntroCmd';
				break;

			}

		}else if(mainMnuId == '3'){
		 // 본인확인	

			switch (subMnuId) {
			
				case "1": 
					hForm.tc.value = 'kcb.acs.non.mn.cmd.BizIdCertIntroCmd';
					break;
					
				case "2": 
					hForm.tc.value = 'kcb.acs.non.mn.cmd.BizIdCertAdvCmd';
					break;
					
				case "3": 
					hForm.tc.value = 'kcb.acs.non.mn.cmd.BizIdCertReqCmd';
					break;
					
			}
			
		}else if(mainMnuId == '4'){
			//고객센터
			switch (subMnuId) {
				case "1": 
					hForm.tc.value = 'kcb.acs.non.mn.cmd.BizCusNmIntroCmd';
					break;
					
				case "2": 
					hForm.tc.value = 'kcb.acs.non.mn.cmd.BizCusMnuDwnCmd';
					break;
					
				case "3": 
					hForm.tc.value = 'kcb.acs.non.mn.cmd.BizCusTelInfoCmd';
					break;
					
				case "4": 
					hForm.tc.value = 'kcb.acs.non.mn.cmd.BizCusBoardCmd';
					break;
			}
		}else if(mainMnuId == '5'){
			//사업자확인
			switch (subMnuId) {
				case "1": 
					hForm.tc.value = 'kcb.acs.non.mn.cmd.BizBsnCnfrmCmd';
					break;
			}
		}
		
		hForm.target = "_self";
		hForm.submit();
		
	}
	
	
}

