	//fix home page issue on certain Safari viewports:
	if($('body.home').length > 0){
		$('html, body').animate({scrollTop: 5}, 50).animate({scrollTop: 0}, 50);
	}
	
	//make submenus line up with main menu items on desktop:
	var padding = 5;
	var menus = ['HomeLoans', 'LendingPartners', 'About'];
	
	$(window).on('load resize', function(e){
		if($(window).width() >= 992){
			menus.forEach(function(element, index, array){
				var left = document.querySelector('#nav'+element).getBoundingClientRect().left;
				$('#nav'+element+'Submenu').css('padding-left', (left+padding)+'px');
			});
		}else{
			$('nav.desktop .submenu').css('padding-left', '');
		}
		
		//fix flexbox on IE11:
		$('body.home .media-body').css('height', '');
		$('body.buying-a-home .media-body').css('height', '');
		if($(window).width() >= 992){
			var h = 0;
			$('body.home .media-body, body.buying-a-home .media-body').each(function(count){
				h = Math.max(h, $(this).outerHeight());
			});
			$('body.home .media-body, body.buying-a-home .media-body').css('height', h+'px');
		}
	});
	
	$(window).on('load', function(e){
		$('.submenu').css('display', '');
		console.log(3);
		
		$('body').on('mousemove', function(e){
			$('.submenu').css('display', '');
		});

		$('.desktop-menu .mainMenu .navItem').on('mouseenter', function(e){
			console.log(e);
			$('.desktop-menu .mainMenu .navItem a').removeClass('active');
			$('.desktop .submenu').removeClass('show-submenu');
			$('.desktop-menu .mainMenu #'+e.target.id).addClass('active');
			$('.desktop #'+e.target.id+'Submenu').addClass('show-submenu');
			e.preventDefault();
			e.stopPropagation();
		});
		$('nav.desktop').on('mouseleave', function(e){
			$('.desktop-menu .mainMenu .navItem a').removeClass('active');
			$('.desktop .submenu').removeClass('show-submenu');
		});

		$('#' + $('.desktop-menu .mainMenu .navItem .active').attr('id') + 'Submenu').addClass('show-submenu');
	});
	
