              }); 

              function toggleChevron(e) {
                  console.log(e.target);
                  console.log($(e.target).prev('.card-header'));
                  console.log($(e.target).prev('.card-header').find('i'));
                  $(e.target)
                      .prev('.card-header')
                      .find('i')
                      .toggleClass('fa-chevron-down fa-chevron-right');
              }

              $('#mobile-accordion').on('hide.bs.collapse', toggleChevron);
              $('#mobile-accordion').on('show.bs.collapse', toggleChevron);
              $('link[href^="/Telerik.Web.UI.WebResource.axd"]').remove();
