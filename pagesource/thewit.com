                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">Download the file</h4>
                </div>
                <div class="modal-body" style="text-align: center">
                    <label>Please enter your e-mail to download the Pre-MIPCOM 2016 Mood report. </label>
                    <br>
                    <input id="emailRequest" type="text" autocomplete="off" name="email">
                    <div class="bg-danger" style="display: none; color: red; margin-top: 5px; margin-left: 50px; margin-right: 50px">
                        Please enter a valid e-mail to download the Pre-MIPCOM 2016 Mood report. 
                    </div>

                    <div style="margin-top: 5px">
                        <a href="#" id="dwFile" class="btn btn-primary">Download the file</a>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <script>
        $(document).ready(function () {
            $("#dwFile").click(function () {
                if (isValidEmailAddress("" + $("#emailRequest").val()) && $("#emailRequest").val() != "") {
                    $(".bg-danger").hide();
                    document.location.href = "download/downloadPreMipCom2016.aspx?email=" + $("#emailRequest").val();
                } else {
                    $(".bg-danger").show();
                }
            });
        });
        function isValidEmailAddress(emailAddress) {
            var pattern = new RegExp(/^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/);
            return pattern.test(emailAddress);
        };


    </script>

    <style>
        #emailRequest {
            background: White none repeat scroll 0 0;
            border: 1px solid #fbcae0;
            border-radius: 3px;
            color: #666666;
            font-family: Georgia,Arial,sans-serif;
            height: 26px;
            line-height: 26px;
            padding: 0 8px;
            text-align: center;
            width: 260px;
        }
    </style>


</body>
</html>