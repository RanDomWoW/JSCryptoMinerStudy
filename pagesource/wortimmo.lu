	 function uncryptLink(cryptedUrl){
	  var nl = "";
	  for(var i = 0; i < cryptedUrl.length; i++)
		 nl = nl + String.fromCharCode(cryptedUrl.charCodeAt(i) - 5);
	  return nl;  
	}
	

	/* We uncrypt crypted links */
	$(".crypted-link").on("click",function(){
		if(!$(this).attr("data-open-type")) {
			var uncryptedUrl = uncryptLink($(this).attr("data-crypted-value"));
			$(location).attr('href', uncryptedUrl);	
		} else {
			if($(this).attr("data-open-type") == "_blank")
				window.open(uncryptLink($(this).attr("data-crypted-value")));
		}
	});

    /* show email alert modal after 2 minutes on website */
    $(document).on('ready', function(){
        if(Cookies.get('firstVisitTime') == null){ // set a cookie to save 
            Cookies.set('firstVisitTime', new Date().getTime(), { expires: 1 });
        }

        //enable timer only from 'recherche' page and the modal has never been seen
        if('accueil' == 'recherche' && Cookies.get('modalAlertEmailShownOnce') == null){
            var timer = window.setInterval(function(){
                var firstVisitTime = Cookies.get('firstVisitTime');
                var now = new Date().getTime();
                var elapsedTime = (now-firstVisitTime)/1000;//in seconds

                if (elapsedTime >= 120.0 && Cookies.get('modalAlertEmailShownOnce') == null) {
                    // We don't open the modal if another one is open
                    if($('.modal.in').length == 0) {
                        $('#listing-alert-email-modal').modal('show');
                        clearInterval(timer);
                    }
                }
            }, 10000);
        }

