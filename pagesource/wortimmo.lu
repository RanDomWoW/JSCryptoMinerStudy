<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/11.0.13/css/intlTelInput.css" />
<link rel="stylesheet" type="text/css" href="https://www.wortimmo.lu/css/iovox.css" />
<script src="/js/formValidation.min.js"></script>
<script src="/js/formvalidation-bootstrap.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-toast-plugin/1.3.1/jquery.toast.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/11.0.13/js/intlTelInput.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {


        $("[data-toggle='popover']").popover();

        $('body').on('shown.bs.popover', function (element) {
            window.initIntel(element);
        });

        $('body').on('click.hideOtherPopper', function (e) {
            if (!$(e.target).hasClass('country-name') && !$(e.target).hasClass('selected-dial-code') && !$(e.target).hasClass('country') && e.target.id != 'iovox-validate-button') {
                $('[data-toggle="popover"],[data-original-title]').each(function () {
                    if (!$(this).is(e.target) && $(this).has(e.target).length === 0 && $('.popover').has(e.target).length === 0) {
                        (($(this).popover('hide').data('bs.popover') || {}).inState || {}).click = false;  // fix for BS 3.3.6
                    }
                });
            }
        });


        window.initIntel = function (element) {
            var iovoxIdAgency = $(element.target).attr('data-agency-id');
            $('.iovox-num').keypress(function (event) {
                if (event.keyCode === 13) {
                    event.preventDefault();
                }
            });

            $('.iovox-call-elem')
                .find('[name="phoneNumber"]')
                .intlTelInput({
                    utilsScript: 'https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/11.0.13/js/utils.js',
                    autoPlaceholder: true,
                    nationalMode: true,
                    preferredCountries: ['fr', 'lu', 'de'],
                    geoIpLookup: function (callback) {
                        $.get("https://ipinfo.io", function () {
                        }, "jsonp").always(function (resp) {
                            var countryCode = (resp && resp.country) ? resp.country : "";
                            callback(countryCode);
                        });
                    },
                    initialCountry: "auto",
                    separateDialCode: true,
                    getExtension: function () {
                        if (window.intlTelInputUtils) {
                            return intlTelInputUtils.getExtension(this._getFullNumber(), this.selectedCountryData.iso2);
                        }
                        return "";
                    },
                    getNumber: function (format) {
                        if (window.intlTelInputUtils) {
                            return intlTelInputUtils.formatNumber(this._getFullNumber(), this.selectedCountryData.iso2, format);
                        }
                        return "";
                    }
                });

            $('.iovox-call-elem')
                .formValidation({
                    framework: 'bootstrap',
                    icon: {
                        valid: 'glyphicon glyphicon-ok',
                        invalid: 'glyphicon glyphicon-remove',
                        validating: 'glyphicon glyphicon-refresh'
                    },
                    fields: {
                        phoneNumber: {
                            validators: {
                                callback: {
                                    message: 'Le numéro de téléphone est invalide',
                                    callback: function (value, validator, $field) {
                                        $field.intlTelInput('isValidNumber') ? $("#iovox-validate-button").attr("disabled", false).removeClass("btn-info").addClass("btn-success") : $("#iovox-validate-button").attr("disabled", true).removeClass("btn-success").addClass("btn-info");
                                        return value === '' || $field.intlTelInput('isValidNumber');
                                    }
                                }
                            }
                        }
                    }
                })
                .on('click', '.country-list', function () {
                    $('.iovox-call-elem').formValidation('revalidateField', 'phoneNumber');
                });

            $('#iovox-validate-button').on('click', function () {
                var number = $('.iovox-num').val().replace(/\D/g, '');

                if ($('.intl-tel-input').find('i').hasClass('glyphicon-ok') && number.length > 1 && !isNaN(number)) {
                    var dialCode = $('.selected-dial-code').html();

                    // Remove leading '0'
                    if (['+33', '+49', '+32'].indexOf(dialCode) > -1 && number.charAt(0) === '0') {
                        number = number.substring(1);
                    }

                    var prefixWithoutPlusSign = dialCode.substring(1, dialCode.length);
                    number = prefixWithoutPlusSign + number;

                    window.initiateCall(number, iovoxIdAgency, element);
                }
            });
        };

        window.initiateCall = function (phone, id, e) {
            $(".popover-title").html("Appel en cours...");
            $(".iovox-call-elem").html('<span class="glyphicon glyphicon-phone-alt vibrate" aria-hidden="true"></span><br />Préparez-vous, nous vous appelons. <br />Vous allez être mis en relation avec l\'agence.</b><br />')
                .addClass("text-center")
                .css("font-size", "10px");
            $.ajax({
                method: "POST",
                url: "https://www.wortimmo.lu/includes/iovox-back.php",
                data: {
                    initCall: true,
                    phone: phone,
                    link_id: id
                },
                success: function (success) {
                    if (success === "error") {
                        $(".popover-title").html("Erreur");
                        $(".iovox-call-elem").html('<span class="glyphicon glyphicon-remove" aria-hidden="true"></span><br />une-erreur-est-survenue-veuillez-re-essayer-ulterieurement<br />').addClass("text-center").css("font-size", "12px").css("color", "red");
                    }

                    setTimeout(function () {
                        $("#" + $(".popover").attr("id")).hide();
                    }, 10000);
                },
                error: function (error) {
                    $(".popover-title").html("Erreur");
                    $(".iovox-call-elem").html(
                        '<span class="glyphicon glyphicon-remove" aria-hidden="true"></span>\
                        <br />Une erreur est survenue, veuillez re-essayer ultérieurement<br />').addClass("text-center").css("font-size", "12px").css("color", "red");
                    setTimeout(function () {
                        $("#" + $(".popover").attr("id")).hide();
                    }, 10000);
                }
            });
        };

    });

</script>
</body>
