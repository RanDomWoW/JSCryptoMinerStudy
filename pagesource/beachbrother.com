<script>
$(document).ready(function(e){ 
    widthTotalLi = 1056 //50px en plus de la largeur centrale car -25 pour li-middle et -25 pour last-li qui ne devrait pas être enlevé (en fait 48 poue gérer les arrondis de chaque li)
    $('.navbar-tcms .menu li').each(function(){
        widthTotalLi -= ($(this).width()+25)
    })
    $('.li-middle').css({width: widthTotalLi+'px'})
})
    
var isOverSubmenu = false; 
var marginLeftSvg = 0    
var overMenu = function(obj,ind)
{
    if(isOverSubmenu)return
    marginLeftSvg = parseFloat(obj.offsetLeft)+($('#menu-'+ind+' a').width()/2-10)
    $('.svgShowUp-'+ind).css({'display': 'block', 'marginLeft': marginLeftSvg+'px'});
    $('.submenu-'+ind).css({'display': 'block'}); 
    if(ind<5)
        $('.submenu-'+ind+'>div').css({'marginLeft': $('.menu li:first-child')[0].offsetLeft+'px'});
    else 
        $('.submenu-'+ind+'>div').css({'marginLeft': ($('.menu li:last-child')[0].offsetLeft+$('.menu li:last-child a').width()-420)+'px'});
    isOverSubmenu = true;
}

var outSubmenu = function(obj,ind)
{
    $(document).mouseover(function(e){
        var x = e.pageX;
        var y = e.pageY;
        var scroll = $(document).scrollTop();
        y = (y- parseFloat(scroll));
        if( (y<80 && (x<obj.offsetLeft || x>(obj.offsetLeft+obj.offsetWidth)) ) || y>245 )
        {
            isOverSubmenu = false
            $('.svgShowUp-'+ind).css({'display': 'none'});
            $('.submenu-'+ind).css({'display': 'none'});
        }
    });    
}

