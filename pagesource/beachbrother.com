<script>
    var pr24h00_prehomeAutoclose = true;
    var pr24h00_prehomeTimeout = 0;
    var pr24h00_cookie = pr24h00_ReadCookieJS("pr24h00_prehome");

    // Récuperation de la prehome en asynchrone
    if(pr24h00_cookie == '' || pr24h00_cookie == null)
    {
        // Récuperation de la prehome en asynchrone
        $.ajax({
            url: "/ajax/getPrehome.php?zoneId=1388",
            type: 'GET',
            dataType: 'text',
            crossDomain: true,
            success: function(data){

                var dataPH = JSON.parse(data);

                if(dataPH.logUrl)
                {
                    var divPH = '<div class="pr24h00_prehome" id="pr24h00_prehomeContainer">';
                    divPH += '<center><div class="pr24h00_prehome-close" onclick="javascript:pr24h00_closePreHome();">Acc&eacute;der directement au site <b>&raquo;</b></div></center>';
                    divPH += '<a class="pr24h00_prehome-content" href="' + dataPH.clickUrl + '" onclick="javascript:pr24h00_closePreHome();" target="_blank"></a>';
                    divPH += '</div><img src="' + dataPH.logUrl + '" width="0" height="0">';
                    $("#osr-prehome").html(divPH);
                    $("#pr24h00_prehomeContainer").css("background", "url(" + dataPH.bannerContent + ") no-repeat center #000");

                    pr24h00_SetCookieJS("pr24h00_prehome", "1", 3600000);
                    pr24h00_startPrehome();
                }
            },
            timeout: 10000
        });
    }

    function pr24h00_closePreHome()
    {
        pr24h00_prehomeAutoclose = true;
        $("#pr24h00_prehomeContainer").hide();
        document.getElementById("pr24h00_prehomeContainer").remove();
    }
    function pr24h00_SetCookieJS(cookieName, cookieValue, expireTime) {
        var today = new Date();
        var expire = new Date();
        if (expireTime==null) expireTime=43200000;
        expire.setTime(today.getTime() + expireTime);
        document.cookie = cookieName + "=" + escape(cookieValue) + ";expires=" + expire.toGMTString() + ";path=/";
    }
    function pr24h00_ReadCookieJS(cookieName) {
        var theCookie=" "+document.cookie;
        var ind=theCookie.indexOf(" "+cookieName+"=");
        if (ind==-1) ind=theCookie.indexOf(";"+cookieName+"=");
        if (ind==-1 || cookieName=="") return "";
        var ind1=theCookie.indexOf(";",ind+1);
        if (ind1==-1) ind1=theCookie.length; 
        return unescape(theCookie.substring(ind+cookieName.length+2,ind1));
    }
    function pr24h00_startPrehome()
    {
        pr24h00_prehomeTimeout += 1;
        if(pr24h00_prehomeTimeout > 20)
        {
            if(pr24h00_prehomeAutoclose == true)
            {
                pr24h00_closePreHome();
            }
            else
            {
                setTimeout("pr24h00_startPrehome()", 1000);
            }
        }
        else
        {
            setTimeout("pr24h00_startPrehome()", 1000);
        }
    }
</script>
