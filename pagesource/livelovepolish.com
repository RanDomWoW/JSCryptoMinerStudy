          type: 'POST',
          dataType: 'jsonp',
          success: function(location) {
            switch(location.country_code) {
              case 'US':
                Cookies.set('customer_location', 'domestic');
                break;
              case 'AU':
              case 'BR':
              case 'CA':
              case 'IL':
              case 'JP':
              case 'NZ':
              case 'ES':
                Cookies.set('customer_location', 'international');
                break;
              default:
                Cookies.set('customer_location', 'domestic');
            }
          }
        } );
      }

      /* ******************************************* */
      /* [YOTPO Star Rating Condensed Format] */

      var reviewsLoaded;
      var lookupCount = 0;

      function replaceReview() {
        var $selector = $('body .product-meta .star-clickable a');
        var selectorFound = false;
        lookupCount++;
        $selector.each(function() {
          $(this).css('visibility', 'hidden');
          var text = $(this).text();
          if (text) selectorFound = true;
          var newText = text.replace(/[\sa-zA-Z]+/, '').replace(/(\d+)/, '($1)');
          $(this).text(newText);
        });
        if (selectorFound || lookupCount > 20) {
          $('body .product-meta .star-clickable a').css('visibility', 'visible');
          clearInterval(reviewsLoaded);
        }
      }

      $('body .product-meta .star-clickable a').ready(function() {
        reviewsLoaded = setInterval(replaceReview, 500);
