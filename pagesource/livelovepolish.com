  <script>
    LLP = {
      liveStoreTest: "livelovepolish",
      testStoreTest: "livelovetest",
      llptools_production_url: "",
      llptools_staging_url: "",
      template: "index",
      //grab current browser width & height to help with responsive JS
      Browser: {
        width: function() {
          return window.innerWidth || document.body.clientWidth
        },
        height: function() { return window.innerHeight || document.body.clientHeight },
        type: (function() {
          if (typeof InstallTrigger !== 'undefined') {
            return 'Firefox';
          }
          else if (/*@cc_on!@*/false || document.documentMode) {
            return 'Internet Explorer';
          }
        })()
      },
      /* Helpers */
      Helper: {
        validateEmail: function(email) {
          var re = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
          return re.test(email);
        },
        getStoreType: function(host) {
          var host = host ? host : window.location.host;
          if (host.indexOf(LLP.liveStoreTest) > -1) {
            return 'live';
          }
          else if (host.indexOf(LLP.testStoreTest) > -1) {
            return 'test';
          }
          else {
            return '';
          }
        },
        getToolsURL: function() {
          var storeType = LLP.Helper.getStoreType();
          if (storeType === 'live') {
            return LLP.llptools_production_url;
          }
          else if (storeType === 'test') {
            return LLP.llptools_staging_url;
          }
          else {
            return '';
          }
        },
        getUrlParameter: function(sParam) {
          var sPageURL = decodeURIComponent(window.location.search.substring(1)),
              sURLVariables = sPageURL.split('&'),
              sParameterName,
              i;

          for (i = 0; i < sURLVariables.length; i++) {
            sParameterName = sURLVariables[i].split('=');

            if (sParameterName[0] === sParam) {
              return sParameterName[1] === undefined ? true : sParameterName[1];
            }
          }
        },
        getGiftCardsTotal: function(cartItems) {
	        // Filter to find any Gift Cards in the Array.
          var giftCards = cartItems.filter(function(i) {
            return i.gift_card === true;
          });
          // Get the sum of the line_price of all the Gift cards in the cart.
          var giftCardsTotal = giftCards.reduce(function (sum, obj) {
            return sum + obj.line_price;
          }, 0);
          // Subtract the sum of Gift cards from the total price.
          return giftCardsTotal;
        }
      }
    };

    

    LLP.product = {};
    
    LLP.product.id = null;
    

    LLP.cart = {};
    LLP.cart.total_price = 0;
    LLP.cart.total_sans_gift = LLP.cart.total_price - LLP.Helper.getGiftCardsTotal([]);

  </script>

