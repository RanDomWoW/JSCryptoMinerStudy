			<script>

			var cur_idx=1;
			var scroll_num = 5;

			function storyScroll()
			{
				if(scroll_num == 0) return false;
				else if(scroll_num == 1)
				{
					var f = document.getElementById("story_scroll"+cur_idx);

					f.style.display = "block";
					return false;
				}

				if(cur_idx > 1)
				{
					var prev_f = document.getElementById("story_scroll"+(cur_idx-1));
					prev_f.style.display = "none";
				}
				if(cur_idx == 1)
				{
					var prev_f = document.getElementById("story_scroll"+scroll_num);
					prev_f.style.display = "none";
				}

				var f = document.getElementById("story_scroll"+cur_idx);

				f.style.display = "block";

				cur_idx = cur_idx == scroll_num ? 1 : cur_idx+1;
			  window.setTimeout("storyScroll();", 3000);
			}
			storyScroll();
			</script>


