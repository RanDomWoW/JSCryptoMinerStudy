    var margLeft = parseInt($(".flexslider .flex-control-nav li").css("marginLeft"));
    var itemWdt = wdt+margLeft;
    var resWidth = 9*wdt+9*margLeft;
    $(".flex-control-nav-stream").css("width",resWidth+"px");
    var streamWidth = resWidth;
    var streamMarginLeft = 0;
    var indx = 0;
    var sliderControlWidth = parseInt($(".flexslider-controls").css("width"));
    $(".flex-direction-nav .flex-next").click(function (){
        streamMarginLeft = parseInt($(".flex-control-nav-stream").css("margin-left"));
        if(streamMarginLeft+streamWidth>sliderControlWidth){
            $(".flex-control-nav-stream").css("margin-left",streamMarginLeft-(wdt+margLeft)+"px");
        }
        else if($(".flexslider .flex-control-nav li:first-child").attr('class') =="flex-active"){
            $(".flex-control-nav-stream").css("margin-left",0+"px");
        }
		$(".slide-title").html($(".flex-control-nav-stream .flex-active .control-title").html());
		$(".slide-byline").html($(".flex-control-nav-stream .flex-active .control-summary").html());
