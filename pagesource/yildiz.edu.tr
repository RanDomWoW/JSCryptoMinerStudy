		if($(this.children[4]).html() == "0"){
			$(".slide-text").css('display','none');
		}
		else{
			$(".slide-text").css('display','block');
		}
	});
	
    $(".flex-direction-nav .flex-prev").click(function (){
        streamMarginLeft = parseInt($(".flex-control-nav-stream").css("margin-left"));
        indx = parseInt($(".flex-active" ).index( ".flex-control-nav-stream li" ));
        if(indx>2) {
            $(".flex-control-nav-stream").css("margin-left", -(indx-2) * itemWdt + "px");
        }
        else
