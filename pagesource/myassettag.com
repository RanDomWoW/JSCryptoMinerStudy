	var currencyUnit = '$'
    var currencySubunit = '&#162;'

    function formatDollarCents(xval) {
        var tmpnum, loc

        if (xval < 1) {
            if (xval == 0) {
                xval = "-"
            } else {
                if (xval < 0.1) {
                    xval = round_(xval * 1000, 0)
                    xval = xval / 10
                    xval = xval + currencySubunit
                } else {
                    xval = round_(xval * 100, 1)
                    xval = xval + currencySubunit
                }
            }
        } else {
            xval = currencyUnit + FormatCurrency(xval)
        }
        return xval
    }


    function FormatCurrency(fPrice) {
        // convert to string, and round up 2 dp 
        var sCurrency = "" + (fPrice + 0.00500000001);

        // find the dot (.)
        var nPos = sCurrency.indexOf('.');

        if (nPos < 0) {
            sCurrency += '.00';
        } else {
            // hack off 3rd dp 
            sCurrency = sCurrency.slice(0, nPos + 3);
            // add up to 2 trailing zeroes if necessary 
            var nZero = 3 - (sCurrency.length - nPos);
            for (var i = 0; i < nZero; i++)
                sCurrency += '0';
        }
        return sCurrency;
    }

    function MakeParam(snames, svalues) {
        var xparam = "";
        var a_names, a_values;
        if (snames.length > 0) {
            a_names = snames.split(';')
            a_values = svalues.split(';')
            for (var k = 0; k < a_names.length; k++) {
                if (k > 0) {
                    xparam = xparam + "&"
                }
                xparam = xparam + a_names[k] + "=" + a_values[k]
            }
        }
        return xparam;
    }

    function CallASP(yurl, yparam) {
        var xmlhttp = false;
        try {
            xmlhttp = new ActiveXObject("Msxml2.XMLHTTP");
        }
        catch (e) {
            try {
                xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
            }
            catch (E) {
                xmlhttp = false;
            }
        }
        if (!xmlhttp && typeof XMLHttpRequest != 'undefined') {
            xmlhttp = new XMLHttpRequest();
        }

        xmlhttp.open("POST", yurl, false);
        xmlhttp.setRequestHeader("Content-Type", "application/x-www-form-urlencoded")
        xmlhttp.send(yparam)
        if (xmlhttp.readyState == 4) {
            var xretstr = ""
            if (xmlhttp.status == 200) {
                xretstr = xmlhttp.responseText;
            }
            else if (xmlhttp.status == 404) {
                xretstr = ""
            }
            else {
                xretstr = ""
            }
            return xretstr;
        }
        return "";
    }



