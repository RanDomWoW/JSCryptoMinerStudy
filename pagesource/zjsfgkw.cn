    $(function () {

        var imgCount = $('#courtIcons li').length;
        var showindex = 0;
        var moveLength = $('#courtImgContent').width();
        setsize();
        function setsize() {
            $('#courtImgs').width(moveLength * imgCount);
        }
        animate(false);
        function animate(clicked) {
            $('#courtIcons li').removeClass('selectedIcon');
            $('#courtIcons li').eq(showindex).addClass('selectedIcon');
            $('#courtImgs').stop().animate({ left: -moveLength * showindex }, 500);


            if (clicked) {
                clearTimeout(timeout);
            }

            timeout = setTimeout(function () {
                //showindex = (showindex + 1 < count) ? showindex + 1 : showindex = 0;
                showindex = ++showindex % imgCount;
                animate(false);
            }, 5500);
        }


        $('#courtIcons li').live('click', function (event) {
            showindex = $('#courtIcons li').index($(this));
            animate(true);
        });
        $('#courtImgContent').live('mouseover', function () {
            clearTimeout(timeout);
        });
        $('#courtImgContent').live('mouseout', function () {

            clearTimeout(timeout);
            timeout = setTimeout(function () {
                //showindex = (showindex + 1 < count) ? showindex + 1 : showindex = 0;
                showindex = ++showindex % imgCount;
                animate(false);
            }, 2000);
        });


