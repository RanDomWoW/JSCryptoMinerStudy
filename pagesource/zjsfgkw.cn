<script type="text/javascript">

    //添加综合公告和开庭公告的Click
    $(function () {
     
        //导航
        $(".menuDrop_element_box,.left_menu li").click(function () {

            var href = $(this).attr("href");
            if (href) {
                if (href.search(/^http:\/\//g) > -1) {
                    window.open(href);
                    return;
                };
                if ($(this).attr("target") == '_blank') {
                    window.open(href);
                } else {

                    location.href = href;
                }
            }




        });

        // 全站搜索
        function placeholderIsSupported() {
            var test = document.createElement('input');
            return ('placeholder' in test);
        }

        var $globalSearchInput = $('#globalSearchInput');
        var placeholder = $globalSearchInput.attr('placeholder');

        if (!placeholderIsSupported) {
            $globalSearchInput.val();

            $globalSearchInput.focus(function () {
                if ($globalSearchInput.val() === placeholder) {
                    $globalSearchInput.val('');
                }
            });

            $globalSearchInput.blur(function () {
                if ($globalSearchInput.val() === '') {
                    $globalSearchInput.val(placeholder);
                }
            });
        }

        $globalSearchInput.keydown(function (e) {
            var key = $globalSearchInput.val();
            if (e.keyCode === 13 && key.length && key !== placeholder) {
                e.preventDefault();

                location.href = '/search/index?q=' + encodeURIComponent(key);
            }
        });

        $("#globalSearchBtn").click(function () {
            var key = $globalSearchInput.val();
            location.href = '/search/index?q=' + encodeURIComponent(key);
        });

    });

   

  

</script>