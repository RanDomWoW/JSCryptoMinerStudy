<script language="javascript">
    var lastScrollP = 0;
    function heartBeat_platform() {
        var diffY;
        if (document.documentElement && document.documentElement.scrollTop)
            diffY = document.documentElement.scrollTop;
        else if (document.body)
            diffY = document.body.scrollTop
        else
        {/*Netscape stuff*/ }


        var clinetHeight = document.documentElement.clientHeight;
        diffY = clinetHeight - 615 + diffY;

        percent = .1 * (diffY - lastScrollP);
        if (percent > 0) percent = Math.ceil(percent);
        else percent = Math.floor(percent);
        if (document.getElementById("vdl_platform")) { document.getElementById("vdl_platform").style.top = parseInt(document.getElementById("vdl_platform").style.top) + percent + "px"; }
        if (document.getElementById("me_baywindow")) { document.getElementById("me_baywindow").style.top = parseInt(document.getElementById("me_baywindow").style.top) + percent + "px"; }
        lastScrollP = lastScrollP + percent;
    }

    lastScrollY = 0;
    function heartBeat_left() {
        var diffY;
        if (document.documentElement && document.documentElement.scrollTop)
            diffY = document.documentElement.scrollTop;
        else if (document.body)
            diffY = document.body.scrollTop
        else
        {/*Netscape stuff*/ }


        var clinetHeight = document.documentElement.clientHeight;
        diffY = clinetHeight - 300 + diffY;

        percent = .1 * (diffY - lastScrollY);
        if (percent > 0) percent = Math.ceil(percent);
        else percent = Math.floor(percent);
        if (document.getElementById("vdl_left")) { document.getElementById("vdl_left").style.top = parseInt(document.getElementById("vdl_left").style.top) + percent + "px"; }
        lastScrollY = lastScrollY + percent;
    }
    lastScrollYR = 0;
    function heartBeat_right() {
        var diffY;
        if (document.documentElement && document.documentElement.scrollTop)
            diffY = document.documentElement.scrollTop;
        else if (document.body)
            diffY = document.body.scrollTop
        else

        {/*Netscape stuff*/ }

        var clinetHeight = document.documentElement.clientHeight;
        diffY = clinetHeight - 560 + diffY;
        percent = .1 * (diffY - lastScrollYR);
        if (percent > 0) percent = Math.ceil(percent);
        else percent = Math.floor(percent);
        if (document.getElementById("vdl_right")) { document.getElementById("vdl_right").style.top = parseInt(document.getElementById("vdl_right").style.top) + percent + "px"; }
        lastScrollYR = lastScrollYR + percent;
