    <script type="text/javascript">
        $.ajaxSetup({ contentType: "application/x-www-form-urlencoded; charset=utf-8" });
        $(document).ready(function () {
            $.ajaxSetup({ contentType: "application/x-www-form-urlencoded; charset=utf-8" });

            $("#courtMenu ul li").mouseenter(function () {
                $(this).find(".menuDrop_container").show();
                if ($('.menuDrop_container', this).length > 0) {
                    $(this).addClass('haveSubMenu');
                }
            });
            $("#courtMenu ul li").mouseleave(function () {
                $(this).find(".menuDrop_container").hide();
                $(this).removeClass('haveSubMenu');
            });
        });
        $(function () {
            PvPost();
            var path = window.location.pathname;
            if (path.split('/').length >= 2) {
                var controllname = path.split('/')[1].toLowerCase();
                //$("#courtMenu")
                $("#courtMenu a").removeClass("selectMenu");
                if (controllname == "") {
                    $("#courtMenu a:eq(0)").addClass("selectMenu");
                }
                else {
                    $.each($("#courtMenu ul li"), function (index, item) {
                        //if ($("a:eq(0)", $(item)).attr("href").toLowerCase().indexOf(controllname, 0) > 0) {
                        //    $("a:eq(0)", $(item)).addClass("selectMenu");
                        //}
                        if ($(item).attr('control').indexOf(controllname, 0) >= 0) {
                            $("a:eq(0)", $(item)).addClass("selectMenu");
                        }
                        
                    });
                }

                //console.log(controllname);
            }
            //console.log(path);
            //console.log(path.split('/').length);

        })
        function PvPost() {
            function getNumItem(number) {
                var number = number + '';
                return number = number.split('');
            }
            $.ajax({
                type: 'POST',
                async: true,
                url: '/Home/GetPvCount',
                success: function (data) {
                    var html = '<span class="pv_title">网站访问量:</span>';
                    var numArr = getNumItem(data.pvCount);
                    for (var i = 0; i < numArr.length; i++) {
                        html += '<span class="pv_item">' + numArr[i] + '</span>';
                    }
                    $("#pv").html(html);
                }
            });
        }
    </script>
