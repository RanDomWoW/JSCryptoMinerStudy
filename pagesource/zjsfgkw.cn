    function autoChangePic() {
        if (clickFlag == 0) {
            index++;
            if (index > 3) {
                index = 1;
            }

            var len = $("#ulNum li").length;
            for (var i = 0; i < len; i++) {
                $("#ulNum li:eq(" + i + ")").removeClass("current");
            }

            $("#container_" + index).addClass("current");

            var len1 = $("#divPics div").length;
            for (var i = 0; i < len1; i++) {
                $("#divPics div:eq(" + i + ")")[0].style.display = "none";
            }
            //var divid = $(this).attr("id").replace("_" + $(this).html(), "");
            $("#container" + index)[0].style.display = "";
            switch (index) {
                case 1:
                    zhuzhuangtu();
                    break;
                case 2:
                    bingtu();
                    break;
                case 3:
                    zhexiantu();
                    break;
            }
        } else {
            clearInterval(time);
            clickFlag = 0;
            time = setInterval("autoChangePic()", 15000);
        }
    }

    function clickChangePic() {
        $('#ulNum li').click(function () {
            clickFlag = 1;
            //切换圆点
            var len = $("#ulNum li").length;
            for (var i = 0; i < len; i++) {
                $("#ulNum li:eq(" + i + ")").removeClass("current");
            }
            $(this).addClass("current");
            //切换数据图
            var len1 = $("#divPics div").length;
            for (var i = 0; i < len1; i++) {
                $("#divPics div:eq(" + i + ")")[0].style.display = "none";
            }

            //更换数据图ID
            var divid = $(this).attr("id").replace("_", "");
            $("#" + divid)[0].style.display = "";
            if (divid == "container1") {
                zhuzhuangtu();
                index = 1;

            }
            else if (divid == "container2") {
                bingtu();
                index = 2;

            }
            else {
                zhexiantu();
                index = 3;

            }
        });
    }
