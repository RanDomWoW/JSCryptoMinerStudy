<!-- script per visualizzare news nella fascia sup e inf -->
<script>
 
  function sizing() {
  var windowwidth=$(window).width();
  var windowheight=$(window).height();
  
  if ( windowwidth > 767) {
    sliderNews();
  } else {
    sliderNewsDestroy();
  }
  
}

function sliderProgetti() {
  $('#news_progetti ul').bxSlider({
    adaptiveHeight:    true,
    infiniteLoop:      true,
    hideControlOnEnd:  true,
    pager:            false,
    auto:              true,
    autoHover:        true,
  });
}

function sliderNews() {
  $('#news_ticker ul').bxSlider({
    infiniteLoop:      false,
    hideControlOnEnd:  true,
    pager:            false,
    auto:              false,
    autoHover:        true,
    minSlides:        1,
    maxSlides:        4,
    slideWidth:        200  
  });
}
  
function sliderNewsDestroy() {
  $('#news_ticker ul').bxSlider().destroySlider();
}
  
function sliderProgettiDestroy() {
  $('#news_progetti ul').bxSlider().destroySlider();
}
  
$(document).ready(sizing);
$(window).resize(sizing);
  
$(document).ready(function(){
  sliderProgetti();
});
  
</script>

