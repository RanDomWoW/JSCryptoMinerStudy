            dataType: "json",
            success: function (result, status, request) {
                if (result.success === true) {
                    $("#" + id).html(result.html);
                    communitySpinner.css("display", "none");
                    $('.user').tooltip({'placement': 'bottom'});
                }
            }
        });
    }
});
