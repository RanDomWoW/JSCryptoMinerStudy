var curr='empty';
function fr(idis, ndx){
         //var idis=idis, ndx=ndx;
         if(ndx=='x'){
             document.getElementById('lcontactus').style.visibility="hidden";
             document.getElementById('pulayer').style.visibility="hidden";
             curr='empty';
             return false;
         }
         if(curr==ndx){
             document.getElementById('lcontactus').style.visibility="hidden";
             document.getElementById('pulayer').style.visibility="hidden";
             curr='empty';
             return false;
         }
         else if(curr=='0' && curr==ndx){
             document.getElementById('lcontactus').style.visibility="hidden";
             document.getElementById('pulayer').style.visibility="hidden";
             curr='empty1';
             return false;
         }
         else if(ndx=='0'){
            curr=ndx;
            document.getElementById('pulayer').style.visibility="hidden";
            document.getElementById(idis).style.visibility="visible";
         }
         else{
            curr=ndx;
            document.getElementById('lcontactus').style.visibility="hidden";
            document.getElementById(idis).style.visibility="visible";
         }
         return false;
}
