function localtime(){
  //return false;
  var date =new Date();
  h = date.getHours();
  m = (date.getMinutes()<10)? "0"+date.getMinutes(): date.getMinutes();
  s = (date.getSeconds()<10)? "0"+date.getSeconds(): date.getSeconds();;
  document.getElementById("ltime").innerHTML = h+':'+m+':'+s;
  setTimeout('localtime()',1000);
}
