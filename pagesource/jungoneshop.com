				};
			}else{
				var lc = new layerConfirm('찜하기 실패', result.message, '찜하기 바로가기', '계속 쇼핑하기');
				
				lc.confirmAction = function(){
					$("#commonPopupLayout").html("");
					$.magnificPopup.close();
					window.location.href="/my-account/wishlist";
				};
			}
			
			/* 
			if (result.code == "success") {
				var lc = new layerConfirm('찜하기 등록', '해당상품이 찜하기 목록에 추가되었습니다.', '찜하기 바로가기', '계속 쇼핑하기');
				
				lc.confirmAction = function(){
					$("#commonPopupLayout").html("");
					$.magnificPopup.close();
					window.location.href="/my-account/wishlist";
				};
			}else{
				alert(result.message);
			}
			 */
		},
		error: function( request, status, error ){
			console.log("code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error);
			return false;			
		}
	});
}

