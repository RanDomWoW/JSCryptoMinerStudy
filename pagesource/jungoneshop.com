	$(document).ready(function() {
		$("#j_username").bind("keydown", function(event) {
			if (event.keyCode == 13) {
				//$("#j_password").focus();
				$("input[name=j_password]").focus();
				return false;
			}
		});

		$("input[name=j_password]").bind("keydown", function(event) {
			if (event.keyCode == 13) {
				goLogin();
				return false;
			}
		});
					
		$("#j_username").bind("keyup",function(){
			 var re = /[ㄱ-ㅎ|ㅏ-ㅣ|가-힣~!@#$%^&*\()\=+']/gi;
			 var temp = $("#j_username").val();
			 if(re.test(temp)){
			 	$("#j_username").val(temp.replace(re,"")); 
			 } 
		});

		var cookieUserId = $.cookie("cookieUserId");

		if (cookieUserId != null && cookieUserId != "") {
			$("#saveId").prop("checked", true);
			$("#saveId").iCheck('update');
			$("#j_username").val(cookieUserId);
			$("input[name=j_password]").focus();
		} else {
			$("#j_username").focus();
		}
	});

	function goLogin() {
		var userid = $.trim($("#j_username").val());
		var userpw = $.trim($("input[name=j_password]").val());

		if (userid == "") {
			alert("아이디를 입력해주세요.");
			$("#j_username").focus();
			return;
		}

		if (userpw == "") {
			alert("비밀번호를 입력해주세요.");
			$("input[name=j_password]").focus();
			return;
		}

		$('#op_id').val(userid);

		var hashPwd = CryptoJS.SHA256(userpw); // SHA-256 암호화
		$('#op_passwd').val(hashPwd);
		//$('#j_password').val("");

		if ($('#saveId').prop("checked")) {
			$.cookie('cookieUserId', userid, {
				expires : 30
			});
		} else {
			$.cookie('cookieUserId', '', {
				expires : 30
			});
		}
		
		
/* 		 */

		
		$("#loginLayeriframe").attr("src","/login/loginHistory?uid="+userid);
		$("#ssofrm").submit();return;
			
	}

