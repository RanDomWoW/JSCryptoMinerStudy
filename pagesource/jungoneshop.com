	//teleshopping 저장
	$(document).on("click", "#teleshoppingPopupSave", function(event){ 	
		event.preventDefault();
		saveTeleshopping(event);
		return false;
	});	
	
	//teleshopping 저장
	function saveTeleshopping(event) {
		
		if( $("input[name='hpNo2']").val() == "" || $("input[name='hpNo2']").val().length < 3 ) {
			alert("전화번호 입력 해주세요.");
			$("input[name='hpNo2']").focus();
			return false;
		}
		
		if( $("input[name='hpNo3']").val() == "" || $("input[name='hpNo3']").val().length < 4 ) {
			alert("전화번호 입력 해주세요.");
			$("input[name='hpNo3']").focus();
			return false;
		}
	
		$.ajax({
			type: "post",
			async: false,
			url : "/guest/saveTeleshoppingPopup",
			data : $("#teleshoppingForm").serialize(),
			dataType : "json",
			error : function( request, status, error ){		
				//console.log( "code:" + request.status+"\n" + "message:" + request.responseText+"\n" + "error:" + error );
				alert("전화주문 예약을 실패하였습니다.");
			},
			success : function(data) 
			{
				var returnData = eval( '(' + data + ')' );
				var returnName = returnData.name;
				var returnHpNo = returnData.hpNo;
				
				$(document).find("#checkName").text(returnName);		
				$(document).find("#checkHpNo").text(returnHpNo);
				
				if($.parseJSON(data).success == "duplicated"){
					alert("고객님의 금일 전화주문 예약이 이미 완료 되었습니다.\n전화주문 예약은 1일 1회만 가능합니다.");
					$.magnificPopup.close();
				}else if($.parseJSON(data).success == "NOK"){
					alert("전화주문 예약을 실패하였습니다.");
					$.magnificPopup.close();
				}else{
					layerpopup('#teleshoppingCheckPopup','inline',false);
				}
				
				$("input[name='hpNo2']").val("");
				$("input[name='hpNo3']").val("");
			}
		});
	}




