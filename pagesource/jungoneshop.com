function kimchishopBuyNowAll(multiDeliveryYn) {
    var arrProductCode = new Array();
    var arrQuantity = new Array();
    var hopeDeliveryY = false;
    var hopeDeliveryN = false;
    var hopeMissMatch = false;
    var multiDeliveryYn = isNullCheck(multiDeliveryYn) ? false : multiDeliveryYn;
    var deliveryStartDate = "";
    var deliveryEendDate = "";

    $("input:checkbox[name='cartlist_01']").each(function(index) {
        if($(this).is(":checked")) {
            var currentObj = $("#addtocart-quantity-" + $(this).val());
            arrProductCode.push($(this).val());
            arrQuantity.push(currentObj.val());
            // 먼저 일반배송, 예약배송은 같이 주문 불가
            if( currentObj.data("hopedelivery") == true ) {
                hopeDeliveryY = true;
                // 희망배송을 묶음 배송하려면 희망 배송일이 같아야만 가능한다.
                if( currentObj.data("deliveryhopestartdate") ) {
                    if( !deliveryStartDate ){
                        deliveryStartDate = currentObj.data("deliveryhopestartdate");
                    }else{
                        if( deliveryStartDate != currentObj.data("deliveryhopestartdate")){
                            hopeMissMatch = true;
                        }else{
                            deliveryStartDate = currentObj.data("deliveryhopestartdate");
                        }
                    }
                }
                if( currentObj.data("deliveryhopeenddate") ) {

                    if( !deliveryEendDate ){
                        deliveryEendDate = currentObj.data("deliveryhopeenddate");
                    }else{
                        if( deliveryEendDate != currentObj.data("deliveryhopeenddate")){
                            hopeMissMatch = true;
                        }else{
                            deliveryEendDate = currentObj.data("deliveryhopeenddate");
                        }
                    }
                }
            }else{
                hopeDeliveryN = true;
            }
        }
    });

    if(arrProductCode.length == 0) {
        alert("상품을 선택하세요");
        return;
    }
    if(hopeDeliveryY && hopeDeliveryN) {
        alert("예약배송상품과 일반배송상품은\n함께 주문하실 수 없습니다.");
        return;
    }else{
        // 예약배송의 날짜가 다른 상품일경우 예외
        if( hopeDeliveryY ){
            if( hopeMissMatch ){
                alert("예약배송상품은 예약배송 가능일이\n같아야 주문 가능합니다.");
                return;
            }
        }
    }


    var sendData = {
        productCode: arrProductCode.join(','),
        quantity: arrQuantity.join(','),
        multiDeliveryYn: multiDeliveryYn,
        buynowYn: true
    };

    $.ajax({
        url: "/cart/add",
        type: 'get',
        dataType: 'json',
        data: sendData,
        error : function( request, status, error ){
        },
        success : function( data ){
            if(data.replaceYn == "Y"){
                var t = new layerAlert("바로구매", "해당 상품이 품절되었습니다.");
                t.confirmAction = function(){
                    window.location.href="/p/"+code;
                };
                return;
            }else{
                if(data.responseMsg == "") {
                    location.replace(data.responseUrl);
                } else {
                    layerAlert("바로구매", data.responseMsg.replace(/\\n/gi, "<br/>"));
                }
            }
        }
    });
}

