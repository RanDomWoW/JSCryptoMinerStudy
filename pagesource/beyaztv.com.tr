<script>
$(function() {
	//slider 
	var pCount = allPrograms.length-1;
	var randomNum=0;
	if(randomNum==0){
	randomNum = Math.ceil(Math.random()*pCount);//random number
	$('.first').attr('durl', allPrograms[randomNum].split('|')[1])
	$('.first').attr('src', allPrograms[randomNum].split('|')[0]).stop(true, true).fadeOut(10).fadeIn(300); 
	} 

	var pnext = parseInt(randomNum)+1;
	var pprev = parseInt(pnext)-2;
	if(pnext>pCount) { pnext=0; }
	if(pprev<-1 || pprev==-1) { pprev=pCount; }

//auto slide function	
function fb() {
timeS = window.setInterval(function(){
  $('#p_next').click();
}, 10000);	
}	
	
//click next 
	$('#p_next').click(function(){
	  //alert(pnext);
	  $('.first').attr('durl', allPrograms[pnext].split('|')[1])
	  $('.first').attr('src', allPrograms[pnext].split('|')[0]).stop(true, true).fadeOut(10).fadeIn(300); 		  
	  pnext++;
	  pprev=pnext-2;
	  if(pnext>pCount) { pnext=0; }
	  if(pprev<-1 || pprev==-1) { pprev=pCount; }
	  clearTimeout(timeS);
	  fb();
	 });
	 
	 //click prev
	$('#p_back').click(function(){
	  //alert(pprev);
	  $('.first').attr('durl', allPrograms[pprev].split('|')[1])
      $('.first').attr('src', allPrograms[pprev].split('|')[0]).stop(true, true).fadeOut(10).fadeIn(300);	
	  pprev--;
	  pnext = pprev + 2
	  if(pprev<-1 || pprev==-1) { pprev=pCount; }	  
	  if(pnext>pCount) { pnext=0; }
	  clearTimeout(timeS);
	  fb();
	 });
	
    //auto slide	
	fb();
	 
	 $('.first').mousemove(
	 function() { $('#pdetail').attr('href',$(this).attr('durl')); $('#pdetaildiv').fadeIn(500, function() { $('#pdetaildiv').fadeOut(3000); } ); }
	 );
	
});

</script>


