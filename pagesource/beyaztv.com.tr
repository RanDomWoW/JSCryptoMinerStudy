<script type="text/javascript">
$(function() {

$("form").submit(function () {
    $(":submit", this).attr("disabled", "disabled");
});  

	//slider 
    
	var newsCount = $('#news_container>span').length;
	var next = 1;
	var prev = newsCount;
	timerx = window.setInterval(function(){ $('.bcarousel-control_next').click(); }, 5000);	
	    
	//click next 
	$('.bcarousel-control_next').click(function(){
     $('#news_container').fadeOut(100);$('#news_container').stop(true,true).fadeIn(500);
	 $('span.news_paper').hide();
	 $('span.news_paper#'+next).stop(true,true).show();
	 //$('.first').attr('src', $('span.news_paper#'+next).attr('data-image-value')).fadeOut(500).fadeIn(1000); 	
		  next++;
		  prev = next - 2
		  if(next>newsCount) { next=1; }
		  if(prev<0 || prev==0) { prev=newsCount; }	  
	 });
	 
	 //click prev
	//click next 
	$('.bcarousel-control_prev').click(function(){
	 $('#news_container').fadeOut(100);$('#news_container').stop(true,true).fadeIn(500);
	 $('span.news_paper').hide();
	 $('span.news_paper#'+prev).stop(true,true).show();
	 //$('.first').attr('src', $('span.news_paper#'+prev).attr('data-image-value')).fadeOut(500).fadeIn(1000); 
			prev--;
			next = prev + 2
			if(prev<0 || prev==0) { prev=newsCount; }	  
			if(next>newsCount) { next=1; }
	 });

	 //news open-close
	 var newsBtnClick = 0;
	 $('#news-btn').click(
	 function() { 
	 if(newsBtnClick==0) { $(this).html('<span class="news_open"></span>');newsBtnClick=1;$('#hide-news').hide('slow');$('#news_ticker').animate({width: 35,opacity: 0.4, overflow:'hidden'}, 1000); }
	 else{ $(this).html('<span class="news_close"></span>');newsBtnClick=0; $('#hide-news').show();$('#news_ticker').animate({width: 437,opacity: 1}, 1000); }
	 });
	 
});

	 //all programs array
	 var allPrograms = new Array(); 
	 
</script>
