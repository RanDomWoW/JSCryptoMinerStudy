<script>
app.banner={
	act_index:"",
	
	pause :false,
	
	last_time:"",
	
	play:{
		banner_0:function(){
			var act_section = $("div[section_id='0']", $("#head_banner"));
			$("span", act_section).hide();
			
			$("img", act_section).css({
				width:"0px",
				height:"0px",
				opacity: 0
			});
			
			$("img:eq(1)", act_section).css({
				left:"477px",
				top:"155px"
			}).animate({
				width:"405px",
				height:"310px",
				left:"275px",
				top:"0px",
				opacity: 1
			});
			
			$("img:eq(0)", act_section).css({
				left:"130px",
				top:"138px"
			}).animate({
				width:"194px",
				height:"153px",
				left:"33px",
				top:"62px",
				opacity: 1
			});
			
			$("img:eq(2)", act_section).css({
				left:"867px",
				top:"152px"
			}).animate({
				width:"303px",
				height:"156px",
				left:"716px",
				top:"74px",
				opacity: 1
			});
			
			$("img:eq(3)", act_section).css({
				left:"1125px",
				top:"147px"
			}).animate({
				width:"84px",
				height:"170px",
				left:"1083px",
				top:"62px",
				opacity: 1
			}, function(){
				var act_section = $("div[section_id='0']", $("#head_banner"));
				
				$("span", act_section).hide().animate({				
					opacity: 'toggle'
				}, "slow");
			});
			
		},
		banner_1:function(){
			var act_section = $("div[section_id='1']", $("#head_banner"));
			
			$("img", act_section).css({
				display:"none",
				left:"50px"
			});
			
			$("img:eq(0)", act_section).animate({
				left:"0px",
				opacity: 'show'
			}, 500);
			
		},
		banner_2:function(){
			var act_section = $("div[section_id='2']", $("#head_banner"));
			
			$("img", act_section).css({
				display:"none",
				left:"130px"
			});
			
			$("img:eq(0)", act_section).animate({
				left:"88px",
				opacity: 'show'
			}, 500);
			
		},
		banner_3:function(){
			
			var act_section = $("div[section_id='3']", $("#head_banner"));
			
			$("img", act_section).css({
				display:"none"
			});
			
			$("img:eq(0)", act_section).animate({
				opacity: 'show'
			}, "slow");
			
			$("img:eq(2)", act_section).animate({				
				opacity: 'show'
			}, "slow");
			
			$("img:eq(1)", act_section).css({
				width:"0px",
				height:"0px",
				left:(act_section.width()/2) +"px",
				top:(act_section.height()/2) +"px"
			}).animate({
				width:"555px",
				height:"232px",
				left:"390px",
				top:"0px",			
				opacity: 'show'
			}, "fast");
			
			$("img:eq(3)", act_section).animate({				
				opacity: 'show'
			}, "slow");
			
		}
	}
};

$(function(){
	
	$("#head_banner").mouseenter(function(){
		app.banner.pause = true;
	});
	
	$("#head_banner").mouseleave(function(){
		app.banner.pause = false;
	});
	
	$(".slider_list li").each(function(index, item){
		$(item).mouseenter(function(){
			app.banner.pause = true;
		});
		
		$(item).mouseleave(function(){
			app.banner.pause = false;
		});
		
		$(item).click(function(){
			app.banner.last_time = new Date();
			
			//app.banner.pause = true;
			
			if(index.toString()==app.banner.act_index.toString()){
				return;
			}
			
			$(this).siblings().removeClass("banner_act");
			$(this).addClass("banner_act");
			
			$(".sections_act", $("#head_banner")).removeClass("sections_act");
			
			var act_section = $("div[section_id='"+ index +"']", $("#head_banner"));
			act_section.addClass("sections_act");
			
			$("div", act_section).hide().animate({				
				opacity: 'toggle'
			}, "slow");
			
			eval("app.banner.play.banner_"+ index +"()");
			
			app.banner.act_index = index;
		});
	});
});

$(function(){
	app.banner.last_time = new Date();
	
	$(".slider_list li:eq(0)").trigger("click");
	
	window.setInterval(function(){
		if(app.banner.pause){
			return;
		}
		
		var now_time = new Date();
		
		if((now_time-app.banner.last_time)<2000){
			return;
		}
		
		var next = app.banner.act_index + 1;
		if(next>3){
			next=0;
		}
		
		var next_item = $(".slider_list li:eq("+ next +")");
		next_item.trigger("click");
	}, 4000);
	
	
});
</script>
