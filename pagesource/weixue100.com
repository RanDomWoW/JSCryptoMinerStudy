//校验手机号是否合法
function is_phone(phonenum){
    var myreg = /^(((13[0-9]{1})|(15[0-9]{1})|(18[0-9]{1}))+\d{8})$/;
    if(!myreg.test(phonenum)){
        alert('请输入有效的手机号码！');
        return false;
    }else{
        return true;
    }
}

//发送验证码时添加cookie
function set_cookie(name, value, seconds, path, domain, secure){
    
    var cookieString = escape(name)+"="+escape(value);
    //判断是否设置过期时间,0代表关闭浏览器时失效
    if(seconds>0){
        var date = new Date();
        date.setTime(date.getTime() + seconds * 1000);
        cookieString += (date ? '; expires=' + date.toGMTString() : '')
        + (path ? '; path=' + path : '/')
        + (domain ? '; domain=' + domain : '')
        + (secure ? '; secure' : '');
    }
    document.cookie = cookieString;
}

//根据名字获取cookie的值
function get_cookie(name){
    
    var cookie_start = document.cookie.indexOf(name);
    var cookie_end = document.cookie.indexOf("; ", cookie_start);
    var cookie_value = cookie_start == -1 ? '' : unescape(document.cookie.substring(cookie_start + name.length + 1, (cookie_end > cookie_start ? cookie_end : document.cookie.length)));
        
    return cookie_value;
}

//开始倒计时
function start_countdown(obj) {
    var countdown=get_cookie("secondsremained");
    if (countdown == 0) {
        obj.removeAttr("disabled");
        obj.html("获取验证码");
        return;
    } else {
        obj.attr('disabled', 'disabled');
        obj.html("重新发送(" + countdown + ")");
        countdown--;
        set_cookie("secondsremained",countdown,countdown+1);
    }
    setTimeout(function() { start_countdown(obj) },1000) //每1000毫秒执行一次
}
        

  
