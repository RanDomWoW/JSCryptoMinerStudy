    $(function(){
        Carousel.init($(".pictureSlider"));
        Carousel.init($(".aboutSlider"))
        if (!(/msie [6|7|8|9]/i.test(navigator.userAgent))){
            new WOW().init();
        };

        /*轮播图*/
        carouselThree(240, 500,2000);
        function carouselThree(tupiankuandu, lunboshijian ,jiangeshijian) {
            //复制一个开头的图片
            //clone()就表示克隆一个元素。克隆的元素，必须使用appentTo追加到一个元素中。
            var curimg = 0;
            var len = $(".carouse-con a").length;
            $(".carouse-con a").clone().appendTo(".carouse-con");
            /*定时器*/
            var timer =	setInterval(rightButFunc,jiangeshijian);

            $(".carouse-con").mouseenter(function(){
                clearInterval(timer);
            });

            $(".carouse-con").mouseleave(function(){
                clearInterval(timer);
                timer =	setInterval(rightButFunc,jiangeshijian);
            });

            $(".right-btn").on("click", rightButFunc);
            function rightButFunc(){
                if(!$(".carouse-con").is(":animated")){
                    if(curimg < len){
                        curimg++;
                        $(".carouse-con").animate({"left":-tupiankuandu*curimg}, lunboshijian, function () {
                            if (curimg >= len) {
                                $(".carouse-con").css("left", 0);
                                curimg = 0;
                            }
                        });
                    }
                }
            }
            $(".left-btn").on("click", leftButFunc);
            function leftButFunc(){
                if(!$(".carouse-con").is(":animated")){
                    if(curimg > 0){
                        curimg--;
                        $(".carouse-con").animate({"left":-tupiankuandu*curimg}, lunboshijian);
                    } else {
                        curimg = len - 2;
                        $(".carouse-con").css("left",-tupiankuandu*(len - 1));
                        $(".carouse-con").animate({"left":-tupiankuandu*curimg}, lunboshijian);
                    }
                }
            }
        }
        //        tab的hover切换
        $('.tab-left-hover').hover(function () {
            var thisId = $(this).attr('id');
            $('.tab-left-hover').removeClass('tab-hover-border');
            $(this).addClass('tab-hover-border');
            console.log( $(this).addClass('tab-hover-border').siblings())
            $('.tab-right-one').not($('#tab'+thisId)).animate({ opacity: 0 }, 300);
            $('.tab-right-one').not($('#tab'+thisId)).css({ "z-index": "0","display":"none" });
            $('#tab'+thisId).stop(true).animate({opacity: 1.0 }, 300);
            $('#tab'+thisId).css({ "z-index": "10","display":"block" });
        });

    });

