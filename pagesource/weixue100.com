$(function($) {


    var footerFun = function(){
        var	e = $(".footer").position().top,
            f = $(".footer-apply").height(),
            g = $(window).height(),
            c = $(".header").height();
        $(window).scrollTop() >= e - g + f ? $(".footer-apply").removeClass("footer-fix"): $(".footer-apply").addClass("footer-fix");


