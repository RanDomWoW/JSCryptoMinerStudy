		$('.animate_revealgrid').each(function(){
			$us.scroll.addWaypoint($(this), '15%', function($elm){
				var $items = $elm.find('.animate_reveal');
				if ($us.$body.hasClass('disable_effects')) return $items.removeClass('animate_reveal');
