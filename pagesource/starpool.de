	<script type="text/javascript">
		document.onreadystatechange = function () {
            if (document.readyState == "interactive") {
                var form = document.getElementById('loginForm');
				var input = document.getElementById('loginField');
				var inputError = document.getElementById('error_p_login');
				var password = document.getElementById('passwordField');
				var passwordError = document.getElementById('error_password');
				var error = document.getElementById('error');
				var forgotPasswordForm = document.getElementById('forgotPW');
				var forgotPasswordInput = document.getElementById('forgotPWLogin');
				var forgotPWMessage = document.getElementById('forgotPWMessage');

				error.style.display = 'none';

				if (form.addEventListener) {
				  form.addEventListener("submit", callback, false);
				  forgotPasswordForm.addEventListener("submit", forgotPasswordHandler, false);
				}
				else {
				  form.attachEvent("onsubmit", callback);
				  forgotPasswordForm.attachEvent("onsubmit", forgotPasswordHandler);
				}

				function clearErrors() {
					error.innerHTML = '';
					forgotPWMessage.innerHTML = '';
					inputError.innerHTML = '';
					passwordError.innerHTML = '';
					input.style.borderColor = '';
					password.style.borderColor = '';
					error.style.display = 'none';
					forgotPWMessage.style.display = 'none';
				}

				function forgotPasswordHandler(event){
					event.preventDefault ? event.preventDefault() : (event.returnValue = false);
					clearErrors();
				  CORS('https://portal.starpool.de/checkPwForgotTarget?userName=' + forgotPasswordInput.value, function(request){
				  //CORS('http://milan.mediaworx.de:9000/checkPwForgotTarget?userName=' + forgotPasswordInput.value, function(request){
				    var xml = request;
				    if (request.responseXML) {
				        xml = this.responseXML;
				    } else if (request.responseText) {
				        if (window.ActiveXObject) {
				          xml = new ActiveXObject('Microsoft.XMLDOM');
				          xml.loadXML(request.responseText);
				        }
				    };

				    var response = request.response || request.responseText;
				    response = JSON.parse(response);

				    if (response.errorHeadline) {
				      forgotPWMessage.style.display = 'block';
				      forgotPWMessage.innerHTML = '<strong>' + response.errorHeadline + '</strong><br/>' + response.errorMessage;
				    } else {
				      clearErrors();
				    }

				    if (response.formAction) {
				      CORS(response.formAction, function(request){
				        var xml = request;
				        if (request.responseXML) {
				            xml = this.responseXML;
				        } else if (request.responseText) {
				            if (window.ActiveXObject) {
				              xml = new ActiveXObject('Microsoft.XMLDOM');
				              xml.loadXML(request.responseText);
				            }
				        };
				        var response = request.response || request.responseText;
				        response = JSON.parse(response);
				        forgotPWMessage.style.display = 'block';
				        forgotPWMessage.innerHTML = response.message;
				        forgotPWMessage.className = '';
				      }, 'GET');
				      // form.action = response.formAction;
				      // input.name = response.renameInto;
				      // form.submit();
				    }
				    if (response.method == 'GET') {
				      window.location.href = response.formAction;
				    }
				  }, 'GET');
				};

				function callback(event){
                    console.info('wu callback', event.type)
					event.preventDefault ? event.preventDefault() : (event.returnValue = false);
					clearErrors();

					if (!input.value || !password.value) {
					  if (!input.value) {
					    inputError.innerHTML += 'Bitte geben Sie Ihren Benutzernamen ein.<br/>';
					    input.style.borderColor = '#C07385';
					  }
					  if (!password.value) {
					    passwordError.innerHTML += 'Bitte geben Sie Ihr Passwort ein.';
					    password.style.borderColor = '#C07385';
					  }
					} else {
					  CORS('https://portal.starpool.de/checkLoginTarget?userName=' + input.value, function(request){
					  //CORS('http://milan.mediaworx.de:9000/checkLoginTarget?userName=' + input.value, function(request){
					    var xml = request;
					    if (request.responseXML) {
					        xml = this.responseXML;
					    } else if (request.responseText) {
					        if (window.ActiveXObject) {
					          xml = new ActiveXObject('Microsoft.XMLDOM');
					          xml.loadXML(request.responseText);
					        }
					    };
						    var response = request.response || request.responseText;
					    response = JSON.parse(response);
					    if (response.errorHeadline) {
					      error.style.display = 'block';
					      error.innerHTML = '<strong>' + response.errorHeadline + '</strong><br/>' + response.errorMessage;
					    } else {
					      clearErrors();
					    }

					    if (response.formAction) {
					      form.action = response.formAction;
					      form.submit();
					    }
					  }, 'GET');
					 }
				};

				function CORS(url, success, method, params) {
				    var xhr = new XMLHttpRequest();
				    if (!('withCredentials' in xhr)) xhr = new XDomainRequest(); // fix IE8/9
				    xhr.open(method, url);
				    xhr.onload = function(){
				  success(xhr);
				};
				if (params) {
				  xhr.send(params);
				} else {
				  xhr.send();
				}
				    return xhr;
				};
			}
		}
	</script>


