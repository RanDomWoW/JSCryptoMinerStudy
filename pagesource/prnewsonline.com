<script type="text/javascript">
    jQuery(function ($) {
        var regionOptions = [{ label: "AK", name: "AK"},{ label: "AL", name: "AL"},{ label: "AR", name: "AR"},{ label: "AZ", name: "AZ"},{ label: "CA", name: "CA"},{ label: "CO", name: "CO"},{ label: "CT", name: "CT"},{ label: "DC", name: "DC"},{ label: "FL", name: "FL"},{ label: "GA", name: "GA"},{ label: "IA", name: "IA"},{ label: "IL", name: "IL"},{ label: "IN", name: "IN"},{ label: "KY", name: "KY"},{ label: "LA", name: "LA"},{ label: "MA", name: "MA"},{ label: "MD", name: "MD"},{ label: "ME", name: "ME"},{ label: "MI", name: "MI"},{ label: "MN", name: "MN"},{ label: "MO", name: "MO"},{ label: "MT", name: "MT"},{ label: "NC", name: "NC"},{ label: "NE", name: "NE"},{ label: "NH", name: "NH"},{ label: "NJ", name: "NJ"},{ label: "NM", name: "NM"},{ label: "NV", name: "NV"},{ label: "NY", name: "NY"},{ label: "OH", name: "OH"},{ label: "OK", name: "OK"},{ label: "OR", name: "OR"},{ label: "PA", name: "PA"},{ label: "SC", name: "SC"},{ label: "TN", name: "TN"},{ label: "TX", name: "TX"},{ label: "UT", name: "UT"},{ label: "VA", name: "VA"},{ label: "VT", name: "VT"},{ label: "WA", name: "WA"},{ label: "WI", name: "WI"},{ label: "c-AU", name: "Australia"},{ label: "c-BE", name: "Belgium"},{ label: "c-BG", name: "Bulgaria"},{ label: "c-BR", name: "Brazil"},{ label: "c-CA", name: "Canada"},{ label: "c-CN", name: "China"},{ label: "c-DE", name: "Germany"},{ label: "c-FR", name: "France, French Republic"},{ label: "c-GB", name: "United Kingdom"},{ label: "c-IL", name: "Israel"},{ label: "c-JP", name: "Japan"},{ label: "c-KW", name: "Kuwait"},{ label: "c-MN", name: "Mongolia"},{ label: "c-NZ", name: "New Zealand"},{ label: "c-TR", name: "Turkey"},{ label: "c-UG", name: "Uganda"},{ label: "c-VE", name: "Venezuela"},];

        var isDev = false;
        var apiBaseUrl = 'http://'+(isDev ? 'prnewsonline.dev.decisionbriefs.com/' : 'firmfinder.prnewsonline.com/')+'api/';
        var logoBaseUrl = 'http://apps.'+(isDev ? 'dev.' : '')+'decisionbriefs.com/images/logos/';
        var dirBaseUrl = 'http://'+(isDev ? 'prnewsonline.dev.decisionbriefs.com/' : 'firmfinder.prnewsonline.com/');
        var searchBtn = $('#search-button');
        var contentsContainer = $('#contents-container');
        var specialtySelect = $('select[name="specialty"]');
        var sectorSelect = $('select[name="sector"]');
        var nameSelect = $('select[name="firm_name"]');
        var stateSelect = $('select[name="state"]');
        var resultsContainer = $('#results-container');
        var noResults = '<h3 style="text-align:center;">No results found.</h3>';

        function addOptions(selector, options) {
            var html = '';
            var oCount = 1;

            $.each(options, function (i, o) {
                name = o.name ? o.name : o.title;
                html += '<option value="'+ o.label+'">'+name+'</option>'
                oCount++;
            });
			selector.append(html);
        }

        $.get(apiBaseUrl + '/vocabulary?type=profile', function (d) {
            addOptions(nameSelect, d.results);
        });

        var state_options = [];
        for (i in regionOptions) {
            state_options.push({'label': regionOptions[i].label, 'name': regionOptions[i].name});
        }
        addOptions(stateSelect, state_options);

        searchBtn.on('click', function () {
            resultsContainer.html('');

            resultsContainer.height(180);
            resultsContainer.removeClass('loaded');

            var keyword = nameSelect.find('option:selected').text();
            var state = stateSelect.val();
            var resultsHtml = '';

            //setup query params
            var params = {
                '_cat' : [],
                'keyword' : (keyword != '') ? keyword : '',
                'region' : (state != '') ? [state] : [], //'region' on production, 'state' on stage
                'type' : 'profile'
            };

            $.get(apiBaseUrl + 'vocabulary', params, function (d) {
                resultsContainer.addClass('loaded');

                //console.log(d);

                if (!d || !d.results.length) {
                    resultsContainer.html(noResults);
                    return false;
                }

                var suffix = d.total_count > 1 ? 's' : '';
                resultsHtml += '<h4>' + d.total_count + ' Firm' + suffix +' Found:</h4>';
                resultsHtml += '<ul>';

                $.each(d.results, function (i, obj) {

                    resultsHtml += '<li>';

                    if (obj.logo) {
                        resultsHtml += '<img src="'+logoBaseUrl+obj.id+'/32x32.e.jpg" />';
                    }

                    resultsHtml += '<a target="_blank" href="'+dirBaseUrl+obj.label+'">'+obj.title+'</a>';
                    resultsHtml += '</li>';
                });

                resultsHtml += '</ul>';

	            resultsContainer.html('');
                resultsContainer.append(resultsHtml);
            });
        });

        $('.search-all-link').click(function (e) {
            e.preventDefault();
            parent.location.href = $(this).prop('href');
        });
    });//end doc ready
</script>
