<script type="text/javascript">
	var elementInACount = $('#pi63Elements .rating-opinion:visible').length;
	var elementShown = elementInACount; 
	var allElements = $('#pi63Elements .rating-opinion').length;
	if($(window).width() < 800){
		var i = 0;
		$('#pi63Elements .rating-opinion.initial').each(function(){
			i = i +1;
			if(i > 9){
				$(this).removeClass('initial').hide();
			}
		});
	}
	$('#pi63Elements .rating-opinion:visible').addClass('initial');
 	$('.tx_customeropinions_pi63 #pi63ShowMore').click(function(){
		if($('#pi63Elements').attr('style')){
			$('#pi63Elements').removeAttr('style');
		}else{
		 elementShown= ( elementShown+elementInACount  <= allElements) ?  elementShown+elementInACount : allElements;
		$('#pi63ShowNone').show();
		$('#pi63Elements .rating-opinion:lt('+elementShown+')').show();
		}
	});
	$('#pi63ShowNone').click(function(){
		$('#pi63Elements .rating-opinion').hide();
		$('#pi63Elements .rating-opinion.initial').show();
		$('body').scrollTop($('.tx_customeropinions_pi63 #pi63ShowMore').offset().top-200);
		 elementInACount = $('#pi63Elements .rating-opinion.initial').length;
		elementShown = elementInACount;
		$(this).hide();
	});
	
	
	
	$('.rating-opinion').click(function(){
			$('.desktop-rating-view').remove();
			var pos = $(this).position();
			var $el = $(this);
			var $newEl = $el.clone().addClass('desktop-rating-view iwi').insertAfter('#pi63Elements');
		 
			$newEl.find('.rating-table').show();
			
		if($(window).width() > 900){
			$newEl.css({'left':pos.left-($(this).outerWidth()/4),'top':pos.top-($(this).outerWidth()/4)});
			var newPosition = $newEl.position();
			if($('#pi63Elements').outerWidth() >= $(window).width() && newPosition.left < 0){
				$newEl.css({'left':0});
			}
			if(newPosition.left + $newEl.outerWidth() > $(window).width()){
				$newEl.css({'left':$(window).width()-$newEl.outerWidth()-10});
			}
			
		}else{
			$newEl.css({'top':pos.top-($(this).outerHeight()/2)});
		}
		$newEl.click(function(){
				$(this).remove();
		});
		//$(this).addClass('active');
	});

	
</script> 
<script type="text/javascript">
 
 
/* minitopbanner */
var cssstyle= $('#customerOpinionsMiniTopBanner').attr('style');
var $lastRatings = $('#pi63Elements .rating-opinion:lt(5)');

function removeMiniBanner(){
	$('#customerOpinionsMiniTopBanner').remove();	
}

function buildMiniBanner(){
 /* var apendHTML = '';
 
	 $('h1:first').before(apendHTML);*/
 
  
  var   $rating = $('span[itemprop="aggregateRating"]'),
        ratingValue = $rating.find('span[itemprop="ratingValue"]').html(),
        bestRating = $rating.find('span[itemprop="bestRating"]').html(),
		ratingCount = $rating.find('span[itemprop="ratingCount"]').html(),
        ratingLink = 'aus %s <a href="tests-presse-und-meinungen/kundenmeinungen/"> Kundenmeinungen</a>',
        ratingButton = '<a href="tests-presse-und-meinungen/kundenmeinungen/" class="bg-green">Jetzt Kundenmeinung abgeben</a>',
      
        $banner = $('#customerOpinionsMiniTopBanner');
		
 // console.log(bestRating+":"+ratingCount+":"+$lastRatings.length);
  $('#customerOpinionsrestStars div').appendTo( $banner.find('.rating-stars'));
  
  $banner.find('.header .rating-label').html(ratingValue+' / '+bestRating);
  $banner.find('.header .starsvotes-subline').html(ratingLink.replace('%s',ratingCount));
  $banner.find('.footer').html(ratingButton);
  $lastRatings.each(function(){
     $(this).clone().appendTo( $banner.find('.content') );
  });
  //some event
 
  //some height
/* while($banner.find('.content').height() > 600){
	 
	  $banner.find('.content .rating-opinion').last().remove();
  }*/
  

  $banner.find('.header').mouseenter(function(){
	  $banner.addClass('hover');
	  
	  setTimeout(function(){
		 if($banner.hasClass('hover')){
			  $banner.addClass('shadow').attr('style',cssstyle+'; height:auto; background-color:#FFF');
			  dataLayer.push({'event':'GAevent', 'eventCategory':'Kundenmeinung Liste', 'eventAction':'angesehen', 'eventLabel':'Kundenmeinung Saal Digital - Siegelbanner'});	
			  $banner.find('.content').show().parent().find('.footer').show();
		 }
	 	
	  }, 500); 
  });
  $banner.mouseleave(function(){
      $(this).removeClass('shadow').removeClass('hover').removeAttr('style').attr('style',cssstyle);
	  $banner.find('.content').hide().parent().find('.footer').hide();
  });
 
}
//Add minitopbanner
$(document).ready(function(){
	
	if(!$('body').hasClass('mobilebrowser') && $('span[itemprop="aggregateRating"]').length > 0 && $('#customerOpinionsMiniTopBanner').length > 0 && $($lastRatings).length > 0){
		buildMiniBanner();

	}else{
		removeMiniBanner();	
	}
});
 

</script> 

