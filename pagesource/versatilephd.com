                $( document ).ready(function() {

                    var obj = document.getElementById('inner-header');
                    if(obj){
                        obj.focus();
                    }

                    $("#vphd_banner_mobile ul.top a.mobile-bars").click(function(e) {
                        e.stopPropagation();
                        e.preventDefault();
                        $("#vphd_banner_mobile nav ul.top > li ul").toggle(200);
                    });

                });

                $(document).click(function(event) { 
                    if(!$(event.target).is('#vphd_banner_mobile ul.top a.mobile-bars')) {
                            $("#vphd_banner_mobile nav ul.top > li ul").hide(200);
                    }          
                    if(!$(event.target).is('#menu-vphd_menu li.menu-item-has-children > a')) {
                            $("#menu-vphd_menu li.menu-item-has-children ul.sub-menu").hide(200);
                    }          
                });

                $("#xxxmenu-vphd_menu li.menu-item-has-children > a").click(function(e) {
                        $(this).parent().children('ul.sub-menu').first().toggle(200);
                        e.stopPropagation();
                        e.preventDefault();
                });

                $('#xxxmenu-vphd_menux li.menu-item-has-children  > a').keydown(function(e) {
                    if (e.which==13) {
                        if($(this).parent().children('ul.sub-menux').first().is(':hidden')) {
                            $(this).trigger('mouseenter');
                        } else {
                            $(this).trigger('mouseleave');
                        }
                    }
                });


            });
