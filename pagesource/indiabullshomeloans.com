<script type = "text/javascript">

$("#submit-form").on('click',function(e){
  e.preventDefault();
  var name = $('input.username').val();
  var email = $('input.email').val();
  var mobile = $('input.mobile').val();  

if (name == '') {
  document.getElementById("validation").innerHTML = "Name is Mandatory"; 
 } 
 if (email == '')
 {
	 document.getElementById("validation1").innerHTML = "Email is Mandatory"; 
 }
   else
		{
			if (!/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(email))
			
			{
				$('#validation1').html('Invalid email address.');	
				
				return false;
			}
		}
 if (mobile == '' )
 {
	 document.getElementById("validation2").innerHTML = "Mobile is Mandatory"; 
 } 
 else if (isNaN( document.form.mobile.value ) || document.form.mobile.value.length != 10 )
  {
	  document.getElementById("validation2").innerHTML = "Mobile number is not valid";
  }	 
 else {
	 
$('div.loader').fadeIn();
// AJAX code to submit form.
$.ajax({
type: "POST",
url: "ajaxjs.php",
data: $('#sub_form').serialize(),

success: function(data) {
  $('div.loader').fadeOut();
  $('.form').hide();
  $('.exit-thankyou').show();
}
});
 }
});
</script>

