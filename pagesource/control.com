    $.fn.extend({

      togglePost: function() {
        var tid = $(this).attr('id').substring(7);
        var arrow = '#thread_' + tid;
        var info = arrow + '_info';
        if ( $( arrow ).hasClass('post_toggle_right') ) {
          $( arrow ).removeClass('post_toggle_right').addClass('post_toggle_down');
          $( info ).slideDown();
        }
        else if ( $( arrow ).hasClass('post_toggle_down') ) {
          $( arrow ).removeClass('post_toggle_down').addClass('post_toggle_right');
          $( info ).slideUp();
        }
      },

      openAll: function() {
        return $(this).each( function() {
          $(this).slideDown();
          // change the graphic for the arrow icon...
          var arrow = $(this).attr('id');
          arrow = arrow.substring(0, arrow.length - 5);
          $( '#' + arrow ).removeClass('post_toggle_right').addClass('post_toggle_down');
        })
      },

      closeAll: function() {
        return $(this).each( function() {
          $(this).slideUp();
          // change the graphic for the arrow icon...
          var arrow = $(this).attr('id');
          arrow = arrow.substring(0, arrow.length - 5);
          $( '#' + arrow ).removeClass('post_toggle_down').addClass('post_toggle_right');
        })
      }
    });

