var defaultHerstellerTxt = '';
var defaultInhaltsstoffTxt = '';
var defaultIndikationTxt = '';

function trisuchonfocus(myForm)
{
	//setze aktuelles form fuer die trisuchsubmit-funktion
	aktivform = myForm;

