<script type="text/javascript"> 
$(document).ready(function(){
	$("#frm_contact").validationEngine();
	//$("#form_newsletter").validationEngine();
});

$(document).ready(function(){
jQuery("#form_newsletter").validationEngine('attach', {
	
			onValidationComplete: function(form, status){
				
				$('.formError ').appendTo(".footer2");
				
				if(status == true){
					var newsletter	=	jQuery('input[name=newsletter]').val();
					var codeMenu	=	jQuery('input[name=menuNews]').val();
					jQuery.ajax({
					type: 'POST',
					url: "http://www.hannibaltv.com.tn/newsletter/verifEmailExists",
					data: "email="+newsletter+"&codeMenu="+codeMenu,
					dataType:"json",
					success: function(result){	
																	
						if(result.jsonValidateReturn[0]=="true"){
							
							jQuery('#form_newsletter').validationEngine('showPrompt', "تم ارسال الاشتراك في النشرة الإخبارية بشكل صحيح", 'pass');
							$('.formError ').appendTo(".footer2");
							jQuery('input[name=newsletter]').attr('value','');
							
						}else{
							jQuery('#form_newsletter').validationEngine('showPrompt', "البريد الإلكتروني موجود مسبقا", 'text');
							$('.formError ').appendTo(".footer2");
							jQuery('input[name=newsletter]').attr('value','');
						}
					},
					error: function(xhr, ajaxOptions, thrownError){
						jQuery('.formError ').appendTo(".footer2");
							jQuery('#form_newsletter').validationEngine('showPrompt', ' الرجاء إعادة المحاولة', 'text');
							 jQuery('input[name=newsletter]').attr('value','');
					} 
				});
				}
			}
	});
	
});

</script>
