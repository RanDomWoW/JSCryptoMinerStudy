<table id="data"></table>

<script>
$(document).ready(function(){
    $.ajax({
        type: "GET",
        url: "http://supermarket.seehuamarketing.com/supermarketads/ub.xml", 
        dataType: "xml",
        success: function(xml) {  
            $(xml).find('nodes').each(function(){    
                var Col0 = $(this).find('title').text();
                var Col1 = $(this).find('url').text();
                $('<tr></tr>').html('<a rel="nofollow" href="'+Col1+'" target="_blank">'+Col0+'</a>').appendTo('#data');
            });
        }
    });
});
</script>

</body>
</html>
