        $('#nav-submit-location').click(submitSearch);
        function submitSearch() {
            debugger;
            //$.cookie("zipcode", null);
            //$.cookie("loc_num", null);

            $('#loading').css('display', 'block');
            var latlong = "";
            var address = document.getElementById("nav-zip").value.trim();
            if (address == '') { alert('Please enter address'); $('#loading').css('display', 'none'); return; }

            var specialCharacters = ['.', '#', '*', '%', ')'];
            for (i = 0; i < specialCharacters.length; i++) {
                if (address.endsWith(specialCharacters[i])) {
                    address = address.slice(0, -1);
                }
            }

            address = address.trim();
            if (address == '') { alert('Please enter proper address'); $('#loading').css('display', 'none'); return; }



            var miles = $('#radius :selected').val();
            if (miles === undefined) {
                miles = 100;
            }

            var isValidZip = true;
            if (address != null && address != "" && address != undefined) {
                isValidAddress = /[A-Za-z0-9'\.\-\s\,]{1,50}/.test(address);
                isValidZip = /(^\d{3}$)|(^\d{5}(-\d{4})?$)|(^[AaBbCcEeGgHhJjKkLlMmNnPpRrSsTtVvWwXxYy]{1}\d{1}[a-zA-Z]{1} *\d{1}[a-zA-Z]{1}\d{1}$)/.test(address);
            }
            if (!isValidAddress) {
                alert("Invalid address entered");
                $('#loading').css('display', 'none');
                return;
            }


            else
                if (address != null && address != "" && address != undefined) {
                    var state = null;
                    var matches = states.filter(function (stateItem) {
                        return stateItem.Text.toUpperCase() === address.toUpperCase() || stateItem.Value.toUpperCase() === address.toUpperCase();
                    });
                    if (matches[0])
                        state = matches[0];
                    if (isValidZip) {
                        // Google tag manager
                        dataLayer.push({
                            'event': 'LocationSearch',
                            'eventAction': 'Zip',
                            'eventLabel': 'Header'
                        });
                        kenshooConversion('6456');
                        ef_fire_pixel('ev_location_search');

                        getCoordinatesForNav("Locations_By_Address", address, function (data) {

                            debugger;
                            if (typeof (data) == "string") {
                               
                                if (data.indexOf('\\') > 0) {

                                    latlong = JSON.parse(data);
                                }
                                else {
                                    latlong = data;
                                }
                            }
                            else {
                                latlong = data;
                            }

                            if (data !== "") {
                                window.location.href = "/locations/?type=5&val=" + data + "&miles=" + miles;
                            } else {
                                window.location.href = "/locations/?type=1&val=" + address + "&miles=" + miles;
                            }

                            //$.ajax({
                            //    type: "GET",
                            //    cache: false,
                            //    url: "../DeviceLocations/GetLocationDetails/LatLong/" + data,
                            //    success: function (viewHTML) {
                            //        console.log(viewHTML);
                            //    },
                            //    error: function (e) {
                            //        console.log(e);
                            //    },
                            //});

                            localStorage.setItem("Coord", data);


                        });
                    }
                    else if (matches[0]) {
                        dataLayer.push({
                            'event': 'LocationSearch',
                            'eventAction': 'State',
                            'eventLabel': 'Location'
                        });
                        window.location.href = "/locations/?type=2&val=" + state.Value + "&miles=" + miles;
                    }
                    else {

                        // Google tag manager
                        dataLayer.push({
                            'event': 'LocationSearch',
                            'eventAction': 'Address',
                            'eventLabel': 'Header'
                        });

                        kenshooConversion('6456');
                        ef_fire_pixel('ev_location_search');

                        getCoordinatesForNav("Locations_By_Address", address, function (data) {
                            latlong = data;
                            if (data !== "") {
                                window.location.href = "/locations/?type=5&val=" + data + "&miles=" + miles;
                            } else {
                                window.location.href = "/locations/?type=4&val=" + address + "&miles=" + miles;
                            }

                            //$.ajax({
                            //    type: "GET",
                            //    cache: false,
                            //    url: "../DeviceLocations/GetLocationDetails/LatLong/" + data,
                            //    success: function (viewHTML) {
                            //        console.log(viewHTML);
                            //    },
                            //    error: function (e) {
                            //        console.log(e);
                            //    },
                            //});

                            localStorage.setItem("Coord", data);
                        });
                    }


                }
        }

    });


</script>














 
