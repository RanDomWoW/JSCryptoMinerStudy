	<script>
		//#1 show & hide navigation (for quicklink buttons)
		//#2 show & hide search + show & hide list me

		$( function()
		{
			$( '#nav li:has(ul)' ).doubleTapToGo();
		});

		//#1
		$(function(){
		$(document).mousedown(function(){
				$('.dropdown .active').removeClass('active').next('ul').hide();
			})
			$('.dropdown').on('mousedown','.subMenu', function(e){
				e.stopPropagation();
				var elem = $(this);
				if(elem.is('.active')) {
					elem.next('ul').slideUp(150);
					elem.removeClass('active');
				} else {
				   $('.dropdown .active').removeClass('active').next('ul').hide();
					elem.addClass('active').next('ul').slideDown(150);
				}
			});
			$('.subMenu').on('mousedown','ul', function(e){
				e.stopPropagation();
				//alert('menu item clicked');
			});
		});


		//#2
		$(function() {
			var listMe		= $('listMe');
			var searchDiv	= $('.searchDiv');

			// listMe toggle
			$('.showMe').click(function() {
				var w = $(window).width();
				// enable the function when window is below 768px
				if(w <= 767 ) {
					$(this).next().toggle(0, 'listMe');
				}
			});

			$('.showMePod').click(function() {
				var w = $(window).width();
				// enable the function when window is below 768px
				if(w <= 767 ) {
					$(this).next().toggle(0, 'listMe');
				}
			});

			//#2
			$('.show_hide').click(function() {
				var w = $(window).width();
				// enable the function when window is below 768px
				if(w <= 767 ) {
					searchDiv.toggle();
					//alert('search item clicked');
					}
			});

			//MOBILE NAV
			$('.show_hide_search').click(function() {
				var w = $(window).width();
				// enable the function when window is below 768px
				if(w <= 767 ) {
					searchDiv.toggle();
					if($("#nav>ul").attr("class") == 'dropdown active'){
							$("#nav>ul").removeClass('active');
						//}else{
						//	$("#nav>ul").addClass('active');
						}
					//alert('search item clicked');
					}
			});


			$('.show_hide_nav').click(function() {
				var w = $(window).width();
				// enable the function when window is below 768px
				if(w <= 767 ) {
						//searchDiv.toggle();
						if($("#nav>ul").attr("class") == 'dropdown active'){
							$("#nav>ul").removeClass('active');
						}else{
							$("#nav>ul").addClass('active');
						}
					}
			});

			$(window).resize(function(){
				var w = $(window).width();

				//show/hide list me
				if(w >= 767 && listMe.is(':hidden')) {
					listMe.show(); //show list me
				}
				if(w <= 767 && listMe.is(':visible')) {
					listMe.hide(); //hide list me
				}
				// show/hide the search form
				if(w >= 767  && searchDiv.is(':hidden')) {
					searchDiv.show();
				}
				if(w <= 767  && searchDiv.is(':visible')) {
					searchDiv.hide();
				}
			});
			$(window).resize();
		});
	</script>


