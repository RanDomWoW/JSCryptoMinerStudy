<script type="text/javascript">
    $(document).ready(function () {

        if ($('.elaph-newswall-main').hasClass('style2')) {
            $('.elaph-newswall-main ol').after('<div id="elaph-newswall-infobox" class="elaph-newswall-loading"></div>');
            var nwGoTimer;
            var j = 0;
            var jmax = $('.elaph-newswall-main ol li').length;
            function nwGo() {
                $('.elaph-newswall-main').find('ol li:eq(' + j + ')').each(function () {

                    if (j == 0) {
                        $('.elaph-newswall-main ol li:eq(' + (jmax - 1) + ')').removeClass('elaph-newswall-active');
                    }
                    else {
                        $(this).prev().removeClass('elaph-newswall-active');
                    }

                    $(this).addClass('elaph-newswall-active');
                    var contentNW = $(this).children().clone();
                    var imgNW = $(this).find('a.elaph-nw-image');
                    $('.elaph-newswall-main').find('ol li a').attr('title', '');

                    if (!$('h2 a', this).data('a_title')) {
                        $('h2 a', this).data('a_title', $('h2 a', this).attr('title'));
                    }

                    $(this).find('h2 a').attr('title', '');
                    $('#elaph-newswall-infobox').removeClass('elaph-newswall-article-image elaph-newswall-loading').children().remove().empty();
                    $('#elaph-newswall-infobox').append(contentNW).find('figure a').append('<span></span>');

                    if (imgNW.length) {
                        $('#elaph-newswall-infobox').addClass('elaph-newswall-article-image');
                    }

                    $('#elaph-newswall-infobox h2 a').attr('title', '');

                    j++;
                });

                if (jmax == j) {
                    j = 0;
                }
            }

            function nwStyle2() {

                if ($('.elaph-newswall-main ol li:eq(0)').hasClass('elaph-newswall-date-marker')) {
                    $('#elaph-newswall-infobox a').prepend('<span class="elaph-newswall-date-intro">Explore <strong>The NewsWall&trade;</strong></span>');
                }
                else if ($('.elaph-newswall-main ol li:eq(0)').hasClass('elaph-nw-article-wo-image')) {
                }
                else {
                    $('#elaph-newswall-infobox').addClass('elaph-newswall-article-image').find('figure a').append('<span></span>');
                }

                $('.elaph-newswall-main ol li:eq(0)').children().clone().appendTo('#elaph-newswall-infobox');
                $('.elaph-newswall-main ol li').hover(function () {
                    var contentNW = $(this).children().clone();
                    var imgNW = $(this).find('figure');
                    var dateTile = $(this).hasClass('elaph-newswall-date-marker');
                    if (!$('h2 a', this).data('a_title')) {
                        $('h2 a', this).data('a_title', $('h2 a', this).attr('title'));
                    }

                    $(this).find('h2 a').attr('title', '');
                    $('#elaph-newswall-infobox').removeClass('elaph-newswall-article-image elaph-newswall-loading').children().remove().empty();

                    $('#elaph-newswall-infobox').append(contentNW).find('figure a').append('<span></span>');
                    if (imgNW.length) {
                        $('#elaph-newswall-infobox').addClass('elaph-newswall-article-image');
                    }
                    $('#elaph-newswall-infobox h2 a').attr('title', '');
                    $('#elaph-newswall-infobox').last()[0].childNodes[3].style.top = '-446px';
                    $('#elaph-newswall-infobox').last()[0].childNodes[3].style.left = '-295px';

                }, function () {
                    $('h2 a', this).attr('title', $('h2 a', this).data('a_title'));
                });
            }

            nwStyle2();

            nwGoTimer = setInterval(nwGo, 5000);

            $('.elaph-newswall-main').hover(function () {
                clearInterval(nwGoTimer);
                $(this).find('li.elaph-newswall-active').removeClass('elaph-newswall-active');
            }, function () {
                nwGoTimer = setInterval(nwGo, 5000);
            });

            if ($('.elaph-newswall-main').children().length > 0)
                $($('.elaph-newswall-main').children()[2]).remove();

            $('#elaph-newswall-infobox').last()[0].childNodes[3].style.top = '-446px';
            $('#elaph-newswall-infobox').last()[0].childNodes[3].style.left = '-295px';
        }
    });
</script>
