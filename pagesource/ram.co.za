            } else if ($(this).val() == 'Prohibited') {
                $('#domesticRestrictedError').html('We are unable to transport a <a href="/files/prohibited_items_schedule.pdf" target="_blank" style="color:red; text-decoration: underline; padding: 0;"><span style="color:red; font-weight: bold;">prohibited</span></a> item').show(250);
            } else {
                $('#domesticRestrictedError').html('').hide(250);
            }

            if ($("#domesticRestrictedItems option:selected").text() == 'Restricted - Firearm') {
                $('#ParcelTypeFirearmDomestic_proxy').click();
            }
        });

        $('#ParcelTypeFirearmDomestic_proxy').click(function () {
            $("#domesticRestrictedItems option:contains('Restricted - Firearm')").attr('selected', true);
            $("#domesticRestrictedItems").attr("disabled", "disabled");
