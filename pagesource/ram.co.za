        });

        $('#ParcelTypeParcelDomestic_proxy').click(function () {
            $("#domesticRestrictedItems option").each(function () {
                if ($(this).text() == "Select") {
                    $(this).attr('selected', 'selected');
                    $('#domesticRestrictedError').html('').hide(250);
                }
            });

            $("#domesticRestrictedItems").removeAttr("disabled");
        });


        $('#intlRestrictedItems').change(function () {
            if ($(this).val() == 'Restricted') {
                $('#intlRestrictedError').html('Your parcel contains a <a href="/files/restricted_items_schedule.pdf" target="_blank" style="color:red; text-decoration: underline; padding: 0;"><span style="color:red; font-weight: bold;">restricted</span></a> Item and may only travel via road freight').show(250);
            } else if ($(this).val() == 'Prohibited') {
                $('#intlRestrictedError').html('We are unable to transport a <a href="/files/prohibited_items_schedule.pdf" target="_blank" style="color:red; text-decoration: underline; padding: 0;"><span style="color:red; font-weight: bold;">prohibited</span></a> item').show(250);
            } else {
                $('#intlRestrictedError').html('').hide(250);
            }
        });

