        function noticiero_anterior_paginar (ruta) {
            var fechas_disponibles = [];
        
            var $noticieros_anteriores = 
            $(ruta);
        
            if ($noticieros_anteriores.length) { 
            
                $noticieros_anteriores.find('.views-row .views-field-created').each(function () {
                    var fecha = $(this).text().split('-');
                    var mes = fecha[1].trim();
                    var anio = fecha[2].trim();

                    if (!(anio in fechas_disponibles)) fechas_disponibles[anio] = [];
                    if (!(mes in fechas_disponibles[anio])) fechas_disponibles[anio][mes] = mes;

                    var $fila = $(this).closest('.views-row');
                    $fila
                    .addClass('anio-'+anio)
                    .addClass('mes-'+mes)
                    .data('mes','mes-'+mes)
                    .data('anio','anio-'+anio);
                });

                var htmlAnios = '';
                var htmlAniosMeses ='';

                for (var anio in fechas_disponibles) {
                    if (fechas_disponibles.hasOwnProperty(anio)) {
                  
                        htmlAnios = '<div class="anio-'+anio+'">'+anio+'</div>' + htmlAnios;
                        htmlAniosMeses += '<div class="anio anio-'+anio+'">';
                    
                        var htmlMeses ='';
                        for (var mes in fechas_disponibles[anio]) {
                            if (fechas_disponibles[anio].hasOwnProperty(mes)) {
                                htmlMeses = '<div class="mes mes-'+mes+'">'+fechas_disponibles[anio][mes]+'</div>' +htmlMeses;
                            }
                        }
                        htmlAniosMeses += htmlMeses + '</div>'
                    }
                }       
                
                var htmlSelector = '<div class="selector-fechas"><div class="selector-anios">'+htmlAnios+'</div><div class="selector-meses">'+htmlAniosMeses+'</div></div>';
                $noticieros_anteriores.prepend(htmlSelector);
                
                var $selector_fechas = $noticieros_anteriores.find('.selector-fechas'); 
 
 
                for (anio in fechas_disponibles) {
                    if (fechas_disponibles.hasOwnProperty(anio)) {
                        $selector_fechas.find('.selector-anios .anio-'+anio).data('anio',anio);
                        $selector_fechas.find('.selector-meses .anio-'+anio).data('anio',anio);
                        for (mes in fechas_disponibles[anio]) {
                            if (fechas_disponibles[anio].hasOwnProperty(mes)) {
                                $selector_fechas.find('.selector-meses .anio-'+anio+' .mes-'+mes).data('mes',mes);
                            }
                        }
                    }
                }        
                
                $selector_fechas.find('.selector-meses div div').click (function () {
                    var mesEscogido = 'mes-'+$(this).data('mes');
                    var anioEscogido = 'anio-'+($(this).parent().data('anio'));
                    $noticieros_anteriores.find('.views-row').hide();
                    $noticieros_anteriores.find('.views-row.'+anioEscogido+'.'+mesEscogido).show();
                    //$noticieros_anteriores.mCustomScrollbar('update');
                    $(this).siblings().removeClass('activo');
                    $(this).addClass('activo');

                });
                
                //$selector_fechas.find('.selector-meses .anio-'+new Date().getFullYear()+' .mes').eq(-1).click();
                
                $selector_fechas.find('.selector-anios div').click (function () {
                    var anioEscogido = 'anio-'+$(this).data('anio');
                    $noticieros_anteriores.find('.views-row').hide();
                    $noticieros_anteriores.find('.views-row.'+anioEscogido).show();
                    $noticieros_anteriores.find('.selector-fechas .selector-meses .anio').hide();
                    $noticieros_anteriores.find('.selector-fechas .selector-meses .anio.'+anioEscogido).show();
                    //$noticieros_anteriores.mCustomScrollbar('update');
                    $noticieros_anteriores.find('.selector-fechas .selector-meses div div').css('font-weight','normal');
                    $selector_fechas.find('.activo').removeClass('activo');
                    $(this).addClass('activo');

                    if ($selector_fechas.find('.selector-meses .'+anioEscogido+' .mes').length==1) {
                        $selector_fechas.find('.selector-meses .'+anioEscogido+' .mes').eq(0).click();
                    }
                    
                });


                
            } 
