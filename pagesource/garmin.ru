                    '    lt.async = true;' +
                    '    lt.src = "http://cs15.livetex.ru/js/client.js";' +
                    '    var sc = document.getElementsByTagName("script")[0];' +
                    '    if (sc) sc.parentNode.insertBefore(lt, sc);' +
                    '    else  document.documentElement.firstChild.appendChild(lt);' +
                    '})();';
                document.body.appendChild(script);
            }
        }

        /**
         * @type {string}
         */
        var currentRegion = '';

        /**
         * @param {function()} callback
         */
        function initGeo(callback) {

            window['geoCallback'] = function() {
                localCallback();
            };

            var geoApi = document.createElement('script');
            // geoApi.src = 'api-maps.yandex.ru/2.1.29/?lang=ru-RU&onload=geoCallback';
//geoApi.src = 'http://api-maps.yandex.ru/2.1/?lang=ru_RU&onload=geoCallback';
            document.body.appendChild(geoApi);

            /**
             */
            function localCallback() {
                ymaps.ready(function() {
                    ymaps.geolocation.get({
                        provider: 'yandex'
                    }).then(function(result) {
                        currentRegion = result.geoObjects.get(0).properties.get('text');
                        callback();
                    });
                });
            }
        }

        /**
         * @return {string}
         */
        function getRegion() {
            return currentRegion;
        }

        /**
         * @param {!Array.<string>} acceptedRegions
         * @param {!Array.<string>} declinedRegions
         * @param {string} currentRegion
         * @return {boolean}
         */
        function checkRegions(acceptedRegions, currentRegion) {
            for (var i = 0; i < acceptedRegions.length; i++) {
                if (currentRegion.indexOf(acceptedRegions[i]) !== -1) {
                    return true;
                }
            }
            return false;
        }

        /**
         *
         */
        function init() {
            var append = createAppender(clientId);
            if (checkRegions(acceptedRegions, getRegion())) {
                append();
            }
        }

        initGeo(init);

    })();
</script>



