<script type="text/javascript">
function changeClass(fc)
{
	x="button";
	for(i=1;i<4;i++)
	{ y=x+i;  if (i == fc) { document.getElementById(y).disabled = true; } else { document.getElementById(y).disabled = false; }  }     	

	x="tda";
	for(i=1;i<2;i++)
	{  y=x+i;     z="ffa_"+fc;  document.getElementById(y).className = z; 	}

	x="tdb";
	for(i=1;i<8;i++)
	{  y=x+i;     z="ffb_"+fc;  document.getElementById(y).className = z;	}

	x="tdc";
	for(i=1;i<4;i++)
	{  y=x+i;     z="ffc_"+fc;  document.getElementById(y).className = z;	}	
}

function show(msg)
{
document.getElementById(msg).style.visibility="visible";
document.getElementById(msg).style.display="block";
}

function hide(msg)
{
document.getElementById(msg).style.visibility="hidden";
document.getElementById(msg).style.display="none";
}

// Slider Begin
index=1  //index of image
 noofphotos=5  //total no.of photos
 time=3000 //time interval for slide show,in milli seconds.
 source="img"  //image name without index number
 extension=".jpeg"  //image extension
  
function prevfun(val)
 {
 index=Number(val);
 document.getElementById("bomma").src=source+index+extension
 for (j = 1; j <= noofphotos; j++)
	{
	tmp="icon"+j
 	     if(j==index)
	           document.getElementById(tmp).src="icon2.jpg";
	     else
	         document.getElementById(tmp).src="icon1.jpg";
	}
index=index-1; 
}
 
function showslider()
 {
index=0;  
showfun();
 }
 
function showfun()
{
if(index>=noofphotos)
index=0;

 index=index+1;
 document.getElementById("bomma").src=source+index+extension
for (j = 1; j <= noofphotos; j++)
{
tmp="icon"+j
 	if(j==index)
	document.getElementById(tmp).src="icon2.jpg";
	else
	document.getElementById(tmp).src="icon1.jpg";
}
setTimeout("showfun()",time)
}
// Slider End
</script>

