		<script type="text/javascript" >
/*<![CDATA[*/

	var log = function($selector) {
	    $selector.each(function() {
	        console.log(this);
	    });
	};
	
	function logDataEntry(index, dataEntry) {
		if(dataEntry.type=='productSuite')
		{
			console.log('entry '+index+':');
			console.log('type: '+dataEntry.type);
			console.log('code: '+dataEntry.productCode);
			console.log('name: '+dataEntry.productName);
			console.log('items: '+dataEntry.items);
			console.log('priceFormated: '+dataEntry.priceFormated);
			console.log('productUrl: '+dataEntry.productUrl);
			console.log('imageLocator: '+dataEntry.imageLocator);
			if(dataEntry.productSuiteEntries) {
				// show subentries
				$.each(dataEntry.productSuiteEntries, function(j) {
					var productSuiteProdId = dataEntry.productSuiteEntries[j].productCode;
					console.log('productSuiteEntry('+j+') code: '+productSuiteProdId);
				});
			}
		}
		else
		{
			var prodId = dataEntry.productCode;
			var isBundle = dataEntry.type == 'bundle';
			console.log('entry '+index+':');
			console.log('type: '+dataEntry.type);
			console.log('code: '+prodId);
			console.log('name: '+dataEntry.productName);
			console.log('format: '+dataEntry.formatName);
			console.log('dispatch: '+dataEntry.dispatch);
			console.log('qty: '+dataEntry.quantity);
			console.log('unitPriceFormated: '+dataEntry.unitPriceFormated);
			console.log('productUrl: '+dataEntry.productUrl);
			console.log('imageLocator: '+dataEntry.imageLocator);
			if(dataEntry.variantSize) {
				console.log('variantSize: '+dataEntry.variantSize);
				console.log('variantBaseCode: '+dataEntry.variantBaseCode);
			} else if(isBundle) {
				// show subentries
				$.each(dataEntry.subEntries, function(j) {
					var subProdId = dataEntry.subEntries[j].productCode;
					console.log('subEntry('+j+') code: '+subProdId);
					if(dataEntry.subEntries[j].variantSize) {
						console.log('subEntry('+j+') variantSize: '+dataEntry.subEntries[j].variantSize);
						console.log('subEntry('+j+') variantBaseCode: '+dataEntry.subEntries[j].variantBaseCode);
						console.log('subEntry('+j+') variantSelectVal: '+dataEntry.subEntries[j].variantSelectVal);
					}
				});
			}
		}
	}

	function ajaxFormSubmit() {
		var options = {
			url: '/minicart.html',			// override for form's 'action' attribute
			beforeSubmit: showRequest, // pre-submit callback
			success: processJson, 		// post-submit callback		
		   dataType:  'json',			// dataType identifies the expected content type of the server response 
		}
		// bind form using 'ajaxForm' which prepares the form to be submitted via AJAX by adding all of the necessary event listeners. 
		// It does not submit the form.
		$('#miniCartForm').ajaxForm(options);
		return false;
	}
	
	function ajaxFormSubmitOnChange(input) {
		var $this = $(input);
		var options = {
			url: '/minicart.html',			// override for form's 'action' attribute
			beforeSubmit: showRequest, // pre-submit callback
			success: processJson, 		// post-submit callback
			dataType:  'json',			// dataType identifies the expected content type of the server response 
		}
		var $form = $this.closest('form');
		$form.ajaxSubmit(options);
		// !!! Important !!! 
		// always return false to prevent standard browser submit and page navigation
		return false;
	}
	
	function ajaxFormSubmitSelect() {
		var options = {
			url: '/minicart.html',			// override for form's 'action' attribute
			beforeSubmit: showRequest, // pre-submit callback
			success: processJson, 		// post-submit callback
			dataType:  'json',			// dataType identifies the expected content type of the server response 
		}
		// bind form using 'ajaxSubmit' and immediately submits the form via AJAX.
		$('#miniCartForm').ajaxSubmit(options);
		// !!! Important !!! 
      // always return false to prevent standard browser submit and page navigation 
		return false;
	}
	
	// post-submit callback 
	function processJson(data, statusText, xhr, $form) {
		console.log('status: '+statusText+'\n\ndata: '+JSON.stringify(data));
		updateMiniCart(data);
		
	}
	 
	function updateMiniCart(data) {
		var dataEntriesLength = data.entries.length;
		console.log('quantity: '+data.quantity);
		console.log('entries: '+dataEntriesLength);
		if(dataEntriesLength>0) {
			// 1st check existing content container 
			var contentContainer = $('#miniCartContentContainer');
			if(contentContainer.length==0) {
				// add new content container from markup template (for ProductSuite page only)
				$('#miniCartContentContainerTmpl').clone().insertAfter('#miniCartContainer a.cart-title')
					.attr('id','miniCartContentContainer').addClass('mini-cart').removeClass('mini-cart-tmpl').show();
			}
			// update existing mini cart content
			var markupList = $('#miniCartForm>dl');
			// 2st check size of markup list with JSON data entries list
			if (markupList.length==0 || markupList.length<dataEntriesLength) {
				// add new entry from markup template (for ProductSuite page only)
				$('#miniCartEntryTmpl').clone().appendTo('#miniCartForm').show();
			}
			$('#miniCartContainer a.cart-title').addClass('toggle');
			if($('#miniCartContainer a.cart-title span').length==0)
				$('#miniCartContainer a.cart-title').append($('<span>'));
			$('#miniCartContainer a.cart-title span:first').text(data.quantity);
			
			// show/hide error messages
			if(data.errorMessages) {
				displayErrorMessages($('#miniCartForm'), data.errorMessages);
			} else {
				$('.mini-cart>.msg-error').remove();
			}
			
			$.each(data.entries, function(index) {
				var prodId = data.entries[index].productCode;
				var isBundle = data.entries[index].type=='bundle';
		      logDataEntry(index, data.entries[index]);
				$.each($('#miniCartForm>dl'), function(i) {
					// update new or existing markup list with entries
			      if(index<=i) {
			      	// only check markup i which equals/is greater than index (otherwise already checked)
			      	var toRemove = true;
				      var pattern = 'product_';
				      var sel = i+1;
				      // console.log(this.id);
			      	if (this.id=='miniCartEntryTmpl') {
			      		// create new entry from template
			      		createMiniCartEntry($('#miniCartForm>dl:nth-child('+sel+')'), data.entries[index]);
			      		toRemove = false;
			      		// break out
			      		return false;
			      	} else if(this.id.substring(pattern.length)==prodId && !isBundle) {
				      	updateMiniCartEntry($('#miniCartForm>dl:nth-child('+sel+')'), data.entries[index]);
				      	toRemove = false;
					      // break out
					      return false;
			      	} else if(this.id.substring(pattern.length)==prodId && isBundle) {
			      		// replace existing entry with bundle
			      		replaceMiniCartEntryWithBundle($('#miniCartForm>dl:nth-child('+sel+')'), data.entries[index]);
					      toRemove = false;
					      // break out
					      return false;
			      	} else if (data.entries[index].variantSize && this.id.substring(pattern.length).indexOf(data.entries[index].variantBaseCode)!=-1) {
			      		// checked that markup Id contains variant base code and replace it with variant entry
			      		replaceMiniCartEntryWithVariant($('#miniCartForm>dl:nth-child('+sel+')'), data.entries[index]);
			      		toRemove = false;
			      		// break out
			      		return false;
				      }
				      if(toRemove) {
				      	// remove obsolete markup entry
				      	$(this).remove();
						}
			      }
				});
			});
			
			// NOTE: after all markup entries have been updated with JSON data entries, it could have been that the last markup entry has been removed with the previous request.
			// Which means that data entries lenght is less than markup entries lenght. So we have to remove the last markup entry manually
			if (dataEntriesLength<markupList.length)
			{
				$('#miniCartForm>dl:nth-child('+markupList.length+')').remove();
			}
			
			// update sub total
			$('#miniCartContentContainer p.sub-total span:first').text(data.subTotalFormated);
			
			// finally show updated/created result if not empty and hidden
			if ($('#miniCartContentContainer').is(':hidden')) {
	      	$('#miniCartContentContainer').show();
	      }
		} else {
			// empty cart remove entry and hide elements
			$('.msg-error').remove();
			$('#miniCartForm>dl:first-child').remove();
			$('#miniCartContainer a.cart-title span:first').remove();
			// DO NOT remove click event handler with .off('click')
			$('#miniCartContainer a.cart-title').toggleClass('active-btn');
			$('#miniCartContentContainer').remove();
		}
	}

	function updateMiniCartEntry(markupEntry, dataEntry) {
		if (dataEntry.type=='productSuite') {
			// product suite entry
			$(markupEntry).find('dd.items span:first').text(dataEntry.items);
			$(markupEntry).find('dd.items span:last').text(dataEntry.priceFormated);
		} else {
			// default cart entry with product
			$(markupEntry).find('dd.quantity span:first').text(dataEntry.quantity);
			$(markupEntry).find(':button.add').val(dataEntry.quantity+1);
			$(markupEntry).find(':button.remove').val(dataEntry.quantity-1);
		}
	}
	
	function createMiniCartEntry(markupEntry, dataEntry) {
		var pattern = 'product_';
		var productCode = dataEntry.productCode;
		var productUrl = dataEntry.productUrl;
		$(markupEntry).attr('id', pattern+productCode);
		$(markupEntry).find('dt.product-link a').attr('href', productUrl).find('img').attr('src', dataEntry.imageLocator);
		$(markupEntry).find('dt.title a').attr('href', productUrl).text(dataEntry.productName);
		$(markupEntry).find('dd.dispatch').text(dataEntry.dispatch);
		$(markupEntry).find('dd.items span:last').text(dataEntry.priceFormated);
		$(markupEntry).find('dd.items span:first').text(dataEntry.items);
		$(markupEntry).find(':button.btn-remove').attr('name', 'removeProductSuite_'+productCode);
	}
	
	function replaceMiniCartEntryWithVariant(markupEntry, dataEntry) {
		var pattern = 'product_';
		var productCode = dataEntry.productCode;
   	var productUrl = dataEntry.productUrl;
   	$(markupEntry).attr('id', pattern+productCode);
   	$(markupEntry).find('dt.product-link a').attr('href', productUrl).find('img').attr('src', dataEntry.imageLocator);
   	$(markupEntry).find('dt.title a').attr('href', productUrl).text(dataEntry.productName);
   	var select = $(markupEntry).find('select');
   	select.attr('name', 'AddVariantProduct'+productCode);
   	setSelectByValue(select, productCode);
   	$(markupEntry).find('dd.dispatch').text(dataEntry.dispatch);
   	$(markupEntry).find('dd.quantity span:last').text(dataEntry.unitPriceFormated);
   	$(markupEntry).find('dd.quantity span:first').text(dataEntry.quantity);
   	$(markupEntry).find(':button.add').val(dataEntry.quantity+1).attr('name', 'updateQuantity_'+productCode);
   	$(markupEntry).find(':button.remove').val(dataEntry.quantity-1).attr('name', 'updateQuantity_'+productCode);
	}

	function replaceMiniCartEntryWithBundle(markupEntry, dataEntry) {
		var subPattern = 'sub_';
		var productCode = dataEntry.productCode;
		var productUrl = dataEntry.productUrl;
		$(markupEntry).find('dt.product-link a').attr('href', productUrl);
		$(markupEntry).find('dt.title a').attr('href', productUrl);
		$(markupEntry).find('dd.dispatch').text(dataEntry.dispatch);
		$(markupEntry).find('dd.quantity span:last').text(dataEntry.unitPriceFormated);
		$(markupEntry).find('dd.quantity span:first').text(dataEntry.quantity);
		$(markupEntry).find(':button.add').val(dataEntry.quantity+1).attr('name', 'updateQuantity_'+productCode);
		$(markupEntry).find(':button.remove').val(dataEntry.quantity-1).attr('name', 'updateQuantity_'+productCode);
		// iterate over all subEntries and change selector if necessary
		$.each(dataEntry.subEntries, function(j) {
			var subProdId = dataEntry.subEntries[j].productCode;
			if(dataEntry.subEntries[j].variantSize) {
				var variantSize = dataEntry.subEntries[j].variantSize;
				var variantBaseCode = dataEntry.subEntries[j].variantBaseCode;
				var variantSelectVal = dataEntry.subEntries[j].variantSelectVal;
				var selectId = subPattern+variantBaseCode;
				var select = $(markupEntry).find('select#'+selectId); // .filter($('#'+selectId));
				select.attr('name', variantSelectVal);
				setSelectByValue(select, variantSize);
			}
		});
	}
	
	function setSelectByValue(select, val)	{
   	select.find('option')
   		.attr('selected', false)
   		.filter("[value='"+val+"']")
   		.attr('selected', true);
	}
	
	function displayErrorMessages(target, messages) {
		var container = $('div.msg-error'),
				totalItems = messages.length;
		if (totalItems > 0 ) {
			if (container.length==0) {
				// create new message container
				container = $('<div>').addClass('msg-error').css('white-space','normal');
				$.each(messages, function(i) {
					var msg = messages[i];
					container.append($('<p>').text(msg));
				});
				container.insertBefore(target);
			} else {
				// update existing message container
				container.empty();
				$.each(messages, function(i) {
					var msg = messages[i];
					container.append($('<p>').text(msg));
				});
			}
		}
	}
	
	// pre-submit callback
	function showRequest(formData, jqForm, options) {
		// formData is an array; here we use $.param to convert it to a string to display it
		// but the form plugin does this for you automatically when it submits the data
		var queryString = $.param(formData);

		// jqForm is a jQuery object encapsulating the form element.  To access the
		// DOM element for the form do this:
		// var formElement = jqForm[0];
		// alert('About to submit: \n\n' + queryString);
		
		// here we could return false to prevent the form from being submitted;
		// returning anything other than false will allow the form submit to continue
		return true;
	}
	
	
