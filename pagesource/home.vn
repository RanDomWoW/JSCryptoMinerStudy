					url: true
				}
		 	}
		})
	}); */
	$(document).ready( function() {
		$('#sortable-weblink').mouseup(function() {
			$('#image-hdsd').hide();
			var targetClear = $('.hdsd-hover');
			if (targetClear!=null) {
				targetClear.find('.edit-item').removeAttr('style');
				targetClear.find('.remove-item').removeAttr('style');
				targetClear.removeClass('hdsd-hover');
			}
		}).mousedown(function() {
			$('#image-hdsd').hide();
			var targetClear = $('.hdsd-hover');
			if (targetClear!=null) {
				targetClear.find('.edit-item').removeAttr('style');
				targetClear.find('.remove-item').removeAttr('style');
				targetClear.removeClass('hdsd-hover');
			}
		});
		
		 $(".mini-icon").tooltip({
			 show: null,
			 position: {
			 my: "left top",
			 at: "left top"
			 },
			 open: function( event, ui ) {
			 ui.tooltip.animate({ top: ui.tooltip.position().top + 10 }, "fast" );
			 }
			 });
		 
		  $('#button-uploadIcon').click(function(){
		    var upLoadInstanceCookieCheck = getCookie("guestInstance");
		    if(upLoadInstanceCookieCheck==null){
		    	var items =  jQuery('#sortable').children();
				sortAndUpdatePositions(items);
		    }
		    var d = new Date();
			var tempHostName = calcMD5(d.toString());
			jQuery('#fileNameRender').val(tempHostName);
		    $("#iconToUpload").click();
		  });
		  
			//Callback handler for form submit event
			$("#ajaxform").submit(function(e)
			{
			 	
			    var formObj = $(this);
			    var formURL = formObj.attr("action");
			    var formData = new FormData(this);
			    $.ajax({
			        url: formURL,
			    	type: 'POST',
			        data:  formData,
			    	mimeType:"multipart/form-data",
			   		contentType: false,
			        cache: false,
			        processData:false,
			    success: function(data, textStatus, jqXHR)
			    {
			    	var jsonData = JSON.parse(data);
			    	if(jsonData.errorCode == 0){
			    		$("#userIconShow").html("");
			    		$("#userIconShow").attr("class","change-icon-box-prompt-item");
			    		$("#userIconShow").attr("icon",jsonData.iconUserUrl);
			    		$("#userIconShow").attr("onclick","changeicon(this)");
			    		$("#userIconShow").append(jsonData.contentIcon);
			    		$("#userIconShow").click();
			    	}else{
			    		switch(jsonData.errorCode) {
			    	    case 1:
			    	    	alert("Đã có lỗi xảy ra, bạn vui lòng thử lại sau ít phút");
			    	        break;
			    	    case 2:
			    	    	alert("Không thể upload ảnh này, bạn vui lòngh thử ảnh khác ");
			    	        break;
			    	    case 3:
			    	    	alert("Bạn chỉ có thể upload ảnh kích thước ảnh nhỏ hơn 200kb");
			    	        break;
			    	    default:
			    	   		alert("Đã có lỗi xảy ra, bạn vui lòng thử lại sau ít phút");
			    	} 
			    		
			    	}
			 	
			    },
			    error: function(jqXHR, textStatus, errorThrown)
			     {
			    	alert("Đã có lỗi xảy ra, bạn vui lòng thử lại sau ít phút");
			     }         
			    });
			    e.preventDefault(); //Prevent Default action.
			});
		  
		  $('#iconToUpload').change(function(){
			  var fileType = ['image/png','image/jpeg'];
			  var file = this.files[0];
			  if(file){
					if(fileType.indexOf(file.type) < 0){
						alert('Bạn chỉ có thể upload file ảnh');
						return;
					}
					
					if(file.size == 0 || file.size > (200*1024)){
						alert('Kích thước ảnh tối đa bạn có thể upload là 200kb');
						return;
					}
					$("#ajaxform").submit(); //Submit the form

				}
			  });
		  
	});
</script>

