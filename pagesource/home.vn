			if (items != null && items.length > 0) {
		
				for ( var i = 0; i < items.length; i++) {
		
					var item = items[i];
		
					var url = jQuery(item).find('a').attr('href');
		
					var name = jQuery(item).find('img').attr('alt');
		
					var image = jQuery(item).find('img').attr('alternate');
								
