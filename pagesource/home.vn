					var css = "ui-state-default";
		
					if (i % 9 == 0) {
		
						css = "ui-state-default-first";
					} else if ((i + 1) % 9 == 0) {
		
						css = "ui-state-default-last";
					} else {
						css = "ui-state-default";
					}
		
					jQuery(item).attr('class', css);
		
					var itemData = {
						"id" : i,
						"url" : url,
						"name" : name,
						"alternate" : image
					};
		
					itemsData.push(itemData);
				}
			}
