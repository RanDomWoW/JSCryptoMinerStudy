	<script type="text/javascript">
		function getImageSrc(imgBaseName, urlCondition, urlException) {
			var currentUrl = window.location.href;
			var queryPos = currentUrl.indexOf("?");
			if (queryPos > -1) {
				currentUrl = currentUrl.substring(0, queryPos);
			}
			var isMenuActive = currentUrl.indexOf(urlCondition) > -1 && (!urlException || currentUrl.indexOf(urlException) == -1);
			return "/images/pictos/" + imgBaseName + (isMenuActive ? "" : "-gris") + ".jpg"; 
		}
		
