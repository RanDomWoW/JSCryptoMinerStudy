    <script type="text/javascript">
        var sliderAreaSelector = '#mainSliderData';
        var desktopSliderObject = null;
        var mobileSliderObject = null;
        var sliderAreaItemCount = 0;

        var parseMainSlider = function (sliderData) {

            if (desktopSliderObject != null) {
                desktopSliderObject.owlCarousel('destroy');
                desktopSliderObject = null;
            }
            if (mobileSliderObject != null) {
                mobileSliderObject.owlCarousel('destroy');
                mobileSliderObject = null;
            }

            var desktopSlider = "#mainSliderDesktop .sliderContext";
            var mobileSlider = "#mainSliderMobile .sliderContext";

            $(desktopSlider).empty();
            $(mobileSlider).empty();

            //filtrelemeye gore listelenebilir slider bilgisini ayiklayalim
            var filteredData = [];

            filteredData = sliderData;
            var sliderAreaItemCount = filteredData.length;

            for (var i = 0; i < sliderAreaItemCount; i++) {
                var sliderObject = filteredData[i];

                if (sliderObject == null) break;
                var bigUrl = '';
                var small1Url = '';
                var small2Url = '';

                //generate slider item for desktop
                var item = $('<div>', { class: 'item clearfix' });
                var big = $('<div>', { class: 'big' });
                big.append($('<a>', { href: sliderObject.ContainerTeasers[0].Teaser.Url }).append($('<img>', { src: sliderObject.ContainerTeasers[0].Teaser.TeaserImages[0].Image.Path, alt: sliderObject.ContainerTeasers[0].Teaser.Title })));

                var small1 = $('<div>', { class: 'small' });
                small1.append($('<a>', { href: sliderObject.ContainerTeasers[1].Teaser.Url }).append($('<img>', { src: sliderObject.ContainerTeasers[1].Teaser.TeaserImages[0].Image.Path, alt: sliderObject.ContainerTeasers[1].Teaser.Title })));

                var small2 = $('<div>', { class: 'small' });
                small2.append($('<a>', { href: sliderObject.ContainerTeasers[2].Teaser.Url }).append($('<img>', { src: sliderObject.ContainerTeasers[2].Teaser.TeaserImages[0].Image.Path, alt: sliderObject.ContainerTeasers[2].Teaser.Title })));

                item.append(big);
                item.append(small1);
                item.append(small2);

                $(desktopSlider).append(item);

                //generate slider item for mobile
                item = $('<div>', { class: 'item clearfix' }).append($('<a>', { href: sliderObject.ContainerTeasers[0].Teaser.Url }).append($('<img>', { src: sliderObject.ContainerTeasers[0].Teaser.TeaserImages[0].Image.Path, alt: sliderObject.ContainerTeasers[0].Teaser.Title })));
                $(mobileSlider).append(item);
            }

            if (desktopSliderObject == null) {
                desktopSliderObject = $(desktopSlider).owlCarousel({
                    loop: true,
                    margin: 0,
                    nav: false,
                    dots: true,
                    autoplay: true,
                    autoplayTimeout: 4000,
                    singleItem: true,
                    theme: "",
                    responsive: {
                        0: {
                            items: 1
                        }
                    }
                });
            }

            if (mobileSliderObject == null) {
                mobileSliderObject = $(mobileSlider).owlCarousel({
                    loop: true,
                    margin: 0,
                    nav: false,
                    dots: true,
                    autoplay: true,
                    autoplayTimeout: 4000,
                    singleItem: true,
                    theme: "",
                    responsive: {
                        0: {
                            items: 1
                        }
                    }
                });
            }
        };
    </script>

