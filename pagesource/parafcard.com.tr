    <script type="text/javascript">
        $(document).ready(function () {

            var cardsAreaSelector = '.mainKartlarSlider';

            var cardsSliderObject = $(cardsAreaSelector).owlCarousel({
                responsive: {
                    0: {
                        items: 1
                    },
                    700: {
                        items: 2
                    },
                    1024: {
                        items: 3
                    },
                    1200: {
                        items: 4
                    }
                },
                nav: true,
                dots: false,
                rewindNav: false
            });

            $(cardsAreaSelector).parent().find('.next').click(function (e) {
                e.preventDefault();
                cardsSliderObject.trigger('owl.next');
            });
            $(cardsAreaSelector).parent().find('.prev').click(function (e) {
                e.preventDefault();
                cardsSliderObject.trigger('owl.prev');
            });

        });

        function HideCards() {
            $("#mainKartlar").hide();
            $("#commonSectionTitleCards").hide();
        }

        function ShowCards() {
            $("#mainKartlar").show();
            $("#commonSectionTitleCards").show();
        }

    </script>


