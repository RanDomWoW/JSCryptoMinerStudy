	<script>	
	
	function isValidNID(NID) {
		var pattern = /^([a-zA-Z]{1}[0-9]{8}[a-zA-Z]{1})$/;
		return pattern.test(NID);
	};
	
	function isValidEmailAddress(emailAddress) {
		var pattern = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
		return pattern.test(emailAddress);
	};
	
	$(document).ready(function(){
		
		$('.text-input').focusout(function() {
			var id = $(this).attr('id');
			var text = $(this).val();
			if(text == "" && $("#" + "err_" + id).length == 0)
			{
				$(this).after("<span id='err_" + id +"' style='color:red;font-weight:bold;'>Kjo fushë është e detyrueshme!<br></span>");				
			}
			
			if(text != "")
			{
				$("#" + "err_" + id).remove();
			}
		});
		
		var password = document.getElementById("TFjalekalimi");
		var confirm_password = document.getElementById("TFjalekalimiPerseri");
		
		var secret_answer = document.getElementById("TPergjigje");
		
		function validateAnswerLength()
		{
			if(secret_answer.value.length < 3)
			{
				secret_answer.setCustomValidity("Minimumi 3 karaktere!");
			}
			else
			{
				secret_answer.setCustomValidity("");
			}
		}
		
		secret_answer.onchange = validateAnswerLength;
		secret_answer.onkeyup = validateAnswerLength;

		function validatePassword(){
		  if(password.value != confirm_password.value) {
			confirm_password.setCustomValidity("Fjalëkalimet nuk janë të njëjtë!");
		  } else {
			confirm_password.setCustomValidity('');
		  }
		}

		password.onchange = validatePassword;
		confirm_password.onkeyup = validatePassword;
		
		
		
		$('#TMail').focus(function(){
			var id = $(this).attr('id');
			$("#" + "errm_" + id).remove();
		}).focusout(function() {
			var id = $(this).attr('id');
			if($(this).val() != "" && !isValidEmailAddress( $(this).val() ) ) 
			{
				$(this).after("<span id='errm_" + id +"' style='color:red;font-weight:bold;'>Emaili nuk është i saktë!<br></span>"); 
			}
		});
		
		$('#TNidUser').focus(function(){
			var id = $(this).attr('id');
			$("#" + "errn_" + id).remove();
		}).focusout(function() {
			var id = $(this).attr('id');
			if( $(this).val() != "" && !isValidNID( $(this).val() ) ) 
			{
				$(this).after("<span id='errn_" + id +"' style='color:red;font-weight:bold;'>NID nuk është i saktë!<br></span>"); 
			}
		});
				
	});
	
	function validateRegisterForm()
	{
		var isValid = true;
		if(!isValidNID($("#TNidUser").val()))
		{
			isValid = false;
		}
		if(!isValidEmailAddress($("#TMail").val()))
		{
			isValid = false;
		}
		if($("#TCelInput").val() == "")
		{
			isValid = false;
		}
		if($("#TPergjigje").val() == "")
		{
			isValid = false;
		}
		if($("#TEmriUser").val() == "")
		{
			isValid = false;
		}
		if($("#TAtesiaUser").val() == "")
		{
			isValid = false;
		}
		if($("#TMbiemriUser").val() == "")
		{
			isValid = false;
		}
		if($("#TDatelindjaUser").val() == "")
		{
			isValid = false;
		}
		
		var password = document.getElementById("TFjalekalimi");
		var confirm_password = document.getElementById("TFjalekalimiPerseri");
		if(password.value != confirm_password.value) {
			isValid = false;
		}
		
		if(isValid)
		{
			$("#Button2").val("Duke u procesuar...");
			__doPostBack('Button2', '');
			return true;
		}
		else
		{
			return false;
		}
	}
	
		
	</script>
