	/*margin-top: 40px;*/
	margin-bottom: 5px;
	float: left;
	width: 100%;
}
div.kelkoo-product { 
	text-align: center;
	padding: 10px;
	box-sizing: border-box;
	border: 1px solid #eee;
	position: relative;
}
.kelkoo-widget .link-box {
	display: block;
	position: absolute;
	left: 0;
    top: 0;
	z-index: 10;
	width: 100%;
	height: 100%;
}
.kelkoo-widget div.title {
	font-size:small;
	color: #000;
	font-weight: 700;
	font-size: 0.75em;
	line-height: 1.35em;
	margin: 8px 0;
}
.kelkoo-widget div.price {
	color: #999;
	font-size: 20px;
	background-color: #f9f9f9;
	border: 1px solid #f1f1f1;
}
.kelkoo-widget .btn-call-action {
	/*padding: 5px 6px;
	background-color: #ff6d40;
	color: #fff;
	font-size: 13px;
	letter-spacing: 1px;*/
	margin-top: 8px;
	box-shadow: 0 0 5px 1px rgba( 0,0,0,0.15 );
	line-height: 1.2em;
	
	background-color: #f5c056;
    color: #000;
    padding: 8px 10px;
    display: inline-block;
    font-size: 14px;
    font-weight: normal;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    -khtml-border-radius: 3px;
    border-radius: 3px;
}
.kelkoo-product:hover .btn-call-action,
.kelkoo-product:focus .btn-call-action	{ 
	background-color: #fc6;
}
.kelkoo-widget .block-price {
	min-height: 61px;
}
.kelkoo-widget {
	overflow: hidden;
}
.kelkoo-widget .price_rebate .strike-rebate {
    font-size: 90%;
    margin-right: 5px;
    text-decoration: line-through;
}
.kelkoo-widget .price_rebate .rebate {
    font-size: 120%;
	color: #ff6d40;
}
@media (min-width: 480px) {
	div.kelkoo-product { 
		width: 50%;
		float: left;
		min-height: 220px;
	}
	.kelkoo-widget div.title {
		min-height: 73px;
	}
}
@media (min-width: 768px) {
	div.kelkoo-product { 
		width: 25%;
	}
}
@media (max-width: 479px) {
	.kelkoo-widget div.title {
		margin-bottom: 10px;
	}
}

.modulo-contatti legend ,
