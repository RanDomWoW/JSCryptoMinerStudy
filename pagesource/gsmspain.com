function modal_load(titulo,url,idmodal){

	idmodal=idmodal || "modal_source";

	$('#'+idmodal+' .modal-title').html(titulo);
	$('#'+idmodal+' .modal-body').html('<br><i class="fa fa-spinner fa-pulse fa-5x"></i><br><br>Cargando');
	$('#'+idmodal+' .modal-body').addClass('text-center');
	$('#'+idmodal+'').addClass('fv-modal-stack');//para zindex
		$('#'+idmodal+'').modal({show:true});
	$('.modal-backdrop').addClass('fv-modal-stack');//para zindex

	setTimeout(function(){
		$('#'+idmodal+' .modal-body').load(url,function() {
			$('#'+idmodal+' .modal-body').removeClass('text-center');
		});
	}, 1000);
}
</script>
