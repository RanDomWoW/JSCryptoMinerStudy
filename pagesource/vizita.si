<script language="javascript">
	var ticker_interval = false;
	
	function do_ticker()
	{
		var ticker_left_offset = 75;
		
		$j(".ticker_item").each(
			function(index,elt) {
				$j(elt).css("left", ticker_left_offset);
				$j(elt).fadeIn(300);
				ticker_left_offset += $j(elt).outerWidth(true);
			}
		);

		ticker_interval = setInterval('ticker_start()', 4000);
	}
	
	function ticker_start()
	{
		// hide first item
		ticker_first_item = $j(".ticker_item").first().html();
		
		$j(".ticker_item").first().fadeOut(500, ticker_remove_first);
	}
	
	function ticker_remove_first()
	{
		var obj = $j(".ticker_item").first();
		$j(".ticker_item").first().find("a").removeClass("selected");
		
		var pos = $j(".ticker_item").last().outerWidth(true) + $j(".ticker_item").last().position().left + 10;

		$j(".ticker_holder").append($j(".ticker_item").first());
		$j(".ticker_item").first().find("a").addClass("selected");

		obj.css("left", pos)
		obj.css("display", "");
		
		ticker_move_items();
	}
	
	function ticker_move_items()
	{
		var ticker_left_offset = 75;
		
		$j(".ticker_item").each(
			function (index, elt) {
				$j(elt).animate({left: ticker_left_offset},1000);
				ticker_left_offset += $j(elt).outerWidth(true);
				
				if (index > 0)
					ticker_left_offset += 10;
			}
		);
	}
	
	function ticker_enter()
	{
		if ( ticker_interval )
			clearInterval(ticker_interval);
			
		ticker_interval = false;
	}
	
	function ticker_leave()
	{
		if ( ticker_interval )
			return;
			
		ticker_interval = setInterval('ticker_start()', 4000);
	}
	
	$j(document).ready( 
		function() { 
			$j(".ticker").mouseenter(ticker_enter);
			$j(".ticker").mouseleave(ticker_leave);
		
			do_ticker(); 
		}
	);
</script>

