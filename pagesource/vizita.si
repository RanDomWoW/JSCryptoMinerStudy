/**
 * This code builds onmouseover menu on the fly,
 * if any LI element has REL=menu
 * @author Matej Balantič <matej.balantic@pop-tv.si>
 */
delayTime = 100;		// in miliseconds
effectTime= 300;		// in milisecond
window.addEvent("domready", function()
{
	document.getElements("li[rel=menu]").each( function (el)  {

		// prepare effects
		el.doFx = new Fx.Styles(el.getElements("ul")[0], {duration:effectTime, wait:false});
		
		// set opacity = 1
		el.getElements("ul")[0].setStyle("opacity",0);



		// click on anchor
		el.getElements("a[rel=menu]")[0].addEvent('click', function (e)
		{
			e = new Event(e).stop();
			el.blur();
		});


		// mouse over
		el.addEvent('mouseover', function (e)
		{
			el.getElements('ul')[0].setStyle("display","block");

			//cancel possible delay
			if (typeof (el.doDelay) !== "undefined")
				el.doDelay = $clear(el.doDelay);

			el.doFx.start({
				opacity:1 
			});
		});

		// mouse out
		el.addEvent('mouseout', function (e)
		{
			// delay mouse out
			el.doDelay = ( function()  {

				el.doFx.start({
					opacity: 0
				});

				el.getElements('ul')[0].setStyle("display","none");

			}).delay(delayTime);
		});


	});

});
</script>

