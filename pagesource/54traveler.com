<script type="text/javascript">
    $(document).ready(function () {
        $.slidebars();
        $('.carousel').carousel();
        $("[data-toggle='popover']").popover();

        //返回顶部
        $("#backToTop").hide();
        $(window).scroll(function(){
            $('#fixed-container').fadeIn('slow');
            if ($(window).scrollTop() > 1000){
                $("#backToTop").fadeIn(300);
            }else{
                $("#backToTop").fadeOut(300);
            }
        });
        $("#backToTop").click(function(){
            $('body,html').animate({scrollTop:0},900);
            return false;
        });
    });

    // 查看更多
    $("#whyUsOpenNew").click(function(){
        $("#whyUsContent").slideDown('slow');
        $("#whyUsOpenNew").hide();
    });
    $("#whyUsClose").click(function(){
        $("#whyUsContent").slideUp('slow');
        $("#whyUsOpenNew").fadeIn('slow');
    });

    // 手机下拉菜单
    function mobile_dropdown(item){
        $("#" + item).slideToggle();
    }

    // 顶部视频播放暂停
    function playVideo(id) {
        $this = document.getElementById(id);
        $('.fenmian').hide();
        if( $(window).width() < 768 ){
            $('video').css('height','auto');
            $('.video-box').fadeIn('slow');
        }else{
            $('video').css('height',$(window).height());
            $('.video-box').slideDown('slow');
            $('#fixed-container').hide();
            $('#menubar').hide();
            $('.carousel-control').hide();
        }
        $this.play();
        $('.playPause').show();
        $this.onended = function(){
            $('.fenmian').fadeIn("slow");
            $('.video-box').hide();
            $('.playPause').hide();
            $('#menubar').show();
        }
    }

    function stopVideo(id) {
        $this = document.getElementById(id);
        $this.pause();
        $('.fenmian').fadeIn("slow");
        $('.video-box').hide();
        $('.playPause').hide();
        $('#menubar').show();
        $('#fixed-container').show();
        $('.carousel-control').show();
    }

    function stopAll(){
        $('.video').each(function(){
            var id = $(this).attr('id');
            $this = document.getElementById(id);
            $this.pause();
        })
    }
</script>
