}

/* Header: ADS:  */ 
.header .header-adv a, .header .header-adv { 
  line-height: 0; display: inline-block; min-width: 73%;
}

/* primary menu */ 
.navigation-dark ul#primary-menu > li a { 
  padding: 0 18px; color: #ffcc00; 
