}
ul#primary-menu > li.current_page_item > a, .navigation-dark ul#primary-menu li.current-menu-item > a, .navigation-dark ul#primary-menu > li.current-menu-parent > a, .navigation-dark ul#primary-menu > li:hover > a { 
