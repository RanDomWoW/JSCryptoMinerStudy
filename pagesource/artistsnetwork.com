        form += '</div><div class="overlay-capture-form">';

        if (offerData.hidden_form && offerData.hidden_form === 'Y') {
          form += '<div style="margin: 0.5em auto" class="text-centered"><a class="fw-overlay-nothanks">Close</a></div>';

        } else {

          form += '<div class="form-group"><label for="fw-campaign-email-input">Enter Your Email Address</label> <div class="error-message">Please enter a valid email</div><input id="fw-campaign-email-input" type="email" class="form-control required" placeholder="your@email.com" name="email"></div><div class="form-group"><input type="submit" class="btn btn-primary fw-overlay-submit" value="' + ( fw_capture.btnLabel || "Sign Up" )+ '"><div style="margin: 0.5em auto" class="text-centered"><a class="fw-overlay-nothanks">No Thanks</a></div><p class="small"><em>' + fw_capture.disclaimer + '</em></p></div></div><div id="fw-capture-form-thanks" class="form-group"><h4>Thank you!</h4><p>' + offerData.thanks_page + '</p><a class="btn btn-default fw-overlay-cancel">Done</a></div>'; 

        }

        form += '</div></form>';
        
        fw_capture.overlay.innerHTML = form;

        document.body.appendChild(fw_capture.overlay);

        fw_capture.activeOverlay = jQuery(document.getElementById('fw-overlay-modal')).on('click', fw_capture.handleClick);

        if (offerData.hidden_form && offerData.hidden_form === 'Y') {
          fw_capture.activeOverlay.addClass('no_form');
        }

        window.setTimeout(function() { fw_capture.activeOverlay.addClass('fadein'); }, 100);

        setCookie('fw_capture_seen', new Date(), 30);
      }

      fw_capture.closeModal = function() {
        if (fw_capture.activeOverlay.is('.no_form')) {
          fw_capture.activeOverlay.removeClass('no_form');
        } else if (!fw_capture.activeOverlay.is('.submitted')) {
          fw_capture.no_thanks(fw_capture.activeOverlay.data('interest-name'));
        }
        // enable scrolling again
        jQuery('body').removeClass('noscroll');
        fw_capture.activeOverlay.removeClass('fadein').addClass('fadeout');
        window.setTimeout(function() {
          fw_capture.activeOverlay.parent().get(0).removeChild(fw_capture.activeOverlay.get(0));
          delete fw_capture.activeOverlay;
        }, 500);
      }

      fw_capture.handleClick = function(evt) {
        var target = jQuery(evt.target);
        console.log('handleClick', target);
        if (target.is('.fw-overlay-cancel, .fw-overlay-close, .fw-overlay-nothanks')) {
          fw_capture.closeModal();
        } 
      }

      fw_capture.fwGetCookie = function(cname) {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for(var i = 0; i <ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                        c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                        return c.substring(name.length, c.length);
                }
        }
        return "";
      }

      // no thanks: WPR-64
      var nothanks_interests = ['spinning','beading','weaving','knitting','crochet','jewelry'];
      fw_capture.no_thanks = function(interest) {
        if (!interest) return;
        interest = interest.toLowerCase();
        if (nothanks_interests.indexOf(interest) > -1) {
          window.location.href = '/free-' + interest + '-downloads/';
        }
      }

      // Tell the ESB I am registering
      fw_capture.notifyESB = function(email,campaign,capture) {
        if (!window._fw) {
          console.log('no _fw, skipping ESB');
          return;
        }
        var obj = _fw.lib.get_utm_vars();
        _fw.lib.set_email(email);
        _fw.lib.set_campaign(campaign);
        _satellite.track("fw_email_capture");
        return jQuery.ajax({
          url: 'https://' + window.location.hostname + '/store/esbcore/api/updateUser',
                method: 'POST',
                crossDomain: true,
                xhrFields: {
                  withCredentials: true
                },
                data: {
                  auth0_email: email,
                        dtm_mage_id: fw_capture.fwGetCookie('fw_mage_id'),
                        dtm_auth_id: fw_capture.fwGetCookie('fw_auth_id'),
                        dtm_session_id: _fw.lib.get_session_id(),
                        dtm_visitor_id: _fw.lib.get_visitor_id(),
                        dtm_get_implied_visitor_interests: _fw.lib.get_single_implied_visitor_interest(),
                        dtm_utm_campaign: campaign || obj['utm_campaign'],
                        dtm_utm_medium: obj['utm_medium'],
                        dtm_utm_source: obj['utm_source'],
                        dtm_utm_term: obj['utm_term'],
                        dtm_utm_content: obj['utm_content'],
                        email_optin_yes: "on",
                        capture: capture || 'unknown',
                        method: "quick_capture"
                }
         })
      }

      fw_capture.captureForm = function(frm) {
        frm = jQuery(frm);
        var email = frm.children('input[type="email"]').val();
        var utm_campaign = frm.children('input[name="utm_campaign"]').val();
        if (fw_capture.testEmail(email)) {
          // serialize the form
          var d = frm.serialize();
          jQuery.post(fw_capture.submissionUrl, d).fail(function() {
            console.log('capture error', arguments);
          })
          fw_capture.notifyESB(email,utm_campaign,'dl_inline');
          frm.addClass('sent').siblings('.fw_capture_thanks').addClass('sent');
        } else {
          console.log('invalid email');
        }
        return false;
      }

      fw_capture.testEmail = function(email) {
        return email_pattern.test(email);
      }

      // legacy function, tied directly to #fw-campaign-email-input
      fw_capture.submitForm = function() {
      	var offerurl = '';
      	if( offerurl.length > 0 )
      	{
      		var capture = "dl_modal";
		var campaign = offerurl;
      	} else {
      		var capture = "nl_modal";
		var campaign = "";
      	}

        var email = document.getElementById('fw-campaign-email-input').value;
        fw_capture.activeOverlay.removeClass('form-error');
        if (!fw_capture.testEmail(email)) {
          fw_capture.activeOverlay.addClass('form-error');
          return false;
        } 
        window.setTimeout(function() {
          fw_capture.activeOverlay.addClass('submitted');
        },100);

        fw_capture.notifyESB(email,campaign,capture).done(function(resp) {
         	if (resp == 'error') { alert('Whoops, there seems to be an issue signing up for our newsletter. Please try again later'); fw_capture.activeOverlay.addClass('form-error');}
         });

        return true;
      }

      window.fw_capture = fw_capture;
    })(window)
    </script>
