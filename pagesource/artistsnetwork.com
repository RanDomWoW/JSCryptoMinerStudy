                                            <div>
                                                <span
                                                    class="after_special {{#price.USD.default_original_formated}}promotion{{/price.USD.default_original_formated}}">
                                                    {{price.USD.default_formated}}
                                                </span>

                                                {{#price.USD.default_original_formated}}
                                                    <span class="before_special">
                                                        {{price.USD.default_original_formated}}
                                                    </span>
                                                {{/price.USD.default_original_formated}}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="result-description text-ellipsis">{{{ _highlightResult.description.value }}}</div>

			    {{#isAddToCartEnabled}}
                                {{#in_stock}}
				    <form action="#" class="form-addtocart" onsubmit="event.preventDefault(); if( '{{type_id}}'.indexOf('aw_giftcard2') === -1 && '{{type_id}}'.indexOf('configurable') === -1 && '{{type_id}}'.indexOf('grouped') === -1 && '{{type_id}}'.indexOf('bundle') === -1 && '{{subscription}}' == 'No'){ajaxLocationAjax(this,'/store/checkout/cart/add/product/{{objectID}}/sku/{{sku}}/defaultQty/1/','{{objectID}}');window.ajaxGetMinicart();if (typeof _fw != 'undefined' ){var products = ';{{sku}};1;{{price.USD.default}}';_fw.lib.cart_add( products );} else {console.error('F+W Analytics Library Not Loaded! ... Please refresh webpage.');}}else{window.location.href='{{url}}';}">
                                        <input type="hidden" name="qty" value="1">
