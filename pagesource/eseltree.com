<script language="JavaScript">
function showtopmenu(pid) { document.getElementById("topmenu1"+pid).style.display="block"; document.getElementById("menusub"+pid).style.display="block"; }
function hidetopmenu(pid) { document.getElementById("topmenu1"+pid).style.display="none"; document.getElementById("menusub"+pid).style.display="none"; }
function menushow(pid) { document.getElementById(pid).style.display="block"; }
function menuhide(pid) { document.getElementById(pid).style.display="none"; }
function gotolist(codes) { document.location.href="/front/productlist.php?code="+codes; }
function gotoblist(brands,codes) { document.location.href="/front/productblist.php?brandcode="+brands+"&code="+codes; }
function bookmarksite(title,url) { 
   // Internet Explorer
   if(document.all)
   {
  window.external.AddFavorite(url, title); 
   }
   // Google Chrome
   else if(window.chrome){
 alert("Ctrl+D키를 누르시면 즐겨찾기에 추가하실 수 있습니다.");
   }
   // Firefox
   else if (window.sidebar) // firefox 
   {
  window.sidebar.addPanel(title, url, ""); 
   }
   // Opera
   else if(window.opera && window.print)
   { // opera 
 var elem = document.createElement('a'); 
 elem.setAttribute('href',url); 
 elem.setAttribute('title',title); 
 elem.setAttribute('rel','sidebar'); 
 elem.click(); 
   }
} 	
function SetCookie(cKey, cValue, cSec) {
	var d = new Date();
	if(cSec>0) {
		d.setTime(d.getTime() + (cSec*1000));
		document.cookie = cKey + '=' + escape(cValue) + ';path=/;expires='+ d.toGMTString();
	} else {
		document.cookie = cKey + '=' + escape(cValue) + ';path=/;';		
	}
}
function GetCookie(cKey) {
	var allcookies = document.cookie;
	var cookies = allcookies.split("; ");
	for (var i = 0; i < cookies.length; i++) {
		var keyValues = cookies[i].split("=");
		if (keyValues[0] == cKey) {
			return unescape(keyValues[1]);
		}
	}
	return "";
}
function getCookie(c_name) {
	cookie = document.cookie;
	index = cookie.indexOf(c_name + "=");
	if (index == -1) return "";
	index = cookie.indexOf("=", index) + 1;
	var endstr = cookie.indexOf(";", index);
	if (endstr == -1) endstr = cookie.length;
	return unescape(cookie.substring(index, endstr));
}
function topbannerhide() {
	//document.getElementById("topBanner").style.display = "none";
	SetCookie("topBanner","block",0);
	document.location.reload();
}
function TopSearchCheck() {
	try {
		if(document.search_tform.search.value.length==0) {
			alert("상품 검색어를 입력하세요.");
			document.search_tform.search.focus();
			return;
		}
		document.search_tform.submit();
	} catch (e) {}
}
function CheckKeyTopSearch() {
	key=event.keyCode;
	if (key==13) {
		TopSearchCheck();
	}
}	
if(GetCookie("topBanner")=="") document.getElementById("topBanner").style.display = "block";
</script>
