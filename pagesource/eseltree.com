<script type="text/javascript" src="/upload/mainslider/jquery-1.7.2.min.js"></script>
<script>
$.fn.mainbanner = function(options) {
	var config = $.extend(
		{
		   banner		: this.find("dd")
		   ,length		: this.find("dd").length
		   ,duration	: 4000
		   ,speed		: 500
		   ,tabBtn		: this.find("dt")	
		},options);

	var current_idx;		// 현재롤링idx
	var next_idx;			// 다음롤링idx
	var timer;				// 타이머
	var current_idx = 0;

	// 초기실행
	config.banner.each(function(){
		$(this).attr("style","opacity:0");
		if($.browser.version == "6.0" || $.browser.version == "7.0" || $.browser.version == "8.0") {
			$(this).attr("style","filter:alpha(opacity=0)");
		}
	});
	config.banner.eq(current_idx).attr("style","opacity:1");
	config.tabBtn.eq(current_idx).addClass('current');

	return this.each(function(){
		rollingPlay();
		config.banner.on('click', function(){
			document.location.href = config.banner.eq(current_idx).find('a').attr('href');
			return false;
		});			
		config.tabBtn.on('click', function(){
			document.location.href = config.banner.eq(current_idx).find('a').attr('href');
			return false;
		});	
		config.banner.on('focusin mouseenter', function(){
			rollingStop();
		});		
		config.banner.on('focusout mouseleave', function(){
			rollingPlay();
		});	
		if(config.tabBtn != '') {
			config.tabBtn.on('focusin mouseenter', function(){
				tab_idx = config.tabBtn.index($(this));
				rollingStop();
				changeImage(current_idx, tab_idx);
				//rollingPlay();
			});
			config.tabBtn.on('focusout mouseleave', function(){
				//tab_idx = config.tabBtn.index($(this));
				//rollingStop();
				//changeImage(current_idx, tab_idx);
				rollingPlay();
			});
		}

		function rollingPlay() {
			timer = setInterval(loop, config.duration);
		}
		function rollingStop() {
			clearInterval(timer);
		}		
		function loop() {
			next_idx	= current_idx + 1;
			if(next_idx >= config.length) next_idx = 0;
			changeImage(current_idx, next_idx);
		}

		function changeImage(tmp_idx1, tmp_idx2) {
			if(tmp_idx1 == tmp_idx2) return;
			config.tabBtn.removeClass('current');
			config.tabBtn.eq(tmp_idx2).addClass('current');
			config.banner.eq(tmp_idx1).animate({"filter": "alpha(opacity=0)",	"opacity":0}, config.speed);
			config.banner.eq(tmp_idx2).animate({"filter": "alpha(opacity=100)","opacity":1}, config.speed);
			current_idx = tmp_idx2;
		}
	});
}
$.fn.brandbanner = function(options) {
	var config = $.extend(
		{
		   banner		: this.find("dd")
		   ,length		: this.find("dd").length
		   ,duration	: 4000
		   ,speed		: 500
		   ,prev		: this.find("dt.prev")	
		   ,next		: this.find("dt.next")	
		},options);

	var current_idx;		// 현재롤링idx
	var next_idx;			// 다음롤링idx
	var timer;				// 타이머
	var current_idx = 0;

	// 초기실행
	config.banner.eq(current_idx).css("left","4px");

	return this.each(function(){
		rollingPlay();
				
		config.prev.on('click', function(){
			rollingStop();
			next_idx	= current_idx - 1;
			if(next_idx < 0) next_idx = config.length - 1;
			changeImage2(current_idx, next_idx);
			rollingPlay();
		});
		config.next.on('click', function(){
			rollingStop();
			loop();
			rollingPlay();
		});			
		config.banner.on('focusin mouseenter', function(){
			rollingStop();
		});	
		config.banner.on('focusout mouseleave', function(){
			rollingPlay();
		});	
		function rollingPlay() {
			timer = setInterval(loop, config.duration);
		}
		function rollingStop() {
			clearInterval(timer);
		}		
		function loop() {
			next_idx	= current_idx + 1;
			if(next_idx >= config.length) next_idx = 0;
			changeImage1(current_idx, next_idx);
		}
		function changeImage1(tmp_idx1, tmp_idx2) {
			if(tmp_idx1 == tmp_idx2) return;
			config.banner.eq(tmp_idx2).css('left','1000px');
			config.banner.eq(tmp_idx1).animate({"left": "-1000px"}, config.speed);
			config.banner.eq(tmp_idx2).animate({"left": "4px"}, config.speed);
			current_idx = tmp_idx2;
		}
		function changeImage2(tmp_idx1, tmp_idx2) {
			if(tmp_idx1 == tmp_idx2) return;
			config.banner.eq(tmp_idx2).css('left','-1000px');
			config.banner.eq(tmp_idx1).animate({"left": "1000px"}, config.speed);
			config.banner.eq(tmp_idx2).animate({"left": "4px"}, config.speed);
			current_idx = tmp_idx2;
		}
	});
}
