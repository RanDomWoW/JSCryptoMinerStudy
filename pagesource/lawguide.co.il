					remove_value_on_focus(inputs[i]);
				}
			});
			function remove_value_on_focus(el){
				var value_holder = el.value;
				el.addEventListener('focus',function(){
					if(this.value !== "" && this.value !== value_holder) return;
					value_holder = this.value;
					this.value = "";
				});
				el.addEventListener('blur',function(){
					if(this.value === ""){
						this.value = value_holder;
					}
				});
			}
		})();
	</script>
