              $('#ZOOM').trigger('reposition');
          });
        }
  });
  return(false);
}
function openDettaglioITA($codlib,event){
  if(!event) { var event = window.event; }
  if( event){var ele = event.target || event.srcElement;}

  jQuery('#ZOOM').html('<br><br><img src="/images/in_attesa.gif" width="75">');

  $('#ZOOM').lightbox_me({
        centered: true, 
        onLoad: function() { 
            $('#ZOOM').load('/dettaglioZOOMB.php?codlib='+$codlib, function() {
              $('#ZOOM').trigger('reposition');
          });
        }
  });
  if(event && false){ 
    $("#dettaglioZOOM").css("position","absolute");
    $("#dettaglioZOOM").css("top",event.pageY+5);
    $("#dettaglioZOOM").css("left",event.pageX+5);
  }
  return(false);
}
