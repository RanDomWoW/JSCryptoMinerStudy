                alert('Please enter your email first!');
                return false;
            }
            if ($('#btn_submit_first_visit').text() == 'PROCESSING...') {
                return false;
            }
            $('#btn_submit_first_visit').text('PROCESSING...');
            $.ajax({
                url: $('#form_for_first_visitor').attr('action'),
                type: $('#form_for_first_visitor').attr('method'),
                data: $('#form_for_first_visitor').serialize(),
                dataType: 'json',
                success: function (data) {
                    var json = data;//$.parseJSON(data);
                    if (json.status == 1 || json.status == "1") {
                        $('.first-visit-inner').html('<div class="first-visit-thanks"><h2 class="first-visit-title">Thank you!</h2><p class="first-visit-success"><b class="green">YOUR CODE:<span>' + json.coupon_code + '</span></b></p><p style="maring-top:15px;">Only valid for 7 days, so hurry to choose your glasses <a href="https://www.firmoo.com/z/buy-one-get-one-free.html">HERE</a></p></div>');
                    }
                    else {
                        alert(json.msg);
                        $('#btn_submit_first_visit').text(fvButtonOriginalText);
                    }
                }
            });
            return false;
        });

    })
</script>
