    <script type="text/javascript">
        $(document).ready(function () {
            $('.color-li').click(function () {
                var source = $(this);
                if (source.hasClass("active")) return false;
                source.parentsUntil('.p-item').find('li').removeClass('active');
                source.addClass('active');
                var targetA = source.parentsUntil('.p-item').siblings('a.pro-image');
                targetA.find('img').css("opacity", "0.2");
                targetA.attr('href', source.attr('data-href'));
                targetA.attr('data-s-img-hover', source.attr('data-s-img-hover'));
                targetA.find('img').attr('src', source.attr('data-image'));
                targetA.find('img').load(function () {
                    targetA.find('img').animate({opacity: 1}, 300);
                })
            });
            $("#customer_testimonials_container").html('<div id="customer_testimonials" data-load_URL="ajax.php?action=33&pid=&limit=20&recommend=Y"></div>');
            $('#subscribeNew_1').submit(function () {
