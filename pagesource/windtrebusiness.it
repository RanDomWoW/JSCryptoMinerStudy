			bgColorBox_06		: dark gray box background
			noBgColorBox		: transparent box background
			
			expandBoxWidth		: expand box content width to 50% of module width (when itemsBoxHorizontal is et)
						
		--><script>
	
			$(document).ready(function(){
				var w = $(window).width();
				var h = $(window).height();	
				var posMover = [-w*2, -w, 0, w];
				var animationInterval = null;

				$('.first-box').width(w).css({'left': posMover[0] + 'px','position':'relative'});
				$('.second-box:eq(0)').width(w).css({'left': posMover[1] + 'px','position':'relative'});
				$('.main-mover').append($('.second-box:eq(0)').clone().width(w).css({'left': posMover[2] + 'px','position':'relative'}));
				$('.main-mover').append($('.second-box:eq(0)').clone().width(w).css({'left': posMover[3] + 'px','position':'relative'}));					

				var secondBoxContent = $('.first-box .box_Item').html();

				$('.second-box:eq(1)').css('background-color','#000000');
				$('.second-box:eq(1) .box_FeaturedImage').css("background-image","url(https://www.windtrebusiness.it/Assets/static/hp-sme-01/hp-sme_05_D.jpg)");
				$('.second-box:eq(1) .box_FeaturedImage img').attr('src','https://www.windtrebusiness.it/Assets/static/hp-sme-01/hp-sme_05_M.jpg');
				$('.second-box:eq(1) .box_Item').html(secondBoxContent);

				var box1 = $('.first-box');
				var box2 = $('.second-box:eq(0)');
				var box3 = $('.second-box:eq(1)');
				var box4 = $('.second-box:eq(2)');

				$('.second-box:eq(1)').find('.cbox_Items').removeClass("cbox_Items");
				$('.second-box:eq(1)').find('.clink_Go').removeClass("clink_Go");
				$('.second-box:eq(1)').find('.cbox_Link').removeClass("cbox_Link");
				$('.second-box:eq(1)').find('.cbox_Last').removeClass("cbox_Last");

				var posBoxes = [box1,box2,box3,box4];

				var mover = $('.main-mover');
				startBefore();
				function startBefore(){		
					$('.main-mover').css('transition','2s');	
					for(var i = 0; i < posMover.length; i++) {						
						posMover[i] = posMover[i] + w;						
					}
					firstBox = posBoxes.shift();						
					posBoxes.push(firstBox);
					$(firstBox).css('left', posMover[3] + 'px');
					$('.main-mover').css('left', - posMover[2] + 'px');
					updateIndicator();

				}

				start();
				function start() {			
					animationInterval = setInterval(function(){	

						for(var i = 0; i < posMover.length; i++) {						
							posMover[i] = posMover[i] + w;							
						}
						firstBox = posBoxes.shift();						
						posBoxes.push(firstBox);
						$(firstBox).css('left', posMover[3] + 'px');
						$('.main-mover').css('left', - posMover[2] + 'px');
						updateIndicator();

					},9000);

				}

				$(window).resize(function(){					
					oldW = w;
					w = $(window).width();
					var gap = 0;
					var nogap = false; //settare false se vuoi settare il gap solo per desktop
					if( w > 600 ) nogap = true;  
					if(nogap) {
						if(oldW > w){
							gap = oldW - w;					
						}else{
							gap = w - oldW;
						}
					}
					if(gap > 100 || nogap == true){

						stop();

						posMover = [-w*2, -w, 0, w];
						$('.first-box').css({'width':w,'left':posMover[0]});
						$('.second-box:eq(0)').css({'width':w,'left':posMover[1]});
						$('.second-box:eq(1)').css({'width':w,'left':posMover[2]});
						$('.second-box:eq(2)').css({'width':w,'left':posMover[3]});		
						$('.main-mover').css({'left':'0px','transition':'0s'});
						posBoxes = [box1,box2,box3,box4];
						updateIndicator();
						startBefore();
						start();
					}
				});

				jQuery(window).on('swipeleft',function(){	
					next();
				});

				jQuery(window).on('swiperight',function(){	
					back();
				});

				function back() {				
					if(posBoxes[2] == box1 || posBoxes[2] == box3) {
						stop();
						for(var i = 0; i < posMover.length; i++) {							
							posMover[i] = posMover[i] - w;								
						}
						lastBox = posBoxes.pop();
						posBoxes.unshift(lastBox);
						$('.main-mover').css('left', - posMover[2] + 'px');
						updateIndicator();
						start();
					}
				}

				function next() {									
					if(posBoxes[2] == box2 || posBoxes[2] == box4) {
						stop();
						for(var i = 0; i < posMover.length; i++) {						
							posMover[i] = posMover[i] + w;
						}
						firstBox = posBoxes.shift();						
						posBoxes.push(firstBox);
						$(firstBox).css('left', posMover[3] + 'px');
						$('.main-mover').css('left', - posMover[2] + 'px');
						updateIndicator();
						start();
					}
				}

				function updateIndicator() {
					boxScena = posBoxes[2];							
					if(boxScena == box2 || boxScena == box4) {							
						$('.lg-dot-button:eq(1)').removeClass('activeb'); 			
						$('.lg-dot-button:eq(0)').addClass('activeb');							
						$('#sel-img2').css('cursor','pointer');	 	
						$('#sel-img1').css('cursor','default');	
					}else{	
						$('.lg-dot-button:eq(0)').removeClass('activeb'); 			
						$('.lg-dot-button:eq(1)').addClass('activeb'); 
						$('#sel-img2').css('cursor','default');	 	
						$('#sel-img1').css('cursor','pointer');
					}
 				}

				function stop() {
					clearInterval(animationInterval);
				}

				$('#sel-img1').on('click',function(){
					back();
				});

				$('#sel-img2').on('click',function(){
					next();
				});

			});

		</script>
