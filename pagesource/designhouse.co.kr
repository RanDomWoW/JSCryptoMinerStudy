		     }
		}).always(function(){
			btnMoreFlag = false;
		});
	})


	function snsshart(self){
		var thisBox = $(self).parent().parent();
		var boxHeight = thisBox.height();
		var popsnsHtml = $('body').find('.main-sns-pop');

		//////////////////주소생시성 이전꺼를 가져와서 수정 2015.3.31 by suc
