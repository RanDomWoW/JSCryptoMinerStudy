        <script type="text/javascript">
            $(function() {
                $('div.product-chooser').not('.disabled').find('div.product-chooser-item').on('click', function() {
                    $(this).parent().parent().find('div.product-chooser-item').removeClass('selected');
                    $(this).addClass('selected');
                    $(this).find('input[type="radio"]').prop("checked", true);
                });
                
                //Comprobar si el cif esta repetido
                $("#inputEmail3").blur(function(){
                    var cif     = $("#inputEmail3").val();
                    cif         = cif.toLowerCase();
                    $("#inputTipoHolder").hide();
                    
                    if ( cif != ''){
                        $.ajax({
                            type: "POST",
                            url: "devolver_tipos_cif_login.php",
                            data: {cif: cif},
                            success: function(response){
                                if(response != ''){
                                    var responseObject = JSON.parse(response);
                                    if(responseObject.length > 1){
                                        $("#inputTipoHolder").show();
                                        $("#inputTipo3").html('');
                                        for(var i = 0; i < responseObject.length; i++){
                                            $("#inputTipo3").append('<option value="' + responseObject[i] + '">' + responseObject[i] + '</option>');
                                        }
                                    }
                                }
                            }
                        });
                    }
                });
            });

            function Enviar(){
                nombre = $("#name").val();
                tlf = $("#phone").val();
                acepto = $("#acepto").is(":checked");
                if(nombre.length == 0 || tlf.length == 0){
                    alert("Por favor rellene los dos campos");
                    return;
                }

                if(!acepto){
                    alert("Debe aceptar el aviso legal");
                    return;
                }

                var datos = "nombre="+nombre+"&tlf="+tlf;
                $.ajax({
                    type: "POST",
                    url: "envia_contacto.php",
                    data: datos,
                    dataType: "text",
                    success: function(data) {
                        alert("Hemos recibido tu contacto, te llamamos lo antes posible y hablamos, Gracias.");
                    },
                    error:function (xhr, ajaxOptions, thrownError){
                        alert(xhr.status);
                    }
                });
            }
         
