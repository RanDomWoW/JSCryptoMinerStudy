    jQuery(document).ready(function() {
        var scop = jQuery(".mid" + '523');
        scop.find(".xshower").hide();
        scop.find(".xshower:first").show();

        var show_item = function(next_index) {
            scop.find(".xshower").hide().removeClass("active");
            scop.find(".btn_active").removeClass("btn_active");
            var nextitem = scop.find(".xshower[index=" + next_index + "]");
