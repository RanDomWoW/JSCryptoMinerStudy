    function sendmail() {
        var mail = $(".email").val();

        var x = document.getElementById('txtEmail');
        var atpos = x.value.indexOf("@");
        var dotpos = x.value.lastIndexOf(".");

        if (mail == "") {
            alert("Địa chỉ mail không hợp lệ");
            $(".email").focus();
        }
        else if (atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= x.length) {
            alert("Địa chỉ mail không hợp lệ");
            return false;
        }

        else {
            var link = "/DesktopModules/TNContactUs/sendmail.ashx?mail=" + mail;
