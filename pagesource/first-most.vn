<script type="text/javascript">
    var first_bood = true;
    jQuery(document).ready(function () {
        loadVideo(1);
        jQuery(".img").click(function () {
            var scop = jQuery(".mid" + '520');
            var me = jQuery(this);
            scop.find(".selected").removeClass("selected");
            me.addClass("selected");
        });
        jQuery(".img:first").click();
    });

    function loadVideo(seed) {
        var scop = jQuery(".mid" + '520');

        var x;
        if (seed == 1) {
            x = scop.find(".xshow:last");
            if (x.length > 0 && x.next(".img").length == 0) {
                return;
            }
        }
        else {
            x = scop.find(".xshow:first");
            if (x.length > 0 && x.prev(".img").length == 0) {
                return;
            }
        }

        scop.find(".xshow").attr("class", "img").hide();

        var limit = 4;
        if (x.length <= 0) {
            x = scop.find(".img:first");
            x.addClass("xshow").fadeIn();
            limit = limit - 1;
        }

        for (i = 0; i < limit; i++) {
            if (seed == 1)
                x = x.next(".img");
            else
                x = x.prev(".img");
            x.addClass("xshow").fadeIn(1500);
        }
        jQuery(".xshow:first").addClass("first");
        jQuery(".xshow:last").addClass("last");
    }

    function show_video(obj) {
        var scop = jQuery(".mid" + '520');
        var me = jQuery(obj);
        var embed_content = ""
        var mediaurl = me.attr("MediaUrl");
        scop.find(".xvideotitle").html(me.attr("title"));

        var ap = 1;
        if (first_bood == true) {
            ap = 0;
        }
        if (mediaurl.indexOf("youtube.com") >= 0)
            embed_content = '<div class="embed-responsive embed-responsive-16by9"><iframe  class="embed-responsive-item" src="' + mediaurl + '?autoplay=' + ap + '&amp;loop=0&amp;rel=0" frameborder="0" allowfullscreen></iframe></div>'
        else {
            embed_content = "" +
                "<embed height='210' width='333' " +
                    "flashvars='file=" + mediaurl +
                               "&height=210" +
                               "&width=333" +
                               "&image=/DesktopModules/TNNewsSolution/swfplayer/logo.jpg" +
            //"&image=" + me.attr("src") +
                               "&autostart=" + !first_bood +
                    "' allowscriptaccess='always' " +
                    "wmode='transparent' " +
                    "allowfullscreen='true' " +
                    "quality='high' " +
                    "bgcolor='#000000' " +
                    "name='movie_player' " +
                    "id='playerID'  " +
                    "src='/DesktopModules/TNNewsSolution/swfplayer/player.swf' " +
                    "type='application/x-shockwave-flash'/>";
            if (!!document.createElement('video').canPlayType && mediaurl.indexOf(".mp4") > 0) {
                embed_content = "<video width='100%' controls>" +
                            "<source src='" + mediaurl + "' type='video/mp4'>" +
                        "</video>";
            }
        }
        if (first_bood)
            first_bood = !first_bood;
        jQuery("#player").html(embed_content);
        return false;
    }
</script>





