			var T1 = T1 || {};
T1.financeServiceTest = function() {
  var proto = {
    tokens : {},
    /**
     * @return {undefined}
     */
    init : function() {
      self.tokens.postRequestFinance = PubSub.subscribe(T1.constants.FINANCE_EXTERNAL_POST_REQUEST, self.onPostRequest);
      self.tokens.postRequestInsurance = PubSub.subscribe(T1.constants.INSURANCE_EXTERNAL_POST_REQUEST, self.onPostRequest);
    },
    /**
     * @param {?} dataAndEvents
     * @param {Object} data
     * @return {undefined}
     */
    onPostRequest : function(dataAndEvents, data) {
      var cache = {
        Country : T1.settings.country,
        Brand : T1.settings.carconfig.brand,
        Language : T1.settings.language
      };
      var prop;
      for (prop in data) {
        if ("prototype" !== prop) {
          cache[prop] = data[prop];
        }
      }
      var options = {
        target : T1.utilities.browser.isSafari ? "_blank" : "_overlayer",
        fields : {
          configuration : JSON.stringify(cache)
        }
      };
      options.action = dataAndEvents === T1.constants.INSURANCE_EXTERNAL_POST_REQUEST ? T1.settings.finance.urlMyInsuranceMiniCC || "//82.196.12.169/configurator" : T1.settings.finance.urlMyFinanceMiniCC || "//82.196.12.169/configurator";
      PubSub.publish(T1.constants.FORM_CROSS_DOMAIN_POST, options);
    },
    /**
     * @param {Object} options
     * @return {undefined}
     */
    requestFinanceData : function(options) {
      var appCount = T1.settings.mock ? "//t1-dev-proxy.herokuapp.com" : T1.settings.loadSaveServer;
      /** @type {string} */
      var url = appCount + "/tfs/car" + (options.multiple ? "s/" : "/") + T1.settings.country;
      /** @type {string} */
      var postData = JSON.stringify(options.configs);
      if (T1.utilities.ieVersion() < 10) {
        /** @type {XDomainRequest} */
        var xdr = new XDomainRequest;
        /**
         * @return {undefined}
         */
        xdr.onprogress = function() {
        };
        /**
         * @return {undefined}
         */
        xdr.onload = function() {
          if (xdr) {
            if (options.success) {
              options.success(JSON.parse(xdr.responseText));
            }
          }
        };
        /** @type {number} */
        xdr.timeout = 6E4;
        xdr.open("POST", url);
        xdr.send(postData);
      } else {
        var test = $.ajax({
          url : url,
          type : "POST",
          data : postData,
          contentType : "application/json",
          crossDomain : true
        });
        test.done(options.success);
        test.fail(function(dataAndEvents, deepDataAndEvents, reply) {
          console.log("error handling finance information " + reply);
        });
      }
    },
    /**
     * @param {?} node
     * @param {?} v
     * @param {?} result
     * @param {string} version
     * @return {?}
     */
    formatFinanceString : function(node, v, result, version) {
      return node && (v && result) ? (node = self.replaceNode(node, "productName", result.name && v.productName ? self.tag({
        source : v.productName.formatted,
        className : "finance-product-name",
        tag : version
      }) : null), node = self.replaceNode(node, "term", result.term && v.term ? self.tag({
        source : v.term.formatted,
        className : "finance-trm",
        tag : version
      }) : null), node = self.replaceNode(node, "taeg", result.effectiveRate && v.effectiveInterestRate ? self.tag({
        source : v.effectiveInterestRate.formatted,
        className : "finance-taeg",
        tag : version
      }) : null), node = self.replaceNode(node, "tan", result.annualRate && v.annualInterestRate ? self.tag({
        source : v.annualInterestRate.formatted,
        className : "finance-tan",
        tag : version
      }) : null), node = self.replaceNode(node, "price", v.monthlyPayment ? self.tag({
        source : v.monthlyPayment.formatted,
        className : "finance-monthly-price",
        tag : version
      }) : null)) : "";
    },
    /**
     * @param {string} n
     * @param {string} name
     * @param {?} reqUrl
     * @return {?}
     */
    replaceNode : function(n, name, reqUrl) {
      /** @type {RegExp} */
      var rclass = new RegExp("\\[" + name + "](.*?)\\[/" + name + "]", "gi");
      return n.replace(rclass, function(dataAndEvents, message) {
        return self.replaceValue(message, reqUrl);
      });
    },
    /**
     * @param {string} messageFormat
     * @param {boolean} value
     * @return {?}
     */
    replaceValue : function(messageFormat, value) {
      return value ? messageFormat.replace(/{value}/g, value) : "";
    },
    /**
     * @param {Object} opt_attributes
     * @return {?}
     */
    tag : function(opt_attributes) {
      return'<{tag} class="{class}">{source}</{tag}>'.replace(/{tag}/g, opt_attributes.tag || "i").replace(/{source}/g, opt_attributes.source || "").replace(/{class}/g, opt_attributes.className || "");
    },
    /**
     * @param {Object} v
     * @param {boolean} dataAndEvents
     * @param {boolean} value
     * @return {?}
     */
    getDisclaimer : function(v, dataAndEvents, value) {
      if (v && v.quote) {
        var results = $('<div class="finance-disclaimer"/>');
        if (value && results.append($("<h4/>").html(value)), dataAndEvents && v.quote.calculations) {
          /** @type {number} */
          var i = 0;
          for (;i < v.quote.calculations.length;i++) {
            var element = $("<div/>").addClass("row").appendTo(results);
            var param = v.quote.calculations[i];
            element.append($("<div/>").addClass("col-xs-6 col-lg-6").html(param.name));
            element.append($("<div/>").addClass("col-xs-6 col-lg-6").html(param.value));
            if (v.quote.calculations[i].format) {
              if (v.quote.calculations[i].format.b) {
                element.css("font-weight", "bold");
              }
            }
          }
        }
        return results.append($("<p/>").html(v.quote.value)), dataAndEvents || value ? results : v.quote.value;
      }
      return null;
    }
  };
  var self = proto;
  T1.constants;
  return{
    /** @type {function (): undefined} */
    init : proto.init,
    /** @type {function (Object): undefined} */
    requestFinanceData : proto.requestFinanceData,
    /** @type {function (?, ?, ?, string): ?} */
    formatFinanceString : proto.formatFinanceString,
    /** @type {function (Object, boolean, boolean): ?} */
    getDisclaimer : proto.getDisclaimer
  };
}();
