<script language="JavaScript" type="text/JavaScript">
function makeWin(url) {

  agent = navigator.userAgent;
  windowName = "Sitelet";
  params  = "";
  params += "toolbar=0,";
  params += "location=0,";
  params += "directories=0,";
  params += "status=0,";
  params += "menubar=0,";
  params += "scrollbars=1,";
  params += "resizable=1,";
  params += "width=500,";
  params += "height=325";

  // close the window to vary the window size
  if (typeof(win) == "object" && !win.closed){
	win.close();
  }

  win = window.open(url, windowName , params);

  if (agent.indexOf("Mozilla/2") != -1 && agent.indexOf("Win") == -1) {
	  win = window.open(url, windowName , params);
  }

  if (!win.opener) {
	  win.opener = window;
  }

  // bring the window to the front
  win.focus();
}

function winopen(url,stuff,morestuff) {
	window.open(url,stuff,morestuff).focus();
}

function shortcutNav(s){
	var gotoProduct = s.options[s.selectedIndex].value;
	if (gotoProduct != 'shortcuts') {
		parent.parent.location.href = gotoProduct;
	}
}

</script>
