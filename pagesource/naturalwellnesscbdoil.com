				$( ".ftrcnt-close" ).click(function() {
					$( ".popup-cnt, .popup-bg" ).hide();
				});
				
				$( "#ftrcnt" ).submit(function( event ) {
					
					event.preventDefault();
					
					var error = 0;
					var val = "";
					
					val = $( "#email" ).val();
					if(!validEmail(val)){
						error = 1;
						$( "#email" ).addClass('error');
						$( "#email-err" ).show();
					}else{
						$( "#email" ).removeClass('error');
						$( "#email-err" ).hide();
					}
					
					val = $( "#fullname" ).val();
					if(!validField(val)){
						error = 1;
						$( "#fullname" ).addClass('error');
						$( "#fullname-err" ).show();
					}else{
						$( "#fullname" ).removeClass('error');
						$( "#fullname-err" ).hide();
					}
					
					val = $( "#telephone" ).val();
					if(!validField(val)){
						error = 1;
						$( "#telephone" ).addClass('error');
						$( "#telephone-err" ).show();
					}else{
						$( "#telephone" ).removeClass('error');
						$( "#telephone-err" ).hide();
					}
					
					val = $( "#state" ).val();
					if(!validField(val)){
						error = 1;
						$( "#state" ).addClass('error');
						$( "#state-err" ).show();
					}else{
						$( "#state" ).removeClass('error');
						$( "#state-err" ).hide();
					}
					
					if(error==0){

						var data = {
							action: 'ftrcnt',
							email: $( "#email" ).val(),
							fullname: $( "#fullname" ).val(),
							telephone: $( "#telephone" ).val(),
							state: $( "#state" ).val(),
							hours: '',
							key: $( "#key" ).val(),
						};
						
						//alert(myajax.url);
						
