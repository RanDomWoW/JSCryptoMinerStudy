									}
								, validOffsets = []
								, hasIcons;

							titles.each(function(index, title) {

								var offsetIndex;

								for(var offset in iconOffsets) {
									offsetIndex = $.inArray( title.toLowerCase(), iconOffsets[offset] );
									if(offsetIndex >= 0) {
										validOffsets.push(offset);
										hasIcons = true;
										return true;
									}
								}

								validOffsets.push(null);
							});

							if (hasIcons) target.addClass("icons");

							$(".bluebox > a", target).each(function(i, el) {
								var $el = $(el)
								  , text = $el.text()
