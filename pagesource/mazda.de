<script>
    $( document ).ready(function() {
        var carData = {};
        $.ajax({
            dataType: "jsonp",
            url: 'https://fmm.mazda.de/integration/countModels/',
            //url: 'https://fmm.mazda.de/integration/countModels/',
            success: function (data) {
                carData = data.data.data;
                var url = $('.fmm__startpage').data('url');
                var val = $('.fmm__startpage select option').first().val();
                var mmcs = $('.fmm__startpage select option').first().data('filter').split(",");
                var lbl = $('.fmm__startpage select option').first().text();
				var count = 0;
				var link = "";
				
				link = 'mmc='+val;
				
				$.each(mmcs, function(index, mmc){
					if(typeof carData.cars[mmc] !== 'undefined'){
						if(typeof carData.cars[mmc].count !== 'undefined'){
							count = count + carData.cars[mmc].count;
						}
					}
				});

                $('.fmm__startpage__txt--amount').html(count);
                $('.fmm__startpage__txt--type').html(lbl);
                $('.fmm__startpage--submit').attr('href',url+'neuwagen/?'+link);

            }
        });

        $('.fmm__startpage select').on('change', function(){
	        var url = $('.fmm__startpage').data('url');
	        var val = $('.fmm__startpage select option:selected').val();
            var mmcs = $('.fmm__startpage select option:selected').data('filter').split(",");
            var lbl = $('.fmm__startpage select option:selected').text();
			var count = 0;
			var link = "";

			link = 'mmc='+val;

			$.each(mmcs, function(index, mmc){
				if(typeof carData.cars[mmc] !== 'undefined'){
					if(typeof carData.cars[mmc].count !== 'undefined'){
						count = count + carData.cars[mmc].count;
					}
				}
			});

            //console.log(mmc,count,carData.cars);
            $('.fmm__startpage__txt--amount').html(count);
            $('.fmm__startpage__txt--type').html(lbl);
            $('.fmm__startpage--submit').attr('href',url+'neuwagen/?'+link);
        });
        
        setTimeout(function(){
	        if(typeof s3_log !== 'undefined'){
		        $('.fmm__startpage').addClass('sophus-found');
				$('body').on('click', '.fmm__startpage--submit', function(e){
					e.preventDefault();
	
					//--Tracking
					var trackingValue = $('.fmm__startpage--dropdown select').val();
					var trackingModel = encodeURIComponent($('.fmm__startpage--dropdown option[value='+trackingValue+']').text().replace(' ','-'));
					//console.log('/neuwagen/find-my-mazda/quicksearch?moi='+trackingModel);
					s3_log('find-my-mazda/quicksearch?moi='+trackingModel);
					
					//--Redirect to href
					window.location.href = $(this).attr('href');
				});
	        }else{
	            $('.fmm__startpage').addClass('no-sophus');
	        }
		},3000);

    });





</script>


