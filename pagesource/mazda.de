						heroImg = $('.hero_image').find("img"); // pages with hero image or hero-carousel

					if ($(heroImg).length < 1) {
						heroImg = $('.hero').find("img")[0]; // main page
					};

					if ($(heroImg).length < 1) { // if there's no promo or hero image

						//set stickingPoint depending on static height and module height
						moduleHeight = $(".docked").height();

						stickingPoint = Math.round(heroHeight/2 - moduleHeight/2 + $("header.row").height());
						$(".docked").css({"top" : stickingPoint});

