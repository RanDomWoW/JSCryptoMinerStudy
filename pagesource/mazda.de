						}

						function getRootTitles(target) {
							return $(".bluebox > a", target).map(function(i, el) {
								return $(el).text();
							});
						}

						function setupIcons(target) {
							var titles = getRootTitles(target)
								, iconOffsets = {
										40 : ["probefahrt", "fahren"],
										140 : ["broschüre"],
										240 : ["newsletter"],
