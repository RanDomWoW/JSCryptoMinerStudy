					}

					function compareOffset (ev) {

						moduleHeight = $(".docked").height();

						var pageYOffset = (typeof window.pageYOffset !== 'undefined') ?
								window.pageYOffset :
								document.documentElement.scrollTop;

						pageYOffset += stickingPoint + moduleHeight;

						if ($(".secondaryCarNav").length > 0) {
							moduleOffset = $(".secondaryCarNav").offset().top + $(".secondaryCarNav").height() + 10;
						} else {
							moduleOffset = 600;
						};

						switch(pin) {
							case "pinscroll":

								if (! docked && pageYOffset >= moduleOffset) {
									dock();

									//Code added so the initial docking behaviour is to slide out
									var $panel = $('.view.box.grid-3.alpha.omega');
									if ($panel.hasClass('reveal')) {
										hide($panel);
									} else {
										show($panel);
									}
									//
								} else if (docked && pageYOffset < moduleOffset) {

									undock();
								}
								break;

							case "alwayspinshow":

								if ((pageYOffset >= moduleOffset) && ($('.view', module).hasClass('reveal'))) {
									// console.log( 'hide 2' );
									hide(view);
									$('.icons').mouseenter(function() {
										show(view);
									});
									$('.icons').mouseleave(function() {
										hide(view);
									});
								} else if ((pageYOffset < moduleOffset)  && (!$('.view', module).hasClass('reveal'))) {
									//console.log( 'show 2' );
									show(view);
								}
								break;


							default:{
								break;
							}
						}
						/*var pageYOffset = (typeof window.pageYOffset !== 'undefined') ?
								window.pageYOffset :
								document.documentElement.scrollTop;

						pageYOffset += stickingPoint;
						// moduleHeight = Math.min(moduleHeight, $('.docked', module).height());
						moduleHeight = 206;
						moduleOffset = module.offset().top + moduleHeight;

						switch(pin) {
							case "pinscroll":
								if (! docked && pageYOffset >= moduleOffset) {
									dock();
								} else if (docked && pageYOffset < moduleOffset) {
									undock();
								}
								break;

							case "alwayspinshow":
								if (pageYOffset >= moduleOffset) {
									//console.log( 'hide 2' );
									hide(view);
								} else if (pageYOffset < moduleOffset) {
									//console.log( 'show 2' );
									show(view);
								}
								break;


							default:{
								break;
							}
						}*/

					}

					function enable () {

						moduleHeight = module.height();

						// maintains origin while docked
						// module.css('min-height', moduleHeight);

						controls.create();

						if(pin === "alwayspin") {return};
						$(window).scroll(compareOffset);
						compareOffset(); // FIXME? this breaks the loading the rest of the code in Win7 IE8


						if(pin === "alwayspinshow") {

							setTimeout(function() {
								if (isDE) {
									$('.docked', module).width($('.view', module).width() +43);
								} else {
									$('.docked', module).width($('.view', module).width());
								}
							}, 250);
							// $(".button.ctaPanel", view).trigger("hover");
						}

					}

					return {
						enable: enable,
						show : show,
						hide : hide
					};
				})();


				// links initial state to data-pin attribute
				(function() {

					if (pin == 'pinscroll') {
						docking.enable();
					// } else if (["alwayspin", "alwayspinshow"].indexOf(pin) >= 0) {
					} else if (pin == 'alwayspinshow') {

						docking.enable();

						docked = true;
						$('> div', module).wrapAll('<div class="docked"></div>');

						var dockedWrap = $('.docked', module);

						// minWidth = $('.view',dockedWrap).width();

						minWidth = 222;

						setTimeout(function() {
							dockedWrap.addClass('fade');
						}, 150);

					}else if (pin == 'alwayspin') {
						docking.enable();
						docked = true;
						$('> div', module).wrapAll('<div class="docked"></div>');

						var dockedWrap = $('.docked', module);

						// minWidth = $('.view',dockedWrap).width();

						minWidth = 222;

						setTimeout(function() {
							dockedWrap.addClass('fade');
						}, 150);
					}


				})();
			}

			(function() {
				$('[data-module-type="calls-to-action"]').each(function() {
					initialise($(this));
				});
			})();

			function bind (instance) {
				initialise(instance);
			}

			return {
				bind: bind
			};
		})();
