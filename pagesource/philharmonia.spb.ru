      <script>
         var tok = '2225210154.5309b23.22a79ab841af40a3a76f07ea118fbfe6', // я уже давал ссылку чуть выше
             userid = '2225210154', // ID пользователя, можно выкопать в исходном HTML, можно использовать спец. сервисы либо просто смотрите следующий пример :)
             kolichestvo =3; // ну это понятно - сколько фоток хотим вывести

         $.ajax({
            url: 'https://api.instagram.com/v1/users/' + userid + '/media/recent',
            dataType: 'jsonp',
            type: 'GET',
            data: {access_token: tok, count: kolichestvo}, // передаем параметры, которые мы указывали выше
            success: function(result){
               console.log(result);
               for( x in result.data ){
               	var insta_text=result.data[x].caption.text;

                  insta_text=insta_text.replace('  ','');
                  insta_text=insta_text.replace('  ','');
                  insta_text=insta_text.replace(/[^а-яА-Яa-zA-ZёЁЙй-—,-.:'"0-9 ]*/img,'');
                  insta_text=insta_text.replace(/\r/img,'');
                  insta_text=insta_text.replace(/\n/img,'');
                  $('.main_phil_insta ._items').append('<div><div class="flip-container">  <div class="flipper">    <div class="front"><a href="'+result.data[x].link+'" ><img src="'+result.data[x].images.standard_resolution.url+'"></a></div><div class="back t_all"><div onclick="window.open('+"'"+result.data[x].link+"'"+')">'+insta_text+'</div><a class="btn rbtn" href="'+result.data[x].link+'" target="_blank"><i class="instagram_icon"></i></a></div></div></div></div>');
                  // result.data[x].images.thumbnail.url - URL картинки 150х150
                  // result.data[x].images.standard_resolution.url - URL картинки 612х612
                  // result.data[x].link - URL страницы данного поста в Инстаграм 
               }
            },
            error: function(result){
               console.log(result); // пишем в консоль об ошибках
            }
         });

      </script>

