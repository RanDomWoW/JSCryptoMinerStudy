<script type="text/javascript">
( function( $ ) {
var offset = $('.BannerTop').offset().top;
var $meuMenu = $('.BannerTop');
var timerbanner = true;
function mytimerbanner() {
    $meuMenu.removeClass('fixarsupperbanner');
	timerbanner = false;
}
$(document).on('scroll', function () {
    if (offset <= ($(window).scrollTop() - 41 )){
		if (timerbanner == true){
			$meuMenu.addClass('fixarsupperbanner');
			
			setTimeout(mytimerbanner, 15000);
			
			$.each($(".open"), function () {
				$meuMenu.removeClass('fixarsupperbanner');
			});	
		}		
    } else {
        $meuMenu.removeClass('fixarsupperbanner');
    }
});

} )( jQuery );


</script>
