<script type="text/javascript">
	function createCookie(name,value,days) {
		if (days) {
			var date = new Date();
			date.setTime(date.getTime()+(days*24*60*60*1000));
			var expires = "; expires="+date.toGMTString();
		}
		else var expires = "";
		document.cookie = name+"="+value+expires+"; path=/";
	}
	
	function readCookie(name) {
		var nameEQ = name + "=";
		var ca = document.cookie.split(';');
		for(var i=0;i < ca.length;i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1,c.length);
			if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
		}
		return null;
	}
	
	function eraseCookie(name) {
		createCookie(name,"",-1);
	}
	
	$(document).ready(function() {
		var SaleCampaignBannerCookie = readCookie('SaleCampaignBanner');
		
		if (SaleCampaignBannerCookie == 'closed') {
			$('#SaleCampaignBanner').hide();
		}else if (SaleCampaignBannerCookie != 'closed') {
			openSaleCampaignBanner();
		};
		
		$('#SaleCampaign_CloseBtn').click(function(e) {
			e.preventDefault();
			closeSaleCampaignBanner();
		});
		
		function closeSaleCampaignBanner () {
			createCookie('SaleCampaignBanner','closed',1);
			$('#SaleCampaignBanner').hide();
		}
		function openSaleCampaignBanner () {         
			$('#SaleCampaignBanner').show();
		}
		
		
		
		
		var path = window.location.pathname;
		var pagen = path.split("/").pop();
		console.log(pagen);
		if ( pagen == 'most-popular-products.html'){
			var ajax_load = '';
			var loadTask = "/";
				loadTask = loadTask.replace('http:','');
			$("div.CustomPplrProduct_outer").load(loadTask + " .FeaturedProductsWrap", function (responseText, textStatus, XMLHttpRequest) {
			});
		} 
		
		
		var path = window.location.pathname;

		var pagen = path.split("/").pop();
		console.log(pagen);
		if ( pagen == 'new-products.html'){
			var ajax_load = '';
            var loadTask = "/new-arrival/";
            loadTask = loadTask.replace('http:','');
            $(".CustomNewProduct_outer").load(loadTask + " ul.productGrid", function (responseText, textStatus, XMLHttpRequest) {
                if (textStatus == "success") {
                    var getNewArrivalProducts = $(".CustomNewProduct_outer").html();
                    $(".CustomNewProduct_outer").html(getNewArrivalProducts );  
                }
            });
		} 
		
		
		/*For Mobile*/
		if ($(window).width() <= 800) {
			var MobileSaleCampaignBannerCookie = readCookie('MobileSaleCampaignBanner');
			
			if (MobileSaleCampaignBannerCookie == 'MobileClosed') {
				$('#MobileSaleCampaignBanner').hide();
			}else if (MobileSaleCampaignBannerCookie != 'MobileClosed') {
				MobileopenSaleCampaignBanner();
			};
			
			$('#MobileSaleCampaign_CloseBtn').click(function(e) {
				e.preventDefault();
				MobilecloseSaleCampaignBanner();
			});
			
			function MobilecloseSaleCampaignBanner () {
				createCookie('MobileSaleCampaignBanner','MobileClosed',1);
				$('#MobileSaleCampaignBanner').hide();
			}
			function MobileopenSaleCampaignBanner () {         
				$('#MobileSaleCampaignBanner').show();
			}
		}
		
    });
</script>
