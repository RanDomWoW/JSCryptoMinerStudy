	var gtmjs = {};

gtmjs.init = function() {
	if (typeof tmCommerce !== 'undefined' && tmCommerce === 2) {
		gtmjs.viewBag();
		gtmjs.summary();		
	}
};

gtmjs.viewBag = function() {
	if(typeof pageName !== 'undefined' && pageName === 'basketPage' && typeof productModel !== 'undefined') {
		window.dataLayer = window.dataLayer || [];
              
        dataLayer.push({
            'event': 'checkout',
            'ecommerce': {
                'checkout': {
                    'actionField': {'step': 1},
                    'products': productModel
                }
            }
        });
	}
}

gtmjs.shippBill = function(step, option) {
    if (typeof productModel !== 'undefined') {
        window.dataLayer = window.dataLayer || [];

        if(typeof option !== 'undefined' && option !== null) {

			dataLayer.push({
                'event': 'checkout',
                'ecommerce': {
                    'checkout': {
						'actionField': {
							'step': step,
							'option': option
						},
						'products': productModel
                    }
                }
			});

        } else {

            dataLayer.push({
                'event': 'checkout',
                'ecommerce': {
                    'checkout': {
						'actionField': {
							'step': step
						},
						'products': productModel
                    }
                }
            });
 
        }
    }
}

gtmjs.summary = function() {
    if(typeof pageName !== 'undefined' && pageName === 'checkoutPlaceOrder' && typeof productModel !== 'undefined') {
        gtmjs.shippBill(4);
    }
}

