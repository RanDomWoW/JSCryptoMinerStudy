        $('.diplomeo-player')
            .on('mouseover mouseout', '.diplomeo-hover', function (e) {
                var $this = $(this);
                var dataAttr = e.type === 'mouseover' ? 'cssHover' : 'css';
                var css = $this.data(dataAttr);
                if (css) {
                    $this.css(css);
                }
            });
        // changer lien diplomeo www.diplomeo.com/* -> diplomeo.com/* sans les 3W.
        $(".diplomeo-player a, .diplomeo-slot a, .diplomeo-banner a").each(function () {
            var lien = $(this).attr('href');
            lien = lien.substring(6);
            $(this).attr('href', 'https://' + lien);
        });

