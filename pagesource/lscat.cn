	<script>

		$(".com").mouseover(function(){
			if($(this).data("type")==1){
				$(this).attr("src","statics/images/dev2/zljd.jpg");
			}else{
				$(this).attr("src","statics/images/dev2/sxjd.jpg");
			}

		});
		$(".com").mouseout(function(){

			$(this).attr("src",$(this).data('src'));
		});

	$(".voice_1 ul li").click(function(){
		if($(this).attr('disable')!=='true'){

			var i = $(this).index();
			$(this).parent().parent().find('ul li div').each(function(a){
				var ass=$(this).attr("class").replace("-active", "");
				$(this).attr("class",ass);
				if(i==a) $(this).attr("class",ass+"-active");
			});

			$(this).parent().parent().parent().find(".block").hide();
			$(this).parent().parent().parent().find(".voice_2 ul li").eq(i).find(".block").show();



		}

	})

	$(".djs").each(function(){
		var date = $(this).data("date");

		var d=showtime(date);
		$(this).text(d);
	})


	function showtime(d){

		day=new Date(d);

		today=new Date();

		iDays  =  parseInt((day  -  today)  /  1000  /  60  /  60  /24)+1    //把相差的毫秒数转换为天数

		iDays = iDays > 0 ?iDays:0;
		return iDays;


	}
	$(function(){


		$(".scrollNews").each(function () {

			var $this = $(this);
			var scrollTimer;

			$this.hover(function(){
				clearInterval(scrollTimer);
			},function(){
				scrollTimer = setInterval(function(){
					scrollNews( $this );
				}, 3000 );
			}).trigger("mouseleave");



		})


	});
	function scrollNews(obj){
		var $self = obj.find("ul:first");
		var lineHeight = $self.find("li:first").height(); //获取行高
		$self.animate({ "marginTop" : -lineHeight-8+"px" }, 800 , function(){
			$self.css({marginTop:0}).find("li:first").appendTo($self); //appendTo能直接移动元素
		})
	}




	</script>
