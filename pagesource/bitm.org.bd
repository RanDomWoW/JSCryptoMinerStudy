<script>
    var baseurl = 'http://bitm.org.bd/';
    function mySerch(text) {
        $.ajax({
            type: 'post',
            url: baseurl + "home/ajaxserch",
            data: {serchval: text},
            dataType: "json",
            success: function (response) {
                if (response.status == 1) {
                    html = '<ul class="search-suggestion-list">';
                    for (var i = 0; i < response.courses.length; i++) {
                        html += ('<li><a href="' + response.courses[i].href + '"><span class="mini-icon"><i class="fa fa-file" aria-hidden="true"></i></span><div class="title ellipsis">' + response.courses[i].course_title + '</div><span class="cat"> ' + response.courses[i].course_type + '</span></a></li>');
                    }
                    html += "</ul>";
                    $('#searchresult').html(html);
                } else {
                    $('#searchresult').html('');
                }
            }
        });
    }
</script>



