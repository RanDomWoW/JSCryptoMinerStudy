<!-- exposebox region-->
<script type="text/javascript">
    $(document).ready(function () {
        try {
            var jb = document.createElement('script');
            jb.type = 'text/javascript';
            jb.async = true;
            jb.src = window.location.protocol + '//sf.exposebox.com/widget/predictl.min.js?c=917';
            var s = document.getElementsByTagName('div')[0];
            s.parentNode.insertBefore(jb, s);

            window.predictlApi = window.predictlApi || function (cb) { var pa = window.predictlApi; pa.or = pa.or || []; pa.or.push(cb); }
            window.addEventListener('predictl:ready', function (event) {
                var predictlApi = event.detail;
                predictlApi.getExposeBoxUserId(function (userId) {
                    if (document.getElementById('isPersonalization')) sendActionToPersonalizationApi(userId);
                    var dimensionValue = userId;
                    ga('set', 'dimension3', dimensionValue);
                    ga('send', 'event', 'EBID', 'send', 'dimension3', { nonInteraction: true });
                });
            });
        } catch (ex) {
            console.log('ExposeBox Init Failed');
        }

        function sendActionToPersonalizationApi(userId) {
            try {
                var jsonData = JSON.parse($('#isPersonalization').text());
                if (jsonData.PageName == 'product') {

                    var actionObj = {
                        "UserId": userId,
                        "ActionSource": 'Desktop',
                        "Url": jsonData.Url,
                        "ModelId": jsonData.Id,
                        "ModelName": jsonData.Title,
                        "Brand": jsonData.Brand,
                        "MainCategory": jsonData.MainCategory,
                        "SubCategory": jsonData.SubCategory,
                        "MinPrice": jsonData.MinPrice,
                        "MaxPrice": jsonData.MaxPrice,
                        "Rate": jsonData.Rate,
                        "NormalizeRate": jsonData.NormalizeRate,
                        "ReviewsAmount": jsonData.ReviewsAmount
                    };

                    actionObj.ActionType = 'PageVisit';
                    sendActionToApi(actionObj);

                    $(".BuyBtn,.stores-boxes a").click(function () {
                        var link = $(this).closest('a');
                        actionObj.ProductId = getParameterByName('pid', link.attr('href'));
                        actionObj.ActionType = 'BuyClick';
                        sendActionToApi(actionObj);

                    });

                    function sendActionToApi(actionObj) {
                        $.ajax({
                            url: 'http://www.zap.co.il/Personalization/InsertUserAction',
                            type: 'post',
                            contentType: "application/json",
                            data: JSON.stringify(actionObj),
                            success: function (response) {
                                console.log(response);
                            }
                        });
                    }
                }
            }
            catch (ex) {
                console.log('Personalization error');
            }
        }

        function getParameterByName(name, url) {
            if (!url) url = window.location.href;
            name = name.replace(/[\[\]]/g, "\\$&");
            var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
                results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';
            return decodeURIComponent(results[2].replace(/\+/g, " "));
        }
    });
</script>

