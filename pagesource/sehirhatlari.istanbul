        		$("#mobilbox").hide();
	        	$("#ebulten").hide();
	        	$("#smsbox").fadeIn("slow");
        	}else if (a == 3){
        		$("#mobilbox").hide();
	        	$("#smsbox").hide();
	        	$("#mobilbox").fadeIn("slow");
        	}
        }
    </script>
