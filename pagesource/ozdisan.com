<!--b2b Login -->
<script type="text/javascript">
  $(function() {
    $("#b2bCustomerCode,#b2bUsername,#b2bPassword")
      .keypress(function(event) {
        if (event.which == 13) {
          $("#b2bDoLogOn").trigger("click");
        }
      });

    $("#b2bDoLogOn")
      .click(function(e) {
        e.stopImmediatePropagation();
        e.preventDefault();

        var CustomerCode = $("#b2bCustomerCode").val();
        var UserName = $("#b2bUsername").val();
        var Password = $("#b2bPassword").val();
        var RememberMe = $("#b2bRememberMe").is(':checked');

        var MessageBox = $("#b2bdisplayMessage");

        var btn_LogOn = $(this);

        var model = {
          CustomerCode: CustomerCode,
          UserName: UserName,
          Password: Password,
          RememberMe: RememberMe,
        };
        MessageBox.html("Lüften bekleyiniz. Bilgileriniz kontrol ediliyor..."); //
        btn_LogOn.prop('disabled', true);
        $.ajax({
          url: "/Account/LogOnB2bAsync",
          type: "post",
          dataType: "json",
          async: true,
          data: {
            model: model,
            ReturnUrl: URLParser(window.location.origin + window.location.search).getParam('ReturnUrl')
          },
          success: function(data) {
            if (data.status) {
              document.location.href = data.data;
              alertify.success("Giriş başarılı");
            } else {
              MessageBox.html(data.message);
            }
          },
          error: function() {
            MessageBox.html('Sunucu ile iletişim kurulamadı, Lütfen tekrar deneyiniz!');
          },
          complete: function() {
            btn_LogOn.prop('disabled', false);
          }
        });

      });
  });
</script>
<!--b2b Login end -->
