<script>
$(document).ready(function() {
  var progress = $('.progress-bar');
  var interval =1800;
  var width = 0;
  var miningprofit = 0;
  var daily = 0;
  var today = '2018-03-20';
  $('#start').click(function() {
    $('#start').replaceWith('<h4><i class="fa fa-clock-o" aria-hidden="true"></i>Please wait...</h4>');
    var counter = setInterval( function() {
      width++;
      progress.css('width',width+'%');
      progress.text(width+'%');
      miningprofit=daily/100*width;
      $('#miningprofit').text(miningprofit.toFixed(4));
      if (width==100) {
        clearInterval(counter);
        setCookie('profit',today,1);
        location.reload();
      }
    } , interval);
  });
  window.balance=15;
  window.fund=0;
  window.pmcrate = 8.217;
  var startprofit = window.balance*1.825*window.pmcrate;
  $('#totalprofit').text(startprofit.toFixed(2));
  $('#balance').ionRangeSlider({
    grid: true,
    prefix: '',
    min: 15,
    max: 3000,
    from: 15,
    onChange: function (data) {
      window.balance = data.from;
      var profit = countProfit();
      $('#totalprofit').text(profit.toFixed(2));
    }
  });
  $('#daily').ionRangeSlider({
    grid: true,
    prefix: '',
    min: 0,
    max: 30,
    from: 0,
    onChange: function (data) {
      window.fund = data.from;
      var profit = countProfit();
      $('#totalprofit').text(profit.toFixed(2));
    }
  });
  function countProfit() {
    var i;
    var profit = 0;
    var perday = 0;
    var balance = window.balance;
    for (i=1;i<365; i++) {
      balance+=window.fund;
      perday=balance*0.005;
      profit+=perday;
    }
    return profit*window.pmcrate;
  }
});
</script></div>
