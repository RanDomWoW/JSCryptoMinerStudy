<SCRIPT type=text/javascript>
function enterkey(e){ 
	 var form = document.loginform;
	 var ev = window.event || e; 
	 if (ev.keyCode==13){ 
		  if(form.username.value==""){
		   	form.username.focus();
		  }else if(form.password.value==""){
		   	form.password.focus();
		  } else if(form.code && form.code.value==""){
		    form.code.focus();
	      }else{
		    form.submit();
	      }
	 } 
} 

function getQueryString(name) {
    var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
    var r = window.location.search.substr(1).match(reg);
    if (r != null) return unescape(r[2]); return null;
}

function doLogin(){
	 var form = document.loginform;
	 if(form.username.value==""){
		form.username.focus();
	  }else if(form.password.value==""){
		form.password.focus();
	  } /*else if(form.code && form.code.value==""){
		form.code.focus();
	  }*/else{
		form.submit();
	  }
 }
 
function txtContrl(textID, textLabel) {
    $(textLabel).click(function () {
        $(textID).focus();
    })
    $(textID).focus(function () {
        $(textLabel).hide();
    });
    $(textID).change(function () {
        $(textLabel).hide();
    });
    $(textID).blur(function () {
        var txtVal = $(textID).val();
        if (txtVal == "") {
            $(textLabel).show();
        }
        else{
        	$(textLabel).hide();
        }
    });
} 

function doFocus(){
	try{
	   document.loginform.username.focus();  
	}
	catch(e){
	}
 }
 
$(function () { 
    txtContrl('#g_u .ph-label', '#g_u .tips-text');
    txtContrl('#g_p .ph-label', '#g_p .tips-text');
    txtContrl('#label_vcode .verifycode-label', '#label_vcode .tips-text'); 
    if($("#msg").html()!= null){
    	$('#err_m').show();  
    	$('#msg').removeAttr('style');
    } 
}); 
