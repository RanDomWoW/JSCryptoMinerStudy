<script type="text/javascript">
    function ShowTokenLoginWin() {
        var serverAddress = 'https://statebank.monpass.mn/';

        var mapForm = document.createElement("form");
        mapForm.target = "Monpass";
        mapForm.method = "POST"; // or "post" if appropriate
        mapForm.action = serverAddress + "TokenLogin/Login";

        document.body.appendChild(mapForm);

        height = 5;
        width = 5;
        t = window.innerHeight - height
        l = window.innerWidth - width

        map = window.open("", "Monpass", "menubar=0,location=0,resizable=0,status=0,titlebar=0,toolbar=0,width=5,height=5,left=" + l + ",top=" + t + ",visible=none");
        window.onmessage = function (e) {
            if (e.data) {
                //            console.log(e.data);
                var obj = JSON.parse(e.data);

                if (obj.Status == 'Success') {
                    $("#eTokenSessionId").val(obj.SeasonId);
                    document.forms["UserForm"].submit();
                }
                if (obj.Status == 'Error') {
                    alert('Алдаа гарлаа');
                }
            }
        };
        var timer = setInterval(function () {
            if (window.closed) {
                clearInterval(timer);
            }
        }, 1000);

        if (map) {
            mapForm.submit();
        } else {
            aler('Токен ашиглахын тулд POPUP зөвшөөрнө үү!')
        }
    }
</script>

