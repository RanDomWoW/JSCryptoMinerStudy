	var actualSlider = 1;
	var sliderPause = false;
	
    function slider(to){
    	nbr = $(".top-news-headline").length;
    	if( to == "auto" ){
	    	if( actualSlider >= nbr ){ goTo = "0px"; actualSlider = 1; }
			else { goTo = "-=220px";  actualSlider++; }
    	} else {
	    	delta = 0 - (actualSlider - to);
	    	goTo = "-="+(delta*220)+"px";
	    	actualSlider = to;
    	}
		goTo = "-"+((actualSlider-1)*220)+"px";
    	
	    $(".top-news-headline").clearQueue().animate({
		    top: goTo
	    }, 1500, "easeOutCirc");
	    
	    $(".top-news-headline h2").eq(actualSlider - 1).delay(100).clearQueue().animate({
		    marginTop: "0px"
	    }, 1500, "easeOutCirc");
	    
	    $(".top-news-headline h2").not(".top-news-headline h2:eq("+(actualSlider - 1)+")").clearQueue().animate({
		    marginTop: "40px"
	    }, 2000, "easeOutCirc");
	   
		$("#slider-marks ul li").removeClass("selected");
	    $("#slider-marks ul li:eq("+(actualSlider-1)+")").addClass("selected");
		
    }
    
   
    
    function autoSlider(){
    	if( !sliderPause ){ slider("auto"); }
    	window.setTimeout("autoSlider()", 5000);
	    
    }
    
    $(window).load(function(){
	    window.setTimeout("autoSlider()", 5000);
	    
	    $("#headlines #slider").mouseenter(function(){  sliderPause = true; });
	    $("#headlines #slider").mouseleave(function(){  sliderPause = false; });
	    
