<script type="text/javascript" src="http://agariohub.net/client/global.js"></script>
<link href="http://agariohub.net/rooms.css" rel="stylesheet" />

<script type="text/javascript">
			var servers = [];
			$(function(){
				var totalOnline = 0;

				if(getCookie("theme") == "dark"){
					$('<link>').appendTo('head').attr({type: 'text/css', rel: 'stylesheet', href: 'http://agariohub.net/rooms-dark.css'});
				}

				function getServer(label, location) {
					for(var i = 0; i < servers.length; i++){
						if(servers[i].label == label && servers[i].location == location)
							return servers[i];
					}
					return null;
				}

				function buildMenu() {
					$("#player-global").html(totalOnline + " Alive Players");

					for(var i = 0; i < servers.length; i++){
						var target = servers[i];

						var location = "";
						switch(target.location) {
							case 1:
								location = "na";
							break;

							case 2:
								location = "eu";
							break;

							case 3:
								location = "as";
							break;

							default:
								location = "unknown";
						}

						$("#servers-" + location).append($("#model-server").html()
							.split("%label%").join(target.label)
							.split("%players%").join(target.message == "no_message" ? target.players + " Alive players" : target.message)
							.split("%can_choose%").join(target.canChoose?"":" disabled")
							.split("%id%").join( target.label.toLowerCase().split(" ").join("-") + target.location ));
					}
				}

				$.ajax({
					type: 'GET',
					url: 'http://api.agariohub.net/servers'
				}).done(function(r){
					$("#loading").fadeOut(200);
					totalOnline = 0;
					for(var i = 0; i < r.servers.length; i++){
						totalOnline += r.servers[i].players;
						if(getServer(r.servers[i].label, r.servers[i].location)){
							getServer(r.servers[i].label, r.servers[i].location).players += r.servers[i].players;
							getServer(r.servers[i].label, r.servers[i].location).canChoose = true;
							if(r.servers[i].message != getServer(r.servers[i].label, r.servers[i].location).message)
								getServer(r.servers[i].label, r.servers[i].location).message = "no_message";
						}else{
							servers.push({
								label: r.servers[i].label,
								players: r.servers[i].players,
								location: r.servers[i].location,
								message: r.servers[i].message,
								canChoose: false
							});
						}
					}
					servers.sort(function(a,b){return b.players-a.players;});
					buildMenu();
					if(getCookie("location") != "")
						$(".location[data-show='" + getCookie("location") + "']").delay(10000).click();
				}).fail(function(){
					addAlert("red", "Error! We couldn't connect to our login servers, try again later!");
				});

				if(getCookie("location") == ""){
					var usTime = Date.now();
					var euTime = 0;
					var asTime = 0;
					$.ajax({
						type: 'GET',
						url: 'http://ping-us.codetunnel.net:14159'
					}).done(function(){
						usTime = Date.now() - usTime;
						euTime = Date.now();
						$.ajax({
							type: 'GET',
							url: 'http://ping-eu.codetunnel.net:14159'
						}).done(function(){
							euTime = Date.now() - euTime;
							asTime = Date.now();
							$.ajax({
								type: 'GET',
								url: 'http://ping-as.codetunnel.net:14159'
							}).done(function(){
								asTime = Date.now() - asTime;
								console.log("Your latency to our US network: " + usTime + "ms");
								console.log("Your latency to our EU network: " + euTime + "ms");
								console.log("Your latency to our AS network: " + asTime + "ms");
								console.info("NOTE: Thats not the actual latency as this time is the time it takes to get an http response!!!");
								console.log("To get the actual latency ping our networks");
								if(euTime < usTime && euTime < asTime)
									$(".location[data-show='servers-eu']").click();
								if(asTime < euTime && asTime < usTime)
									$(".location[data-show='servers-as']").click();
							});
						});
					});
				}
				setInterval(function(){
					$.ajax({
						type: 'GET',
						url: 'http://api.agariohub.net/servers'
					}).done(function(r){
						totalOnline = 0;
						for(var i = 0; i < servers.length; i++){
							servers[i].players = 0;
							servers[i].message = "";
						}
						for(var i = 0; i < r.servers.length; i++){
							var respServer = r.servers[i],
								target     = getServer(r.servers[i].label, r.servers[i].location);

							totalOnline += respServer.players;
							target.players += respServer.players;
							if(target.message == ""){
								target.message = respServer.message;
							}else if(target.message != respServer.message){
								target.message = "no_message";
							}
						}
						for(var i = 0; i < servers.length; i++){
							var onlineMessage = servers[i].players + " Alive Players";
							if(servers[i].message != "no_message") onlineMessage = servers[i].message;
							$(".online[data-id='" + servers[i].label.toLowerCase().split(" ").join("-") + servers[i].location + "']").html(onlineMessage);
						}
						$("#player-global").html(totalOnline + " Alive Players");
					});
				}, 10000);

				$(".location").click(function(){
					$(".servers").hide();
					$(".servers#" + $(this).data("show")).show();

					$(".location").removeClass("selected");
					$(this).addClass("selected");

					setCookie("location", $(this).data("show"), 365);
				});
				$("#mask").click(function(){
					$("#mask").fadeOut(400);
					$(".modal").fadeOut(200);
				});
				$(".modal-btn").click(function(){
					$("#mask").fadeOut(400);
					$(".modal").fadeOut(200);
				});
			});
			function connectServer(id){
				var that = this;
				$.ajax({
					type: 'GET',
					url: 'http://api.agariohub.net/servers'
				}).done(function(r){
					r.servers.reverse();
					for(var i = 0; i < servers.length; i++){
						if(id != servers[i].label.toLowerCase().split(" ").join("-") + servers[i].location)
							continue;
						for(var j = 0; j < r.servers.length; j++){
							if(r.servers[j].label != servers[i].label || r.servers[j].location != servers[i].location)
								continue;
							if(["OFFLINE", "Under Maintenance!", "Arena Closed!", "Full!"].indexOf(r.servers[j].message) > -1)
								continue;
							window.location = "http://agariohub.net/" + r.servers[j].label.toLowerCase().split(" ").join("-") + "-" + r.servers[j].arena;
							return;
						}
					}
					addAlert("red", "There is no available arena running this gamemode at the moment");
				});
			}
			function connectArena(id){
				var that = this;
				$.ajax({
					type: 'GET',
					url: 'http://api.agariohub.net/servers'
				}).done(function(r){
					r.servers.reverse();
					for(var i = 0; i < r.servers.length; i++){
						if(id != r.servers[i].label.toLowerCase().split(" ").join("-") + r.servers[i].location + r.servers[i].arena)
							continue;
						if(["OFFLINE", "Under Maintenance!", "Arena Closed!", "Full!"].indexOf(r.servers[i].message) > -1)
							continue;
						window.location = "http://agariohub.net/" + r.servers[i].label.toLowerCase().split(" ").join("-") + "-" + r.servers[i].arena;
						return;
					}
					addAlert("red", "This arena is unavailable at the moment");
				});
			}
			function chooseArena(id){
				var that = this;
				$.ajax({
					type: 'GET',
					url: 'http://api.agariohub.net/servers'
				}).done(function(r){
					r.servers.reverse();
					$(".arenas").html("");
					for(var i = 0; i < servers.length; i++){
						if(id != servers[i].label.toLowerCase().split(" ").join("-") + servers[i].location)
							continue;
						$(".modal h2").html(servers[i].label);
						for(var j = 0; j < r.servers.length; j++){
							if(r.servers[j].label != servers[i].label || r.servers[j].location != servers[i].location)
								continue;
							$(".arenas").append($("#model-arena").html()
								.split("%label%").join(r.servers[j].label + " #" + r.servers[j].arena)
								.split("%players%").join(r.servers[j].message == "no_message" ? r.servers[j].players + " Alive Players" : r.servers[j].message)
								.split("%id%").join( r.servers[j].label.toLowerCase().split(" ").join("-") + r.servers[j].location + r.servers[j].arena )
								.split("%list%").join(r.servers[j].users.join("<br>")));
						}
						break;
					}
					$("#mask").fadeIn(200);
					$(".modal").fadeIn(400);
				});
			}
			function addAlert(color, message) {
				$("#alerts").append($("#model-alert").html().split("%color%").join(color).split("%message%").join(message));
			}
			function removeMe(me){
				$(me).fadeOut(200, function(){
					$(me).remove();
				});
			}
		</script>

