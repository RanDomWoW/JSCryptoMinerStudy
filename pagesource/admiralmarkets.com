        <script>

            // LazyLoad
            var observer = lozad('.lozad', {
                threshold: 0.1,
                load: function(el) {
                    el.src = el.getAttribute("data-src");
                }
            });
            observer.observe();

            

            var detectModule = (function () {

                var data = [];
                var unknown = '-';

                // detect browser with User Agent string
                var nVer = navigator.appVersion;
                var nAgt = navigator.userAgent;

                var setBrowser = function () {

                    data['browser'] = unknown;

                    // Opera
                    if ((nAgt.indexOf('Opera')) != -1) {
                        data['browser'] = 'Opera';
                    }
                    // Opera Next
                    if ((nAgt.indexOf('OPR')) != -1) {
                        data['browser'] = 'Opera';
                    }
                    // Edge
                    else if ((nAgt.indexOf('Edge')) != -1) {
                        data['browser'] = 'Microsoft Edge';
                    }
                    // MSIE
                    else if ((nAgt.indexOf('MSIE')) != -1) {
                        data['browser'] = 'MSIE';
                    }
                    // Chrome
                    else if ((nAgt.indexOf('Chrome')) != -1) {
                        data['browser'] = 'Chrome';
                    }
                    // Safari
                    else if ((nAgt.indexOf('Safari')) != -1) {
                        data['browser'] = 'Safari';
                    }
                    // Firefox
                    else if ((nAgt.indexOf('Firefox')) != -1) {
                        data['browser'] = 'Firefox';
                    }
                    // MSIE 11+
                    else if (nAgt.indexOf('Trident/') != -1) {
                        data['browser'] = 'MSIE';
                    }
                };

                var setMobile = function () {

                    data['mobile'] = false;
                    // is mobile?
                    data['mobile'] = /Mobile|mini|BlackBerry|Kindle|IEMobile|webOS|Fennec|Android|iP(ad|od|hone)/.test(nVer);
                };

                var setDevicePlatform = function () {

                    // operating system
                    data['os'] = unknown;

                    var clientStrings = [
                        {s:'Windows 10', r:/(Windows 10.0|Windows NT 10.0)/},
                        {s:'Windows 8.1', r:/(Windows 8.1|Windows NT 6.3)/},
                        {s:'Windows 8', r:/(Windows 8|Windows NT 6.2)/},
                        {s:'Windows 7', r:/(Windows 7|Windows NT 6.1)/},
                        {s:'Windows Vista', r:/Windows NT 6.0/},
                        {s:'Windows Server 2003', r:/Windows NT 5.2/},
                        {s:'Windows XP', r:/(Windows NT 5.1|Windows XP)/},
                        {s:'Windows 2000', r:/(Windows NT 5.0|Windows 2000)/},
                        {s:'Windows ME', r:/(Win 9x 4.90|Windows ME)/},
                        {s:'Windows 98', r:/(Windows 98|Win98)/},
                        {s:'Windows NT 4.0', r:/(Windows NT 4.0|WinNT4.0|WinNT|Windows NT)/},
                        {s:'Windows CE', r:/Windows CE/},
                        {s:'Android', r:/Android/},
                        {s:'Open BSD', r:/OpenBSD/},
                        {s:'Sun OS', r:/SunOS/},
                        {s:'Linux', r:/(Linux|X11)/},
                        {s:'iOS', r:/(iPhone|iPad|iPod)/},
                        {s:'Mac OS X', r:/Mac OS X/},
                        {s:'Mac OS', r:/(MacPPC|MacIntel|Mac_PowerPC|Macintosh)/},
                        {s:'QNX', r:/QNX/},
                        {s:'UNIX', r:/UNIX/},
                        {s:'BeOS', r:/BeOS/},
                        {s:'OS/2', r:/OS\/2/},
                        {s:'Search Bot', r:/(nuhk|Googlebot|Mail|Openbot|Slurp|MSNBot|Yandex|bingbot)/}
                    ];

                    for (var id in clientStrings) {
                        var cs = clientStrings[id];
                        if (cs.r.test(nAgt)) {
                            data['os'] = cs.s;
                            break;
                        }
                    }
                };

                return {
                    init: function () {
                        setBrowser();
                        setMobile();
                        setDevicePlatform();
                    },
                    getBrowser: function () {
                        return data['browser'];
                    },
                    isMobile: function () {
                        return data['mobile'];
                    },
                    getDevicePlatform: function () {
                        return data['os'];
                    },
                    getDevice: function () {
                        if(isMobile && screen.width < 699){
                            return 'mobile';
                        }
                        if (isMobile && screen.width >= 700){
                            return 'tablet';
                        }

                        return 'desktop';
                    }
                }
            }());

            detectModule.init();

            

            // Sourcebuster (analytics)
