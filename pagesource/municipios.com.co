	<script type="text/javascript">
        $(document).ready(function(){
            
            			$('.flexslider').flexslider({animation: "slide"});
			var mouseOver = false;
            $("#campobusqueda").keyup(function() {
                if($("#campobusqueda").val().length >=3){
                    $.get('http://www.municipios.com.co/autocompletar.php', { q: $("#campobusqueda").val()}, function(data) {
                        $('#autocompletar').html(data);
                        $('#autocompletar').css('left',$("#campobusqueda").offset().left);
                        $('#autocompletar').css('top',$("#campobusqueda").offset().top+$("#campobusqueda").outerHeight());
                        $('#autocompletar').show();
                        
                        $("#autocompletar")
                        .mouseover(function() { mouseOver = true; })
                        .mouseout(function() { mouseOver = false; });

                    });
                }
            });
            
            $("#campobusqueda").blur(function() {
                if(!mouseOver){
                    $('#autocompletar').html('');
                    $('#autocompletar').hide();
                }
            });

            $("#toggle_buscar").click(function() {
            	if( $("#menu form").css('display') == "none") $("#menu form").css('display','block');
            	else $("#menu form").css('display','none');
            });
            
            			
        });
    </script>
    
