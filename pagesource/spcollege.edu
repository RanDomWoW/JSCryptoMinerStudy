                        $("#go-keywords").attr("placeholder", "SEARCH - CURRENT TITANS");
                        $("#search-form-container").slideDown("slow");
                    } else {
                        $("#search-form-container").slideUp();
                        $("#go-keywords").attr("placeholder", "SEARCH");
                    }
                    e.stopPropagation();
                });

                $('#links-btn, #links-btn2').click(function (e) {
                    $("#search-form-container").hide();
                    if ($("#quickLinkList").is(":hidden")) {
                        $("#quickLinkList").slideDown("slow");
                    } else {
                        $("#quickLinkList").slideUp();
                    }
                    e.stopPropagation();
                });

                $('#quicklink-entry-toggle').click(function (e) {
                    if ($("#quickLinkListMobile").is(":hidden")) {
                        $("#quickLinkListMobile").slideDown("slow");
                    } else {
                        $("#quickLinkListMobile").slideUp();
                    }
                    e.stopPropagation();
                });



                $(window).click(function (e) {
                    if (!$('#search-form-container').is(":hidden")) {
                        
                        if (!$(e.target).parents().hasClass('search-container')) {
                            //window.alert("test");
                            $('#search-form-container').slideUp();
                            $("#go-keywords").attr("placeholder", "SEARCH");
                        }
                    }
                    if (!$('#quickLinkList').is(":hidden")) {
                        if (!$(e.target).parent().hasClass('quicklink-block')) {
                            $('#quickLinkList').slideUp();
                        }
                    }
                    if (!$('#quickLinkListMobile').is(":hidden")) {
                        if (!$(e.target).parent().hasClass('inner-wrapper')) {
                            $('#quickLinkListMobile').slideUp();
                        }
                    }
                });

            </script>


            
