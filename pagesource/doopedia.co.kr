<script type="text/javascript">

	function goLogin(){
		var objF = document.frmLoginRtnUrl;
		var encdata = uriEncode(objF.rtn_url.value);
		objF.rtn_url.value = encdata;
		objF.submit();
	};

	function beforeLogin(obj){
		var encdata = uriEncode(obj.rtn_url.value);
		obj.rtn_url.value = encdata;			
		
		if(obj.mem_id.value == ""){
			FES.alert("아이디를 입력해 주세요.", function(){
				obj.mem_id.focus();
			});
			return false;
		}else if(obj.mem_pwd.value == ""){
			FES.alert("비밀번호를 입력해 주세요.", function(){
				obj.mem_pwd.focus();
			});
			return false;
		}else{
			return true;
		}
	};

	function uriEncode(data){ 
		var encdata = '';  
		if(data != ""){ 
			if(data.indexOf('&') > 0){
				var datas = data.split('&');  
				for(i=0;i<datas.length;i++){
					var pos = '';
					if(i!=0){
						pos = '&';
					}
					if(data.indexOf('=') > 0){
						var strVal = datas[i].split('=');
						var strVal1 = strVal[0];
						var strVal2 = strVal[1];
						if(strVal2 !=''){
							strVal = pos + strVal[0] + "=" + encodeURIComponent(strVal[1]);
						}else{
							strVal = pos + strVal[0] + "=" + strVal[1];
						}
						encdata += strVal;
					}
				}						
			}else{
				encdata = data;
			}
		}else{
			encdata = ""; 
		} 
		return encdata;
	};		
	
	function goRegist(){
		

				goLogin();
		

	};
	
