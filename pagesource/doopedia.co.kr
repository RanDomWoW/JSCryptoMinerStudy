			FES.alert('검색날짜를 올바르게 입력하여 주십시오.', function(){});
			return false;
		}
		
		var frm= document.getElementById('fmGalList');
		frm.action = "/photobox/comm/community.do?_method=adminSearchList";
		frm.target = "";
		document.getElementById("page").value = 1;
		
		frm.submit();
		//return true;
	};
	
	//아이디 저장
	function saveMemId(){
		var frmLoginForm = document.getElementById("frmLoginForm");
		var mem_id = frmLoginForm.mem_id.value;
		if(frmLoginForm.idremember.checked){
			$.post('/doopedia/member/member.do?_method=saveMemIdCookie',
					{mem_id:mem_id},
					function(data){ ; }
			);
		}else{
			$.post('/doopedia/member/member.do?_method=removeMemIdCookie',
					{mem_id:mem_id},
					function(data){ ; }
			);		
		}
	};		
	
	//로그인
	function topBeforeSubmit(){
		frontLoading.show();
		beforeLogout();
		var frmLoginForm = document.getElementById("frmLoginForm");
		if(frmLoginForm.mem_id.value == ""){
			FES.alert("아이디를 입력해 주세요.", function(){
				frontLoading.hide();
				frmLoginForm.mem_id.focus();
			});
			return false;
		}else if(frmLoginForm.mem_pwd.value == ""){
			FES.alert("비밀번호를 입력해 주세요.", function(){
				frontLoading.hide();
				frmLoginForm.mem_pwd.focus();
			});
			return false;
		}else{
			travelLogin(); //여행기 자동로그인 막음 poppppp 20160523
			saveMemId();
			wait(500);
			return true;
		}
	};
	
	function wait(msecs)
	{
		var start = new Date().getTime();
		var cur = start;
		while(cur - start < msecs)
		{
		cur = new Date().getTime();
		}
	}
	
	function travelLogin(){
		var frmLoginForm = document.getElementById("frmLoginForm");
		
		$('#fm_id').val(frmLoginForm.mem_id.value);
		$('#fm_pw').val(frmLoginForm.mem_pwd.value);
		
		var travelLogin = document.getElementById("travelLoginFm");
		travelLogin.target = 'travelLogin';
		travelLogin.action = 'https://www.doopediat.com/10_Menu/LoginOK';//11_Menu/sso_login
		travelLogin.submit();
		return false;
	}
	
	function doopediaLogout(){
 		var travelLogin = document.getElementById("travelLoginFm");
 		travelLogin.target = 'travelLogin';
 		travelLogin.action = 'http://www.doopediat.com/10_Menu/LogoutOK';
 		travelLogin.submit();

        wait(500);
		
		var frmLoginForm = document.getElementById("frmLoginForm");
		frmLoginForm.action = '/doopedia/member/member.do?_method=logOut';
		frmLoginForm.submit();
	}
	
	function beforeLogout(){
		$.ajax({
    		type: "GET",
    		url: 'http://www.doopediat.com/10_Menu/LogoutOK',
    		dataType:"json",
    		success: function(data){
    		}
    	});
		
		$.ajax({
    		type: "GET",
    		url: '/doopedia/member/member.do?_method=logOut',
    		dataType:"json",
    		success: function(data){
    		}
    	});
	}
	
	function openEnIdPwdSearchPopup(){
		popupWindow("enIdPwdSearchPopup", "/doopedia/member/member.do?_method=enIdPwdSearch", 640, 280);
	}
</script>
			


