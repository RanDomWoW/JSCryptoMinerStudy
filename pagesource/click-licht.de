<style type="text/css" data-dropper-inlined="//www.click-licht.de/includes/plugins/kk_dropper/drops/CL_GridBanner/css/gridbanner.css?v=1.0.0">
.cl-grid-banner {
	margin: -10px;
}

.cl-grid-banner-row, .cl-grid-banner-column {
	display: flex;
	flex: 1 0 auto;
}

.cl-grid-banner-row {
	flex-direction: row;
}

.cl-grid-banner-column {
	flex-direction: column;
}

.cl-grid-banner-sizer {
	width: 100%;
	padding-top: 100%;
}

.cl-grid-banner-child {
	flex: 1;
	position: relative;
}

.cl-grid-banner-image {
	position: absolute;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	background: #F2F1EF no-repeat center center;
	background-size: cover;
	cursor: default;
	padding: 1.75em;
	border-radius: 2px;
	z-index: 0;
	transition: all 0.5s ease-out;
	overflow:hidden;
}

.cl-grid-banner-image:hover {
	color: inherit;
	filter: grayscale(80%);
}
.cl-grid-banner-mobile .cl-grid-banner-image:hover {
	/*DO NOTHING CUZ OF MOBILE*/
	filter: grayscale(0%);
}

.cl-grid-banner-image[href] {
	cursor: pointer;
}
.cl-grid-banner-wrapper{
	position:absolute;
	overflow:hidden;
}

.cl-grid-banner-wrapper:hover .cl-grid-banner-overlay{
	transform: translateY(-30px); 
}

.cl-grid-banner-wrapper:hover .cl-grid-banner-subtitle{
	transform: translateY(-160px); 
}

.cl-grid-banner-mobile .cl-grid-banner-wrapper:hover .cl-grid-banner-subtitle{
	/*DO NOTHING CUZ OF MOBILE*/
	transform: translateY(0) !important; 
}

.cl-grid-banner-mobile .cl-grid-banner-wrapper:hover .cl-grid-banner-overlay{
	/*DO NOTHING CUZ OF MOBILE*/
	transform: translateY(0) !important; 
}

.cl-grid-banner-subtitle{
	position: absolute;
	bottom: -150px;
	right: 10px;
	padding: 0.3em 0.5em 0.3em 0.5em;
	background: rgba(134, 131, 125, 0.92);
	font-size: 0.9em;
	font-weight: 300;
	color:white;
	transition: all 0.5s cubic-bezier(0.21, 1.51, 0.46, 0.99);
	transform: rotate3d(0,0,1,-40deg);
	pointer-events: none;
}

.cl-grid-banner-overlay {
	position: absolute;
	bottom: 10px;
	right: 10px;
	padding: 1.2em 0.8em 0.3em 0.8em;
	font-size: 1.2em;
	font-weight: 300;
	font-family: "Sansation";
	letter-spacing: -0.04em;
	line-height: normal;
	color: white;
	text-align:right;
	text-shadow: 0px 1px 3px rgba(0,0,0,0.2);
	font-style: italic;
	background-size: auto auto;
	background-size: cover;
	background: url(/templates/CL-Next/themes/clnext/images/green_bg_slider_top.png);
	transition: all 0.2s ease-out;
	pointer-events: none;
}

.cl-grid-banner-child.size-s {
	flex: 1;
}

.cl-grid-banner-child.size-m {
	flex: 1;
}

.cl-grid-banner-child.size-l {
	flex: 2;
}

.cl-grid-banner-child.size-xl {
	flex: 3;
}
</style>

<style type="text/css" data-dropper-inlined="//www.click-licht.de/includes/plugins/kk_dropper/drops/CL_Banner/cl-banner.css?v=1.0.0">
.cl-banner{
	display: block;
	width: 100%;
	float: left;
	position: relative;
	line-height: 1;
}

.cl-banner:after{
	content: "";
	clear: both;
}

.cl-banner .cl-banner-image{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background-size: cover;
	background-repeat: no-repeat;
	background-position: top left;
	line-height: 0;
	border-radius: 2px;
}

.cl-banner .cl-banner-image-tag{
	width: 100%;
	line-height: 0;
	display: block;
	float: left;
	border-radius: 2px;
}

.cl-banner .cl-banner-image-tag:after{
	content: "";
	display: block;
	clear:both;
}

.cl-banner .cl-banner-link{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 1;
	background: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVQYV2NgYAAAAAMAAWgmWQ0AAAAASUVORK5CYII=") 0 0 repeat;
}

.cl-banner .cl-banner-overlay{
	position: absolute;
	left: 0;
	bottom: 0;
	padding: 1.5em;
	width: 100%;
	background: #F2F1EF;
	background: rgba(238, 238, 238, 0.88);
	border-radius: 0 0 2px 2px;
}

.cl-banner .cl-banner-overlay.cl-banner-overlay-small{
	font-size: 0.85em;
}

.cl-banner .cl-banner-overlay.cl-banner-overlay-mini{
	font-size: 0.75em;
}

.cl-banner .cl-banner-title{
	font-size: 1.8em;
	font-weight: 300;
	font-family: "Sansation";
	letter-spacing: -0.04em;
	line-height: normal;
}

.cl-banner .cl-banner-title:after{
	content: "";
	display: inline-block;
	margin-left: 0.5em;
	border: 8px solid transparent;
	border-left-color: #89B40F;
}

.cl-banner.dont-link .cl-banner-title:after{
	display: none;
}

.cl-banner .cl-banner-text{
	font-weight: 300;
	font-size: 1em;
	color: #6F6C6C;
	margin-top: 0.5em;
	display: block;
	white-space: normal;
	line-height: normal;
}

.cl-banner .cl-banner-overlay-image{
	position: absolute;
	right: 20px;
	bottom: 40px;
	width: 124px;
	height: 124px;
	border-radius: 0 0 2px 2px;
}

.cl-banner.has-overlay .cl-banner-overlay{
	padding-right: 170px;
}

.cl-banner.hoveroverlay .cl-banner-text{
	overflow: hidden;
	margin: 0;
	height: 0;
	opacity: 0;
	transition: height 200ms, margin 200ms, opacity 200ms;
	vertical-align: top;
}

.cl-banner.hoveroverlay:hover .cl-banner-text{
	transition: height 200ms 300ms, margin 200ms 300ms, opacity 200ms 300ms;
	height: 1.5em;
	opacity: 1;
}
/*.cl-banner .textElement{position:absolute !important;}*/
</style>

