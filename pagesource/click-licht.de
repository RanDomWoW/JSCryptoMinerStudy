	box-sizing: border-box;
	display: block;
}

.grid-system, .kk-flexlayout {
	display: inline-block;
	width: 100%;
	vertical-align: top;
}

.kk-flexlayout {
	display: block;
}

.grid-system .grid-system-row {
	display: block;
	display: -webkit-flex;
	display: flex;
}

.grid-system .grid-space {
	display: block;
	float: left;
}

.grid-system .grid-clear {
	clear: both;
	display: block;
	-webkit-flex: 0;
	flex: 0;
	line-height: 0;
	width: 100%;
}

.grid-system.equal-spaces > .grid-system-row > .grid-space.equal-space {
	display: -webkit-flex;
	display: flex;
	-webkit-flex: auto;
	flex: auto;
	-webkit-flex-direction: column;
