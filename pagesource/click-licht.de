	<script>

		function setCookie(cname, value) {
			var now = new Date(),
				time = now.getTime(),
				expireTime = time + 86400000*365;
			now.setTime(expireTime);
			document.cookie = cname + '=' + value + ';expires=' + now.toGMTString() + ';path=/';
		}

		function getCookie(cname) {
		    var name = cname + '=';
		    var ca = document.cookie.split(';');
		    for(var i = 0; i <ca.length; i++) {
		        var c = ca[i];
		        while (c.charAt(0) == ' ') {
		            c = c.substring(1);
		        }
		        if (c.indexOf(name) == 0) {
		            return c.substring(name.length, c.length);
		        }
		    }
		    return '';
		}

		function cookieNotice (data) {

			var selectors = data.selectors,
				contents = data.contents,
				wrapper = selectors.wrapper,
				htmlSelector = selectors.html,
				clickSelector = selectors.click,
				$wrapper = $(wrapper),
				$html = $(wrapper + htmlSelector),
				$click = $(wrapper + clickSelector);

			$html
				.prepend(contents.de_de.text);

			$click
				.prepend(contents.de_de.click_text);

			$wrapper
				.show();

			$click.on('click ontouchstart', function() {
				setCookie('cookieNoticeRead', 'true');
				$(wrapper)
					.remove();
			});

		}

		$(window).load(function() {

			if (getCookie('cookieNoticeRead') !== 'true') {

				document.cookie = 'cookieNoticeRead=false';

				cookieNotice({

					selectors:{
						wrapper: '.cookie-notice',
						html: ' p',
						click: ' span'
					},

					contents:{
						de_de: {
							text: 'Diese Website verwendet <a class="green" href="/Datenschutz#Cookies" target="_blank" title="Zur Datenschutzseite">Cookies' + '<' + '/' + 'a>' + ', um Inhalte zu personalisieren, Funktionen für soziale Medien anbieten zu können und die Zugriffe auf unserer Website zu analysieren.<br>',
							click_text: 'Hinweis gelesen und nicht wieder anzeigen'
						}
					}

				});

			}

		});

	</script>
