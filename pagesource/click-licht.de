<style type="text/css" data-dropper-inlined="//www.click-licht.de/includes/plugins/kk_dropper/drops/CL_BottomBar/bottombar.css?v=1.0.0">
/*********************************
 Bottom Bar
**********************************/
#bottom-bar{
	display: inline-block;
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	text-align: left;
	z-index: 4;
	padding: 0.54em 0;
	background: rgb(46, 46, 46);
	background: rgba(46, 46, 46, 0.92);
}

#bottom-bar.stay-open{
	bottom: 0 !important;
}

#bottom-bar .center-page{
	position: static;
}

#bottom-bar .bottom-bar-item{
	display: inline-block;
	padding: 0.25em 1.5em;
	border-right: 1px solid white;
	border-right: 1px solid rgba(255,255,255,0.2);
}

#bottom-bar .bottom-bar-item:first-child{
	padding-left: 0;
}

#bottom-bar .bottom-bar-item.last{
	border-right: 0 none;
}

#bottom-bar .bottom-bar-label{
	font-family: "Sansation";
	font-size: 1.2em;
	color: white;
	letter-spacing: -0.03em;
	font-weight: 300;
	opacity: 0.7;
	transition: opacity 200ms;
	cursor: pointer;
}

#bottom-bar .bottom-bar-item:hover .bottom-bar-label,
#bottom-bar .bottom-bar-item.active .bottom-bar-label{
	opacity: 1;
}


#bottom-bar .bottom-bar-item.active .bottom-bar-label{
	color: #89B40F;
}

#bottom-bar .bottom-bar-label:after{
	content: "\f10d";
	font-family: "Ionicons";
	margin-left: 0.75em;
	opacity: 0.7;
}

#bottom-bar .bottom-bar-item.active .bottom-bar-label:after{
	content: "\f104";
}

#bottom-bar .bottom-bar-item.empty .bottom-bar-label:after{
	display: none;
}

#bottom-bar .bottom-bar-label .count{
	background: white;
	color: rgb(51, 51, 51);
	display: inline-block;
	width: 2em;
	height: 2em;
	border-radius: 2em;
	line-height: 2em;
	text-align: center;
	font-weight: bold;
	margin-right: 0.5em;
	font-size: 0.7em;
	vertical-align: middle;
	position: relative;
	top: -0.1em;
}

#bottom-bar .bottom-bar-item.active .bottom-bar-label .count{
	background: #89B40F;
}


#bottom-bar .bottom-bar-content{
	position: absolute;
	width: auto;
	display: block;
	bottom: 100%;
	padding: 1em;
	visibility: hidden;
	opacity: 0;
	margin-left: -2em;
	box-shadow: 0 0 0 1px rgba(0,0,0,0.1);
	border-radius: 2px 2px 0 0;
	text-align: center;
	transform: translateY(20px);
	transition: all 200ms;
}

#bottom-bar .bottom-bar-item.active .bottom-bar-content{
	opacity: 1;
	visibility: visible;
	transform: translateY(0);
}

#bottom-bar .bottom-bar-content .bar-articles{
	white-space: nowrap;
}

#bottom-bar .bottom-bar-content .bar-article{
	width: 200px;
	float: left;
	display: inline-block;
	white-space: normal;
}

#bottom-bar .bottom-bar-content .article{
	width: 100%;
	height: 385px;
}

#bottom-bar .bottom-bar-content .bar-articles .bar-article:last-child .article .article-wrap{
	border-right: 0 none;
}

#bottom-bar .scroll-to-top {
	color: white;
	background: #89B40F;
	display: inline-block;
	float: right;
	position: absolute;
	top: 0;
	bottom: 0;
	padding: 0;
	font-size: 1.2em;
	line-height: 2.8em;
	width: 3em;
	text-align: center;
	margin-left: -3em;
	cursor: pointer;
	transition: top 200ms;
}

#bottom-bar.scrolled .scroll-to-top{
	top: -40px;
	border-radius: 2px 2px 0 0;
}

</style>

