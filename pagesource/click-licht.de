<style type="text/css" data-dropper-inlined="//www.click-licht.de/includes/plugins/kk_dropper/drops/CL_Nosto/nosto.css?v=1.0.3">
.cl-nosto {
	position: relative;
}

.cl-nosto.cl-nosto-loading {
	height: 200px;
}

.cl-nosto:after {
	content: "";
	visibility: collapse;
	opacity: 0;
	position: absolute;
	left: 50%;
	top: 0;
	width: 200px;
	height: 200px;
	margin-left: -100px;
	z-index: 1;
	background: url(/includes/plugins/kk_dropper/drops/CL_Nosto/img/cl-loading-spinner.png) center center no-repeat;
	animation: cl-nosto-loader-anim 2000ms linear infinite;
	transition: opacity 300ms, visibility 300ms;
}

.cl-nosto.cl-nosto-loading:after {
	visibility: visible;
	opacity: 1;
}

.cl-nosto > * {
	opacity: 1;
	transition: opacity 1000ms 100ms;
}

.cl-nosto.cl-nosto-loading > * {
	opacity: 0;
	transition: none;
}

@keyframes cl-nosto-loader-anim{
	0% {
		transform: rotate(0deg);
	}
	100% {
		transform: rotate(356deg);
	}	
}
</style>

