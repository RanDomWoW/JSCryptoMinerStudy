		var per_width	=	$('#sitelist_items div').eq(0).width();
		var dl_index	=	function(){
			$('#sitelist_items div').eq(0).insertAfter($('#sitelist_items div').eq(count-1));
			$('#sitelist_items').css('left',0);
		}

		var dl_roll	=	function(){
			if(!stopped2){
				if(direction=='left')
				{
					$('#sitelist_items').animate({left:per_width*(-1),top:0}, speed, dl_index);
				}
				else if(direction=='right')
				{
					$('#sitelist_items div').eq(count-1).insertBefore($('#sitelist_items div').eq(0));
					$('#sitelist_items').css('left',per_width*(-1));
					$('#sitelist_items').animate({left:0,top:4}, speed);
				}
				
			}
			setTimeout(dl_roll,time_x);
		}

		$('#sitelist_items').mouseover(function(){stopped2 = true;});
		$('#sitelist_items').mouseout(function(){stopped2 = false;});
		
		$('.cont_slst_forwlink').mouseover(function(){stopped2 = true;});
		$('.cont_slst_forwlink').mouseout(function(){stopped2 = false;});
		$('.cont_slst_backlink').mouseover(function(){stopped2 = true;});
		$('.cont_slst_backlink').mouseout(function(){stopped2 = false;});

		setTimeout(dl_roll, time_x);
		
	});
	
	function show_prenxt(direction){
		var time_x =  2000;
		var speed  = 400;
		if(time_x/2 < speed)	time_x = 2*speed;
		var stopped2	=	false;
		//var direction	=	'left';
		var count	=	$('#sitelist_items div').length;
		var per_width	=	$('#sitelist_items div').eq(0).width();
		
		var dl_index	=	function(){
			$('#sitelist_items div').eq(0).insertAfter($('#sitelist_items div').eq(count - 1));
			$('#sitelist_items').css('left', 0);
		}
		if(direction=='left')
		{
			$('#sitelist_items').animate({left:per_width*(-1),top:0}, speed, dl_index);
		}
		else if(direction=='right')
		{
			$('#sitelist_items div').eq(count-1).insertBefore($('#sitelist_items div').eq(0));
			$('#sitelist_items').css('left',per_width*(-1));
			$('#sitelist_items').animate({left:0,top:3}, speed);
		}
	}
	
