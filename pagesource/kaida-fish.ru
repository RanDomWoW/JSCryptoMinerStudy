<script>
	function responseSearchHelper (response) {

		

		var itemHtml = '';

		if(response.length) {

			$('.response_show_all').css('display', 'block');
			$('.response').fadeIn(600);

			for (var i = response.length - 1; i >= 0; i--) {

				var item = response[i];

				if (item.name.length>40) {
					var spiner = '...';
				} else {
					var spiner = '';
				}

				itemHtml += '<div class="response_item"><a href="'+item.url+'">'+
							'<img src="/thumb/37x37xCUT'+item.picture+'" alt="'+item.name+'" /></a>'+
							'<a class="ra" href="'+item.url+'">'+item.name.slice(0,40)+spiner+'</a></div>';

			};

			$('.response_items_list').html(itemHtml);

		} else {
			$('.response').fadeIn(600);
			$('.response_show_all').css('display', 'none');
			$('.response_items_list').html('<p class="response_empty">По вашему запросу ничего не найдено</p>');
		
		}
	}

	$('#title-search').on('keyup', '#title-search-input', function(){
		var val = $(this).val();

		$('.response_show_all').attr('href', '/search/?q='+val);

		if(val.length>1) {

			$.ajax({
				type: 'POST',
				url: '/ajax/ajax-helpers.php',
				dataType: 'json',
				data: {
					type: 'search',
					query: val
				},
				success: function(data) {

					responseSearchHelper(data);

				}
			});

		}

	});

	$('#title-search').on('blur', '#title-search-input', function(){
		$('.response').fadeOut(200);
	});
</script>		<div class="clear"></div>
