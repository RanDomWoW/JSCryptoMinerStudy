    color: #333;
    font-size: 50px;
    margin-top: auto;
    margin-bottom: auto;
    margin-right: 1%;
    text-align: center;
    vertical-align: middle;
    width: 35%;
}

fieldset.kennzeichen .kennzeichen-button {
	width: 50%;
	float: right;
	height: 100%;
}

