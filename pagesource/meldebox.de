<style>.autocomplete-suggestions {
	background: #239bde;
	border-top: 0;
	box-shadow: 0 5px 11px 1px rgba(0,0,0,.2);
	box-sizing: border-box;
	cursor: default;
	display: none;
	max-height: 254px;
	overflow: hidden;
	overflow-y: auto;
	position: absolute;
	text-align: left;
	z-index: 9999;
}

.autocomplete-suggestion {
	color: #fff;
	font-size: 18px;
	overflow: hidden;
	padding: .25em .5em;
	position: relative;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.autocomplete-suggestion b {
	font-weight: bold;
}

.autocomplete-suggestion.selected {
	background: #fff;
	color: #23527c;
}

.search-field-box {
	width: 100% !important;
}

@media (min-width: 768px) {
	.search-field-box {
		width: 300px !important;
	}
}
