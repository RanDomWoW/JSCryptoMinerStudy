  <script type="text/javascript">



    // send a ga event for an download, with info of the page
    // envia um evento do google analytics, para avisar que um usuário fez um
    // download ou de pdf ou de jpg/jpeg
    function send_ga(){
      var location = window.location.pathname;
      var division = "";

        //list of divisions to test
      var division_arr = [
        '/produtos/materiais-eletricos',
        '/produtos/fechaduras-e-ferragens',
        '/produtos/cilindros-hidraulicos',
        '/produtos/utilidades-domesticas',
        '/suporte/downloads/fechaduras-e-ferragens',
        '/suporte/downloads/materiais-eletricos',
        '/suporte/downloads/cilindros-hidraulicos',
        '/suporte/downloads/utilidades-domesticas',
        '/suporte/downloads/busca',
      ];

        //find out the current division using the url path and matching with de division_arr
      var n = -1;
      for(var i=0, len=division_arr.length; i < len; i++){

        n = location.search(division_arr[i]);
          //-1 is the result if not found
          //-1 equivale à nenhum resultado encontrado
        if(n != -1){
          division = division_arr[i];
        }

      }

        //if an division was found, send it to ga!
      if(division != ""){
        ga('send', 'event', division, 'download', location);
        console.log("sent: " + division + " location:" + location);
      }

    }

      //adiciona nos links para pdf e imagem a função de evento do ga
    jQuery('a[href$=".pdf"]').click(function(){
      send_ga();
    });
    jQuery('a[href$=".jpg"]').click(function(){
      send_ga();
    });


  </script>
