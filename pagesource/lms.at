<script>
var dark = '#909090';
var bright = '#d7d7d7';
var timeouts = [];

function show_image(index) {
    for (var i = 0; i < timeouts.length; i++) {
        clearTimeout(timeouts[i]);
    }
    var slider = $("#slider");
    run_animation($('#si-' + index), $('#slide-circle-' + index), true);
}

function show_mobile_image(index) {
    $(".frontpage-image-link").css('display', 'none');
    $('.slide-circle-mobile').css("color", bright);
    $('#slide_mobile_image_' + index).parent().show();
    $('#slide-circle-mobile_' + index).css("color", dark);
}

$(document).ready(function () {
    var slider = $("#slider");
    var slide_circles = $('.slide_circles');
    var stopped = false;
    var block_run_animation = false;
    $('#slide-circle-0').css('color', dark);
    $('.slide-circle-mobile:first').css('color', dark);
    var slide_images = $('.frontpage-images-for-desktop');

    slide_images.hover(function () {stopped = true;}, function () {
        stopped = false;
        for (var i = 0; i < timeouts.length; i++) {
            clearTimeout(timeouts[i]);
        }
        timeouts = [];
        run_animation();
    });

    window.run_animation = function(next_image, next_slide_circle, immediately) {
        var current_image_direction;
        var next_image_direction;
        if(immediately) {
            var interval = 0;
        } else {
            var interval = 5000
        }

        if(stopped) return;
        var next_image;
        var next_slide_circle;
        if (!block_run_animation) {
            timeouts.push (setTimeout(function () {

                if(stopped) return;
                var current_image = slider.find('.frontpage-image-link:visible');

                var current_slide_circle = slide_circles.find('.slide-circle-desktop[style*="144"]');
                current_slide_circle.css('color', bright);

                if (!(next_image && next_slide_circle)) {
                    next_image = current_image.next();
                    next_slide_circle = current_slide_circle.next().next();
                }

                if (!next_slide_circle.length) {
                    $('#slide-circle-0').css('color', dark);
                } else {
                    next_slide_circle.css('color', dark);
                }

                if (!next_image.length) next_image = slider.find('a:first');

                switch ("fade") {
                    case "fade":
                        next_image.fadeIn(1500).promise().done(function () {
                            next_image.css('display', 'inline');
                            block_run_animation = false;
                            run_animation();
                        });
                        current_image.fadeOut(1500).promise().done(function () {
                            current_image.css({'display': 'none'});
                        });
                        break;
                    case "slide-left":
                        current_image_direction = "left";
                        next_image_direction = "right";
                        break;
                    case "slide-right":
                        current_image_direction = "right";
                        next_image_direction = "left";
                        break;
                    case "slide-up":
                        current_image_direction = "up";
                        next_image_direction = "down";
                        break;
                    case "slide-down":
                        current_image_direction = "down";
                        next_image_direction = "up";
                        break;
                }

                if (current_image_direction) {
                    next_image.show("slide", {direction: next_image_direction}, 1500).promise().done(function () {
                        next_image.css('display', 'inline');
                        block_run_animation = false;
                        run_animation();
                    });
                    current_image.hide("slide", {direction: current_image_direction}, 1500).promise().done(function () {
                        current_image.css({'display': 'none'});
                    });
                }
            }, interval));
        }
    }
    run_animation();
});
</script>


