<script>
var toolbar_collapsed = false;

function collapse_toolbar() {
    try {
        for(var i in CKEDITOR.instances) {
            CKEDITOR.instances[i].execCommand("toolbarCollapse");
        }
    } catch(e) {}
}

$( document ).ready(function() {
    setTimeout(function(){
        if($(window).width() / parseFloat($("body").css("font-size")) < 50) {
            toolbar_collapsed = true;
            collapse_toolbar();
        }
    }, 2000);
});

$( window ).resize(function() {
    if($(window).width() / parseFloat($("body").css("font-size")) < 50 && !toolbar_collapsed) {
        toolbar_collapsed = true;
        collapse_toolbar();
    } else if ($(window).width() / parseFloat($("body").css("font-size")) > 50 && toolbar_collapsed){
        toolbar_collapsed = false;
        collapse_toolbar();
    }
});
</script>

  


