      $(function() {
          var ret = $("marquee").marquee();
          if($.cookie("offset")) {
            var offset = $.cookie("offset");
            $("#newsTicker div:nth-child(1)").find("div").scrollLeft(offset);
            $(".news-parent").css("visibility","visible");
          } else { $(".news-parent").css("visibility","visible"); }
          $("#newsTicker").hover(function() {
            $(ret).trigger("pause");
          },function() { $(ret).trigger("unpause"); });
          $(window).unload(function() {
            var offset = $("#newsTicker div:nth-child(1)").find("div").scrollLeft();
            $.cookie("offset",offset);
          });
      });
