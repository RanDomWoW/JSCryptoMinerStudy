        <script>
        $( document ).ready(function() {
            $(document).on("click","[id^=mobile-click-]",function(){
                $("[id^=proj-]").removeClass("show")
            });
            var next, prev;
            next = function(el,seats) {
                if (el.next().length > 0) {
                    return el.next();
                } else {
                    return seats.first();
                }
            };

            prev = function(el,seats) {
                if (el.prev().length > 0) {
                    return el.prev();
                } else {
                    return seats.last();
                }
            };
            $('.carousel-toggle').on('click', function(e) {
                var id = $(e.currentTarget).data('id');
                var carousel = $('#'+id+' .carousel');
                var seats = $('#'+id+' .carousel-seat');
                var el, i, j, new_seat, ref;
                el = $('#'+id+' .is-ref').removeClass('is-ref');
                if ($(e.currentTarget).data('toggle') === 'next') {
                    new_seat = next(el,seats);
                    carousel.removeClass('is-reversing');
                } else {
                    new_seat = prev(el,seats);
                    carousel.addClass('is-reversing');
                }
                new_seat.addClass('is-ref').css('order', 1);
                for (i = j = 2, ref = seats.length; 2 <= ref ? j <= ref : j >= ref; i = 2 <= ref ? ++j : --j) {
                    new_seat = next(new_seat,seats).css('order', i);
                }
                carousel.removeClass('is-set');
                return setTimeout((function() {
                    return carousel.addClass('is-set');
                }), 50);
            });
            /* END Flex-Box CAROUSEL*/
        });
    </script>
