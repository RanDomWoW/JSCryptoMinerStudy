<script>

  $( document ).ready(function() {
    $(".navbar-dropdown").mouseover(function(){
      var dropdown = $(this);
      dropdown.addClass("active");

      $(".navbar-dropdown-menu").each(function(){
        var dropdownMenu = $(this);
        if(dropdown.data("dropdown") === dropdownMenu.data("menu")) {
          dropdownMenu.show();
        } else {
          dropdownMenu.hide();
        }
      });

      $(".navbar-dropdown").each(function(){
        if($(this).data("dropdown") !== dropdown.data("dropdown")) {
          $(this).removeClass("active");
        }
      });
    });

    $("li.navbar-dropdown :not([data-dropdown])").mouseleave(function() {
      $(".navbar-dropdown").each(function(){
        var dropdown = $(this);
        dropdown.removeClass("active");
      });
    });

    $(".navbar-dropdown-menu").mouseleave(function(e){
      $(".site").css("position", "static");
      var dropdownMenu = $(this);

      dropdownMenu.hide();
      $(".gsc-control-cse").css("display", "block");
      $(".navbar-dropdown").each(function(){
        var dropdown = $(this);
        dropdown.removeClass("active");
      });
    });

    $(document).mouseout(function(e){
      if(!e.relatedTarget){
        $(".navbar-dropdown-menu").hide();
      }
    });

    // social icon links
    $(".social-icon").click(function(){
      var icon = $(this);
      var link = icon.data("link");
      window.open(link, "_blank");
    });

    // social icon links
    $(".navbar-btn-contact-us").click(function(){
      var btn = $(this);
      var link = btn.data("link");
      window.location.href = link;
    });

    $(".search-dropdown").on("click", function() {
      if(!$(".search").hasClass("flex-it")) {
        $(".social-icon-li").removeClass("opened");
        $(this).parent().addClass("opened");
        hideFast($(".social-share"));
        showIt($(".search"));
      }
      else {
        $(this).parent().removeClass("opened");
        hideIt($(".search"));
      }
    });

    $(".social-dropdown").on("click", function() {
      if(!$(".social-share").hasClass("flex-it")) {
        $(".social-icon-li").removeClass("opened");
        $(this).parent().addClass("opened");
        hideFast($(".search"));
        showIt($(".social-share"));
      }
      else {
        $(this).parent().removeClass("opened");
        hideIt($(".social-share"));
      }
    });

    function hideIt(div){
      div.css("opacity", "0");
      div.one($.support.transition.end,function(){
        div.removeClass("flex-it");
      });
    }

    function hideFast(div){
      div.removeClass("flex-it")
      div.css("opacity", "0");
    }

    function showIt(div){
      div.addClass("flex-it");
      setTimeout(function() {
        div.css("opacity", "1");
      }, 0);
    }

    var urlLocation = window.location.href,
    location = urlLocation.split("/")[3],
    careers = (urlLocation.split("/")[4] === "careers");

    if(location && !careers){
      $(".link-" + location).addClass("current");
    }else if(careers){
      $(".link-careers").addClass("current");
    }

  });


</script>

