//发送随机密码	
var flagPupup = true;
var sysSecondPupup = 20;
var interValObjPupup = null;

function getDynamicPWDCampus() {
	var phoneNum = $("#cellphone").val();
	if(phoneNum.length != 11) {
		$("#logonMessagePPupup").show();
		$("#loginMessagePupup").html("请输入手机号");
		return;
	}
	if(flagPupup) {
		$("#loginMessagePupup").html("请稍等一会...");
		flagPupup = false;
	} else {
		$("#loginMessagePupup").html("请稍等一会再操作");
		return;
	}
	interValObjPupup = window.setInterval(setRemainTimePupup, 1000);
	$.ajax({
        type: "POST",//用POST方式传输
        dataType:"json",//数据格式:JSON 
        url:webRootPath+"/randPwd_getPhoneDynamicPWD.action",//目标地址 
        data:"accountID="+$("#cellphone").val(),
        async:false,//作用是防止在ajax成功调用之前就调用这个时候数据个数还没有初始化
        success:function(json) {
			if(json.TSR_RESULT == 0) {
				$("#getDynamicHrefPupup").html("");
				$("#logonMessagePPupup").show();
				$("#loginMessagePupup").removeClass("err");
				$("#loginMessagePupup").addClass("loginmsg");
