function searchInfo(){
	if(!checkWord()){
		alertMsg("只允许输入中文字符、字母、数字、-、(、)和空格组成的字符串!");
		return false;
	}else{
		$("#searchForm").submit();
	}
}

function checkWord(){
	var str=$("#searchKeyWordId").val();
	str = str.replace(/\s+/g,"");//去除所有空格
	var reg=/^[0-9A-Za-z\-()（）\u4E00-\u9FA5]+$/; //只允许输入中文字符、字母、数字、-、(、)和空格组成的字符串
	return reg.test(str);
}

function getNowTimeInfo(){
		var date = new Date();
		var hours = date.getHours();
		var timeName = '您好';
		if (0<hours && hours<6){
			timeName= "晚上好";
		} else if (6<hours && hours<12){
			timeName= "上午好";
		} else if (12<hours && hours<18){
			timeName= "下午好";
		} else {
			timeName= "晚上好";
		}
		if(getSessionInfo()!='null'&&getSessionInfo()!=''&&getSessionInfo()!=null){
		    var sessionInfo = getSessionInfo();
		    var userName = getSessionInfo().userName;
			timeName=userName+" "+timeName;
			
		}
		$("#timeName").html(timeName);
	}
	
