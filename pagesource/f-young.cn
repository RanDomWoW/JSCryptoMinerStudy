function setRemainTimePupup() {  
  		if (sysSecondPupup > 0) {  
    	sysSecondPupup = sysSecondPupup - 1;  
    	var second = Math.floor(sysSecondPupup % 20);             // 计算秒      
   		$("#showTimeMsgPupup").html(second);  
	} else {//剩余时间小于或等于0的时候，就停止间隔函数  
  			window.clearInterval(interValObjPupup);  
  			//这里可以添加倒计时时间为0后需要执行的事件  
  			flagPupup = true;
  			sysSecondPupup = 20;
  			$("#getDynamicHrefPupup").html("再次获取");
  			$("#showTimeMsgPupup").html("");  
 		}  
}

