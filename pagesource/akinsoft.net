<script type="text/javascript">
	$(document).ready(function () {
		$("input.focus").focus(function () {
			if (this.value == this.defaultValue) {
				this.value = "";
			} else {
				this.select();
			}
		});
		$("input.focus").blur(function () {
			if ($.trim(this.value) == "") {
				this.value = (this.defaultValue ? this.defaultValue : "");
			}
		});
	});
	function control(){
		if ( $('#'+bayi_kodu).val()	==	""	){
			alert("Bayi Kodunu giriniz...");
			$('#'+bayi_kodu).focus();
			return false;
		}
		if ( $('#'+bayi_sifresi).val() == "" ){
			alert("Þifreyi giriniz...");
			$('#'+bayi_sifresi).focus();
			return false;
		}
		kontrol	=	1;
		return true;
	}
</script>
