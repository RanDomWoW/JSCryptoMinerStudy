<script type="text/javascript">
  var menu_time_show = undefined;
  var menu_time_hide = undefined;
  var first_call = true;
  /////////////////////////////////////////////
  // MENU
  $("#main_menu ul li").mouseenter(function(eventData) {
    clearTimeout(menu_time_hide);
    index = $(eventData.currentTarget).index("#main_menu ul li");
    if (first_call==true){
      preload_menu(index);
      menu_time_show = setTimeout(function(){ show_menu(index); }, 400);
      first_call = false;
    }else{
      if (menu_time_show==undefined){
        menu_time_show = setTimeout(function(){ show_menu(index); }, 200);
      }
    }
    $("#main_menu ul li").removeClass("active");
    $(this).addClass("active");

  });
  $("#preview_menu").mouseenter(function() {
    clearTimeout(menu_time_hide);
  }).mouseleave(function() {
      if (menu_time_show!=undefined){
        clearTimeout(menu_time_show);
      }
      menu_time_hide = setTimeout(function(){ $("#preview_menu").hide(); menu_time_hide=undefined; first_call = true;$("#main_menu ul li").removeClass("active");$("#ombre_megamenu").removeClass("hover");}, 200);
    });
  $("#main_menu").mouseleave(function(eventData) {
    if (menu_time_show!=undefined){
      clearTimeout(menu_time_show);
    }
    menu_time_hide = setTimeout(function(){ $("#preview_menu").hide(); menu_time_hide=undefined; first_call = true;$("#main_menu ul li").removeClass("active"); $("#ombre_megamenu").removeClass("hover");}, 200);
  });
  function show_menu(index){
    $("#ombre_megamenu").addClass("hover");
    $("#preview_menu").attr("class","main_menu_pane bg_color_"+index);
    $("#preview_menu").css("left",$("#main_menu").offset().left);
    $("#preview_menu").html($("#pop_content_"+index).html()).show();

    if ($("#pop_content_"+index).hasClass("new")){
      $("#pop_content_"+index).addClass("load");
      $("#pop_content_"+index).removeClass("new");

      categ = $("#pop_content_"+index).attr('categ');

      $.ajax({
        url: 'ajax_category.php?categ='+categ,
        success: function(data) {
          $("#pop_content_"+index).html(data);
          $("#preview_menu").html($("#pop_content_"+index).html()).show();
        }
      });
    }
    menu_time_show=undefined;
  };

