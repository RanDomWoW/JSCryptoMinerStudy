                        form.Email.focus();
                        passed = false;
                    }
                    // **** Check for commas ****
                    else if (intComma != -1) {
                        alert("Votre Courriel ne doit pas contenir une virgule ou apostrophe.");
                        form.Email.focus();
                        passed = false;
                    }
                    // **** Check for a space ****
                    else if (intSpace != -1) {
                        alert("Votre courriel est trop long.");
