<script type="text/javascript">
function trackingPlayer(action, data) {
function reload() {
var url = window.location.href;
url = url.split("&startplaying");
window.location.href = url[0];
}
if (typeof data.videoid === "undefined") {
return;
}
var videoId = data.videoid,
$videoPlayer = $('.videoplayer[data-video=' + videoId + ']'),
player,
eTracker,
position;
if ($videoPlayer.length) {
player = $videoPlayer.data('player');
eTracker = $videoPlayer.data('tracker');
position = player.api.currentTime();
if (!eTracker) {
return;
}
position = typeof position !== 'undefined' ? position : 0;
if (action == "pause") {
eTracker.setState("paused", position);
} else if (action == "play") {
eTracker.setState("playing", position);
} else if (action == "stop") {
eTracker.setState("stopped", position);
} else if (action == "rewind") {
eTracker.setState("paused", position);
eTracker.setState("playing", position - 10);
} else if (action == "forward") {
eTracker.setState("paused", position);
eTracker.setState("playing", position + 10);
} else if (action == "related") {
window.setTimeout(reload, 500);
}
}
}
