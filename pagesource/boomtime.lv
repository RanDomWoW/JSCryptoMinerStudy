<script type="text/javascript">
$(document).ready(function(){
	$("#prf_login").focus();
});

function login_focus(){
	$("#prf_login").focus();
}

function password_focus(){
	$("#prf_pass").focus();
}


function authorize(){

	var postvars = {
			 prf_login : $("#prf_login").val(),
			 prf_pass  : $("#prf_pass").val()
			}

	$("#auth_button").html('Autorizācija');
	$("#auth_button_s").attr('disabled', true);

	$.post('/ajax/authorize_ajax.html', postvars, function(input){
			input  = eval("(" + input + ")");
			if (input.data[0].result){
			switch(input.data[0].result){
				case 'pending':
					$("#auth_result").hide();
					document.location.href = '/authorize/confirm.html';
					break;

				case 'enabled':
					$("#auth_result").hide();
											document.location.reload();
										break;

				case 'disabled':
					$("#auth_button").html('Ieiet');
					$("#auth_button_s").removeAttr("disabled");
					$("#auth_result").html('Lietotājs ir nobloķēts<br />iemesls: '+input.data[0].reason);
					$("#auth_result").show();

			}
		}else{
			$("#auth_button").html('Ieiet');
			$("#auth_button_s").removeAttr("disabled");
			$("#auth_result").html('Autorizācijas kļūda');
			$("#auth_result").show();
		}
	});
}
</script>
