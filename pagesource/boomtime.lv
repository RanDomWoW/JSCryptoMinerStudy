<script>

	$(window).load(function(){

		$('.resize62').each( function() {

			var w = $(this).width();
			var h = $(this).height();

			if (w >= h){
				var m = (62 / 100 * (100-(h/w*100))/2);
				$(this).height(62);
				$(this).css({"margin":"0 0 0 -"+m+"px"});
			}else{
				var m = (62 / 100 * (100-(w/h*100))/2);
				$(this).width(62);
				$(this).css({"margin":"-"+m+"px 0 0 0"});
			}
		});

		$('.crop').css({"visibility":"visible"});;
	});

</script>

