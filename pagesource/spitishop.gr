						b = apos !== -1 ? a.substring(0, apos) : a,
						$parent = $(this).parent(),
						c, cpos, d = '', i = 0, item = ''; 						
						if (typeof ga != 'function') return true;
						e.preventDefault(); 
						while (!$parent.hasClass('dropdown') && !$parent.hasClass('yamm-fw') && !$parent.hasClass('open')) {
							$parent = $parent.parent();
							i += 1;
							if ($parent.attr('id') == 'megamenu') break;
						}
						if (i < 2) {
							ga('send','event','main-menu','click', b);
