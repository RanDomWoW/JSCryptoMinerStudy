<script type="text/javascript">

function setCookie(cname, cvalue, exminutes) {
    var d = new Date();
    d.setTime(d.getTime() + (exminutes*60*1000));
    var expires = "expires="+d.toUTCString();
    document.cookie = cname + "=" + cvalue + "; " + expires;
}

function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
    }
    return "";
}

function checkCookiePopup() {
  if (checkVisits()) {
    return false;
  }
    var spitishop_popup = getCookie("spitishop_popup");
    if (spitishop_popup != null && spitishop_popup != "") {
      if(spitishop_popup == "shown") return false;
      var time = new Date();
      spitishop_popup = new Date(spitishop_popup);
      if (spitishop_popup <= time) {
        return true;
      }
      else {
        console.log(spitishop_popup);
        popup = setTimeout(call_popup, (spitishop_popup.getTime() - time.getTime()));
        return false;
      }
    } else {
      var spitishop_popup = new Date();
      spitishop_popup.setSeconds(spitishop_popup.getSeconds() + 10);
      setCookie("spitishop_popup", spitishop_popup, 30);
      popup = setTimeout(call_popup, 10000);
    }
}

//returns false if visits < 3
function checkVisits() {
  var spitishop_visits = getCookie("spitishop_visits");
  var spitishop_popup_c = getCookie("spitishop_popup");
    if (spitishop_visits != null && spitishop_visits != "") {
        var spitishop_visits = parseInt(getCookie("spitishop_visits"));
      if (spitishop_visits < 3) {
        if(spitishop_popup_c == null || spitishop_popup_c=="") setCookie("spitishop_visits", spitishop_visits+1, 365*24*60);
        return false;
      }
      else
        return true;
    } else {
      setCookie("spitishop_visits", 1, 365*24*60);
    }
}

function call_popup() {
  setCookie("spitishop_popup", "shown", 30);
  var url = window.location.href;
    $.ajax({
        type  : "GET",
        cache : false,
        url   : url,
        data  : $(this).serializeArray(),
        success: function(data) {
          setTimeout( function() {
            $.fancybox(
              '<div class="nm_newsletter_outer"><div class="nm_newsletter_left box-flow"><img src="/modules/homemodalwindow/img/left-image.jpg" alt="Εγγραφείτε στο ενημερωτικό μας δελτίο" /></div><div class="nm_newsletter_right box-flow"><img src="/modules/homemodalwindow/img/text2.png" alt="" /><div id="nm_extra"> </div><form action="/" method="post"><div class="extra_margin"><input id="newsletter-input" class="box-flow inputNew form-control grey newsletter-input" name="email" type="text" placeholder="Συμπληρώστε το e-mail σας" /><input id="nm_template" name="nm_template" type="hidden" value="0" /></div><div class="box-flow nm_full"><span class="nm_text box-flow">Επιλέξτε το φύλο σας<br />για εγγραφή</span> <button class="btn btn_main-right nm_margin_right box-flow transition-roll" name="submitNewsletterFemale" type="submit">ΓΥΝΑΙΚΑ</button><button class="btn btn_main-right box-flow transition-roll" name="submitNewsletterMale" type="submit">ΑΝΔΡΑΣ</button><input name="action" type="hidden" value="0" /></div></form></div></div>',
              { 
                'padding'       : 0,



                'width'         : 810,
                'height'        : 510,
                'autoScale'     : false,
                'scrolling'     : 'no'
            });
			var nm_rand = 2;
			$('#nm_template').val(nm_rand);
			if (nm_rand == 2){
				$('#nm_extra').html('<img src="/modules/homemodalwindow/img/text-extra.png"/>');
			}
          },200);
        }
      });
}

jQuery(document).ready(function() {
  if (checkCookiePopup()) {
    call_popup();
  }
});


</script>

