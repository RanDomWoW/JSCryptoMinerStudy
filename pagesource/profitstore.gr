  else
  {
    document.getElementById('nt_slideup_modules').style.height = 'auto';
    document.getElementById('nt_slideup_modules').style.overflow = 'visible';
    document.getElementById('nt_slideup_btn_min').style.display = 'inline';
    document.getElementById('nt_slideup_btn_max').style.display = 'none';
    document.cookie= "nt_slideup_status=maximized";
  }
}

