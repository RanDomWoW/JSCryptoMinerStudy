function nt_getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
    }
    return "";
}

function hide_slideup()
{
  document.getElementById('nt_slideup').style.display = 'none';
  document.cookie= "nt_slideup_display=hidden";
}

function toggle_slideup()
{
  if (document.getElementById('nt_slideup_modules').clientHeight > 0)
  {
    document.getElementById('nt_slideup_modules').style.height = 0;
    document.getElementById('nt_slideup_modules').style.overflow = 'hidden';
    document.getElementById('nt_slideup_btn_min').style.display = 'none';
    document.getElementById('nt_slideup_btn_max').style.display = 'inline';
    document.cookie= "nt_slideup_status=minimized";
