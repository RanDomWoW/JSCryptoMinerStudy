  function setBBCookie(cookie_key,cookie_value,cookie_path,max_age){
    document.cookie = cookie_key +'='+cookie_value+';path='+cookie_path+';max-age='+max_age;
  }
  
  function getBBCookie(key){
    var cookie,cookies = document.cookie.split(";");
    for(var i = 0;i<cookies.length;i++){
      cookie = cookies[i].split("=");
      
      if(cookie[0].trim() == key){
        return decodeURIComponent(cookie[0]);
      }
    }
    
    return false;
  }
