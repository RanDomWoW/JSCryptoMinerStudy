	$('#showcase').fadeIn(1500);
	$("#showcase").awShowcase({ //sp枠
		content_width:	1024,
		content_height:	349,
		fit_to_parent:	false,
		auto:		true,
		interval:	10000,
		continuous:	true,
		loading:	true,
		tooltip_width:	200,
		tooltip_icon_width:	32,
		tooltip_icon_height:	32,
		tooltip_offsetx:	18,
		tooltip_offsety:	0,
		arrows:			true,
		buttons:		true,
		btn_numbers:		false,
		keybord_keys:		true,
		mousetrace:		false,
		pauseonover:		true,
		stoponclick:		false,
		transition:		'hslide',
		transition_speed:	500,
		transition_delay:	300,
		show_caption:		'onhover',
		thumbnails:		true,
		thumbnails_position:	'outside-last',
		thumbnails_direction:	'horizontal',
		thumbnails_slidex:	1,
		dynamic_height:		true,
		speed_change:		false,
		viewline:		false
	});

	// TOPスライダーへマウスが乗った時
	$("#showcase").live('mouseenter', function() {
		$(".showcase-arrow-previous").show();
		$(".showcase-arrow-next").show();
	});
	// TOPスライダーからマウスが外れた時
	$("#showcase").live('mouseleave', function() {
		$(".showcase-arrow-previous").hide();
		$(".showcase-arrow-next").hide();
	});
	// ページ読み込み時は隠す
	$(".showcase-arrow-previous").hide();
	$(".showcase-arrow-next").hide();

	$('#slide-fc').show();
	$("#slide-fc").slider({// FC
	time: 50,
	speed: 3
	});

	$('#tab-container').easytabs({ //タブ
updateHash: false,
});

	$('#tab-container2').show();
	$('#tab-container2').easytabs({ //タブ
updateHash: false,
});

	$('.slider').show();
	var sudoSlider = $(".slider").sudoSlider({//4boxのキャプション
	effect: "fade",
	auto:true,
	prevNext:false
	});

	$('.boxgrid.caption').hover(function(){//ホバーによって追加される高さ//4boxのキャプション
	$(".cover", this).stop().animate({top:'54px'},{queue:false,duration:160});
	}, function() {//最初に見ている位置指定
	$(".cover", this).stop().animate({top:'165px'},{queue:false,duration:160});
	});

	$(".iframe").colorbox({
		iframe:true, width:"800px", height:"630px",
		onComplete: function() {
			is_movie_modal = true;
			player = document.getElementById("showcase_movie");
			if (player) { 				player.stopVideo();
			}
		},
		onClosed: function() {
			is_movie_modal = false;
		}
	});//モーダル
});

function onYouTubePlayerReady(playerId) {
	if (is_movie_modal == true) {
		ytplayer = document.getElementById("showcase_movie");
		ytplayer.stopVideo();
	}
}
</script>


