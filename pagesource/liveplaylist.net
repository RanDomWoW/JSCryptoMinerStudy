                            if (targetElement.attributes) 
                                for (var i in targetElement.attributes) 
                                    errorMessage += targetElement.attributes[i].nodeName+':'+targetElement.attributes[i].nodeValue+' ,';
                            */
                            errorMessage += targetElement.outerHTML;
                        }else{
                            var string = mixed;
                            
                            if (
                                (string!='Script error.')   &&  // scripte von fremden domains ausschliessen
                                (string.indexOf('atomicFindClose')<0)     // mysterious mobile Safari error
                                ){  
                                    errorMessage  = string;
                                    errorMessage += (url)?  ' at URL: ' +url  : '';
                                    errorMessage += (line)? ' on line: '+line : '';
                            }else   errorMessage = 'blacklist Error';
                        }
                        
                    }catch(e) { errorMessage ='Error in onError function' }
                    
                    if( (!data.appSettings['isTestApp'])&&(errorMessage!='blacklist Error')){ 
                        var myAjax = new Ajax();
