            function createLoader(params){

                var spinner              = document.createElement('div');
                function onError(){
                    spinner.innerHTML = '<img alt="loader" src="'+params['alternateImgSrc']+'" style="width:'+params['width']+'; height:'+params['height']+'" />';
                }
                try{
                    var keyframeprefix = Optional.features.animation;
                    if (keyframeprefix!==null){
                       
                        spinner.style.cssText   = "width: "+params['width']+";height: "+params['height']+";text-align: center;font-size: 10px;";
                        
                        var cssAnimation = document.createElement('style');
                        cssAnimation.type = 'text/css';
                        var text = '@'+keyframeprefix+'keyframes stretchdelay {0%, 40%, 100% { '+keyframeprefix+'transform: scaleY(0.2);}  20% { '+keyframeprefix+'transform: scaleY(1.0);}}';
                        if (cssAnimation.styleSheet)
                             cssAnimation.styleSheet.cssText = text;
                        else cssAnimation.appendChild(document.createTextNode(text));
                        spinner.appendChild(cssAnimation);
                        
                        var rect_array         = new Array();
                        for (var i=0;i<5;i++) {
                           var rect              = document.createElement('div');
                           var delay             = (-1.1+(i/10)).toFixed(1);
                           rect.style.cssText    = "margin: 0 10% 0 0;background-color: "+params['color']+";height: 100%;width: 10%;display: inline-block;"+keyframeprefix+"animation: stretchdelay 1.2s infinite ease-in-out;"+keyframeprefix+"animation-delay: "+delay+"s;"
                           spinner.appendChild(rect);
                        }
                      
                        
                     }else onError();
                  }catch(e){onError()}
                  return spinner;
            }
                        
            
