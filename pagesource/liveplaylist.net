            function Ajax(){
                this.postParam  = null;
                var request_obj = null;
                
                try{ 
                    request_obj = new XMLHttpRequest();
                }catch (e){
                    try{
                        request_obj = new ActiveXObject("Msxml2.XMLHTTP");
                    }catch (e){
                        try{
                            request_obj = new ActiveXObject("Microsoft.XMLHTTP");
                        }catch (error){
                            trace('AjaxError: '+error)
                        }
                    }  
                }
                
                this.open = function(method_str,URL_str,callBack_func){     
                      request_obj.open(method_str,  encodeURI(URL_str), true);//request ist asynchron   
                      request_obj.onreadystatechange = function(){            
                         switch(request_obj.readyState) {
                            case 4:
                                if(request_obj.status!=200){
                                    trace('Ajax Error status: '+ request_obj.status);
                                }else{
                                    var request_json = null;
                                    try{
                                        request_json = eval('(' + request_obj.responseText + ')');
                                    }catch(e){ 
                                        //ErrorLog('Ajax responseText not json: '+e);
                                        trace('Ajax responseText not json: '+e);
                                        //trace(request_obj.responseText);
                                        request_json = new Object();
                                    }
                                    callBack_func(request_json);
                                }
                            break;
                    
                            default:
                                return false;
                            break;     
                         }
                      };// end onreadystatechange
            
                      request_obj.setRequestHeader("Content-Type","application/x-www-form-urlencoded");//This is for POST only.
                      request_obj.setRequestHeader("Content-Encoding","gzip");
                      if (method_str=='POST')request_obj.send(this.postParam);
                      else request_obj.send(null);
                }
            } // end Ajax   
     
            function onScriptReady(scriptElement, callBackFunction){
            
                function onLoad(e){
                    //trace('onScriptReady');
                    callBackFunction(e);
                    //trace('loaded: '+scriptElement.id);
                }
                
                scriptElement.onreadystatechange = function(e){
                    //trace('scriptElement.onreadystatechange');
                    scriptElement.onload = null;
                    if (!this.readyState || this.readyState === "loaded" || this.readyState === "complete"){
                        scriptElement.onreadystatechange    = null;
                        
                        var oldErrorFunc = scriptElement.onerror;
                        scriptElement.onerror = function(e){
                            //ie<9 fix   
                            if (!e) e = window.event;
                            if (!e.target) e.target = scriptElement; 
                            trace('error: '+scriptElement.id);
                            scriptElement.hasError = true;
                            if (oldErrorFunc) oldErrorFunc(e);
                        }
                        
                        //ie<9 fix   
                        if (!e) e = window.event;
                        if (!e.target) e.target = scriptElement; 
                        
                        setTimeout(function(e){
                            if (!scriptElement.hasError) onLoad(e);
                        },100);
                    } 
                }
                
                scriptElement.onload = function(e){
                    //trace('scriptElement.onload');
                    scriptElement.onreadystatechange = null;
                    onLoad();
                }
                
            }
            
            
            function loadExternalScript(params){
                /*
                params.url         
                params.parentElem  
                params.id           
                params.onLoad     
                params.onError
                */   
                var scriptExist = false;
                if (params.id) {
                    
                    if (document.getElementById(params.id)){
                        trace('script schon vorhanden');
                        scriptExist = true;
                        if (typeof params.onLoad =='function'){
                            setTimeout(function(){
                                params.onLoad(); 
                            },100);
                        }
                    }
                }
                
                if (!scriptExist){
                    //trace('lade script');
                    var script      = document.createElement('script');
                   
                    onScriptReady(script, function(e){ //ie wirft bei einem ladefehler "onload". bei eimem fehler wird jedoch nicht die callback funktion JSONP.callBackFunc ausgefuhrt und JSONP.result ist null;
                        //trace('onloadsc');
                        if (typeof params.onLoad =='function') params.onLoad();
                    });   
                    
                    
                    script.onerror = function(e){
                            if (params.id){
                                var errorScript = document.getElementById(params.id);
                                if (errorScript) {
                                    errorScript.parentNode.removeChild(errorScript);
                                }
                            }
                            
                            if (typeof params.onError=='function') params.onError(e);
                            else errorLog(e,null,null);
                         }
                    
                    script.setAttribute('type', 'text/javascript');
                    script.setAttribute('src', params.url);
                    script.setAttribute('async', true);
                    if (params.id) script.setAttribute('id', params.id);
                    
                    if (!params.parentElem ) params.parentElem  = document.getElementsByTagName('head')[0];
                    params.parentElem.appendChild(script);
                }
                
            }

            function errorLog(mixed, url, line){
                if (errorLog.src){
                    try {
                        var errorMessage = '';
                        if (typeof mixed == 'object'){
                            var errorEvent = mixed;
                            trace(errorEvent);   
                            // folgender funktionsteil ist noch nicht optimal!
                            var targetElement   = (errorEvent.target)?errorEvent.target:errorEvent;
