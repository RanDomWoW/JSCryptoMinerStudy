        <script type="text/javascript">
            //<![CDATA[

            $(document).ready(function () {

                var item_lista_1 = $("#lista_videos li:first-child");

                item_lista_1.addClass('reproductor_video_seleccionado');

                var src_video = item_lista_1.find('a').attr('href');
                var src_poster = item_lista_1.find('img').attr('src');

                var video = $('#reproductor_video_lista');

                video.attr('poster', src_poster);
                video.find('source').attr('src', src_video);

                video[0].load();

                $(".video_portada_link").click(function () {

                    $(".video_lista").each(function () {
                        $(this).removeClass('reproductor_video_seleccionado');
                    });

                    var id_vid = $(this).attr("id");
                    var ruta_vid = $(this).attr("href");
                    var ruta_img = $(this).find('img').attr("src");
                    var li_video = $("#vl_" + id_vid);

                    li_video.addClass('reproductor_video_seleccionado');

                    var video = $('#reproductor_video_lista');

                    video.attr('poster', ruta_img);
                    video.find('source').attr('src', ruta_vid);

                    video[0].load();
                    video[0].play();

                    return false;

                });
            });
            
            $(window).load(function () {
                var viewport = $(window).width();
                igualarNotasAA(viewport);
            });

            function igualarNotasAA(viewport) {

                var elementosBloque = $('.bloque > ul > li');
                /* Inicializo altura mínima*/
                elementosBloque.each(function (indice, elemento) {
                    elemento.style.minHeight = '1px';
                });
                elementosBloque.each(function (indice, elemento) {
                    if (indice % 2 !== 0) {
                        var maxH = Math.max(elemento.offsetHeight, elementosBloque[indice - 1].offsetHeight) + 10;
                        elemento.style.minHeight = maxH + 'px';
                        elementosBloque[indice - 1].style.minHeight = maxH + 'px';
                    }
                });
            }

            function doSomething() {
                var viewport = $(window).width();
                igualarNotasAA(viewport);
            }

            var resizeTimer;
            $(window).resize(function () {
                clearTimeout(resizeTimer);
                resizeTimer = setTimeout(doSomething, 100);
            });

            //]]>
        </script>

