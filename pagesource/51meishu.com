<script type="text/javascript">
function guanbi(){
	document.getElementById("gb").style.display="none";
	document.getElementById("dk").style.display="block";
	
}

function dakai(){
	document.getElementById("gb").style.display="block";
	document.getElementById("dk").style.display="none";
	
	}
	
var picBox = document.getElementById('picBox'),
    on = document.getElementById('ls'),
    imgs = picBox.getElementsByTagName('img');
picBox.onmouseenter = function(e){
    on.style.display = 'none';
    var random = getRandom(1,imgs.length-1);
    for(var i=1; i<imgs.length; i++){
        imgs[i].style.display =  i === random ? 'block' : 'none';
    }
    e.preventDefault();
};
picBox.onmouseleave = function(){
    on.style.display = 'block';
    for(var i=1; i<imgs.length; i++){
        imgs[i].style.display = 'none';
    }
};
function getRandom(n,m){
    n = Number(n);
    m = Number(m);
    if(isNaN(n) || isNaN(m)){
        return Math.random();
    }
    if(n > m){
        var temp = n;
        n = m;
        m = temp;
    }
    return Math.round(Math.random()*(m-n)+n);
}

</script>

