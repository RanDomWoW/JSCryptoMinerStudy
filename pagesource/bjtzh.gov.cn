             $("#task").val('');
        }).blur(function(){
            var task = $("#task").val();
            if(!task){
             $("#task").val('事项名称');
            }
        });
  $("#person").focus(function(){
   var person = $("#person").val();
   if(person && person == '申请人/单位')
             $("#person").val('');
        }).blur(function(){
            var person = $("#person").val();
            if(!person){
             $("#person").val('申请人/单位');
            }
        });
  $("#code").focus(function(){
   var code = $("#code").val();
   if(code && code == '受理编号')
             $("#code").val('');
        }).blur(function(){
            var code = $("#code").val();
            if(!code){
             $("#code").val('受理编号');
            }
        });
 }
</script>





