		j(document).ready(function(){
		  j('.bxslider2').bxSlider({
			  auto:true,
			  pause: 3000,
			  touchEnabled:true,
			  preventDefaultSwipeY:false,
		  });
		});
	</script>
	 
			<script>
		 var j = jQuery.noConflict();
			var fix = j(".header_absolute");             //メニューオブジェクトを格納
			if(fix.length){								 //要素があった場合
			  j('main').addClass('is-alert-on');
			  var fixTop = fix.offset().top;
			  //console.log(fixTop)
			  j(window).scroll(function () {             //スクロールが発生したら開始
				if(j(window).scrollTop() >= fixTop) {    //スクロール時の縦座標がメニューの縦座標以上なら…
				  fix.css("position","fixed");           //メニューに position:fixed 付与
				  fix.css("top","0");
				  j(".burger_absolute").css("position","fixed");
				} else {
				  fix.css("position","");                //メニューの position を空に
				  fix.css("top","");    
				j(".burger_absolute").css("position","");//メニューの top を空に
				}
			  });

			}
		</script>
	</body>
</html>