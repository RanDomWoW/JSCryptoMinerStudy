    <script type="text/javascript">
        var owl;

        $(document).ready(function () {
            owl = $("#homeslider");

            var homeslider_navigation = false;
            var homeslider_itemsScaleUp = false;
            var homeslider_autoPlay = false;
            var homeslider_stopOnHover = false;
            var homeslider_paginationNumbers = false;
            var homeslider_responsive = false;
            var homeslider_lazyLoad = false;
            var homeslider_autoHeight = false;
            var homeslider_addClassActive = false;


            var homeslider_items = parseInt('1');
            var homeslider_slideSpeed = parseInt('200');
            var homeslider_paginationSpeed = parseInt('800');

            homeslider_autoPlay = Boolean(true);
            if (homeslider_autoPlay == true) {
                homeslider_autoPlay = parseInt('5000');
            }

            homeslider_itemsScaleUp = Boolean(false);
            homeslider_stopOnHover = Boolean(true);
            homeslider_navigation = Boolean(true);
            homeslider_paginationNumbers = Boolean(false);
            homeslider_responsive = Boolean(true);
            homeslider_lazyLoad = Boolean(false);
            homeslider_autoHeight = Boolean(false);
            homeslider_addClassActive = Boolean(false);

            owl.owlCarousel({
                pagination:false,
                items: homeslider_items,
                itemsScaleUp: homeslider_itemsScaleUp,
                slideSpeed: homeslider_slideSpeed,
                paginationSpeed: homeslider_paginationSpeed,
                autoPlay: homeslider_autoPlay,
                stopOnHover: homeslider_stopOnHover,
                navigation: homeslider_navigation,
                navigationText: ["<i class='fa fa-chevron-left'></i>", "<i class='fa fa-chevron-right'></i>"],
                paginationNumbers: homeslider_paginationNumbers,
                responsive: false,
                baseClass: 'owl-carousel',
                lazyLoad: homeslider_lazyLoad,
                autoHeight: homeslider_autoHeight,
                addClassActive: homeslider_addClassActive,
                afterInit: homeslider_afterOWLinit
            });

            function homeslider_afterOWLinit() {
                if (homeslider_items != 1) {
                    return false;
                }
                $('#homeslider .owl-controls .owl-page').append('<a class="item-link" href="javascript:;"/>');

                $.each($('#homeslider .owl-controls .item-link'), function (i) {
                    $(this).css({
                        'background': 'url(' + $('#homeslider .item:eq(' + i + ')').find('img').attr('src') + ') center center no-repeat',
                        '-webkit-background-size': 'cover',
                        '-moz-background-size': 'cover',
                        '-o-background-size': 'cover',
                        'background-size': 'cover',
                    })
                    .click(function () {
                        owl.trigger('owl.goTo', i);
                    });
                })

                $('#homeslider .owl-pagination').prepend('<a href="#prev" class="prev-owl"/>');
                $('#homeslider .owl-pagination').append('<a href="#next" class="next-owl"/>');

                $(".next-owl").click(function () {
                    owl.trigger('owl.next');
                });

                $(".prev-owl").click(function () {
                    owl.trigger('owl.prev');
                });
            }
        });
    </script>

