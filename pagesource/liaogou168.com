    <script type="text/javascript">
        window.onload = function () {
            setCurr(0);
            setFocus();
        }
        function setFocus () {
            var canScroll = true;
            var Scrolling;
            var IMG = $('#Focus .show a');

            while (IMG.length < 5) {
                // console.log(1)
                $('#Focus .show')[0].innerHTML += $('#Focus .show')[0].innerHTML;
                IMG = $('#Focus .show a');
            }

            IMG[0].className = 'on';
            IMG[1].className = 'after';
            IMG[IMG.length - 1].className = 'before';
            IMG[IMG.length - 2].className = 'bb';

            $('#Focus .bg')[0].innerHTML = $('#Focus .show')[0].innerHTML;

            function ScrollLeft(){
                if (canScroll) {
                    canScroll = false;

                    setTimeout(function(){
                        $('#Focus a.bb').attr('class','');
                    },1000)
                    $('#Focus a.before').attr('class','bb');
                    $('#Focus a.on').attr('class','before');
                    $('#Focus a.after').attr('class','on');
                    if ($('#Focus a.on').next()[0]) {
                        $('#Focus a.on').next().attr('class','after');
                    }else{
                        $('#Focus .show a:first').attr('class','after');
                        $('#Focus .bg a:first').attr('class','after');
                    }

                    var H3 = $('#Focus h3.on')
                    H3.attr('class','');
                    if (H3.next()[0]) {
                        H3.next().attr('class','on');
                    }else{
                        $('#Focus h3:first').attr('class','on');
                    }

                    if (!Scrolling) {
                        Scrolling = setInterval(function(){
                            ScrollRight()
                        },5000);
                    }

                    setTimeout(function(){
                        canScroll = true;
                    },1100)

                }else{
                    return;
                }
            }

            function ScrollRight(){
                if (canScroll) {
                    canScroll = false;

                    if ($('#Focus a.bb').length == 0) {
                        if ($('#Focus a.before').prev()[0]) {
                            $('#Focus a.before').prev().attr('class','bb');
                        }else{
                            $('#Focus .show a:last').attr('class','bb');
                            $('#Focus .bg a:last').attr('class','bb');
                        }
                    }


                    setTimeout(function(){
                        $('#Focus a.aa').attr('class','');
                    },1000);
                    $('#Focus a.after').attr('class','aa');
                    $('#Focus a.on').attr('class','after');
                    $('#Focus a.before').attr('class','on');
                    $('#Focus a.bb').attr('class','before');
                    if ($('#Focus a.before').prev()[0]) {
                        $('#Focus a.before').prev().attr('class','bb');
                    }else{
                        $('#Focus .show a:last').attr('class','bb');
                        $('#Focus .bg a:last').attr('class','bb');
                    }

                    var H3 = $('#Focus h3.on')
                    H3.attr('class','');
                    if (H3.prev()[0]) {
                        H3.prev().attr('class','on');
                    }else{
                        $('#Focus h3:last').attr('class','on');
                    }

                    if (!Scrolling) {
                        Scrolling = setInterval(function(){
                            ScrollRight()
                        },5000);
                    }

                    setTimeout(function(){
                        canScroll = true;
                    },1100)
                }else{
                    return;
                }
            }

            document.getElementById('Focus').getElementsByTagName('button')[0].onclick = function () {
                if (canScroll) {
                    clearInterval(Scrolling)
                    Scrolling = false;
                    ScrollRight();
                }
            }

            document.getElementById('Focus').getElementsByTagName('button')[1].onclick = function () {
                if (canScroll) {
                    clearInterval(Scrolling)
                    Scrolling = false;
                    ScrollLeft();
                }
            }

//            Scrolling = setInterval(function(){
//                ScrollLeft()
//            },5000);
        }

