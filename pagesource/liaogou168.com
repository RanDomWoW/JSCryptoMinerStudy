            Rank.getElementsByTagName('table')[1].style.display = 'none';

            document.getElementById('Rank_tal').className = '';
            document.getElementById('Rank_pac').className = '';

            $('#Rank .' + obj.id)[0].style.display = 'table';
            obj.className = 'on';
        }
        function ChangeLeague (obj) {
            var League = document.getElementById('League');
            var Btn = $('#League button[name=league]');

            for (var i = 0; i < League.getElementsByTagName('tbody').length; i++) {
                League.getElementsByTagName('tbody')[i].style.display = 'none';
            }

            for (var i = 0; i < Btn.length; i++) {
                Btn[i].className = '';
            }

            $('#League .' + obj.id)[0].style.display = 'table-row-group';
            obj.className = 'on';
        }

        $('button[name=football_recommend]').on('click', function() {
            $('button[name=football_recommend].on')[0].className = '';
            this.className = 'on';

            var orderBy = this.value;
            $.ajax({
                "url": "/index/recommend?orderBy=" + orderBy,
                "dataType": "html",
                "success": function (html) {
                    if ($.trim(html) != "") {
                        $('#rem_football').html(html);
                    }

                },
                "error": function () {

                }
            });
        });
        $('button[name=basketball_recommend]').on('click', function() {
            $('button[name=basketball_recommend].on')[0].className = '';
            this.className = 'on';

            var orderBy = this.value;
            $.ajax({
                "url": "/index/recommend?orderBy=" + orderBy + "&sport=2",
                "dataType": "html",
                "success": function (html) {
                    if ($.trim(html) != "") {
                        $('#rem_basketball').html(html);
                    }
                },
                "error": function () {

                }
            });
        });

        var tschedule = window.setInterval('changeLeagueEveryMin()',10000);
        var changeLeagueIndex = 0;
        function changeLeagueEveryMin() {
            changeLeagueIndex++;
            changeLeagueIndex = changeLeagueIndex%6;
            ChangeLeague($('button[name=league]')[changeLeagueIndex]);
            $('button[name=league]')[changeLeagueIndex].className = 'on';
        }
    </script>
    <script type="text/javascript">
        function refresh() {
                            $.ajax({
                "url": "/change/live.json?time=" + (new Date().getTime()),
                "dataType": "json",
                "success": function (json) {
                    for (var ID in json) {
                        var dataItem = json[ID];
                        var timeItem = $('#time_' + ID);
                        var scoreItem = $('#score_' + ID);
                        var liveItem = $('#live_' + ID);
                        if (timeItem) {
                            var timeStr = dataItem.time;
                            timeStr = timeStr.replace("<span class=\"minute\">'", "<em>'</em>");
                            timeItem.html(timeStr);
                        }
                        if (scoreItem && liveItem.length > 0) {
//                            var currentScore = dataItem.score;
//                            scoreItem.html(currentScore);
                            scoreItem[0].style.color = "#bc1c25";
                            scoreItem.html('正在直播');
                        }
                        if (liveItem && liveItem.length > 0){
                            if(liveItem[0].src == "//static.liaogou168.com/img/icon_home_live.png") {
                                liveItem[0].src = "//static.liaogou168.com/img/icon_home_video_live.gif";
                            }
                            if (timeItem == '已结束'){
                                liveItem[0].src = "//static.liaogou168.com/img/icon_home_live.png";
                                if (scoreItem && liveItem.length > 0) {
                                    scoreItem[0].style.color = "#333";
                                    scoreItem.html('已结束');
                                }
                            }
                        }
                    }
                },
                "error": function () {

                }
            });
                    }
        window.setInterval('refresh()',5000);
    </script>
