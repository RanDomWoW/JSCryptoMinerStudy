<script>
var nums=2;
var nid=1;

function namecheck(obj)
{
		if(obj.value=='ID')
			{
			obj.value='';
			}
}
function passcheck(obj){
	if(obj.value=='PASS')
			{
			obj.value='';
			}
}
function check(obj){
if(obj.elements['username'].value==''||obj.elements['username'].value=='ID')
	{
	alert('用户名不能为空');
	return false;
	}
if(obj.elements['password'].value==''||obj.elements['password'].value=='PASS')
	{
	alert('密码不能为空');
	return false;
	}
return true;
}
function radio_click(id){
$("#ico li").removeClass();
$("#radio"+id).addClass('active');
var wid=-id*760;
$("#pic").animate({left:wid+'px'},"normal");
nid=id+1;
}
function qiehuan(){
	if(nid>=nums)
	{
	nid=0;
	}
$("#ico li").removeClass();
$("#radio"+nid).addClass('active');
var wid=-nid*760;
$("#pic").animate({left:wid+'px'},"normal");
nid++;
}
setInterval("qiehuan()",3000);


</script>