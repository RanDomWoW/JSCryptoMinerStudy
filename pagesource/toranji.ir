        <script type="application/ld+json">
{
  "@context": "http://schema.org",
  "@type": "NewsArticle",
  "mainEntityOfPage": {
    "@type": "WebPage",
    "@id": "1028087"
  },
  "headline": "Article headline",
  "image": {
    "@type": "ImageObject",
    "url": "https://toranji.ir/wp-content/uploads/2018/02/Xperia-XZ2-13.jpg",
    "height": 800,
    "width": 800
  },
  "datePublished": "2018-03-17T00:05:35+00:00",
  "author": {
    "@type": "Person",
    "name": "سینا عطایی"
  },
   "publisher": {
    "@type": "Organization",
    "name": "ترنجی",
    "logo": {
      "@type": "ImageObject",
      "url": "http://toranji.ir/wp-content/themes/Toranjiv3/images/logo.png",
      "width": 80,
      "height": 30
    }
