                        $( ".password-requirements" ).removeClass("hide");
                        isVisibleRules = false
                    }                  
                });
            });

            function closeSignupModal() {
                setTimeout(function() {$('#signup-candidate-modal').modal('hide');}, 2000);
            }

            function validatePassword(password) {
                var isValidMinOneDigit = passValidator.validateMinOneDigit(password);
                var isValidLength = passValidator.validateLength(password);
                var isValidMinOneLowerCase = passValidator.validateMinOneLoweCase(password);
                var isValidMinOneUpperCase = passValidator.validateMinOneUpperCase(password);
                var isValidMinOneSpecialCharacter = passValidator.validateMinOneSpecialCharacter(password);

                if (isValidMinOneDigit) {
                    $( ".isValidMinOneDigit" ).addClass("completed");
                } else {
                    $( ".isValidMinOneDigit" ).removeClass("completed");
                }

                if (isValidLength) {
                    $( ".isValidLength" ).addClass("completed");
                } else {
                    $( ".isValidLength" ).removeClass("completed");
                }

                if (isValidMinOneLowerCase) {
                    $( ".isValidMinOneLowerCase" ).addClass("completed");
                } else {
                    $( ".isValidMinOneLowerCase" ).removeClass("completed");
                }

                if (isValidMinOneUpperCase) {
                    $( ".isValidMinOneUpperCase" ).addClass("completed");
                } else {
                    $( ".isValidMinOneUpperCase" ).removeClass("completed");
                }

                if (isValidMinOneSpecialCharacter) {
                    $( ".isValidMinOneSpecialCharacter" ).addClass("completed");
                } else {
                    $( ".isValidMinOneSpecialCharacter" ).removeClass("completed");
                }

                if(isValidMinOneDigit && isValidLength && isValidMinOneLowerCase && isValidMinOneUpperCase && isValidMinOneSpecialCharacter){
                    $( ".password-requirements" ).addClass("hide");
                    $( ".password-requirements-success" ).removeClass("hide");
                }else{
                    $( ".password-requirements-success" ).addClass("hide");
                    $( ".password-requirements" ).removeClass("hide");
                }
            }
        </script>
	
	
