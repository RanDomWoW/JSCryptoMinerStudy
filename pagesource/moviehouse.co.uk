			ga('linker:autoLink', ['jack-roe.co.uk']);
			ga('send', 'pageview');

			window.onload = function() {
				var a = document.getElementsByTagName('a');
				var filetypes = /\.(zip|exe|dmg|pdf|doc.*|xls.*|ppt.*|mp3|txt|rar|wma|mov|avi|wmv|flv|wav)$/i;
				
				for (i = 0; i < a.length; i++) {

					var isThisDomain = a[i].href.match(document.domain.split('.').reverse()[1] + '.' + document.domain.split('.').reverse()[0]);

					// track downloads
					if (a[i].hostname === location.hostname) {
						if (a[i].href.match((filetypes))) {
							a[i].setAttribute('target', '_blank');
							a[i].onclick = function() {
								ga('send', 'event', 'Downloads', 'Click', this.getAttribute('href'));
							};
						}
					}
					// track external links
					else if (a[i].href.match(/^https?\:/i) && !isThisDomain) {
						a[i].setAttribute('target', '_blank');
						a[i].onclick = function () {
							ga('send', 'event', 'External', 'Click', this.getAttribute('href'));
						};
					}
					// track mailto links
					else if (a[i].href.match(/^mailto\:/i)) {
						a[i].setAttribute('target', '_blank');
						a[i].onclick = function () {
							ga('send', 'event', 'MailTo', 'Click', this.getAttribute('href'));
						};
					}
					// track tel links
					else if (a[i].href.match(/^tel\:/i)) {
						a[i].setAttribute('target', '_blank');
						a[i].onclick = function () {
							ga('send', 'event', 'Tel', 'Click', this.getAttribute('href'));
						};
					}
				}
			}
		</script>



	<script>

		$(document).ready(function () {

			$('.coming-soon').click(function () {
				$('.coming-soon').removeClass('active');
				$(this).addClass('active');
			})

			$('#homepage-venue-select select').change(function () {
				if (this.options[this.selectedIndex].value != "") {
					location = this.options[this.selectedIndex].value;
				}
			});

		///// Book Now
	    function fillDropDown(selectElement, data) {

	        selectElement.empty();
	        selectElement.append(
				$('<option/>')
					.attr('value', '')
					.text('')
			);
	        $.each(data, function (index, item) {

	            if (isEmptyOrSpaces(item.Item2)) {

	                selectElement.append(
					$('<option disabled />')
						.text(item.Item1)
				    );

	            } else {

	                selectElement.append(
					$('<option/>')
						.attr('value', item.Item2)
						.text(item.Item1)
				    );

	            }

	        });

	        selectElement.trigger('chosen:updated');
	    }

		$.post('/Home/Movies/', {},
		function (data, status) {
			fillDropDown($('.filter-movie'), data);
		});

		$.post('/Home/Cinemas/', {},
		function (data, status) {
			fillDropDown($('.filter-cinema'), data);
		});

		$.post('/Home/AllDates/', {},
		function (data, status) {
			fillDropDown($('.filter-dates'), data);
		});

		$('#resetAll').click(function () {

			// set the state of the refresh icon
			$('#resetAll .fa-refresh').addClass("hide");
			$('#resetAll .fa-ticket').removeClass("hide");
			
			$.post('/Home/Movies/', {},
			function (data, status) {
				fillDropDown($('.filter-movie'), data);
			});

			$.post('/Home/Cinemas/', {},
			function (data, status) {
				fillDropDown($('.filter-cinema'), data);
			});

			$.post('/Home/AllDates/', {},
			function (data, status) {
				fillDropDown($('.filter-dates'), data);
			});

			$('.book-now').removeClass('show').addClass('hide');
			$('.cinema-now').removeClass('show').addClass('hide');
			$('.times').removeClass('show').addClass('hide');

		});

		function SetBookNowButton(selectElement, data, id) {

		    $.each(data, function (index, item) {

		        if (id == item.Item2) {
                    $(".book-now").attr("href", item.Item3);

                    if (!(isEmptyOrSpaces($('select.filter-cinema').val()))) {

                        $(".book-now").attr("href", item.Item3 + "?locationCode=" + $('select.filter-cinema').val());
                    }

                    $(".cinema-now").attr("href", item.Item3).text(item.Item1 + " Listings");
		        }

		    });
		}

		function fill(selectElement, data, id) {

		    selectElement.empty();

		    selectElement.append(
				$('<option/>')
					.attr('value', '')
					.text('')
			);

		    $.each(data, function (index, item) {

		        if(!(isEmptyOrSpaces(item.Item2))){
                
		            if (isEmptyOrSpaces(id)) {

		                selectElement.append($('<option/>').attr('value', item.Item2).text(item.Item1));

		            } else {

		                if (id == item.Item2) {

		                    selectElement.append($('<option/>').attr('value', item.Item2).text(item.Item1));
		                    selectElement.val(id);
		                }
		            }

		        } else {

		            if (isEmptyOrSpaces(id)) {
		                selectElement.append($('<option disabled />').text(item.Item1));
		            } 
		        }
		    });

		    selectElement.trigger('chosen:updated');
		}

		function isEmptyOrSpaces(id) {
		    return id === null || id.match(/^ *$/) !== null;
		}

		function getTimes(data, status) {

			// set the state of the refresh icon
			$('#resetAll .fa-refresh').removeClass("hide");
			$('#resetAll .fa-ticket').addClass("hide");


		    var id = $(this).attr('id');

			var params = {
				'performanceId': $('select.filter-movie').val(),
				'cinemaId': $('select.filter-cinema').val(),
				'dateId': $('select.filter-dates').val(),
				
			}

			$.post('/Home/BookNow', params, function (data, status) {

			    fill($('.filter-movie'),  data[0].Value, $('select.filter-movie').val());
			    fill($('.filter-cinema'), data[1].Value, $('select.filter-cinema').val());
			    fill($('.filter-dates'),  data[2].Value, $('select.filter-dates').val());

			    if ((!isEmptyOrSpaces($('select.filter-movie').val()))) {

			        if (isEmptyOrSpaces($('select.filter-cinema').val()) || isEmptyOrSpaces($('select.filter-dates').val())) {

			            SetBookNowButton($('.filter-movie'), data[0].Value, $('select.filter-movie').val());

			            $('.book-now').removeClass('hide').addClass('show');
			            $('.cinema-now').removeClass('show').addClass('hide');

			        } else {

			            $('.book-now').removeClass('show').addClass('hide');
			            $('.cinema-now').removeClass('show').addClass('hide');
			        }

			    } else if ((!isEmptyOrSpaces($('select.filter-cinema').val()))) {

			        if (isEmptyOrSpaces($('select.filter-movie').val())) {

			            SetBookNowButton($('.filter-cinema'), data[1].Value, $('select.filter-cinema').val());

			            $('.cinema-now').removeClass('hide').addClass('show');
			            $('.book-now').removeClass('show').addClass('hide');

			            if (!(isEmptyOrSpaces($('select.filter-dates').val()))) {

			                var $a = $(".cinema-now"),
                                href = $a.attr("href");

			                $a.attr("href", href.split('?')[0] + "?date=" + $('select.filter-dates').val());
			            }

			        } else {

			            $('.cinema-now').removeClass('show').addClass('hide');
			        }

			    } else if (isEmptyOrSpaces($('select.filter-movie').val()) && isEmptyOrSpaces($('select.filter-cinema').val()) && isEmptyOrSpaces($('select.filter-dates').val())) {

			        $('.book-now').removeClass('show').addClass('hide');
			        $('.cinema-now').removeClass('show').addClass('hide');
			    } else {
			        $('.book-now').removeClass('show').addClass('hide');
			        $('.cinema-now').removeClass('show').addClass('hide');
			    }

			    $.get('/Home/Times/', params, function (data, status) {
			    	$('.times').html(data);
			    	$('.times').removeClass('hide').addClass('show');
			    });


			});


		};

		$('.filter-cinema, .filter-movie, .filter-dates').on('change', getTimes);


		// main tabs
		$('#movie-tab').click(function (event) {
			event.preventDefault();

			$('#event-tab').removeClass('tab__item--active');
			$('#coming-tab').removeClass('tab__item--active');

			$('#event-tab').addClass('tab__item--disabled');
			$('#coming-tab').addClass('tab__item--disabled');


			$('#movie-tab').removeClass('tab__item--disabled');
			$('#movie-tab').addClass('tab__item--active');

			$('#tab-movie').removeClass('hide');
			$('#tab-event').addClass('hide');
			$('#tab-coming').addClass('hide');

		});

		$('#event-tab').click(function (event) {
			event.preventDefault();

			$('#movie-tab').removeClass('tab__item--active');
			$('#coming-tab').removeClass('tab__item--active');

			$('#movie-tab').addClass('tab__item--disabled');
			$('#coming-tab').addClass('tab__item--disabled');

			$('#event-tab').removeClass('tab__item--disabled');
			$('#event-tab').addClass('tab__item--active');

			$('#tab-event').removeClass('hide');
			$('#tab-movie').addClass('hide');
			$('#tab-coming').addClass('hide');

		});

		$('#coming-tab').click(function (event) {
			event.preventDefault();

			$('#event-tab').removeClass('tab__item--active');
			$('#movie-tab').removeClass('tab__item--active');

			$('#event-tab').addClass('tab__item--disabled');
			$('#movie-tab').addClass('tab__item--disabled');

			$('#coming-tab').removeClass('tab__item--disabled');
			$('#coming-tab').addClass('tab__item--active');

			$('#tab-coming').removeClass('hide');
			$('#tab-event').addClass('hide');
			$('#tab-movie').addClass('hide');

		});


		// these funcs are for the coming soon tab
		$('.all-button').click(function (event) {
			event.preventDefault();

			$('.movie').removeClass('hide');
			$('.event').removeClass('hide');

			$('#all-header').removeClass('hide');
			$('#event-header').addClass('hide');
			$('#movie-header').addClass('hide');

		});

		$('.event-button').click(function (event) {
			event.preventDefault();

			$('.event').removeClass('hide');
			$('.movie').addClass('hide');

			$('#event-header').removeClass('hide');
			$('#all-header').addClass('hide');
			$('#movie-header').addClass('hide');
		});

		$('.movie-button').click(function (event) {
			event.preventDefault();

			$('.movie').removeClass('hide');
			$('.event').addClass('hide');

			$('#movie-header').removeClass('hide');
			$('#all-header').addClass('hide');
			$('#event-header').addClass('hide');
		});

		});

        $('.chosen-search').hide();

	</script>

</body>
</html>