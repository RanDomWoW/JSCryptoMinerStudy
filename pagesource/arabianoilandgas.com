}

function nextNewsItem(){
	playPauseImg = document.getElementById("Controller").childNodes[0];
	index++;
	if(index==listLength){index=0;}
	clearTimeout(t1);clearTimeout(t2);
	char_pos = 0;
	textticker(index);
	playPauseImg.alt = "Pause";
	playPauseImg.src="/pictures/furniture/btn_gray_pause.gif";
}

function prevNewsItem(){
	playPauseImg = document.getElementById("Controller").childNodes[0];
