    function displayAutoSmallBoxes() {
        jQuery(".zone.automatic-box").each(function () {
            var currentAutoBox=jQuery(this);

            var json_config = currentAutoBox.children(".tiles").attr("data-json-config");
            var whether_with_ads =jQuery.parseJSON( currentAutoBox.children(".tiles").attr("data-whether-with-ads") );
            var whether_with_blogbar =jQuery.parseJSON( currentAutoBox.children(".tiles").attr("data-whether-with-blogbar") );   
            
            jQuery.ajax({
                    type: "GET",
                    cache: false,
                    url: "/HttpHandlersNew/NewAutoSmallBoxHandler.ashx",
                    data: { "JsonConfig": json_config, "WhetherWithAds": whether_with_ads, "WhetherWithBlogBar": whether_with_blogbar, "Within_FamilySubsite": "False" },
                    success: function (htmlSegment) {                   
                        if (jQuery.trim(htmlSegment) == "") {
                            currentAutoBox.children(".tiles").html("");
                            currentAutoBox.hide();
                        }
                        else {
                            if (whether_with_ads) {
                                currentAutoBox.children(".tiles").children(".ajax-loader-image").remove();

                                currentAutoBox.children(".tiles").children(".auto_small_box--tile_ad").before(jQuery("<div>" + htmlSegment + "</div>").children(".auto_small_box--tile_ad").prevAll());
                                currentAutoBox.children(".tiles").children(".auto_small_box--tile_ad").show();
                                currentAutoBox.children(".tiles").children(".auto_small_box--tile_ad").after(jQuery("<div>" + htmlSegment + "</div>").children(".auto_small_box--tile_ad").nextAll());
                            }
                            else {
                                currentAutoBox.children(".tiles").empty().html(htmlSegment);

                                if (currentAutoBox.hasClass("zone_with_blogbar"))
                                    combineBlogBarWithZone("automatic-box");
                            }

                            currentAutoBox.find("a.tile__anchor_link, a.tile__read_more, a.tile__image_wrapper, a.tile__booking").each(function () { jQuery(this).on("click", siteSections_clickHandler); });
                        }
                    },
                    error: function () {
                        currentAutoBox.children(".tiles").html("");
                        currentAutoBox.hide();
                        
                    }
            });

        });
    }


    function displayVideoBoxes() {
        jQuery(".zone.video-box").each(function () {
            var currentVideoBox = jQuery(this);
            var articleBody = currentVideoBox.children(".video_article_body");
            var videoObject = articleBody.find("embed[src*='youku.com'],embed[src*='tudou.com']");
            if (videoObject.length == 0) {
                videoObject = articleBody.find("iframe[src*='youku.com'],iframe[src*='tudou.com']");
            }
            if (articleBody.html() != "" && videoObject.length > 0) {
                currentVideoBox.find(".video_wrapper").append(videoObject[0]);
                currentVideoBox.show();
            }
        });
    }
