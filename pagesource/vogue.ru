                        contentType: 'application/json; charset=utf-8;',
                        headers: {
                            'x-G-CNID': getCookie('G_CNID') || 'undefined'
                        },
                        data: JSON.stringify({
                            'domain': 'vogue.ru',
                            'path': window.location.pathname
                        }),
                        timeout: 300
                    })
                    .done(function (response) {
                        if (response.response_map && response.response_map.segment_names.length) {
                            googletag.cmd.push(function() {
                                googletag.pubads().setTargeting('ru_segments', response.response_map.segment_names);
                            });
                        }
                    })
                    .fail(function(response) {
                        console.log('Segmantator API didn\'t respond longer than 300ms', response);
                    })
                    .always(defineBannersSlots);
                };
