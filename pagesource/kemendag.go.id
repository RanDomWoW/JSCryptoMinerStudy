	<style>

  .bgImage{
    top: 0;
   bottom: 0;
   left: 0;
   right: 0;
    background-image:url('files/images/asn2017rekrutmen.jpg');
    width:800px;
    height:250px;
    background-repeat: no-repeat;

    opacity: 1;
   filter:alpha(opacity=100);
   text-shadow: 1px 1px #ffffff;
  }

  img.bgImage{
left:-300px;

  }

a#navig-next{
	float:right;
	margin-right:20px;
	font-size:18px;
}

a#navig-prev{
	float:right;
	margin-right:10px;
	font-size:18px;
