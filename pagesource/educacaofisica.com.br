		jQuery(".img-frame img").each(function(index, element) {
			var oThis = this;
			if(jQuery(this).parents(".left-wrap").length >0) {
				jQuery('<img/>').load(function() {
					var tamanho = jQuery(oThis).parents(".left-wrap").eq(0);
				
					var novaAltura = tamanho.height();
					var novaLargura = Math.round(this.width * (novaAltura / this.height));
					
					// Verificar se deve ser encaixado pela altura
					if(novaLargura< tamanho.width()) {
						novaLargura = tamanho.width();
						novaAltura = Math.round(this.height * (novaLargura / this.width));
					}
					
					// APlicar novo tamanho
					jQuery(oThis).css("cssText", "width:" + novaLargura + "px !important; height:" + novaAltura + "px !important;");
					
					// Centralizar imagem
					jQuery(oThis).parents(".img-frame").css({"cssText": "width:" + novaLargura + "px !important; height:" + novaAltura + "px !important; background-color: " + jQuery(oThis).parents(".img-frame").css("background-color")
