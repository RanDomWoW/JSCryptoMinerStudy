<script>
//ais_interface_setup(loadtheme, loadsite, loadlanguage, multilanguage, loadfile)
ais_interface_setup('full-menu','home','th','','');

$(document).ready(function () {
	async_load('th');
	var iter = 0;
	var update = setInterval(function () {
		if (typeof data_source != 'undefined') {
			clearInterval(update);
			getBanner(data_source);


			setTimeout(function () {
				swiperTopbanner();
				setTimeout(function () {
					$('#slider-topbanner .swiper-slide').matchHeight();
				}, 100);
			}, 500);
		} else if (iter >= 3) {
			data_source = "http://www.ais.co.th/2017/herobanner/data/default.txt"; /// chang url 
			clearInterval(update);
			getBanner(data_source);

			setTimeout(function () {
				swiperTopbanner();
				setTimeout(function () {
					$('#slider-topbanner .swiper-slide').matchHeight();
				}, 100);
			}, 500);
		}
		iter++;
	}, 1000);
	setTimeout(function () {
		$('#herobaner_ais').css('min-height', 'auto');
	}, 1100);
	
	 

});

function getBanner(data_source) {
	$.getJSON(data_source, function (myObj) {
		$.each(myObj, function (index, val) {
			var banner = $('<div class="swiper-slide thumbnail"></div>');
			var des = $('<a target="_blank"></a>');
			var pc_item = $('<div class="col-xs-0 col-sm-12 panel0"></div>');
			var mb_item = $('<div class="col-xs-12 col-sm-0 panel0"></div>');
			var pc_img = $('<img alt="ais hilight banner" width="500" height="500"/>');
			var mb_img = $('<img alt="ais hilight banner" width="1600" height="500"/>');
			des = des.attr('href', val.link);
			pc_img = pc_img.attr('src', val.large);
			mb_img = mb_img.attr('src', val.small);
			pc_img.appendTo(pc_item);
			mb_img.appendTo(mb_item);
			pc_item.appendTo(des);
			mb_item.appendTo(des);
			des.appendTo(banner);
			banner.appendTo('#herobaner_ais');
		})
	})
}
</script>
