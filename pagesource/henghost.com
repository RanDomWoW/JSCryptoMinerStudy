<script>
$(function(){
$.getJSON('https://my.henghost.com/yun.php?type=userlogin&callback=?',function(data){
	if(data.status=="success"){		
		$(".topnav .navbar-left li:first").find("a").attr("href","http://my.henghost.com/clientarea.php");
		$(".topnav .navbar-left li:first").find("a").text("您好,"+data.msg+"欢迎回来");
	}
});
getcartall();
function getcartall(){
$.getJSON('https://my.henghost.com/yun.php?type=scartall&datatype=json&callback=?',function(data){
	if(data.status=="success"){		
		if(data.cart_products){
			$.each(data.cart_products,function(i,v){
				var tr='<tr class="cart_products"><td>'+v.productsname+'('+v.domain+')</td> <td>￥'+v.price+'/'+v.billingcycle+'</td> <td><a href="javascrips:;" n="'+i+'">移除</a></td> </tr>';
				$(".shop tbody").append(tr);
			});
		}
		if(data.cart_domain){
			$.each(data.cart_domain,function(i,v){
				var tr='<tr class="cart_domain"><td>'+v.domain+'</td> <td>￥'+v.price+'/'+v.billingcycle+'</td> <td><a href="javascrips:;" n="'+i+'">移除</a></td> </tr>';
				$(".shop tbody").append(tr);
			});
		}
		if(data.promo>0){
			$(".shop .shop_tab_p1 label").show();			
		}
		$(".shop .shop_tab_p1 label").html("折扣：￥"+data.promo);
		$(".shop .shop_tab_p1 .shop_sp1").html("￥"+data.total);
		if(data.total==0){
			
		}
	}
});
}
function clearcart(t,n){
	$.getJSON('https://my.henghost.com/yun.php?type=delscart&t='+t+'&n='+n+'&callback=?',function(data){
		
	});
	$(".cart_products,cart_domain").remove();
	getcartall();
}
$(document).on("click",".shop .shop_tab .cart_products a",function(){
	var n=$(this).attr("n");
	$(this).parent().parent().remove();
	clearcart(1,n);
});
$(document).on("click",".shop .shop_tab .cart_domain a",function(){
	var n=$(this).attr("n");
	$(this).parent().parent().remove();
	clearcart(2,n);
});
$(".shop .shop_tab_p2 span:eq(1)").click(function(){	
	$(".shop tbody tr").not("tr:eq(0)").html("");
	$(".shop .shop_tab_p1 .shop_sp1").html("￥0.00");
	$(".shop .shop_tab_p1 label").hide();
	clearcart(3,3);
})
$(".shop .shop_tab_p2 span:eq(0)").click(function(){
	window.location.href="https://my.henghost.com/cart.php?a=checkout";
})

	$(".cloudrouter").click(function(){
		var house="hk";
		$.getJSON('https://my.henghost.com/yun.php?type=cloudtype&buytype=orderrouter&house='+house+'&callback=?',function(data){ 
			window.location.href="https://www.henghost.com/cloud-order.shtml";                     
		});   
	});


});
</script>

