function removeilicesemt(removeText,remElem){
		$.ajax({
		type: "POST",
		url: "ilcesemtajaxAC",
		data: "query=" + removeText + "&act=sessionRem",
		dataType: "html",
		success: function(html){
				$('#selection-ajax').children("#"+remElem).remove();
				if ($('#selection-ajax').html()==""){
					$('#currentSehir').hide();
					$.ajax({
						type: "POST",
