	var sliderUlWidth = slideCount * windowW;

	$('#slider').css({ width: windowW, height: slideHeight });
	$('#slider ul').css({ width: sliderUlWidth, marginLeft: - windowW });
	$('#slider ul li:last-child').prependTo('#slider ul');
};

function moveRight() {
	$('#slider ul').animate({
		left: - windowW
	}, 700, function () {
		$('#slider ul li:first-child').appendTo('#slider ul');
		$('#slider ul').css('left', '');
	});
};
