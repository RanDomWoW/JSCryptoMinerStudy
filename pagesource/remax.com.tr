function simpleSearchControl(){
	if($('#secim').val()=="gd" || $('#secim').val()=="ofis"){
		$('#frm').submit();
	}else{
		if (Trim($('#ililcesemtAC').val()) == ""){
			divAlert("Lütfen Arama Yapacağınız Kriterleri Belirleyiniz.");
			}else{
				if($('#ililcesemtAC').val().length>5 && ($.isNumeric($('#ililcesemtAC').val()) || $.isNumeric($('#ililcesemtAC').val().substring(0,4)))){ 
					window.location='/'+$('#ililcesemtAC').val();
				}else {
					if($('#simpleSatilik').hasClass('homeIslemtipiAktifButton')){ url='satilik'; }
					if($('#simpleKiralik').hasClass('homeIslemtipiAktifButton')){ url='kiralik'; }
//					alert(autocomp);
					if(autocomp==1){				
						realTextVal = $('#ililcesemtACHidden').val();
					}else{
						realTextVal = $('#ililcesemtAC').val();
					}
	
					url+='/'+$('#secimSimple').val()+'/'+$.trim(realTextVal.replace(/\|/g,'%20').replace(/ /g,'/').replace(/\+/g,'='))+'/';
	//				alert(url);
	//				return false;
	
					if($('#minfiyatSimple').val()){ url+=$('#minfiyatSimple').val(); }
					if($('#maxfiyatSimple').val()){ url+='-'+$('#maxfiyatSimple').val(); }
					if($('#minfiyatSimple').val()||$('#maxfiyatSimple').val()){ url+=$('#fiyatbirimSimple').val()+'/'; }
	
					if($('#minmetrekareSimple').val()){ url+=$('#minmetrekareSimple').val(); }
					if($('#maxmetrekareSimple').val()){ url+='-'+$('#maxmetrekareSimple').val(); }
					if($('#minmetrekareSimple').val()||$('#maxmetrekareSimple').val()){ url+='m2/'; }
	
					if($('#odaSayisiSimple').val()!=""){ url+=$('#odaSayisiSimple').val()+'=1/'; }
					$("#loader").show();
					if(autocomp==1){
						$.each($(".autocomplete-suggestion"),function(a,b) {
						if($.trim($('#ililcesemtAC').val())==$.trim($(this).text())&&url.indexOf('/~')<0){url+='~';}
						});
					}
		setTimeout("yonlendir()",1000);
		  }
	  }
	}
}

