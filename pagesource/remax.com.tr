windowW = $(window).width();
$("#slider ul li").css('width',windowW+'px');
$("#slider ul li").css('background-size',windowW+'px');
$("#slider ul li").css('background-position','center center');

	setInterval(function () {
		moveRight();
	}, 10000);

	var slideCount = $('#slider ul li').length;
	var slideHeight = $('#slider ul li').height();
	var sliderUlWidth = slideCount * windowW;

	$('#slider').css({ width: windowW, height: slideHeight });
	$('#slider ul').css({ width: sliderUlWidth, marginLeft: - windowW });
	$('#slider ul li:last-child').prependTo('#slider ul');
};

function imgKonum2()
