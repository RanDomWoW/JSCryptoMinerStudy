window.onload = function() {

(function() {
    var emScrollTop = document.getElementById('scroll-top');
 	var ssTop = +(document.body.scrollTop || document.documentElement.scrollTop);

    if (ssTop < 300) {
        emScrollTop.style.display = "none";
    }else {
        emScrollTop.style.display = "block";
    }

		function scroll() {
			ssTop = document.body.scrollTop || document.documentElement.scrollTop;
        if (ssTop >= 300) {
            emScrollTop.style.display = "block";
          
        }else {
            emScrollTop.style.display = "none";
        }
    }
    var timer = 0;
    emScrollTop.onclick = function() {
    	clearInterval(timer)
        timer = setInterval(scrTop, 16);
