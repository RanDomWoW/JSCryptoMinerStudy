    function scrTop() {
        ssTop -= 128;
        if (ssTop <= 0) {
            clearInterval(timer);
            emScrollTop.style.display = "none";
        };
        document.body.scrollTop = ssTop;
        document.documentElement.scrollTop = ssTop;
    }

    if (document.addEventListener) {
        document.addEventListener('DOMMouseScroll', scroll, false);
    };
    window.onmousewheel = document.onmousewheel = scroll;
