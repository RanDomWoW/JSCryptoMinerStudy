        var path = '';
        for(i=0 ; i < file.length-1 ; i++){
            path = ( i == 0 )?path + file[i]:path + '/' + file[i];
        }
        jQuery(this).attr('src',path+'/'+filename.replace(/_on.([a-zA-Z]{3,3})jQuery/, ".jQuery1"));
        //jQuery(this).attr('src',path+'/'+filename.replace('_on.','.'));
    });
});

