jQuery(document).ready(function(){
    jQuery(".imgon").mouseover(function (){
        var file = jQuery(this).attr('src').split('/');
        var filename = file[file.length-1];
        var path = '';
 
        for(i=0 ; i < file.length-1 ; i++) {
            path = ( i == 0 )?path + file[i]:path + '/' + file[i];
        }
        jQuery(this).attr('src',path+'/'+filename.replace(/.([a-zA-Z]{3,3})jQuery/, "_on.jQuery1"));
        //jQuery(this).attr('src',path+'/'+filename.replace('.','_on.'));
 
    }).mouseout(function(){
        var file = jQuery(this).attr('src').split('/');
