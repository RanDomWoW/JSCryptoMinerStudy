		var index = index+1;        
		var imgwidth = 1280 // 이미지 넓이
		var leftoffset = index*imgwidth-imgwidth;
		$(this).attr("data-index",index);
		if(index == 1){
			$(this).addClass("on");
			$(this).css("left","0").css("z-index","3");
		}
	});    
	$(".vthum > ul > li > a").each(function(index){
		var index = index+1;
		$(this).attr("data-index",index);
		if(index == 1){
			$(".vthum > ul > li > a[data-index='1']").addClass("paging_on");
		}
	});   
	$(".vprev").click(moveprev);
	$(".vnext").click(movenext);    
	function movenext(){
		if(!$('.visual_list').is(":animated")){
		    if($('.on').attr("data-index") == vleng){
			     var nextindex = 1;	
		    }else{
			     var nextindex = parseInt($(".on").attr("data-index"))+1;
		    }
		    //alert(nextindex)
		    $(".on").css("z-index","2").removeClass("on").siblings("li[data-index='"+nextindex+"']").css("left","1280px").addClass("on");
		    $(".vthum > ul > li > a").removeClass("paging_on");
		    $(".vthum > ul > li > a[data-index='"+nextindex+"']").addClass("paging_on");
		    $(".visual_list > li:not(:animated)").animate({"left" : "-=1280"},500,function(){
			$('.visual_list').css("left","0");
			$('.visual_list > li').css("left","0");
			$(".on").css("z-index","3");
			$('.visual_list > li').not('.on').css("z-index","1");

		    });
		}
	}
	function moveprev(){
		if(!$('.visual_list').is(":animated")){
		if($(".on").attr("data-index") == 1){
		var previndex = vleng;	
		}else{
		var previndex = parseInt($(".on").attr("data-index"))-1;
		}
		$(".on").css("z-index","2").removeClass("on").siblings("li[data-index='"+previndex+"']").css("left","-1280px").addClass("on");
