		$(".visual_list > li:not(:animated)").animate({"left" : "+=1280"},500,function(){
			$('.visual_list').css("left","0");
			$('.visual_list > li').css("left","0");
			$(".on").css("z-index","3");
			$('.visual_list > li').not('.on').css("z-index","1");
		});
		}
	}    
	$(".vthum > ul > li > a").click(function(){
		var i = $(this).attr("data-index");
		var k = $(".on").attr("data-index");
		//alert(i+" : "+k);
		$(this).addClass("paging_on");
		$(this).parent().siblings().find("a").removeClass("paging_on");
		if(i>k){
		$(".on").css("z-index","2").removeClass("on").siblings("li[data-index='"+i+"']").css("left","1280px").addClass("on");
		$(".visual_list > li:not(:animated)").animate({"left" : "-=1280"},500,function(){
			$('.visual_list').css("left","0");
			$('.visual_list > li').css("left","0");
			$(".on").css("z-index","3");
			$('.visual_list > li').not('.on').css("z-index","1");
		});
		}else if(i==k){
        }else{
		$(".on").css("z-index","2").removeClass("on").siblings("li[data-index='"+i+"']").css("left","-1280px").addClass("on");
		$(".visual_list > li:not(:animated)").animate({"left" : "+=1280"},500,function(){
			$('.visual_list').css("left","0");
			$('.visual_list > li').css("left","0");
			$(".on").css("z-index","3");
			$('.visual_list > li').not('.on').css("z-index","1");
			});	
		}
	});    
	timer=setInterval(timerHandler,5000);

	function timerHandler()	{
		movenext();
	}
	$(".vthum a,.vprev,.vnext").hover(function(){
		clearInterval(timer);
	},function(){
		timer=setInterval(timerHandler,5000);
	})
}) 
</script> 

