	lilength = $(".rban_lists > ul > li").length;
	$(".rban_lists > ul > li").each(function(index){
		var index = index+1;
		var imgwidth = 1280 // 이미지 넓이
		var leftoffset = index*imgwidth-imgwidth;
		$(this).attr("data-index",index);
		$(".rthum ul").append("<li><a href='javascript:;' data-index='"+index+"'><img src='/design/vittz/img/main_area3_ball.png' alt='' /></a></li>");
		if(index == 1){
			$(this).addClass("act");
			$(this).css("left","0").css("z-index","3");
			$(".rthum > ul > li a[data-index='1']").addClass("paging_on")
		}
	});
	$(".rthum > ul > li > a.paging_on img").each(function(){
		$(".rthum > ul > li > a.paging_on img").attr("src",$(".rthum ul li a.paging_on img").attr("src").replace(".png","_over.png"))
	})				
	$(".rprev").click(moveprev);
	$(".rnext").click(movenext);
	function movenext(){

		if(!$('.rban_lists > ul').is(":animated")){
		    if($('.act').attr("data-index") == lilength){
			    var nextindex = 1;	
		    }else{
			    var nextindex = parseInt($(".act").attr("data-index"))+1;
		    }
		    //alert(nextindex)

		    $(".act").css("z-index","2").removeClass("act").siblings("li[data-index='"+nextindex+"']").css("left","1280px").addClass("act");
		    $(".rthum > ul > li > a").removeClass("paging_on");
		    $(".rthum > ul > li > a[data-index='"+nextindex+"']").addClass("paging_on");
		    $(".rban_lists > ul > li").animate({"left" : "-=1280"},500,function(){
			    $('.rban_lists > ul').css("left","0");
			    $('.rban_lists > ul > li').css("left","0");
			    $(".act").css("z-index","3");
			    $('.rban_lists > ul > li').not('.act').css("z-index","1");
			    $(".rthum > ul > li > a > img").each(function(){
				    $(".rthum > ul > li > a > img").attr("src",$(".rthum > ul > li a img").attr("src").replace("_over.png",".png"))
			    })				
				    $(".rthum > ul > li > a.paging_on img").attr("src",$(".rthum > ul > li a.paging_on img").attr("src").replace(".png","_over.png"))
		    });
		}
        
	}
	function moveprev(){
		if(!$('.rban_lists > ul').is(":animated")){
		if($(".act").attr("data-index") == 1){
		var previndex = lilength;	
		}else{
		var previndex = parseInt($(".act").attr("data-index"))-1;
		}
		$(".act").css("z-index","2").removeClass("act").siblings("li[data-index='"+previndex+"']").css("left","-1280px").addClass("act");
		$(".rthum > ul > li > a").removeClass("paging_on");
		$(".rthum > ul > li > a[data-index='"+previndex+"']").addClass("paging_on");
		$(".rban_lists > ul > li").animate({"left" : "+=1280"},500,function(){
			$('.rban_lists > ul').css("left","0");
			$('.rban_lists > ul > li').css("left","0");
			$(".act").css("z-index","3");
			$('.rban_lists > ul > li').not('.act').css("z-index","1");
			$(".rthum > ul > li > a > img").each(function(){
				$(".rthum > ul > li > a > img").attr("src",$(".rthum > ul > li a img").attr("src").replace("_over.png",".png"))
			})				
				$(".rthum > ul > li > a.paging_on img").attr("src",$(".rthum > ul > li a.paging_on img").attr("src").replace(".png","_over.png"))
		});
		}
	}
	$(".rthum a").click(function(){
		var i = $(this).attr("data-index");
		var k = $(".act").attr("data-index");

		$(this).addClass("paging_on");
		$(this).parent().siblings().find("a").removeClass("paging_on");
		if(i>k){
		$(".act").css("z-index","2").removeClass("act").siblings("li[data-index='"+i+"']").css("left","1280px").addClass("act");
		$(".rban_lists > ul > li").animate({"left" : "-=1280"},500,function(){
			$('.rban_lists > ul').css("left","0");
			$('.rban_lists > ul > li').css("left","0");
			$(".act").css("z-index","3");
			$('.rban_lists > ul > li').not('.act').css("z-index","1");
			$(".rthum > ul > li > a > img").each(function(){
				$(".rthum > ul > li > a > img").attr("src",$(".rthum > ul > li > a > img").attr("src").replace("_over.png",".png"))
			})				
				$(".rthum > ul > li > a.paging_on img").attr("src",$(".rthum > ul > li > a.paging_on img").attr("src").replace(".png","_over.png"))
		});
		}else{
		  $(".act").css("z-index","2").removeClass("act").siblings("li[data-index='"+i+"']").css("left","-1280px").addClass("act");
		      $(".rban_lists > ul > li").animate({"left" : "+=1280"},500,function(){
			     $('.rban_lists > ul').css("left","0");
			     $('.rban_lists > ul > li').css("left","0");
			     $(".act").css("z-index","3");
			     $('.rban_lists > ul > li').not('.act').css("z-index","1");
			     $(".rthum > ul > li > a > img").each(function(){
				    $(".rthum > ul > li > a > img").attr("src",$(".rthum > ul > li > a > img").attr("src").replace("_over.png",".png"))
			     })				
				    $(".rthum > ul > li > a.paging_on img").attr("src",$(".rthum > ul > li > a.paging_on img").attr("src").replace(".png","_over.png"))
		      });	
		}
	});                   
    /* 메인 중앙 배너 자동롤링 */    
    $(".rprev,.rnext").hover(function(){
        clearInterval(timer2);
    },function(){

        timer2 = setInterval(automove2,5000);
    });
    
    automove2 = function(){
      movenext();
    }
    timer2 = setInterval(automove2,5000);            
}) 
</script> 

