$(function() {

$('#index_subscribe_email').on('focus',function(){
$(this).attr('placeholder','');
});

$('#index_subscribe_email').on('blur',function(){
$(this).attr('placeholder','Ваш email');
});

var subscribeInProgress = false;

$( "#index_subscribe_submit" ).on( "click", function() {
if( subscribeInProgress ) return true;
if( $('#index_subscribe_email').val().length<=0 ) return true;
//
var btn = $( this );
var container = btn.closest( ".index-banner-wrapper-inner" );
var field = container.find( "#index_subscribe_email" );
var email = String( field.val() );
var correct = /^[a-zA-Z0-9][-_a-zA-Z0-9\.]{1,20}@[a-zA-Z0-9][-_a-zA-Z0-9\.]+\.[-_a-zA-Z0-9]{2,5}$/ig
.test( email );

if( correct ) {
//showSuccess();
subscribeInProgress = true;
$.ajax({
"url": "/ajax/index_subscribe.php",
"type": "POST",
"timeout": 20000,// 20 сек
"data": {
"email": email,
"do_submit": "Y",
},
"complete": function(data) {
subscribeInProgress = false;
},
"error": function() {
showErrorServer();
},
"success": function( data ) {
data = $(data);
console.log(data);

if( data.find( ".error-flag" ).length > 0 ) {
showErrorEmail();
} else if( data.find( ".error-email-exist" ).length > 0 ){
showErrorEmailExist();
} else if( data.find( ".success-flag" ).length > 0 ) {
showSuccess();
} else {
showErrorServer();
}

ga('send', 'event', 'banner-submit-email', 'submit');
yaCounter25391549.reachGoal('email-banner-submit');
}
});
} else {
showErrorEmail();
}

function showErrorEmail() {
var masterPosition = btn.position();
container.find( ".index-subscribe-errors-email" )
.css({ "top": masterPosition.top + "px", "left": masterPosition.left + "px" })
.fadeIn( 200, function() {
setTimeout( function() {
$( ".index-subscribe-errors-email" ).fadeOut( 2000 );
}, 1000 );
});
}

function showErrorServer() {
var masterPosition = btn.position();
container.find( ".index-subscribe-errors-server" )
.css({ "top": masterPosition.top + "px", "left": masterPosition.left + "px" })
.fadeIn( 200, function() {
setTimeout( function() {
$( ".index-subscribe-errors-server" ).fadeOut( 2000 );
}, 1000 );
});
}

function showErrorEmailExist() {
var masterPosition = btn.position();
container.find( ".index-subscribe-errors-email-exist" )
.css({ "top": masterPosition.top + "px", "left": masterPosition.left + "px" })
.fadeIn( 200, function() {
setTimeout( function() {
$( ".index-subscribe-errors-email-exist" ).fadeOut( 2000 );
}, 1000 );
});
}


function showSuccess() {
