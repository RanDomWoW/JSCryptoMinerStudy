<script>
  function getActiveCategoryClass() {
    var genre = $.Params.get('genre%5B%5D');
    var main  = $.Params.get('set%5Bmain%5D');
    var criteria = $.Params.get('criteria');
    if (criteria) {
      if (main == "leisure") {
        if (genre == "syukuhaku") return "stay";
        else if (genre == "leisure") return "play";
      }
      else if (main) {
        if (main == "beauty") return "beauty";
        else if (main == "health") return "health";
        else if (main == "lesson") return "lesson";
        else if (main == "delivery") return "mailorder";
        else if (main == "campaign") return "benefits";
        else if (main == "syukuhaku") return "stay";
        else return genre;
      }
      else if (!main && criteria == "date") {
        return "new";
      }
    }
    return null;
  }

  km.util.adjustAnchorAreaId('.kp-category a');
  jQuery(function($) {
    $('.kp-category ul').children('li.toggle').each(function() {
      if ($(this).hasClass(getActiveCategoryClass()))
        $(this).addClass("active-category");
    });
  });
</script>



