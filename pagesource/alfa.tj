  //More FUNC
		$(function(){
			var adjustheight = 105;
			var moreText = "œÓ‰Ó·ÌÂÂÖ";
			var lessText = "—‚ÂÌÛÚ¸";
			var maxheight = $(".more-block").height();
			$(".more-block").each(function () {
				if ($(this).height()> adjustheight) {
					$(this).css('height', adjustheight).css('overflow', 'hidden');
					$(this).parent('.more-less').append('<p class="more-link"><a class="adjust more">œÓ‰Ó·ÌÂÂÖ</a></p>');
					$("a.adjust").text(moreText);
					$(function(){
						$('a.adjust').click(function(){
							var iteration=$(this).data('iteration')||1
							switch ( iteration) {
								case 1:
									$(this).parents("div:first").find(".more-block").animate({height:maxheight},350,"swing");
									$(this).text(lessText);
									break;
								
								case 2:
									$(this).parents("div:first").find(".more-block").animate({height:adjustheight},350,"swing");
									$(this).text(moreText);
									break;
							}
							iteration++;
							if (iteration>2) iteration=1
							$(this).data('iteration',iteration)
						})
					})
				};
			});
		});
	
	//Width of slider nav
	var sl_outer = $('.sl_outer_er').width();
	
