        });

        var selectedStates = ['ak', 'az', 'ca', 'co', 'fl', 'hi', 'il', 'ma', 'me', 'mi', 'nv', 'nm', 'or', 'wa'];

        $(document).ready(function () {
            $('#state-map-home').vectorMap({
                map: 'usa_en',
                backgroundColor: '#fff',
                borderColor: '#999',
                borderOpacity: 0.4,
                borderWidth: 1,
                color: '#f4f3f0',
                enableZoom: false,
                hoverColor: '#72C02C',
                hoverOpacity: null,
                selectedColor: '#588033',
                selectedRegions: selectedStates,
                showTooltip: true,
                onRegionOver: function (event, code) {
                    var skip_index = selectedStates.indexOf(code);
                    if (skip_index == -1) {
                        event.preventDefault();
                        return false;
                    }
                    $(this).css("cursor", "pointer");

                    $(".state-list-home li").removeClass("map-overlay");
                    $(".state-list-home li[data-stateabbr='" + code + "']").addClass("map-overlay");
                },
                onRegionOut: function (event, code, region) {
                    $(this).css("cursor", "");
                    $(".state-list-home li").removeClass("map-overlay");
                },
                onRegionClick: function (element, code, region) {
                    var skip_index = selectedStates.indexOf(code);
                    if (skip_index == -1) {
                        event.preventDefault();
                        return false;
                    }

                    var redirect_url;

                    switch (code) {
                        case 'ak':
                            redirect_url = '/alaska/'
                            break;
                        case 'az':
                            redirect_url = '/arizona/'
                            break;
                        case 'ca':
                            redirect_url = '/california/'
                            break;
                        case 'co':
                            redirect_url = 'https://www.coloradopotguide.com'
                            break;
                        case 'fl':
                            redirect_url = '/florida/'
                            break;
                        case 'hi':
                            redirect_url = '/hawaii/'
                            break;
                        case 'il':
                            redirect_url = '/illinois/'
                            break;
                        case 'me':
                            redirect_url = '/maine/'
                            break;
                        case 'ma':
                            redirect_url = '/massachusetts/'
                            break;                      
                        case 'mi':
                            redirect_url = '/michigan/'
                            break;
                        case 'nv':
                            redirect_url = '/nevada/'
                            break;
                        case 'nm':
                            redirect_url = '/new-mexico/'
                            break;
                        case 'or':
                            redirect_url = '/oregon/'
                            break;
                        case 'wa':
                            redirect_url = '/washington/'
                            break;
                    }
                    window.location = redirect_url;
                }
            });
        });
    </script>


