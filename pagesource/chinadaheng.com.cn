<script type="text/javascript">

$('div[name="city"]').inputbox({
		height:29,
		width:254
	});

$(function(){
	var glen = $("#gundiv ul li").length;
	$("#gundiv ul").css("width",209 * (glen));
	
});

$("#zsgun").hScrollPane({
	mover:"ul",
	moverW:function(){return $("#zsgun li").length*209-8;}(),
	showArrow:true,
	handleCssAlter:"draghandlealter"
});



$(".fullSlide").hover(function(){
    $(this).find(".prev,.next").stop(true, true).fadeTo("show", 0.5)
},
function(){
    $(this).find(".prev,.next").fadeOut()
});
$(".fullSlide").slide({
    titCell: ".hd ul",
    mainCell: ".bd ul",
    effect: "fold",
    autoPlay: true,
    autoPage: true,
    trigger: "click",
    startFun: function(i) {
        var curLi = jQuery(".fullSlide .bd li").eq(i);
        if ( !! curLi.attr("_src")) {
            curLi.css("background-image", curLi.attr("_src")).removeAttr("_src")
        }
    }
});
</script>
