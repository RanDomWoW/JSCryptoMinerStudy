                mode: 'thumbnails-mobile-a',
                container: 'taboola-mobile-below-article-thumbnails-' + page,
                placement: 'Mobile Below Article Thumbnails',
                target_type: 'mix'
              });
        }
    };

    var load_more = function() {
        $('#more').html("Loading ...").attr('disabled','disabled');;
        $.ajax({
            type: "GET",
            cache: true,
            url: '/sapi/homepages',
            data: { t: 0, p: page},
            success: function (response) {

                append_advert(page);
                append_content(response);
                lazy_load_images();

                if(page >= 9) {
                    $('#more').hide();
                } else {
                    page = page + 1;
                    $('#more').html("Load more").removeAttr('disabled');
                }

