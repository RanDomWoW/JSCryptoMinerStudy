                right_url: items[1].url,
                right_img: items[1].image,
                right_img_alt: items[1].alt_str,
                right_title: items[1].title,
                right_summary: items[1].summary,
                right_tags: items[1].tags,
                right_is_video: items[1].type == 'videos',
                right_created_time_1: items[1].created_time_1,
                right_created_time_2: items[1].created_time_2,
            };

            $(".contentBox").append(template(data));
        }
    };

    var append_advert = function(page) {
        $(".contentBox").append('<div id="taboola-below-article-thumbnails-'+page+'"  class="show-lg show-xl show-md"></div>');
        $(".contentBox").append('<div id="taboola-mobile-below-article-thumbnails-'+page+'" class="show-sm"></div>');

        var width = $(window).width();
        if(width >= 600) {
          window._taboola = window._taboola || [];
          _taboola.push({
            mode: 'thumbnails-v',
            container: 'taboola-below-article-thumbnails-' + page,
            placement: 'Below Article Thumbnails',
            target_type: 'mix'
          });
        } else {
