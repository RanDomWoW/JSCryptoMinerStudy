//必須項目
function  ErrExit(key,name,val){
  if (val!=''){return '' ;}
  var Comment='※' + name + 'を正しく入力してください。';
  SetErr(key,Comment);
}
//タブストップ
function  ErrSPTAB(key,name,val){
  if (val==''){return ;}
  if (!val.match(/($s)+/)){return ;}

  var Comment='※' + name + 'にスペース、タブ、改行は含めないで下さい。';
  SetErr(key,Comment);
}
//文字数
function ErrMAXLEN(key,name,val,len){
  if (val==''){return ;}
  if (val.length <= len){return ;}//>

  var Comment='※' + name + 'は' + len + '文字以内にしてください';
  SetErr(key,Comment);
}
//カナ
function ErrKANA(key,name,val){
  if (val==''){return ;}
  val =hiraganaToKatagana(val);

  $('#' + key).val(val);
  if (val.match(/^[\u30A0-\u30FF]+$/)){return ;}

  var Comment='※' + name + 'はカタカナで入力してださい。';

  SetErr(key,Comment);
}
//数字
function ErrNUM(key,name,val){
  if (val===''){return ;}
  val=toHalfWidth(val);
  $('#' + key).val(val);
 if (val.match(/^[0-9]+$/)){return ;}
  var Comment='※' + name + 'は数字で入力してださい。';
  SetErr(key,Comment);
}
//Email
function ErrEMAIL(key,name,val){
  if (val===''){return ;}
  if (val.match(/^([a-zA-Z0-9])+([a-zA-Z0-9\._-])*@([a-zA-Z0-9_-])+([a-zA-Z0-9\._-]+)+$/)){return ;}
  var Comment='※正しい' + name + 'を入力してください';
  SetErr(key,Comment);
}
//Email確認
function ErrEMAIL2(key,name,val){
