<script>
    
    $(function () {
        $("#inSearch").kendoAutoComplete({
            minLength: 2,
            dataTextField: "id",
            template: $.proxy(kendo.template("<span class='autostyle'> #= formatValue(data.label, this.val()) #</span>"), $("#inSearch")),
            dataSource: {
                serverFiltering: true,
                transport: {
                    read: {
                        url: "/Ajax/AutoComplete.asmx/LookupTokens",
                        type: "POST",
                        dataType: "json",
                        contentType: "application/json; charset=utf-8",
                    },
                    parameterMap: function (data, type) {
                        if (type == "read") {
                            var dat = { term: data.filter.filters[0].value, type: "jobtitlelookup" };
                                return JSON.stringify(dat);
                            }
                        }
                    },
                    schema: {
                        data: "d",
                        model: {
                            id: "label"
                        }
                    }
                }

        }).keyup(function (e) {
            if (e.which === 13) {
                $(".job_search input[type=submit]").click();

            }
        });


    });

    function formatValue(itemText, text) {

        var formatedText = text.trim();
        var textMatcher = new RegExp(formatedText, "ig");

        return itemText.replace(textMatcher, function (match) {
            return "<strong>" + match + "</strong>";
        });

        //'<span class="autostyle">#: data.label #</span>'
    }
</script>
                
