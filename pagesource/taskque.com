<script>
    $(function () {

        var cost = 5;
        var usercount = $('#numofuser').val();

        $('#user_val').text(usercount + ' User');
        $('#cost_val').text('$' + ( usercount * cost) + '/m');

        $('#postVal').on('click', function () {
            var preval = $('#numofuser').val();
            var newValue = Number(preval) + 1;
            $('#numofuser').val(newValue);
            $("#hidden_input").val(newValue);

            usercount = parseInt($("#numofuser").val());
            $('#user_val').text(usercount + ' User');
            console.log($('#cost_val').text().indexOf('y'));
            if ($('#cost_val').text().indexOf('y') > 0) {
                $('#cost_val').text('$' + (usercount * cost) * 12 + '/y');
            } else {
                $('#cost_val').text('$' + (usercount * cost) + '/m');
            }

        });

        //$('body').delegate('#postVal', 'click', function(){
        $('#negVal').on('click', function () {
            if ($('#numofuser').val() > 10) {
                var newValue = Number($('#numofuser').val()) - 1;
                $('#numofuser').val(newValue);
                $("#hidden_input").val(newValue);

                usercount = parseInt($("#numofuser").val());
                $('#user_val').text(usercount + ' User');
                if ($('#cost_val').text().indexOf('y') > 0) {
                    $('#cost_val').text('$' + (usercount * cost) * 12 + '/y');
                } else {
                    $('#cost_val').text('$' + (usercount * cost) + '/m');
                }
            }
        });

        $('#yearly_pricing').on('click', function () {

            usercount = parseInt($("#numofuser").val());
            $('#user_val').text(usercount + ' User');
            $('#cost_val').text('$' + (usercount * cost) * 12 + '/y');
        });

        $('#monthly_pricing').on('click', function () {

            usercount = parseInt($("#numofuser").val());
            $('#user_val').text(usercount + ' User');
            $('#cost_val').text('$' + (usercount * cost) + '/m');

        });
        $("#urnumcontact").keydown(function (e) {
            // Allow: backspace, delete, tab, escape, enter and .
            if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 110, 190]) !== -1 ||
                    // Allow: Ctrl+A, Command+A
                (e.keyCode == 65 && (e.ctrlKey === true || e.metaKey === true)) ||
                    // Allow: home, end, left, right, down, up
                (e.keyCode >= 35 && e.keyCode <= 40)) {
                // let it happen, don't do anything
                return;
            }
            // Ensure that it is a number and stop the keypress
            if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
                e.preventDefault();
            }
        });

        $('.ytvid').on('click',function(){
            var t = $(this).attr('data-ivideo');
            if($(this).attr('href').indexOf('youtube')>=0){
                //$('#watchVid .modal-body').html('<iframe width="560" height="315" src="https://www.youtube-nocookie.com/embed/'+t+'?autoplay=1" frameborder="0" allowfullscreen></iframe>');
                $('#watchVid .modal-body').html('<iframe src="https://www.youtube.com/embed/'+t+'?autoplay=1&rel=0&amp;controls=0&amp;showinfo=0" frameborder="0" allowfullscreen></iframe>');
            }
            if($(this).attr('href').indexOf('vimeo')>=0){
                //$('#watchVid .modal-body').html('<iframe src="https://player.vimeo.com/video/'+t+'?autoplay=1&color=ff9933&title=0&byline=0&portrait=0" width="560" height="315" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>');
                $('#watchVid .modal-body').html('<iframe src="https://player.vimeo.com/video/'+t+'?autoplay=1&color=ff9933&title=0&byline=0&portrait=0" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>');
            }
            $('#watchVid').modal();
            return false;
        });
        $('#watchVid').on('hidden.bs.modal', function (e) {
            $('#watchVid .modal-body').html('');
        });

        $('.ytvid2').on('click',function(){
            $('.fullvidarea').slideDown(300,'',function(){
                var oftop = $('.fullvidarea').offset().top-75;
                $("html,body").animate({scrollTop:oftop},1e3,"",function(){$(this).stop(true,true)});
            });
        });
        
            });

    $('[data-target="#staticSignup"]').on('click', function() {
        var reg_type = $(this).data('registration_type');
        if(reg_type == 'paid') {
            var numofuser = $('#numofuser').val();
            var subs_type = 'month';
            if($('#cost_val').text().indexOf('y') > 0) {
                subs_type = 'year';
            }

            var date = new Date();
            var minutes = 5;
            date.setTime(date.getTime() + (minutes * 60 * 1000));
            $.cookie('numberOfUsers', numofuser, { expires: date });
            $.cookie('subsType', subs_type, { expires: date });
        } else{
            $.removeCookie('numberOfUsers');
            $.removeCookie('subsType');
            $.removeCookie('show_payment_page');
        }
        return true;
    });

    $('[data-show_payment_page="true"]').on('click', function() {
        var date = new Date();
        var minutes = 5;
        date.setTime(date.getTime() + (minutes * 60 * 1000));
        $.cookie('show_payment_page', true, { expires: date });
        return true;
    });

