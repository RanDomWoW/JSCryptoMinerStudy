        <script type="text/javascript"> 
            function findGeoLoc() {
                navigator.geolocation.getCurrentPosition(
                    function(pos) {
                        var geocoder    = new google.maps.Geocoder();
                        var latlng      = new google.maps.LatLng(pos.coords.latitude, pos.coords.longitude);
                        geocoder.geocode({'latLng': latlng}, function(results, status) {
                            var uGeoLoc = '';
                            var glLocality = '';
                            var glPolitical = '';
                            var glInKerala = false;                            
                            if (status == google.maps.GeocoderStatus.OK) { 
                                if ('undefined' != typeof results[1]) { 
                                    $.each(results, function(i, ac) {
                                        if ('locality' == ac.types[0] && '' == glLocality) {
                                            glLocality = ac.address_components[0].long_name;
                                        }
                                        if ('political' == ac.types[0] && '' == glPolitical) {
                                            glPolitical = ac.address_components[0].long_name;
                                        }
                                        if ('administrative_area_level_1' == ac.types[0] &&
                                            'KL' == ac.address_components[0].short_name) {
                                            glInKerala = true;
                                        }
                                    });
                                }
                            }
                            if (glInKerala) {
                                uGeoLoc = ('' != glLocality) ? glLocality : glPolitical;
                            }
                            $('[data-qk-el-name="location"]').val(uGeoLoc);
                            var uglExpDate = new Date();
                            uglExpDate.setTime(uglExpDate.getTime()+(30*24*60*60*1000));
                            document.cookie = "uGeoLoc="+uGeoLoc+";expires="+uglExpDate.toGMTString()+";path=/;domain=.quickerala.com";
                        });
                    },
                    function() {
                        var uglExpDate = new Date();
                        uglExpDate.setTime(uglExpDate.getTime()+(1*24*60*60*1000));
                        document.cookie = "uGeoLoc=;expires="+uglExpDate.toGMTString()+";path=/;domain=.quickerala.com";
                    }                            
                );
            }
            function checkLoc() {
                if (navigator.geolocation && '' == $('[data-qk-el-name="location"]').val()) {
                    var docCookies = document.cookie.split('; ');
                    var dCookies = {};
                    for (dci = docCookies.length-1; dci >= 0; dci--){
                        var dc = docCookies[dci].split('=');
                        dCookies[dc[0]] = dc[1];
                    }
                    if ('undefined' == typeof dCookies['uGeoLoc']) {
                        google.maps.event.addDomListener(window, 'load', findGeoLoc);
                    } else {
                        $('[data-qk-el-name="location"]').val(dCookies['uGeoLoc']);
                    }
                }
            }
            checkLoc();
        </script>

