<script>
       var adsIsInit =  false;
        function getAdsFix() {

        if(adsIsInit)return false;
        adsIsInit = true;

            $('.adfox-banner-background').show();

            $("#content").css({
                 position: "relative",
                zIndex: 12,
                background: "#fff",
                marginTop: 140 
            });

            function setTopAds() {
                var bottom =  ( $("#footer-widgets").offset().top - $(window).scrollTop()-$(window).outerHeight() ) * -1;
                if(bottom > 0){
                     $(".adfox-banner-background").css({
                         top: 'auto',
                         bottom: bottom
                     });
                     return false;
                }

                var top = $(".b-header").outerHeight(true)+$('.b-navigation').outerHeight(true)-$(window).scrollTop();
                top = (top<0) ? 0 : top;
                $(".adfox-banner-background").css("top", top);
            }
            setTopAds();
            $(window).scroll(function(event) {
                setTopAds();
            });

            function sizeBrandingAds() {
                 var tops = { width : 0,height:0},
                left        = { width : 0,height:0},
                right       = { width : 0,height:0,left:0};

                left.width = right.width = ($("body").outerWidth(true)-$("#content").outerWidth(false)) /2;
                left.height = right.height = $("#content").outerHeight(true);
                tops.height = parseInt($("#content").css("marginTop") )+10;
                tops.width  = $("body").outerWidth(true);
                right.left = $("#sidebar").offset().left+$("#sidebar").outerWidth();

                $('.ads-left-abs').css(left);
                $('.ads-top-abs').css(tops);
                $('.ads-right-abs').css(right);
            }
            sizeBrandingAds();
            $(window).resize(function(event) {
              sizeBrandingAds();
            });
        }

        jQuery(document).ready(function($) {
             if($(".adfox-banner-background").length){
                    getAdsFix();
                }
        });
        $(window).load(function() {
              if($(".adfox-banner-background").length){
                    getAdsFix();
                }
        });

    </script>
