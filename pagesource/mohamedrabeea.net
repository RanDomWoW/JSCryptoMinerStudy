    <script>
        if (window.innerWidth <= 500)
        {
            $("#youtube").removeClass("youtube");
            $("#youtube").parent("div").addClass("text-center");
        }
        $(function () {
            $("#btnSearch").click(function (e) {
                e.preventDefault();
                if ($("#t-search").val() == null || $("#t-search").val() === "" || $("#t-search").val() === undefined)
                {
                    alert("يجب ادخال كلمة البحث");
                    return;
                }
                $(".model").show();
                $.ajax({
                    method: "POST",
                    contentType: "application/json; charset=utf-8;",
                    url: "../BLL/BookStore.asmx/SearchTextResult",
                    dataType: "json",
                    data: JSON.stringify({ term: $("#t-search").val() }),
                    success: function (data) {

                        var lst = JSON.parse(data.d);
                        if (lst.length > 0)
                            window.location.href = "List.aspx?bookId=" + lst[0].id;//+ "&i=" + ui.item.id + "&id=" + ui.item.itemid;

                    },
                    complete: function () {
                        $(".model").hide();
                    }
                });

            });

            $("#t-search").on("keypress", function (event) {
                if (event.which == 13 && !event.shiftKey) {
                    event.preventDefault();
                    // $("#submitForm").submit();
                    $("#btnSearch").trigger("click");
                }
            });

            $("#t-search").autocomplete({
                source: function (request, response) {
                    if ($("#t-search").val() == null || $("#t-search").val() === "" || $("#t-search").val() === undefined) {
                        alert("يجب ادخال كلمة البحث");
                        return;
                    }

                    $.ajax({
                        method: "POST",
                        contentType: "application/json; charset=utf-8;",
                        url: "../BLL/BookStore.asmx/SearchTextResult",
                        dataType: "json",
                        data: JSON.stringify({ term: request.term }),
                        success: function (data) {
                           
                            var lst = JSON.parse(data.d);
                            response(lst);
                        }
                    });
                },
                minLength: 3,
                select: function (event, ui) {
                   
                    if (ui.item.link != "") {
                        window.location.href = "bookPage.aspx?bookId=" + ui.item.id;//+ "&i=" + ui.item.id + "&id=" + ui.item.itemid;
                    }
                    else {
                        window.location.href = "List.aspx?bookId=" + ui.item.id;//+ "&i=" + ui.item.id + "&id=" + ui.item.itemid;
                    }
                    return;
                    //alert("Selected: " + ui.item.value + ", ItemCatId: " + ui.item.id);
                    //log("Selected: " + ui.item.value + ", ItemCatId: " + ui.item.id);
                }
            });

        });
    </script>
