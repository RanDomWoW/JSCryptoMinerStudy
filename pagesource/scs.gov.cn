	})

    $('.nav ul li').click(function(){
       var thisId = $(this).attr("id");
       if(isSelect != thisId){	
       $("#"+isSelect).removeClass("nav_h1");
	   $("#"+isSelect).removeClass("nav_h");
       $(this).addClass("nav_h1");	
       isSelect = thisId;
    }
})
});
