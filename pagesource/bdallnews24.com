    <script type="text/javaSCRIPT">
		var headlineCount = 4;
		var timerId=0;
		var mouseover=false;
		startTimer('0');
		
		function startTimer(count){
			if(!mouseover){
				for(var i=0;i<headlineCount;i++){
					if(i==count){
						document.getElementById('headlineNewsTitle'+i).style.display='block';
						document.getElementById('headlineNewsPicture'+i).style.display='block';
					}else{
						document.getElementById('headlineNewsTitle'+i).style.display='none';
						document.getElementById('headlineNewsPicture'+i).style.display='none';
					}
				}
			}
			//alert(count);
			count++;
			if(count>headlineCount-1){
				count=0;
			}
			//alert(count);
			timerId=setTimeout("startTimer('"+count+"')",8000);		
		}
		
		function showManset(count){
			mouseover=true;
			//alert(count);
			for(var i=0;i<headlineCount;i++){
				if(i==count){
					document.getElementById('headlineNewsTitle'+i).style.display='block';
					document.getElementById('headlineNewsPicture'+i).style.display='block';
				}else{
					document.getElementById('headlineNewsTitle'+i).style.display='none';
					document.getElementById('headlineNewsPicture'+i).style.display='none';
				}	
			}		
		}
		function mouseOut(){
			mouseover=false;				
		}
		function mouseOver(){
			mouseover=true;				
		}
	
		$(".newsticker-jcarousellite").jCarouselLite({
			btnNext: ".scrollingUp",
			btnPrev: ".scrollingDown",
			circular: false,
			vertical: true,
			hoverPause:true,
			visible:5,
			speed:5000,
		});
	</script>
