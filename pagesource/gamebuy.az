<script type="text/javascript">
	$(function(){

		$( "input" ).keyup(function(){                  
                    providerName = $(this).val();
                    searchType = $(this).attr("id");
                    providerShow(providerName,searchType);
                }).click(function(){
                	providerName = $(this).val();
                	searchType = $(this).attr("id");
                	providerShow(providerName,searchType);
                });

		function providerShow (providerName,searchType){
		pageGet = "https://gamebuy.az/ajax/ajaxPayment.php";
		
		
			if(typeof providerName == 'undefined') {
				providerName = '';
			}
			if(typeof searchType == 'undefined') {
				pageType = 'onlinePaymentGame';
			}else{
				pageType = searchType;
			}

		$.ajax({

					type: "GET",
					url:pageGet,
					data: {"type":pageType,"name":providerName},
					dataType: "json",
					success: function(data){
					$('ul.onlinePaymentProviderMainBox').html(data.provider);
					//$('.searchBarOnlinePayment__input').attr("id",pageType);
				},error: function(){
						alert("Xeta1");
				}});
		}

		providerShow();
		$(document).on("click","a[rel='load']",function(e){

				pageGet = "https://gamebuy.az/ajax/ajaxPayment.php";
				pageurl = "https://gamebuy.az/"+$(this).attr("href")+".html";
				pageType = $(this).attr("href");

				$.ajax({
					type: "GET",
					url:pageGet,
					data: {"type":pageType},
					dataType: "json",
					success: function(data){
					$('ul.onlinePaymentMenu').html(data.menu);
					$('ul.onlinePaymentProviderMainBox').html(data.provider);
					$('.searchBarOnlinePayment__input').attr("id",pageType);
					if(pageType == "onlinePaymentEwallet"){
						title='Elektron pul qabına və ya Elektron pul kisəsinə ödəniş etmək';
					}else if(pageType== "onlinePaymentSocial"){
						title='Sosial şəbəkələrə  ödəniş etmək';	
					}else if(pageType == "onlinePaymentGame"){
						title='Online oyunlara ödəniş etmək';	
					}else if(pageType == "onlinePaymentGiftCard"){
						title='Gift Kart və ya Hədiyyə kart almaq';	
					}

					$('title').html(title);
				},error: function(){
						alert("Xeta2");
				}});		

				if(pageurl!=window.location){
					window.history.pushState({path:pageurl},'',pageurl);	

				}

				return false;  

			});

		});

		/*$(window).bind('popstate', function() {

			$.ajax({url:location.pathname,success: function(data){	
					$('ul.onlinePaymentMenu').html(data);
			}});

		});*/
		$(window).on('popstate', function(){ 
			var path = location.pathname.replace('/','').replace('.html','');
			pageGet = "https://gamebuy.az/ajax/ajaxPayment.php";
			$.ajax({
					type: "GET",
					url:pageGet,
					data: {"type":path},
					dataType: "json",
					success: function(data){
					$('ul.onlinePaymentMenu').html(data.menu);
					$('ul.onlinePaymentProviderMainBox').html(data.provider);
					$('.searchBarOnlinePayment__input').attr("id",path);
					if(path == "onlinePaymentEwallet"){
						title='Elektron pul qabına və ya Elektron pul kisəsinə ödəniş etmək';
					}else if(path== "onlinePaymentSocial"){
						title='Sosial şəbəkələrə  ödəniş etmək';	
					}else if(path == "onlinePaymentGame"){
						title='Online oyunlara ödəniş etmək';	
					}else if(path == "onlinePaymentGiftCard"){
						title='Gift Kart və ya Hədiyyə kart almaq';	
					}

					$('title').html(title);
				},error: function(){
						alert("Xeta3");
				}});		



		});
</script>
 

