<script language="javascript">                     //鼠标切换
function GetObj(objName){if(document.getElementById){
 return eval('document.getElementById("'+objName+'")')}else{return eval('document.all.'+objName)}} 
function showTab(group,n,max){
	for(i=1;i<=max;i++){GetObj("ttab_"+group+"_"+ i).style.display = 'none';}     //max代表具有几个切换
	GetObj("ttab_"+group+"_"+ n).style.display = 'block';
}
function switchTab(id,num,current){                                                  //num代表具有几个切换
	for(i=1;i<=num;i++){
		GetObj(id+""+ i).className = 'off';	
		GetObj(id+"-div"+ i).style.display = 'none';	
	}
	GetObj(id+""+ current).className = 'on';
	GetObj(id+"-div"+ current).style.display = 'block';
}
</script>
 <script type="text/javascript">
	function rollCon(obj){
		//初始化操作
		obj.con2.innerHTML=obj.con1.innerHTML;
		this.con=obj.con;
		this.height=obj.con1.offsetHeight;
		this.timer;
		this.speed=30;
		var _this=this;
		this.con.onmouseover=function(){clearInterval(_this.timer);}
		this.con.onmouseout=function(){_this.start();}
	}
	rollCon.prototype={
		start:function(){
			var _this=this;
			this.timer=setInterval(function(){_this.run()},this.speed);
		},
		run:function(){
			if(this.con.scrollTop>=this.height){
				this.con.scrollTop-=this.height;
			}else 
				this.con.scrollTop++;
		}
	}
	function turn_tops(){
		var obj_turn_header=$('#turn_header span');
		var m=n=0;
		for(var i=0;i<obj_turn_header.length;i++){
			if($(obj_turn_header[i]).css('display')=='block'){
				m=i;
			}
		}
		if(m<(obj_turn_header.length-1)){
			n=m+1;
		}else{
			n=0;
		}
		for(var j=0;j<obj_turn_header.length;j++){
			$(obj_turn_header[j]).css('display','none');
		}
		$(obj_turn_header[n]).css('display','block');
	}
	setInterval(turn_tops,2000);
</script>
