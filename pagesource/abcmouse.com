ABCmouseUtils.getPosition = function(width, height, stylePosition) {
	var pageSize = ABCmouseUtils.getPageSize();
	var position = {x: 0, y: 0};
	
	position.x = Math.round((pageSize.width - width)/2);
	position.y = Math.round((pageSize.height - height)/2);

	if(stylePosition=='absolute'){
		var myOffset = ABCmouseUtils.getScrollXY();
		position.y += myOffset.y;
	}
	if(position.y < 0){
		position.y = 0;
	}
		
	if(position.x < 0)
		position.x = 0;
	
	return position;
};

ABCmouseUtils.getPageSize = function() {
	var width = 0
	var height = 0;
	//This is used to adjust to the android scale
	var contentScale = document.getElementsByName("contentScale")[0] || {value:1};

	/*	Was getting value "1,1" on desktop,
		so manually setting this if not mobile
		[Mark Mutti, Sept 15 '14, Task #110503]
	*/
	if(ABCmouseUtils.isMobile==true){
		var scale = contentScale.value;
	}else{
		var scale = 1;
	}
	
	var app_pattern = /ela_app.+iphone/i;
	
	if(app_pattern.test(navigator.userAgent) || /iphone.+ela_app/i.test(navigator.userAgent)) { //usin app on iphone
		//return {width: 1060, height: 770}; //fix for the iphone
	}else{
		scale = 1;
	}
	
	if(window.innerWidth == null) {
		width = document.documentElement.clientWidth;
		height = document.documentElement.clientHeight;
	} else {
		width = window.innerWidth;
		height = window.innerHeight;
	}
    
	return {width: Math.round(width/scale), height: Math.round(height/scale)};
};

ABCmouseUtils.getScrollXY = function() {
	var scrOfX = 0, scrOfY = 0;
	//This is used to adjust to the android scale
	var contentScale = document.getElementsByName("contentScale")[0] || {value:1};
	var scale = contentScale.value;

	if( typeof( window.pageYOffset ) == 'number' ) {
		//Netscape compliant
		scrOfY = window.pageYOffset;
		scrOfX = window.pageXOffset;
	} else if( document.body && ( document.body.scrollLeft || document.body.scrollTop ) ) {
		//DOM compliant
		scrOfY = document.body.scrollTop;
		scrOfX = document.body.scrollLeft;
	} else if( document.documentElement && ( document.documentElement.scrollLeft || document.documentElement.scrollTop ) ) {
		//IE6 standards compliant mode
		scrOfY = document.documentElement.scrollTop;
		scrOfX = document.documentElement.scrollLeft;
	}

	return {x: Math.round(scrOfX/scale), y: Math.round(scrOfY/scale)};
};

