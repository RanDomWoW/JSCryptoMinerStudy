ABCmouseUtils.getFormElementValue = function(formElement) {
	formElement = (typeof(formElement) == "string") ? document.getElementById(formElement) : formElement;
	
	if(formElement == null) {
		return false;
	}

	switch(formElement.type) {
		case "text":
		case "email":
		case "number":
		case "tel":
		case "textarea":
