	videoSource = srcString + newFile;
	if(isIE8 == -1 && videoDiv.canPlayType && videoDiv.canPlayType('video/mp4')) {
		videoSource = videoSource;
		videoplayer.video_file_name = newFile;
	}
	else if(isIE8 != -1) {
		videoSource = videoSource;
		videoplayer.video_file_name = newFile;
	}
	else {
		videoSource = videoSource.replace('.mp4','.ogv');
