				if(button.closeButton === true) {
					self.close();
				} else if(typeof button.onclick == "function") {
					button.onclick(self);
				}
			};
		}(button));

        var dataset = button.dataset || {};
        for (property in dataset) {
            if (dataset.hasOwnProperty(property)) popup_button.setAttribute("data-" + property, dataset[property]);
        }

		popup_buttons.appendChild(popup_button);
