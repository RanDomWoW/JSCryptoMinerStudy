<script type="text/javascript">
    $(document).ready(function () {
        var switched = 0, agent_type = '',
            $switchMode = $('#switchMode');
        if (Cookies) {
            switched = Cookies.get('switched_to_pc');
            agent_type = Cookies.get('agent_type');

            if (switched === undefined) {
                switch (agent_type) {
                    case 'desktop':
                        switched = 1;
                        break;
                    case 'tablet':
                        switched = 2;
                        break;
                }
            }
        }

        if (switched) {
            switch (agent_type) {
                case 'tablet':
                    $switchMode.show();
                    break;
                case 'smp':
                    $switchMode.show();
                    break;
                default:
                    $switchMode.remove();
            }
        } else {
            $switchMode.remove();
        }
    });
</script>    
