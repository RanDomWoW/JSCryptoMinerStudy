$('body').on('click', '.EURO_loader', function(){
    
   // console.log('EURO_loader');
	var locID 	= $(this).attr('data-locID'),
		offset 	= $('.content_bloc').attr('data-offset');

	offset++;
	
	$('.content_bloc').attr('data-offset', offset);
	$('.EURO_loader').html('<i class="fa fa-spinner fa-spin"></i>');
	
	$.ajax({
		url: '/euro2016_assets/assets/includes/ajax/EURO_article_loader.php', 
		data: {'locID': locID, 'offset': offset}
	}).done(function(data){

		if (data){
			$('.content_bloc').append(data);
			$('.focuspoint').focusPoint();
			$('.EURO_loader').html('Load More');
		}
		else {
			$('.EURO_loader').hide();
		}

		var liveStreamPlaying = 'No';
		
		if(jwplayer("player").getState()=="PLAYING" && window.curPlayType=='On Air'){				  
              liveStreamPlaying = 'Yes';              
        }
        else{
        	  liveStreamPlaying = 'No';
        }

		ga_set_content_groups(window.gaCategory, window.gaParent_category, liveStreamPlaying);
		ga('send', 'pageview', window.gaPage+'/page-'+offset+'/');
	});
});


