		<div class="card-block">
			<div class="card-author"></div>
			<h3 class='card-title text-capitalize title-font'><a data-behaviour='sjax' href='{{data.link}}'>{{{data.title.rendered}}}</a></h3>
		</div>
	</div>

</script>
<script id="tmpl-post-avatar" type="text/html">
	<# if( data.authordata ){#>
