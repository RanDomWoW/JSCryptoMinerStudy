  <!-- /snippets/ajax-cart-template.liquid -->

  <script id="CartTemplate" type="text/template">
  
    <form action="/cart" method="post" novalidate class="cart ajaxcart">
      <div class="products-container">
        {{#items}}
        <div class="product">
          <div class="image-container">
            <a href="{{url}}"><img src="{{img}}" alt=""></a>
          </div>
          <div class="meta-container">
            <div class="product-info">
              <h1>{{styleName}}{{#if polarized}}<span class="polarized">{{polarized}}</span>{{/if}}</h1>
              <h2>{{frameColor}}{{#if lensColor}} + {{lensColor}}{{/if}}</h2>
            </div>
            <div class="product-form">
              <div class="qty-adjust">
                <input type="text" name="updates[]" class="ajaxcart__qty-num" value="{{itemQty}}" min="0" data-id="{{key}}" data-line="{{line}}" aria-label="quantity" pattern="[0-9]*">
                <button type="button" class="ajaxcart__qty-adjust ajaxcart__qty--plus" data-id="{{key}}" data-line="{{line}}" data-qty="{{itemAdd}}">
                  <i class="nav-arrow"></i>
                </button>
                <button type="button" class="ajaxcart__qty-adjust ajaxcart__qty--minus" data-id="{{key}}" data-qty="{{itemMinus}}" data-line="{{line}}">
                  <i class="nav-arrow"></i>
                </button>
              </div>
              {{#if discountsApplied }}
                <small class="ajaxcart-item__price-strikethrough"><s>${{originalLinePrice}}</s></small>
                <span class="product-price"><span class="dollar-sign">$</span>{{linePrice}}</span>
              {{else}}
             	<span class="product-price"><span class="dollar-sign">$</span>{{linePrice}}</span>
              {{/if}}
            </div>
          </div>
        </div>
        {{/items}}
      </div>
      <div class="ajaxcart__footer">
        <div class="subtotal-container">
          <label>Subtotal:</label>&nbsp;<span class="subtotal"><span class="dollar-sign">$</span>{{totalPrice}}</span>
        </div>
        {{#if totalCartDiscount}}
          <div class="discount-container">
            <p class="ajaxcart__savings text-center"><em>{{totalCartDiscount}}</em></p>
          </div>
        {{/if}}
        <div class="shipping-container">
          free shipping $50+ & easy returns!<br>
          <span>taxes calculated at checkout</span>
        </div>
        <button type="submit" class="btn cart__checkout" name="checkout">
          Proceed to checkout
        </button>
      </div>
    </form>
  
  </script>
  <script id="AjaxQty" type="text/template">
  
    <div class="ajaxcart__qty">
      <button type="button" class="ajaxcart__qty-adjust ajaxcart__qty--minus" data-id="{{key}}" data-qty="{{itemMinus}}">
        <span aria-hidden="true">&minus;</span>
      </button>
      <input type="text" class="ajaxcart__qty-num" value="{{itemQty}}" min="0" data-id="{{key}}" aria-label="quantity" pattern="[0-9]*">
      <button type="button" class="ajaxcart__qty-adjust ajaxcart__qty--plus" data-id="{{key}}" data-qty="{{itemAdd}}">
        <span aria-hidden="true">+</span>
      </button>
    </div>
  
  </script>
  <script id="JsQty" type="text/template">
  
    <div class="js-qty">
      <button type="button" class="js-qty__adjust js-qty__adjust--minus" data-id="{{key}}" data-qty="{{itemMinus}}">
        <span aria-hidden="true">&minus;</span>
      </button>
      <input type="text" class="js-qty__num" value="{{itemQty}}" min="1" data-id="{{key}}" aria-label="quantity" pattern="[0-9]*" name="{{inputName}}" id="{{inputId}}">
      <button type="button" class="js-qty__adjust js-qty__adjust--plus" data-id="{{key}}" data-qty="{{itemAdd}}">
        <span aria-hidden="true">+</span>
      </button>
    </div>
  
  </script>


