      <script>
          $(document).ready(function () {
              function paidScroller() {
                  var win_no = 6;
                  if($( document ).width()<=425){
                    var win_no = 2;
                  } else if($( document ).width()<=550){
                    var win_no = 3;
                  } else if($( document ).width()<=650){
                    var win_no = 4;
                  } else if($( document ).width()<=825){
                    var win_no = 5;
                  }
                  //var psX = 0;
                  var psPos = $("#pcScroll ul").position();
                  var psX = psPos.left;
                  var pswC = $("#pcScroll ul li").length;
                  var pswW = $("#winnersPanel").width() / win_no;
                  $("#pcScroll ul li").width(pswW + 'px');
                  //var pswW = 168;
                  var psW = pswC * pswW;
                  $("#pcScroll ul").width((psW * 2) + 'px');

                  // duplicate html
                  var wins = $("#pcScroll ul").html();
                  $("#pcScroll ul").html(wins + wins);

                  setInterval(function () {
                      //alert("pswC="+pswC+", pswW="+pswW+", pswW="+psW+", psX="+psX);
                      $("#pcScroll ul").animate({
                          left: "-=" + pswW,
                      }, 1000, function () {
                          // Animation complete.
                          var psPos = $("#pcScroll ul").position();
                          var psX = psPos.left;
                          //alert("pswC="+pswC+", pswW="+pswW+", pswW="+psW+", psX="+psX);
                          if (psX <= (psW * -1)) {
                              $("#pcScroll ul").css({"left": "0px"});
                              //alert("reset -> pswC="+pswC+", pswW="+pswW+", pswW="+psW+", psX="+psX);
                          }
                      });
                  }, 3500);
              }
              paidScroller();
          });
      </script>
