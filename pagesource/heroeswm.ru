   for (i = 0; i < 4; i++)
   {
      t = sc + i;
      if (t >= 9) t -= 8;
      document.getElementById("th"+(i+1)).src = "http://dcdn2.heroeswm.ru/i/index2012/screenshots/_thumbs/"+t+".jpg";
   }
}

function clearLogin()
{
   if (log.lreseted.value == 0)
