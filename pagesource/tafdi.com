<script data-cfasync="false" src="/cdn-cgi/scripts/d07b1474/cloudflare-static/email-decode.min.js"></script><script>
    var url = 'https://tafdi.com';

    $.ajaxSetup({
        headers: {'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')}
    });

    $(function () {
        var social_sticky = $('.social-sticky');
        var social_sticky_close = $('.social-sticky i');

        if (!Cookies.get('social-sticky')) {
            social_sticky.show();
        }

        social_sticky_close.click(function (e) {
            e.preventDefault();
            social_sticky.hide();
            Cookies.set('social-sticky', '1', {expires: 1});
        });

        $('body').css('padding-bottom', $('.footer').height() + 20);

        $('[data-toggle="tooltip"]').tooltip();

        $('body').on('focus', '.comment-write textarea', function () {
            $(this).addClass('focus');
        });

        $('body').on('click', '#imdb_id_update', function (e) {
            e.preventDefault();

            $.get("/api/imdb/" + $("#imdb_id").val(), function (data) {

                if (data.available === 'true' && window.location.href.indexOf('titles/create') != -1) {
                    window.location.href = window.location.href.replace('create', 'edit') + '/' + data.id;
                } else {
                    if (data.response === 'false') {
                        $("#imdb_id").closest('div').addClass('has-error');
                        $(".poster img").attr('src', '/images/posters/no_poster.jpg');
                    } else {
                        $("#imdb_id").closest('div').removeClass('has-error');
                    }

                    $(".poster img").attr('src', '/images/posters/' + data.poster);
                    $("#type_id").val(data.type_id);
                    $("#title").val(data.title);
                    ($("#plot").val()) ? '' : $("#plot").val(data.plot);
                    $("#directors").val(data.directors);
                    $("#writers").val(data.writers);
                    $("#actors").val(data.actors);
                    $("#title_languages").val(data.languages);
                    $("#countries").val(data.countries);
                    $("#poster").val(data.poster);
                    $("#genres").val(data.genres);
                    $("#year").val(data.year);
                    $("#imdb_rating").val(data.imdb_rating);
                    $("#time").val(data.time);
                    $("#series_id").val(data.series_id);
                    $("#season").val(data.season);
                    $("#episode").val(data.episode);
                    $("#released").val(data.released);
                    $("#imdb_votes").val(data.imdb_votes);

                    if (data.type == 'episode') {
                        $("#series_id").attr('disabled', false);
                        $("#season").attr('disabled', false);
                        $("#episode").attr('disabled', false);
                    } else {
                        $("#series_id").attr('disabled');
                        $("#season").attr('disabled');
                        $("#episode").attr('disabled');
                    }
                }
            });
        });

        $('body').on('focusout', '#poster', function (e) {
            if ($(this).val().indexOf('http') == -1)
                $(".poster img").attr('src', '/images/posters/' + $(this).val());
            else
                $(".poster img").attr('src', $(this).val());
        });

        $('body').on('click', '.movie-buttons a.event', function (e) {
            e.preventDefault();

            request = {
                _token: $('meta[name="csrf-token"]').attr('content'),
                title_id: $(this).data('id'),
                event_type: $(this).data('type'),
                seek: $("#video_embed").length ? jwplayer("video_embed").getPosition() : 0
            };

            $.post("/api/event", request, function (data) {
                if (data.response === 'false') {
                    alert(data['error']);
                } else {
                    for (var event in data.events) {
                        if (data.events[event] == 1) {
                            $('.movie-buttons .' + event).addClass('active');
                            $('.movie-buttons .' + event + ' small span').text(parseInt($('.movie-buttons .' + event + ' small span').text()) + 1);
                        } else {
                            $('.movie-buttons .' + event).removeClass('active');
                            $('.movie-buttons .' + event + ' small span').text(parseInt($('.movie-buttons .' + event + ' small span').text()) - 1);
                        }
                    }
                }
            });
        });

        var pauseInterval = setInterval(function () {
            if ($('.movie-buttons a.pause').hasClass('active') && jwplayer("video_embed").getPosition() > 0) {

                request = {
                    _token: $('meta[name="csrf-token"]').attr('content'),
                    seek: jwplayer("video_embed").getPosition(),
                    title_id: $('.movie-buttons a.pause').data('id')
                };

                $.post("/api/pause", request, function (data) {
                    if (data.response === 'false') {
                        clearInterval(pauseInterval);
                    }
                });
            }
        }, 5000);

        $('body').on('click', '.movie-buttons a.play', function (e) {
            e.preventDefault();
            jwplayer("video_embed").seek($(this).data('seek'));
            $(this).hide();
        });

        $('body').on('change', '.movie-search-form select', function (e) {
            $('.movie-search-form').submit();
        });

        $('body').on('click', '.comment .reply', function (e) {
            e.preventDefault();
            $this = $(this);

            $.get("/api/auth", function (data) {
                if (data.response === 'false') {
                    html = '<div class="comment-write-login"><span>Yorum yazmak için <a href="' + url + '/login">giriş</a> yapın veya <a href="' + url + '/register">kayıt</a> olun!</span></div>';
                } else {
                    html = '<div class="input-group comment-write">';
                    html += '<textarea class="form-control" placeholder="yeni bir cevap yaz.."';
                    html += 'type="text"></textarea>';
                    html += '<span class="input-group-addon">';
                    html += '<a class="share" href="#" data-parent-id="' + $this.data('comment-id') + '" data-title-id="' + $this.data('title-id') + '">' + '<i class="fa fa-share"></i></a>';
                    html += '</span>';
                    html += '</div>';
                }

                $this.closest('.comment-body').append(html);
                $this.hide();
            });
        });

        $('body').on('click', '.comment-write .share', function (e) {
            e.preventDefault();
            var $this = $(this);

            request = {
                _token: $('meta[name="csrf-token"]').attr('content'),
                parent_id: $(this).data('parent-id'),
                title_id: $(this).data('title-id'),
                text: $(this).closest('.comment-write').find('textarea').val()
            };

            $.post("/api/comment", request, function (data) {
                if (data.response === 'true') {
                    if (request.parent_id) {
                        $this.closest('.comment').append(data.html);
                        $('.comment-write textarea').val('');
                        $('.comment-write').hide();
                    } else {
                        $('.comments').prepend(data.html);
                        $('.comment-write textarea').val('');
                        $('.comment-write').hide();
                    }
                }
            });
        });

        $('body').on('keydown', '.comment-write textarea', function (event) {
            if (event.keyCode == 13) {
                $(this).closest('.comment-write').find('.share').trigger('click');
            }
        });

        $('body').on('click', '.comment .like, .comment .unlike', function (e) {
            e.preventDefault();
            var $this = $(this);

            request = {
                _token: $('meta[name="csrf-token"]').attr('content'),
                comment_id: $this.data('comment-id'),
                type: $this.data('type')
            };

            $.post("/api/rating", request, function (data) {
                if (data.response === 'true') {
                    for (var rating in data.ratings) {
                        if (data.ratings[rating] == 1) {
                            $this.closest('.comment-heading').find('.' + rating + ' span').text(parseInt($this.closest('.comment-heading').find('.' + rating + ' span').text()) + 1);
                        } else {
                            $this.closest('.comment-heading').find('.' + rating + ' span').text(parseInt($this.closest('.comment-heading').find('.' + rating + ' span').text()) - 1);
                        }
                    }
                } else {
                    alert('Lütfen üye girişi yapın veya kayıt olun!')
                }
            });
        });

        $('body').on('click', '.log-remove', function (e) {
            e.preventDefault();
            var $this = $(this);

            request = {
                _token: $('meta[name="csrf-token"]').attr('content'),
                log_id: $this.data('id')
            };

            $.post("/api/log/remove", request, function (data) {
                if (data.response === 'true') {
                    $this.closest('tr').remove();
                } else {
                    alert('Bir şeyler ters gitti!')
                }
            });
        });

        $('body').on('submit', '#edit_account_form', function (e) {
            e.preventDefault();
            var $this = $(this);

            var form = $('#edit_account_form')[0]; // You need to use standart javascript object here
            var formData = new FormData(form);

            $.ajax({
                url: '/u',
                data: formData,
                processData: false,
                contentType: false,
                type: 'POST',
                success: function (data) {
                    if (data.response === true) {
                        location.reload();
                    } else {
                        $('#edit_account_form label').each(function (index) {
                            var form_group = $(this).closest('.form-group');
                            form_group.removeClass('has-error');
                            form_group.find('.help-block').hide();
                            for (var error in data.errors) {
                                if ($(this).attr('for') == error) {
                                    var html = '<p class="help-block">' + data.errors[error] + '</p>';
                                    form_group.addClass('has-error');
                                    form_group.append(html)
                                }
                            }
                        });
                    }
                }
            });
        });

        $(".embed-responsive").mouseover(function () {
            $(this).find('.download-links').show();
        }).mouseout(function () {
            $(this).find('.download-links').hide();
        });
    })
</script>

