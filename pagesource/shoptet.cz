                    <script>
                        $(document).ready(function() {
                            var placeholder = 'Zadejte pouze váš e-mail';

                            $('#mainEmailInput').bind('focus', function() {
                                setTimeout(function() {
                                    document.getElementById('mainEmailInput').setSelectionRange(0, 0);
                                }, 0);
                                if ($(this).val() === '') {
                                    $(this).val(placeholder);
                                }
                            });

                            $("#mainEmailInput").keydown(function() {
                                if (this.value == placeholder) {
                                    this.value = '';
                                }
                            }).blur(function() {
                                if (this.value == '') {
                                    this.value = placeholder;
                                }
                            });

                            $('#mainEmailInput').focus();

                        });
                    </script>
