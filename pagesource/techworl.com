  <script>
function validateForm() {
    var name = document.forms["registration_form_name"]["cname"].value;
  if (name == null || name == "") {
        //alert("Please fill the name");
    document.registration_form_name.cname.focus();
    document.getElementById("cname").style.borderColor="red";
    return false;
    }
  if(!isNaN(name)){
    //alert("Please fill the name with characters");
    document.registration_form_name.cname.focus();
    document.getElementById("cname").style.borderColor="red";
    return false;
  }
  if (name != null && isNaN(name)){
    document.getElementById("cname").style.borderColor="";
  }
  var email = document.forms["registration_form_name"]["email"].value;
  if (email == null || email == "") {
        //alert("Please Fill the Email");
    document.registration_form_name.email.focus();
    document.getElementById("email").style.borderColor="red";
    return false;
    }
  var emailfilter=/^\w+[\+\.\w-]*@([\w-]+\.)*\w+[\w-]*\.([a-z]{2,4}|\d+)$/i
  var b=emailfilter.test(email);
  if(b==false)
  {
  document.getElementById("email").style.borderColor="red";
  document.registration_form_name.email.focus();
  return false;
  }
  if(email != null && b==true){
  document.getElementById("email").style.borderColor="";
  }
  
  var ph = document.forms["registration_form_name"]["phone"].value;
  if (ph == null || ph == "") {
        //alert("Please Fill the phone number");
    document.getElementById("phone").style.borderColor="red";
    document.registration_form_name.phone.focus();
    return false;
    }
  if(isNaN(ph)){
    //alert("Please fill the phone with Numbers");
    document.getElementById("phone").style.borderColor="red";
    document.registration_form_name.phone.focus();
    return false;
  }
  if(ph != "" && !isNaN(ph)){
    document.getElementById("phone").style.borderColor="";
  }
}
</script>


      
