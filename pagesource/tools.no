<script type="text/ng-template" id="/AngularTemplate/Sorting">
 


<section class="sorting__content">
    <article class="delta--regular tl">
        <span class="epsilon uc mr- is-hidden--max-xxs">Sortering</span>
        <tools-dropdown options="{{options}}"
                        view-value-property="DisplayName" model-value-property="Id"
                        placeholder="Velg sortering" is-in-edit-mode="true"
                        ng-model="selectedSortOption.Id" is-read-only="true"
                        class="dropdown__hover fixed-size--10rem dropItem ml0">
        </tools-dropdown>
    </article>
</section>
</script>

<script type="text/ng-template" id="/AngularTemplate/Filtering">


<section class="category-toolbar__item is-hidden--md">
    <article class="is-hidden is-visible--xl">
        <h2 class="category-toolbar__title  epsilon uc">Filter</h2>
        <ul class="category-toolbar__items ml">
            <li><a class="epsilon--regular category-toolbar__action" href data-ng-click="showFilter()" ng-class="{ 'category-toolbar__action--active': ngModel }">Vis</a></li>
            <li><a class="epsilon--regular category-toolbar__action" href data-ng-click="hideFilter()" ng-class="{ 'category-toolbar__action--active': !ngModel }">Skjule</a></li>
        </ul>
    </article>
</section>
</script>

<script type="text/ng-template" id="/AngularTemplate/Dropdown">


<div class="dropdown {{extraClassForHolder}}">
    <section class="dropdown__trigger  {{extraClass}}" ng-mousedown="onDropdownMousedown()" ng-click="toggleState(); $event.stopPropagation(); $event.preventDefault()"
             ng-class="{ 'is-active': isOpen.active, 'dropdown__trigger--disabled': isDisabled || isDropdownDisabledByParent() }" 
             ng-show="isInEditMode" ng-switch="state">
        <span ng-show="state != 'open' || readOnly" ng-bind="selectedItemViewValue || placeholder"></span>
        <input type="text" class="u-1/1 dropdown__filter" 
               ng-show="state == 'open' && !readOnly" ng-model="filteringPhrase" placeholder="{{ selectedItemViewValue || placeholder }}" 
               ng-keyup="{esc: 'cancelFilter($event)'}" ng-keydown="onInputKeyDown($event)" ng-click="$event.stopPropagation()" />
        <i class="dropdown__trigger-ico">
            <?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="18px" height="11px" viewBox="0 0 18 11" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
    <defs></defs>
    <g id="Page-1" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
        <path d="M11.6742036,5.83851388 L4.14977084,-1.59780821 C3.95084794,-1.79440174 3.95011035,-2.11544204 4.15001324,-2.31300408 L4.69513592,-2.8517434 C4.89507896,-3.04934512 5.21903691,-3.0494127 5.41880593,-2.85198296 L13.85056,5.48103572 C13.9499812,5.57929276 13.9998962,5.70863634 13.9999984,5.8381134 C14.0003293,5.96798319 13.9504041,6.09731705 13.85056,6.19599203 L5.41880593,14.5290107 C5.21988302,14.7256042 4.89503881,14.7263332 4.69513592,14.5287712 L4.15001324,13.9900318 C3.95007021,13.7924301 3.95000183,13.4722657 4.14977084,13.274836 L11.6742036,5.83851388 L11.6742036,5.83851388 Z" id="angle-down" fill="#FFFFFF" transform="translate(9.000000, 5.838350) rotate(-270.000000) translate(-9.000000, -5.838350) "></path>
    </g>
</svg>
        </i>
    </section>
    <span ng-show="!isInEditMode" ng-bind="selectedItemViewValue" data-dropdown-value></span>
    <input name="{{inputName}}" ng-value="ngModel" type="hidden"/>
    <div class="dropdown__content {{extraClassForContent}}" ng-class="{'is-open': isOpen.active}" ng-if="!isDropdownDisabledByParent()">
        <ul class="dropdown__list  {{extraClass}}" ng-class="{'is-open': isOpen.active}">
            <li class="dropdown__list-item" ng-mouseover="isMouseOverOption($index)" ng-repeat="option in dropdownOptions track by $index"
                 ng-class="{ 'dropdown__list-item--disabled': isOptionDisabled(option), 'dropdown__list-item--selected': $index === selectedIndex }">
                <a class="{{extraClassForItem}}" href="javascript:void(0)" ng-bind="getViewValue(option)" ng-click="select(option, true)" tabindex="-1"></a>
            </li>
        </ul>
        
    </div>
</div>


</script>

<script type="text/ng-template" id="/AngularTemplate/ModalDialog">


<div class="box">
    <h2 class="beta pv" data-ng-bind="text"></h2>
    <ul class="layout normalized-font">
        <li class="layout__item u-1/2">
            <button class="btn btn--full" data-ng-click="closeThisDialog(item)">
                <span class="delta--regular">Bekreft</span>
            </button>
        </li>
        <li class="layout__item u-1/2">
            <button class="btn btn--secondary btn--full" data-ng-click="closeThisDialog(false)">
                <span class="delta--regular">Avbryt</span>
            </button>
        </li>
    </ul>
    <button aria-label="Dismiss" class="ngdialog-close"></button>
</div>
</script>

<script type="text/ng-template" id="/AngularTemplate/DialogWithDynamicContent">


<div class="popup-cont">
    <div class="close-btn"><span class="icon-cancel" ng-click="closeThisDialog()"></span></div>
    <div bind-compiled-html="ngDialogData.content"></div>
</div>
</script>

<script type="text/ng-template" id="/AngularTemplate/Loader">
    
<div class="loader"></div>

</script>

<script type="text/ng-template" id="/AngularTemplate/ColorList">
    

<div class="color-list" ng-show="ngModel.TermsList.length > 0 && (isViewActive(viewTypes.tiles) || forceVisibility)">
    <ul>
        <li data-ng-repeat="item in ngModel.TermsList track by $index" class="color-list__list-item" ng-click="select(item, true)">
            


<svg xmlns="http://www.w3.org/2000/svg" width="26" height="32" class="color-square pointer"
     ng-class="{ 'color-square--disabled': isOptionDisabled(item) }">

    <rect x="1" y="1" width="23" height="23" ng-style="{'fill': getColor(item.colors[0])}" ng-hide="is2Colors(item)" />

    <polygon points="1,1 1,24 24,1" ng-style="{ 'fill': getColor(item.colors[0]) }" class="ng-hide" ng-show="is2Colors(item)" />
    <polygon points="1,24 24,24 24,1" ng-style="{ 'fill': getColor(item.colors[1]) }" class="ng-hide" ng-show="is2Colors(item)" />

    <line x1="0" y1="31" x2="25" y2="31" class="color-square__underline color-square__underline--high ng-hide"
          ng-show="ngModel === item.value" />

    <rect x="1" y="1" width="23" height="23" fill="none" stroke="#d3d3d3" stroke-width="1" stroke-linecap="square" />
</svg>

        </li>
        <li class="more-colors-count">
            <span ng-hide="!ngModel.moreColorsCount"> +{{ ngModel.moreColorsCount }}&nbsp;mer </span>
        </li>
    </ul>        
</div>

</script>

<script type="text/ng-template" id="/AngularTemplate/Input">
    <label class="delta--regular" for="{{id}}">{{to.label}}</label>
    <input type="{{to.type}}" ng-model="model[options.key]" placeholder="{{to.placeholder}}" class="delta--regular u-1/1"
           ng-class="{'danger': ((form.$submitted || fc.$touched) && fc.$invalid)}" />
</script>

<script type="text/ng-template" id="/AngularTemplate/Checkbox">
    <label class="checkbox {{to.extraClass}}" for="{{id}}">
        <input type="checkbox" class="vam" ng-model="model[options.key]">
        <?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="18px" height="18px" viewBox="0 0 18 18" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
  <g>
      <rect class="outer" stroke-width="1" stroke="#979797" fill="#FFFFFF" x="0" y="0" width="18" height="18" rx="0"></rect>
      <path d="M3,9.83333333 L4.73333333,8.16666667 L7.33333333,10.6666667 L13.2666667,5 L15,6.66666667 L7.33333333,14 L3,9.83333333 Z" class="inner" fill="#FFFFFF"></path>
  </g>
</svg>

        <span class="delta--regular vam">{{to.label}}</span>
    </label>
</script>

<script type="text/ng-template" id="/AngularTemplate/Select">
    <label class="delta--regular" for="{{id}}">{{to.label}}</label>
    <div class="select u-1/1">
        <select class="form-control" ng-model="model[options.key]" id="{{id}}">
            <option disabled selected value>{{to.placeholder}}</option>
        </select>
    </div>
</script>

<script type="text/ng-template" id="/AngularTemplate/ErrorLine">
    <formly-transclude></formly-transclude>
    <div ng-messages="options.formControl.$error"
         ng-if="(!options.form.$valid && options.formControl.$error && fc.$touched) || (!options.form.$valid && options.formControl.$error && options.form.$submitted)">
        <div class="danger" ng-message="{{::name}}" ng-repeat="(name, message) in ::options.validation.messages">
            {{ message(fc.$viewValue, fc.$modelValue, this)}}
        </div>
    </div>
</script>
</body>
</html>