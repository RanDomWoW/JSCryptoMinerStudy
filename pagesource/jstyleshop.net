<script>
function SMS_top_fixed_Menu(el){
	if( SMS$(".JS_MainCate").length){
		var fix_menu = SMS$('.JS_MainCate').offset().top; 
		SMS$(window).scroll(function(){
			if(SMS$(window).scrollTop() > fix_menu && SMS$(window).scrollTop() < 50){
				SMS$(el).css({"display":"none"});
                SMS$("div.JS_MainCate2 > h1 > a").stop().animate({bottom: "-3px"}, 300);
                SMS$(".JS_MainCate2").show();
                SMS$(".JS_R_SubCate").hide();
				SMS$(".SMS_JSright_Close").css({"top":"100px"});
			}else if(SMS$(window).scrollTop() == fix_menu){
				SMS$(el).css({"display":"block"});
                SMS$("div.JS_MainCate2 > h1 > a").stop().animate({bottom: "40px"}, 10);
                SMS$(".JS_MainCate2").hide();
                SMS$(".JS_R_SubCate").show();
				SMS$(".SMS_JSright_Close").css({"top":"150px"});
			}
		});
        
        SMS$(document).ready(function(){
            if(SMS$(window).scrollTop() > 50){
				SMS$(el).css({"display":"none"});
                SMS$("div.JS_MainCate2 > h1 > a").stop().animate({bottom: "-3px"}, 300);
                SMS$(".JS_MainCate2").show();
                SMS$(".JS_R_SubCate").hide();
				SMS$(".SMS_JSright_Close").css({"top":"100px"});
			}
        });
	}
    
}
SMS_top_fixed_Menu(".JS_MainCate");

</script>

