<script>
function JS_New_setCookie(c_name,value,exdays){
    var exdate=new Date();
    exdate.setDate(exdate.getDate() + exdays);
    var c_value=escape(value) + ((exdays==null) ? "" : ";  expires="+exdate.toUTCString());
    document.cookie=c_name + "=" + c_value+"; path=/";
}

function JS_New_deleteCookie(cookieName){
	var expireDate = new Date();
	expireDate.setDate( expireDate.getDate() - 1 );
	document.cookie = cookieName + " = " + "; expires=" + expireDate.toGMTString() + "; path=/";
	document.cookie = cookieName + " = " + "; expires=" + expireDate.toGMTString() + "; path=/html";
    document.cookie = cookieName + " = " + "; expires=" + expireDate.toGMTString() + "; path=/shop";
}

function JS_New_getCookie(name){
	var i,x,y,ARRcookies=document.cookie.split(";");
	for(i=0;i<ARRcookies.length;i++){
		x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
		y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
		x=x.replace(/^\s+|\s+$/g,"");
		if (x==name){
			return unescape(y);
		}
    }
}
		
				
		
function SMSsetResponsive(){
	var _SMSresize = SMS$(window).width();
	if( _SMSresize <= 1200 ){
		SMS$("#aside").fadeOut();
	}else{
		SMS$("#aside").fadeIn();
	}
}

SMS$(window).on('resize', function (){ SMSsetResponsive(); });


