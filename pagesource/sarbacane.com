<script type="text/javascript">
    $(document).ready(function(){

        $(".pubTV").fancybox();

        home_header();

        $('.carousel').slick({
            prevArrow: '<button type="button" class="slick-prev"><i class="material-icons">keyboard_arrow_left</i></button>',
            nextArrow: '<button type="button" class="slick-next"><i class="material-icons">keyboard_arrow_right</i></button>'
        });

        $("#home-emailbuilder").waypoint(function() {
            img = $("#home-emailbuilder").find("img").attr("src");
            $("#home-emailbuilder").find("img").attr("src", img.replace("png", "gif"));
            this.destroy();
        },{
            offset: '90%'
        });

        $("#home-destinataires").waypoint(function() {
            img = $("#home-destinataires").find("img").attr("src");
            $("#home-destinataires").find("img").attr("src", img.replace("jpg", "gif"));
            this.destroy();
        },{
            offset: '90%'
        });

        $("#home-sms").waypoint(function() {
            img = $("#home-sms").find("img").attr("src");
            $("#home-sms").find("img").attr("src", img.replace("png", "gif"));
            this.destroy();
        },{
            offset: '90%'
        });

        $("#home-automation").waypoint(function() {
            img = $("#home-automation").find("img").attr("src");
            $("#home-automation").find("img").attr("src", img.replace("png", "gif"));
            this.destroy();
        },{
            offset: '90%'
        });

    });
    $(window).resize(function(){
        home_header();
    });
    function home_header(){
        var heightWindow = $(window).height();
        if(heightWindow <= $("#home-header").height()){ heightWindow = $("#home-header").height(); }
        var imgScreen = $("#home-header").find(".screen");
        $("#home-header").css("height", heightWindow);
        $("#home-header").find(".content").css("top", ( (heightWindow-200) - $("#home-header").find(".content").height())/3 );
        imgScreen.css("height", (heightWindow/3)*2.5);
        imgScreen.css("top", $("#home-header").find(".content").offset().top+30);
        imgScreen.fadeTo( "slow" , 1);
        $("#home-header").find(".content").fadeTo( "slow" , 1);
    }
    $(window).scroll(function(){
        if($(window).scrollTop()<=0){
            if(!$("header").hasClass("home")){
                $("header").addClass("home");
            }
        } else {
            if($("header").hasClass("home")){
                $("header").removeClass("home");
            }
        }
    });
</script>


