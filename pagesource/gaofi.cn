<script type="text/javascript">
	$(function() {
		var user_id = $.cookie('user_id');
		var user_login = $.cookie('user_login');
		var user_name = $.cookie('user_name');
		var profile = $.cookie('profile');
		user_name = user_name == null ? user_login : user_name;
		var rurl = encodeURIComponent(window.location);
		if (user_id) {
			var photo = '<div class="glyphicon glyphicon-user" title="'+user_name+'"></div>';
			if (profile) {
				photo = '<div class="pull-left"><img src="'+profile+'" style="width:20px;height:20px;"></div>';
			}
			$('.navbar-right')
					.append(
							'<li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">'
									+ photo
									+ '<span class="caret"></span></a><ul class="dropdown-menu">'
									+ '<li><a href="/my/home/">我的主页</a></li>'
									+ '<li><a href="/passport/logout/?rurl='
									+ rurl + '">登出</a></li>' + '</ul></li>');
		} else {
			$('.navbar-right').append(
					'<li><a href="/passport/?rurl=' + rurl
							+ '#login">登录</a></li>');
		}
		
		var searchFun = function(){
			var wd = $('input[name=wd]').val();
			if(!wd){
				return;
			}
			wd = encodeURIComponent(wd);
			var random = new Date().getTime();
			var nav = $(document.body).attr('nav');
			if('nav_reading' == nav){
				var curUrl = window.location.href;
				var url = 'http://www.gaofi.cn/book/list/?wd='+wd+"&random="+random;
				if(curUrl.indexOf("online=1")!=-1){
					url = url + '&online=1';
				}
				window.location.href = url;
			}else if('nav_tags' == nav){
				var url = 'http://www.gaofi.cn/tags/?wd='+wd+"&random="+random;
				window.location.href = url;
			}else if('nav_blog' == nav){
				var url = 'http://www.gaofi.cn/article/list/?wd='+wd+"&random="+random;
				window.location.href = url;
			}else if('nav_resource' == nav){
				var url = 'http://www.gaofi.cn/resource/list/?wd='+wd+"&random="+random;
				window.location.href = url;
			}else if('nav_interview' == nav){
				var url = 'http://www.gaofi.cn/interview/flower/list/*?wd='+wd+"&random="+random;
				window.location.href = url;
			} else if('nav_url' == nav){
				var url = 'http://www.gaofi.cn/url/?wd='+wd+"&random="+random;
				window.location.href = url;
			}else{
				var url = 'https://www.baidu.com/s?wd=';
				window.open(url + wd +" site:www.gaofi.cn");
			}
		}
		$('.top-search-form').keyup(function(event) {
			if (13 == event.keyCode) {
				searchFun();
				return false;
			}
		});
		$('#top-search-btn').click(function(){
			searchFun();
		});
	});
</script>
