	<script language="javascript">
		function to_audit_ssl_image(){
			if(location.protocol=="https:"){

				var http_img_array = [];
				var imgs = document.getElementsByTagName("img");

				for(var i=0;i<imgs.length;i++){
					if(imgs[i].src.substring(0,5)=="http:"){
						http_img_array.push(imgs[i].src);
					}
				}

				if(http_img_array.length > 0){

					var http_img_json_string = JSON.stringify(http_img_array);

					xmlhttp_age_check = createXMLHttp();
					if(xmlhttp_age_check){
						xmlhttp_age_check.onreadystatechange=function(){
							if(xmlhttp_age_check.readyState==4 && xmlhttp_age_check.status==200){

							}
						}
					}
					var ajax_url="ajax_website_module_api.php?module_type=audit_ssl_image&audit_url="+encodeURIComponent(location.href)+"&audit_http_img_json="+encodeURIComponent(http_img_json_string)+"&s="+ new Date().getTime();
					xmlhttp_age_check.open("Post",ajax_url,true);
					xmlhttp_age_check.setRequestHeader("Content-Type","application/x-www-form-urlencoded");
					xmlhttp_age_check.send(null);
				}
			}
		}
