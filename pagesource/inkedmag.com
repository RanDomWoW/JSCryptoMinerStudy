			jQuery(function($) {

				var pageNum     = 2;
				var loadmore    = 'on';
				var fadeSpeed   = 500;
				var totalPages  = 542;
				var container   = '.infinite';
				var child       = 'article';
				var loadingWrap = '#cpxi-loading';
				var loading     = '<div id="cpxi-loading" style="margin:0 auto;text-align:center;"><img alt="Loading More Posts" src="http://www.inkedmag.com/wp-content/uploads/2016/10/ripple-1.svg"><div><em>Loading More Posts</em></div></div>';
				
				if( $(container).length ) {
				
					$(document).on('scroll resize', function() {
						
						if ($(window).scrollTop() + $(window).height() + 350 > $(document).height()) {
							if ( loadmore == 'on' ) {
								loadmore = 'off';
								
								var path = window.location.origin + window.location.pathname + 'page/' + pageNum + window.location.search;
								
								if( pageNum <= totalPages ) {					
									$.ajax({
										url: path,
  	    	    	  	dataType: 'html',
										cache: false,
  	    	    	  	beforeSend: function() {
											$(container).append($(loading).fadeIn(fadeSpeed));
											console.log( 'Getting post from page '+pageNum+' from '+this.url );
										},
									}).done(function( responce, textStatus, object ) {
										
										pageNum++;
										loadmore = 'on';
										
										$(loadingWrap).fadeOut(fadeSpeed, function(){ 
											$(this).remove();
											$(responce).find(child).not('.feature-post article').appendTo( $(container).fadeIn(fadeSpeed) );
										});
										
									}).fail(function( jqXHR, textStatus, errorThrown ) {
										loadmore = 'off';
										$(container).append('<div class="loading-error" style="margin: 0 auto;width: 100%;text-align: center;">'+textStatus+', '+errorThrown+'</div>');
										$(loadingWrap).fadeOut(fadeSpeed, function(){ $(this).remove() });
									});

								} else {
									$(container).append($(loading).fadeIn(fadeSpeed));
									$(loadingWrap).find('em').text('No More Posts to Load');
									$(loadingWrap).fadeOut(fadeSpeed, function(){ $(this).remove() });
									loadmore = 'off';
									return false;
								}
							}
						}
					});
				}
				
			});
			
			</script>


