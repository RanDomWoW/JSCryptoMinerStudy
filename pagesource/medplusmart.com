<script>
var totalJsonpReq = 0;
var totalJsonpRes = 0;
$(document).ready(function(){
	checkingSingleSignOn();
});

function checkingSingleSignOn(){
	var logedinUser = $('#sessionUser').val();
	var ssoChecksum = $('#ssoChecksum').val();
	var isSecure = true;
	if(window.location.href.indexOf("https") == -1){
		isSecure = false;
	}
	var createSSON = readCookie("createSSON");
	if(createSSON != null  && createSSON == 'true' && (logedinUser != null && logedinUser != undefined && $.trim(logedinUser).length > 0)){
		var urlClassName = "buddyServerUrl";
		if(!isSecure){
			urlClassName = "buddyNonSecureServerUrl";
		}
		totalJsonpReq = $("."+urlClassName).length;
		if($("."+urlClassName).length) {
			$("."+urlClassName).each(function(){
		  	 jQuery.ajax({	
			        url:$(this).val()+"?userName="+logedinUser+"&ssoChecksum="+ssoChecksum,
			        dataType: 'jsonp',
			        async: false,
			        jsonpCallback:buddySpree,
			        jsonp: false,
			        timeout: 5000,
			    });
			});
		} else {
			window.location.href = window.location.href;
		}
	}
}


function buddySpree() {
	totalJsonpRes++;
	if(totalJsonpRes == totalJsonpReq){
		createCookie("createSSON",'',-1);
	}
}
</script>
