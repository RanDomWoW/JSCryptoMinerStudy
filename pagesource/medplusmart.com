function showRegisterPopUp(){
	$(".registrationPopUp").show();
	$(".signinPopUp").hide();
	$("#newUserForm").find(".form-control").val("");
	$("#newUserForm").find(".form-group").removeClass("has-error");
	$("#termNcondForReg").val("1");
	$("#termNcondCheck").addClass("active")
}

function showSigninPopUp(){
	$(".signinPopUp").show();
	$("#forgotPassword").hide();
	$(".registrationPopUp").hide();
	$("#popupLoginForm").find(".form-control").val("");
	$("#popupLoginForm").find(".help-block").html("");
	$("#loginServerErrorMsg").html("");
}

$(document).ready(function() {
	$(".shopByCategory").mouseenter(function() {
	  $(".main-nav").show();
	});
	$(".shopByCategory").mouseleave(function(e) {
		var goingto=e.relatedTarget|| event.toElement;
		if(!$(goingto).hasClass("main-nav")) {
			$(".main-nav").hide();
		}
	});
	$('.main-nav').hover(
	  function() {
		  $(".main-nav").show();
	  }, function() {
		  $(".main-nav").hide();
	  });
	
	$(window).scroll(function(){
        if ($(this).scrollTop() > 60 && $(".header-bar").length) {
        	$(".main-nav").css({top:$(".header-bar").offset().top+60});
        } else {
        	$(".main-nav").css({top: 'auto'});
        }
    });
});
</script>
