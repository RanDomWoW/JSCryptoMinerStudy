    <script type="text/javascript">


        $(document).ready(function(){
            lazyload();
            slideShow();


            $("#slideshow li:first-child").addClass('show');
            function slideShow(){
                var current = $('#slideshow .show');
                //If index != 0/false then show next img
                var next = current.next().length ? current.next() :
                        // if index == false then show first img
                        current.siblings().first();
                //*** Swap out the imgs and class ***//
                current.hide().removeClass('show');
                next.fadeIn("slow").addClass('show');
                setTimeout(slideShow, 3000);
            };



            // for advertisement click count
            $(".adClick").click(function(){
                var adId = $(this).data("adid");
                console.log(adId);
                $.ajax({type: "POST",
                    headers: {
                        'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                    },
                    url: "/adCount/"+adId
                });
            });

            window.onscroll = function () {
                lazyload();
            };


            function lazyload() {
                var lazyImage = document.getElementsByClassName('lazy');
                for(var i = 0; i<lazyImage.length; i++){
                   if(elementViewPort(lazyImage[i])){
                       lazyImage[i].setAttribute('src', lazyImage[i].getAttribute('data-src'));
                   }
                }
            }

            function elementViewPort(el) {
              var rect =  el.getBoundingClientRect();
                return (
                    rect.top >= 0 &&
                    rect.left >= 0 &&
                    rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) &&
                    rect.right  <= (window.innerWidth || document.documentElement.clientWidth)
                );
            }

            // for popup modal
            $("#popup-modal").modal("show");

        });
    </script>

    <script type="text/javascript">
        jQuery(document).ready(function ($) {


            var slideCount = $('#slider ul li').length;
            var slideWidth = $('#slider ul li').width();
            var slideHeight = $('#slider ul li').height();
            var sliderUlWidth = slideCount * slideWidth;

            $('#slider').css({ width: slideWidth, height: slideHeight });

            $('#slider ul').css({ width: sliderUlWidth, marginLeft: - slideWidth });

            $('#slider ul li:last-child').prependTo('#slider ul');

            function moveLeft() {
                $('#slider ul').animate({
                    left: + slideWidth
                }, 200, function () {
                    $('#slider ul li:last-child').prependTo('#slider ul');
                    $('#slider ul').css('left', '');
                });
            };

            function moveRight() {
                $('#slider ul').animate({
                    left: - slideWidth
                }, 200, function () {
                    $('#slider ul li:first-child').appendTo('#slider ul');
                    $('#slider ul').css('left', '');
                });
            };

            $('a.control_prev').click(function (event) {
                event.preventDefault()
                moveLeft();
            });

            $('a.control_next').click(function (event) {
                event.preventDefault();
                moveRight();
            });
//video
            var slideVideoCount = $('#slider-video ul li').length;
            var slideVideoWidth = $('#slider-video ul li').width();
            var slideVideoHeight = $('#slider-video ul li').height();
            var sliderVideoUlWidth = slideVideoCount * slideVideoWidth;

            $('#slider-video').css({ width: slideVideoWidth, height: slideVideoHeight });

            $('#slider-video ul').css({ width: sliderVideoUlWidth, marginLeft: - slideVideoWidth });

            $('#slider-video ul li:last-child').prependTo('#slider-video ul');

            function moveLeftVideo() {
                $('#slider-video ul').animate({
                    left: + slideVideoWidth
                }, 200, function () {
                    $('#slider-video ul li:last-child').prependTo('#slider-video ul');
                    $('#slider-video ul').css('left', '');
                });
            };

            function moveRightVideo() {
                $('#slider-video ul').animate({
                    left: - slideVideoWidth
                }, 200, function () {
                    $('#slider-video ul li:first-child').appendTo('#slider-video ul');
                    $('#slider-video ul').css('left', '');
                });
            };

            $('#a.control_prev_video').click(function (event) {
                event.preventDefault();
                moveLeftVideo();
            });

            $('a.control_next_video').click(function (event) {
                event.preventDefault();
                moveRightVideo();
            });

        });
    </script>

    <script type="text/javascript">


        $(document).ready(function () {
            $("#submitForm").click(function(event){
                event.preventDefault();
                var value =  $(this).closest("form").find("input[name='poll']:checked").val();


                $('.repeat-message').empty();
                $('.poll-update').empty();

                if(value == undefined){
                    $('.repeat-message').append('Please select a value');
                    return false;
                }

                $.ajax({type: "POST",
                    headers: {
                        'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                    },
                    url: "/pollUpdate/"+value,
                    success : function (data) {
                        if(data.message){
                            $('.repeat-message').append(data.message);
                        }
                        if(data.id){
                            $('.total-vot').empty();
                            var totalVot = Number(data.poll_yes) + Number(data.poll_no) + Number(data.no_comment);
                            $('.poll-update').before('<p class="total-vot"> ভোট দিয়েছেন <strong>' + toBangla(totalVot)+'</strong> জন</p>');
                           $('.poll-update').append(
                                   '<p>হ্যা <strong>'+ toBangla(data.poll_yes) +'</strong></p>'
                                   +'<p>না <strong>'+ toBangla(data.poll_no) +'</strong></p>'
                                   +'<p>মতামত নেইঁ <strong>'+ toBangla(data.no_comment) +'</strong></p>'
                           );
                            $('.repeat-message').append('আপনার মতামতের জন্য ধন্যবাদ ।');
                        }
                    }

                });
            });
        });



            var finalEnlishToBanglaNumber={'0':'০','1':'১','2':'২','3':'৩','4':'৪','5':'৫','6':'৬','7':'৭','8':'৮','9':'৯'};

             function toBangla(retStr) {
                for (var x in finalEnlishToBanglaNumber) {
                    retStr = String(retStr).replace(new RegExp(x, 'g'), finalEnlishToBanglaNumber[x]);
                }
                return retStr;
            }



    </script>
