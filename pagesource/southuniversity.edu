<script type="text/javascript">
    function RedirectWithQS() {
        var eml = $("#InputEmail").val();
        if (validateEmail(eml)) {
            var mediaWidth = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);

            if (mediaWidth >= 1024) {
                SharedScope.Rif.Email = eml;
                SharedScope.$apply();

                $("#PopupRif").modal("show");
            }
            else {
                document.cookie = "rifemail=" + eml;
                window.location.href = "/Request-Information";
            }
        }
        else {
            $("#emailDiv").addClass('error-empty');
        }
    }
    function validateEmail(email) {
        var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return re.test(email);
    }
</script>
