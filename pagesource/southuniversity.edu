	<script type="text/javascript">
		$(document).ready(function () {
			$("#LocationId").change(function () {
				var selectedCampusId = $(this).val();
				if (selectedCampusId > 0) {
					try {
						$.ajax({
							url: "/admissions/programsbylocation",
							type: 'GET',
							dataType: 'json',
							data: { locationId: selectedCampusId },
							contentType: 'application/json',
							success: ProgramsByLocationSuccess,
							error: ProgramsByLocationError
						});
					}
					catch (ex) { alert(ex); }
				}
			});
		});

		function ProgramsByLocationSuccess(results, status) {
			$("#ProgramId").empty();
			var programOptions = '', lastOutcomeName = '';
			programOptions += '<option selected value="">Select a Program</option>';
			//fill programs options
			$.each(results.Programs, function (i, po) {
				if (po.OutcomeName != lastOutcomeName) {
					programOptions += '<optgroup  label="' + po.OutcomeName + '">' + po.OutcomeName + '</optgroup>';
					lastOutcomeName = po.OutcomeName;
				}

				programOptions += '<option value="' + po.ProgramOfferingID + '">' + po.ProgramName + ' (' + po.OutcomeCode + ')</option>';
			});
			$("select#ProgramId").html(programOptions);

		}
		function ProgramsByLocationError(er, bb, cc) {

			console.log(er);
		}

		$(document).ready(function () {
			$("#rafsubmit").on("click", function () {
				var validationError = false;
				if ($("#LocationId").val() == "") { $("#LocationId").addClass("riferror-missing"); validationError = true; };
				if ($("#ProgramId").val() == "") { $("#ProgramId").addClass("riferror-missing"); validationError = true; };
				var fnameval = $("#Rollover_FirstName").val()
				if (fnameval == "" || fnameval == "YOUR FIRST NAME") { $("#Rollover_FirstName").addClass("riferror-missing"); validationError = true; };

				if (!validationError) {
					$("#rifleader").submit();
				}
			});
		});
		function FieldValChange(fieldID) {
			var fieldVal = $("#" + fieldID).val();
			if (fieldVal != null && fieldVal.length > 0) {
				$("#" + fieldID).removeClass("riferror-missing");
			}
		}
	</script>
