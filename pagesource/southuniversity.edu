<div class="modal fade" id="PopupRif"
	ng-controller="RifControllerHeader">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header"
				ng-if="SubmittedRif == null">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
				<h4>Request Information</h4>
				<p>Learn more today. Complete the form below.</p>
			</div>
			<div class="modal-body popup-rif-container">
				<div class="busy-panel ng-cloak"
					ng-if="pendingRequests > 0"
					ng-cloak>
					<div class="cursor">
						<img src="/assets/Images/busy.gif">
						<div>Now processing...</div>
					</div>
					<div class="bg"></div>
				</div>

				<div class="inquiry-error-header ng-cloak"
					ng-if="ErrorInfo != null && ErrorInfo.IsError"
					ng-cloak>
					<h4>The following error(s) have occurred:</h4>
					<p ng-repeat="message in ErrorInfo.Messages">
						{{ message }}
					</p>
				</div>

				<div class="inquiry-thankyou"
					ng-if="SubmittedRif != null">
					<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
					<h4>Thank you {{ SubmittedRif.FirstName }} {{ SubmittedRif.LastName }}! </h4>
					<br />
					<p>A representative of South University will contact you shortly to help you find the program that matches your interest. Please further explore our school using the links below.</p>
					<p class="return-to-previous"
                        ng-if="Result != null && Result.CallerReadyTrackingId != null && Result.CallerReadyTrackingId.length > 0">
						<a target="_blank" href="/request-information/ThankYou_NoSignal">Click here to talk now</a>
					</p>
					<div class="row-content">
						<h3>Open House <span class="is-grey">At South University</span></h3>
						<p>Learn more about our community and degree programs at our next Open House at South University.</p>
						<a href="https://learn.southuniversity.edu/open-house?source=SCWEB" class="is-arrow-cta btn btn-default rsvp-btn" title="RSVP">RSVP Now</a>
					</div>

					<div class="row-content facebook">
						<h3>Facebook</h3>
						<p>Meet friends and classmates in our community.</p>
						<a href="https://www.facebook.com/SouthU" title="Featured Alumni">View our page &raquo;</a>
					</div>
				</div>

				<form id="request-info-form" class="request-info-form" name="InquiryForm1"
					ng-class="{ 'submit-tried': FormSubmitTried }"
					ng-submit="NextStep(InquiryForm1)"
					ng-if="SubmittedRif == null"
					ng-show="CurrentStep == 1">

					<input type='hidden' id='leadid_token' name='universal_leadid' value='' />

					<fieldset class="Header-Form-Step-1 section-1">
						<legend class="section-1-legend"></legend>

						<select name="locationofinterest" aria-labelledby="Location of Interest"
							ng-options="location.ID as location.Name group by location.State for location in Locations"
							ng-model="Rif.LocationID"
							ng-required="true">
							<option value="" disabled>Select a Campus or Online*</option>
						</select>

						<select name="programofinterest" aria-labelledby="Program of Interest"
							ng-options="offering.ProgramOfferingID as offering.ProgramName + ' (' + offering.OutcomeCode + ')' group by offering.OutcomeLevelName for offering in Offerings | orderBy:['OutcomeLevelId','ProgramName']"
							ng-model="Rif.ProgramOfferingID"
							ng-required="true">
