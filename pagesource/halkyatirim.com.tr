                showItems: 1,
                animation: 'fade',
                mousePause: false,
                height: 54,
                direction: 'up'
            });

          
            $(".btnSearch").click(function () {
                var searchText = $("#txtSearch").val();
                if(searchText!="")
                    location.href = '/SiteSearch.aspx?languageCode=tr-TR&query=' + searchText;
            });
            $("#txtSearch").keypress(function (e) {
                if (e.which == 13) {
                    var searchText = $("#txtSearch").val();
                    if(searchText!="")
                        location.href = '/SiteSearch.aspx?languageCode=tr-TR&query=' + searchText;
                    return false;
                }
            });
            $('.dvanimate2').vTicker({
                speed: 500,
                pause: 3000,
                showItems: 1,
                animation: 'fade',
                mousePause: false,
                height: 16,
                direction: 'up'
            });
            $('.dvanimate4').vTicker({
                speed: 0,
                pause: 0,
                showItems: 1,
                animation: 'fade',
                mousePause: false,
                height: 16,
                direction: 'up'
            });
            $('.dvanimate3').vTicker({
                speed: 500,
                pause: 2000,
                showItems: 1,
                animation: 'fade',
                mousePause: false,
                height: 24,
                direction: 'up'
