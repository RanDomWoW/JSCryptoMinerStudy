            $(".tooltip-li").mouseenter(function () {
                $(".ul-ToolTip-SubMenu").css("visibility", "visible");
            });
            $(".tooltip-li").mouseleave(function () {
                $(".ul-ToolTip-SubMenu").css("visibility", "hidden");
            });

            $(".tooltip-liUyari").mouseenter(function () {
                $(".ul-ToolTip-SubMenuUyari").css("visibility", "visible");
            });
            $(".tooltip-liUyari").mouseleave(function () {
                $(".ul-ToolTip-SubMenuUyari").css("visibility", "hidden");
            });

        });
        function IMKBEndex() {
            $('.IMKBEndeks').vTicker({
                speed: 500,
                pause: 6000,
                showItems: 2,
                animation: 'fade',
