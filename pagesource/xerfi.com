             $(this).css('background', 'white url(' + PATH_QUERY +'img/' + $(this).find('> div > span').text() + '.jpg) 0% 0% no-repeat');
         });
         $('#univers .univers').removeAttr('style').addClass('fermer').removeClass('ouvert'); 
         $('#univers .close').removeAttr('style');                                                                                     
       }
    }
  
    
    $('.domaine img').live('click', function(){ 
