    <script type="text/javascript">
        var slimScrollHeight = '390px';
        var slimScrollSize = '19px';
        $(document).ready(function() {
            $("span[href='#modal_city_choice']").fancybox({
                wrapCSS : 'modal_city',
                minWidth              :    1150,
                /*height              :   700,*/
                overlayOpacity     :    '0.4',
                overlayColor         :    '#000',
                hideOnContentClick :   false,
                hideOnOverlayClick          :   false,
                transitionIn         :   'elastic',
                transitionOut     :   'elastic',
                modal : true,
                titleShow : false,
                showCloseButton : true,
                fitToView   : true,
                autoSize    : true,
                afterShow : function(){
                    slimScrollHeight = Math.round($(window).height() * 0.35) + 'px'; // костыль для учета высоты экрана пользователя
                    $('#modal_city_choice .b-popup-city-regions-h.g-scrollable.scroll_visible').slimScroll({
                        height: slimScrollHeight,
                        alwaysVisible: true,
                        size: slimScrollSize,
                        start : $('#modal_city_choice .b-popup-city-regions-item.selected').length ? $('#modal_city_choice .b-popup-city-regions-item.selected') : undefined
                    });
                    $('#modal_city_choice .b-popup-city-offices.g-scrollable.scroll_visible').slimScroll({
                        height: slimScrollHeight,
                        size: slimScrollSize,
                        alwaysVisible: true,
                        start : $('#modal_city_choice .b-popup-city-offices .search_cities_row a.selected').length ? $('#modal_city_choice .b-popup-city-offices .search_cities_row a.selected') : undefined
                    });
                    if ($('.b-popup-city-offices-tabs').height() <= '400') {
                            $('.b-popup-city-location-2col .g-scrollable.scroll_visible').css('border-right','none').css('overflow','');
                            $('.b-popup-city-location-2col .slimScrollBar').css('width','0');
                        }
                    else {
                        $('.b-popup-city-location-2col .g-scrollable.scroll_visible').css('border-right','20px solid rgba(0, 0, 0, 0.1)');
                    }
                    // Метрика
                    if (typeof  window['yaCounter' + window.YMT_id] != 'undefined') {
                        window['yaCounter' + window.YMT_id].reachGoal("OPEN-CITY-CHOICE-WINDOW");
                    }
                },
                afterClose : function(){
                    $('.slimScrollBar').remove();
                    $('#modal_city_choice .b-popup-city-regions-h.g-scrollable.scroll_visible').slimScroll({
                        destroy:true
                    });
                    $('#modal_city_choice .b-popup-city-offices.g-scrollable.scroll_visible').slimScroll({
                        destroy:true
                    });
                },
                helpers: {
                    overlay: {
                        locked: false
                    }
                }
            });
            // Поиск по городам
            function get_data(query, next) {
                next(slowly_loaded_data, query);
            };
            $(function(){
                $("#modal_city_choice input[name=location-search]").autoSuggest(
                    "/ax/autosuggest", {
                        queryParam: 'a_query',
                        minChars: 2,
                        matchCase: false,
                        startText: 'Введите первые буквы вашего города',
                        selectedItemProp: "name",
                        searchObjProps: "name",
                        selectionLimit: 0,
                        resultClick: function(elem)
                        {
                            var city_name = elem.attributes.name.replace(/(\s\<.*)$/i, '');
                            $('.as-selections .location-search.as-input').val(city_name);
                            //
                            showRegions(elem.attributes.country_id, elem.attributes.region_id);  // Выбираем страну
                            showRegionCities(elem.attributes.region_id); // Получаем города
                            selectCity(elem.attributes.city_id); // Выбираем город
                        }
                    }
                );
            });
            /*
            * Баннеры под отслеживанием заказа
