<script>
    // shop token
    (function(p,r,e,d,i,c,t){p['Akanoo']=i;p[i]=p[i]||function(){
    (p[i].c=p[i].c||[]).push(arguments)},p[i].l=1*new Date();c=r.createElement(e),
    t=r.getElementsByTagName(e)[0];c.async=1;c.src='//cdn.akanoo.com/t/'+d+'.js';t.parentNode.insertBefore(c,t)
    })(window,document,'script','625ac20257eb','at');

    // Page type
    
        at('type', 'home');
    

    // Basket content
    
    

    // Transaction tracking
    

    // Existing Voucher in Basket or other overlay
    
        at('incentive', '0');
    

    // User Data
    

    // Shop Id, Page Id and Product Data
    

    var registerAkanooGAEvent = function(action, campaign_id, group) {
        var act = [
            '_trackEvent',
            'akanoo',
            action,
            campaign_id
        ];

        if (typeof group !== 'undefined') {
            act.push(group);
        }

        _gaq.push(act);
    };

    // Tracking Akanoo's Targeting with own Analytics
    at('bind', 'target', function(event) {
        // event.group: group assignment
        // "0":test / "1":control
        registerAkanooGAEvent('target', event.campaign, event.group);
    });

    //Display Event
    at('bind', 'display', function(event) {
        registerAkanooGAEvent('display', event.campaign);
    });

    //Confirm Event
    at('bind', 'confirm', function(event) {
        if (event.campaign) {
            var req = $.ajax({
                url: '/cart/add-voucher/',
                method: 'POST',
                data: {'code': event.campaign}
            });

            req.done(function(data, textStatus, jqXHR) {
                if ('error' in data) {
                    at('trigger', 'redeem', event.campaign, '0');
                } else {
                    // cart page
                    var $checkout_process_cart_step = $('#ch-pr-cart');
                    if ($checkout_process_cart_step.length) {
                        $checkout_process_cart_step.trigger('replace_cart_total', [data.total_area_html]);
                    }
                    at('trigger', 'redeem', event.campaign, '1');
                }
            });

            req.fail(function(jqXHR, textStatus, errorThrown) {
                at('trigger', 'redeem', event.campaign, '0');
            });
        }
        registerAkanooGAEvent('confirm', event.campaign);
    });

    // Abort Event
    at('bind', 'abort', function(event) {
        registerAkanooGAEvent('abort', event.campaign);
    });

    // required to track the view
    at('send');
</script>
<!-- end Akanoo -->


