		$("#portalSliderShowNext h3").html( $("#newsList li.next input[name='entry']").attr("value") );
		
		$("#portalSliderShowPrincipal").fadeOut(1000, function(){
			$("#newsList li.active").removeClass("active");
			$("#newsList li.next").addClass("active").removeClass("next");
			$("#portalSliderShowPrincipal").attr( "href", $("#newsList li.active a").attr("href") );
			$("#portalSliderShowPrincipal img").attr( "src", $("#newsList li.active input[name='image']").attr("value") );
			$("#portalSliderShowPrincipal h2").html( $("#newsList li.active input[name='title']").attr("value") );
			$("#portalSliderShowPrincipal h3").html( $("#newsList li.active input[name='entry']").attr("value") );
			$("#portalSliderShowPrincipal").fadeIn(0);
			if ( $("#portalSliderShowPrincipal").attr("href") != $("#newsList li.active a").attr("href") )
				$("#newsList li.active").removeClass("active");
			timeoutId = setTimeout("changeActiveNews()",5000);
			sliderAnimate = false;
		});
	}
}
</script>

