function setHeightSlider()
{
    cWidth = window.innerWidth;
    sWidth = parseInt($("#portalSlider").css("width").replace('px',''));
    $("#portalSlider").css("height", parseInt(sWidth*350/980)+"px");
    if ( cWidth < 850 )
    {
        $("#links").css("height", "auto");
        $("#links .text").css("height", parseInt($("#links img").css("height").replace('px',''))+"px");
        $("#liveTV").css("height", "auto");
    }
    else
    {
        $("#links").css("height", parseInt(sWidth*350/980)+"px");
        $("#liveTV").css("height", parseInt(sWidth*350/980)+"px");
    }
    
    if ( $("#liveTV > img").length )
    {
        width = parseInt($("#liveTV > img").css("width").replace('px',''));
        $("#liveTV > img").css("height", parseInt(width*9/16)+"px");
    }
}
$( window ).resize(function() {
    setHeightSlider();
});
    
