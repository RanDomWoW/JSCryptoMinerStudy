		}
		else
		{
			$("#addsBannerL").css("width", bannerWidth+"px").css("margin-left", "-"+(630+bannerWidth)+"px").css("display", "block");
			$("#addsBannerR").css("width", bannerWidth+"px").css("display", "block");
			$("#addsBannerR a object").attr("width", bannerWidth).attr("height", (bannerWidth*1000/190));
		}
	}
	
	if ( document.body.offsetWidth < 1330 )
