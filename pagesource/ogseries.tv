		$("#portalSliderShowPrincipal h3").html( $("#newsList li.active input[name='entry']").attr("value") );
		timeoutId = setTimeout("changeActiveNews()",5000);
	}
	else
	{
		if ( !$("#newsList li.active").next('li').length )
			$("#newsList li").first().addClass("next");
		else
		{
			if ( !$("#newsList li.next").length )
				$("#newsList li.active").next().addClass("next");
		}
		
		$("#portalSliderShowNext").attr( "href", $("#newsList li.next a").attr("href") );
		$("#portalSliderShowNext img").attr( "src", $("#newsList li.next input[name='image']").attr("value") );
