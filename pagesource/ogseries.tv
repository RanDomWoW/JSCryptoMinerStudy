function proccessMatchResult( type )
{
	var total1 = 0;
	var total2 = 0;
	for ( i = 0 ; i <= 7 ; i++ )
		if ( $("[name=round"+i+"_p1]").length )
			if ( $("[name=round"+i+"_p1]").val() != "" && $("[name=round"+i+"_p2]").val() != "" )
				if ( type == 1 )
				{
					total1 += parseInt($("[name=round"+i+"_p1]").val());
					total2 += parseInt($("[name=round"+i+"_p2]").val());
				}
				else
				{
					total1 += parseInt($("[name=round"+i+"_p1]").val()) > parseInt($("[name=round"+i+"_p2]").val()) ? 1 : 0;
					total2 += parseInt($("[name=round"+i+"_p1]").val()) < parseInt($("[name=round"+i+"_p2]").val()) ? 1 : 0;
				}
	$("#matchResultGlobalP1").val( total1 );
	$("#matchResultGlobalP2").val( total2 );
}

