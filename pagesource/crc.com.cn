  <script type="text/javascript" src="./images/jquery1.11.min.js"></script>
  <script type="text/javascript" src="./images/modernizr.js"></script>
  <script>
      $(function(){

          if(!isMobile()){
              $(".in-crc-btn").css({
                  "position": "absolute",
                  "left": "50%",
                  "transform": "translateX(-50%)",
                  "z-index": "1000",
                  "bottom":"50px"
              });
          }

          if(!Modernizr.csstransforms3d || !isSuportSvg()){
              emptyBox();
          }
          else{
              $(".tsfc").eq(0).css({"display":"block"});
          }

          function isSuportSvg(){
              return document.implementation.hasFeature("http://www.w3.org/TR/SVG2/feature#GraphicsAttribute", 2.0);
          }
          function isMobile(){
              var userAgentInfo = navigator.userAgent,Agents = ["Android", "iPhone","SymbianOS", "Windows Phone","iPad", "iPod"],flag = false;
              for (var v = 0; v < Agents.length; v++) {
                  if (userAgentInfo.indexOf(Agents[v]) > 0) {
                      flag = true;
                      break;
                  }
              }
              return flag;
          }
          function emptyBox(){
              var box1=$("#showBox1");
              var box2=$("#showBox2");
              var box3=$("#showBox3");
              var box4=$("#showBox4");

              box1.removeClass().addClass("show-city").find(".yun,.city,.float-text ").remove();
              box2.removeClass().addClass("show-city").find(".yun,.city,.float-text ").remove();
              box3.removeClass().addClass("show-city").find(".yun,.city,.float-text ").remove();
              box4.removeClass().addClass("show-city").find(".yun,.city,.float-text ").remove();
              if(!Modernizr.csstransforms3d){
                  $(".show-city").find(".float-mod").remove();
              }

              setSize();
              box1.css({
                  "background-image":"url('images/IE01.jpg')"
              }).append('<div class="float-text" style="left:45%;top:80px;"><h2>1938年</h2><h3>华润的前身“联和行”在香港中环毕达街毕达行成立</h3></div>');
              box2.css({
                  "background-image":"url('images/IE02.jpg')"
              }).append('<div class="float-text" style="top:100px; left:185px;"><h2>1952-1979年</h2><h3>华润在香港中国银行大厦办公</h3></div>');
              box3.css({
                  "background-image":"url('images/IE03.jpg')"
              }).append('<div class="float-text" style="left:200px;top:90px;"><h2>1983年</h2><h3>香港华润大厦落成</h3></div>');
              box4.css({
                  "background-image":"url('images/IE04.jpg')"
              }).append('<div class="float-text" style="left:200px;top:90px;">' +
                  '<h2 style="font-size: 55px; color:#ed9c46;text-shadow: 2px 0px 0px #f56ef6;line-height: 64px;">2018年</h2>' +
                  '<h3 style="font-size: 18px;">华润成立80周年，中国华润大厦落成</h3></div>');
              $(window).resize(function(){
                  setSize();
              });
              box1.fadeIn(1000);
              var textTimer=setTimeout(function(){
                  box1.find(".float-text").show();
                  clearTimeout(textTimer);
              },500);
              showPic(1);

              function setSize(){ //box尺寸
                  var w=$(window).width(),h=$(window).height();
                  $(".show-city").css({
                      "width":w+"px",
                      "height":h+"px"
                  });
              }
              function showPic(index){
                  var timer,city,textTimer;
                  if(index<=3){
                      timer=setTimeout(function(){
                          city=$(".show-city").eq(index);
                          city.fadeIn(2000);
                          if(Modernizr.opacity){
                              city.find(".float-text").fadeIn(1000);
                          }
                          else{
                              textTimer=setTimeout(function(){
                                  city.find(".float-text").show();
                                  clearTimeout(textTimer);
                              },1000);
                          }
                          city.siblings(".show-city").fadeOut(1000);
                          if(Modernizr.opacity) {
                              city.siblings(".show-city").find(".float-text").fadeOut(500)
                          }
                          else{
                              city.siblings(".show-city").find(".float-text").hide();
                          }
                          clearTimeout(timer);
                          showPic(index+1);
                      },5000);
                  }
              }

              if(!Modernizr.csstransforms3d){
                  $(".in-crc-btn").css({
                      "position": "absolute",
                      "left": "50%",
                      "margin-left": "-64px",
                      "z-index": "1000",
                      "bottom":"50px",
                      "transform": ""
                  });
              }

          }
      });


  </script>
