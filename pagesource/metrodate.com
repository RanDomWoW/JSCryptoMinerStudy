<script type="text/javascript">
	function ChangeSignupScreens() { 
		var tamt = 280;
		var numloops = 8;
		var elem1 = Get("DivSignupScreen1");
		var elem2 = Get("DivSignupScreen2");
	
		for (var tloop=1; tloop <= numloops; tloop++)
			setTimeout(function(loopnum) { 
				return function() { 
					MoveSignupScreens(elem1, elem2, Math.floor(((tamt * -1) / numloops) * loopnum), 280 - Math.ceil((tamt / numloops) * loopnum)); 
				}; 
			}(tloop), 60 * tloop);

		document.getElementById("ctl00_BodyContent_RightColumnSignupControl_TextBoxSignupEmail").focus();
	}
	
	function MoveSignupScreens(elem1, elem2, pos1, pos2) { 
		elem1.style.left = pos1 + "px";
		elem2.style.left = pos2 + "px";
	}
</script>
