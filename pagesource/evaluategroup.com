<script type="text/javascript">
	var imageCount;
	var isInEditorMode;
	var useAutomaticTransition;
	var uploadInitiated = false;
	var indexOfTopImage = 0;
	var indexOfTopText = 0;
	var transitionMethod;
	var transitionDuration = 10000;
	var slideMethod = 'easeInOutQuint';
	var slideDuration = 2500;
	var fadeTextInAfterSlide = false;

	var _slidingImageTimeoutId = -1;
	var _slidingTextTimeoutId = -1;

	function showBannerUploadUI()
	{
		uploadInitiated = false;
		$get('ctl00_CtlPageBanner_BtnStartUpload').style.display = 'inline';
		$get('ctl00_CtlPageBanner_BtnCancelUpload').style.display = 'inline';
		$get('ctl00_CtlPageBanner_BtnApply').style.display = 'none';
		$get('ctl00_CtlPageBanner_BtnUndo').style.display = 'none';
		$get('ctl00_CtlPageBanner_PnlUploadContainer').style.display = 'block';
		$get('ctl00_CtlPageBanner_PnlProgressContainer').style.display = 'none';

		$get('ctl00_CtlPageBanner_BtnChangeBannerImage').style.display = 'none';
		$get('ctl00_CtlPageBanner_BtnEditText').style.display = 'none';
		$get('ctl00_CtlPageBanner_BtnPreviousBackground').style.display = 'none';
		$get('ctl00_CtlPageBanner_BtnNextBackground').style.display = 'none';
		$get('ctl00_CtlPageBanner_PnlPageBannerUploader').style.display = 'block';

		var objRemoveBannerImageBtn = $get('ctl00_CtlPageBanner_BtnRemoveBannerImage');
		if (objRemoveBannerImageBtn != null)
			objRemoveBannerImageBtn.style.display = 'none';
	}
	function hideBannerUploadUI()
	{
		uploadInitiated = false;
		$get('ctl00_CtlPageBanner_BtnChangeBannerImage').style.display = 'inline';
		$get('ctl00_CtlPageBanner_BtnEditText').style.display = 'inline';
		$get('ctl00_CtlPageBanner_BtnPreviousBackground').style.display = 'inline';
		$get('ctl00_CtlPageBanner_BtnNextBackground').style.display = 'inline';
		$get('ctl00_CtlPageBanner_PnlPageBannerUploader').style.display = 'none';

		var objRemoveBannerImageBtn = $get('ctl00_CtlPageBanner_BtnRemoveBannerImage');
		if (objRemoveBannerImageBtn != null)
			objRemoveBannerImageBtn.style.display = 'inline';
	}
	function initiateUpload()
	{
		$get('ctl00_CtlPageBanner_BtnStartUpload').style.display = 'none';
		$get('ctl00_CtlPageBanner_PnlUploadContainer').style.display = 'none';

		var progressBar = $get('progressBar');
		progressBar.style.visibility = 'hidden';
		progressBar.style.display = 'block';
		progressBar.style.width = '0';
		progressBar.style.backgroundColor = '';
		progressBar.innerHTML = '';

		$get('ctl00_CtlPageBanner_PnlProgressContainer').style.display = 'block';
		startFileUpload('ctl00_CtlPageBanner_AsyncBannerUploader');
		uploadInitiated = true;
	}
	function cancelUpload()
	{
		if (uploadInitiated)
			stopFileUpload('ctl00_CtlPageBanner_AsyncBannerUploader');
		
		hideBannerUploadUI();
	}
	function AsyncBannerUploader_ProgressUpdate(percentage)
	{
		if (percentage > 0)
		{
			var progressBar = $get('progressBar');
			progressBar.style.visibility = 'visible';
			progressBar.style.display = 'block';
			progressBar.style.width = percentage + '%';
			progressBar.innerHTML = '&nbsp;Uploading';
		}
	}
	function AsyncBannerUploader_Complete()
	{
		var progressBar = $get('progressBar');
		progressBar.style.visibility = 'visible';
		progressBar.style.display = 'block';
		progressBar.style.width = '100%';
		progressBar.innerHTML = '&nbsp;Complete';
	}
	function AsyncBannerUploader_Cancelled()
	{
		var progressBar = $get('progressBar');
		progressBar.style.visibility = 'visible';
		progressBar.style.display = 'block';
		progressBar.style.width = '100%';
		progressBar.innerHTML = '&nbsp;Cancelled';
	}
	function AsyncBannerUploader_UploadError(errMsg)
	{
		var progressBar = $get('progressBar');
		progressBar.style.visibility = 'visible';
		progressBar.style.display = 'block';
		progressBar.style.width = '100%';
		progressBar.style.backgroundColor = '#c00000';
		if (typeof(errMsg) != "undefined" && errMsg != "")
			progressBar.innerHTML = errMsg;
		else
			progressBar.innerHTML = '&nbsp;Upload Failed';
	}

	Sys.WebForms.PageRequestManager.getInstance().add_beginRequest(function () {
		Sys.Debug.trace('PageRequestManager::beginRequest event raised [isInEditorMode = ' + isInEditorMode + ', imageCount = ' + imageCount + ']');
		if (!isInEditorMode && imageCount > 1)
		{
			if (isInImageTransition())
				stopImageTransition();
			if (isInTextTransition())
				stopTextTransition();
		}
	});

	Sys.WebForms.PageRequestManager.getInstance().add_pageLoaded(function () {
		Sys.Debug.trace('PageRequestManager::pageLoaded event raised [isInEditorMode = ' + isInEditorMode + ', imageCount = ' + imageCount + ']');
		if (!isInEditorMode && imageCount > 1 && useAutomaticTransition)
		{
			startImageTransition();
			startTextTransition();
		}
	});

	function isInImageTransition()
	{
		if (_slidingImageTimeoutId != -1)
			return true;

		if ($('.BackgroundImageContainer img').eq(indexOfTopImage).is(':animated'))
			return true;

		return false;
	}

	function isInTextTransition()
	{
		if (_slidingTextTimeoutId != -1)
			return true;

		if ($('.TextContainer div.BannerText').eq(indexOfTopText).is(':animated'))
			return true;

		return false;
	}

	function switchImageTo(i)
	{
		if (indexOfTopImage == i)
			return;
	
		if (isInImageTransition())
			stopImageTransition();

		if (isInTextTransition())
			stopTextTransition();

		var imgs = $('.BackgroundImageContainer img');
		imgs.each(function (index)
		{
			var logicalIndex = (index + ((imageCount - 1) * i)) % imageCount;
			var bannerZIndex = imageCount * 2 - 2 * logicalIndex - 1;

			var img = imgs.eq(index);
			img.css('z-index', bannerZIndex);
		});

		var textContainers = $('.TextContainer .BannerText');
		textContainers.each(function (index)
		{
			var logicalIndex = (index + ((imageCount - 1) * i)) % imageCount;
			var textZIndex = imageCount * 2 - 2 * logicalIndex - 1 + 1;

			var textContainer = textContainers.eq(index);
			textContainer.css('z-index', textZIndex);

			if (index == i)
			{
				textContainer.css('display', 'block');
				if (slideMethod == "fading")
					textContainer.css('opacity', '1');
			}
			else
			{
				if( slideMethod == "fading" )
					textContainer.css('display', 'none');
			}
		});

		indexOfTopImage = i;
		indexOfTopText = i;

		updateImageSelector(indexOfTopImage);

		if (!isInEditorMode && imageCount > 1 && useAutomaticTransition)
		{
			startImageTransition();
			startTextTransition();
		}
	}

	function updateImageSelector(selectedIndex)
	{
		var oldCurrentImage = $('.image-selector .unselectable-image-button');
		var newCurrentImage = $('.image-selector img').eq(selectedIndex);

		oldCurrentImage.removeClass('unselectable-image-button');
		oldCurrentImage.addClass('selectable-image-button');

		newCurrentImage.removeClass('selectable-image-button');
		newCurrentImage.addClass('unselectable-image-button');
	}

	function startImageTransition()
	{
		switch (transitionMethod)
		{
			case 'sliding':
				_slidingImageTimeoutId = setTimeout('startImageTransition_sliding()', transitionDuration);
				break;

			case 'fading':
				startImageTransition_fading();
				break;
		}
	}
	function stopImageTransition()
	{
		switch (transitionMethod)
		{
			case 'sliding':
				stopImageTransition_sliding();
				break;

			case 'fading':
				stopImageTransition_fading();
				break;
		}
	}
	function startTextTransition()
	{
		switch (transitionMethod)
		{
			case 'sliding':
				_slidingTextTimeoutId = setTimeout('startTextTransition_sliding()', transitionDuration);
				break;

			case 'fading':
				startTextTransition_fading();
				break;
		}
	}
	function stopTextTransition()
	{
		switch (transitionMethod)
		{
			case 'sliding':
				stopTextTransition_sliding();
				break;

			case 'fading':
				stopTextTransition_fading();
				break;
		}
	}

	function startImageTransition_sliding()
	{
		_slidingImageTimeoutId = -1;

		Sys.Debug.trace('startImageTransition_sliding() called');
		Sys.Debug.trace('startImageTransition_sliding: sliding out image #' + indexOfTopImage);
		Sys.Debug.trace('startImageTransition_sliding: transition starting. z-indexes: ' + $('.BackgroundImageContainer img').eq(0).css('z-index') + ', ' + $('.BackgroundImageContainer img').eq(1).css('z-index') + ', ' + $('.BackgroundImageContainer img').eq(2).css('z-index'));

		$('.BackgroundImageContainer img').eq(indexOfTopImage).animate({ 'right': '100%' }, slideDuration, slideMethod, function ()
		{
			indexOfTopImage = (indexOfTopImage + 1) % imageCount;

			var imgs = $('.BackgroundImageContainer img');
			imgs.each(function (index)
			{
				var img = imgs.eq(index);
				img.css('z-index', (((parseInt(img.css('z-index')) + 1) % (imageCount * 2)) + 2) - 1);
			});

			$(this).css('right', '0');

			Sys.Debug.trace('startImageTransition_sliding: transition complete. z-indexes: ' + $('.BackgroundImageContainer img').eq(0).css('z-index') + ', ' + $('.BackgroundImageContainer img').eq(1).css('z-index') + ', ' + $('.BackgroundImageContainer img').eq(2).css('z-index'));

			updateImageSelector(indexOfTopImage);
			startImageTransition();
		});
	}
	function stopImageTransition_sliding()
	{
		Sys.Debug.trace('stopImageTransition_sliding() called');

		if (_slidingImageTimeoutId != -1)
			clearTimeout(_slidingImageTimeoutId);

		if ($('.BackgroundImageContainer img').eq(indexOfTopImage).is(':animated'))
		{
			$('.BackgroundImageContainer img').eq(indexOfTopImage).stop(false, false);
			$('.BackgroundImageContainer img').eq(indexOfTopImage).css('right', '0');
		}
	}

	function startImageTransition_fading()
	{
		Sys.Debug.trace('startImageTransition_fading() called');
		Sys.Debug.trace('startImageTransition_fading: fading out image #' + indexOfTopImage);
		Sys.Debug.trace('startImageTransition_fading: transition starting. z-indexes: ' + $('.BackgroundImageContainer img').eq(0).css('z-index') + ', ' + $('.BackgroundImageContainer img').eq(1).css('z-index') + ', ' + $('.BackgroundImageContainer img').eq(2).css('z-index'));

		$('.BackgroundImageContainer img').eq(indexOfTopImage).fadeOut(transitionDuration, function ()
		{
			indexOfTopImage = (indexOfTopImage + 1) % imageCount;

			var imgs = $('.BackgroundImageContainer img');
			imgs.each(function (index)
			{
				var img = imgs.eq(index);
				img.css('z-index', (((parseInt(img.css('z-index')) + 1) % (imageCount * 2)) + 2) - 1);
			});

			$(this).css('display', 'block');

			Sys.Debug.trace('startImageTransition_fading: transition complete. z-indexes: ' + $('.BackgroundImageContainer img').eq(0).css('z-index') + ', ' + $('.BackgroundImageContainer img').eq(1).css('z-index') + ', ' + $('.BackgroundImageContainer img').eq(2).css('z-index'));

			updateImageSelector(indexOfTopImage);
			startImageTransition();
		});
	}

	function stopImageTransition_fading()
	{
		Sys.Debug.trace('stopImageTransition_fading() called');
		$('.BackgroundImageContainer img').eq(indexOfTopImage).stop(false, false);
		$('.BackgroundImageContainer img').eq(indexOfTopImage).css('opacity', '1');
	}

	function startTextTransition_sliding()
	{
		_slidingTextTimeoutId = -1;

		Sys.Debug.trace('startTextTransition_sliding() called');
		Sys.Debug.trace('startTextTransition_sliding: fading out text #' + indexOfTopText);
		Sys.Debug.trace('startTextTransition_sliding: transition starting. z-indexes: ' + $('.TextContainer div.BannerText').eq(0).css('z-index') + ', ' + $('.TextContainer div.BannerText').eq(1).css('z-index') + ', ' + $('.TextContainer div.BannerText').eq(2).css('z-index'));

		var indexOfNextText = (indexOfTopText + 1) % imageCount;
		$('.TextContainer div.BannerText').eq(indexOfTopText).css('display', 'block');

		if( fadeTextInAfterSlide )
			$('.TextContainer div.BannerText').eq(indexOfNextText).css('display', 'none');

		$('.TextContainer div.BannerText').eq(indexOfTopText).animate({ 'right': '100%', 'left': '-100%' }, slideDuration, slideMethod, function ()
		{
			if (fadeTextInAfterSlide)
				$('.TextContainer div.BannerText').eq(indexOfNextText).css('opacity', '0').fadeTo(transitionDuration / 2, 1);

			indexOfTopText = (indexOfTopText + 1) % imageCount;

			var textDivs = $('.TextContainer div.BannerText');
			textDivs.each(function (index)
			{
				var textDiv = textDivs.eq(index);
				textDiv.css('z-index', (((parseInt(textDiv.css('z-index')) + 1) % (imageCount * 2)) + 2) - 1);
			});

			$(this).css('left', '');
			$(this).css('right', '');

			Sys.Debug.trace('startTextTransition_sliding: transition complete. z-indexes: ' + $('.TextContainer div.BannerText').eq(0).css('z-index') + ', ' + $('.TextContainer div.BannerText').eq(1).css('z-index') + ', ' + $('.TextContainer div.BannerText').eq(2).css('z-index'));

			startTextTransition();
		});
	}

	function stopTextTransition_sliding()
	{
		Sys.Debug.trace('stopTextTransition_sliding() called');

		if (_slidingTextTimeoutId != -1)
			clearTimeout(_slidingTextTimeoutId);

		if ($('.TextContainer div.BannerText').eq(indexOfTopText).is(':animated'))
		{
			$('.TextContainer div.BannerText').eq(indexOfTopText).stop(false, false);
			$('.TextContainer .BannerText').eq(indexOfTopText).css('left', '');
			$('.TextContainer .BannerText').eq(indexOfTopText).css('right', '');

			if (fadeTextInAfterSlide)
			{
				var indexOfNextText = (indexOfTopText + 1) % imageCount;
				$('.TextContainer div.BannerText').eq(indexOfNextText).stop(false, false);

				$('.TextContainer .BannerText').eq(indexOfNextText).css('opacity', '');
				$('.TextContainer .BannerText').eq(indexOfNextText).css('display', 'block');
			}
		}
	}

	function startTextTransition_fading()
	{
		Sys.Debug.trace('startTextTransition_fading() called');
		Sys.Debug.trace('startTextTransition_fading: fading out text #' + indexOfTopText);
		Sys.Debug.trace('startTextTransition_fading: transition starting. z-indexes: ' + $('.TextContainer div.BannerText').eq(0).css('z-index') + ', ' + $('.TextContainer div.BannerText').eq(1).css('z-index') + ', ' + $('.TextContainer div.BannerText').eq(2).css('z-index'));

		var indexOfNextText = (indexOfTopText + 1) % imageCount;
		$('.TextContainer div.BannerText').eq(indexOfTopText).css('display', 'block');
		$('.TextContainer div.BannerText').eq(indexOfNextText).css('display', 'none');

		$('.TextContainer div.BannerText').eq(indexOfNextText).css('opacity', '0').fadeTo(transitionDuration, 1);

		$('.TextContainer div.BannerText').eq(indexOfTopText).css('opacity', '1').fadeTo(transitionDuration, 0, function ()
		{
			indexOfTopText = (indexOfTopText + 1) % imageCount;

			var textDivs = $('.TextContainer div.BannerText');
			textDivs.each(function (index)
			{
				var textDiv = textDivs.eq(index);
				textDiv.css('z-index', (((parseInt(textDiv.css('z-index')) + 1) % (imageCount * 2)) + 2) - 1);
			});

			$(this).css('display', 'none');

			Sys.Debug.trace('startTextTransition_fading: transition complete. z-indexes: ' + $('.TextContainer div.BannerText').eq(0).css('z-index') + ', ' + $('.TextContainer div.BannerText').eq(1).css('z-index') + ', ' + $('.TextContainer div.BannerText').eq(2).css('z-index'));

			startTextTransition();
		});
	}

	function stopTextTransition_fading()
	{
		Sys.Debug.trace('stopTextTransition_fading() called');
		$('.TextContainer div.BannerText').eq(indexOfTopText).stop(false, false);
		$('.TextContainer .BannerText').eq(indexOfTopText).css('opacity', '1');

		var indexOfNextText = (indexOfTopText + 1) % imageCount;

		$('.TextContainer div.BannerText').eq(indexOfNextText).stop(false, false);
		$('.TextContainer .BannerText').eq(indexOfNextText).css('display', 'none');
	}
