		var timeout	= 500;
		var closetimer	= 0;
		var ddmenuitem	= 0;

		function mopen(id)
		{	
			// cancel close timer
			mcancelclosetime();

			// close old layer
			if(ddmenuitem) ddmenuitem.style.visibility = 'hidden';

			// get new layer and show it
			ddmenuitem = document.getElementById(id);
			ddmenuitem.style.visibility = 'visible';

		}
		// close showed layer
		function mclose()
		{
			if(ddmenuitem) ddmenuitem.style.visibility = 'hidden';
		}

		// go close timer
		function mclosetime()
		{
			closetimer = window.setTimeout(mclose, timeout);	
		}

		// cancel close timer
		function mcancelclosetime()
		{
			if(closetimer)
			{
				window.clearTimeout(closetimer);
				closetimer = null;
			}
		}

		// close layer when click-out 
		document.onclick = mclose; 

			
		var current_theme_color = "#5A5152";
		
		function changeThemeColor (newColor, themeName)
		{
			var themeIcons = document.getElementsByName ("theme_icon");
			for (var index = 0; index < themeIcons.length; index ++)
			{
				if (themeIcons[index].src.indexOf (themeName) != -1)
					themeIcons[index].src = "images/theme_" + themeName + "_selected.jpg";
				else
					themeIcons[index].src = themeIcons[index].src.replace ("_selected", "");
			}
			var elements = new Array ("td", "span","div","font");
			for (var j = 0; j < elements.length; j++)
			{
				var themeBasedElements = document.getElementsByTagName (elements [j]);
			
				for (var i = 0; i < themeBasedElements.length; i++)
				{
					if (themeBasedElements[i].className == "header_bar" || themeBasedElements[i].className == "block_service")
					{
						themeBasedElements [i].style.backgroundColor = newColor;
					}
				} //for
			}
			var searchButton = document.getElementById ("search_button");
			searchButton.src = "images/search_" + themeName + ".jpg";
			
			var fontIcons = document.getElementsByName ("font_icons");
			for (var index = 0; index < font_icons.length; index ++)
			{
				fontIcons[index].src = "images/" + fontIcons[index].id + "_" + themeName + ".jpg";
			}
			current_theme_color = newColor;			
			setCookie ("theme", themeName, 365);			
		} //changeThemeColor
		
		function setCookie(c_name,value,expiredays)
		{
			var exdate = new Date();
			exdate.setDate(exdate.getDate()+expiredays);
			document.cookie=c_name+ "=" +escape(value)+
			((expiredays==null) ? "" : ";expires=" + exdate.toGMTString());
		}
			
		function changeFont (newFont, fontName)
		{
			var spans = document.getElementsByTagName ("span");
			
			for (var index = 0; index < spans.length; index ++)
			{
				if (spans [index].className.indexOf ("small_font") != -1)
				{
					spans [index].style.fontSize = newFont + "px";
				}
				else
					if (spans [index].className.indexOf ("large_font") != -1)
					{
						spans [index].style.fontSize = (parseInt(newFont) + 1) + "px";
					}
			}
			
			setCookie ("font", fontName, 365);			
		} //changeThemeColor
	
		function trim (text) 
		{
			return text.replace(/^\s+|\s+$/g,"");
		}


		</script>
