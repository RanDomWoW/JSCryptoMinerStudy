     <script type="text/javascript">       
        function isMobileBrowser(){

            if(navigator.userAgent.toLowerCase().indexOf('viewit') > -1)
                return;
            if(navigator.userAgent.toLowerCase().indexOf('ipad') > -1)
                return;   
            
            if(window.location.pathname.toLowerCase().indexOf('/gnovocorpor') > -1)
                return;
            if(window.location.pathname.toLowerCase().indexOf('/ggeolocaliz') > -1)
                return;
            if(window.location.pathname.toLowerCase().indexOf('/datas-comemorativas/gdatas') > -1)
                return;
            if(window.location.pathname.toLowerCase().indexOf('/floriculturas/gcidades') > -1)
                return;
            if(window.location.pathname.toLowerCase().indexOf('/kitdiadamulher/gcombo') > -1)
                return;
        
            var mobiles = [ "midp", "j2me", "avant", "docomo",
                            "novarra", "palmos", "palmsource",
                            "240x320", "opwv", "chtml",
                            "pda", "windows ce", "mmp/",
                            "blackberry", "mib/", "symbian",
                            "wireless", "nokia", "hand", "mobi",
                            "phone", "cdm", "up.b", "audio",
                            "SIE-", "SEC-", "samsung", "HTC",
                            "mot-", "mitsu", "sagem", "sony"
                            , "alcatel", "lg", "eric", "vx",
                            "NEC", "philips", "mmm", "xx",
                            "panasonic", "sharp", "wap", "sch",
                            "rover", "pocket", "benq", "java",                         
                            "pg", "vox", "amoi",
                            "bird", "compal", "kg", "voda",
                            "sany", "kdd", "dbt", "sendo",
                            "sgh", "gradi", "jb", "dddi",
                            "moto", "iphone"];
				
            for (i = 0; i < mobiles.length; i++) {
                if(navigator.userAgent.toLowerCase().indexOf(mobiles[i].toLowerCase()) > -1)
                {                   
                    window.location.href = window.location.href.toLowerCase().replace('/www.', '/m2.');
                    return;
                }
            }
        }

        isMobileBrowser();        
    </script>

