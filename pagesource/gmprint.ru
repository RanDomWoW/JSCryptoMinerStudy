			$(this).removeClass('pressed');
		});

		$('#main_video_play').on('click', function(){

			var html = '<iframe frameborder="0" width="1080" height="450" allowfullscreen="allowfullscreen" src="https://www.youtube.com/embed/dsqpgKIFt1U?rel=0&autoplay=1&showinfo=0"></iframe>';
			$('.main-video-substrate').addClass('hide');
			//$('.main-video-substrate').animate({opacity: 0}, 500 );
			setTimeout(function(){

				$('#main_video .iframe').addClass('active');
				$('#main_video .iframe').append(html);
				setTimeout(function() {
					return $('#main_video .close').show();
				}, 800);
			},800);
		});
		$('#main_video .close').on('click', function(){
			$('#main_video .iframe').removeClass('active');
			$('#main_video .iframe').html('');
			$('#main_video .close').hide();
			$('.main-video-substrate').removeClass('hide');
