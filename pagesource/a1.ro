	var col2Scrollable = $("article").length && $(".in-scroll .mb-20").length;
	if ( col2Scrollable ){
		$(".in-scroll .mb-20:last").addClass( "a1-admodule" );
		$(".a1-admodule").css( "margin-bottom", "0px" );

		var topSpan = $("article").offset().top;
		var topsHeight = $(".a1-admodule").offset().top - topSpan;
		var articleHeight = $('article').height();
		var bannerHeight = $('.a1-admodule').height();
		var scrollSpan = articleHeight - bannerHeight;
	}

	$(window).scroll(function () {

		var leaderSpan = $('.first-lead').offset().top;
      	if ($(window).scrollTop() >= leaderSpan - 10) {
          	$('.first-lead div.col-md-12').addClass('fixed');
      	} else {
          	$('.first-lead div.col-md-12').removeClass('fixed');
      	}

	    if ( col2Scrollable ){

			var articleHeight = $('article').height();
			var scrollSpan = articleHeight - bannerHeight;
			var trigger = $(window).scrollTop() - topSpan - bannerHeight;

			if ($(window).scrollTop() >= topSpan + topsHeight) {
	          	$('.a1-admodule').addClass('fixed');
	      	} else {
	          	$('.a1-admodule').removeClass('fixed');
	      	}
	      	if (trigger >= scrollSpan - bannerHeight) {
	          	$('.a1-admodule').addClass('bottom');
	      	} else {
	          	$('.a1-admodule').removeClass('bottom');
	      	}
	      	$('.in-scroll').css('height',articleHeight);

	    }

	});
	var inTimer = function() {
	  	$('.first-lead').removeClass('fixer');
	  	console.log("fixer removed");
  	}
  	var goTimer = function() {
  		setTimeout(inTimer, 2000);
    }
    goTimer();
}
</script>




