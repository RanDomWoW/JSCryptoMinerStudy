                var m = h.getDate();
                var j = "";
                var g = "";
                var l = "";
                var f = 0;
                var n = 0;
                var k = 0;
                var clsname = "odd";
                $.each(i, function (o, p) {
                    l = "";
                    f++;
                    $.each(p, function (r, s) {
                        var q = s.NgayDau.substring(0, 2);
                        n++;
                        if (n % 2 == 0)
