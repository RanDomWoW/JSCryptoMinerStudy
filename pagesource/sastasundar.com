<script type="text/javascript">
function click_quickview(prodId)
{
	$('#prodId').click();
	
}
function quick_view(prodId)
{
	$('#non_otc_description').hide();
	$('#otc_prod_description').hide();
	$('#go_to_details').hide();
	$('#uses_div').hide();
	$.ajax({
			type: 'POST',
			url: 'https://www.sastasundar.com/index.php/category/quick_view/'+prodId,
			data: { },
			success: function(result){
				var obj = jQuery.parseJSON(result);
				$('#prod_title_txt').html(obj.DisplayName);
				if(obj.ProductImage!=null){
					$('#ProductImage').attr('src','https://res.sastasundar.com/incom/images/product/small/'+obj.ProductImage);
				}
				else{
					$('#ProductImage').attr('src','https://res.sastasundar.com/incom/images/product/default_product1460098720.jpg');
				}
				
				$('#brand').html(obj.MfgGroup);
				$('#dossage').html(obj.Conversion1+' '+obj.Unit2+' / '+obj.Unit1);
				$('#price_div').html(obj.price_txt1);
				$('#saving_txt').html(obj.price_txt2);
				
				/***** Add To Cart Button *****/

				$('#add_to_cart_btn').hide();
				$('#oos_add_to_cart_btn').hide();
				if(obj.UserTypeID != 'B')
				{
					if(obj.out_of_stock == 1)
					{						
						//$('#add_to_cart_btn').addClass('oos_add_to_cart');
						$('#oos_add_to_cart_btn').attr('ProductId',obj.prodIdEncoded);
						//$('#oos_add_to_cart_btn').attr('courierable',obj.IsCourierable);
						$('#oos_add_to_cart_btn').attr('DosageAlert',obj.DosageAlert);
						$('#oos_add_to_cart_btn').attr('DosageRestriction',obj.DosageRestriction);
						$('#oos_add_to_cart_btn').show();
					}
					else
					{
						
							$('#add_to_cart_btn').attr('ButtonFor',"cart");
							$('#add_to_cart_btn').attr('HealthProfileId',"");
							$('#add_to_cart_btn').attr('ProductId',obj.prodIdEncoded);
							$('#add_to_cart_btn').attr('DosageAlert',obj.DosageAlert);
							$('#add_to_cart_btn').attr('DosageRestriction',obj.DosageRestriction);
							$('#add_to_cart_btn').attr('DisplayName',obj.ProductName);
							
							$('#add_to_cart_btn').attr('offerprice',obj.offer_price);
							$('#add_to_cart_btn').attr('iscourierable',obj.IsCourierable);
					
						$('#add_to_cart_btn').show();
					}

				}
				
				/***** Go To Details *****/
				
				$.ajax({
				type: 'POST',
				url: 'https://www.sastasundar.com/index.php/category/get_prod_URL/?productId='+prodId+'&productName='+obj.ProductName,
				data: { },
				async: false,
				success: function(result){
						$("#go_to_details").removeAttr("href");
						$("#go_to_details").attr("href",result);
						$('#go_to_details').html('&nbsp;&nbsp;&nbsp;<i class="glyphicon glyphicon-eye-open"></i> View Details');
						$('#go_to_details').show();
					}
				});
				/***** Product Description ****/
				if(obj.PrescriptionOTC != "O"){
					$('#uses_div').html('<li>Generic Name - '+obj.SaltName+'</li><li>Commonly Used for - '+obj.DiseaseSymptoms+'</li>');
					$('#uses_div').show();
					$('#non_otc_description').show();					
					$('#Snapshot').html(obj.DrugSnapshot);
					$('#Usage').html('<span style="color:#ED1C24; padding:0 5px 0 0">*</span>Store your medicines as recommended in label<p><strong class="text-primary">When to use</strong></p>'+obj.Useage+'<p><strong class="text-primary">How to use</strong></p>'+obj.DosageRecomIntakeAdvice);
					if(obj.DrugInteractions=='null' || obj.DrugInteractions==null || obj.DrugInteractions=='' )
					{
					$('#Warnings').html('<p><strong class="text-primary">Risk Warnings</strong></p>'+obj.AdverseDrugReactions+'<p><strong class="text-primary">Precautions</strong></p>'+obj.Precautions);
					}
					else
					{
						 	$('#Warnings').html('<p><strong class="text-primary">Risk Warnings</strong></p>'+obj.AdverseDrugReactions+'<p><strong class="text-primary">Precautions</strong></p>'+obj.Precautions+'<p><strong class="text-primary">Drug interaction</strong></p>'+obj.DrugInteractions);
					}
					$(".commmonTab:first").click();
					
				}else{
					if((obj.ProductShortDescription != '') && (obj.ProductShortDescription != null)){
						$('#uses_div').html('<li>'+obj.ProductShortDescription+'</li>');							
						$('#otc_prod_description').show();
						//$('#otc_prod_description').html('<p><strong class="text-primary">DESCRIPTION</strong></p>'+obj.ProductDescription);
						$('#otc_prod_description').html('<p><strong class="text-primary">DESCRIPTION</strong></p>'+obj.ProductShortDescription);
			
					}					
				}								
			}
	});
}
