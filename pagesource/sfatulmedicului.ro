<script>
	(function($){
	$(document).ready(function(){
		var speed = 300,		//fade speed
				autoswitch = true,  // Auto slider option
				autoswitch_speed = 7000; // Auto slider speed

				$('#homepage_slider_holder .slide').first().addClass('active'); // Add initial active class
				$('#homepage_slider_holder .slide').hide(); //Hide all sliders
				$('#homepage_slider_holder .active').show(); //show active slide
				$('#homepage_slider_holder #slide-next').on('click',nextSlide); //Slide next
				$('#homepage_slider_holder #slide-prev').on('click',prevSlide); //Slide prev

				//Autoslider Handler
				if(autoswitch == true){
					setInterval(nextSlide,autoswitch_speed);
				}

				//Switch to next slide
				function nextSlide(){
					$('#homepage_slider_holder .active').removeClass('active').addClass('oldActive');
						if($('#homepage_slider_holder .oldActive').is(':last-child')){
							$('#homepage_slider_holder .slide').first().addClass('active');
						} else {
							$('#homepage_slider_holder .oldActive').next().addClass('active');
						}
					$('#homepage_slider_holder .oldActive').fadeOut(speed).removeClass('oldActive');
					$('#homepage_slider_holder .active').fadeIn(speed);
				}

				//Switch to prev slide
				function prevSlide(){
					$('#homepage_slider_holder .active').removeClass('active').addClass('oldActive');
						if($('#homepage_slider_holder .oldActive').is(':first-of-type')){
							$('#homepage_slider_holder .slide').last().addClass('active');
						} else {
							$('#homepage_slider_holder .oldActive').prev('div').addClass('active');
						}
					$('#homepage_slider_holder .oldActive').fadeOut(speed).removeClass('oldActive');
					$('#homepage_slider_holder .active').show(); //show active slide
					$('#homepage_slider_holder.active').fadeIn(speed);	
				}

	});
})(jQuery)

