<script>
    // --------上方分享扭hover

    var oldicon;
    var newicon;

    $('.top_menu .share').hover(function () {
        oldicon = $(this).children().children('img').attr("src");
        var newiconsrc = oldicon.replace(".png", "_hover.png")
        //newicon = $(this).children().children('img').attr("data-src");
        $(this).children().children('img').attr("src", newiconsrc);
    }, function () {
        $(this).children().children('img').attr("src", oldicon);
    })

    // --------第一層選單被選擇

    var aa;
    var bb = 0;

    $('#nav201707 .nav .contect .main_menu > li').hover(function () {
        // --------紅色深底線隱藏

        if ($(this).hasClass("simpleLink")) {
            if ($('#nav201707 .nav .line').hasClass("active")) {
                $('#nav201707 .nav .line').removeClass('active');
            }
        }
        else {
            $('#nav201707 .nav .line').addClass('active');

        }


        // --------箭頭動態
        $(this).children().children('img').addClass('active').parent().parent().siblings().children().children('img').removeClass('active');
        // --------底線動態


        $(this).addClass('active').siblings().removeClass('active');


        aa = $(this).index();
        var _window = $(window).width();

        if (aa < 4) {

            $('#nav201707 .nav .second_menu').show();
            $('#nav201707 .nav .second_menu .item ul').eq(aa).show().siblings().hide();
            $('#nav201707 .nav .second_menu .item ul').eq(aa).children('li').eq(bb).addClass('active');
            if (_window > 1024) {
                $('#nav201707 .nav .third_menu').show();
                $('#nav201707 .nav .overlay').show();
                $('#nav201707 .nav .third_menu .item > div').eq(aa).show().siblings().hide();
                $('#nav201707 .nav .third_menu .item > div').eq(aa).children('ul').eq(bb).show();
            }


            // --------第二層選單被選擇

            $('#nav201707 .nav .second_menu .item > ul > li').hover(function () {
                $(this).addClass('active').siblings().removeClass('active');
                bb = $(this).index();
                $('#nav201707 .nav .third_menu .item > div').eq(aa).show().siblings().hide();
                $('#nav201707 .nav .third_menu .item > div').eq(aa).children('ul').eq(bb).show().siblings().hide();
            }
                , function () {
                })
        } else {
            $('.nav .overlay').hide();
            $('#nav201707 .nav .second_menu').hide();
            $('#nav201707 .nav .third_menu').hide();
        }
    }, function () {
        if (aa > 3) {
            $(this).removeClass('active');
        }
    })

    // --------離開選單

    $('#nav201707 .nav .second_menu').hover(function () {
    }, function () {
        $(this).hide().children().children().children().children('ul').hide();
        $('#nav201707 .nav .second_menu .item ul li').removeClass('active')
        $('#nav201707 .nav .contect .main_menu li img').removeClass('active');
        $('#nav201707 .nav .line').removeClass('active');
        $('#nav201707 .nav .contect .main_menu > li').removeClass('active');
        $('.nav .overlay').hide();
        aa = 0;
        bb = 0;
    });

    $('#nav201707 .nav .noArrow').mouseover(function () {
    }, function () {
        $('.nav .overlay').hide();
    });

    $('#editorCollectMenuButton').mouseover(function () {
    }, function () {
        $('.nav .overlay').show();
    });

    $('#nav201707 .top_menu').mouseover(function () {
    }, function () {
        $('#nav201707 .nav .overlay').hide();
        $('#nav201707 .nav .second_menu').hide();
        $('#nav201707 .nav .third_menu').hide();

        $('#nav201707 .nav .line').addClass('active');
        $('#nav201707 .nav .line').css("display", "block");
        $('#nav201707 .nav .line').css("margin", "0");
        $('#nav201707 .nav .contect .main_menu > li').removeClass('active');


    });



    $('#nav201707 .nav .second_menu .closeforsecondMenu').click(function () {
        $('.second_menu').hide();
        $('.line').removeClass('active');
        $('.main_menu li').removeClass('active').children().children('img').removeClass('active');
    })












    // --------卷軸往下NAV固定

    var fixed;

    $(window).scroll(function () {
        fixed = $(window).scrollTop();
        if (fixed > 0) {
            $('#nav201707 .nav').addClass('active').children('.line').addClass('active').siblings('.contect').addClass('active').children('.top_menu').addClass('active').siblings('.logo').addClass('active');
        } else {
            $('#nav201707 .nav').removeClass('active').children('.line').removeClass('active').siblings('.contect').removeClass('active').children('.top_menu').removeClass('active').siblings('.logo').removeClass('active');
        }
    })
</script>


