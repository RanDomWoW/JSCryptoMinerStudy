            $("#divFooter").corner("top 8px");
            $(".divToolTip-Title").corner("5px");

            hu = window.location.search.substring(1);
            gy = hu.split("&");
            for (i = 0; i < gy.length; i++) {
                ft = gy[i].split("=");
            }
            if(ft[0].toLowerCase() == 'news_id'){
                var newsid = $("#NewsId").val();
                if (newsid != 'newsid') {
                    ShowNews(newsid);
                }
            }

            var PageName;
            if (typeof urlParams["MType"] === 'undefined') {
                PageName = '';
            } else {
                if (urlParams["MType"].toLowerCase() == "nomobile") {
                    PageName = 'MType=NoMobile&';
                } else {
                    PageName = '';
                }
            };

            $.ajax({
                url: 'Modules/NewsPaper/NewsPaperThumbData.aspx?' + PageName + 'NpType=' + $("#IsVijeh").val() + '&NPN_Id=' + $("#NPNId").val(),
                beforeSend: function (data) {
                    $("#divNewsPaperThumb").html("");
                },
                success: function (data) {
                    $("#divNewsPaperThumb").html(data);
                    $("#ThumbNail").val("1");
                }
            });

        });

        $("#btnThumbNail").click(function () {
            var divOverlay = document.getElementById('OverlayBox');
            divOverlay.style.display = 'block';
            $("#divThumbNail").show("slow");
        });

        $("#btnCloseThumbNail").click(function () {
            $("#divThumbNail").hide(1000);
            var divOverlay = document.getElementById('OverlayBox');
            divOverlay.style.display = 'none';
            $("#ThumbNail").val("0");
        });

        $(document).click(function (event) {
            if (!$(event.target).closest('#divThumbNail').length && $("#ThumbNail").val() == "1") {
                //if ($('#divThumbNail').is(":visible")) {
                //}
                $("#divThumbNail").hide();
                var divOverlay = document.getElementById('OverlayBox');
                divOverlay.style.display = 'none';
                $("#ThumbNail").val("0");
            }
        });

        var urlParams;
        (window.onpopstate = function () {
            var match,
                pl = /\+/g,  // Regex for replacing addition symbol with a space
                search = /([^&=]+)=?([^&]*)/g,
                decode = function (s) { return decodeURIComponent(s.replace(pl, " ")); },
                query = window.location.search.substring(1);

            urlParams = {};
            while (match = search.exec(query))
                urlParams[decode(match[1])] = decode(match[2]);
        })();
    </script>
