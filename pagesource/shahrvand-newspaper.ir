<script type="text/javascript">
    function ShowMenu(item, desc) {
        var divOverlay = document.getElementById('OverlayBox');
        divOverlay.style.display = 'block';
        document.getElementById("TopMenuBox_lblTitle").innerHTML = desc;
        var divInfo = document.getElementById('divinformation');
        divInfo.style.display = 'block';
        ShowMenuItem(item);
        parent.scrollTo(0, 0);
    }

    function HideInfo() {
        var divInfo = parent.document.getElementById('divinformation');
        divInfo.style.display = 'none';
        var divOverlay = document.getElementById('OverlayBox');
        divOverlay.style.display = 'none';
        parent.scrollTo(0, 0);
        $("#divInfoData").html("");
    }

    function ShowMenuItem(menu) {
        var PageName = '';
        if (typeof urlParams["MType"] === 'undefined') {
            //PageName = '';
            var url = $(location).attr('href').split("/").slice(3, 4).toString().toLowerCase();
            if (url == 'news:nomobile') {
                PageName = '?MType=NoMobile';
            } else if (url == 'news:mobile') {
                PageName = '';
            }
        } else {
            if (urlParams["MType"].toLowerCase() == "nomobile") {
                PageName = '?MType=NoMobile';
            } else {
                PageName = '';
            }
        };

        var target;
        var pagetype = $("#srcname").val();
        target = '/Modules/Information/' + menu + '.aspx' + PageName;
        $("#divInfoData").show();
        $.ajax({
            url: target,
            beforeSend: function (data) {
                $("#divInfoData").hide();
                $(".divInfoLoading").show();
            },
            success: function (data) {
                $("#divInfoData").html(data);
                $(".divInfoLoading").hide();
                $("#divInfoData").show();
            }
        });
    }
</script>

