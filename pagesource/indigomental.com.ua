	<script type="text/javascript">
		if ($('.template.free_lesson')[0]) {
					var $loc = null;
			var city;
			
			for (var i = 0; i < $loc.length; i++) { 
				$(".towns select.select-lesson") 
		            .append($("<option></option>")
		            .attr("value",$loc[i]['email'])
		            .text($loc[i]['city']));	
			}
			// for (var i = 0; i < $loc.length; i++) { 
			// 	$(".towns select.select-district") 
		 //            .append($("<option></option>")
		 //            .attr("value",$loc[i][''])
		 //            .text($loc[i]['city']));	
			// }
			$('body').on('click', '.towns .select-lesson .current', function(e) {
				$('.select-lesson ul [data-value="Город"]').remove();
			});
			$('body').on('click', '.towns .select-lesson ul li', function(e) {
				e.preventDefault();
				
				city = $(this).text();
				$(".area .select-lesson ul li").remove();
				$(".district .select-district ul li").remove();
				$(".area .select-lesson .current").text('Район');
				$(".time .select-lesson .current").text('Время');
				var flaq=0;
				$(".district .nice-select").css("display","block");
				for (var i = 0; i < $loc.length; i++) {
					if (city == $loc[i]['city'] ) {
						for (var j = 0; j < $loc[i]['region'].length; j++) {
							$(".area .select-lesson ul ")
					            .append($("<li class='option'></li>")
					            .attr("data-value", $loc[i]['region'][j]['region_title'])
					            .text($loc[i]['region'][j]['region_title']));
					        if ($loc[i]['region'][j]['district']) {
					        	flaq=1;
						        $(".district .select-district ul ")
						            .append($("<li class='option'></li>")
						            .attr("data-value", $loc[i]['region'][j]['district'])
						            .text($loc[i]['region'][j]['district']));
							}
						}
						if (!flaq) {
							$(".district .nice-select").css("display","none");
						}
						
					}
				}
			});	
			$('body').on('click', '.area .select-lesson ul li', function(e) {
				e.preventDefault();
				region = $(this).text();
				$(".time .select-lesson ul li").remove();
				$(".time .select-lesson .current").text('Время');
				for (var i = 0; i < $loc.length; i++) {
					for (var j = 0; j < $loc[i]['region'].length; j++) {
						if (region == $loc[i]['region'][j]['region_title'] ) {
							for (var k = 0; k < $loc[i]['region'][j]['full_time'].length; k++) {
								$(".time .select-lesson ul ")
					            .append($("<li class='option'></li>")
					            .attr("data-value", $loc[i]['region'][j]['full_time'][k]['hour'] + " " + $loc[i]['region'][j]['full_time'][k]['days'])
					            .text($loc[i]['region'][j]['full_time'][k]['hour'] + " " +$loc[i]['region'][j]['full_time'][k]['days']));
							}
						}
					}
				}
			});	
		$('.template.free_lesson .block-form').change(function(){
			var c = $('.towns .select-lesson li.selected').text();
			var e = $('.towns .select-lesson li.selected').attr('data-value');
			var r = $('.area .select-lesson li.selected').text();
			var t = $(".time .select-lesson li.selected").text();
			$('.city input').attr('value', c+", "+r+", "+t);
			$('.emailcity input').attr('value', e);
		});
	    }
	</script>
		
