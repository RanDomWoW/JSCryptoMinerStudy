				switch (display_on) {
					case 'scroll_up':
						onScroll = function () {
							if ($(document).scrollTop() >= $(document).height() / 2) {
								$(window).off('scroll', onScroll);
								showLightbox();
							}
						};
						$(window).scroll(onScroll);
