            <div class="sk-bounce2"></div>
            <div class="sk-bounce3"></div>
        </div>
    </body-content>

    <!-- Check browser support -->
    <script type="text/javascript">
        // Test if modernBrowser
        var modernBrowser = (function () {
            'use strict';
            try {
                var xhr = new XMLHttpRequest();
                return 'withCredentials' in xhr;
            } catch (error) {
                return false;
            }
        })();

        // Test if promises are supported
        var promiseSupport = false
        if(typeof Promise !== 'undefined' && Promise.toString().indexOf('[native code]') !== -1) {
            promiseSupport = true
        }

        var currentUser;
        var hasAuth = localStorage.getItem('ngStorage-hasAuthenticated');
        var needsAuth = document.location.href.indexOf('home') > -1;

        if (modernBrowser && promiseSupport && (hasAuth || needsAuth) && JSON.parse(hasAuth)) {
            currentUser = new Promise(function (resolve, reject) {
                var xhr = new XMLHttpRequest();

            var apiUrl = 'https://api.projectcamp.us/api/v1';
            if (window.location.hostname === 'staging.projectcamp.us') apiUrl = 'https://projectcampus-staging.herokuapp.com/api/v1';
            if (window.location.hostname === 'localhost') apiUrl = 'http://localhost:4000/api/v1';

                xhr.open('GET', apiUrl + '/users/me?isSlug=true', true);
                xhr.withCredentials = true;
                xhr.onload = function () {
                    if (this.status >= 200 && this.status < 300) {
                        resolve(xhr.response);
                    } else {
                        reject({
                            status: this.status,
                            statusText: xhr.statusText
                        });
                        currentUser = 'failed';
                    }
                };
                xhr.onerror = function () {
                    reject({
                        status: this.status,
                        statusText: xhr.statusText
                    });
                };
                xhr.send();
            });
        };

        if (!modernBrowser) {
            document.getElementById('boot-spinner').style.display = 'none';
            document.getElementById('body').innerHTML += '<div id="browser-warning">'+
                '<h3>Your browser seems a bit old.</h3>'+
                '<p>Please <a href="http://browsehappy.com/" target="_blank">upgrade</a> or use another browser to access Projectcampus.</p>'+
                '<p>We recommend <a href="http://www.google.com/chrome/" target="_blank">Chrome</a> or <a href="https://www.mozilla.org/firefox" target="_blank">Firefox</a>.</p>'+
                '<p>Please <a href="mailto:support@shareworks.nl">contact us</a> if you keep having this problem.</p>'+
                '</div>';
        };
    </script>

