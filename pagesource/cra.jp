        function loopsliderPosition(){
            jQuery('#loopslider_wrap').css({left:'0'});
            jQuery('#loopslider_wrap').stop().animate({left:'-' + (loopWidth) + 'px'},80000,'linear');
            setTimeout(function(){
                loopsliderPosition();
            },80000);
        };
 
        jQuery('#loopslider_wrap ul').clone().appendTo('#loopslider_wrap');
    });

    // footer tab
    jQuery('.footer-tab li').click(function() {
        var index = jQuery('.footer-tab li').index(this);
        jQuery('.content-inner').css('display','none');
        jQuery('.content-inner').eq(index).fadeIn();
        jQuery('.footer-tab li').removeClass('select');
        jQuery(this).addClass('select')
    });
});
