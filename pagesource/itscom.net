<script type="text/javascript">
$(function() {
	    var showFlag = false;
	    var slideUpWrap = $('#slideUpWrap');
	    slideUpWrap.css('bottom', '-135px');
	    var showFlag = false;
	    $(window).scroll(function () {
	        if ($(this).scrollTop() > 450) {
	            if (showFlag == false) {
	                showFlag = true;
	                slideUpWrap.stop().animate({'bottom' : '0'}, 500);
									$('#container').css("padding-bottom", "100px");
	            }
	        } else {
	            if (showFlag) {
	                showFlag = false;
	                slideUpWrap.stop().animate({'bottom' : '-135px'}, 500);
	            }
	        }
	    });
	    $('#slideCloseBtn').click(function () {
	        $('#slideUpBox').animate({'bottom' : '-135px'}, 500);
					$('#slideUpBox_small').animate({'bottom' : '0'}, 500);
	    });
			$('#slideUpBox_small').click(function () {
	        $('#slideUpBox').animate({'bottom' : '0'}, 500);
					$('#slideUpBox_small').animate({'bottom' : '-66px'}, 500);
	    });
	});
</script>
