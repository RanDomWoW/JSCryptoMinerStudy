		<script language="JavaScript">
		function LTrim(s)
		{ 
			for(var i=0;i<s.length;i++)
			{
			if(s.charAt(i)!=" ")
			{
				return s.substr(i,s.length-i);
			}
		}
		return "";
		}
		function RTrim(s)
		{ 
			for(var i=s.length-1;i>=0;i--)
			{
			if(s.charAt(i)!=" ")
			{
				return s.substr(0,i+1);
			}
		}
		return "";
		}
		function Trim(s)
		{
			return  RTrim(LTrim(s));
		}
		
