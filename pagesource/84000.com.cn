function ValidateData()
{
	var msg;
	var ok = 0; //0：验证通过；其余：不通过 
	for (i=0;i<document.forms['Form1'].elements.length;i++)
	{	
		var obj = document.forms['Form1'].elements(i);
		if(obj.id!="")
		{
			var validateflag = obj.getAttribute("validateflag",false);
			if((obj.style.visibility!="hidden") && (obj.disabled==false) && (obj.getAttribute("readonly") == false) &&(validateflag!=null) )
			{
				switch (validateflag) 
				{
					case "1" :   //验证是否为空
						if(obj.value =="") 
						{
							obj.style.border = "2px groove pink";
							msg = '请在边框为红色的域输入信息!'
							ok = 1;
						}
						break;
				   case "2" :	// 验证是否为数字
						if (!ValidateNumeric(obj.value))
						{
							obj.style.border = "2px groove blue";
							//obj.focus();
							msg = '请在边框为蓝色的域输入数字!';
							ok = 2;
						}
						break;
				}
				if (ok > 1) //除了＂为空＂的验证外，都先逐个提示用户
				{
					break;
				}
			}
		}
	}
	if(ok > 0)
	{
		alert(msg);
		return false;
	}
	return true;
} 
//功能：判断是否为数字
//参数：值
function ValidateNumeric(value)
{
	var re = /^[0-9]+$/;
	return re.test(value);
}
