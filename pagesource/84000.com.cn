		function preLineName(lineName) {
		var reg1 = /(\D+)(\d+)(路)(区间)*/g;
		var reg2 = /(\d+)(路)(区间)*/g;
		
		lineName = Trim(lineName);
		
		if(lineName.match(reg1)) {
			return encodeURI(lineName.replace(reg1, "$1$2$3$4"));
		}
		
		else if(lineName.match(reg2)) {
			return encodeURI(lineName.replace(/(\d+)(路)(区间)*/g, "$1$3"));
		}
		
		return encodeURI(lineName);
	}

