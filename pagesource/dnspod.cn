    }
  , changebg: function(nextPos, arr){
      var color,classname;
      if(/^#\w*/.test(arr[nextPos])){
        color = arr[nextPos];
      }else{
        var opt = arr[nextPos].split('#');
        classname = opt[0] ? opt[0] : 'origin';
        color = opt[1] ? ('#'+opt[1]) : '#4fa6e7';
