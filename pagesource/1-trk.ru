<script type="text/javascript">
$(document).ready(function() {
	$(".various").fancybox({
		maxWidth	: 800,
		maxHeight	: 600,
		fitToView	: false,
		width		: '70%',
		height		: '70%',
		autoSize	: false,
		closeClick	: false,
		openEffect	: 'none',
		closeEffect	: 'none'
	});
});
</script>
<script>
$(".user_form").on("submit", function(event) {
  	event.preventDefault();
  	var errName = false;
  	var errEmail = false;
  	var nameForm = $("input[name='call_name']").val();
  	var textMessage = '<div style="margin:25px; font-family:Verdana; fon-size: 12px;" align="center"><b>'+ nameForm + ', Ваш отзыв принят.<br>После проверки он будет размещен на сайте.</b></div>';
  	$("input[type='text']").css("border", "1px solid #797979");
  	$("input[type='email']").css("border", "1px solid #797979");
  	
  	if($("input[name='call_name']").val() == '') {
  		errName = true;
  		$("input[name='call_name']").css("cssText", "border: 1px solid red!important");
  		alert("Пожалуйста, представьтесь");
  		return false;
  	} else {
  		errName = false;
  	}
  	if($("input[name='adress']").val() == '') {
  		errEmail = true;
  		$("input[name='adress']").css("cssText", "border: 1px solid red!important");
  		alert("Укажите Ваш email");
  		return false;
  	} else {
  		errEmail = false;
  	}
  	if(errName == false && errEmail == false) {
  		var currentForm = $(this);
	    var str = currentForm.serialize();
	    
	    $.ajax({
	      type: "POST",
	      url: "/php_mail/mail.php",
	      data: str,
	      success: function(data){
	        if(data != "TRUE") {
	          $(currentForm).append(data);
	        } else {
	          $(currentForm).html(textMessage);
	        }
	      }
	    });
  	}
  });
</script>
<script>
var jcarousel = $('.jcarousel');
        jcarousel
            .on('jcarousel:reload jcarousel:create', function () {
                var carousel = $(this),
                    width = carousel.innerWidth();

            })
            .jcarousel({
            	wrap: 'circular',
            	visible: 6,

                
            });

        $('.projects-images-nav-prev')
            .jcarouselControl({
                target: '-=1'
            });

        $('.projects-images-nav-next')
            .jcarouselControl({
                target: '+=1'
            });
</script>
