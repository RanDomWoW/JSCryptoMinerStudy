	<script language="javascript">

function disp(e)

{

var left=0;

var top=0;

var beg=0;

var het=parseInt(document.body.clientHeight);

if (document.documentElement && document.documentElement.scrollTop)

	beg=parseInt(document.documentElement.scrollTop)

else if (document.body)

	beg=parseInt(document.body.scrollTop);

left=parseInt(document.getElementById(e.id).offsetLeft)+400;

top=parseInt(document.getElementById(e.id).offsetTop)+parseInt(document.getElementById(e.id).offsetHeight)+513;

document.getElementById("s"+ e.id).style.display='';

document.getElementById(e.id).style.fontSize=18.0;

var bhet=parseInt(document.getElementById("s"+ e.id).offsetHeight);

if (bhet>=het)

top=beg;

else if ((top+bhet) < (beg+het))

top=top;

else if (top-bhet > beg)

top=top-bhet;

else

top=beg;

window.status=top + ";;;" + left

document.getElementById("s"+ e.id).style.left=left.toString() + "px";

document.getElementById("s"+ e.id).style.top=top.toString() + "px";



}

function hide(e)

{

document.getElementById(e.id).style.fontSize=13.0;

document.getElementById("s"+e.id).style.display='none';

}



</script>

