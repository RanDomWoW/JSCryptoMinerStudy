<script type="text/template" id="comment-template">
<div class="comment" data-id="{{=comment.comment.id}}">
   <!-- main -->
   <div class="main">
      <div class="from">
					{{ _(comment.itemsFrom).each(function(item){ }}
						<img style="width: 75px; height: 55px;" class="image" src="https://upgrade.gg/static/imgs/pre-skin-min.png" data-src="{{=item.image}}">
					{{ }); }}
      </div>
      <div class="arrow"></div>
      <div class="to">
					{{ _(comment.itemsTo).each(function(item){ }}
						<img style="width: 75px; height: 55px;" class="image" src="https://upgrade.gg/static/imgs/pre-skin-min.png" data-src="{{=item.image}}">
					{{ }); }} 
			</div>
   </div>
   <!-- bottom -->
   <div class="title bottom">
   	<div class="profit">
   		<i class="diamond icon"></i>Profit: +${{= comment.comment.profit}}
   	</div>
   	<div class="thumbs-up">
   		{{= comment.comment.likes}} likes<i class="thumbs outline up icon"></i>
   	</div>
   </div>
   <!-- hover -->
   <div class="hover">
      <div class="user-pane">
         <div class="image" style="background-image: url({{= comment.user.avatar}});">
						<div class="like-icon" data-id="{{= comment.comment.id}}">
    					<i class="thumbs up icon"></i>
  					</div>
				 </div>
         <div class="text">
            <span class="name">{{= comment.user.username}}</span>
            <span class="comment-text">{{= comment.comment.text}}</span>
         </div>
      </div>
   </div>
</div>
</script>

