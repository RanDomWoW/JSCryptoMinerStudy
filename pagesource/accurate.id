</head>
<body class="metro" style="display: none;">
	<script type="text/javascript">
	jQuery(function($){
		app.addStaticParam('1230',null);
		app.init(['/js/base/BaseClass.js',
		    '/js/base/BaseFormClass.js',
            '/js/base/BaseFeedbackClass.js']);
	});
	</script>
	<div id="main" class="container fg-white">
		<div class="grid fluid">
			<div class="row visible-md-block visible-lg-block">
				<div class="span12">&nbsp;</div>
			</div>
			<div class="row visible-md-block visible-lg-block">
				<div class="span12">&nbsp;</div>
			</div>
			<div class="row visible-md-block visible-lg-block">
				<div class="span12">&nbsp;</div>
			</div>
			<script type="text/javascript">
	jQuery(function($){
		app.initialized.subscribe(function(newValue) {
			app.loadModel('login');
		});
		app.registerInitCallback('login', function(self) {
			self._init({
				login: {
					email: ''
				}
			});
		});
	});	
	</script>
	<!-- USER-LOGIN -->
			<div class="row visible-xs-block visible-sm-block">
		<div class="col-xs-12 padding20 text-center">
			<img src="/images/accurate-logo.svg?" style="width: 100%">
		</div>
	</div>
	<div id="login" class="row">
		<div class="col-md-7 visible-md-block visible-lg-block padding20 text-center">
			<h1>
				<img src="/images/accurate-logo.svg?" style="width: 100%">
			</h1>
			<br/>
			<img src="/images/web-tagline.svg?" style="width: 100%"/>
		</div>
		<div class="col-xs-12 col-md-5 padding20 login-background">
						<form action="/auth.do" method="post">
				<fieldset>
					<label>Email</label>
					<input type="text" name="email" class="fg-black" placeholder="Akun Email Anda" value="" data-bind="metroTextfield: {}, value: login.email"/>
					<label>Password</label>
					<input type="password" name="password" class="fg-black" placeholder="Password Akun Anda" data-bind="metroTextfield: {}, value: login.password"/>
				</fieldset>
	
				<div class="row">
					<div class="span6">
						<button name="btnForgot" type="button" style="color: #fff !important;" data-bind="metroButton: {type: 'link'}, click: showForgot">Lupa Password?</button>
					</div>
					<div class="span6 text-right">
						<button name="btnRegister" type="button" data-bind="metroButton: {type: 'warning'}, click: showRegister">Daftar Akun Baru</button>
					</div>
				</div>
				<div class="row">
					<div class="span12 text-center">
						<button name="btnLogin" type="submit" style="width: 98%;" data-bind="metroButton: {size: 'large', type: 'primary'}">Login</button>
					</div>
				</div>
			</form>
			<div class="row">
				<div class="span6 text-center">
					<form action="/auth/facebook" method="POST">
					   	<button class="socialBtn facebook" type="submit" style="width: 96%; text-align: center;">
					   	Login Facebook						</button>
					</form>
				</div>
				<div class="span6 text-center">
					<form action="/auth/google" method="POST">
					    <button class="socialBtn google" type="submit" style="width: 96%; text-align: center;">
						Login Google						</button>
					</form>
				</div>
			</div>
		</div>
		
		<div id="forgot-form" title="Form Reset Password" data-bind="metroDialog: {}">
			<div class="container" style="width: 20em !important;">
				<div class="grid fluid">
					<div class="row">
						<div class="span12">
							<form data-bind="submit: submitForgot">
								<fieldset>
									<label>Alamat Email</label>
									<input type="text" name="email" placeholder="Alamat email Anda" data-bind="metroTextfield: {}, value: forgot.email"/>
								</fieldset>
								
								<div class="text-right">
									<button name="btnSumbitForgot" type="submit" data-bind="metroButton: {size: 'large', type: 'success'}">Kirim</button>
								</div>
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
	
		<div id="register-form" title="Daftar Akun Baru" data-bind="metroDialog: {}">
			<div class="container" style="max-width: 610px !important;">
				<div class="grid fluid">
					<form data-bind="submit: submitRegister">
					<div class="row no-margin">
						<div class="col-sm-12 no-padding">
							<label>Lengkapi form pendaftaran akun baru dibawah ini:</label>
						</div>
					</div>
					<div class="row">
						<div class="col-sm-12 col-md-6">
							<input type="text" name="name" placeholder="Nama lengkap" data-bind="metroTextfield: {}, value: register.name"/>
						</div>
						<div class="col-sm-12 col-md-6">
							<input type="text" name="nickName" placeholder="Nama panggilan" data-bind="metroTextfield: {}, value: register.nickName"/>
						</div>
						<div class="col-sm-12 col-md-6 text-center padding5">
							<label>
								<input type="radio" name="male" value="true" data-bind="metroRadio: {}, checked: register.male"/>
								Laki-Laki							</label>
							<label>
								<input type="radio" name="male" value="false" data-bind="metroRadio: {}, checked: register.male"/>
								Perempuan							</label>
						</div>
						<div class="col-sm-12 col-md-6">
							<input type="text" name="email" placeholder="Email, Contoh: abc@gmail.com" data-bind="metroTextfield: {}, value: register.email"/>
						</div>
						<div class="col-sm-12 col-md-6">
							<input type="password" name="password" placeholder="Password / kata sandi" data-bind="metroTextfield: {}, value: register.password"/>
						</div>
						<div class="col-sm-12 col-md-6">
							<input type="password" name="verifyPassword" placeholder="Ketik ulang password / kata sandi" data-bind="metroTextfield: {}, value: register.verifyPassword"/>
						</div>
						<div class="col-sm-12 col-md-6">
								<a href="#" tabindex="-1" onclick="$(this).find('img').attr('src','/captcha/' + Math.round(Math.random()*10000) + '.jpg');$('input[name^=captcha]').val('');return false;"><img id="captcha_img" src="/captcha/0.7777485351078232.jpg" style="width: 100%; min-height: 1px; height: auto;" title="Klik untuk Perbarui Captcha"/></a>
						</div>
						<div class="col-sm-12 col-md-6">
							<input type="text" name="captcha" placeholder="Kode pengaman disebelah kiri / atas" data-bind="metroTextfield: {}, value: register.captcha"/>
						</div>
						<div class="col-xs-12 padding10">
							<label>
								<input type="checkbox" name="agreeTerms" value="true" data-bind="metroCheckbox: {}, checked: register.agreeTerms">
								Saya setuju dengan semua <a href="http://cpssoft.com/aol-user-toc" target="_blank">aturan dan kondisi yang berlaku</a>
							</label>
						</div>
						<div class="col-xs-12 text-right">
							<button name="btnRegisterNewAccount" type="submit" style="width: 100%;" data-bind="metroButton: {size: 'large', type: 'success'}">Daftar Akun Baru</button>
						</div>
					</div>
					</form>
				</div>
			</div>
		</div>
	</div>
		</div>
	</div>
	<div id="panel-result" style="display: none;"></div>
	<div id="error-dialog" title="Terjadi Permasalahan pada Pemrosesan" data-bind="metroDialog: {icon: 'icon-warning'}" style="display:none;">
	<div class="container" style="width: auto !important;min-width: 400px !important; max-width: 800px !important;">
		<div class="grid fluid no-margin">
			<div class="row no-margin">
				<div class="span12">
					<table>
					<tr>
						<td style="vertical-align: top;">
							<div class="tile half bg-red inline">
								<div class="tile-content icon">
									<i class="icon-cancel-2"></i>
								</div>
							</div>
						</td>
						<td>
							<label class="no-margin">Silakan perbaiki permasalahan berikut ini:</label>
							<ul class="no-margin" data-bind="css: { unstyled: messages().length == 1 }">
								<!-- ko foreach: messages -->
								<li><label class="fg-crimson" data-bind="text: $data"></label></li>
								<!-- /ko -->
							</ul>
						</td>
					</tr>
					</table>
				</div>
			</div>
		</div>
	</div>
	<!-- ko if: buttons().length > 0 -->
	<div class="button-container text-right">
		<!-- ko foreach: buttons -->
		<button type="button" data-bind="metroButton: config, text: label, click: handler"></button>
		<!-- /ko -->
	</div>
	<!-- /ko -->
</div>
	<div id="info-dialog" title="Informasi" data-bind="metroDialog: {shadow: true, icon: 'icon-info'}" style="display:none;">
	<div class="container" style="width: auto !important;min-width: 400px !important; max-width: 800px !important;">
		<div class="grid fluid no-margin">
			<div class="row no-margin">
				<div class="span12">
					<table>
					<tr>
						<td style="vertical-align: top;">
							<div class="tile half bg-lightBlue">
								<div class="tile-content icon">
									<i class="icon-info-2"></i>
								</div>
							</div>
						</td>
						<td>
							<ul class="no-margin" data-bind="css: { unstyled: messages().length == 1 }">
								<!-- ko foreach: messages -->
								<li><label class="fg-crimson" data-bind="text: $data"></label></li>
								<!-- /ko -->
							</ul>
						</td>
					</tr>
					</table>
				</div>
			</div>
		</div>
	</div>
	<!-- ko if: buttons().length > 0 -->
	<div class="button-container text-right">
		<!-- ko foreach: buttons -->
		<button type="button" data-bind="metroButton: config, text: label, click: handler"></button>
		<!-- /ko -->
	</div>
	<!-- /ko -->
</div>
	<div id="success-dialog" title="Proses Berhasil Dilakukan" data-bind="metroDialog: {icon: 'icon-smiley'}" style="display:none;">
	<div class="container" style="width: auto !important;min-width: 400px !important; max-width: 800px !important;">
		<div class="grid fluid no-margin">
			<div class="row no-margin">
				<div class="span12">
					<table>
					<tr>
						<td style="vertical-align: top;">
							<div class="tile half bg-green">
								<div class="tile-content icon">
									<i class="icon-checkmark"></i>
								</div>
							</div>
						</td>
						<td>
							<ul class="no-margin" data-bind="css: { unstyled: messages().length == 1 }">
								<!-- ko foreach: messages -->
								<li><label class="fg-crimson" data-bind="text: $data"></label></li>
								<!-- /ko -->
							</ul>
						</td>
					</tr>
					</table>
				</div>
			</div>
		</div>
	</div>
</div>
	<div id="confirm-dialog" title="Konfirmasi" data-bind="metroDialog: {icon: 'icon-info', sysButtons : { btnClose : false }, overlayClickClose: false}" style="display:none;">
	<div class="container" style="width: auto !important;min-width: 400px !important; max-width: 800px !important;">
		<div class="grid fluid no-margin">
			<div class="row no-margin">
				<div class="span12">
					<table>
					<tr>
						<td style="vertical-align: top;">
							<div class="tile half bg-lightBlue">
								<div class="tile-content icon">
									<i class="icon-info-2"></i>
								</div>
							</div>
						</td>
						<td>
							<ul class="no-margin" data-bind="css: { unstyled: messages().length == 1 }">
								<!-- ko foreach: messages -->
								<li><label class="fg-crimson" data-bind="text: $data"></label></li>
								<!-- /ko -->
							</ul>
						</td>
					</tr>
					</table>
				</div>
			</div>
		</div>
	</div>
	<!-- ko if: buttons().length > 0 -->
	<div class="button-container text-right">
		<!-- ko foreach: buttons -->
		<button type="button" data-bind="metroButton: config, text: label, click: handler"></button>
		<!-- /ko -->
	</div>
	<!-- /ko -->
</div>
	
