		function callback_submit() { 			var spec = $('#callback_spec option:selected');
			var form = {
				contact_fio: $("#callback_username").val(),
				contact_phone: $("#callback_phone").val(),
				target: "Запрос обратного звонка",
				speciality_id: spec.val(),
				speciality: spec.text()
			};
			$.ajax({
				type: "POST",
				dataType: "json",
				url: "/v2/Callback/add",
				data: form,
				success: function (data) {
					if (data['class']=='box ok_msg') $('#callback_form').hide();
					$('#callback_msg').attr('class', data['class']).text(data['message']).show();
				},
				error: function() {
					$('#callback_msg').attr('class', 'box error_msg').text('Ошибка отправки заявки. Попробуйте позже.').show();
				}
			});
			return false;
		}

