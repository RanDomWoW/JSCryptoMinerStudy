				if (thedate > curdate) {
					alert('Введенная вами дата: '+bd+' еще не наступила. Введите, пожалуйста, правильную дату.');
					return false;
				}
				var nm=$("input[name=xs_omsNumber]").val();
				var sv=$("input[name=xs_omsSaveToProfile]").is(":checked") ? 1 : 0;
				top.location.href='/zapis-v-polikliniky-Moskvy?autofire=1&omsNumber='+nm+'&birthDate='+bd+'&omsSaveToProfile='+sv;
				return false;
