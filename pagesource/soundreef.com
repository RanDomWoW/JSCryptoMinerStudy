var ZnThemeAjax = {"ajaxurl":"\/wp-admin\/admin-ajax.php","zn_back_text":"Indietro","zn_color_theme":"dark","res_menu_trigger":"992","top_offset_tolerance":"","logout_url":"http:\/\/www.soundreef.com\/wp-login.php?action=logout&redirect_to=http%3A%2F%2Fwww.soundreef.com&_wpnonce=25a9471b7c"};</script> <script type='text/javascript' src='http://www.soundreef.com/wp-content/themes/kallyas/js/znscript.min.js?ver=4.13.0'></script> <script type='text/javascript' src='http://www.soundreef.com/wp-content/plugins/animated-text-element//assets/js/typed.min.js?ver=1.0.0'></script> <script type='text/javascript' src='http://www.soundreef.com/wp-content/plugins/animated-text-element//assets/js/script.js?ver=1.0.0'></script> <script type='text/javascript' src='http://www.soundreef.com/wp-content/themes/kallyas/addons/slick/slick.min.js?ver=4.13.0'></script> <script type='text/javascript' src='http://www.soundreef.com/wp-includes/js/wp-embed.min.js?ver=4.7.3'></script> <!-- Zn Framework inline JavaScript--><script type="text/javascript">jQuery(document).ready(function($) {
var currentLang = $('html').attr('lang');

// -----------------------------------------

$('.sr_open_hidden_section a').bind('click', function(event){
    event.preventDefault();
    
    var sectionId = $(this).attr('href');
    var offset = -100;
    
    $('.sr_hidden_section').removeClass('opened');
    $(sectionId).slideDown().addClass('opened');
    
    $('html, body').animate({
        scrollTop: $(sectionId).offset().top + offset
    }, 1000);
});


$('.sr_close_hidden_section a').bind('click', function(event){
    event.preventDefault();
    
    var sectionId = $(this).attr('href');
    var offset = -100;
    
    $('.sr_hidden_section').removeClass('opened');

    $('html, body').animate({
        scrollTop: $(sectionId).offset().top + offset
    }, 1000);
    
});

// Icon bar fix
if($('#users_icon_bar').length > 0){
    $('#users_icon_bar .row .znColumnElement.col-md-2').each(function(){
        $(this).removeClass('col-md-2').addClass('col-md-1');
    });
}

// Audio Player (JPlayer)
if($("#sr_audio_player").length > 0){
    
    $.getScript("https://cdnjs.cloudflare.com/ajax/libs/jplayer/2.1.0/jquery.jplayer/jquery.jplayer.js", function(){
        
        var $audioLinks = $('.sr_audio_links');
        var $links = $audioLinks.find('a');
    
        $("#sr_audio_player").jPlayer({
            supplied: "mp3",
            solution: 'html',
            wmode:"window"
        });
        
        $audioLinks.on('click', 'a', function(event){
            event.preventDefault();
            
            if($(this).hasClass('playing')){
                $links.removeClass('playing');
                $links.find('.btn-element-icon').attr('data-zn_icon', ''); //Play
                $('#sr_audio_player').jPlayer("pause");
            }else{
                $('#sr_audio_player').jPlayer("pause");
                $audioLinks.find('a').removeClass('playing');
                $links.find('.btn-element-icon').attr('data-zn_icon', ''); //Play
                $(this).find('.btn-element-icon').attr('data-zn_icon', ''); //Pause
                $(this).addClass('playing');
                station = $(this).attr('href');
                $('#sr_audio_player').jPlayer("setMedia", {mp3: station}).jPlayer('play');
            }
        });
        
    });
}

// Artists carousel 

$('.kl-iosslider .more .btn').click(function(){
    var $bandDetails = $(this).closest('.kl-iosslide-caption').find('.band_details');
    var $popup = $('#artists_band_details');
    
    populateBandsPopup($bandDetails, $popup);
});

// Artists list and search

if($("#sr_artists_catalog").length > 0){
    
    $.getScript("https://cdnjs.cloudflare.com/ajax/libs/crypto-js/3.1.9-1/crypto-js.min.js", function(){
        
        var $catalog = $('#sr_artists_catalog');
        var $searchBar = $('.sr_artists_catalog_search');
        
        // Bands events
        $catalog.on('click', '.sr_content_panel .artist', function(event){
            event.preventDefault();
            var $bandDetails = $(this).find('.band_details');
            var $popup = $('#artists_band_details');
            populateBandsPopup($bandDetails, $popup);
            $catalog.find('.artist.template .name').trigger('click');
        });
        
        $catalog.on('click', '.sr_content_panel .artist a.name', function(event){
            event.preventDefault();
        });
        
        // Featured bands
        searchBands($catalog, null, 1, 1);
        
        // Bands search
        $searchBar.on('submit', 'form', function(event){
            event.preventDefault();
            
            var bandName = $(this).find('input[name="s"]').val();
            if(bandName.length >= 1){
                searchBands($catalog, bandName, 1, null);
            }else{
                searchBands($catalog, null, 1, 1);
            }
        });
    
    });
    
}

// Functions ------

function populateBandsPopup($bandDetails, $popup){
        if($bandDetails.length == 0 || $popup.length == 0){
            return;
        }
        
        $popup.find('.band_popup_name h3').text($bandDetails.find('.band_details_name').text());
        $popup.find('.band_popup_bio p').html($bandDetails.find('.band_details_bio').text());
        if($bandDetails.find('.band_details_image_url').text()){
            $popup.find('.band_popup_image img').attr('src', $bandDetails.find('.band_details_image_url').text());
            $popup.find('.band_popup_image').show();
        }else{
            $popup.find('.band_popup_image').hide();
        }
        
        var facebook = $bandDetails.find('.band_details_facebook').text();
        var twitter = $bandDetails.find('.band_details_twitter').text();
        var soundCloud = $bandDetails.find('.band_details_sound_cloud').text();
        var spotify = $bandDetails.find('.band_details_spotify').text();
        
        var $icon = $popup.find('.band_popup_social .elm-sc-icon-0');
        if(facebook){
            $icon.closest('.elm-social-icons-item').show();
            $icon.attr('href', 'http://www.facebook.com/'+facebook);
        }else{
            $icon.closest('.elm-social-icons-item').hide();
            $icon.attr('href', '#');
        }
        
        var $icon = $popup.find('.band_popup_social .elm-sc-icon-1');
        if(twitter){
            $icon.closest('.elm-social-icons-item').show();
            $icon.attr('href', 'http://twitter.com/'+twitter);
        }else{
            $icon.closest('.elm-social-icons-item').hide();
            $icon.attr('href', '#');
        }
        
        var $icon = $popup.find('.band_popup_social .elm-sc-icon-2');
        if(soundCloud){
            $icon.closest('.elm-social-icons-item').show();
            $icon.attr('href', 'http://soundcloud.com/'+soundCloud);
        }else{
            $icon.closest('.elm-social-icons-item').hide();
            $icon.attr('href', '#');
        }
        
        var $frameContent = $popup.find('.band_popup_player .band_player_frame'); 
        if(spotify){
            $frameContent.show();
            $frameContent.attr('src', '//open.spotify.com/embed/artist/' + spotify); 
        }else{
            $frameContent.hide(); 
            $frameContent.attr('src', '');
        }
}

function searchBands($catalog, bandName, page, featured){

    var action = $catalog.find('.sr_search_form').attr('action');
    var publicKey = "fc1f073fe91403f00d2219185fdea79b";
    var priKey = "62eb419db884b3175d10e4a6153262c6";
    var timestamp = Math.round(new Date().getTime()/1000);
    
    bandName = bandName || '';
    page = page || $catalog.find('.sr_search_form input[name="page"]').val();
    featured = featured;
    
    if (bandName){
        bandName = bandName.toLowerCase(); 
    }
    
    var requestData = {locale: currentLang, name: bandName, page: page, timestamp: timestamp};
    if (featured){
        requestData = {featured: featured, locale: currentLang, name: bandName, page: page, timestamp: timestamp};
    }
    
    var signature = getApiSignature(priKey, requestData);

    $.ajax({
        url: action,
        data: requestData,
        method: 'POST',
        type: 'POST',
        dataType: 'json',
        cache: false,
        headers: {
            "x-publickey": publicKey,
            "x-signature": signature,
            "Authorization": "Basic " + btoa('soundreef' + ":" + 't3stsit32017')
        },
        beforeSend: function(xhr){
            $catalog.find('.sr_loader').show();
            $catalog.find('.sr_empty_content').hide();
            $catalog.find('.sr_content_panel').hide();
        },
        error: function(data, textStatus){
            $catalog.find('.sr_loader').hide();
            $catalog.find('.sr_content_panel').show();
        },
        success: function(data){
            if(data.data.bands){
                populateBandCatalog(data, $catalog);
                $catalog.find('.sr_loader').hide();
                
                if(data.data.bands.length > 0){
                    $catalog.find('.sr_empty_content').hide();
                    $catalog.find('.sr_content_panel').show();
                }else{
                    $catalog.find('.sr_empty_content').show();
                    $catalog.find('.sr_content_panel').hide();
                }
            }
        }
    });
    
}

function populateBandCatalog(bandsData, $catalog){
    var $contentPanel = $catalog.find('.sr_content_panel').empty();
    var $artistTemplate = $catalog.find('.artist.template');
    
    for(var i=0; i < bandsData.data.bands.length; i++){
        var artist = bandsData.data.bands[i];
        
        $clonedArtist = $artistTemplate.clone().removeClass('template').show();
        $clonedArtist.find('.name').text(artist.name);
        if(artist.image){
            $clonedArtist.find('.artist_image').html('<img src="'+artist.image+'"/>');
        }
        $clonedArtist.find('.band_details .band_details_name').text(artist.name);
        $clonedArtist.find('.band_details .band_details_bio').text(artist.band_bio ? artist.band_bio : 'Info not available');
        $clonedArtist.find('.band_details .band_details_image_url').text(artist.image ? artist.image : '');
        $clonedArtist.find('.band_details .band_details_facebook').text(artist.facebook ? artist.facebook: '');
        $clonedArtist.find('.band_details .band_details_twitter').text(artist.twitter ? artist.twitter : '');
        $clonedArtist.find('.band_details .band_details_spotify').text(artist.spotify ? artist.spotify : '');
        $clonedArtist.find('.band_details .band_details_sound_cloud').text(artist.sound_cloud ? artist.sound_cloud : '');
        
        $contentPanel.append($clonedArtist);
    }
}

function getApiSignature(pkey, requestData){
    var dataString = '';
    var array = $.map(requestData, function(value, index) {
        dataString +=  index+""+value;
    });
    
    var signature = CryptoJS.HmacSHA256(dataString, pkey).toString(CryptoJS.enc.Hex);
    return signature;
}

