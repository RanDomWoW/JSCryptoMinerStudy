<script type="text/javascript">
	jQuery(document).ready(function() {
		console.log(Cookies.get('pngwfsignupignore'));
		if (typeof Cookies.get('pngwfsignupignore') == 'undefined') {
			jQuery.fancybox.open(jQuery("#newslettercontent").html(), {
				afterClose: function(e) {
					Cookies.set('pngwfsignupignore', 'ignore', { expires: 14 });
				}
			});
		}
		
		jQuery(".newsletterbtn").click(function(e) {
			var emailaddr = jQuery('input[name=subscribe_eml]').val();
			var phonenum = jQuery('input[name=subscribe_phone]').val();
			//alert('TEST' + emailaddr);
			if (emailaddr != '') {
				if (!validateEmail(emailaddr)) {
					alert('Please enter a valid email address');
				} else {
					$.ajax({
						method: "POST",
						url: "/ajax/sign-up-for-email-popup",
						data: { 'email': emailaddr, 'phone': phonenum }
					}).done(function( msg ) {
						//alert( "Data Saved: " + msg );
						var nljson = JSON.parse(msg);
						jQuery(".nlformfield").remove();
						jQuery(".nlmessagefield").html('<br/><strong style="color: green;">' + nljson.message + '</strong> <br/><br/><a href="javascript:closeNewsletter(1);" style="font-size: 0.75rem; color: #242429;">Close this popup &gt;</a>');
  					});
				}
			} else {
				alert('Please enter an email address');
			}
		})
		
		
	});
	
	function closeNewsletter(nlflag) {
		if (nlflag === 1) {
			Cookies.set('pngwfsignupignore', 'ignore', { expires: 150 });
		}
		
		jQuery.fancybox.close( 'all' );
	}
	
	function validateEmail(email) {
		var email_reg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
		return email_reg.test( email );
	}
</script>
