<script src="http://ont.by/tv/js/jquery.cookie.js"></script>
<script>

	$(document).ready(function(){

		var townN, town = $.cookie('town');

		if(!town){
			town = 0;
			$(".tspan").text("Минск");
			$.cookie('town',town, { path: '/' });
		}


		switch(town){
			case "0": townN = "Минск"; break;
			case "1": townN = "Брест"; break;
			case "2": townN = "Гомель"; break;
			case "3": townN = "Могилев"; break;
			case "4": townN = "Витебск"; break;
			case "5": townN = "Гродно"; break;
		}


		$.getJSON('http://ont.by/dev/cron/weather.xml', function(data){
			var icon = data.list[town].weather[0].icon;
			var cels = data.list[town].main.temp;
			$(".cels").text(cels+"°");
			$(".w-icon").html("<img src='http://openweathermap.org/img/w/"+icon+".png'>");
			if(icon[2]=='d'){$(".d-n").text("день");}else{$(".d-n").text("ночь");}
			$(".tspan").text(townN);
			$(".weather").show();
		});

		$(".town-select").hover(function(){$(".sto").show();}, function(){$(".sto").hide();});

		$(".sto div").bind('click', function(){
			$(".tspan").text($(this).text());
			var num = $(this).attr('data-ref');
			$.removeCookie('town', { path: '/' });
			$.cookie('town',num, { path: '/' });

			$.getJSON('http://ont.by/dev/cron/weather.xml', function(data){
				var icon = data.list[num].weather[0].icon;
				var cels = data.list[num].main.temp;
				$(".cels").text(cels+"°");
				$(".w-icon").html("<img src='http://openweathermap.org/img/w/"+icon+".png'>");
				if(icon[2]=='d'){$(".d-n").text("день");}else{$(".d-n").text("ночь");}
			});

		});

	});




</script>

 
           

            


