<script type="text/javascript">
    var currentHomeSchedule = "";
    var currentProgramLetter = "";
    var currentSport = "";
    var currentAanbiedersLetter = "";

    function chooseHomeSchedule(id) {
        //console.log("chooseHomeSchedule " + id + " was " + currentHomeSchedule);
        if (currentHomeSchedule != "") {
            $("#scli" + currentHomeSchedule).removeClass("active");
            $("#schedule" + currentHomeSchedule).hide();
        }

        currentHomeSchedule = id;
        $("#scli" + currentHomeSchedule).addClass("active");
        $("#schedule" + currentHomeSchedule).show();
        /*$("#schedule" + currentHomeSchedule).scrollTop(0);*/

        $("#schedule" + currentHomeSchedule).mCustomScrollbar("update");
        $("#schedule" + currentHomeSchedule).mCustomScrollbar("scrollTo", "top");
    }

    function chooseProgramLetter(id) {
        //console.log("chooseProgramLetter " + id + " was " + currentProgramLetter);
        if (currentProgramLetter != "") {
            $("#item" + currentProgramLetter).removeClass("active");
        }
        $(".programme-item").hide();

        currentProgramLetter = id;
        $("#item" + currentProgramLetter).addClass("active");
        $(".item" + currentProgramLetter).show();

        $("#program-scroll").mCustomScrollbar("update");
        $("#program-scroll").mCustomScrollbar("scrollTo", "top");
    }

    function chooseSport(id) {
        console.log("chooseSport " + id + " was " + currentSport);
        if (currentSport != "") {
            $("#item-sport" + currentSport).removeClass("active");
        }

        currentSport = id;
        $("#item-sport" + currentSport).addClass("active");

        if (id == "0") {
            $(".sports").show();
        } else {
            $(".sports").hide();
            $(".itemSport" + currentSport).show();
        }

        $("#sport-scroll").mCustomScrollbar("update");
        $("#sport-scroll").mCustomScrollbar("scrollTo", "top");
    }

    function chooseAanbiedersLetter(id) {
        //console.log("chooseAanbiedersLetter " + id + " was " + currentAanbiedersLetter);
        if (currentAanbiedersLetter != "") {
            $("#letAan" + currentAanbiedersLetter).removeClass("active");
        }
        $(".presenters").hide();

        currentAanbiedersLetter = id;
        $("#letAan" + currentAanbiedersLetter).addClass("active");
        $(".itemAan" + currentAanbiedersLetter).show();

        $("#aanbieder-scroll").mCustomScrollbar("update");
        $("#aanbieder-scroll").mCustomScrollbar("scrollTo", "top");
    }

    $(function() {
        chooseHomeSchedule(1);
        //chooseProgramLetter("A");
        //chooseSport(0);
        //chooseAanbiedersLetter("A");

        $("#tab1 a").click(function (e) {
            chooseHomeSchedule(1);
        });

        $("#tab2 a").click(function (e) {
            chooseProgramLetter("A");
        });

        $("#tab3 a").click(function (e) {
            chooseSport("0")
        });

        $("#tab4 a").click(function (e) {
            chooseAanbiedersLetter("A");
        });
    });
</script>

