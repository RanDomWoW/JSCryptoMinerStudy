    <script type="text/javascript">
        var _fp;
        function FooterSlidePanel(offer) {
            var isExpanded = $(".footer .active").length == 1;
            if (_fp != offer && isExpanded) {
                FooterSlidePanel(_fp)
                FooterSlidePanel(offer)
                return;
            }
            _fp = offer;

            $('.footerPopup .offer').hide();
            $('.footerPopup .lnk').hide();
            $(".footer .bk, .footer .logo").removeClass("active");

            if (offer) {
                $('.footerPopup .offer').show();
                if (!isExpanded)
                    $(".footer .bk").addClass("active");
            }
            else {
                $('.footerPopup .lnk').show();
                if(!isExpanded)
                    $(".footer .logo").addClass("active");
            }

            $('.footerPopup').slideToggle(250);
        }
        $(function () {
            $("<div class='tableBorder'></div>").insertBefore(".Tabela3");
            if ($(window).width() < 990) {
                $(".noStack").wrap("<div class='horScroll'></div>");
            }
			
			if ($(window).width() < 600) {			
                $(".slider .content-slider li").each(function (i, el) {
                    var img = $(el).css("background-image");
                    if (img.indexOf(".jpg") > 0)
                        img = img.replace(".jpg", "_m.jpg");
                    else
                        img = img.replace(".png", "_m.png");

                    $(el).css("background-image", img);                    
                });
            }

            $(".topMenu .mnu").hover(function (e) {
                $(".topMenu .sub:visible").hide(); $(this).removeClass("mnu").addClass("mnuO");
                MenuTopToggle(this)
            }, function (e) {
                $(this).removeClass("mnuO").addClass("mnu");
                setTimeout(function () { if ($(".topMenu .mnuO").length == 0) $(".topMenu .sub").hide(); }, 10);
            })
        })

        function MenuSlideShow() {
            $(".mnuTopContent").slideToggle();                        
        }
        var mnuS;

        function MenuTopToggle(s) {
            mnuS = $(s);
            var id = mnuS.attr("m");
            $("div[mnu=" + id + "]").slideToggle(1);
            $("div[mnu=" + id + "]").unbind('mouseenter').unbind('mouseleave').hover(function () { mnuS.addClass("mnuO") }, function () { $(".topMenu .sub:visible").hide(); mnuS.removeClass("mnuO") })
        }
        function MenuSlideToggle(sender, id, depth) {
            sender = $(sender);
            if (sender.hasClass("exp")){
                sender.removeClass("exp").addClass("col");
                sender.attr("src", sender.attr("src").replace("Exp", "Col"));
                if (!sender.hasClass("full")) {
                    $.post("/Handlers/Ajax.ashx", { action: "menu", id: id, depth: depth }, function (data) {
                        sender.addClass("full");
                        data = $("<div class=\"padLeft10\" />").append(data);
                        data.hide();
                        data.insertAfter(sender.parent())                                                
                        data.slideDown(200);
                    });
                }
                else
                    sender.parent().next().slideDown(200);
            }
            else {
                sender.parent().next().slideUp(200);
                sender.removeClass("col").addClass("exp");
                sender.attr("src", sender.attr("src").replace("Col", "Exp"));
            }
       }
    </script>
