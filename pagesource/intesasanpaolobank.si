        if (frmOk && eval($("#ctl00_AdvisorForm1_chkResponseMail").attr("checked")))
            frmOk = Page_ClientValidate("FrmAdvisorContact");     

        return frmOk;
    }

    function AdvisorEnableResponseMail(checked)
