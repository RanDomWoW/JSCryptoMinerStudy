                alert('Izpolniti morate polje Telefon.')
                args.IsValid = false;
            }
            else
                args.IsValid = true;
        }
    }

    function GetMeetingType()
    {
        var id = $("#ctl00_AdvisorForm1_rdlMeetingType input:checked").val();
        return id;
    }
    
    function ChangeUnitSelection()
    {            
        $.post("/Handlers/Ajax.ashx", { action: "officeSchedule", mType : GetMeetingType()}, function(data) {
            $("#ctl00_AdvisorForm1_ddlUnit").html(data);
            ChangeOffice($("#ctl00_AdvisorForm1_ddlUnit").val());
        });           
    }
            
    function ChangeOffice(val)
    {                        
        $.post("/Handlers/Ajax.ashx", { action: "officeSchedule", id: val, mType : GetMeetingType()}, function(data) {
            $("#ctl00_AdvisorForm1_ddlDate").html(data);
            ChangeOfficeDate($("#ctl00_AdvisorForm1_ddlDate").val());
        });           
    } 
    function ChangeOfficeDate(val)
    {                        
        var oID = $("#ctl00_AdvisorForm1_ddlUnit").val();

        $.post("/Handlers/Ajax.ashx", { action: "officeSchedule", id: oID, date:val, mType : GetMeetingType()}, function(data) {
            $("#ctl00_AdvisorForm1_ddlHour").html(data);

        });           
    } 

    function ValidateAdvisorForm()
    {
        var frmOk = true;
            
