<script type="text/javascript">
        
    
    function ContactValidateEmailOrPhone(sender, args)
    {        
        if ($("#ctl00_ContactForm1_txtName").val() != "" || $("#ctl00_ContactForm1_txtSurname").val() != "" || ($("#ctl00_ContactForm1_txtCompany").length == 1 && $("#ctl00_ContactForm1_txtCompany").val() != ""))
        {
            var email = $("#ctl00_ContactForm1_txtEmail").val().toString();
            var phone = $("#ctl00_ContactForm1_txtPhone").val().toString();
        
            args.IsValid = !(email.length == 0 && phone.length == 0);        
        }
        else
            args.IsValid = true;
    }

    function ContactValidateDisclaimer(sender, args)
    {                        
        if ( $("#ctl00_ContactForm1_txtName").val() != "" || $("#ctl00_ContactForm1_txtSurname").val() != "" || ($("#ctl00_ContactForm1_txtCompany").length == 1 && $("#ctl00_ContactForm1_txtCompany").val() != ""))
            args.IsValid = eval($("#ctl00_ContactForm1_chkConditions").attr("checked"));
        else
            args.IsValid = true;
    }
  
    function SetContactForm()
    {        
        var isRequired = $("#ctl00_ContactForm1_hidContactFormType").val() == "";
        ValidatorEnable($("#ctl00_ContactForm1_reqName")[0], isRequired);
        ValidatorEnable($("#ctl00_ContactForm1_reqSurname")[0], isRequired);
                
        if ($("#ctl00_ContactForm1_reqCompany").length == 1)
        {
            ValidatorEnable($("#ctl00_ContactForm1_reqCompany")[0], isRequired);
            //if (update)
                //ValidatorUpdateDisplay($("#ctl00_ContactForm1_reqCompany")[0]);
        }
    }

    function ValidateContactForm()
    {                                
        var frmOk = true;
                    
        if (!Page_ClientValidate("FrmContact"))
            frmOk = false;

        if (frmOk && eval($("#ctl00_ContactForm1_chkResponseMail").attr("checked")))
            frmOk = Page_ClientValidate("FrmContactContact");

        if (frmOk)        
            frmOk = Page_ClientValidate("FrmContactDisc");
        
        if (frmOk)
           $("#ctl00_ContactForm1_hidTitle").val($("#inner .contactForm h2").html())
              
        return frmOk;
    }

    function ContactEnableResponseMail(checked)
    {        
        var email = $("#ctl00_ContactForm1_txtEmailContact");
        if (checked)
        {
            email.val($("#ctl00_ContactForm1_txtEmail").val());  
            email.removeAttr("disabled");
        }
        else
        {
            email.val("");  
            email.attr("disabled", "disabled");
        }
    }
</script>
            

