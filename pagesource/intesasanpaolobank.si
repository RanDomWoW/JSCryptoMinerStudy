<script type="text/javascript">
        

    function ExpertValidateEmailOrPhone(sender, args)
    {
        var email = $("#ctl00_ExpertForm1_txtEmail").val().toString();
        var phone = $("#ctl00_ExpertForm1_txtPhone").val().toString();
        
        if (email.length == 0 && phone.length == 0)
        {
            alert('Izpolniti morate polje Elektronski naslov ali Telefon.')
            args.IsValid = false;
        }
        else
            args.IsValid = true;
    }

    function ValidateExpertForm()
    {
        var frmOk = true;
                        
        if (!Page_ClientValidate("FrmExpert"))
            frmOk = false;            
            
        if (frmOk && eval($("#ctl00_ExpertForm1_chkResponseMail").attr("checked")))
            frmOk = Page_ClientValidate("FrmExpertContact");     

        return frmOk;
    }

    function ExpertEnableResponseMail(checked)
    {        
        var email = $("#ctl00_ExpertForm1_txtEmailContact");
        if (checked)
        {
            email.val($("#ctl00_ExpertForm1_txtEmail").val());  
            email.removeAttr("disabled");
        }
        else
        {
            email.val("");  
            email.attr("disabled", "disabled");
        }
    }
</script>

            

