                width: 'auto',
                modal: true,
                autoOpen: false,
                draggable: false,
                position: { my: "center", at: "center", of: window },
                title: "Newsletter Signup",
                buttons: [
                    {
                        text: "OK",
                        click: function(){
                            $(this).dialog("close");
                        }
                    }
                ]
            });

            function submitForm() {
                setTimeout($("#NewsPopup").dialog("open"),500);
            }

            $("#EmailSignUpForm").on("submit", submitForm);
            
        });
    </script>

