</script>
<script type="text/javascript" language="javascript">


	var c_value = 'city';

	$("input:radio[name=search_selection]").click(function() { 
  	  		 c_value = $(this).val();
			 if(c_value == 'campus'){
				 $('input:text#input_text').val('');
			 	 //document.getElementById('searchResultSet').innerHTML = '';
				 $("#outerDiv").hide();
				 $("#mapDiv").slideDown();
			 }
			 else if(c_value == 'region'){
				 $('input:text#input_text').val('');
			 	 //document.getElementById('searchResultSet').innerHTML = '';
				 $("#outerDiv").hide();
				 $("#mapDiv").slideDown();
				 searchCampus();
			 }
			 else{
			 	$('input:text#input_text').val('');
				 //document.getElementById('searchResultSet').innerHTML = '';
				 $("#outerDiv").hide();
				 $("#mapDiv").slideDown();
				// searchCampus();
			 }
		  });	
		  
		 function search_region() {
				 $('input:text#input_text').val('');
			 	 document.getElementById('searchResultSet').innerHTML = '';
				 searchCampus();
		 }
		  
	function searchCampus(){
	
		jQuery.noConflict();
		document.getElementById('searchResultLoader').style.display = 'block';
		var input_select = '';
		//alert (c_value);
		//alert (c_value);
		if(c_value == 'region'){
			input_select = document.getElementById('regions').value;
		}
			//alert (document.getElementById('search_selection').checked.value);
		jQuery.ajax({
			/*url: "../operations/campus_search.php",*/ //LIVE DEMO
			url: "operations/campus_search.php", //LIVE 
			type: "POST", 
			data: { input_string: input_string = document.getElementById('input_text').value ,input_select:input_select, selection_string: c_value},
			success: function(data){
				if((data != 'NOT_FOUND') && (data != 'SMALL_STRING')){
					var jsonObj = jQuery.parseJSON(data);
					document.getElementById('searchResultSet').innerHTML = '';
					htmlText = "";
					for(i=0; i<jsonObj.length; i++){
						var phone = '';
						var map = '';
						if(jsonObj[i][3] == "" && jsonObj[i][4] != ""){
							phone = jsonObj[i][4];
						}else{
							phone = jsonObj[i][3] + ", " + jsonObj[i][4];
						}
						if(jsonObj[i][10] != "C"){
							<!--web = "<div><a href=\"/home?campus=" + jsonObj[i][0] + "\" target=\"_blank\"><img src=\"images/viewWebsite.png\" border=\"0\"></a></div>";-->
							web = "<span class=\"buttonLocation\"><a href=\"http://campus.alliedschools.edu.pk/" + jsonObj[i][11] + "\" target=\"_blank\">View Website</a></span>";
						}else{
							web = '';
						}
						if(jsonObj[i][7] == "" && jsonObj[i][8] == "" || jsonObj[i][10] == "C" || jsonObj[i][10] == "B"){
							if(jsonObj[i][10] == "C"){
								map ='<div class=\"resultSet_cancel\"><strong>Cancelled</strong></div>';
							}else if(jsonObj[i][10] == "B"){
								<!--map ='<div><a style=font-size:13px;color:#C60 href=\"/lnk_contact?campus=" + jsonObj[i][0] + "\" target=\"_blank\"><img src=\"images/map_icon.jpg\" border=\"0\"/></a></div><div class=\"resultSet_cancel\"><strong>Blocked</strong></div>';-->
								map ="<span class=\"buttonLocation\"><a href=\"http://campus.alliedschools.edu.pk/" + jsonObj[i][11] + "/ContactUs\" target=\"_blank\">View on Map</a></span><div class=\"resultSet_cancel\"><strong>Blocked</strong></div>";								
							}else{
								map = '';
							}
						}else{
							<!--map = "<div><a style=font-size:13px;color:#C60 href=\"/lnk_contact?campus=" + jsonObj[i][0] + "\" target=\"_blank\"><img src=\"images/map_icon.jpg\" border=\"0\"/></a></div>";-->
							map = "<span class=\"buttonLocation\"><a href=\"http://campus.alliedschools.edu.pk/" + jsonObj[i][11] + "/ContactUs\" target=\"_blank\">View on Map</a></span>";
						}
						
						
						htmlText += "	<div class=\"resultSet\">" +
											"<div class=\"resultSet_title\">"+jsonObj[i][1]+"</div>" +
											"<div>"+jsonObj[i][2]+"</div>" +
											"<div><img src=\"images/icon_phone.png\">"+phone+"</div>" +
											"<div>"+jsonObj[i][6]+"</div>" +
											 web +
											 map +
										"</div>";
					}
				}else if(data == 'NOT_FOUND'){
					htmlText = "Sorry! no result found";
				}else if(data == 'SMALL_STRING'){
					htmlText = "Minimum String length is 3";
				}else{
					htmlText = "Unknonw Error accoured";
				}
				document.getElementById('searchResultSet').innerHTML = htmlText;
				document.getElementById('searchResultLoader').style.display = 'none';
			}
		});
		return false;
	}
	
