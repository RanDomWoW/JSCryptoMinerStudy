    <script>
        var loginFormApp = {
            actionPasswordForm: "https://www.liltmedia.com/front/forgot_password_ajax",
            urlLoginForm: "https://www.liltmedia.com/front/ajax_login_validation",
            urlCancelPage: "https://www.liltmedia.com/cancel"
        };
            function doForgotPassword() {
                $("#message_forgot").html();

                var action = loginFormApp.actionPasswordForm;
                var form_data = {
                    email: $("#password_form #username_forgot").val(),
                    forgot_password: 1,
                    is_ajax: 1
                };

                $.ajax({
                    type: "POST",
                    url: action,
                    data: form_data,
                    success: function(response)
                    {
                        $("#bt_signin").html("Submit");

                        if(response == 'Your password has been sent to your email.') {
                            $("#message_forgot").html("<p class='forgot-success'>"+ response +"</p>");
                        }
                        else {
                            $("#message_forgot").html("<p class='forgot-error'>"+ response +"</p>");

                        }
                    },
                    error:function(jqXHR, textStatus, errorThrown){
                        var resultHtml = '<p class="login-error">There was an error please submit form again.</p>';
                        $('div#message_forgot').html(resultHtml);
                        return false;
                    }
                });

                return false;
            }
            function validate_login() {
                var emailaddressVal = $("#login #username").val();
                var passwordVal = $("#login #password").val();

                if(emailaddressVal == '' && passwordVal == '') {
                    $('div#message').html('<p class="login-error">Please enter your email address and password.</p>');
                }
                else if(emailaddressVal == '') {
                    $('div#message').html('<p class="login-error">Please enter your email address.</p>');
                }
                else if (passwordVal == '') {
                    $('div#message').html('<p class="login-error">Please enter a password.</p>');
                }
                else {
                    var serializedData = $('#login').serialize();
                    $.ajax({
                        type: 'POST',
                        url: loginFormApp.urlLoginForm,
                        async: false,
                        data: serializedData,
                        dataType: "json",
                        success: function(data, textStatus, errorThrown){
                            if(data.result == 'valid') {
                                $('#login').submit();
                                return true;
                            } else if(data.result == 'already_connected'){
                                /* Redirect the user to MA*/
                                window.location.href = data.redirect_url;
                            } else {
                                var resultHtml = '<p class="login-error">' + data.error_msg + '</p>';

                                $('div#message').html(resultHtml);
                                return false;
                            }
                        },
                        error: function(qXHR, textStatus, errorThrown){
                            var resultHtml = '<p class="login-error">There was an error please submit form again.</p>';
                            $('div#message').html(resultHtml);
                            return false;
                        }
                    });
                }
            }
            $('.login_btn').on('click', function(event){
                event.preventDefault();
                validate_login();
            });
            $('.password_btn').on('click', function(event){
                event.preventDefault();
                doForgotPassword();
            });
            $("#open-password").click(function(){
                $("#password_form").show();
                $("#login").hide();
                return false;
            });
            $("#back-login").click(function(){
                $("#login").show();
                $("#password_form").hide();
                return false;
            });
    </script>
