    var suggestWidth = 0;
    var suggestButtonWidth = 0;

    $(document).ready(function () {
        suggestWidth = $(".suggtext").width();
        suggestButtonWidth = $("#searchButton").width();

        SetCartItemCount();
        $(".suggestion").hide();
        $(".suggtext").focusout(function () {
            //$(".suggestion").hide();
            $(".suggtext").animate({ width: suggestWidth })
        })

        $(document).mouseup(function (e) {
            var container = $(".suggestion");

            if (!container.is(e.target) // if the target of the click isn't the container...
                && container.has(e.target).length === 0) // ... nor a descendant of the container
            {
                container.hide();
            }
        });
        $(".suggtext").focusin(function () {
            var w = suggestWidth * 2.5;
            $(".suggtext").animate({ width: w })
            var suggestText = $("#searchKey").val();
            if (suggestText.length < 3)
                return;
            $.ajax({
                url: "/home/searchsuggestion" + "?term=" + suggestText,
                type: 'GET',
                contentType: "application/json; charset=utf-8",
                crossDomain: false,
                success: function (data) {
                    FillSuggestion(data);
                    //$(".suggestion").show(700);
                }
            });
        })
        $("#searchKey").keyup(function () {
            var suggestText = $("#searchKey").val();
            if (suggestText.length < 3) {
                $(".suggestion").hide();
            }
        });
    });
</script>

    <script type="text/javascript">
        $(document).ready(function () {
            $('.suggtext').autocomplete({
                source: function (request, response) {
                    var $element = $(this.element);
                    var jqXhr = $element.data('jqXHR');
                    if (jqXhr) jqXhr.abort();
                    $element.data('jqXHR', $.ajax({
                        url: "/home/searchsuggestion" + "?term=" + this.term,
                        type: 'GET',
                        contentType: "application/json; charset=utf-8",
                        crossDomain: false,
                        success: function (data) {
                            FillSuggestion(data);
                        }
                    }));
                },
                minLength: 3,
                position: { my: "right top", at: "right bottom" },
                select: function (event, ui) {
                    var xhr = $(this).data('xhr');
                    if (xhr) xhr.abort();
                    var label = ui.item.label;
                    label = encodeURIComponent(label);
                    window.location = "/search" + "?searchKey=" + label + "&autocomplete=true";
                }
            }).autocomplete("widget").addClass("searchDetail");
        });

        function FillSuggestion(data) {
            $("#resultTextStatic").show();
            $("#divBrands").show();
            $("#divCategories").show();
            var term = $("#searchKey").val();
            //$(".suggestion").show(200);
            var items = data.split('|');
            var currentCategory = "";
            var ul = $('#suggestResultUl');
            var ulBrand = $('#ulBrand');
            ulBrand.empty();
            var ulCategory = $('#ulCategory');
            ulCategory.empty();
            var ulSuggest = $('#ulSuggest');
            ulSuggest.empty();
            var ulSuggestRight = $('#ulSuggestRight');
            ulSuggestRight.empty();
            var suggestCount = 0;
            for (var i = 0; i < items.length; i++) {
                var detail = items[i].split(',');
                if (detail[1] != currentCategory) {
                    ul.append("<li>" + detail[1] + "</li>");
                    currentCategory = detail[1];
                }
                if (currentCategory == "Suggest") {
                    var text = detail[0] + " (" + detail[2] + ")";
                    var regex = new RegExp('(' + term + ')', 'gi');
                    text = text.replace(regex, "<strong>$1</strong>");
                    if (suggestCount % 2 == 0) {
                        ulSuggest.append("<li><a href=\"/search?searchKey=" + term + "&keywordKey=" + encodeURIComponent(detail[0]) + "\">" + text + "</a></li>");
                    }
                    else {
                        if (detail[2] != "0")
                            ulSuggestRight.append("<li><a href=\"/search?searchKey=" + term + "&keywordKey=" + encodeURIComponent(detail[0]) + "\">" + text + "</a></li>");
                        else
                            ulSuggestRight.append("<li><a href=\"#\">&nbsp;</a></li>");
                    }
                    suggestCount++;
                    //ul.append("<li><a href=\"/search?searchKey=" + detail[0] + "\">" + detail[0] + " (" + detail[1] + ")</a></li>");
                }
                if (currentCategory == "Brand") {
                    var text = detail[0] + " (" + detail[2] + ")";
                    var regex = new RegExp('(' + term + ')', 'gi');
                    text = text.replace(regex, "<strong>$1</strong>");
                    ulBrand.append("<li><a href=\"/search?searchKey=" + term + "&brandKey=" + encodeURIComponent(detail[0]) + "\">" + text + "</a></li>");
                }
                if (currentCategory == "Category") {
                    var text = detail[0] + " (" + detail[2] + ")";
                    var regex = new RegExp('(' + term + ')', 'gi');
                    text = text.replace(regex, "<strong>$1</strong>");
                    ulCategory.append("<li><a href=\"/search?searchKey=" + term + "&categoryKey=" + encodeURIComponent(detail[0]) + "\">" + text + "</a></li>");
                }
            }

            if (suggestCount == 0) {
                $(".suggestion").hide();
                ulSuggest.append("<li><a href=\"#" + detail[0] + "\"><strong>" + term + "</strong> ile ilgili bir sonuç bulunamamıştır.</a></li>");
                ulSuggestRight.append("<li><a href=\"#\">&nbsp;</a></li>");
                $("#divBrands").hide();
                $("#divCategories").hide();
                $("#resultTextStatic").hide();
            }
            else {
                $(".suggestion").show(200);
            }
        }
    </script>


