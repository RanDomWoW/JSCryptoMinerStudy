				circular: true,
				scroll: {
					easing: 'quadratic',
					duration: 0.3,
					timeoutDuration: 3000,
					onBefore: function( data ) {
						if (isFirstRun) {
							isFirstRun = false;
							sleep(3000);
						} else {
							$capt.trigger( 'slideTo', [ '.' + data.items.visible.first().attr( 'alt' ) ] );
							$timr.stop().animate({
								opacity: 0
							}, data.scroll.duration);
						}
					},
					onAfter: function() {
						$timr.stop().animate({
							opacity: 1
						}, 150);
					}
				},
				pagination: {
