                <script type="text/javascript">
                            $(function() {
                                var createCookie = function(name, value, days) {
                                    var expires;
                                    if (days) {
                                        var date = new Date();
                                        date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                                        expires = "; expires=" + date.toGMTString();
                                    }
                                    else {
                                        expires = "";
                                    }
                                    document.cookie = name + "=" + value + expires + "; path=/";
                                }

                                var getCookie = function(c_name) {
                                    if (document.cookie.length > 0) {
                                        c_start = document.cookie.indexOf(c_name + "=");
                                        if (c_start != -1) {
                                            c_start = c_start + c_name.length + 1;
                                            c_end = document.cookie.indexOf(";", c_start);
                                            if (c_end == -1) {
                                                c_end = document.cookie.length;
                                            }
                                            return unescape(document.cookie.substring(c_start, c_end));
                                        }
                                    }
                                    return "";
                                }

                                var getRandomInt = function(min, max) {
                                    return Math.floor(Math.random() * (max - min + 1)) + min;
                                }

                                var $count = 542222;

                                    $cookNum  = $count;

                                var $countHundred = $count + 100;

                                var flagCount = true;



                                var wordKilla = function(number, one, two, five) {
                                    if (number < 0)
                                        number = 0 - number;

                                    number %= 100;
                                    if (number >= 5 && number <= 20)
                                        return five;

                                    number %= 10;
                                    if (number == 1)
                                        return one;

                                    if (number >= 2 && number <= 4)
                                        return two;

                                    return five;
                                }

                                var iterator = function() {
                                    var $step = getRandomInt(0, 4); ///

                                    //if (cookieCount > $count + 100){
                                    //    $step = 0;
                                    //}

                                    $count = $count + $step;

                                    var cookieCount = getCookie('counter');

                                    if (cookieCount > $count) {
                                        $count = parseInt(cookieCount);
                                    } else {
                                        createCookie('counter', $count, 0);
                                    }

                                    //добавляю к числу врачей разделительный пробел при тысячных вида * ***, ** ***, *** ***
                                    var $count1 = $count + '',
                                        $num1 = $count1.slice(-3),
                                        $num2 = $count1.slice(0, -3);

                                    //console.log($count1 + ' <--- 1');
                                    //console.log($num1 + ' <--- 2');
                                    //console.log($num2 + ' <--- 3');

                                    var $countHasNbsp = $num2 + ' ' + $num1; // то самое число =)

                                    $('#countUp').html($countHasNbsp);
                                    var doc = wordKilla($count, "врач!", "врача!", "врачей!"); ///
                                    $('#wordKilla').html(doc);
                                    document.title = $countHasNbsp + " " + doc + " Доктор на работе — половина врачей России";

                                    if($countHundred < $count){
                                        createCookie('counter', $cookNum, 0);
                                        $count = $cookNum;
                                        //flagCount = false;
                                    }

                                }

                                iterator();

                                $countUp = setInterval(function() {
                                    if(flagCount){
                                        iterator();
                                    }
                                }, 5000); ///

                            });
                </script>

                <h3>Коллективный<br /><span>врачебный разум</span></h3>
