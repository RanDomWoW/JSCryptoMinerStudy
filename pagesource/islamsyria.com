 <script type="text/javascript">
       $('document').ready(function(){
            $(".font-button").bind("click", function () {
                var size = parseInt($('.kaf p span span,.kaf p span,.kaf p,.kaf,.kaf div span').css("font-size"));
                if ($(this).hasClass("plus")) {
                    size = size + 2;
                } else {
                    size = size - 2;
                    if (size <= 10) {
                        size = 10;
                    }
                }
                $('.kaf p span span,.kaf p span,.kaf p,.kaf,.kaf div span').css("font-size", size);
                return false;
            });
        });
    </script>
  
  
  
  
  
