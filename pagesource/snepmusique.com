			$('.with-scroll').jScrollPane({'autoReinitialise': true});


			$('#content .content table').not('.noScript').each(function() {
				// Reinit class to table in content part
				$(this).removeClass('table table-striped').addClass('table table-striped');

				// Reinit theader to table in content part
				if ($(this).find('thead').length == 0) {
					var nbTd = $(this).find('tbody tr').eq(0).find('td').length;
					$(this).prepend('<thead><tr>' + ('<th></th>'.repeat(nbTd)) + '</tr></thead>');
				}
			});

			// Reorder pagination in a list
			$('.pagination-nav .pagination').each(function() {
				var links = $(this).find('a, span'),
						active = $(this).find('span.current');

				links.wrap('<li></li>');
				active.parent('li').addClass('active');
				$(this).replaceWith('<ul class="pagination">' + $(this).html() + '</ul>');
			});


			//Add modal attribute for Contact link in menu
			$('#menu-item-323 a').click(function() {
				$('#myModal').modal({
					remote: $('#menu-item-323 a').attr('href') + ' #content'
				});
				return false;
			});


			//Show/hide target
			$('.showHide').click(function() {
				var target = $(this).attr('href');
				var jqtarget = $(target);
				var target2 = target;
                                target2 = target2.replace('buy', 'listen');
                                if(target2 != target ){
                                
                                }
                                else target2 = target2.replace('listen', 'buy') ;
                                var jqtarget2 = $(target2);
                                
                                
                                if (jqtarget2.hasClass("visible")) {
					jqtarget2.hide().removeClass('visible');
				}
                                
                                if (jqtarget.hasClass("visible")) {
					jqtarget.hide().removeClass('visible');
				}
				else {
					jqtarget.show().addClass('visible');
				}

				return false;
			});

