<div style="display:none">
	<div id="entra" class="gs_SSOwindow">
		<p class="sso_Enter">Entra in <span>Giunti Scuola</span></p>
		<!--
 		<p class="sso_Notice">Questo contenuto è riservato agli utenti registrati. Accedi o registrati.</p> 
		<p class="sso_Notice">Questo contenuto è riservato ai seguenti profili:<br>
		- Registrato<br>
		- Abbonato riviste digitale<br>
		- Abbonato "più" rivista
		</p>
		-->
				
			
		<form class="sso_Form cmg_Float" method="post">
			<input type="hidden" value="login" name="sso_action">
			<fieldset class="sso_FieldsetLeft sso_FieldsetLogin" style="display:block;">
				<label>
					<span class="sso_Label">E-mail:<span class="gs_mini">o Nome utente</span></span>
					<input type="text" value="" name="sso_email">
					<span class="sso_Help vtip" title="Inserisci il nome utente o l'indirizzo email che hai scelto al momento della registrazione.">?</span>
				</label>
				<label>
					<span class="sso_Label">Password:</span>
					<input type="password" value="" name="sso_pwd" autocomplete="off">
					<span class="sso_Help vtip" title="Inserisci la tua password di almeno 5 caratteri, composta da lettere e/o numeri. Se l'hai dimenticata, clicca sul link in basso.">?</span>
				</label>
				<label class="sso_LabelSubmit">
					<button type="submit" class="btnBlue btnBig">Entra</button>
				</label>
				<p class="sso_LinkSmall sso_LinkRecover"><a href="#recupera">Hai dimenticato i dati di accesso?</a></p>
			</fieldset>
			
			<fieldset class="sso_FieldsetLeft sso_FieldsetRecover" style="display:none;">
				<label class="sso_LabelEmail">
					<span class="sso_Label">E-mail:</span>
					<input type="text" value="" name="sso_emailRecovery">
					<span class="sso_Help vtip" title="Inserisci l'indirizzo email che hai scelto al momento della registrazione.">?</span>
				</label>
				<label class="sso_LabelSubmit">
					<button type="submit" class="btnBlue btnBig">Recupera</button>
				</label>
				<p class="sso_LinkSmall"><a href="#indietro">Indietro</a></p>
			</fieldset>
			
			<fieldset class="sso_FieldsetRight">
				<p class="sso_RegisterTitle">Non sei ancora registrato?</p>
				<p class="sso_RegisterDescr">Entra anche tu a far parte della più grande community di insegnanti italiani sul web!</p>
				<p class="sso_RegisterAction"><button type="button" class="btnGreen btnBig" onclick="window.location.href='http://www.giuntiscuola.it/registrazione/';">Iscriviti</button></p>
				<p class="sso_LinkSmall"><a href="http://www.giuntiscuola.it/registrazione/">Perché dovrei registrarmi?</a></p>
			</fieldset>
		</form>
		
		</div>
	<script>
	$(function(){
		// go recover
		$('.gs_SSOwindow a[href="#recupera"]').click(function(){
			$('.sso_FieldsetLogin').hide();
			$('.sso_FieldsetRecover').fadeIn();
			$('.gs_SSOwindow a[href="#indietro"]').hide();
			$('.gs_SSOwindow .sso_FieldsetLogin input[name="email"]').val('')
			$('.gs_SSOwindow .sso_FieldsetLogin input[name="pwd"]').val('');
			$('.gs_SSOwindow .sso_Form input[name="sso_action"]').val('recover');
			var t = setTimeout(function(){
				$('.gs_SSOwindow a[href="#indietro"]').fadeIn();
			},1000);
			return false;
		});
		// go back
		$('.gs_SSOwindow a[href="#indietro"]').click(function(){
			$('.sso_FieldsetRecover').hide();
			$('.sso_FieldsetLogin').fadeIn();
			$('.gs_SSOwindow .sso_FieldsetRecover input[name="sso_emailRecovery"]').val('');
			$('.gs_SSOwindow .sso_Form input[name="sso_action"]').val('login');
			return false;
		});
		// login
		$('.gs_SSOwindow .sso_FieldsetLogin button').click(function(){
			var err=0;
			if( $('.gs_SSOwindow .sso_FieldsetLogin input[name="email"]').val()=='' ) {
				$('.gs_SSOwindow .sso_FieldsetLogin input[name="email"]').addClass('sso_InputError');
				var t = setTimeout(function(){
					$('.gs_SSOwindow .sso_FieldsetLogin input[name="email"]').removeClass('sso_InputError');
				},2000);
				err=1;
			}
			if( $('.gs_SSOwindow .sso_FieldsetLogin input[name="pwd"]').val()=='' ) {
				$('.gs_SSOwindow .sso_FieldsetLogin input[name="pwd"]').addClass('sso_InputError');
				var t = setTimeout(function(){
					$('.gs_SSOwindow .sso_FieldsetLogin input[name="pwd"]').removeClass('sso_InputError');
				},2000);
				err=1;
			}
			if( err ) return false;
		});
		// recover
		$('.gs_SSOwindow .sso_FieldsetRecover button').click(function(){
			if( $('.gs_SSOwindow .sso_FieldsetRecover input[name="email"]').val()=='' ) {
				$('.gs_SSOwindow .sso_FieldsetRecover input[name="email"]').addClass('sso_InputError');
				var t = setTimeout(function(){
					$('.gs_SSOwindow .sso_FieldsetRecover input[name="email"]').removeClass('sso_InputError');
				},2000);
				return false;
			}
		});
		
	});
	
	</script>
</div>






