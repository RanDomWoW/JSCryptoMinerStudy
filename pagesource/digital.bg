  <script>
  	
    function toggleClass(element, className) {
                if (!element || !className) {
                    return;
                }
                var classString = element.className, nameIndex = classString.indexOf(className);
                if (nameIndex === -1) {
                    classString += ' ' + className;
                }
                else {
                    classString = classString.substr(0, nameIndex) + classString.substr(nameIndex + className.length);
                }
                element.className = classString;
            }
            var allButtons = document.querySelectorAll('.share-btn-mobile');
            var elToChange = document.querySelectorAll('.insider');
            for (var i = 0; i < allButtons.length; i++) {
                    allButtons[i].onclick = (function (x) {
                        return function () {
                            var el = elToChange[x];
                            toggleClass(el, 'active');
                        };
                    })(i);
                };
    
  </script>

            <script type="text/javascript" src="/js/jquery.validate.min.js"></script>
        <script type="text/javascript">
            

                /* registration validation */
                 $(function(){

                      $("#register_form").validate({
                        // Specify the validation rules
                        errorClass: "error-input",
                        errorElement: 'div',
                        rules: {
                            nickname: {
                                required: true,
                                minlength: 3,
                                remote: {
                                    url: "ajaxtabs/validateUsername",
                                    type: "post"
                                }
                            },
                            email: {
                                required: true,
                                email: true
                            },
                            pass: {
                                required: true,
                                minlength: 5
                            },
                            repeat_pass: {
                                required: {
                                    depends:  function(element) {
                                        return $("#pass").val().length;
                                    }
                                },
                                equalTo: "#pass"
                            },
                            captcha: {
                                required: true
                            }
                        },

                        // Specify the validation error messages
                        messages: {
                            nickname: {
                                required: "Въведете Потребителско Име",
                                minlength: "Потребителското име трябва да бъде поне 3 символа",
                                remote: "Потребителското трябва да съдържа само букви и цифри"
                            },
                            email: "Въведете email",
                            old_pass: "Въведете старата парола",
                            pass: {
                                required: "Въведете парола",
                                minlength: "Паролата трябва да бъде поне 5 символа"
                            },
                            repeat_pass: {
                                required: "Повторете Вашата парола",
                                equalTo: "Паролите не съвпадат"
                            },
                            captcha:
                            {
                                required: "Въведете кода от картинката"
                            }

                        },

                        submitHandler: function(form)
                        {
                             var flag = true;
                             var s_email = $("#email").val();
                             if(s_email)
                             {
                                 $.ajax({
                                      type: "POST",
                                      url: "/user/validateEmail",
                                      data: 'email='+s_email,
                                      success: function(a)
                                      {
                                         if(a == 'found')
                                         {
                                             flag = false;
                                             $('#email').addClass('error-input');
                                             add_this = '<div for="email" generated="true" class="error-input">Този е-мейл вече се използва от друг потребител.</div>';
                                             $(add_this).insertAfter('#email');
                                         }
                                         else flag = true;

                                         if(flag == true)
                                         {
                                             var c_code = $("#captcha").val();
                                             if(c_code)
                                             {
                                                $.ajax({
                                                  type: "POST",
                                                  url: "/ajaxtabs/validateCaptchas",
                                                  data: 'c_code='+c_code,
                                                  success: function(b)
                                                  {
                                                     if(b == 'wrong')
                                                     {
                                                         flag = false;
                                                         $('#captcha').addClass('error-input');
                                                         add_this = '<div for="captcha" generated="true" class="error-input">Въведеният код е грешен.</div>';
                                                         $(add_this).insertAfter('#captcha');
                                                     }
                                                     else flag = true;
                                                     if(flag == true)
                                                     {
                                                         form.submit();
                                                     }
                                                  }
                                                });
                                             }
                                         }
                                      }
                                 });
                             }
                             return false;
                        }
                    });

                });
                /* end registration validation */

                /* login validation */
                $(function(){
                      $("#login_form").validate({
                        errorClass: "error-input",
                        errorElement: 'div',
                        rules: {
                            email: {
                                required: true,
                                email: true
                            },
                            pass: {
                                required: true,
                                minlength: 5
                            }
                        },
                        messages: {
                            email: "Въведете email",
                            pass: {
                                required: "Въведете парола",
                                minlength: "Паролата трябва да бъде поне 5 символа"
                            }
                        },
                        submitHandler: function(form)
                        {
                             form.submit();
                             return false;
                        }
                    });

                });
                /* end login validation */
            
        </script>
    
