$(document).ready(function(){
    var cuentaSin;
    var cuentaCon;
    $('a[name=selCu]').each(function(){
    	var datos=$(this).text().split(",");
        cuentaSin = datos[0];
        cuentaCon = calcularDV(cuentaSin.trim());
        $(this).text(cuentaCon+' - '+datos[1]);
    });
    
})

function focusElemento(idElemento){
    var pos = $(idElemento).position();
    $('html, body').animate({
        scrollTop: (0,pos.top)
    }, 500);
}

function calcularDV(input){  
    var sum = 0;
    var inputStr = String(input);
    for(i=input.length-1, m=2; i>=0; i--, m++){
        sum = sum + inputStr[i] * m;
        if(m==7){
            m=1;
        }
    }
    var modulo = sum%11;
    var resto = 11-modulo;
    if(resto === 10){
        return input+"-k";
    } else if(resto === 11){
        return input+"-0";
    } else {
        return input+"-"+resto;
    }
}
</script>



