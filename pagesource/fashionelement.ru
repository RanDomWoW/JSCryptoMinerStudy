			$('.control-holder .slide-' + i).click(function(){
			$('.control-holder li').removeClass('active-slide');
			$('.control-holder .slide-'+i).addClass('active-slide');
				$('.fso-slider').slick('slickGoTo', i);                 
			})
		});
		
		$('.fso-slider').on('beforeChange', function(event, slick, currentSlide, nextSlide){
			$('.control-holder li').removeClass('active-slide');
			$('.control-holder .slide-'+nextSlide).addClass('active-slide');
		});
		
		$('.play').click(function(){
			$('.fso-slider').slick('slickPause');
			$('.play').css('display','none');
			$('.pause').css('display','block');
		});
		$('.pause').click(function(){
			$('.fso-slider').slick('slickPlay');
			$('.pause').css('display','none');
			$('.play').css('display','block');
		});
	});
	
	</script>
