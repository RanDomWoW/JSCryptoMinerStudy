    function reszieQQ() {
        if ($(window).width() < 1253) {
            $(".kefuQQ").css("right", 10 + "px");
        } else {
            var qqRight = ($(window).width() - 1152) / 2 - 104;
            //console.info(qqRight);
            //console.info($(window).width());
            $(".kefuQQ").css("right", qqRight + "px");
        }
    }
    reszieQQ();
    $(window).resize(function () {
        reszieQQ();
    });
</script>

