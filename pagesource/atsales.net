            $(this).find(".redProTypeBg").fadeOut(100, 0);
        });
        $(".restPic").load(function () {
            var img = new Image();
            img.src = $(this).attr("src");
            //$(this).parent().parent().width(img.width);
            var restDiv = ".restDiv" + $(this).attr("var");
            $(restDiv).width(img.width);
            $(restDiv).find(".restDivName").width(img.width);
            $(restDiv).find(".showContent").width(img.width);
            $(restDiv).find(".redProTypeBg").width(img.width);


            var restDivHr = $(restDiv).find(".restDivHr");

            restDivHr.css("margin-left", (img.width / 2 - restDivHr.width() / 2) + "px");
