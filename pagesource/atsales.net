        var curKey = e.which;
        if (curKey == 13 || curKey == 49) {
            headGlobalSerach();
        }
    });

    var headGlobalSerach = function () {
        var searchTxt = $(".searchTxt").val();
        if ($.trim(searchTxt) != "") {
            location.href = '/product/list/' + $(".searchTxt").val() + "/s";
        } else {
            alert("请输入搜索关键字");
        }
