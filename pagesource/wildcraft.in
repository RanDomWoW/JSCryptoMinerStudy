mentById('firstname_error').innerHTML='<div class="error-message">'+''+'</div>';document.getElementById('lastname_error').innerHTML='<div class="error-message">'+''+'</div>';document.getElementById('email_error').innerHTML='<div class="error-message">'+''+'</div>';document.getElementById('pwd_error').innerHTML='<div class="error-message">'+''+'</div>';document.getElementById('c_pwd_error').innerHTML='<div class="error-message">'+''+'</div>';document.getElementById('login_error').innerHTML='<div class="error-message">'+''+'</div>';document.getElementById('forgot-error').innerHTML='<div class="error-message">'+''+'</div>';});function displaysearch(){$j(".searchautocomplete").toggleClass('new_search_active')
