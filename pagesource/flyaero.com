<script type="text/javascript">

	$('#dpd1').datepicker({ 
		numberOfMonths: 2,
		showButtonPanel: true,
		dateFormat: "dd/mm/yy", 
		minDate: new Date(),		
		onSelect: function(date){
        var selectedDate = new Date(date);
        //var msecsInADay = 86400000;
        //var endDate = new Date(selectedDate.getTime());
        $("#dpd2").datepicker("option", "minDate", $('#dpd1').val());
		//$('#dpd2').datepicker('show');
		}
	});
	
	$('#dpd2').datepicker({ 
		numberOfMonths: 2, 
		showButtonPanel: true,
		dateFormat:"dd/mm/yy" 
	});
	
	$('#btnfrom').click(function(){
		$('#dpd1').datepicker('show'); 
	});
	
	$('#btnto').click(function(){
		$('#dpd2').datepicker('show'); 
	});
	
	$('.ui-datepicker-div').css('zIndex', 9999999);
		
	function getFormattedDate(date) {
	    //alert(date);
		if (date.length==0) return;
		var months = new Array("Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec");
		var datearr = date.split('/');
		var day = datearr[0];
		//var month = (("JanFebMarAprMayJunJulAugSepOctNovDec".indexOf(datearr[1]) / 3) + 1 );
		var month = parseInt(datearr[1]);
		var year = datearr[2];
		return year + '/' + month + '/' + day;
	}
	
	function controlMyFlightDate(selectedEntry)
	{
		$("#selectionHidden").val(selectedEntry);
		if(selectedEntry=="R"){
	          $(".trip-from-date").show();
	          $(".trip-to-date").show();
	          //$(".trip-opsion").hide();
	      }
	      if(selectedEntry=="O"){
	          $(".trip-from-date").show();
	          $(".trip-to-date").hide();
	          //$(".trip-opsion").hide(); 
	      }
	      if(selectedEntry=="M"){
	          $(".trip-from-date").show();
	          $(".trip-to-date").hide();
	          //$(".trip-opsion").show(); 
	      }
		
	}
	
	
</script>        



