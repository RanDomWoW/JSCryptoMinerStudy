		function showHideGB(){
			var gb = document.getElementById("gb");
			var gbtab = document.getElementById("gbbtn");
			var w = gb.offsetWidth;
			var chatango = document.getElementById("chatango");
			gb.opened ? moveGB(0, 35-w) : moveGB(25-w, 0);
			gb.opened = !gb.opened;
			if(gb.opened) {
				chatango.style.display = "block";
				currentPos = jQuery(this).scrollTop();
				gbtab.style.backgroundPosition = "-35px 0px";
				showing=true;
			}
			else{
				showing=false;
				gbtab.style.backgroundPosition = "0px 0px";
				chatango.style.display = "none";
			}
			allowShow = false;
		}

		function moveGB(x0, xf){
			var gb = document.getElementById("gb");
			var dx = Math.abs(x0-xf) > 10 ? 5 : 1;
			var dir = xf>x0 ? 1 : -1;
			var x = x0 + dx * dir;
			gb.style.right = x.toString() + "px";
			if(x0!=xf){setTimeout("moveGB("+x+", "+xf+")", 10);}
		}

