<script>
$(function() { 	
	
	/* 
	$(".showDetailSearch").click(function(){
		$("#search-form-wrapper").toggle();
		//$("#detailSearchLink").trigger('click');
	}); 
	*/
	
	$("#detailSearchLink").click(function(){ 
		
		if ( !$("#detailSearch_advanced").length) {
			window.location = SITE_URL+'search&mode=detail';			
			return true;
		}
		
		$("#search-mode").val('detail');
		$('#quickSearch').slideUp(300);  
		$('#detailSearch_advanced').slideDown(300);		
		$(this).hide(); 
		$('#hideDetailSearchLink').show();
		$('#moreSearchParamsLink').hide();
		moreSearchParams();
	});
	$("#moreSearchParamsLink").click(function(){ 	
		$(this).hide(); 
		moreSearchParams();
	});
	$("#lessSearchParamsLink").click(function(){ 
		$(this).hide(); 
		lessSearchParams();
	});
	$("#hideDetailSearchLink").click(function(){
		$("#search-mode").val('quick');
		$('#detailSearch').hide();
		$('#detailSearch_advanced').slideUp(300);
		$(this).hide(); 
		$('#detailSearchLink').show();
		$(".ds-desc-wrapper").slideDown(300);
		$("#quickSearch").slideDown(300);
		$("#centerContainer.detail-search-active").animate({"margin-top": "10px"}); 
		lessSearchParams();
		$('#moreSearchParamsLink').hide();
		$('#lessSearchParamsLink').hide();
	});
	
	function lessSearchParams(){
		$('.more-search-options').slideUp(300);
		$('.basic-options').show();
		//$('#detailSearch_advanced').animate({height: "200px"});	
		//$('#detailSearch_advanced .ds-advanced-inner').animate({height: "auto"});	
		$('#moreSearchParamsLink').show();
		$('html,body').animate( { scrollTop: 0 } , 1000);
	}
	
	function moreSearchParams(){
		$('.more-search-options').addClass('visible').slideDown(300);
		$('.basic-options').hide();
		//$('#detailSearch_advanced').animate({height: "400px"});	
		$('#detailSearch_advanced .ds-advanced-inner').animate({height: "95%"});	
		$('#lessSearchParamsLink').show();
	}
	
	$('.mask').change(function(){
		var id = $(this).attr('data-mask-id');
	    $('#'+id).val($(this).val());
	});
	
	if (window.location.href.substr(window.location.href.lastIndexOf('/') + 1) == 'search&mode=detail') {
		moreSearchParams();
	}
});

function doOnSubmitSearch(){
	switch($("#search-mode").val()){
		case 'quick':
			clearFormElements('#searchForm','mode,ad_type,description,my_ads');
			disableFormElements('#searchForm','mode,ad_type,description,my_ads');
			break;
		case 'detail':
			$("input[name='description']").val('');
			break;
	}
	
}



$("#searchForm").submit(function () {
    // prevent the default GET form behavior (name-value pairs)
    //event.preventDefault();
    
    doOnSubmitSearch(); 

    var strHoodValue = '';
    var strHoodPartValue = '';

    // Get the action attribute
    var action = $("#searchForm").attr("action");
    
    // get multiselect values and disable for submit
 	var arrHoodValues = $("#detail_search_id_hood").val();
    if (arrHoodValues != null){
 		strHoodValue = 'id_hood=' + arrHoodValues.join(',');
    }
    
 	var arrHoodPartValues = $("#detail_search_id_hood_part").val(); 

    if (arrHoodPartValues != null){
 		strHoodPartValue = 'id_hood_part=' + arrHoodPartValues.join(',');
    }
    
    $("#detail_search_id_hood").attr('disabled',true);
    $("#detail_search_id_hood_part").attr('disabled',true);
    
    var strvalues = $("#searchForm").serialize();
    
    var searchUrl = action + '?' + strvalues + '&' + strHoodValue + '&' + strHoodPartValue;

    window.location.href = searchUrl;
    
    return false;
});
</script>
