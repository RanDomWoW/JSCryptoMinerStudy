    <script type="text/javascript">
			$(document).ready(function(){
				setInterval(function(){rotateBanner("#banner_ad img");}, 30000);

				if (undefined == $.cookie("seen-cookie-policy")){
					$("#cookie-policy").slideDown();
					$.cookie("seen-cookie-policy", 1);
				}

				$("#cookie-policy .close").click(function(){
					$("#cookie-policy").slideUp();
				});

				if (undefined == $.cookie("seen-registration")){
					$.cookie("seen-registration", 1);
					$("#banner_ad").hide();
					$(".lightbox").fadeIn();
				}

				$(".lightbox .popup a.close").click(function(){
					$("#banner_ad").show();
					$(".lightbox").fadeOut();
				});

				$(".lightbox form input.text").on('click focusin', function() {
					this.value = '';
					$(this).unbind("click");
					$(this).unbind("focusin");
				});

				$("#registration").validate({
					rules:{
						firstname: {
							required: true,
							minlength: 2
						},
						lastname: {
							required: true,
							minlength: 2
						},
						schoolname: {
							required: false,
							minlength: 2
						},
						county:{
							required: false,
						},
						email: {
							required: true,
							email: true
						}
					}
				});

				$(function(){
    			$("#registration button[type=submit]").click(function(){
						if ($("#registration").valid()){
       				$.ajax({
        						type: "POST",
        						url: "submit_user_info.php",
        						data: $("#registration").serialize(),
        						success: function(data){}
       				});
							$("#banner_ad").show();
							$(".lightbox").fadeOut();
							return false;
						}
    			});
				});
			});

		</script>
