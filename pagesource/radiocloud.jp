    <!-- カテゴリーのタブ -->
    <script>
        $(function () {

            // 特定のタブを開いて遷移
            $(window).load(function () {
                var hash = location.hash;
                hash = (hash.match(/^#tab\d+$/) || [])[0];
                if ($(hash).length) {
                    var tabname = hash.slice(4);
                    var tabname = tabname - 1;
                    // var tabbar = tabname;
                    var tabbar = 0;
                    $(".programlist").addClass('hidden');
                    $(".programlist").eq(tabname).removeClass('hidden');
                    $("#tab li").removeClass('select');
                    $("#tab li").eq(tabbar).addClass('select');
                    // $("#tab li").eq(tabbar).addClass('select');
                  } else {
                    //初期表示
                    $(".programlist").addClass('hidden');
                    $(".programlist").eq(0).removeClass('hidden');
                }
            });

            // タブメニュー
            $("#tab li").click(function () {
                var num = $("#tab li").index(this);
                var category_num = num;
                $(".programlist").addClass('hidden');
                $(".programlist").eq(num).removeClass('hidden');
                $("#tab li").removeClass('select');
                $(this).addClass('select');
            });

            // エピソード更新日順／放送日順ソートスイッチ
            var seqsw_num = 0;
            $("#seqsw li").removeClass('select');
            $("#seqsw li").eq(seqsw_num).addClass('select');

            // エピソード更新日順／放送日順ソートスイッチ
            $("#seqsw li").click(function () {
                var num = $("#seqsw li").index(this);
                $("#seqsw li").removeClass('select');
                $(this).addClass('select');
            });

            //----エピソード高さ合わせ↓↓↓----
            var baseW = 569;
            var baseH = setBaseHeight();

            unifyHeight(baseH);

            //元のprogram_boxの高さを取得
            function setBaseHeight() {
                var baseH = new Array();
                //ジャンル毎に全program_box内h4の高さを取得
                                    var length = $('.cate_all .program_box').length - 1;
                    var tmpH = new Array();
                    for (i = 0; i <= length; i++) {
                        tmpH['key' + i] = $('.cate_all .program_box h4:eq(' + i + ')').height();
                    }
                    baseH['cate_all'] = tmpH;
                                        return baseH;
            }

            //高さ合わせ
            function unifyHeight(baseH) {
                //画面幅により表示列数変更
                var width = $('.programlist').width();
                var boxNum = width > baseW ? 3 : 2;
                var devNum = width > baseW ? 2 : 1;

                var length = 0, startI = 0, modH = 0, boxH = 0;
                //ジャンル毎
                                        length = $('.cate_all .program_box').length - 1;
                startI = 0, modH = 0, boxH = 0;
                //ジャンル内のbox数回繰り返し
                for (i = 0; i <= length; i++) {
                    //高さ比較
                    boxH = baseH['cate_all']['key' + i];
                    modH = boxH > modH ? boxH : modH;

                    //最大列or末尾の場合、その行のboxに高さを設定
                    if (i % boxNum == devNum || i == length) {
                        for (j = startI; j <= i; j++) {
                            $('.cate_all .program_box h4:eq(' + j + ')').height(modH);
                        }
                        startI = i + 1;
                        modH = 0;
                    }
                }
                            }

            //画面リサイズ時の高さ変更
            var timer = false;
            $(window).resize(function () {
                if (timer !== false) {
                    clearTimeout(timer);
                }
                timer = setTimeout(function () {
                    unifyHeight(baseH);
                }, 200);
            });
            //----エピソード高さ合わせ↑↑↑----
        });
    </script>

