        <script>
            $(function () {
                var $form = $(".registerFormHolder"),
                    $registerForm = $("#registerForm"),
                    registerFormStarted = false,
                    startRegisterForm = function () {
                        if (!registerFormStarted) {
                            registerFormStarted = true;
                            $registerForm.find("input[name=username]").trigger("change");
                            $registerForm.find("input[name=best_per]").trigger("change");
                            $registerForm.find("input[name=pwd]").trigger("change");
                            $registerForm.find("input[name=vorname]").trigger("change");
                            $registerForm.find("input[name=nachname]").trigger("change");
                            $registerForm.addClass("started");
                        }
                    };

                $form.find(".registerCaller").click(function (ev) {
                    ev.preventDefault();
                    $form.find(".loginCaller").removeClass("active").addClass("inactive");
                    $form.find(".registerCaller").removeClass("inactive").addClass("active");
                    $("#loginForm").hide();
                    $("#registerForm").show();
                }).click();
                $form.find(".loginCaller").click(function (ev) {
                    ev.preventDefault();
                    $form.find(".loginCaller").removeClass("inactive").addClass("active");
                    $form.find(".registerCaller").removeClass("active").addClass("inactive");
                    $form.find(".moveHolder").removeClass("register");
                    $("#loginForm").show();
                    $("#registerForm").hide();
                    $("#loginUsername").focus();
                });

                $form.find("select[name=best_per]").on("change", function () {
                    startRegisterForm();
                    if ($(this).val() == "sms") {
                        $form.find(".showEmail").hide();
                        $form.find(".showHandy").show();
                    } else {
                        $form.find(".showEmail").show();
                        $form.find(".showHandy").hide();
                    }
                });
                $registerForm.find("input[name=username]").on("keyup change", function () {
                    var $input = $(this),
                        $row = $input.parents(".form-group").first();
                    startRegisterForm();
                    if ($input.val() == "") {
                        $row.removeClass("has-error has-success loading");
                    } else {
                        $row.addClass("loading");
                        $.get("/ajax/username_exists.php", {username: $input.val()}, function (res) {
                            $row.removeClass("loading");
                            if (res == "1") {
                                $row.removeClass("has-error").addClass("has-success");
                            } else {
                                $row.addClass("has-error").removeClass("has-success");
                                $row.find(".form-control-feedback.error").attr("title", "Diser Benutzername wird bereits verwendet");
                            }
                        });
                    }
                });
                $registerForm.find("input[name=pwd]").on("keyup change", function () {
                    var $row = $(this).parents(".form-group").first();
                    startRegisterForm();
                    if ($(this).val().length == 0) {
                        $row.removeClass("has-error").removeClass("has-success");
                    } else if ($(this).val().length < 6) {
                        $row.addClass("has-error").removeClass("has-success");
                        $row.find(".form-control-feedback.error").attr("title", "Das Passwort muss mindestens 6 Zeichen lang sein");
                    } else {
                        $row.removeClass("has-error").addClass("has-success");
                    }
                });
                $registerForm.find("input[name=vorname], input[name=nachname]").on("keyup change", function () {
                    startRegisterForm();
                    var $row = $(this).parents("div.name").first();
                    if ($(this).val().length == 0) {
                        $row.removeClass("has-error").removeClass("has-success");
                    } else if ($(this).val().length < 2) {
                        $row.addClass("has-error").removeClass("has-success");
                        $row.find(".form-control-feedback.error").attr("title", "Bitte gib deinen Namen ein");
                    } else {
                        $row.removeClass("has-error").addClass("has-success");
                    }
                });
                $registerForm.find("input[name=nr_nummer]").on("keyup change", function () {
                    var $row = $(this).parents(".form-group").first(),
                        val = $(this).val();
                    if (val == "0") {
                        $(this).val("");
                    }
                    startRegisterForm();
                    if (val.length == 0) {
                        $row.removeClass("has-error").removeClass("has-success");
                    } else if (val.length < 8) {
                        $row.addClass("has-error").removeClass("has-success");
                        $row.find(".form-control-feedback.error").attr("title", "Die Nummer muss mindestens 8 Zeichen lang sein");
                    } else {
                        $row.removeClass("has-error").addClass("has-success");
                    }
                });
                var checkEmail = function () {
                    var parts1 = $registerForm.find("input[name=email]").val().split("@"),
                        parts2 = (parts1.length == 2 ? parts1[1].split(".") : []);
                    return (parts1[0].length > 0 && parts2.length > 1 && parts2[parts2.length - 1].length >= 2);
                };
                $registerForm.find("input[name=email]").on("keyup change", function () {
                    var $row = $(this).parents(".form-group").first();
                    startRegisterForm();

                    if ($(this).val().length == 0) {
                        $row.removeClass("has-error").removeClass("has-success");
                    } else if (!checkEmail()) {
                        $row.addClass("has-error").removeClass("has-success");
                        $row.find(".form-control-feedback.error").attr("title", "Bitte gib eine gültige E-Mail-Adresse ein");
                    } else {
                        $row.removeClass("has-error").addClass("has-success");
                    }
                });
                $registerForm.submit(function (ev) {
                    if ($(this).find(".has-error").length > 0) {
                        ev.preventDefault();
                        alert("Bitte behebe die oben angebenen Probleme - danach kann's losgehen!");
                    } else if (
                        $registerForm.find("input[name=username]").val() == "" || $registerForm.find("input[name=vorname]").val() == "" ||
                        $registerForm.find("input[name=nachname]").val() == "" || $registerForm.find("input[name=pwd]").val() == ""
                    ) {
                        ev.preventDefault();
                        alert("Bitte fülle alle Felder aus - danach kann's losgehen!");
                    }
                });
                if ($registerForm.find("input[name=username]").val() != "" || $registerForm.find("input[name=vorname]").val() != "" || $registerForm.find("input[name=nachname]").val() != "") {
                    startRegisterForm();
                }
                $(".kt_abholen_holder .kt_abholen_caller a").click(function (ev) {
                    ev.preventDefault();
                    $(".kt_abholen_holder .kt_abholen_caller").remove();
                    $(".kt_abholen_holder").append("<img src='/pics/loading.gif' alt='Hole Karotaler ab..'>");
                    var hash = $(this).data("hash");
                    $.getJSON(append_sid("/items/ajax-ktabholen.php?hash=" + hash), function (data) {
                        $("#items_kt_div").html("<i>Abgeholt</i>. <b>KT (neu)</b>: " + data);
                        $("#persstart_kt_div").html("<b>Karotaler (neu)</b>: " + data);
                        $("#sidebar_kt_div").html("<b>Karotaler (neu)</b>: " + data);
                        sidebar_mini_cont_cache[9] = null;
                        $(".kt_abholen_holder").html("<span class='pics_but_accept16'></span> Abgeholt. Du hast jetzt: " + data + " KT<br><a href='/items/kaufen.php'><span class='pics_pfeil_rechts'></span> Item-Shop</a>");
                    });
                });
            });
        </script>

