				}
				$('#dipImage_304685' + nums).hide("slide", { direction: directionOff }, speed);
			}else{
				$('#dipImage_304685' + nums).fadeOut(speed);
			}
			
			if($('#videoShell_304685' + nums).length){
				$('#videoShell_304685' + nums).fadeOut(speed);
			}
			if($('#dipContent_304685' + nums).length){
				$('#dipContent_304685' + nums).fadeOut(speed);
			}
			
			if(nums >= totalDips){nums = 1;}else{nums = nums + 1;}
			
			if($('#nav_304685' + nums).length){
				$('#nav_304685' + nums).addClass('on');
			}
			
			if($('#nav_li_304685' + nums).length){
				$('#nav_li_304685' + nums).addClass('on');
