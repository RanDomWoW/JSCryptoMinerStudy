	function switchDip_304685(num,pause){
		clearTimeout(slideshowTimer_304685);
		var timer;
		var nums = num;
		var slideshowMotion = slideshowMotion_304685();
		var speed = slideshowMotionSpeed_304685();
		var currentNum = document.getElementById('currentNum_304685').value;
		var totalDips = document.getElementById('totalDips_304685').value;
		var video = document.getElementById('video_304685').value;
		if(speed == '' || speed == null){
			speed = 800;
		}
		
		var direction = directions_304685(slideshowMotion);
		var directionOn = direction.directionOn;
		var directionOff = direction.directionOff;
		
		if(totalDips > 1 && video == 0){
		
			if(nums < 1){nums = 1;}
			if(nums > totalDips){nums = totalDips;}
			
			if($('#nav_304685' + nums).length){
