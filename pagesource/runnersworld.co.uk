<script type="text/template" id="list-box-template">

    <article class="cell-style-1 component ">

        <figure class="list-media cell-style--image">
            <a href="<%= article_url %>" title="<%= article_title %>">
                
                <% if (article_image_url != false) { %>
                    <img class="thumbnail_image" src="<%= article_image_url %>" alt="<%= article_title %>" />
                <% } else {%>
                    <img class="thumbnail_image" src="/assets/images/placeholder.jpg" alt="<%= article_title %>" />
                <% } %>

                <% if (article_channel == '2' || extra == 'gallery') { %>
                    <span class="cell-badge gallery top-right">Gallery</span>
                <% } else if (extra == 'video') { %>
                    <span class="cell-badge video">Video</span>
                <% } %>
            </a>
        </figure>

        <div class="cell-style--content">

            <hgroup>
                <% if (category_name != '') { %>
                    <h4 class="cell-style--content-category">
                        <a href="<%= category_url %>" title="<%= category_name %>">
                            <%= category_name %>
                        </a>
                    </h4>
                <% } %>

                <% if ( article_sponsorship != false &&
                        article_sponsorship.sponsor_type != 'Not required' &&
                        article_sponsorship.sponsor_name != 'Not required' ) { %>
                    
    <span class="sponsor-label">
        <span class="sponsor-label--type"><%= article_sponsorship.sponsor_type %></span>
        <span class="sponsor-label--name"><%= article_sponsorship.sponsor_name %></span>
    </span>
                <% } %>

                <h3 class="cell-style--content-heading">
                    <a href="<%= article_url %>" title="<%= article_title %>">
                        <%= article_title %>
                    </a>
                </h3>

            </hgroup>

            <p>
                <a href="<%= article_url %>" title="<%= article_title %>">
                    <%= article_description %>
                </a>
            </p>

        </div>

    </article>

</script>

