<script type="text/javascript">
    $(document).ready(function() {
        var base_url_hiden = window.location;
        
        $("li.li_left").hover( function () {
            var appen_string = $(this).attr("lang");
            if(appen_string != '')
            {
               $(".navigation").css('overflow','inherit');
                $(this).append($("<b>"+appen_string+"</b>")); 
            }
            
            },
            function () {
            $(this).find("b:last").remove();
            }
        );
        //li with fade class
        //$("li.li_left").hover(function(){$(this).fadeOut(100);$(this).fadeIn(500);});
        if(base_url_hiden == "http://admicro.vn/#mua-vi-tri-quang-cao" || base_url_hiden == "https://admicro.vn/#mua-vi-tri-quang-cao")
        {
            $('html, body').animate({scrollTop:$('.coming_soon').offset().top}, 'slow');
            $("#tracking").hide();
            $('.ac:last').css('display','block');
            $('.ac_bottom_shadow').css('display','none');
            $('.ac_top_shadow').css('display','none');
        }
        else
        {
           
            if(base_url_hiden == "http://admicro.vn/#bao-gia-san-pham" || base_url_hiden == "https://admicro.vn/#bao-gia-san-pham")
            {
                $('html, body').animate({scrollTop:$('.coming_soon').offset().top}, 'slow');
                $("#tracking").hide();
               $('.ac:first').css('display','block');
               $('.ac_bottom_shadow').css('display','none');
               $('.ac_top_shadow').css('display','none');
            }
            else
            {
                if(base_url_hiden == "http://admicro.vn/" || base_url_hiden == "https://admicro.vn/")
                {
                    $('.ac:last').css('display','none');
                    $('.ac:first').css('display','none');
                }
                else
                {
                    var utm_source = '1';
                    if(utm_source != 1) {
                        $('.ac:last').css('display','none');
                        $('.ac:first').css('display','none');
                    }
                    else{
                        window.location = window.location +" ";
                        var base_url_hiden = window.location +" ";                      
                        var res = base_url_hiden.replace("http://admicro.vn/#","");
                        //res = base_url_hiden.replace("https://admicro.vn/#","");
                        $('html, body').animate({scrollTop:$('.coming_soon').offset().top}, 'slow');
                        $("#tracking").hide();
                        $('.ac:first').css('display','block');
                        $('.ac_bottom_shadow').css('display','none');
                        $('.ac_top_shadow').css('display','none');
                        var id_foderl2_left = $('.navigation dl ul li ').find('.'+res).attr('lang');
                        $('.navigation dl ul li ').find('.'+res).addClass('active_f');
                        //var parrent = $('.navigation dl ul li ').find('.'+res).parent().parent().parent().parent().children('dt.active').find('a.active').text();
                        var parrent = $('.navigation dl ul li ').find('.'+res).parent().parent().parent().parent().children('dt').text();
                        var id_parrent = $('.navigation dl ul li ').find('.'+res).parent().parent().parent().parent().children('dt a').attr('lang');
                        $('dd').hide();
                        $('.navigation').remove('.active');
                        $('.navigation dl ul li ').find('.'+res).parent().parent().parent().parent().children('dt').addClass('active');
                        $('.navigation dl ul li ').find('.'+res).parent().parent().parent().parent().children('dt.active a').addClass('active');
                        $('.navigation dl ul li ').find('.'+res).parent().parent().parent().parent().children('dd').slideDown();
                        var class_foder = "item_"+id_foderl2_left;                
                        $(".content_right").addClass(class_foder);
                        if(id_foderl2_left != "") 
                        {
                            $.ajax({
                                    url : base_url + 'landing/foder',
                                    data : {idfoder:id_foderl2_left},
                                    type : 'POST',
                                    dataType : 'json',
                                    success : function(data) {                                         
                                                $(".content_right").html('');
                                                $(".content_right").html(data['html']);  
                                    }                                       
                              });
                        }
                    }
                    
                    
                }
                
            }
            
        }
        $('#tracking_title').click(function() {
                
                if($('#tracking_content').is(':hidden') ) {
                    $('html, body').animate({scrollTop:$('.coming_soon').offset().top}, 'slow');
                    $("#tracking_title .tracking_text .tracking_title").html('');
                    var html_click = '<h3>Khách hàng của Admicro có thể sử dụng hệ thống đo kiểm bên thứ ba</h3> <img src="//admicro.vn/skins/default/images/muiten_xuong.png" />';
                    $("#tracking_title .tracking_text .tracking_title").html(html_click);  
                    $('#tracking_content').slideDown('normal');
                    $('#tracking').css('padding-bottom','0px')
                }
                else {  
                    
                    $("#tracking_title .tracking_text .tracking_title").html('');
                    var html_click = '<h3>Khách hàng của Admicro có thể sử dụng hệ thống đo kiểm bên thứ ba</h3> <img src="//admicro.vn/skins/default/images/muiten_ngang.png" />';
                    $("#tracking_title .tracking_text .tracking_title").html(html_click); 
                    $('#tracking_content').slideUp('normal');
                    $('#tracking').css('padding-bottom','13px')
                }
                
            });
	    
    });
</script>

