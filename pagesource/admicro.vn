<script>
$(document).ready(function(){
	//chuột phải hiện POPUP list folder để move file
	$('a#move-file').mousedown(function(){
    	
    	var bg = $('div#popup_bg');
		var obj = $('div#ttv_popup_move_file');
		var btnClose = obj.find('#ttv_popup_close_move_file');		
		
		//Hiện các đối tượng
		bg.animate({opacity:0.2},0).fadeIn(1000); //cho nền trong suốt
		obj.fadeIn(1000).absoluteCenter(); //căn giữa popup và thêm draggable của jquery UI cho phần header của popup

		//Đóng popup khi nhấn nút
		btnClose.click(function(){
			bg.fadeOut(1000);
			obj.fadeOut(1000);
		});

		//Đóng popup khi nhấn background
		bg.click(function(){
			btnClose.click(); //Kế thừa nút đóng ở trên
		});
		
		//Đóng popup khi nhấn nút Esc trên bàn phím
		$(document).keydown(function(e){
			if(e.keyCode==27){
				btnClose.click(); //Kế thừa nút đóng ở trên
			}
		});
		
    });
	//------------------------------------------------------

	//Chọn folder nào thì highlight folder đó lên
	$('li.noClick').click(function(){
		var ul = $('ul.ul_noClick');
		ul.find('li.noClick').removeClass('mf_selected');
		$('.noClick').css('background-color','#ffffff');
		$(this).addClass('mf_selected');
		$('.mf_selected').css('background-color','#666666');
		
	    
	});
	
})
</script><script type="text/javascript">

function shareLink(obj)
{
	var clip = new ZeroClipboard( document.getElementById(obj.id), {
	  moviePath: base_url + "js/default/ZeroClipboard.swf"
	});
	clip.on( 'complete', function(client, args) {
		$('#mess_top').remove();
		$('body').append('<div id="mess_top">Ðã copy url vào Clipboard.</div>');
		setTimeout(function(){
			$('#mess_top').animate({
				opacity: 0
			}, 500);
		}, 3000);
	});
}

$(document).ready(function(){
	$('.btn123').each(function(index){
		shareLink(this);
	});
});
</script>

