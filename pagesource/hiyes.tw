                  //依照num內編號顯示Tab建案輪播ul
                  $(".main_list_content #n" + num).css({ display: "block" });

                  //將停駐點移至新的Tab
                  $(".btn_submenu_c" + num).removeClass("btn_submenu_c" + num)
                                          .addClass("btn_submenu_c" + num + "_o");

                  moveAccount = 1
                  //目前選的ul裡面li的總數
                  howManyIssues = $(".main_list_content #n" + num).children().length;
                  widthIssue = 968;
                  $("#main_l").css({ display: "none" });
                  $("#main_r").css({ display: "none" });

                  if (howManyIssues <= 11) {
                      $("#main_l").css({ display: "none" });
                      $("#main_r").css({ display: "none" });

                      //沒輪播要把timer清掉
                      clearTimeout(_CaseListTimer);

                  } else if (howManyIssues > 11) {
                      $("#main_l").css({ display: "none" });
                      $("#main_r").css({ display: "block" });

                      //建立輪播
                      initAutoPlay($("#main_r"));
                     
                  }



              }

              $(function () {
                  
                  init();

                  //右邊按鈕
                  $("#main_r").bind('click', function (event) {
                      if (!_isMoving) {
                          _isMoving = true;
                          $("#main_l").fadeIn();
                          if (moveAccount >= howManyIssues - 10) {

                          } else {
                              $(".main_list_content #n" + num).animate({ left: "+=" + widthIssue * -1 }, 500, "easeOutQuad", function () {
                                  _isMoving = false;
                              });
                              moveAccount += 11;
                              if (moveAccount >= howManyIssues - 10) {
                                  $(this).fadeOut();
                                  initAutoPlay($("#main_l"));
                              } else {
                                  initAutoPlay($(this));
                              }
                          }
                      }
                  });
                  //左邊按鈕
                  $("#main_l").bind('click', function (event) {
                      if (!_isMoving) {
                          _isMoving = true;
                          $("#main_r").fadeIn();
                          if (moveAccount <= 0) {

                          } else {
                              $(".main_list_content #n" + num).animate({ left: "+=" + widthIssue }, 500, "easeOutQuad", function () {
                                  _isMoving = false;
                              });

                              moveAccount -= 11;

                              if (moveAccount <= 1) {
                                  $(this).fadeOut();
                                  initAutoPlay($("#main_r"));
                              } else {
                                  initAutoPlay($(this));
                              }
                          }

                      }
                  });


                  $("#submenu_cont span").bind('click', function (event) {
                      //讓目前的ul隱藏並且回到原來的位置
                      $(".main_list_content #n" + num).css({ display: "none" });
                      $(".main_list_content #n" + num).css("left", 0);

                      //先移除目前的停駐Tab
                      $(".btn_submenu_c" + num + "_o").removeClass("btn_submenu_c" + num + "_o")
                                                      .addClass("btn_submenu_c" + num);

                      //記錄變更後的停駐編號
                      num = parseInt($(this).children("a").attr("idx"));

                      //重新初始化輪播
                      init();

                  });

                  /**********地區slideshow 結束*************/
              });




    </script>
