              var _CaseList_AutoPlay_Time = 5000; //輪播間隔
              var _isMoving = false;

              //自動輪播初始化
              function initAutoPlay(elem) {
                  clearTimeout(_CaseListTimer);
                  _CaseListTimer = setTimeout(function () {
                      elem.trigger("click");
                  }, _CaseList_AutoPlay_Time);

              }

              //地區slideshow
              function init() {

