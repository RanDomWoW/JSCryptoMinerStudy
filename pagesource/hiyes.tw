	  <script>


	      var _KV_Width;
	      var _isMoving = false;
	      var nowShowIdx = 0;

	      var _KVTimer; //主視覺輪播用
	      var _KV_AutoPlay_Time = 7000;

	      function initAutoplay_KV() {
	          clearTimeout(_KVTimer);
	          _KVTimer = setTimeout(function () {
	              $(".btn_head_next").trigger("click");
	          }, _KV_AutoPlay_Time);
	      }

	      function prepareNextKV(way, idx) {
	          //way: -1(舞台向右移動，左按鈕) or 1(舞台向左移動，右按鈕)
	          var target = $("#KVBackStage .KVElement").eq(idx).clone();

	          target.css("left", (way * _KV_Width) + "px");

	          $("#KVViewPort").append(target);

	      }

	      $(function () {
	          _KV_Width = $("#KVViewPort").width();

	          //綁輪播
	          initAutoplay_KV();

	          $(".btn_head_pre").click(function () {
	              if (!_isMoving) {
	                  _isMoving = true;
	                  nowShowIdx = (nowShowIdx - 1) % $("#KVBackStage .KVElement").length;
	                  nowShowIdx = (nowShowIdx == -1) ? $("#KVBackStage .KVElement").length - 1 : nowShowIdx;

	                  prepareNextKV(-1, nowShowIdx);
	                  $("#KVViewPort .KVElement").animate({ "left": "+=" + _KV_Width }, 500, function () {
	                      _isMoving = false;

	                      if ($(this).css("left") != "0px") $(this).remove();

                          //綁輪播
	                      initAutoplay_KV();
	                  });


	              }
	          });
	          $(".btn_head_next").click(function () {
	              if (!_isMoving) {
	                  nowShowIdx = (nowShowIdx + 1) % $("#KVBackStage .KVElement").length;

	                  prepareNextKV(1, nowShowIdx);
	                  $("#KVViewPort .KVElement").animate({ "left": "-=" + _KV_Width }, 500, function () {
	                      _isMoving = false;
	                      if ($(this).css("left") != "0px") $(this).remove();

	                      //綁輪播
	                      initAutoplay_KV();
	                  });
	              }
	          });
	      });



	  </script>
