    <script>
                    (function(container){
                var TT = {
                    appReady: false,
                    currency: 'EUR',
                    currencyText: '€'
                };

                TT.utils = {
                    unifyList: function (valueList) {
                        var result = [];

                        for (var t = 0; t < valueList.length; t++) {
                            if (result.indexOf(valueList[t]) === -1) {
                                result.push(valueList[t]);
                            }
                        }

                        return result;
                    },
                    toArray: function(obj) {
                        var array = [];

                        for (key in obj) {
                            array.push(obj[key]);
                        }
                        
                        return array;
                    },
                    getDeviceType: function() {
                        var deviceMinWidths = {
                            desktop: 1024,
                            tablet: 768,
                            mobile: 320,
                            xs: 0
                        };

                        for (var device in deviceMinWidths) {
                            if (window.screen.width >= deviceMinWidths[device]) {
                                return device;
                            }
                        }
                    }
                };

                container.TT = TT;
            })(window);
            </script>

                                

