		// JQuery plugin to load and insert image into an element
		$.fn.refimage = function(src, f, ref){
			return this.each(function(){
			var i = new Image();
			i.src = src;
			i.onload = f;
			obj = $(this);
			if(obj.find(ref))
				obj.find(ref).append(i);
			else
				this.appendChild(i);
		});
		}    

		// tabs plugin to load images on demand
		$.tools.tabs.addEffect("loadTabImage", function(tabIndex, done) {
			
			var idx = tabIndex+1;
			// find the correct DIV
			var tabSpan = "div#pane"+idx + ".loading";
			// get image url for the (loading) div
			var murl = "http://www.arlt.com/out/pictures/wysiwigpro/image_"+idx+".jpg";

			// use ratzfatz image?
			if(imgArr[tabIndex])
				murl = imgArr[tabIndex];

			// remove loading class to load image and append only once
			$(tabSpan).removeClass('loading').refimage(murl, function () {
				$(this).fadeIn();
			}, "a");

			// now fade current tab in
			var conf = this.getConf(),
			speed = conf.fadeOutSpeed,
			panes = this.getPanes();
			if (speed) {
				panes.fadeOut(speed);
			} else {
				panes.hide();
			}
			panes.eq(tabIndex).fadeIn(conf.fadeInSpeed, done);

			// the supplied callback must be called after the effect has finished its job
