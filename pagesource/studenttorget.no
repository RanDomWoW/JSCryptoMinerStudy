        function renderInCenter(target) {
            browserWidth = document.documentElement.clientWidth - target.width();
            browserHeight = document.documentElement.clientHeight - target.height();
            scrollX = document.documentElement.scrollLeft || document.body.scrollLeft;
            scrollY = document.documentElement.scrollTop || document.body.scrollTop;
            var _pos =
                { x: (browserWidth / 2) + scrollX, y: (browserHeight / 2) + scrollY };
            target.css({ top: _pos.y, left: _pos.x });
        }

        function renderInTop(target) {
            target.css({ top: 0 });
