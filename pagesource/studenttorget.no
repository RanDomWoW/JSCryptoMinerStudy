    <script type="text/javascript">
        function showCandidateLogin(rs, src, url, isxhr, id, type, target, mode) {
            makeLoginContainer();

            rs = rs | 0;
            var postdata = {};
            postdata.id = 0;
            if (src) {
                postdata.from = src;
                postdata.objectid = id;
            }
            if (url) {
                postdata.url = url;
            }
            if (isxhr) {
                postdata.isxhr = true;
                if (type && target && mode) {
                    postdata.type = type;
                    postdata.target = target;
                    postdata.mode = mode;
                }
            }
            if (rs === 0) {
                postdata.regUrl = false;
            }
            else {
                postdata.regUrl = true;
            }
            $("#logincntr").load("/Account/CandidateLoginForm", postdata);
        }
    </script>

