<script>
  var bc_totalpage = 15;
  var bc_timeoutH;
  var bc_current=1;
  var bc_history=1;
  var bc_play = true;
  function play_stop(){
    bc_play = !bc_play;
    if(bc_play){
      update_timer();
      $('.btn_play > img').attr('src', 'project/cns/html/images/btn_stop.jpg');
    } else {
      $('.btn_play > img').attr('src', 'project/cns/html/images/btn_play.jpg');
    }
  }
  function next_china(){
    if(!bc_play){return;}
    if(bc_current == bc_totalpage){
      get_home_china(1, $('.home_pagination img:nth-child(1)'));
    } else {
      get_home_china(bc_current+1, $('.home_pagination img:nth-child('+bc_current+1+')'));
    }
  }
  function update_timer(){
    clearTimeout(bc_timeoutH);
    bc_timeoutH = setTimeout(function(){ next_china() }, 5000);
  }
  $(window).load(function(){
    $('#home_beautiful_china').height($('#hbc_'+bc_current).height());
  });
  $(document).ready(function(){
    setTimeout(function(){
      $('#home_beautiful_china').height($('#hbc_'+bc_current).height());
    }, 100);
    update_timer();
  });
  
  function get_home_china(call, img){
    if(call == bc_current){ return; }
    function slide_div(){
      $('#hbc_'+bc_history).animate({"left":(-$('#home_beautiful_china').width())+"px"}, "fast");
      $('#hbc_'+bc_current).show().css({left:$('#home_beautiful_china').width()+'px', 'top': '0px'})  // Set the left to its calculated position
             .animate({"left":"0px"}, "fast", function(){
              $('#home_beautiful_china').height($('#hbc_'+bc_current).height());
              $('#hbc_'+bc_history).hide();
      });
    }
    update_timer();
    bc_history = bc_current;
    bc_current = call;
    
    $('.home_pagination > img').attr('src', 'project/cns/html/images/topic/pagination_dot.png');
    $('.home_pagination > img:nth-child('+call+')').attr('src', 'project/cns/html/images/topic/pagination_dot_select.png');
    if($('#hbc_'+bc_current).length){
      slide_div();
    } else {
      var jqxhr = $.ajax( '?file=box_home_beautiful_china&call='+call )
      .done(function(data) {
        var child = $(data);
        $('#home_beautiful_china').append(child);
        slide_div();
      })
      .fail(function() {
        //alert( "error" );
      })
      .always(function() {
        //alert( "complete" );
      });
    }
    
  }
  $(document).ready(function() {
    $(window).resize(function(){
      $('#home_beautiful_china').height($('#hbc_'+bc_current).height());
    });
  });
</script>
