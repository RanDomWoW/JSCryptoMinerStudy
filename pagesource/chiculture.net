    );
    $.fn.textWidth = function(){
      var html_org = $(this).html();
      var html_calc = $('<span>' + html_org + '</span>');
      $('#text-width').append(html_calc);
      var width = html_calc.width();
      html_calc.remove();
      return width;
    };
    $('.scroller').css('width', ($('.scroller').textWidth()+100)+'px');
    $('.scroller2').css('width', ($('.scroller').textWidth()+100)+'px');



    if($('.scroller').textWidth() > $('.news_details').width()){
         var scroller1, scroller2, scroller3;
        scroller1 = $('.scroller');
        scroller2 = $('.scroller2');
        scroller2.css('left', (scroller1.width()+20)+'px');
        function scrolltoptext(){
            if(!lockscrolltext){
                var left =  parseInt(scroller1.css('left').replace('px', ''));
              if(left > -scroller1.width()){
                scroller1.css('left', (left-1)+'px');
              } else {
                //scroller1.css('left', $('#banner_text > .container').width()+'px');
                scroller3 = scroller1;
                scroller1 = scroller2;
                scroller2 = scroller3;
              }
              //console.log((left+scroller1.width()))
              scroller2.css('left', (left+scroller1.width()+10)+'px');
            }
          setTimeout(scrolltoptext, 20);
        }
        setTimeout(scrolltoptext, 3000);
    } else {
        $('.scroller2').hide();
    }
});
</script>
