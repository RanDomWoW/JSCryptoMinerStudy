	<script type="text/javascript">
    var isLogin = false;
    var moduleId = null;  
    var user = new $ApprrrUser("");
    var data = [];
	  var auth = false;

    var mibpgPreloader = null;
    var loginPreloader = null;

    var h2Title = null;
	  var modalLogin = null;
    var modalModules = null;
    var modalMibpg = null;
    
    var ulModules = null;
    var txtMibpg = null;

	  var anchLogin = null;
	  var loginModal = null;
	  var msgLogin = null;
	  var txtLoginUsername = null;
	  var txtLoginPass = null;

    function clearLogin() {
      msgLogin.innerHTML = "";
      txtLoginUsername.value = "";
      txtLoginPass.value = "";
      txtMibpg.value = "";
    }

	  function showM() {
      clearLogin();
      if (auth) {
        modalLogin.style.display = "none";
        modalModules.style.display = "block";
      }
      else {
        modalLogin.style.display = "block";
        modalModules.style.display = "none";
      }
      modalMibpg.style.display = "none";
	    loginModal.show();
	  }

	  function loadLogin() {
      h2Title = $get('ctlLogin_h2Title');
	    modalLogin = $get("modalLogin");
      modalModules = $get("modalModules");
      modalMibpg = $get("modalMibpg");

      txtMibpg = $get("ctlLogin_txtMibpg");

      ulModules = $get('ulModules');

      mibpgPreloader = $get('mibpgPreloader');
      loginPreloader = $get('loginPreloader');

	    if (auth) {
	      modalLogin.style.display = "none";
        modalModules.style.display = "block";
        modalMibpg.style.display = "none"

        onGetModulesSuccess(data);
	    }
      	    
      txtLoginUsername = $get("txtLoginUsername");
	    txtLoginPass = $get("txtLoginPass");
	    loginModal = $find("ctlLogin_loginModal");
	    anchLogin = $get('anchLogin');
	    msgLogin = $get("ctlLogin_msgLogin");

	    $addHandler(anchLogin, "click", showM);
	    $addHandler($get("btnLogin"), "click", loginBtnClicked);
      $addHandler($get("anchMibpg"), "click", validateMIBPG);
	  }

    function unloadLogin() {
      $removeHandler(anchLogin, "click", showM);
	    $removeHandler($get("btnLogin"), "click", loginBtnClicked);
      $removeHandler($get("anchMibpg"), "click", validateMIBPG);
    }

	  function loginBtnClicked(evt) {
	    logOn();
	  }

	  function logOn() {
      if (isLogin) {
        return;
      }

      loginPreloader.style.display = "block";

	    if (IsEmptyString(txtLoginUsername.value) || IsEmptyString(txtLoginPass.value)) {
	      msgLogin.innerHTML = 'Molimo da pravilno ispunite polja za prijavu.';	  
        msgLogin.style.display = "block";
        loginPreloader.style.display = "none";
	      return;
	    }

      msgLogin.style.display = "none";

      isLogin = true;
	    $ApprrrUser.AuthenticateUser([txtLoginUsername.value, txtLoginPass.value], onLoginSuccess, onLogFailure);
	  }

    function onMibpgAuthSuccess(res) {
      mibpgPreloader.style.display = "none";
      document.location = "http://www.apprrr.hr/Components/Handlers/ModuleHandler.ashx?m=" + moduleId;
    }

    function validateMIBPG() {
      mibpgPreloader.style.display = "block";

      if (txtMibpg.value.length == 0) {
        msgLogin.innerHTML = 'MIBPG polje je obavezno.';
        msgLogin.style.display = "block";
        mibpgPreloader.style.display = "none";
        return false;
      }

      user.MibpgAuthenticate([txtMibpg.value], onMibpgAuthSuccess, onLogFailure);

      msgLogin.style.display = "none";
      return true;
    }

    function checkMIBPG(id) {
      moduleId = id;
      h2Title.innerHTML = "MIBPG";
	    modalLogin.style.display = "none";
      modalModules.style.display = "none";
      modalMibpg.style.display = "block"
    }

    function checkOIB(id) {
      moduleId = id;
    }

    function onGetModulesSuccess(res) {
      ulModules.innerHTML = "";

      for (var i = 0; i < res.length; i++) {
        var a = document.createElement("a");
        a.appendChild(document.createTextNode(res[i].Name));

        switch (res[i].ValidationType) {
          case 0:
            a.setAttribute("href", "http://www.apprrr.hr/Components/Handlers/ModuleHandler.ashx?m=" + res[i].Id);
            break;
          case 1:
            a.setAttribute("href", "javascript:checkMIBPG(" + res[i].Id + ");");
            a.setAttribute("onclick", "checkOIB(" + res[i].Id + ");");
            break;
          case 2:
            a.setAttribute("href", "javascript:checkOIB(" + res[i].Id + ");");
            a.setAttribute("onclick", "checkOIB(" + res[i].Id + ");");
            break;
          default:
            a.setAttribute("href", "javascript:void(0);");
            break;
        }        

        var li = document.createElement("li");
        li.appendChild(a);
        ulModules.appendChild(li);
      }

      h2Title.innerHTML = 'Arkod moduli';
	    modalLogin.style.display = "none";
      modalModules.style.display = "block";
      modalMibpg.style.display = "none"
    }

	  function onLoginSuccess(res) {
      loginPreloader.style.display = "none";
      isLogin = false;
      user = res;
      auth = true;
      user.GetModules([], onGetModulesSuccess, onLogFailure);
	  }

	  function onLogFailure(e) {
      loginPreloader.style.display = "none";
      mibpgPreloader.style.display = "none";
      isLogin = false;
      if (e.ErrorCode == 1001) {
        clearLogin();
        modalMibpg.style.display = "none";
        modalLogin.style.display = "block";

        msgLogin.innerHTML = 'Sessija je istekla, molimo Vas da se ponovo ulogirate.';
      }
      else {
	      msgLogin.innerHTML = e.Message;
      }
      msgLogin.style.display = "block";
	  }

	  Sys.Application.add_load(loadLogin);
	  Sys.Application.add_unload(unloadLogin);
  </script>

