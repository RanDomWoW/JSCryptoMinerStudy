            }
            else if (eventKeyCode == Sys.UI.Key.enter) {
                if (preventDefaultAction)
                    ev.preventDefault();
                if (enterFunction != null)
                    enterFunction();
            }
        }
        function IsEmptyString(str) {
            return str.replace(/^\s+|\s+$/g, '').length == 0;
        }
        function topMasterLoad() {
            $addHandler(document, "keypress", documentKeypress);
        }

        function topMasterUnload() {
            $removeHandler(document, "keypress", documentKeypress);
        }

        Sys.Application.add_load(topMasterLoad);
