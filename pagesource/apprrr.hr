            var eventKeyCode = ev.keyCode ? ev.keyCode : ev.rawEvent.keyCode;

            if (eventKeyCode == Sys.UI.Key.esc) {
                if (preventDefaultAction)
                    ev.preventDefault();
                if (escFunction != null)
