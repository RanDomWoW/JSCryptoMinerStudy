<script type="text/javascript">
$(document).ready(function(){
        $(".local-red").css("opacity","0.4");
setInterval(function() {
       if($(".local-red").css("opacity") == 0){
  $(".local-red").css("opacity","1");
 }else{
  $(".local-red").css("opacity","0");
 }  }, 600);


    var cars = [
        '/img/icon/black.png'
        , '/img/icon/blue.png'
        , '/img/icon/taxi-black.png'
        , '/img/icon/taxi-white.png'
    ];

    $('#footer-car')
        .css('background', 'url(' + cars[Math.floor(Math.random() * (3 - 0 + 1)) + 0] + ')')
        .css('background-repeat', 'no-repeat')
        .css('background-size', '100% 100%');

    function vehicle_animate() {
        if(parseInt($("#footer-car").css('left')) > parseInt($('body').width())) {
            $("#footer-car").css("left", $('#footer-car').data('left'));
        }
        $("#footer-car").animate({
            "left": "+=100px"
        }, 900, 'linear', vehicle_animate);
    }

    $(document).ready(function(){
        vehicle_animate();
    });
});
</script>


