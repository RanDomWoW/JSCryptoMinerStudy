<script type="text/javascript">

$(document).ready(function() {
    getState();
    function displayModal(title, message, type = 'success', align = 'center', reload = false){
        if(type == 'success'){
            color = '#3CB371';
        }else if(type == 'error'){
            color = '#B22222';
        }
        if(reload == true){
            $('#reload').show();
            $('#noreload').hide();
        }else{
            $('#reload').hide();
            $('#noreload').show();    
        }
        $('#modalInfo .modal-content').css('background-color', color);
        $('#modalInfo #modal-content').css('text-align', align);
        $('#modalInfo #modal-title').text(title);
        $('#modalInfo #modal-text').html(message);
        $('#modalInfo').modal('show');
    }

    $('#form-city').validate({
        rules: {
            email: {
                required: true
                , email: true
            }
            , city_name: "required"
            , city: {
                required: true
                , number: true
            }
        }
        , messages: {
            email: "E-mail é necessário"
            , city_name: "Estado é necessária"
            , city: "Cidade é necessária"
            
        }
        , showErrors: function(errorMap, errorList) {
            var message = '';
            var element = '';
            var display = false;

            $.each(errorList, function (index, error) {
                display = true;
                message += '<li>' + error.message + '</li>';       
                element += error.element;        
            });

            if(display == true){
                displayModal('Ops, preencha os campos corretamente!', message, 'error', 'left');
            }
        }
    });
});
function getState(){
    $("#inputCity").block({
          message: '<img src="/image/loading4move64x64.gif" width="32" />',
          css : { border: '0px', borderRadius: '10px' }
        });
     $.ajax({
            url: "https://www.4move.me/api/getstate"
            , method: 'get'
            , data: {}
            , success: function(result) {
                //application.cities = result;
                //html = '<option>Estado</option>';
                $('#inputCity').append($('<option>', { 
                    value: 'Estado', 
                    text: 'Estado'
                }));
                for (var i = 0; i <= (result.object.length-1); i++) {
                    $('#inputCity').append($('<option>', { 
                        value: result.object[i].state,
                        text: result.object[i].state 
                    }));
                   // html += '<option class="stateSelect" data-val="' + result.object[i].state + '</option>';
                }
                //$('#inputCity').html(html);
            }
            ,error: function(result){
                $("#inputCity").html("Erro ao localizar cidade"); 
            }
            ,complete: function(result){
                $("#inputCity").unblock();
            }
        });

}
    
    

</script>
