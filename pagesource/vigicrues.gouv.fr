  <script>

    document.getElementById('geoloc').addEventListener('click',getLocation);

    function getLocation() {
        if (navigator.geolocation) {
            navigator.geolocation.getCurrentPosition(displayPosition, showError, {maximumAge:600000,timeout:10000});
        } else {
            alert("La géolocalisation n'est pas supportée par ce navigateur.");
        }
    }

    function showError(error) {
        switch(error.code) {
            case error.PERMISSION_DENIED:
                alert("L'utilisateur a refusé les requêtes de géolocalisation.");
                break;
            case error.POSITION_UNAVAILABLE:
                alert("L'information de géolocalisation est indisponible.");
                break;
            case error.TIMEOUT:
                alert("La requête pour récupérer la géolocalisation de l'utilisateur a dépassé les délais.");
                break;
            case error.UNKNOWN_ERROR:
                alert("Une erreur inconnue s'est produite.");
                break;
        }
    }

    function displayPosition(position) {
//      alert("Latitude: " + position.coords.latitude + ", Longitude: " + position.coords.longitude);
      var url = "./services/geolocalisation/bulletin/index.php?latitude="+position.coords.latitude+"&longitude="+position.coords.longitude+"&callback=?";

      $.getJSON(url, function(json) {
        // Redirection
        if(json && json.GeolocInfoVigiCru && json.GeolocInfoVigiCru.CdEntVigiCru) {
          document.location.href = "./niv2-bassin.php?CdEntVigiCru="+json.GeolocInfoVigiCru.CdEntVigiCru;
        }
      });
    }
  </script>

