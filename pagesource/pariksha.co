          processData: false,
          contentType: false
      })
      .done(function(res) {
          var q=JSON.parse(res);
          if(q['status']==0){
            $div.removeClass("hide").html('<span>'+q['message']+'</span>');
            setTimeout(function(){
              $div3.addClass("hide")
            },3000);  
          }else if(q['status']==1){
