    $("#modal-log-in").on("show.bs.modal", function (e) {
      $("#InvalidEmail").addClass("hide");
      $div.addClass("hide").html("");
      //if($("body").attr("data-origin") == "MP"){
      var redirect_URI = window.location.href;
      
      //GATE SPECIFIC
      var originBody = $("body").attr("data-origin");
      if(originBody == "gate" || originBody == "jee" || originBody == "prime"){
        var temp = $(e.relatedTarget).attr("data-extra");
        console.log(e.relatedTarget);
        
        if(temp){
          redirect_URI = temp;
        }
        var arr = {gate:10001, jee:1, prime:10001};
        $("#TSSignup").attr("onclick","window.location.href='/users/signup/"+arr[originBody]+"?redirect_uri="+encodeURIComponent("https://www.pariksha.co"+redirect_URI)+"'");
      }
      
      //$("#redirect_URI").text(redirect_URI);
