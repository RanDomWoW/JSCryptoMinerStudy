<script src="https://apis.google.com/js/api:client.js"></script>
<script async>
  var googleUser = {};
  var startApp = function() {
    gapi.load('auth2', function(){
      // Retrieve the singleton for the GoogleAuth library and set up the client.
      auth2 = gapi.auth2.init({
        client_id: '485753397831-fq8jn0si6hjf0amaopg9u4ehpdp4b7or.apps.googleusercontent.com',
        cookiepolicy: 'single_host_origin',
        // Request scopes in addition to 'profile' and 'email'
        //scope: 'https://www.googleapis.com/auth/plus.login'
      });
      if($("#GplusSignup").length!=0){
        attachSignin(document.getElementById('GplusSignup'));
      }
      attachSignin(document.getElementById('GplusLogin'));
    });

  };
  
  function getParameterByName(name) {
      var match = RegExp('[?&]' + name + '=([^&]*)').exec(window.location.search);
      return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
  }

  function attachSignin(element) {
    auth2.attachClickHandler(element, {},
      function(googleUser) {
        $("#InvalidEmail").addClass("hide");
        /*document.getElementById('buttonText').innerText = "Signed in with Google"; //+ googleUser.getBasicProfile().getName();*/
        //console.log(googleUser);
        var redirect_URI = $("#redirectURIForm2").val();
        
        var params = '';
        var id_token = googleUser.getAuthResponse().id_token;
        params += 'idtoken=' + id_token;
        var origin = element.getAttribute("id");
        params += '&origin=' + origin;
        if(origin == 'GplusSignup'){
          params += '&examCode=' + $("#signUpExam").val();
          var ref = $("#refForm").val();
          if(ref != ""){
            params += '&ref=' + ref;
          }
          
          var temp = getParameterByName("redirect_uri");
          if(temp){
            redirect_URI = temp;
          }
          /*var series_id = $("#series_id").text();
          if(series_id != ""){
            params += '&series_id=' + series_id; 
          }*/
        }

        if(redirect_URI != ""){
          params += '&redirect_uri=' + encodeURIComponent(redirect_URI);
        }
        var xhr = new XMLHttpRequest();
        xhr.open('POST', 'https://www.pariksha.co/users/gplusSignin');
        xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
        xhr.onload = function() {
          var resp = JSON.parse(xhr.responseText);
          if(resp['status']==1){
            if(resp['redirect_uri'] && resp['redirect_uri'] !== '/'){
              window.location.href= resp['redirect_uri'];
            }else{
              window.location.href= "/students/index"+window.location.search;
            }
          }else if(resp['status']==-1 && origin=="GplusLogin"){
            $("#InvalidEmail").removeClass("hide");
          }else{
            alert(resp['message']);
          }
        };
        xhr.send(params);
      }, function(error) {
        alert(JSON.stringify(error, undefined, 2));
      }
    );
  }
  
  startApp();
  $(document).ready(function () {
    // Modal
    var log_in=getParameterByName("log_in");
    if(log_in=="1"){
      email=getParameterByName("email");
      if(email){
        $("#UserUsername").val(email);
      }
      $("#modal-log-in").modal("show");
    }
  })
</script></div>

