<script async>
  function toggleModals(oldM,newM){
    if(($("#"+oldM).data('bs.modal') || {}).isShown){
      $("#"+oldM).on("hidden.bs.modal", function () { 
          $("#"+oldM).off("hidden.bs.modal");
          $("#"+newM).modal('show');
      }).modal('hide');
    }else{
      $("#"+newM).modal('show');
    }
  }

  function examSelected(exam,series_id){
    if(exam=='placements'){
      window.location.href='/placements';
    }else if(exam=='prime'){
      window.location.href='/prime';
    }else{
      window.location.href='/users/signup/'+exam;
    }
  }

  function targetYearSelected(index){
