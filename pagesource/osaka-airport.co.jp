<!-- /.col --></div>
<script type="text/javascript">
document.addEventListener('DOMContentLoaded', function() {
  var now = new Date();
  var datetime = now.getFullYear() + ( "0"+( now.getMonth()+1 ) ).slice(-2) + ( "0"+now.getDate() ).slice(-2) + ( "0"+now.getHours() ).slice(-2) + ( "0"+now.getMinutes() ).slice(-2) + ( "0"+now.getSeconds() ).slice(-2);
  jQuery.ajax({
    type: 'GET',
    url: '/json/weather/point-itm.json?time=' + datetime,
    dataType: 'json',
    success: function(json){
      // 現在時刻よりキーを生成
      var now = new Date();
      var hour = now.getHours();
      var key = Math.floor(hour / 3);

      // 7時までは前日分2日目のデータを使用する(更新が7時頃のため)
      var day = hour < 7 ? 1 : 0;

      // 天気コード
      var code = json.forecast[day].weather[key];

      // 天気と気温
      var wind = json.forecast[day]['weather_ja'][key] + '<br>' + json.forecast[day]['temperature'][key] + '&deg;C';

      jQuery("#weather-image").attr('src', '/sites/all/themes/kix/images/common/weather_pnt/' + code + '.png');
      jQuery("#weather-detail").append(wind);
    }
  });
});
</script>

