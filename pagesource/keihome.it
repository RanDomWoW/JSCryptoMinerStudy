        <script type="text/javascript">
        jQuery.noConflict();
            (function ($) {
                $(function () {
                

                    <!-- link to other page -->
                    $('.login-redirect').click(function ()
                    {
                        var url = $(this).attr('url');
                        self.location = url;
                    });
                    
                    
                    var login_url_origin = "/"; 
                    
                    <!-- on click on a button, the relative box goes fadeIn and the others go fadeOut -->
                    
                    $('.access-button-login').click(function ()
                    {
                        $('#change-modal-login').fadeIn();
                        $('#change-modal-registration').fadeOut();
                        $('#change-modal-recovery').fadeOut();
                        
                        this_url_origin = $(this).attr('url_origin');
                        
                        
                        if (typeof this_url_origin !== typeof undefined && this_url_origin !== false) {
                            login_url_origin = this_url_origin;
                        }
                        
                    });
                    
                    $('.access-button-registration').click(function ()
                    {
                        $('#change-modal-login').fadeOut();
                        $('#change-modal-registration').fadeIn();
                        $('#change-modal-recovery').fadeOut();
                    });
                    
                    $('.access-button-recovery').click(function ()
                    {
                        $('#change-modal-login').fadeOut();
                        $('#change-modal-registration').fadeOut();
                        $('#change-modal-recovery').fadeIn();
                    });
                    
                    
                    
                    
                    <!-- ajax for the login access -->
                    
                    
                    $('#modal-login-submit').click(function() {
                        
                        var username = $('#modal-login-email').val();
                        var password = $('#modal-login-password').val();
                        
                        
                        $.ajax({
                        
                            type: 'POST',
                            url: 'http://www.keihome.it/ajax/modal-login.php',
                            data: { login_email : username, login_password : password, login_checkbox : '1', url_origin : login_url_origin },
                            dataType: 'json',
                            
                            success: function (data) {
                                
                                
                                if (data.error_login == true) {
                                    $('#modal-login-email').removeClass(data.class_error_name).addClass(data.email_class_error);
                                    $('#modal-login-password').removeClass(data.class_error_name).addClass(data.password_class_error);
                                    
                                
                                } else {
                                    self.location = data.url_origin;
                                    
                                }
                                
                            },
                            error : function (request, status, error) {
                                
                                
                            }
                            
                        });
                        
                        
                    });
                    
                    
                    
                    
                    
                    <!-- ajax for the registration submit -->
                    
                    
                    $('#modal-registration-submit').click(function() {
                    
                        //e.preventDefault();
                        
                        var name = $('#modal-registration-name').val();
                        var surname = $('#modal-registration-surname').val();
                        var email = $('#modal-registration-email').val();
                        var email_confirm = $('#modal-registration-email-confirm').val();
                        var password = $('#modal-registration-password').val();
                        var password_confirm = $('#modal-registration-password-confirm').val();
                        var newsletter = $('#modal-registration-newsletter').val();
                        
                        if ($("#modal-registration-privacy").is(":checked") == true) {
                            var privacy = '1';
                        } else {
                            var privacy = '0';
                        }
                        
                        
                        
                        $.ajax({
                        
                            type: 'POST',
                            url: 'http://www.keihome.it/ajax/modal-registration.php',
                            data: { registration_name : name, registration_surname : surname, registration_email : email, registration_email_confirm : email_confirm, registration_password : password, registration_password_confirm : password_confirm, registration_privacy : privacy, registration_newsletter : newsletter },
                            dataType: 'json',
                            
                            success: function (data) {
                                
                                
                                if (data.error_registration == true) {
                                
                                    $('#modal-registration-name').removeClass(data.class_error_name).addClass(data.name_class_error);
                                    $('#modal-registration-surname').removeClass(data.class_error_name).addClass(data.surname_class_error);
                                    $('#modal-registration-email').removeClass(data.class_error_name).addClass(data.email_class_error);
                                    $('#modal-registration-email-confirm').removeClass(data.class_error_name).addClass(data.email_confirm_class_error);
                                    $('#modal-registration-password').removeClass(data.class_error_name).addClass(data.password_class_error);
                                    $('#modal-registration-password-confirm').removeClass(data.class_error_name).addClass(data.password_confirm_class_error);
                                    $('#modal-registration-privacy-container').removeClass(data.class_error_name).addClass(data.privacy_class_error);
                                    
                                    $('#modal-registration-error-string').html(data.error);
                                    
                                    
                                    
                                } else {
                                
                                    self.location = data.url_redirect;
                                    
                                }
                                
                            },
                            error : function (request, status, error) {
                                
                            }
                            
                        });

                        
                    });
                    
                    
                    
                    
                    
                    <!-- ajax for the password recovery -->
                    
                    
                    $('#modal-recovery-submit').click(function() {
                        
                        var email = $('#modal-recovery-email').val();
                        
                        
                        $.ajax({
                            
                            type: 'POST',
                            url: 'http://www.keihome.it/ajax/modal-recovery.php',
                            data: { recovery_email : email },
                            dataType: 'json',
                            
                            success: function (data) {
                                
                                $('#modal-recovery-email').removeClass(data.class_error_name).addClass(data.email_class_error);
                                $('#modal-recovery-msg').html(data.error);
                                
                            },
                            error : function (request, status, error) {
                                
                            }
                            
                        });

                        
                    });
                    


                });
            })
        (jQuery);

        </script>
        
        
        
        
        

