    $scope.getRandomColor = function (index) {

      var myArray = ['#fec606', '#3197d6', '#8870ff', '#233e75', '#a6c446', '#d6cbc7', '#89cff3', '#000000', '#7fcbdb', '#8bc34a'];

      if (myArray.length > index ) {

        return myArray[index];

      } else {

        return myArray[Math.floor(Math.random() * myArray.length)];

      }
    }

  });

  app.controller('NovidadesCtrl', function ($scope, $http) {
    $scope.viewModel = viewModel;
