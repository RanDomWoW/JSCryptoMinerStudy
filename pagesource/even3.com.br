      return $sce.trustAsHtml(text);
    };
  }]);

  app.controller('PricingCtrl', function ($scope, $http) {
    $scope.valorMaximo = 500;
    $scope.sliderVisivel = false;
    $scope.mostrarSlider = function () {
      $scope.sliderVisivel = true;
      $scope.$broadcast('rzSliderForceRender');
    }
    $scope.slider = {
      value: 30,
      options: {
        floor: 0,
        ceil: $scope.valorMaximo,
        translate: function (value) {
          var precoCalculo = value - 30;
          var titulo = ' trabalhos';

          if (value <= 30) {
            $scope.precoModulo = 0;
          } else if (value <= 100) {
            var preco = 4.99;
            var total = preco * precoCalculo;
            $scope.precoModulo = total;
          } else if (value <= 400) {
            var preco = 4.49;
            var total = preco * precoCalculo;
            $scope.precoModulo = total;
          } else {
            var preco = 3.99;
            var total = preco * precoCalculo;
            $scope.precoModulo = total;
          }
          $scope.quantidade = value;

          if (value <= 1) {
            titulo = ' trabalho';
          }

          return value + titulo;
        }
      },
      stepValues: [100, 200, 300, 400]
    };
  });

  $(document).ready(function () {
    $('#quote-carousel').carousel({
      pause: true,
      interval: false,
    });
  });

</script>

