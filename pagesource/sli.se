<script>
    var fwModalWindow = {
        id: 'a-modal-window',
        load: function (control_path, args) {
            var data = {
                'method' : 5,
                'cp' : control_path
            };

            for (var key in args) {
                if (args.hasOwnProperty(key)) {
                    data[key] = args[key];
                }
            }

            ajaxPostCall('/framework/logic/_calls.php', data, 'text')
                .done(function (response) {
                    if (response != '') {
                        $.when($('#a-modal-window-container').html(response)).then(
                            $('#' + fwModalWindow.id).modal('show')
                        );
                    }
                })
                .fail(function () {

                });
        },
        hide: function() {
            $('#' + this.id).modal('hide');
        }
    };
