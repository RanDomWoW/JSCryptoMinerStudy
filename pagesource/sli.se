<script src="/static/4.5.6/framework/vendors/jquery-validate/jquery.validate.min.js" type="text/javascript"></script>
<script type="text/javascript">

    $(document).ready(function() {

        //Reset alla formulärfält, för de som har autofill påslaget
        $('#member_form')[0].reset();

        $('#mem_email').blur(checkWhitelist);

        $("input[type='text']").bind("keypress", function(e) {
            if (e.keyCode === 13) {
                checkWhitelist();
                return false;
            }
        });

        $('#mem_school').focus(function() {
            $(".schoolnamesearchinfo").hide();
            $(".arrow-left").hide();
            $(this).trigger('keydown.autocomplete');
        });

        $("#showinfotext").hover(function() {
            $(".schoolnamesearchinfo").show("2000");
            $(".arrow-left").show("2000");
        }, function() {
            $(".schoolnamesearchinfo").hide();
            $(".arrow-left").hide();
        });

        $("#member_form").validate({
            onkeyup: false,
            rules: {
                mem_first_name: {
                    required: true,
                    minlength: 2
                },
                mem_last_name: {
                    required: true,
                    minlength: 2
                },
                mem_user_type: {
                    required: true
                },
                mem_email: {
                    required: true,
                    email: true
                }
            },
            messages: {
                mem_first_name: {
                    required: "Ange förnamn",
                    minlength: jQuery.format("Ange minst {0} bokstäver")
                },
                mem_last_name: {
                    required: "Ange efternamn",
                    minlength: jQuery.format("Ange minst {0} bokstäver")
                },
                mem_email: {
                    required: "Du måste välja e-postadress",
                    email: "Ange en giltig e-postadress"
                },
                mem_user_type: {
                    required: "Ange typ"
                }

            }, errorElement: "div",
            submitHandler: function() {
                if (validateSchoolNow() && validateMunicipalityNow()) {
                    form.submit();
                }
            },
            success: function(div) {
                div.html("ok").addClass("checked");
            }
        });

        $('.label_check, .label_radio').click(function(a) {
            $(a).attr("checked", "checked");
            setupLabel();
        });

        setupLabel();

        $("#mem_school").focus(validateSchoolMunicipality);
        $("#mem_school").blur(validateSchool);
        $("#mem_municipality").blur(validateMunicipality);

        $(".label_radio input").each(function() {
            $(this).css({"opacity": "0", "visibility": "visible"});
        });

        $("#radio-01").css("cursor", "pointer");

        $(".error").each(function() {
            if ($(this).html() !== "") {
                $(this).css("display", "block");
            }
        });

        if ($($(".schoolerror")[0]).html() !== "") {
            $(".schoolerror").css("display", "block");
        }

        $("#login_email").focus();

        $("#mem_municipality").autocomplete({
            source: function(request, response) {
                var data_item = {
                    'method':'7',
                    's': request.term
                };
                ajaxPostCall('/framework/logic/_calls.php', data_item, 'json')
                    .done(function (data) {
                        response($.map(data.message, function (item) {
                            return {
                                label: item.name,
                                id: item.id
                            };
                        }));
                    })
                    .fail(function () {

                    });
            },
            minLength: 1,
            select: function(event, ui) {
                $('#mem_municipality_id').val(ui.item.id);
                $('#mem_municipality_name').val(ui.item.label);
                $('#mem_school_id').val('');
                $('#mem_school_name').val('');
                $('#mem_school').val('');
            }
        });

        $("#mem_school").autocomplete({
            source: function(request, response) {
                var data_item = {
                    'method':'8',
                    's': request.term,
                    'mi': $('#mem_municipality_id').val()
                };
                ajaxPostCall('/framework/logic/_calls.php', data_item, 'json')
                    .done(function (data) {
                        response($.map(data.message, function (item) {
                            return {
                                label: item.name,
                                id: item.id
                            };
                        }));
                    })
                    .fail(function () {

                    });
            },
            minLength: 1,
            select: function(event, ui) {
                $('#mem_school_id').val(ui.item.id);
                $('#mem_school_name').val(ui.item.label);
            }
        });
    });

    function validateSchoolMunicipality() {
        if ($("#mem_municipality").val() !== $("#mem_municipality_name").val() || $("#mem_municipality").val() === "") {
            $("#mem_school_id").val("");
            $("#mem_school_name").val("");
            $("#mem_school").val("");
            $("#err_mem_school").removeClass("checked");
            $("#err_mem_school").html("Du måste välja en kommun först");
            $("#err_mem_school").css("display", "block");
            return false;
        }
    }
    function validateSchool() {
        setTimeout(validateSchoolNow, 300);
    }
    function validateSchoolNow() {
        if ($("#mem_school").val() !== $("#mem_school_name").val() || $("#mem_school").val() === "") {
            $("#mem_school_id").val("");
            $("#mem_school_name").val("");
            $("#mem_school").val("");
            $("#err_mem_school").removeClass("checked");
            $("#err_mem_school").html("Du måste välja en skola");
            $("#err_mem_school").css("display", "block");
            return false;
        }

        $("#err_mem_school").addClass("checked");
        $("#err_mem_school").html("ok");
        $("#err_mem_school").css("display", "block");
        return true;
    }
    function validateMunicipality() {
        setTimeout(validateMunicipalityNow, 300);
    }
    function validateMunicipalityNow() {
        if ($("#mem_municipality").val() !== $("#mem_municipality_name").val() || $("#mem_municipality").val() === "") {
            $("#mem_municipality_id").val("");
            $("#mem_municipality_name").val("");
            $("#mem_municipality").val("");
            $("#err_mem_municipality").removeClass("checked");
            $("#err_mem_municipality").html("Du måste välja en kommun");
            $("#err_mem_municipality").css("display", "block");
            return false;
        }

        $("#err_mem_municipality").addClass("checked");
        $("#err_mem_municipality").html("ok");
        $("#err_mem_municipality").css("display", "block");
        return true;
    }
    function radioCheck() {
        $("#err_mem_user_type").html("ok");
        $("#err_mem_user_type").css("display", "block");
        $("#err_mem_user_type").addClass("checked");
    }

    function setupLabel() {
        if ($('.label_radio input').length) {
            $('.label_radio').each(function() {
                $(this).removeClass('r_on');
            });

            $('.label_radio input:checked').each(function() {
                $(this).parent('label').addClass('r_on');
            });
        }
    }

    function checkWhitelist() {
        if ($("#mem_email").attr('readOnly')) {
            return;
        }

        var data_item = {
            'method' : '1',
            'ema' : $("#mem_email").val()
        };

        ajaxPostCall('/framework/logic/_calls.php', data_item, 'json')
            .done(function (response) {
                if (response.success) {
                    $("#mem_first_name").val(response.message.first_name);
                    $("#mem_last_name").val(response.message.last_name);

                    $("#mem_school").val(response.message.school_name);
                    $("#mem_school_id").val(response.message.school_id);
                    $("#mem_school_name").val(response.message.school_name);
                    $("#mem_municipality").val(response.message.municipality_name);
                    $("#mem_municipality_id").val(response.message.municipality_id);
                    $("#mem_municipality_name").val(response.message.municipality_name);

                    $("#mem_municipality").prop('readOnly', true).addClass('disabled');
                    $("#mem_school").prop('readOnly', true).addClass('disabled');
                    $("#mem_first_name").prop('readOnly', true).addClass('disabled');
                    $("#mem_last_name").prop('readOnly', true).addClass('disabled');
                    $("#mem_email").prop('readOnly', true).addClass('disabled');

                    if (response.message.type === "1") {
                        $("#radio-01").prop("checked", true);
                        $("#radio-01").val(response.message.type).after("<input type='hidden' name='mem_user_type' value='1' />");
                    } else {
                        $("#radio-02").prop("checked", true);
                        $("#radio-02").val(response.message.type).after("<input type='hidden' name='mem_user_type' value='2' />");
                    }

                    $("#mem_birth_year").val(response.message.birth_year);
                    $("#mem_class_name").val(response.message.class_name);

                    setupLabel();
                    $("#radio-02").prop('disabled', true);
                    $("#radio-01").prop('disabled', true);
                    $("#mem_email").after("<img title=\"Klicka för att låsa upp\" style=\"margin-top:10px;margin-left:5px;\" src=\"/images/lock.png \" onclick=\"location.reload()\" />");

                    $(".error").css('display', 'none');
                    $("#err_mem_school").css('display', 'none');
                    $("#err_mem_municipality").css('display', 'none');
                }
            })
            .fail(function () {
            });
    }

</script>

