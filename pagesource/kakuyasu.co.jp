<script type="text/javascript" src="/ec/resources/scripts/jquery.menu-aim.js"></script>
<script>
	var showMegaMenu = function() {
		var $menu = $(".dropdown-menu");
		$menu.menuAim({
		    activate: activateSubmenu,
		    deactivate: deactivateSubmenu
		});
		function activateSubmenu(row) {
		    var $row = $(row),
			submenuId = $row.data("submenuId"),
			$submenu = $("#" + submenuId),
			height = $menu.outerHeight(),
			width = $menu.outerWidth();

		    // Show the submenu
		    $submenu.css({
			display: "block",
			top: -1,
			left: width - 3,  // main should overlay submenu
			height: height - 4  // padding for main dropdown's arrow
		    });
		    $row.find("a").addClass("maintainHover");
		}
		function deactivateSubmenu(row) {
		    var $row = $(row),
			submenuId = $row.data("submenuId"),
			$submenu = $("#" + submenuId);

		    // Hide the submenu and remove the row's highlighted look
		    $submenu.css("display", "none");
		    $row.find("a").removeClass("maintainHover");
		}
		//メガメニュー
		$("#megamenu > li:first-child").hover(function(){
		    var menu = $('.dropdown-menu'); 
		    $('.dropdown-toggle').addClass('active');//ボタンのオンオフのマーク付与  
		    if (menu.is(':hidden')){
			menu.slideUp(0);        
			menu.slideDown(120);                         
		    }else{
			$('.dropdown-toggle').removeClass('active');
			menu.slideUp(100);
		    }
		});
	};

	var categoryXml = '/root/swf/xml/category1.xml';
	if($('.deli_menu.takuhai').length > 0) {
		categoryXml = '/root/swf/xml/category2.xml';
	}
	$.ajax({
		url: categoryXml,
		type:'get',
		dataType:'xml',
		timeout:10000,
		success:function(xml, status){
			if(status!='success')return;
			var categories = $(xml).find('category');
			var h= "";
			categories.each(function(i, e) {
				h += '<li data-submenu-id="menu-' + i + '"> <span>' + $(this).attr('name') + '</span>';
				h += '<div id="menu-' + i +  '" class="popover">\n'
				h += '<p class="tit">' +  $(this).attr('name') + '</p>\n';
				h += '<ul>';
				var subs = $(this).find('sub');
				subs.each(function(j, e) {
					var s = $(this);
					h += '<li><a href="' + s.attr('href') + '">' + s.text() + '</a></li>\n';
				});
				h += '</ul>\n';
				h += '</div>\n';	
			});
			$('.dropdown-menu').html(h);
			showMegaMenu();
		}
	});

</script>
