<script type="text/javascript">
	function chkValue_hour(num){
	    if(8 <= num && num < 9)         return '0'+ (num+1);
	    else if(9 <= num && num < 18)   return '' + (num+1);
	    else                            return '08';
	}
	function chkValue_min(num){
	    if(num<10)      return '00';
	    else if(num<20) return '10';
	    else if(num<30) return '20';
	    else if(num<40) return '30';
	    else if(num<50) return '40';
	    else            return '50';
	}
	
	window.onload = function () {
	    var now = new Date();
	    var hour = String(chkValue_hour((now.getHours())));
	    var min  = String(chkValue_min((now.getMinutes())));
	    var count_hour = infoForm1.time1.length;
		for(var i=0; i< count_hour; i++){
			if(infoForm1.time1.options[i].value==hour){
				infoForm1.time1.options[i].selected = true;
			}
		}
	    var count_min = infoForm1.time2.length;
		for(var i=0; i< count_min; i++){
			if(infoForm1.time2.options[i].value==min){
				infoForm1.time2.options[i].selected = true;
			}
		}
	}
</script>


