<script>
$(function(){
    var btn=$('#bannertab a'),
        img=$('#banner > div.bannerlogo a'),
        len=img.length,
        text=$('#banner >div.text div'),
        prev=$('#banner a.prev'),
        next=$('#banner a.next'),
        stop=false,i=0;
    btn.eq(0).addClass('hover');
    img.eq(0).fadeIn();
    text.eq(0).fadeIn();
    var mouseover=function(cbtn){
        btn.removeClass('hover');
        $(cbtn).addClass('hover')
        i=$(cbtn).parent().index();
        img.fadeOut().eq(i).fadeIn()
        text.fadeOut().eq(i).fadeIn()
    }
    var bindStopTarget=function(){
        for(var i=0;i<arguments.length;i++){
            arguments[i].mouseover(function(){stop=true}).mouseout(function(){stop=false;})
        }
    }
    bindStopTarget(img,text,prev,next,btn)
    btn.mouseover(function(e){
        mouseover(this)
    })
    setInterval(function(){
        if(stop) return;
        i=(i+1)%len;
        mouseover(btn.eq(i))
    },4000)
    
    next.click(function(){
        i=(i+1)%len;
        mouseover(btn.eq(i))
    })
    prev.click(function(){
        i=i!=0?((i-1)% len):len-1;
        mouseover(btn.eq(i))
    })
})
</script>
