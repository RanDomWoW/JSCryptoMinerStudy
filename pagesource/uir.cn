    <script type="text/javascript">
      $(function(){
        var oEmbed =$('#ImagePlayer176');
        var imgArr = oEmbed.attr('flashvars').match(/<image>([\s\S]*?)<\/image>/g);
        var imgLink = oEmbed.attr('flashvars').match(/<link>([\s\S]*?)<\/link>/g);
        var imgplayjson = [];
        var imglinkeJson = [];
        $('.banner').hide();
        for (var i = 0; i < imgArr.length; i++) {
          imgplayjson.push(decodeURIComponent(imgArr[i].match(/<image>([\s\S]*?)<\/image>/)[1]))
        };
        for (var i = 0; i < imgLink.length; i++) {
          imglinkeJson.push(decodeURIComponent(imgLink[i].match(/<link>([\s\S]*?)<\/link>/)[1]))
        };
        
        var lunbohtml ='<ul id="img-list"></ul><span class="img-alt"></span></div><ol id="img-btn"></ol>';
        $('.overall .banner2')[0].innerHTML = lunbohtml; 
        if(!imgplayjson || imgplayjson.length == 0 ){
          return;			
        }else {
          //showADContent(imgplayjson);  //将代码插入到页面
          carousePlay(imgplayjson,imglinkeJson);
        }
        
        //渐变轮播图效果
        function carousePlay(imgplayjson,imglinkeJson){
          var str = '',
              str2 = '',
              imgs = imgplayjson,
              links = imglinkeJson,
              oUl = document.getElementById('img-list'),
              oOl = document.getElementById('img-btn'),
              //oShade = document.getElementById('carousel-shade'),
              olLi = oOl.children,
              aLi = oUl.children,
              imgNum = 0,
              Autotimer = null;
          for (var i = 0; i < imgs.length; i++) {
            str += '<li>'+'<a href="'+ links[i] + '" target="_blank">'+'<img src="'+imgs[i]+'"  width="1400" height="450"></a></li>'
            str2 += '<li>'+(i+1)+'</li>';
          };
          oUl.innerHTML = str;
          oOl.innerHTML = str2;
          //oShade.innerHTML = '<span class="img-alt">'+imgs[0].Alt+'</span>';	
          //zIndex的添加
          for(var i=aLi.length-1;i>=0;i--){
            aLi[i].style.zIndex = aLi.length-i;
          }
          for(var i=1;i<aLi.length;i++){
            aLi[i].style.opacity = 0.3;
          }	
          
          var onOff = true;
          var zIndex = 20;		
          var oLi = aLi[0];
          olLi[0].className = 'show';
          for(var i=0;i<olLi.length;i++){
            olLi[i].index = i;
            olLi[i].onclick = function(){
              clearInterval(Autotimer);
              var _index = this.index;
              autoPlay(_index);
            }
          }
          //自动轮播
          Autotimer = setInterval(function(){
            imgNum++;
            if (imgNum==imgs.length)imgNum=0;
            autoPlay(imgNum);
          },5000)
          
          //移入移出轮播图区域
          $('.banner').hover(function(){
            clearInterval(Autotimer);
          },function(){
            Autotimer = setInterval(function(){
              imgNum++;
              if (imgNum==imgs.length)imgNum=0;
              autoPlay(imgNum);
            },5000)
          })
          //轮播函数
          function autoPlay(n){
            if(!onOff)return;
            var _this = n;
            onOff = false;				
            aLi[n].style.zIndex = zIndex++;	
            for(var i=0;i<olLi.length;i++){
              olLi[i].className = '';
            }
            olLi[_this].className = 'show';		
            move(oLi,{'opacity':.3},200,'linear');			
            move(aLi[n],{'opacity':1},500,'linear',function(){
              onOff = true;
            });
            // move(oShade,{'height':0},300,'linear',function(){
            // oShade.innerHTML = '<span class="img-alt">'+imgs[_this].Alt+'</span>'
            //move(oShade,{'height':30},500,'linear');
            // });
            oLi = aLi[n];
          }
        }	
        
        var Tween = {
          linear: function (t, b, c, d){  //匀速
            return c*t/d + b;
          }
        }
        function move(obj,json,d,fx,callback){
          var timer = null;
          var j = {};
          for(var attr in json){
            j[attr] = {};
            if(attr == 'opacity'){
              j[attr].begin = parseFloat(getStyle(obj,attr));//起始的位置。
            }else{
              j[attr].begin = parseInt(getStyle(obj,attr));//起始的位置。
            }
            j[attr].conut = json[attr] - j[attr].begin;//总的目标点。
          }
          var duration = d;//总时间
          var oldTime = new Date().getTime();
          timer = setInterval(function(){
            var newDate = new Date().getTime();
            var t = newDate - oldTime;
            if(t > duration){
              t = duration;
            }
            for(var attr in json){
              var v = Tween[fx](t, j[attr].begin,j[attr].conut, duration);
              if(attr == 'opacity'){
                obj.style[attr] = v;
                obj.style.filter = 'alpha(opacity='+(v*100)+')';
              }else{
                obj.style[attr] = v + 'px';
              }
            }
            if(t == duration){
              clearInterval(timer);
              callback && callback.call(obj);
            }
          },16);
        }
        function getStyle(obj,attr){
          return obj.currentStyle?obj.currentStyle[attr]:getComputedStyle(obj)[attr];
        }	
      });
    </script>
