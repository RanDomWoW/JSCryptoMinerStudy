<script type="text/javascript">
$(document).ready(function(){
	
	
		// Yazi Yazdırma (haberler, li)
	function textWriting(sId, bazDeger) {
		var topVeri = $("#"+sId+" ul "+bazDeger).length; 
		var vUzunluk = $("#"+sId+" ul "+bazDeger).width(); 
		var vYukseklik = $("#"+sId+" ul "+bazDeger).height(); 
		var topYukseklik = topVeri * vYukseklik;
		var saymasi = 0;
		var hzYaz = 3000;var hzBekle = 5000;var hzSil = 1000;

		function yazReiz(){
			$("#"+sId+" .maske").stop().animate({width: vUzunluk},hzYaz).animate({width: vUzunluk},hzBekle).animate({width: "0px"},hzSil);
			if(topYukseklik > ((saymasi*vYukseklik)+vYukseklik)) {
				zmn2 =setTimeout(kaydirReiz, (hzYaz+hzBekle+hzSil+100));
			} else {
				saymasi=-1; 
				zmn2 =setTimeout(kaydirReiz, (hzYaz+hzBekle+hzSil+100));
			}
			//console.log(topVeri+" "+vUzunluk+" "+vYukseklik+" "+topYukseklik+" "+saymasi+" "+(saymasi*vYukseklik)+" "+saymasi+" "+saymasi);
		}
		function kaydirReiz(){
			saymasi++;
			$("#"+sId+" ul").stop().css("marginTop", ((saymasi*vYukseklik))* (-1));
			zmn =setTimeout(yazReiz, (0));
		}
		yazReiz();
	} 
	
	textWriting('haberler', 'li');
	
	
})
</script>







        
