<script type="text/javascript">
  // 메인롤링
$(function(){
	var speed = 5000;
	var popAction;
	var num = $('#visual-box ul li p').prev().find('img').attr('src');
	
	//롤링이미지
	function fnMainVisualRoll(){
		var obj = $('#visual-box ul li').filter('.on');
		var targetObj;

		if(obj.next().get(0) == null){
			targetObj = $('#visual-box ul li:first').get(0);
		}else{
			targetObj = obj.next().get(0);
		}

		$(targetObj).find('p').animate({'opacity':'1'}, function(){
			obj.find('p').animate({'opacity':'0'});

			obj.removeClass('on');
			obj.find('span > a > img').attr('src',num.replace('on','off'));
			$(targetObj).addClass('on');
			$(targetObj).find('span > a > img').attr('src',num.replace('off','on'));
		});

	}

	//버튼
   $('#visual-box ul li a').bind('mouseover focus',function(){
		clearInterval(popAction);

		var obj = this;
		$(obj).parent().parent().find('p').animate({'opacity':'1'}, function(){
			$('#visual-box ul li p').each(function(){
				if(this != $(obj).parent().parent().find('p').get(0) && $(this).css("opacity") == "1"){
					$(this).animate({'opacity':'0'});
				}
			});

			$('#visual-box ul li a').parent().parent().removeClass('on');
			$('#visual-box ul li a img').attr('src',num.replace('on','off'));
			$(obj).parent().parent().addClass('on');
			$(obj).parent().parent().find('a').find('img').attr('src',num.replace('off','on'));
		});
	});
	

	 $('#visual-box ul li a').bind('mouseout blur',function(){
		fnStartMainVisual();
	});

	function fnStartMainVisual(){
		if($('#visual-box ul li').length > 1){
			popAction = setInterval(fnMainVisualRoll, speed);
		}
	}

	fnStartMainVisual();
});
</script>
