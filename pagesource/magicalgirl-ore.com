      width +
      '" height="' +
      height +
      '" src="https://www.youtube.com/embed/' +
      youtubeId +
      '" frameborder="0" allowfullscreen></iframe>';

    var addIframeDom = $(addIframe);

    if (specifiedSize) {
      addIframeDom.addClass("is-specifiedSize");
    }
    
    _this.after(
      addIframeDom
    );

    _this.remove();

  });

});


