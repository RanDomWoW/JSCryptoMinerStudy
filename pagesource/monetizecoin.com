<script>

jQuery(window).on("load", function () {
    "use strict";



    var timeout = null;
    $('#calc_amount').on('input', function() {
        clearTimeout(timeout);
        timeout = setTimeout(calculateResults.bind(this), 300);
    });
    Number.prototype.formatMoney = function(c, d, t){
    	var n = this, 
    	    c = isNaN(c = Math.abs(c)) ? 2 : c, 
    	    d = d == undefined ? "." : d, 
    	    t = t == undefined ? "," : t, 
    	    s = n < 0 ? "-" : "", 
    	    i = String(parseInt(n = Math.abs(Number(n) || 0).toFixed(c))), 
    	    j = (j = i.length) > 3 ? j % 3 : 0;
    	   return s + (j ? i.substr(0, j) + t : "") + i.substr(j).replace(/(\d{3})(?=\d)/g, "$1" + t) + (c ? d + Math.abs(n - i).toFixed(c).slice(2) : "");
    	 };


    function calculateResults() {
    	var $elem = $(this);
		var invest = parseInt($elem.val()); var bonus; var runtime; var profit; var total; var roi; var totalreturn; var perday; var totalbonus; var perdayrev;
    	if(invest <= 1000) 				{ bonus = 0; runtime = 299; }
    	if(invest > 1000 && invest < 5000) 	{ bonus = 0.1; runtime = 239; }
    	if(invest > 5000 && invest <= 10000) 	{ bonus = 0.2; runtime = 179; }
    	if(invest > 10000) 				{ bonus = 0.25; runtime = 120; }
    	
    	//invest / 100 * return * days
    	
    	roi = parseFloat($('#calc_roi').val());
    	perday = (invest/100);
    	totalbonus = bonus + roi;
    	perday = perday * totalbonus;
    	
    	profit = perday * runtime;
    	totalreturn = (profit + invest);
    	perdayrev = (totalreturn / runtime).formatMoney(0, '.', ',');
    	totalreturn = totalreturn.formatMoney(0, '.', ',');
    	profit = (profit).formatMoney(0, '.', ',');
    	
    	

    	if(invest> 1000) { $('#calc_roi').val(roi + '% + ' + bonus + '% (Bonus)'); } else { $('#calc_roi').val(roi + '%'); }
        $('#calc_length').val(runtime + ' Days');
        $('#calc_result_profit').val(profit);
        $('#perdayrev').val(perdayrev);
        $('#calc_result_roi').val(totalreturn);

    }

    calculateResults.bind($('#calc_amount'));


}); 
</script>
