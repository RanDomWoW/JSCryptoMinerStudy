        $(function() {
                var cggpromo = $('#cgg-promo');
                var products = cggpromo.find('.cgg-promo_products');

                products.hover(
                        function(e) {
                                $(this).addClass('hovering');
                        }, function() {
                                $(this).removeClass('hovering');
                        }
                )

                function switchgiftpromo(n) {

                        products.each(function() {

                                if ($(this).hasClass('hovering')) {
                                        return true;
                                }

                                var current = $(this).find('.current');
                                if (!current.length) {
                                        $(this).find('.cgg-promo_product').first().addClass('current');
                                } else {
                                        var next = current.next();
                                        if (!next.length) {
                                                next = $(this).find('.cgg-promo_product').first();
                                        }
                                        current.removeClass('current');
                                        next.addClass('current');
