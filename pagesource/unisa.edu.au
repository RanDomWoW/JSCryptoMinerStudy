    //object to control all slideshow components
    function Slideshow(slideshow,title,controls,caption) {
        this.$show = slideshow;                 //jquery obj. container for slideshow slides
        this.$title = title                     //jquery obj. container for slideshow title
        this.$ctrls = controls;                 //jquery obj. container for slideshow controls
        this.$caption = caption;                //jquery obj. container for slide captions
        this.imgFolder = '/PageFiles/151781/'; //folder location of images
        this.imgStore = [];                     //store images in array for easy reference
        this.resizeShow = resizeShow;           //resize show when window resizes
        this.insert = insert;                   //insert slide into slideshow
        this.play = play;                       //auto advance slideshow based on time
        this.stop = stop;                       //stop auto advancing of slideshow
        this.kill = kill;                       //entirely remove slideshow from DOM
        this.getImg = getImg;                   //load iamge with ajax, file it in imgStore
        this.initialise = initialise;            //setup slideshow
        this.smallScreenInit = smallScreenInit;  //setup slideshow for small screen (static image only)
        this.btnCtrls = btnCtrls;         //enable selected buttons to influence slideshow
        this.buildCtrls = buildCtrls;           //build slideshow controls to match slides in show
        this.updateTitle = updateTitle          //update title to match slide shown
        this.updateCtrls = updateCtrls;         //update controls to indicate which slide is visible.
        this.updateCaption = updateCaption;     //update caption to match visible slide

        var self = this;                        //handy for passing context into functions
        var slideInterval;                      //time tracking of slideshow auto-advancing
        var counter = slides.length -1;         //track which slide to show next auto-advancing.
        var isTouch = (('ontouchstart' in window) || (navigator.msMaxTouchPoints > 0)); //detect if device has touchscreen interface
        
        function updateCounter(num){
            if (num) {
                counter = num;
            }else{
                if (counter > 0) {
                        --counter;
                }else{
                    counter = slides.length -1;
                }                   
            }
        }
        function insert(data,duration) {
            var self = this;
            var imgURL = this.imgFolder + data.src;
            var $slide = $("<div class='slide'></div>").css({
            "background-image":"url('"+ imgURL +"')",
            "height":this.$show.height(),
            "opacity":0,
            });
            var $oldSlide = $(this.$show.find('.slide'));
            this.$show.append($slide);
            $slide.animate({"opacity":1},duration,function(){
                $oldSlide.remove();
            });
            this.updateTitle(data.title,duration)
            this.updateCaption(data.caption,duration);
            this.updateCtrls();
        }
        function play() {
            //clear any existing process
            clearInterval(slideInterval);
            //makes default slides auto advance
            slideInterval = window.setInterval(function(){
                updateCounter();
                self.insert(slides[counter],1500);
            }, 7000);
        }
        function stop() {
            //stops default slideshow auto advancing
            clearInterval(slideInterval);
        }
        function kill() {
            //kill slideshow: for instances where slideshow needs to be removed entirely!
            this.stop();
            this.btnCtrls(false);
            this.$show.find(".slide").remove();
        }
        function resizeShow() {
            var height = $(window).height();
            this.$show.height(height)
            this.$show.find('.slide').height(height);
        }
        function buildCtrls() {
            var self = this,
                $slideLst = $("<ul  class='nav-slides'/>"),
                $ctrlsLst = $("<ul  class='nav-ctrls'/>")
                    .append("<li><a class='icon-13-play on' href='#'><span class='visuallyhidden'>play</span></a></li>")
                    .append("<li><a class='icon-13-pause' href='#'><span class='visuallyhidden'>pause</span></a></li>");
                
                //setup ctrlsLst button listeners
                $ctrlsLst.on('click',function(e){
                    e.preventDefault();
                    if (e.target.textContent == "play"){
                        self.play();
                    }else if(e.target.textContent == "pause"){
                        self.stop();
                    }
                    $(this).find("a").removeClass("on");
                    $(e.target).addClass("on");
                    
                });
                
            //add button into controls for each slide in show
            for (i = 0 ; i < slides.length ; i++) {
                var btn = $("<li><a class='icon-13-slide' href='#'><span class='visuallyhidden'>"+i+"</span></a></li>")
                    .prependTo($slideLst);
            }
            //add click listener
            $slideLst.on('click','a',function(e){
                var num = $(e.target).find(':first-child').html();
                e.preventDefault();
                self.stop();
                updateCounter(num);
                self.insert(slides[counter],350);
                self.play();
            });
            this.$ctrls.append($slideLst);
            this.$ctrls.append($ctrlsLst);
        }
        function updateCtrls() {
            var $links = this.$ctrls.find('.icon-13-slide');
            $links.each(function(){
                if ($(this).find(':first-child').html() == counter) {
                    $(this).addClass("on");
                }else{
                    $(this).removeClass("on");
                }            
            });
        }
        function updateTitle(text,duration){
            var self = this;
            duration = duration*0.3;
            this.$title.animate({"opacity":0},duration,function(){
                self.$title.html(text);
                //self.positionTitle(); //title positioned at 50% from bottom - dynamic positioning not required!
                self.$title.animate({"opacity":1},duration);
            });
        }

        function updateCaption(text,duration) {
            var self = this;
            this.$caption.animate({"opacity":0},duration*0.5,function(){
                self.$caption.html(text);
                self.$caption.animate({"opacity":1},duration*0.5);
            });   
        }
        function getImg(url) {
            var self = this;
            var $img = $('<img/>').load(function(data){
                self.imgStore.push(this);
                //play slideshow if all assets are loaded
                if (self.imgStore.length == assetList.length) {
                    self.btnCtrls(true);
                    self.play();
                }
            }).attr('src',url);
        }
        function btnCtrls(status) {
            //button control of slideshow. Status either true (control enabled) or false (control disabled)
            //only implement if deivce is not touch interface
            //if (isTouch == false) {
            var iOS = /(iPod|iPhone|iPad)/g.test(navigator.userAgent);
            if (!iOS) {
                self = this;
                var $links = $('#section-btns').find('a');
                if (status) {
                    //enable slideshow input when user interacts with links on page
                    $links.hover(
                        function(e){
                            //mouse enter stuff
                            self.stop();
                            var num = ($links.index(e.target));
                            self.insert(sectionSlides[num],350);
                        },
                        function(){
                            //mouse exist stuff
                            self.play();
                        }
                    );
                }else{
                    $links.off();
                }           
            }
        }
        function initialise() {
            var self = this;
            
            //**TO-DO: this list needs to be generated dynamically... I think
            var slideZero = {
                    "title":"Welcome to Australia's university of enterprise",
                    "caption":"Jeffrey Smart Building, City West campus.",
                    "src":"s0-welcome.jpg",
                };
            
            //build show controls
            this.buildCtrls();    
            // Insert initial slide into slideshow
            this.insert(slideZero,0);
            //load all images and start playing slideshow
            $(assetList).each(function(){
                var url = self.imgFolder + this;
                self.getImg(url);
            });
            
            //resize slideshow to fit window
            $(window).resize(function(){
                self.resizeShow();
                if ($(window).width() < 900){
                    self.stop();
                    self.kill();
                    }
            });
            $(window).trigger('resize');
        }
        function smallScreenInit(){
            var self = this;
            var height = Math.max($(window).height(),this.$show.height());
            //**TO-DO: this list needs to be generated dynamically... I think?
            var slideZero = {
                    "title":"Welcome to Australia's university of enterprise",
                    "caption":"Jeffrey Smart Building, City West campus",
                    "src":"s0-welcome.jpg",
                };
            this.$show.height(height);
        }
    }

