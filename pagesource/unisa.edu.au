$(document).ready(function() {
    var slideshow = new Slideshow($('#slideshow'),$('#slideshow-title'),$('#slideshow-controls'),$('#slideshow-caption'));
   
    if($(window).width() < 900){
        slideshow.smallScreenInit();
    }else{
        slideshow.initialise(); 
    }
    
    $(window).on('scroll',function(){
        if ($('footer').offset().top - $(window).scrollTop() < $(window).height()){
            $('#footer-below').addClass('on');    
        }else{
            $('#footer-below').removeClass('on');
        }
    });
        
    $('#footer-below').on('click',function(e){
        e.preventDefault();
        if ($(this).hasClass('on')) {
            //scroll up to hide footer
            var offset = $('footer').offset().top - $(window).height();
            $('html,body').animate({
                scrollTop: offset    
            },1000);
        }else{
            //scroll down to show footer
                var offset = Math.min($('footer').offset().top,$('footer').height());
                $('html,body').animate({
                    scrollTop: offset    
                },1000);
        }
    })

});
</script>

