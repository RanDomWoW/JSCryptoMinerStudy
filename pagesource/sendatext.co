		lineWidth: 10, // the width of the circle line in pixels
		start :'top',
		fontSize:"36px",
		fontFamily:"gothic"
	};
			var captchaVerify = false;
		
	
	
	function capcha_filled(){
		captchaVerify = true;
	}
	function capcha_expired(){
		captchaVerify = false;
	}

	function onloadCallback() {
        grecaptcha.render('captcha', { 
			'sitekey' 	: '6LdueigTAAAAAEabxf5RH2tEd9VAryFgi8pcki98',
			'callback' 	: capcha_filled,
			'expired-callback' : capcha_expired,
		});
    };
		
		$(".country-dropdown").change();
		//sms
		$("#textbox textarea").bind("keyup keypress change",function(e){
			if($(this).val().length > 120 && e.which !== 0 && e.which != 8 )
			{
				$(this).val($(this).val().substr(0,120))
				return false;
			}
			$("#char-left span").text(120 - parseInt($(this).val().length));
		});
		$("#btnsms").click(function(){


			$(".alert").removeClass('error').text("");
			
			
			code		= $("#dialCode").val();
			$phoneNumber=$("#phone_number").val();
			sms_text	= $("#textbox textarea").val();
			
			if(! code )
			{
				$(".alert").addClass('error').text("Please select a country");
				return false;
			}

			if($phoneNumber.length < 6)
			{
				$(".alert").addClass('error').text("Please enter valid phone number");
				$("#phone_number").focus();
				return false;
			}

			if( ! sms_text)
			{
				$(".alert").addClass('error').text("Please enter text");
				$("#textbox textarea").css('border','1px solid red').focus();
				return false;
			}
			
			if(captchaVerify == false)
			{
				$(".alert").addClass('error').text("Please verify captcha");
				return false;
			}
			
			country 	= $(".country-dropdown option:selected").attr('data-country');
			shortName	= $(".country-dropdown option:selected").attr('data-shortname');
			var cd 		= new Date(); 
			userLocalTime= cd.getFullYear() + "-" + parseInt(cd.getMonth()+1) + "-" + cd.getDate() + " " + cd.getHours() + ":" + cd.getMinutes() + ":" + cd.getSeconds();
			
			loader = $('.percent-loading').ClassyLoader(option);
			$("#sending-sms").addClass('move-top');
			$.ajax({
				url	: 'sms/send',
				method:"POST",
				data: {number:$phoneNumber,sms_text:sms_text,code:code,country :country,shortName:shortName,ult:userLocalTime },
				success:function(data)
				{
					
					$("#sending-sms").attr('class','move-top-top');
					$(".notification-img").addClass(data['type']+"-img");
					$(".notification-msg").html(data['message']);
					
				}
				
			});
			return false;
		});



	
</script>    </div>
