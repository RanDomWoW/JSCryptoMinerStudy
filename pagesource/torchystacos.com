<script type="text/javascript">
	jQuery(document).ready(function() {

	container = $('.container');
	cover = $('.cover');
	play = $('#play');
	pause = $('#pause');
	mute = $('#mute');
	muted = $('#muted');
	close = $('#close');
	song = new Audio('https://torchystacos.com/wp-content/uploads/2013/10/Sacred-Menu-1.ogg','https://torchystacos.com/wp-content/uploads/2013/10/Sacred-Menu-1.mp3');
	duration = song.duration;

	if (song.canPlayType('audio/mpeg;')) {
    	song.type= 'audio/mpeg';
    	song.src= 'https://torchystacos.com/wp-content/uploads/2013/10/Sacred-Menu-1.mp3';
	} else {
    	song.type= 'audio/ogg';
    	song.src= 'https://torchystacos.com/wp-content/uploads/2013/10/Sacred-Menu-1.ogg';
	}



	play.live('click', function(e) {
		e.preventDefault();
		song.play();

		$(this).replaceWith('<a class="button" id="pause" href="" title="Pause"></a>');
		container.addClass('containerLarge');
		cover.addClass('coverLarge');
		$('#close').fadeIn(300);
		$('#seek').attr('max',song.duration);
	});

	pause.live('click', function(e) {
		e.preventDefault();
		song.pause();
		$(this).replaceWith('<a class="button" id="play" href="" title="Play"></a>');

	});
	
	$('.close-secret').live('click', function(e) {
		e.preventDefault();
		song.pause();
		$('#pause').replaceWith('<a class="button" id="play" href="" title="Play"></a>');

	});

	mute.live('click', function(e) {
		e.preventDefault();
		song.volume = 0;
		$(this).replaceWith('<a class="button" id="muted" href="" title="Sound On"></a>');

	});

	muted.live('click', function(e) {
		e.preventDefault();
		song.volume = 1;
		$(this).replaceWith('<a class="button" id="mute" href="" title="Sound Off"></a>');

	});

	$('#close').click(function(e) {
		e.preventDefault();
		container.removeClass('containerLarge');
		cover.removeClass('coverLarge');
		song.pause();
		song.currentTime = 0;
		$('#pause').replaceWith('<a class="button gradient" id="play" href="" title=""></a>');
		$('#close').fadeOut(300);
	});



	$("#seek").bind("change", function() {
		song.currentTime = $(this).val();
		$("#seek").attr("max", song.duration);
	});

	song.addEventListener('timeupdate',function (){
		curtime = parseInt(song.currentTime, 10);
	$("#seek").attr("value", curtime);
	});









	
	
});

</script>

