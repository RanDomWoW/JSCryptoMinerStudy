function search(opacUrl,keyword,searchWay,booktype,marcFormat,sortWay,sortOrder,
			startPubdate,endPubdate,rows,hasHolding,curlibcodes) {
	//这里做一些默认的处理，其他情况可以提供默认值，但查询字必须，如果没有传入关键字，会进行提示
	if (keyword == ""){
		alert("请输入查询关键字");
		return ;
	}
	if (!searchWay) {
		searchWay = "";
	}
	if (!booktype) {
		booktype = "";
	}
	if (!marcFormat) {
		marcFormat = "";
	}
	if (!startPubdate) {
		startPubdate = "";
	}
	if (!endPubdate) {
		endPubdate = "";
	}
	if (!rows) {
		rows = "10";
	}
	if (!sortWay) {
		sortWay = "score";
	}
	if (!sortOrder) {
		sortOrder = "desc";
	}
	//if (hasHolding == "true" || !hasHolding){
	//	hasHolding = 1;
//	}
	
	var limitCurlibcodes = "";
	if (!curlibcodes){
		for (curlibcode in curlibcodes) {
			limitCurlibcodes = limitCurlibcodes +"curlibcode="+curlibcode+"&";
		}
	}
	
	var toLocation = opacUrl+"search?searchWay="+searchWay+"&q="+keyword+
			"&booktype="+booktype+"&marcformat="+marcFormat+"&sortWay="+sortWay+"&sortOrder="+sortOrder+
			"&rows="+rows+"&startPubdate="+startPubdate+"&endPubdate="+endPubdate+"&hasholding="+hasHolding;
	if (limitCurlibcodes != "") {
		toLocation += limitCurlibcodes;
	}
	window.open(toLocation);
}


