		obj.options[0]=new Option('任意词','');
		obj.options[1]=new Option('题名','title');	
		obj.options[2]=new Option('ISBN/ISSN','isbn');
		obj.options[3]=new Option('著者','author');
		obj.options[4]=new Option('主题词','subject');
		obj.options[5]=new Option('分类号','class');
		obj.options[5]=new Option('订购号','orderno');
		obj.options[5]=new Option('出版社','publisher');
	}else if(obj2[1].checked){
		obj.options[0]=new Option('标题','标题');
