    <div class="modal fade" id="instant-search">
      <div class="modal-dialog modal-jlpt">
        <div class="modal-content">
          <div class="modal-body">
              <button type="button" class="close close-modal-jlpt" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
              <div class="tab-container">
                <div label="Từ vựng" ng-if="tabSelected == 0">
                    <div class="widget-container word-base-form" ng-if="originQuery">
                        Từ <span class="origin-query">{{ originQuery }}</span> có thể có dạng nguyên thể là <span class="base-form-word">{{ baseFormWord }}</span>
                    </div>
                    <div class="words-list" ng-if="words != null">
                        <div ng-repeat="word in words">
                            <ng-word data="word" aux="wordAux" ng-if="$index == 0"></ng-word>
                            <ng-word data="word" ng-if="$index != 0"></ng-word>
                        </div>
                    </div>
                    <div class="google-translate" ng-if="words == null || words.length == 0">
                        <ng-google-translate data="googleTranslate" aux="wordAux" ng-if="googleTranslate"></ng-google-translate>
                    </div>
                    <ng-synonyms data="googleTranslate" ng-if="googleTranslate"></ng-synonyms>
  
                   
  
                    <div class="suggest-list widget-container" ng-if="suggest != null && suggest.length > 0">
                        <div class="suggest-title">Các từ liên quan:</div>
                        <span class="suggest-word" ng-repeat="word in suggest" ng-click="startQuery(word.word)">{{ word.word }}<span ng-if="$index < (suggest.length - 1)">, </span> </span>
                    </div>      
  
                    <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
                      <p>Các chữ Hán :</p>
                      <div class="panel panel-default" ng-repeat="kanji in resultKanjis">
                          <div class="panel-heading" id="heading{{kanji._id}}" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse{{kanji._id}}" aria-expanded="true" 
                              aria-controls="collapse{{kanji._id}}">
                            <h4 class="panel-title">
                              <span class="kanji"> {{ kanji.kanji }} </span>
                              「{{ kanji.on }}」 
                              <b class="mean">{{kanji.mean}}</b>
                            </h4>
                          </div>
                          <div id="collapse{{kanji._id}}" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading{{kanji._id}}">
                            <div class="panel-body">
                              <ng-kanji-result-search-word data="kanji"></ng-kanji-result-search-word>
                            </div>
                          </div>
                      </div>
                    </div>
  
  
                    <div class="no-result" ng-if="noResults">
                        Không tìm thấy từ vựng nào có liên quan tới: {{ query }}
                    </div>
                </div>
                <div label="Hán tự" ng-if="tabSelected == 1">
                    <div class="list-kanji" ng-if="kanjis != null">
                        <div class="btn-group" ng-if="kanjis.length > 1">
                            <button type="button" ng-repeat="kanji in kanjis" class="btn btn-default btn-lg draw-single-kanji" ng-click="changeKanjiShow($index);" ng-class="kanjiSeletectClass($index);">{{ kanji.kanji }}</button>
                        </div>
                        <div id="kanji-detail-result">
                            <ng-kanji data="getCurrentKanji()"></ng-kanji>
                        </div>
                    </div>
                    <div class="no-result" ng-if="noResults">
                        Không tìm thấy chữ: {{ query }}
                    </div>                  
                </div>
                <div label="Mẫu câu" ng-if="tabSelected == 2">
                    <div class="examples-list widget-container" ng-if="examples != null">
                        <ng-example ng-repeat="exam in examples" index="{{$index + 1}}" data="exam"></ng-example>
                    </div>
                    <div class="no-result" ng-if="noResults">
                        Không tìm thấy ví dụ nào có liên quan tới: {{ query }}
                    </div>
                </div>
                <div label="Ngữ pháp" ng-if="tabSelected == 3">
                    <div class="instant-grammar-container">
                        <ng-grammar data="grammarDetail" ng-if="grammarDetail != null"></ng-grammar>
                    </div>
                    <div class="no-result" ng-if="noResults">
                        Không tìm thấy ngữ pháp: {{ query }}
                    </div>
                </div>
            </div>
          </div>
          <div class="modal-footer">
            <!-- <button type="button" class="btn btn-default" data-dismiss="modal">Close</button> -->
          </div>
        </div><!-- /.modal-content -->
      </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->
  
    <div class="modal fade" id="add-word">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
              <button type="button" class="close close-modal-add-word" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <h4 class="modal-title">Đóng góp từ mới</h4>
          </div>
          <div class="modal-body">
              <form>
                <div class="form-group">
                  <label for="newword-word" class="control-label">Từ:</label>
                  <input type="text" class="form-control" id="newword-word" ng-model="newword.word">
                  <label for="newword-mean" class="control-label">Nghĩa:</label>
                  <input type="text" class="form-control" id="newword-mean" ng-model="newword.mean">
                  <label for="newword-phonetic" class="control-label">Phiên âm hiragana:</label>
                  <input type="text" class="form-control" id="newword-phonetic" ng-model="newword.phonetic">
                  <label for="newword-example" class="control-label">Ví dụ:</label>
                  <input type="text" class="form-control" id="newword-example" ng-model="newword.example">
                  <label for="newword-example-mean" class="control-label">Nghĩa ví dụ:</label>
                  <input type="text" class="form-control" id="newword-example-mean" ng-model="newword.example_mean">
                </div>
              </form>
          </div>
          <div class="modal-footer">
              <button type="button" class="btn btn-default" data-dismiss="modal">Đóng</button>
              <button type="button" class="btn btn-primary" ng-click="addWord.send(newword);">Gửi</button>
          </div>
        </div>
      </div>
    </div>
  
    <div class="modal fade" id="reportModal" tabindex="-1" role="dialog" aria-labelledby="reportModalLabel">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close close-modal-report" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title" id="exampleModalLabel">Báo dữ liệu sai</h4>
          </div>
          <div class="modal-body">
            <form>
              <div class="form-group">
                <label for="recipient-name" class="control-label">Dữ liệu:</label>
                <input type="text" class="form-control" id="recipient-name" ng-model="report.entry">
              </div>
              <div class="form-group">
                <label for="message-text" class="control-label">Giải thích điểm sai:</label>
                <textarea class="form-control" id="message-text" ng-model="report.comment"></textarea>
                <label for="message-text" class="control-label warning" ng-if="report.comment == '' && report.noComment">Bạn hãy chỉ ra lỗi sai giúp chúng tôi!</label>
              </div>
            </form>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Đóng</button>
            <button type="button" class="btn btn-primary" ng-click="report.send(report.comment);">Gửi</button>
          </div>
        </div>
      </div>
    </div>
    <div class="modal fade" id="alertModal" tabindex="-1" role="dialog" aria-labelledby="alertModalLabel">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-body">
              {{ alert.message }}
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-default close-modal-alert" data-dismiss="modal">Đóng</button>
          </div>
        </div>
      </div>
    </div>
  
    <div class="notify-current alert alert-notify">
      {{ alert.notify }}
    </div>
  
    <div class="modal fade" id="confirmDeleteHistoryModal">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close close-delete-history-modal" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h4 class="modal-title">Xóa lịch sử</h4>
          </div>
          <hr>
          <div class="modal-body">
            Bạn có muốn xóa toàn bộ lịch sử không ?
          </div>
          <hr>
          <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Hủy</button>
            <button type="button" class="btn btn-danger deleteHistory">Xóa</button>
          </div>
        </div>
      </div>
    </div>
  
    <div class="modal fade" id="resetPassword">
      <div class="modal-dialog">
        <form name="resetPassword">
          <div class="modal-content">
            <div class="modal-header">
              <button type="button" class="close close-reset-password-modal" data-dismiss="modal" aria-hidden="true">&times;</button>
              <h4 class="modal-title">Lấy lại mật khẩu</h4>
            </div>
            <div class="modal-body">
              <div class="form-group">
                <p>Nhập tài khoản email của bạn:</p>
                <input type="email" class="form-control" required ng-model="emailResetPassword">
              </div>
            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-default close-reset-password-modal" 
              data-dismiss="modal">Đóng</button>
              <button type="button" class="btn btn-primary resetPassword"
              ng-disabled="!resetPassword.$valid" 
              ng-click="resetPasswordClick(emailResetPassword)">
                Gửi
              </button>
            </div>
          </div>
        </form>
      </div>
    </div>
  
    <ng-flashcard data="1" ng-if="initFlashcard"></ng-flashcard>
  
    <div class="modal fade" id="petition-login">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h4 class="modal-title">Yêu cầu đăng nhập</h4>
          </div>
          <div class="modal-body">
               Bạn cần đăng nhập để sử dụng tính năng này!<br>
               <a class="btn btn-link" ng-click="goLogin()" href="#login">Đăng nhập</a>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
          </div>
        </div>
      </div>
    </div>

  <script src="js/lib.min.js?v=1503"></script>
  <script src="js/app.min.js?v=1503"></script>
  <script src="js/templates.js?v=1503"></script>

  <script>
  	window.addEventListener('load', function() {
	    FastClick.attach(document.body);
	  }, false);
    
    window.onload = function() {
      var element = document.getElementById("search-text-box");
      if (element != null && element.length > 0) {
        element.focus();  
      }
    }
  </script>
</body>
</html>