	$('#popupclose').click(function() { popupClose(); });
	// ポップアップ中央部のaタグ以外の部分クリックを無効
	$('#popupin').click(function(e){
		if(!$(e.target).closest('#popupin a').length){
			return false;
		}
	})
}, false );

// 設定量のスクロールで表示
window.addEventListener('scroll', function(){
    if(window.pageYOffset > pupTop){ popupOpen();}
}, false );

// ポップアップ表示
function popupOpen(){

    //ページごとに発動を制御
    var ignite_flag = $('span').hasClass('login_popup_js');
    if(ignite_flag === false){
        console.log('発動対象外');
        return;
    }

	if(!flagClose){
		$('#popup').fadeIn(200);
		// 念のため1/100秒遅らせてセンターへ移動
		setTimeout(function(){moveCenterBox('popupin')},10);
	}
}

// ポップアップ非表示
function popupClose(){
	flagClose = true;
	$('#popup').fadeOut(200);
}

// ボックスの中央表示
//window.addEventListener('load', function(){ moveCenterBox('popupin'); }, false );
window.addEventListener('resize', function(){ moveCenterBox('popupin'); }, false );

// 指定要素を中央に移動
function moveCenterBox(boxId){
	console.log(123);
	// 画面サイズ取得
	var winWidth = window.innerWidth;
	var winHeight = window.innerHeight;
	// 指定ボックスサイズ取得
	var boxWidth = document.getElementById(boxId).clientWidth;
	var boxHeight = document.getElementById(boxId).clientHeight;
	// センターに移動
	var boxLeft = (winWidth - boxWidth) / 2;
	var boxTop = (winHeight - boxHeight) / 2;
	document.getElementById(boxId).style.marginTop = boxTop + 'px';
	document.getElementById(boxId).style.marginLeft = boxLeft + 'px';
}
</script>



