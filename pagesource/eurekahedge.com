                function sendChatMessage() {


                    if ($("#chatMessageTextBox").val().trim().length) {
                        $("#logInChatBox").hide();
                        $("#loadingSymbol").show();
                        var now = new Date;
                        var utc_timestamp = Date.UTC(now.getUTCFullYear(), now.getUTCMonth(), now.getUTCDate(), now.getUTCHours(), now.getUTCMinutes(), now.getUTCSeconds(), now.getUTCMilliseconds());

                        var chatContent = [{ 'date': utc_timestamp, 'Owner': 'User', 'message': $("#chatMessageTextBox").val().trim(), 'UserName': $("#userName").val() }]

                        $.ajax({
                            url: '/ChatBox/sendChatMessage',
                type: 'POST',
                data: { 'emailId': $("#emailIdTag").val().trim(), 'chatMessage': JSON.stringify(chatContent), 'userName': $("#userName").val() },
                dataType: 'Json',
                success: function (data) {
                    if ('infoMsg' in data) {
                        console.log('nodata');
                    }
                    else {
                        if (data.messages) {
                            var requiredMessage = JSON.parse(data.messages);
                            var requiredMessage = sortByKey(requiredMessage, 'date');
                            var numberOfmessages = requiredMessage.length;
                            var storedHTML = "";
                            for (var each in requiredMessage) {
                                var dateTime = parseInt(requiredMessage[each].date);
                                messageDateTime = moment(dateTime).format('DD-MM-YYYY h:mm a');

                                if (requiredMessage[each].Owner == 'User') {
                                    if (requiredMessage[each].message != '') {
                                        var htmlContent = "<div class='chat-right' style='margin-bottom: 7px;'> <span>" + requiredMessage[each].message + "</span></div><div style='font-size: 71%;font-style: italic;padding-top: 16%;padding-left: 53%;'>" + messageDateTime.toLocaleString() + "</div><br />";
                                    }
                                    else {
                                        var htmlContent = "";
                                    }
                                }
                                else {
                                    var htmlContent = "<div class='chat-left' style='margin-bottom: 7px;'><span>" + requiredMessage[each].message + "</span></div><div style='font-size: 71%;font-style: italic;padding-top: 16%;padding-left: 9%;padding-right: 80px;'>" + requiredMessage[each].UserName + "," + messageDateTime.toLocaleString() + "</div><br/>"
                                }
                                storedHTML += htmlContent;
                            }
                            if (numberOfmessages < 3) {
                                storedHTML += "<div id='ehcp_user_message_div' style='padding-top:25px;'><p id='adminTypingTag' style='display:none;font-size:10px;color:#c0bcbc;position:absolute;margin-top:-20px'>typing...</p><input style='width:150px;' type='text' id='chatMessageTextBox' onchange='detectUserRead();' onkeyup='this.onchange();' onpaste='this.onchange();' oninput='this.onchange();' onclick='this.onchange();'/>&nbsp;&nbsp;&nbsp;&nbsp;<button  onclick='sendChatMessage()'>Send</button></div>"
                            }
                            else if (numberOfmessages == 3) {
                                storedHTML += "<div id='ehcp_user_message_div' style='padding-top:25px;'><p id='adminTypingTag' style='display:none;font-size:10px;color:#c0bcbc;position:absolute;margin-top:-20px'>typing...</p><input style='width:150px;' type='text' id='chatMessageTextBox' onchange='detectUserRead();' onkeyup='this.onchange();' onpaste='this.onchange();' oninput='this.onchange();' onclick='this.onchange();'/>&nbsp;&nbsp;&nbsp;&nbsp;<button  onclick='sendChatMessage()'>Send</button></div>"
                            }
                            else {
                                storedHTML += "<div id='ehcp_user_message_div' style='padding-top:25px;'><p id='adminTypingTag' style='display:none;font-size:10px;color:#c0bcbc;position:absolute;margin-top:-20px'>typing...</p><input style='width:150px;' type='text' id='chatMessageTextBox' onchange='detectUserRead();' onkeyup='this.onchange();' onpaste='this.onchange();' oninput='this.onchange();' onclick='this.onchange();'/>&nbsp;&nbsp;&nbsp;&nbsp;<button  onclick='sendChatMessage()'>Send</button></div>"
                            }
                            $("#messageContentDiv").html(storedHTML);
                            $("#loadingSymbol").hide();
                            $("#logInChatBox").show();
                            $('#wholeChatMessages').animate({ scrollTop: $('#wholeChatMessages').prop('scrollHeight') }, 0);
                        }
                    }
                }
            });
        };
    }



    $('#chatMessageTextBox').on('input', function (e) {
        alert('Changed!')
    });
