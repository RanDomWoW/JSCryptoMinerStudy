        $("#chatHeader").css('background-color', '#ff8400');
        $.ajax({
            url: '/ChatBox/userReadUpdate',
            type: 'POST',
            data: { 'emailid': $("#emailIdTag").val() },
            dataType: 'Json',
            success: function (data) {
            }
        });
    }

    //onchange="detectUserRead();"
    //onkeyup="this.onchange();" onpaste="this.onchange();" oninput="this.onchange();"


    function validateEmail(email) {
        var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return re.test(email);
    }


    (function () {
        $('.wp-live-chat-wraper').on('click', function () {
            $('.chat').slideToggle('swing', function () {
                jQuery("#wp-live-chat-header").addClass('active');
               
                //$('#wp-live-chat-header').removeClass('wplc-color-bg-1-green wplc-color-2 active').addClass('wplc-color-bg-1 wplc-color-2 active');
                $('#live-chat').fadeIn(30);
                if (!$('.chat').is(':hidden')) {
                    worker();
                    $("#chatHeader").css('background-color', '#FF8400');
                    $.ajax({
                        url: '/ChatBox/userReadUpdate',
                        type: 'POST',
                        data: { 'emailid': $("#emailIdTag").val() },
                        dataType: 'Json',
                        success: function (data) {
                        }
                    });
                }
                else {
                   // clearTimeout(worker);
                    clearTimeout(timer);
                    jQuery("#wp-live-chat-header").removeClass('active');
                    $('#live-chat').fadeOut(30);
                }
            });
            $('#wholeChatMessages').animate({ scrollTop: $('#wholeChatMessages').prop('scrollHeight') }, 0);

        });

        $('.chat-close').on('click', function (e) {
            e.preventDefault();
            $('#live-chat').fadeOut(300);
            clearTimeout(timer);

        });

    })();
    </script>
