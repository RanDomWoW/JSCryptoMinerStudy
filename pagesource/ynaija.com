/* Custom CSS */
/* Background Takeover */
.site-content{
    background-color: transparent !important;
}
.footer-sidebar, .copyright {
    margin-left: -30px;
    margin-right: -20px;
}
#masthead, .main-menu-fixed {
    background-color: transparent !important;   
}
#content{
   background-color: #fff;
}
#content .wrapper .grids{
    padding-top:15px;
    margin-left: 0px;
    background: #fff;
   /* background: rgba(255, 255, 255, 0.7) !important; */
}
.wrapper .grids .grid-8{
    margin-left: 15px;
}
.sticky-bar-container .grid-4 {
    margin-left: 15px;
}
.entries .grid-4{
    margin-left: 10px;
}
.single .entry-content {
    padding: 10px;
    background-color: #fff;
}
.title-with-sep, .layout-full .title-with-sep .title, .layout-full .title-with-sep .entry-title {
    background:none;
    background-image: none !important;
}
.layout-full .title-with-sep .entry-title{
   // background-color: transparent !important; 
}
.sidebar{
    margin-right: 15px;
}
#bg-takeover {
height: 100%;
width: 100%;
position: fixed;
top: 0;
left: 0;
z-index: 0;
background-repeat: repeat-y;
background-image: url("https://static.ynaija.com/wp-content/uploads/2018/02/06183212/Bounce-Awoof-Page-takeover1.png");
background-size:cover;
}
#bg-takeover a{
position:fixed;
}
#bg-takeover a:link {
height: inherit;
width: 100%;
display: block;
}
#wrap{
position:relative;
width: 85%;
margin:auto;
}
.top-strip, .main-menu-container {
    border-bottom: 1px solid #fff !important;
    border-left: 1px solid #fff;
    border-right: 1px solid #fff;
    border-top: 1px solid #fff;  
    background-color: #fe0000;    
}
.page-header {
    margin-bottom: 0px;
    /* padding:20px 0px 5px 30px;
    margin-left:-8px; */
    background-color: #ffffff; 
    /* background: rgba(255, 255, 255, 0.7) !important; */
}
.at-custom-sidebar-counter {
    background-color: #fff;
}
/* End Background Takeover */

.header .logo {
   max-width:250px;
   float:left;
}
.logo-mobile{
   display: none;
}
#branding {
    margin-top: 10px;
    margin-bottom: 10px;
}

.sticky-active .main-menu-fixed{
  top:32px!important;
}

/* Post Image Size */
.grid-layout .entry-image, .format-posts .entry-image, .related-posts .entry-image, .category-posts .entry-image, .featured-posts .entry-image, .latest-reviews .entry-image {
    max-height: 220px;
}

.home-section.advertising{
  width:105%;
  margin-left:-10px
}


/* Resize Top menu */
.secondary-menu>ul>li{
   line-height: 40px;
}

/* Reduce main menu font */
.main-menu > ul > li {
   font-size: 14px !important; 
}

/*  .main-menu>ul>li>a {
   height: 40px !important;
   line-height: 40px !important;
}
.main-menu>ul>li:after {
   margin-top:9px  !important;
}
 */

#masthead #searchform, #masthead #searchform input[type="text"], .top-strip{
  height: 50px;
}
.top-strip nav{
  float:left;
}
.main-menu {
    border-bottom: 0px solid #ffffff !important;
}

/* posts comment title */
h3.comment-reply-title{
      font-size: 1.2em;
}


/* Ads display Desktop and Mobile */
.header-leaderboard{
   width:728px;
}
.post-header-ad{
   width: 970px;
   margin:auto;
   height: auto;
}
.header-dfp {
   width: 728px;
}
.mobile-ad, .post-header-ad.mobile{
   display: none !important;
}
.desktop-ad{
   width: 728px;
   display: block;
}

/* Display on Desktop only */
.hide-mobile{
   display: block !important;
}

/* restrict post slider height movement */
.flexslider{
   max-height: 500px;
}

/* breadcumbs resize text */
#breadcrumbs{
   font-size: 13px;
   text-align:center;
   padding: 0px 10% 10px 10%;
   text-transform: capitalize;
    /* margin-top:-20px; */

}
#open-pageslide{
   color: #fff;
}
#breadcrumbs, #breadcrumbs a{
   color: #8c919b;  
}

.breadcrumb_last{
   display:none;
}

.entry-meta, .entry-meta a {
  color: #000;
  font-size: 14px;
}

.page-content h4, .entry-content h4 {
    font-size: 18px;
    text-transform: uppercase;
    letter-spacing: 0.05em;
    line-height: 150%;
    text-align:center !important;
}
.widget_ti-about-site img {
    max-width: 45%;
    margin: 0px;
}

#ti_code_banner-2 h3, #ti_code_banner-3 h3, #ti_code_banner-4 h3, #ti_code_banner-5 h3, #ti_code_banner-6 h3 {
   margin-bottom: 5px;
   font-size: 7px;
}
.td-adspot-title{
   font-family: Oswald;
   letter-spacing: 0.2em;
    text-transform: uppercase;
   font-size: 7px;
    font-weight: normal;
    font-style: normal;
    text-align: center;
}
.sidebar {
    padding: 15px;
}
.sidebar .widget {
    margin-bottom: 15px;
    padding-bottom: 20px;
}
.title-with-bg .title, .title-with-bg .entry-title, .title-with-sep .title, .title-with-sep .entry-title, .classic-layout .entry-title a {
    font-size: 70%;
}
.tag-box {
    font-size:14px;
    margin-bottom: 30px;
    padding-top: 10px;
    padding-bottom: 10px;   
    border-bottom: 1px solid #000;
}

.hentry .code-ad{
    width:106%;
    margin-left:-2%;
}
.ad-block div[style], img[Attributes Style]{
   width:100% !important;
   height:auto;
}
/* resize and adjust featured image */
.entry-media .base-image{
  max-width:300px;
  float:left;
  padding-right: 20px;
  display: block;
}
.mobile-featured  .base-image{
    display: none;
}

/* Only on Desktop view */
@media only screen and (min-width: 568px) {
.header-banner .logo, .header-default .logo {
    /* max-width: 60%; */
}
}

/* Mobile view */
@media screen and (max-width: 1200px) {
   /* Ads display Desktop and Mobile */
  .mobile-ad, .post-header-ad.mobile{
    display: block !important;
  }
  .desktop-ad, .post-header-ad.desktop, .header-leaderboard{
    display: none !important;
  }
}
@media screen and (max-width: 782px) {
.gr__ynaija_com{
   margin-top: 0px !important;
}
.gr__ynaija_com .top-strip{
   margin-top:46px;
}
}
@media screen and (max-width: 750px) {
  .grid-8, .grid-4{
    margin-left: 0px !important;
  }
  .header .inner>.inner-cell {
    margin-bottom: 0px;
  }
  .ad-block{
    margin-bottom: 7px !important;
  }
  .top-strip .wrapper{
    width:100% !important;
    padding: 0;
  }
 .hide-top, .logo, .posts-slider-section{
    display: none !important;
  }
  .logo-mobile{
    display: block;
    width:150px;
    float:right;
  }
  #branding {
    margin-top: 0px;
    margin-bottom: 0px;
  }

 /* Hide Latest title on Homepage header mobile */
  .latest-posts .section-header{
    display:none !important;
    margin-bottom: 10px;
  }
  .sidebar{
    /* display: none !important; */
    border:none !important;
    padding: 0px;
  }

  /* Reduce mobile leaderboard */
  .post-header-ad.mobile{
     width: 90%;
  }

   /* Reduce Image bottom space */
  .single .entry-media, .base-image img {
    margin-bottom: 10px;
  }
  
  /* featured image placement on mobile */
  .entry-media .base-image{
    display:none;
  }
  .mobile-featured .base-image{
    display:block;
    float: none;
    padding-right: 0px;
  }

  

    /* Display on Desktop only */
    .hide-mobile{
      display:none !important;
    }
    
   /* Retain Tag split view on mobile */
   .nav-previous {
      float: left;
   }
   .nav-next {
      float: right;
   }
   .nav-next, .nav-previous {
      width: 36%;
      padding: 0 7%;
      text-align: center;
   }
   .nav-single .sep {
      display: block;
      position: absolute;
      top: 0;
      left: 50%;
      width: 1px;
      height: 100%;
      border-left: 1px dotted #444;
      margin: 0;
      background: none;
    }
    .nav-next [class^="icomoon-"], .nav-previous [class^="icomoon-"] {
      display: block;
    }
    .single-box.nav-single{
        margin-bottom: 30px;
