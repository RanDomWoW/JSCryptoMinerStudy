$( document ).ready(function() {
	 var action = $("#socialFeedPopupUrl").val();
	 var socialSite = "facebook";
		var options = {
				url: action,
				data: {socialSite:socialSite},
				type: 'GET',
				
				success: function (data)
				{				
					var feedData = JSON.parse(data);
					var feedDataLength = feedData.data.length;	
					
						var count = 0;
						for(var i=0; i<feedDataLength; i++){							
							var type = feedData.data[i].type;
							
							if(type!=="link" && count<10){
								
								var imageSrc= feedData.data[i].full_picture;								
								var description = feedData.data[i].message;
								var result = urlify(description);
								console.log("result" +result);
								var link =	feedData.data[i].link;
								$(".trends-fb-feed").append('<div class="fb-feed-image"><a href="'+link+'" target=_blank><img src="'+imageSrc+'"></a></div><div class="fb-feed-description">'+result+'</div>');
								$(".social-feed-header .trends-fb-feed .fb-feed-image img").attr("src" ,imageSrc);
								$(".social-feed-header .trends-fb-feed .fb-feed-description").html(result);
								count++;
							}
						}

				},
				error: function (data)
				{
					
				}
			}; 
		$.ajax(options);	
});

function urlify(text) {
    var urlRegex = /(https?:\/\/[^\s]+)/g;
    return text.replace(urlRegex, function(url) {
        return '<a href="' + url + '" target=_>' + url + '</a>';
    })
}

