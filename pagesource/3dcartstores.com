<script>
var waypoints = $('.magic-area').waypoint({
	
	handler: function(direction) {
		
		if($(window).width() < 1023) return;

		$('.magic-circle-inner, .magic-center-title').addClass('animated fadeIn');
		$('.magic-circle-outer').addClass('animated fadeIn slow-transition');
			
			
		setTimeout(function() {
			
			$('.magic-circle-outer').removeClass('animated fadeIn slow-transition');
			$('.magic-circle-outer').addClass('rotated');
			
			setTimeout(function() {
				$('.magic-pins').addClass('animated fadeIn');
				setTimeout(function() {
					$('.magic-pins').removeClass('pre-animation');
					
					setTimeout(function() {
						$('.magic-area-left .magic-area-text h4, .magic-area-left .magic-area-text p').addClass('animated fadeInLeft');
						$('.magic-area-right .magic-area-text h4, .magic-area-right .magic-area-text p').addClass('animated fadeInRight');
					}, 600);
					
					
				}, 500 );
				
				
			}, 600);
			
		}, 500);
		
	},
	offset: '50%'
});

function initHero() {
	
	var windowW = window.innerWidth;
	var windowH = window.innerHeight;
	
	var heroH = windowH;
	
	console.log(window.innerHeight);
	
	if(heroH > 990) heroH = 990;
	
	if(windowW < 1600 && windowW >= 1024) {
		heroH = (55 * windowW)/100;
	}
	
	if(windowW < 1023) heroH = 550;
	
	if(windowW < 767) heroH = 400;
	
	
	$('.tc7-hero').css("height", heroH);
	
	
	var heroTextH = $('.tc7-hero .container1170').outerHeight();
	
	var heroImgH = heroH - heroTextH;
	
	$('.hero-image').css("height", heroImgH);
	
	//$('.tc7-hero').addClass('fadeIn animated');
}

$(document).ready(function(e) {
   initHero();
   
   $('#sliderarea-carousel').carousel(); 
   
   $('.fv-item a').click(function(e) {
	   
	   /*$('.fv-item a').removeClass('active');
	   $(this).addClass('active');*/
		
	});
	
	$('#sliderarea-carousel').on('slide.bs.carousel', function (e) {
		
		//var slideFrom = $(this).find('.active').index();
		//var slideTo = $(e.relatedTarget).index();
		
		var slideTo = $(e.relatedTarget).index() + 1;
		
		$('.fv-item a').removeClass('active');
		
		$('.fv-item:nth-child('+slideTo+') a').addClass('active');
		
		
	});
	
	$('#fv-items-toggle').click(function(e) {
        $('#fv-items').slideToggle();
    });
   
});
$(window).resize(function(e) {
   initHero();
});

</script>

