<script type="text/javascript">
    $().ready(function () {
        var $modalReview = $("#jsDialogReview");
        var $sendReviewForm = $modalReview.find('form');
        var $sendReviewText = $sendReviewForm.find("textarea");
        var $sendReviewType = $sendReviewForm.find("select");
        var $sendReviewError = $sendReviewForm.find(".jsReviewError");
        var $sendReviewGreeting = $modalReview.find(".jsReviewGreeting");
        var $sendReviewGreetingText = $sendReviewGreeting.find(".jsReviewGreetingText");
        
        $(".jsInitAddReviewModal").on("click", function(){
            $modalReview.modal({
                keyboard: false
            });
            $sendReviewForm.data('review', $(this).data("review"));

            return false;
        });
        
        $sendReviewType.on("change", function(){
            var select = $(this);
            var style = select.find(':selected').data('style')
            select.closest(".form-group").removeClass('success').removeClass('warning').addClass(style);
        });
        
        $sendReviewForm.on("submit", function() {
            var $form = $(this);
            
            $.ajax({
                type: "POST",
                url: "/review/add/",
                data: {
                    'review': $form.data('review'),
                    'score': $sendReviewType.val(),
                    'text': $sendReviewText.val()
                },
                dataType: 'json'
            }).success(function (data) {
                if (!data.error) {
                    $sendReviewForm.hide();
                    $sendReviewGreetingText.html(data.message);
                    $sendReviewGreeting.show();
                } else {
                    $sendReviewError.html(data.message).show();
                }
            });
            
            return false;
        });
        
        $modalReview.on('hidden.bs.modal', function (e) {
            $sendReviewError.hide();
            $sendReviewGreeting.hide();
            $sendReviewForm.show();
            $sendReviewType[0].selectedIndex = 0;
            $sendReviewText.val("");
            $sendReviewGreetingText.html("");
            $sendReviewForm.find(".form-group").removeClass('success').removeClass('warning');
        });
    });
</script>
