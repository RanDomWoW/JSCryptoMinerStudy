<script>
    head.ready(function() {
        var start = $("section.articles-list-itm").length;
        var limit = 6;
        var category_alias = 'main-page-category';
        var pregcalendar_id = '';
        var htmlArticle = '';

        var addArticles = function(item) {
            htmlArticle += '<section class="articles-list-itm row-3 ' + item['article_tags_inline'] + '" itemtype="http://schema.org/BlogPosting" itemscope="" itemprop="blogPost" data-id="' + item['id'] + '" date_pub_int="' + item['date_pub_int'] + '">'
                + '<div class="articles-list-itm-wrp row-3">'
                    + '<a class="articles-list-img row-3" href="' + item['url'] + '">'
                        + '<img class="articles-list-img-itm" src="' + item['avatar_data']['images']['latest'] + '">'
                    + '</a>'
                    + '<div class="articles-list-statistic clearfix">'
                        + '<time class="articles-list-date" itemprop="datePublished" datetime="' + item['datetime'] + '" pubdate="">' + item['date_pub'] + '</time>'
                        + '<div class="articles-list-views">' + item['views'] + '</div>'
                    + '</div>'
                    + '<h2 class="articles-list-title row-3" itemprop="headline">'
                        + '<a class="articles-list-title-lnk" itemprop="url" href="' + item['url'] + '">'
                            + '<span itemprop="name">' + item['title'] + '</span>'
                        + '</a>'
                    + '</h2>'
                    + '<a class="articles-list-category" href="' + item['category_url'] + '">'
                        + item['category_title']
                    + '</a>'
                    + '<a class="articles-list-readmore" href="' + item['url'] + '">'
                        + '<span>Еще</span>'
                    + '</a>'
                + '</div>'
            + '</section>';
        }

        // bind filter button click
        $('.filters-button-group').on('click', 'li', function() {
            $('#readmore-btn').show();

            var filterValue = $(this).attr('data-filter') || '*';

            var tagAlias = filterValue.replace('.', '');

            isotopeContainer.one('layoutComplete', function( event, filteredItems ) {
                if (0 !== tagAlias.localeCompare('*')) {
                    var newLimit = limit - filteredItems.length;

                    if (newLimit > 0) {
                        var elems = [];
                        var articles = $('#article-panes > section').toArray();

                        articles.forEach(function(item){
                            elems.push($(item).attr('data-id'));
                        });

                        $.ajax({
                            type: 'POST',
                            url: '/article/index/latest',
                            data: {
                                needRefreshCache: true,
                                limit: newLimit,
                                elems: elems,
                                tag_alias: tagAlias
                            }
                        }).done(function(data) {
                            var data = JSON.parse(data);

                            htmlArticle = '';

                            data.forEach(addArticles);

                            isotopeContainer.isotope('insert', $(htmlArticle));

                            $(htmlArticle).imagesLoaded(function(){
                                isotopeContainer.isotope('layout');
                            });
                        });
                    }
                }
            });

            isotopeContainer.isotope({
                filter : filterValue
            });
        });

        // Show more button
        $('#readmore-btn').click(function() {
            start = $("section.articles-list-itm").length;

            var elems = [];
            var articles = $('#article-panes > section').toArray();

            articles.forEach(function(item){
                elems.push($(item).attr('data-id'));
            });

            $(this).addClass('load');

            var filterValue = $('.filters-button-group > li.is-checked').attr('data-filter') || '*';

            var tagAlias = filterValue.replace('.', '');

            var ajaxData = {
                needRefreshCache: true,
                start: start,
                limit: limit,
                elems: elems
            };

            if (0 !== tagAlias.localeCompare('*')) {
                ajaxData.tag_alias = tagAlias;
            }

            if (category_alias.length) {
                ajaxData.category_alias = category_alias;
            }

            if (pregcalendar_id.length) {
                ajaxData.pregcalendar_id = pregcalendar_id;
            }

            $.ajax({
                type: 'POST',
                url: '/article/index/latest',
                data: ajaxData
            }).done(function(data) {
                var data = JSON.parse(data);

                htmlArticle = '';

                data.forEach(addArticles);

                // Appends elements into container element, adds elements as items, and arranges items with filtering and sorting
                isotopeContainer.isotope('insert', $(htmlArticle));

                $(htmlArticle).imagesLoaded(function(){
                    isotopeContainer.isotope('layout');

                    $('#readmore-btn').removeClass('load');
                });

                if (data.length < limit) {
                    $('#readmore-btn').hide();
                }
            });

            return false;
        });
    });
</script>                        
