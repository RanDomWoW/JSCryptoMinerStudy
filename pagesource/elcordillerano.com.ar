        function remover_banners_segun_version(tipo){
            
            /* SI ES MOBILE ELIMINO TODOS LOS BANNERS SOLO DESKTOP */
            if($("body").hasClass("es_mobile")){
                $("."+tipo+".solo_desktop").each(function(){
                    var cantidad_banners = $(this).parent().children().length;
                    if(cantidad_banners == 1){
                        $(this).parent().parent().parent().remove();
                    }else{
                        $(this).remove();
                    }
                });
            /* SI ES DESKTOP ELIMINO TODOS LOS BANNERS SOLO MOBILE */
            }else{
                $("."+tipo+".solo_mobile").each(function(){
                    var cantidad_banners = $(this).parent().children().length;
                    if(cantidad_banners == 1){
                        $(this).parent().parent().parent().remove();
                    }else{
                        $(this).remove();
                    }
                    
                });
            }
                
        }
        function reposicionamiento_elementos_header(){
            var ancho_body = $("body").width();
            
            if(ancho_body<=750 && $(".espacio_logo_desktop").attr("estado")=="lleno"){
                var contenido_logo = $(".espacio_logo_desktop").html();
                
                $(".espacio_logo_mobile").html(contenido_logo);
                $(".espacio_logo_desktop").html("");
                $(".espacio_logo_desktop").attr("estado","vacio");
                
                $("#columna_banner_header_izquierda").addClass("mobile");
                $("#columna_banner_header_derecha").addClass("mobile");
            }   
            
            if(ancho_body>750 && $(".espacio_logo_desktop").attr("estado")=="vacio"){
                var contenido_logo = $(".espacio_logo_mobile").html();
                
                $(".espacio_logo_desktop").html(contenido_logo);
                $(".espacio_logo_mobile").html("");
                $(".espacio_logo_desktop").attr("estado","lleno");
                
                $("#columna_banner_header_izquierda").removeClass("mobile");
                $("#columna_banner_header_derecha").removeClass("mobile");
            }      
        }
        
