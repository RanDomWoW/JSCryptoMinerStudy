        <script>

            /** 
            * Método encargado de cargar los banners random    
            **/        
            function cargar_banners_random(){

                
                $(".ly_elementos_individual_contenido.zona_banners").each(function(){
                    
                    banners = $(this).find("banner");
                    var zona = $(this);
                    var zonas_tipos_id = parseInt(zona.attr("zonas_tipos_id"));
                    
                    switch(zonas_tipos_id){
                        /* Estáticos */
                        case 1:

                            $(banners).each(function(){
                                generar_banner(zona,$(this));
                            });

                        break;
                        /* Aleatorios */
                        case 2:
                       
                            if(typeof(banners.eq(0).attr("id"))!="undefined"){

                                var banner_elegido_random = $(banners[Math.floor(Math.random()*banners.length)]);
                                
                                generar_banner(zona,banner_elegido_random);
                            }
                            
                        break;
                    }
                    
                    
                });

                $("banner").remove();

                $(".individual_banner_imagen img").each(function(){                    
                    $(this).attr("src",$(this).attr("data-src-banner"));
                })
                
            }

            function generar_banner(zona,banner){
                var id = $(banner).attr("id");
                var zonas_id = $(banner).attr("zonas_id");
                var enlace = $(banner).attr("enlace");
                var target = $(banner).attr("target");
                var ancho = $(banner).attr("ancho");
                var alto = $(banner).attr("alto");
                var url_archivo = $(banner).attr("url_archivo");
                var mobile = $(banner).attr("mobile");
                var desktop = $(banner).attr("desktop");
                
                var web_es_mobile = $("body").hasClass("es_mobile");
                
               
                /* SI ESTAMOS DESDE DISPOSITIVO MOBILE */
                if(web_es_mobile){
                    
                    /* Si ES MOBILE Y DESKTOP */
                    if(mobile || (mobile && desktop) ){
                        $(zona).append('<div class="individual_banner_imagen"><a href="'+enlace+'" target="'+target+'" > <img data-src-banner= "'+url_archivo+'" src="/img/blanco.png" style="width:'+ancho+' ;height:'+alto+'" /> </a></div>');                      
                    }
                
                /* SI ESTAMOS EN DISPOSITIVO DESKTOP */
                }else{
                    
                    /* Si ES MOBILE Y DESKTOP */
                    if(desktop || (mobile && desktop)){
                        $(zona).append('<div class="individual_banner_imagen"><a href="'+enlace+'" target="'+target+'" > <img data-src-banner= "'+url_archivo+'" src="/img/blanco.png" style="width:'+ancho+' ;height:'+alto+'" /> </a></div>');                      
                    }
                }
                
               
            }

        </script>
            
