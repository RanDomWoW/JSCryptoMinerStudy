<script type="text/javascript">
$(document).ready(function(){
    $('#bli_boton').css({'height':$('#panelDerecha').css('height'), 'cursor':'pointer'});
    $('#bli').show();


var ocultoMenu = false;
var ocultandoseMenu = false;
var fijadoMenu = true;
var tamIzq;
function ocultarMenu(){
    ocultandoseMenu = true;
    tamIzq=$('#panelIzquierda').width();
    $("#panelIzquierda").animate({
        'margin-left': '-'+(tamIzq)+'px',
        opacity: 0
    }, 400, function() {
        $('#bli_boton_i,#bli_boton_d').toggle();
        ocultoMenu = true;
        ocultandoseMenu = false;
        fijadoMenu = false;
    }); 
    
    $("#panelDerecha").animate({
        width:''+($(window).width()-($('#bli_boton').width()+1))+'px',
        'padding-left':'0px'
    }, 400);
}

function mostrarMenu(){
    
    $("#panelIzquierda").animate({
        'margin-left':"0px",
        opacity:1
    }, 400, function(){
        $('#bli_boton_i,#bli_boton_d').toggle();
        ocultoMenu = false;
    });

    $("#panelDerecha").animate({
        width:($(window).width()-tamIzq-$('#bli_boton').width()-5)+'px',
        'padding-left': "0px"
    }, 400);
}

$('#bli_boton').click(function(){fijadoMenu=true});

$('#bli_boton').toggle(ocultarMenu, mostrarMenu);

});
</script>
