<script type="text/javascript">
		function getSlide(from, to) {
			$('.slideshowContainer .slide').removeClass('lastactive');
			$('.slideshowContainer').find("[data-slidenum='"+to+"']").addClass('active');
			$('.slideshowContainer').find("[data-slidenum='"+from+"']").removeClass('active').addClass('lastactive');
			$('.slideshowContainer .slide.active').css('opacity','0');
			$('.slideshowContainer .slide.active').transition({ opacity:1 }, 500, 'out');
			$('.slideshowContainer .slide.lastactive').transition({ opacity:0 }, 500, 'in');
		}
	$(document).ready(function() {
			var slideCount = $('.slideshowContainer .slide').length;
			var crntSlide = 1;
			var lastSlide = 0;
			var dtnImg = false;
			var timerOver = 0;
			var slidetimer = 0;

			function nextSlide() {
				lastSlide = crntSlide;
				if (crntSlide == slideCount) {
					crntSlide = 1;
				} else {
					crntSlide++;
				}
				getSlide(lastSlide, crntSlide);	
			}
			
			function prevSlide() {
				lastSlide = crntSlide;
				if (crntSlide == 1) {
					crntSlide = slideCount;
				} else {
					crntSlide--;
				}
				getSlide(lastSlide, crntSlide);
			}

			function sliderInterval(){
				nextSlide();
			}
			
			if($('.slideshow .dtnad').length > 0)
			{
				dtntimer = setInterval(function()
				{
					if($('.slideshow .dtnad img').length > 1)
						dtnImg = true;
					if(dtnImg == true && timerOver == 0)
						timerOver = 1;
						clearInterval(dtntimer);
						slidetimer = setInterval(sliderInterval, 5000);
				},100)
			} else {
				slidetimer = setInterval(sliderInterval, 5000);
			}
			
			$('.slideshow .slide').each(function()
			{
				$(this).removeClass('active');
				if($(this).attr('data-slidenum') == 1)
					$(this).addClass('active');
			});

			$('.slideshow .slide-content').each(function()
			{
				$(this).removeClass('active');
				if($(this).attr('data-slidenum') == 1)
					$(this).addClass('active');
			});

			$('.slideshowContainer .next').click( function (e) {
				clearInterval(slidetimer);
				nextSlide();
				slidetimer = setInterval(sliderInterval, 5000);
			});

			$('.slideshowContainer .prev').click( function () {
				clearInterval(slidetimer);
				prevSlide();
				slidetimer = setInterval(sliderInterval, 5000);
			});			
			
	});
</script>


 

