var dom_par = dom_obj.parentNode
dom_par.removeChild(dom_obj)

var slide_wrap = document.createElement('div');
slide_wrap.classList.add('slide_selector')
slide_wrap.innerHTML = tag
var dom_toinsert = document.getElementsByClassName("slide_wrap")[0];
dom_toinsert.appendChild(slide_wrap);

$('.slide_selector').fadeIn(1000);
$('.slide_selector').slick({
arrows: false,
dots: true,
autoplay: true,
autoplaySpeed: 5000,
