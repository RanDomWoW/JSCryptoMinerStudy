    tag = tag + '<p><a href="' + href2 +'" target="' +  banner[item]['target'] + '"><img src="' +  banner[item]['src']  + '" alt="' + banner[item]['alt'] + '" /></a></p>'
   }
  }
 }
}else{
 var i = 0;
 for (var item in banner) {
  if(i < 9){
    tag = tag + '<p><a href="' + banner[item]['href'] +'" target="' +  banner[item]['target'] + '"><img src="' +  banner[item]['src']  + '" alt="' + banner[item]['alt'] + '" /></a></p>'
   }
  i++;
 }
}

