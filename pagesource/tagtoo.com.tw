<script>
        $(document).ready(function () {
            //owl Carousel
            $("#index-slides").owlCarousel({
                items:1,
                loop:true,
                margin:0,
                nav:true,
                navText : ["", ""]
            });
            $("#customer-slides").owlCarousel({
                items:6,
                loop:true,
                nav:false,
                navText : ["", ""],
                autoplay:true,
                autoplayTimeout:3000,
                center: true,
                responsive:{
                    300:{
                        items:2,
                        center: false,
                        margin:0
                    },
                    600:{
                        items:3
                    },
                    800:{
                        items:5
                    },
                    1100:{
                        items:6,
                        center: false
                    }
                }
            });
            var owl = $('.owl-carousel');
            owl.owlCarousel();
            $('.owl-carousel .btn-more').click(function() {
                owl.trigger('next.owl.carousel');
            })



            var $dots = $('.owl-dot');

            function goToNextCarouselPage() {
                var $next = $dots.filter('.active').next();

                if (!$next.length)
                    $next = $dots.first();

                $next.trigger('click');
            }
            function goTopPrevCarouselPage() {
                var $prev = $dots.filter('.active').prev();

                if (!$prev.length)
                    $prev = $dots.first();

                $prev.trigger('click');
            }

            $('.btn-owl-next').click(function () {
                goToNextCarouselPage();
            });
            $('.btn-owl-prev').click(function () {
                goTopPrevCarouselPage();
            });
        });


        $(document).foundation();
    </script>
