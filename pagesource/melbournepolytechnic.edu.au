	var _courseFinderAutoComplete = $("#course-finder-autocomplete");
	var _courseData;
	var _shortCourseData;
	//$.getJSON( "/api/courses/json/index.json", function( data ) {
	//LIVE
	$.getJSON( "/api/courses/json/", function( data ) {
		//$.getJSON( "/api/courses.json", function( data ) {
		_courseData = data;
	});

	$.getJSON( "/api/shortcourses/json/", function( data ) {
		//$.getJSON( "/api/shortcourses.json", function( data ) {
		_shortCourseData = data;
	});


	/* bindings =============================================================== */

	// arrow down
	$('#down-arrow-move').on('click', function(){
		scrollToCourseFinder();
		return false;
	});

	// courses link
	$('.courses-link').on('click' , function(){
		showCourseOverlay( $(this) );
		return false;
	});

	// courses link
	$('.courses-overlay li').on('mouseover' , function(){
		$(this).addClass("hover");
		return false;
	}).on('mouseout', function(){
		$(this).removeClass("hover");
	});

	$('.courses-link.no-subcategory a').on('click' , function(){
		var lnk = $(this);
		showCourseOverlay( lnk.parent() );

		$(".courses-overlay").addClass("sub").find("li.active").removeClass("active");
		lnk.siblings(".courses-overlay").addClass("active");

		keywords = lnk.text().toLowerCase().trim();
		keywords = keywords.replace("&", "and");

		//console.log("Search for [" +  keywords+"]");
		//
		if(keywords == "early childhood education and care") {
			keywords = "early childhood education";
		}

		var	items = [], maxItems = 10,	i = 0;
		var displayItems = [];
		var tmp;


		if( _courseData.courses.length > 0 ){
			$.each( _courseData.courses, function( key , val ) {

				tmpName = val.name;
				tmpName = tmpName.toLowerCase();
				tmpAreas = val.studyareas;
				tmpAreas = tmpAreas.join();
				tmpAreas = tmpAreas.toLowerCase();

				if(tmpName.indexOf(keywords) !== -1 || tmpAreas.indexOf(keywords) !== -1 ) {

					tmp = "<li><a href='" + val.link + "'><span>" + val.name;
					if(val.focusofstudy && val.focusofstudy != ''){
						tmp = tmp + " - " + val.focusofstudy;
					}
					tmp = tmp +"</span></a></li>";
			  		items.push( tmp ); // push matching items into an array
			  		i++;
			  		if( i < maxItems ){ displayItems.push( tmp ) } // only display first x items
		  		}
			});
			lnk.parent().find(".container ul").remove();
			lnk.parent().find(".container").append("<ul>"+displayItems.join( "" )+"<li class='close-sub'><a href='#' class='close-button close-sub desktop'><i class='icon-chevron-left'></i> back</a></li></ul>" );


			_courseFinder.height($('.courses-overlay.active').outerHeight()+ lnk.find("ul").outerHeight());


			lnk.parent().find("ul").addClass('show');

			$(".close-sub .close-button").on('click', function(){

				$(this).closest("ul").remove();

				$(".courses-overlay").removeClass("sub").removeClass("active");

				_courseFinder.height($('.courses-overlay.active').outerHeight()+60);

				closeCourseOverlay();

			 	return false;
			});

			lnk.parent().find("ul a").on("click", function(e){
				window.location = $(this).attr("href");
				return true;
			});
		}

		//});
		return false;
	});

	$('.courses-link ul a').on('click' , function(){


		var lnk = $(this);
		if(lnk.hasClass("lnk-direct")) {

			window.location = lnk.attr("href");
			return false;
		}

		$(".courses-overlay").addClass("sub").find("li.active").removeClass("active");
		lnk.parent().addClass("active");

		keywords = lnk.text().toLowerCase();
		keywords = keywords.replace("&", "and").trim();

		//console.log("Search for [" +  keywords+"]");

		var	items = [], maxItems = 10,	i = 0;
		var displayItems = [];
		var tmp;

		if( _courseData.courses.length > 0 ){
			$.each( _courseData.courses, function( key , val ) {


				tmpName = val.name.toLowerCase();
				tmpAreas = val.studyareas;
				tmpAreas = tmpAreas.join();
				tmpAreas = tmpAreas.toLowerCase();

				if(tmpName.indexOf(keywords) !== -1 || tmpAreas.indexOf(keywords) !== -1 ) {
					tmp = "<li><a href='" + val.link + "'><span>" + val.name;
					if(val.focusofstudy && val.focusofstudy != ''){
						tmp = tmp + " - " + val.focusofstudy;
					}
					tmp = tmp +"</span></a></li>";
			  		items.push( tmp ); // push matching items into an array
			  		i++;
			  		if( i < maxItems ){ displayItems.push( tmp ) } // only display first x items
		  		}
			});
			lnk.append("<ul>"+displayItems.join( "" )+"<li class='close-sub'><a href='#' class='close-button close-sub desktop'><i class='icon-chevron-left'></i> back</a></li></ul>" ); // populate the result list
			//lnk.append("<ul>"+displayItems.join( "" )+"</ul>" );

			_courseFinder.height($('.courses-overlay.active').outerHeight()+ lnk.find("ul").outerHeight());

			lnk.find("ul").addClass('show');

			$(".close-sub .close-button").on('click', function(){
				var subcategory = $(this).closest(".active");
				subcategory.removeClass("active").find("ul").remove();
				$(".courses-overlay").removeClass("sub");
				$(".courses-overlay li.hover").removeClass("hover");
				_courseFinder.height($('.courses-overlay.active').outerHeight()+60);
			 	return false;
			});

			lnk.find("a").on("click", function(e){
				e.stopPropagation();
				return true;
			});
		}

		return false;
	});

	// close overlay
	$('.courses-overlay .close-button').on('click' , function(){
		closeCourseOverlay( $(this) );
		return false;
	})




	/* Functions ============================================================== */

	// initialise swiper
	function initSwiper(){
		var mySwiper = new Swiper ('.swiper-container', {
			pagination: ".swiper-pagination",
			paginationClickable: true,
			grabCursor: true,
			slidesPerView: 4,
			spaceBetween: 20,
			breakpoints: {
				320: {
					slidesPerView: 1,
					spaceBetween: 5
				},
				480: {
					slidesPerView: 1,
					spaceBetween: 5
				},
				768: {
					slidesPerView: 3,
					spaceBetween: 20
				},
				1024: {
					slidesPerView: 3,
					spaceBetween: 20
				},
				1440: {
					slidesPerView: 3,
					spaceBetween: 20
				}
			}
		})
	}


	// scroll to the course finder section
	function scrollToCourseFinder(){
		$('html, body').animate({
			scrollTop: _courseFinder.offset().top
		}, 300, "swing")
	}


	// show course overlay
	function showCourseOverlay( elem ){

		if(elem.find('.courses-overlay').length > 0) {
			elem.closest('.courses').addClass("show-overlay");
		}
		$('.courses-overlay.active').removeClass('active');
		elem.find('.courses-overlay').addClass('active');


		_courseFinder.height(elem.find('.courses-overlay').outerHeight()+60);

		elem.find('ul').addClass('show');

		scrollToCourseFinder()
	}


	// close course overlay
	function closeCourseOverlay( elem ){

		$('ul.show').removeClass('show');
		$('.active').removeClass('active');
		$(".courses-overlay").removeClass("sub");

		_courses.removeClass("show-overlay");
		_courses.find("ul ul").remove();
		if(elem) {
			elem.parent().removeClass('active');
		}else {
			_courseFinder.find(".courses-overlay.active").removeClass("active");
		}
		_courseFinder.removeAttr("style");
	}

	function clearSearchResults() {
		populateSearchResults('');
		_courses.removeClass("show-overlay");
		_courseFinder.removeAttr("style");
	}

	// populate search results
	function populateSearchResults( keywords ){

		// if keywords is undefined, dont bother
		if( typeof keywords === 'undefined' ){ return }
			if( keywords == '' ){ _courseFinderAutoComplete.hide(); return; }

		// security check and url encode keywords
		keywords = encodeURIComponent( keywords.toLowerCase() )

		var words = keywords.split("%20");

			var	items = [], maxItems = 10,	i = 0;
			var displayItems = [];
			var tmp;
			var _viewAll = $('#course-finder-autocomplete .view-all');

			// show the results panel
			_courseFinderAutoComplete.slideDown();

			if( _courseData.courses.length > 0 || _shortCourseData.courses.length > 0  ){

				_courseFinderErrorMessage.hide(); // hide any error messages

				for (j = 0; j < words.length; j++) {
					keyword = words[j];

					if(keyword.length >= 3) {

						$.each( _courseData.courses, function( key , val ) {
							tmpName = val.name;
							tmpName = tmpName.toLowerCase();
							tmpAreas = val.studyareas;
							tmpAreas = tmpAreas.join();
							tmpAreas = tmpAreas.toLowerCase();

							//console.log(tmpName +"== "+keyword);
							if(tmpName.indexOf(keyword) !== -1 || tmpAreas.indexOf(keyword) !== -1 ) {


								tmp = "<li><a href='" + val.link + "'><span>" + val.name;
								if(val.focusofstudy && val.focusofstudy != ''){
									tmp = tmp + " - " + val.focusofstudy;
								}
								tmp = tmp +"</span></a></li>";

								if($.inArray(tmp,items) <= -1) {

							  		items.push( tmp ); // push matching items into an array
							  		i++;
							  		if( i < maxItems ){
							  			displayItems.push( tmp );
							  		} // only display first x items
								}else {
									//console.log("IGNORE " + val.name);

								}
					  		}
						});

						$.each( _shortCourseData.shortcourses, function( key , val ) {
							tmpName = val.name;
							tmpName = tmpName.toLowerCase();

							//console.log(tmpName +"== "+keyword);
							if(tmpName.indexOf(keyword) !== -1 ) {


								tmp = "<li><a href='" + val.link + "'><span>" + val.name;
								tmp = tmp +"</span></a></li>";

								if($.inArray(tmp,items) <= -1) {

							  		items.push( tmp ); // push matching items into an array
							  		i++;
							  		if( i < maxItems ){
							  			displayItems.push( tmp );
							  		} // only display first x items
								}else {
									//console.log("IGNORE " + val.name);

								}
					  		}
						});
					}


				}

				//console.log("---- END LOOP ----");


				_courseFinderAutoComplete.find("ul").html( displayItems.join( "" ) ); // populate the result list

				if(displayItems.length > 0) {
					if( i > maxItems ){
						_viewAll.css('display' , 'block')
							.attr('href' , '/courses/search/' + keywords + '/all/all/all/all/false/1/relevance') // update the href
							.find('.result-count').html( items.length ) // show number of results
					} else {
						_viewAll.hide();
					}
					_courses.addClass("show-overlay");
					_courseFinder.height($('#course-finder-autocomplete .container').outerHeight()+120);
				} else {

					_courseFinderErrorMessage.show().find('.keywords').html( _courseSearchInput.val() );
					_courseFinderAutoComplete.find(".no-result-link").attr('href' , '/courses/search/' + keywords + '/all/all/all/all/false/1/relevance')
					_courseFinderAutoComplete.find("ul").html('');

					_viewAll.hide();

				}


			} else {

			}

		//});
	}



	// search input handler
	function courseSearchInputHandler(){
		// vars
		var typingTimer;
		var doneTypingInterval = 0;

		//on keyup, start the countdown
		_courseSearchInput.on('keyup', function () {

			if($('.courses-overlay.active').length>0) {
				closeCourseOverlay();
			}

			if(_courseSearchInput.val().length == 0) {

				_courseFinder.find("button[type=submit]").show();
				_courseFinder.find("button[type=reset]").hide();
				clearSearchResults();
			} else {
				_courseFinder.find("button[type=submit]").hide();
				_courseFinder.find("button[type=reset]").show();

				clearTimeout(typingTimer);
				typingTimer = setTimeout(doneTyping, doneTypingInterval);

				_courseSearchInput.addClass('searching');

			}
		});

		//on keydown, clear the countdown
		_courseSearchInput.on('keydown', function () {
			clearTimeout(typingTimer);
		});

		// user has finished typing
		function doneTyping () {
			populateSearchResults( _courseSearchInput.val() );
			_courseSearchInput.removeClass('searching');
		}

		_courseFinder.find("button[type=reset]").click(function(){
			_courseSearchInput.val("").trigger("keyup");
			return false;
		});

	}




</script>

