        <script>
        var useFileApi = window.File && window.FileReader && window.FileList && window.Blob;
        var UploadHost = 'http://upload.my-files.ru';
        var UploadSessionKey = '45b7ec62-19ea-4f31-bfce-3150791de556';
        var InnerFileUpload;
        var StoredFileName;
        var IsChangesSaved = false;
        var IsFileUploaded = false;
        var IsCanceled = false;
        var UploadPurpose = 'Send';
        var SelectedFileSize;

        var $fileUploadButton = $("#fileUploadButton");
        var selectFileDefaultText = $("#selectFileText").html();
        $(document)
            .on("dragend dragstart dragenter dragleave drag dragover",
                function(e) {
                    e.stopPropagation();
                    event.preventDefault();
                    if (event.type === "dragleave")
                        $("#selectFileText").html(selectFileDefaultText);
                    else
                        $("#selectFileText").html("ОТПУСТИТЕ ЗДЕСЬ");
                });
        $(document)
            .on("drop",
                function(e) {
                    e.stopPropagation();
                    e.preventDefault();
                    var files = e.originalEvent.dataTransfer.files;
                    var file = files[0];
                    if (!file) {
                        $("#selectFileText").html(selectFileDefaultText);
                        return;
                    }
                    UploadFile(file);
                });
        $(document).on("paste", function(e) {
            var cd = e.clipboardData || e.originalEvent.clipboardData;
            for (var i = 0; i < cd.items.length; i++) {
                if (cd.items[i].type.indexOf("image") === 0) {
                    var blob = cd.items[i].getAsFile();
                    var d = new Date();
                    blob.name = "image " + (d.toLocaleDateString() + " " + d.toLocaleTimeString()).replace(/:/g, "-") + "."+blob.type.split("/")[1];
                    UploadFile(blob);
                    break;
                }
            }
        });

        function UploadComplete() {
            IsFileUploaded = true;
            $('#UploadProgressHeader').text('Приём файла завершён.');
            CheckIfSubmitNeeded();
        }

        var LastStatusSequence = 0;
        var progressChar;

        onLogin(function() {
            $('#LoginInvite').hide();
        });
        onLogout(function() {
            $('#LoginInvite').show();
        });

        function DisableDrop() {
            $(document).unbind("drop");
            $(document).unbind("paste");
            $(document)
                .on("drop",
                    function(e) {
                        e.stopPropagation();
                        e.preventDefault();
                    });

        }

        function FileSelected(e) {
            UploadFile(e.target.files[0]);
        }

        function ShowUploadPreview(file) {
            if (file.type.indexOf("image") !== 0) return;
            var $p = $("#uploadPreviewPanel");
            var r = new FileReader();
            r.onload = function(e) {
                $p.html("<img class=\"uploadThumb\" src=\"" + e.target.result + "\" />");
                $p.show();
            };
            r.readAsDataURL(file);
        }

        function UploadFile(file) {
            var isTor = false;
            if (isTor) {
                alert("Размещение файлов через Tor не разрешается.");
                return;
            }
            if (!useFileApi) {
                alert('К сожалению, ваш браузер не поддерживается. Установите современный браузер.');
                return;
            }
            DisableDrop();
            ShowUploadPreview(file);
            $('#SelectFilePanel').hide();
            $('#UploadProgressPanel').show();
            $('#UploadProgressMessage').text('Подготовка к приёму файла.');
            $('#UploadProgressMessage').show();
            setUnloadMessage();

            SelectedFileSize = file.size;
            if (SelectedFileSize > 5 * 1024 * 1024 * 1024)
                $('#DropDownListStorageTerm').val('10').change();
            var contentType = file.type !== "" ? file.type : "application/octet-stream";

            var partSize = 50000000;
            var partsCount = Math.ceil(file.size / partSize);
            var started = 0;
            var completed = 0;
            var nextPart = 0;
            var maxConcurrents = 1;
            var haveError = false;
            var uploadStartTime = new Date();
            var errorsCount = 0;

            var sendPart = function(partNum) {
                started++;
                var part = {
                    Offset: partNum * partSize,
                    Size: Math.min(partSize, file.size - partNum * partSize)
                };
                var xhr = new XMLHttpRequest();
                xhr.open('PUT', UploadHost +
                    '/api/v1/uploads/' + UploadSessionKey + '/data' +
                    '?SessionToken=' + $.cookie('session_token') +
                    '&Offset=' + part.Offset);
                xhr.setRequestHeader('X-Filename', encodeURIComponent(file.name));
                xhr.setRequestHeader('X-File-Size', encodeURIComponent(file.size));
                xhr.setRequestHeader("Content-Type", contentType);

                xhr.upload.onloadstart = function(e) {
                    if (part.Offset === 0)
                        $('#UploadProgressMessage').text($('#UploadProgressMessage').text() + '..');
                };

                var loaded = part.Offset;
                var lastProgressUpdated = new Date()
                xhr.upload.onprogress = function (e) {
                    var loaded = e.loaded + part.Offset;
                    if ((new Date() - lastProgressUpdated) > 350 || e.loaded == partSize || loaded == file.size) {
                        lastProgressUpdated = new Date()
                        var passedTime = new Date() - uploadStartTime;
                        var bps = passedTime !== 0 ? 1000 * loaded / passedTime : NaN;

                        $('#PBarFileName').text(file.name);
                        $('#PBarFileSize').text((file.size / 1024).toFixed(2));
                        $('#PBarReceived').text((loaded / 1024).toFixed(2));
                        $('#PBarSpeed').text((bps / 1024).toFixed(2));
                        $('#PBarProgress').text((loaded / file.size * 100).toFixed(2));
                        $('#UploadProgressBar').width((loaded / file.size * 100) + '%');

                        $('#UploadProgressData').show();

                        $('#UploadParametersForm').show();
                        if (isLoggedIn != true)
                            $('#LoginInvite').show();

                        $('#UploadProgressMessage').text('');
                        $('#UploadProgressMessage').hide();
                    }
                };

                xhr.onreadystatechange = function (e) {
                    if (e.target.readyState === 4) {
                        completed++;
                        if (e.target.status === 200) {
                            errorsCount = 0;
                            var response = JSON.parse(e.target.response);
                            if (response.status === 'Committed') {
                                StoredFileName = response.linkName;
                                UploadComplete();
                            }
                            else {
                                while (started - completed < maxConcurrents &&
                                    nextPart < partsCount &&
                                    !IsCanceled &&
                                    !haveError)
                                    sendPart(nextPart++);
                            }
                        } else {
                            errorsCount++;
                            if (!IsCanceled) {
                                if (errorsCount < 12)
                                    setTimeout(function() {
                                            sendPart(partNum);
                                        },
                                        15000);
                                else {
                                    haveError = true;
                                    alert("Произошла ошибка.");
                                }
                            }
                        }
                    }
                };
                xhr.send(file.slice(part.Offset, part.Offset + part.Size));
            };
            console.log('Staring upload ' + UploadSessionKey);
            sendPart(nextPart++);
        }

        function CheckIfSubmitNeeded() {
            //alert('DEBUG:CheckIfSubmitNeeded; IsFileUploaded=' + IsFileUploaded + '; IsChangesSaved=' + IsChangesSaved);
            if (IsFileUploaded && IsChangesSaved) {
                ga('send', 'event', 'Upload', 'UploadDone');
                resetUnloadMessage();
                location = '//' + location.host + '/' + StoredFileName;
            }
        }

        function ChangesBeginSave() {
            $('#SaveChangesButton').prop('disabled', true);
            $('#SaveChangesButton').addClass('disabled');
            $('#SaveChangesButton').removeClass('btn-success');
            $('#SaveChangesButton').addClass('btn-default');
        }

        function ChangesSaved() {
            IsChangesSaved = true;
            CheckIfSubmitNeeded();
        }

        function ChangesUnSaved() {
            IsChangesSaved = false;
            $('#SaveChangesButton').prop('disabled', false);
            $('#SaveChangesButton').removeClass('disabled');
            $('#SaveChangesButton').removeClass('btn-default');
            $('#SaveChangesButton').addClass('btn-success');
        }

        function SaveChanges() {
            if (!CheckRequirements())
                return;
            grecaptcha.execute();
        }

        function onCaptchaSubmit(token) {
            var recaptchaResponse = token

            ChangesBeginSave();

            sessionToken = $.cookie('session_token');
            var data = {
                AccessParams: $('#DropDownListAccessParams').val(),
                StorageTerm: $('#DropDownListStorageTerm').val(),
                FileDescription: $('#TextBoxFileDescription').val(),
                SessionToken: sessionToken,
                UploadPurpose: UploadPurpose,
                RecaptchaResponse: recaptchaResponse,
                ControlFieldlksdjjf209jlsgh01hks: true
            };

            $.ajax({
                type: "PATCH",
                url: UploadHost + '/api/v1/uploads/' + UploadSessionKey,
                data: JSON.stringify(data),
                contentType: 'application/json',
                complete: function(jqXHR, textStatus) {
                    //alert('DEBUG: save call complete. Status=' + textStatus);
                },
                success: function(data) {
                    ChangesSaved();
                },
                error: function(jqXHR, textStatus, errorThrown) {
                    alert('Ошибка сохранения параметров...');
                    ChangesUnSaved();
                }
            });
        }

        function CheckPublicLink() {
            if ($('#DropDownListAccessParams').val() == 'ForAll' && $('#TextBoxFileDescription').val().length < 4) {
                alert('Необходимо ввести описание, чтобы добавить файл в каталог.');
                return false;
            }
            return true;
        }

        function CheckTerm() {
            var termDays = $('#DropDownListStorageTerm').val();
            if (termDays === 'FirstDownload')
                termDays = 1;
            if (termDays === 'Infinite')
                termDays = 99999;
            if (SelectedFileSize > 5 * 1024 * 1024 * 1024 && termDays > 20) {
                alert('Файлы размером более 5 Гбайт могут быть сохранены на срок не более 20 дней.');
                return false;
            }
            if (termDays < 90) return true;
            if (SelectedFileSize > 1024 * 1024 * 1024 && termDays > 183) {
                alert('Файлы размером более 1 Гбайт могут быть сохранены на срок не более полугода.');
                return false;
            }
            if (SelectedFileSize > 400 * 1024 * 1024 && termDays > 366) {
                alert('Файлы размером более 400 Мбайт могут быть сохранены на срок не более 1 года.');
                return false;
            }
            if (SelectedFileSize > 100 * 1024 * 1024 && termDays > 731) {
                alert('Файлы размером более 100 Мбайт могут быть сохранены на срок не более 2 лет.');
                return false;
            }
            if (SelectedFileSize > 20 * 1024 * 1024 && SelectedFileSize && termDays > 1827) {
                alert('Файлы размером более 20 Мбайт могут быть сохранены на срок не более 5 лет.');
                return false;
            }
            if (termDays >= 731 && !isLoggedIn) {
                alert('Войдите, чтобы не потерять ссылку на файл за время его хранения. Для срока от 2 лет вход обязателен.');
                return false;
            }
            return true;
        }

        function CheckRequirements() {
            return CheckPublicLink() && CheckTerm();
        }

        function CancelUpload() {
            IsCanceled = true;
            $('#UploadProgressData').hide();
            $('#UploadParametersForm').hide();
            $('#UploadProgressHeader').text('Отмена...');
            $.ajax({
                type: "DELETE",
                url: UploadHost + '/api/v1/uploads/' + UploadSessionKey,
                dataType: "text",
                success: function() {
                    $('#UploadProgressHeader').text('Приём файла отменён.');
                    resetUnloadMessage();

                    location.reload();
                },
                error: function() {
                    alert('Ошибка...');
                }
            });
        }

        function onUnloadMessage() {
            return "В настоящее время идёт передача файла. Прервать?";
        }

        function setUnloadMessage() {
            window.onbeforeunload = onUnloadMessage;
        }

        function resetUnloadMessage() {
            window.onbeforeunload = null;
        }
    </script>
