        onError: function(e) {
            dataLayer.push({'event': 'interaccion','eAction':'Error','eLabel':'mensaje'});
        },
        onSuccess: function(e) {

			e.preventDefault();

			var nombre = $('#first_name').val();
			var apellido = $('#last_name').val();
			var email = $('#email').val();
			var telefono = $('#phone').val();
			var passw = $('#password').val();
			var pais = $('#user_country').val();
			var name_pais = $('#user_country option:selected').text().toLowerCase();

			//Grabación de usuario
			$.ajax({
        		dataType: 'json',
        		url: 'http://www.metodotrading.com/wp-content/themes/xtb/includes/create_user.php',
        		cache: false,
        		type: 'POST',
        		data: {
	        		nombre : nombre,
        			apellido : apellido,
        			email : email,
        			telefono : telefono,
        			password : passw,
        			pais: pais
        		}
        	}).done(function(response){

				var result = response.result;

				if (result == 'OK') {

					//Envío CRM
					
					if ( pais == 'ES' ) {
						var branch = 'PLES';
					}
					else if ( pais == 'CL' ) {
						var branch = 'BZCL';
					}
					else if ( pais == 'PE' ) {
						var branch = 'BZPE';
					} else {
						var branch = 'BZUY';
					}
					
					//UTMS
					function getCookie(cname) {
					    var name = cname + "=";
					    var ca = document.cookie.split(';');
					    for(var i=0; i<ca.length; i++) {
					        var c = ca[i];
					        while (c.charAt(0)==' ') c = c.substring(1);
					        if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
					    }
					    return '';
					}

					var cookieCRMUTM			=	getCookie('CRMUTM').split('|');
					var utm_source				=	cookieCRMUTM[0];
					var utm_medium				=	cookieCRMUTM[1];
					var utm_content				=	cookieCRMUTM[2];
					var utm_term				=	cookieCRMUTM[3];
					var utm_campaign			=	cookieCRMUTM[4];
					
					if ( utm_source == '' || utm_source == 'undefined' || utm_source == undefined ) {
						utm_source = 'registro_metodotrading_' + name_pais;
					}
					
					if ( utm_medium == '' || utm_medium == 'undefined' || utm_medium == undefined ) {
						utm_medium = 'sitioweb';
					}
					
					if ( utm_content == '' || utm_content == 'undefined' || utm_content == undefined ) {
						utm_content = 'metodotrading';
					}
					
					if ( utm_term == '' || utm_term == 'undefined' || utm_term == undefined ) {
						utm_term = pais;
					} 
					
					if ( utm_campaign == '' || utm_campaign == 'undefined' || utm_campaign == undefined ) {
						utm_campaign = 'registro';
					}
					
					if ( pais == 'ES' ) {
		        		var url = 'http://www.metodotrading.com/wp-content/themes/xtb/includes/envio_es.php';
	        		} else {
		        		var url = 'http://www.metodotrading.com/wp-content/themes/xtb/includes/envio_latam.php';
	        		}

					$.ajax({
		        		dataType: 'json',
		        		url: url,
		        		cache: false,
		        		type: 'POST',
		        		data: {
			        		nombre: nombre,
		        			apellido: apellido,
		        			email: email,
		        			telefono: telefono,
		        			pais: pais,
		        			name_pais: name_pais,
		        			origen: 'Registro',
		        			branch: branch,
		        			linkurl: linkurl,
		        			utm_source: utm_source,
		        			utm_medium: utm_medium,
		        			utm_content: utm_content,
		        			utm_term: utm_term,
		        			utm_campaign: utm_campaign
		        		}
		        	}).done(function(response){

						window.location = 'http://www.metodotrading.com/gracias?lead=registro&result=' + result;
						
					});


				} else {

					$('.signup-form').attr('action', 'http://www.metodotrading.com/gracias?lead=registro&result='+result);
					$('.signup-form').unbind('submit').submit();

				}

			});

		}

    });

