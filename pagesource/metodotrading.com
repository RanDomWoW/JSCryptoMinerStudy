	<script>
	
		$(document).ready(function() {
			
			geoUpdateHoras();
	
			var onSuccess = function(location){
	
				var geoCountry		=	location.country.iso_code;
	
				setCookie( 'mtcountry' , geoCountry , 7 );
	
			}
	
		});
	
		function setCookie( cname , cvalue , exdays) {
			var d = new Date();
			d.setTime( d.getTime() + ( exdays * 24 * 60 * 60 * 1000 ) );
			var expires = "expires=" + d.toUTCString();
			document.cookie = cname + "=" + cvalue + "; " + expires + ";domain=.metodotrading.com;path=/";
		}
	
		function geoUpdateHoras() {
			
			var horas = [];
	
			$('.geoloc').not('.loaded').each(function() {
				horas.push( $(this).html() );
			});
	
			if ( horas.length > 0  || 1 == 1 ) {
	
				var onSuccess = function(location){
					
					var geoCountry		=	location.country.iso_code;
					
					var timeZone		=	location.location.time_zone;
					
										var semTipo = 'page';
										
					// Si no es ninguno de los paises basicos, se utiliza la hora por IP.
					if(geoCountry != 'CL' && geoCountry != 'MX' && geoCountry != 'ES' && geoCountry != 'CO' && geoCountry != 'PE'){
						$.ajax({
							url : 'http://www.metodotrading.com/wp-admin/admin-ajax.php',
							type : 'POST',
							async : true,
							cache : false,
							dataType: 'json',
							data : { action : 'updateHoras', timezone: timeZone, horas: horas, tipo: semTipo },
						}).done(function( response ) {
							//console.log(response);
							//var json_response = jQuery.parseJSON( response );
							//console.log(json_response);
							$('.geoloc').not('.loaded').each(function(index) {
								$(this).html(response[index]).addClass('loaded');
							});
				
						});
					}
	
				};
	
				var onError = function(error){
	
					/*
					console.log(
						"Error:\n\n"
						+ JSON.stringify(error, undefined, 4)
					);
					*/
	
				};
	
				geoip2.city(onSuccess, onError);
	
			}
		}
	
	</script>
	

