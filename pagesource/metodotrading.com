    FormValidation.Validator.securePassword = {
        validate: function(validator, $field, options) {
            var value = $field.val();
            if (value === '') {
                return true;
            }

            // Check the password strength
            if (value.length < 6) {
                return {
                    valid: false,
                    message: 'La contraseña debe tener al menos 6 caracteres'
                };
