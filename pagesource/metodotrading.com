	$('#main').on('click', '.wp-pagenavi a', function(e) {

		e.preventDefault();

		var parentDiv = $(this).parent('div.wp-pagenavi').parent('div.text-center');
		var target = parentDiv.attr('data-pagination-target');
		var format = parentDiv.attr('data-pagination-format');
		var postType = parentDiv.attr('data-pagination-posttype');
		var filterKey = parentDiv.attr('data-pagination-filterkey');
		var filterValue = parentDiv.attr('data-pagination-filtervalue');
		var nivel_actual = parentDiv.attr('data-pagination-nivel_actual');
		var tematica = parentDiv.attr('data-pagination-tematica');

		var page = $(this).attr('data-pagination-page');

		if ( !page ) {
			page = $(this).text();
		}

		var action = 'updatePagination';

		$.ajax({
			url : 'http://www.metodotrading.com/wp-admin/admin-ajax.php',
			type : 'POST',
			async : true,
			cache : false,
			dataType: 'json',
			data : { action : action, nivel_actual : nivel_actual, tematica: tematica, format : format, posttype : postType, page : page, filterkey : filterKey, filtervalue : filterValue, target : target },
		}).done(function( response ) {

			if ( response.response == 'success' ) {

				$('#' + target).html( response.content );

				geoUpdateHoras();

			} else {

				alert( response.content );

			}

		});

	});

