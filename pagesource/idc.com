    <style>
        .idc-privacy-notice {
            width: 100%;
            min-height: 145px;
            position: fixed;
            opacity: 1;
            top: 0;
            left: 0;
            padding: 50px !important;
            text-align: center;
            background-color: #1b325f;
            background-color: rgba(27,50,95,0.8);
            z-index: 9999;
            color: #fff;
            -webkit-transition: all 0.2s ease;
            transition: all 0.2s ease;
        }

        .idc-privacy-notice a {
            color: #fff;
            border-bottom: 1px solid #fff;
        }

        .idc-privacy-notice a:hover,
        .idc-privacy-notice a:focus {
            color: #ffcd07;
            border-bottom-color: #ffcd07;
        }

        .idc-privacy-notice span.close-btn {
            line-height: 20px;
            font-size: 45px;
            height: 24px;
            top: 20px;
            right: 20px;
            overflow: hidden;
            font-weight: 800;
            cursor: pointer;
            position: absolute;
            display: inline-block;
        }

        .idc-privacy-notice span.close-btn:after {
            display: inline-block;
            content: '\00d7';
            color: #ccc;
            font-family: "Open Sans", "Trebuchet MS", monospace;
        }

        .idc-privacy-notice span.close-btn:hover:after {
            color: #fff;
        }

        .idc-privacy-notice.closed {
            opacity: 0;
            pointer-events: none;
        }

        .idc-privacy-notice button#notice-agree {
            outline: none;
            -webkit-appearance: none;
            -moz-appearance: none;
            padding: 9px 18px; color: #01010f;
            cursor: pointer;
            background-color: #FFCD07;
            margin-left: 30px;
            font-size: 16px;
            border: none;
            vertical-align: middle;
            display: inline-block;
            border-radius: 0;
            -webkit-transition: background-color 0.2s ease;
            transition: background-color 0.2s ease;
        }

        .idc-privacy-notice button#notice-agree:hover {
            background-color: #FEDE5B;
        }

        @media (max-width: 800px) {
            .idc-privacy-notice button#notice-agree {
                display: block;
                margin: 0 auto;
                margin-top: 20px;
            }
        }
    </style>

