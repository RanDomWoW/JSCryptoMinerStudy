<script type="text/javascript">
    cookies.set('customerID', cookies.get('beacon_id'));
    cookies.set('taxAmount', '');
    cookies.set('cart_discountamount', '');
    cookies.set('cart_couponcode', '');
    cookies.set('cart_lineitems', []);

    if (document.body.id == 'cart'){
        $(window).ready(function() {

            var cart_discountamount = document.querySelector('table.CartContents .coupon-discount .ProductPrice');
            cart_discountamount = cart_discountamount !== null ? cart_discountamount.innerHTML : 0;
            cart_discountamount = cart_discountamount !== 0 ? cart_discountamount.replace('$', '') : 0;
            cookies.set('cart_discountamount', cart_discountamount);

            var cart_subtotal = document.querySelector('table.CartContents .SubTotal.First .ProductPrice');
            cart_subtotal = cart_subtotal !== null ? cart_subtotal.innerHTML : 0;
            cart_subtotal = cart_subtotal !== 0 ? cart_subtotal.replace('$', '') : 0;

            function updateCouponCode(){
                var cart_couponcode = document.querySelector('table.CartContents .couponcode');
                cart_couponcode = cart_couponcode !== null ? cart_couponcode.innerHTML : '';
                cookies.set('cart_couponcode', cart_couponcode);            
            }

            updateCouponCode();
            $(document).on('click', '.Right .CouponCode form .btn.alt', function(event) { updateCouponCode(); });
            $(document).on('click', 'table.CartContents a[href*="cart.php?action=removecoupon"]', function(event) { updateCouponCode(); });

            var cart_lineitems = [];
            var cart_items = document.querySelectorAll('table.CartContents > tbody > tr');

            for (var i = 0; i < cart_items.length; i++) {
                var cart_item = cart_items[i];
                var lineItem = {
                    quantity: "",
                    unitPrice: "",
                    unitPriceLessTax: "",
                    SKU: "",
                    productName: ""
                };
                var productName = cart_item.querySelector('.ProductName a').innerText;
                var SKU = cart_item.querySelector('.ProductName .ProductSKU').innerText;
                var quantity = parseInt(cart_item.querySelector('.CartItemQuantity input').value);

                var unitPrice = cart_item.querySelector('.CartItemIndividualPrice');
                unitPrice = unitPrice !== null ? unitPrice.innerHTML : 0;
                unitPrice = unitPrice !== 0 ? unitPrice.replace('$', '') : 0;
                unitPrice = Math.round(unitPrice * 1e2) / 1e2;

                var discountAmount = Math.round(Math.abs(cookies.get('cart_discountamount')) * 1e2) / 1e2;
                var discountperc = discountAmount / cart_subtotal
                var unitPriceLessTax = unitPrice - (unitPrice * discountperc);

                lineItem.quantity = quantity;
                lineItem.unitPrice = unitPrice;
                lineItem.unitPriceLessTax = unitPriceLessTax;
                lineItem.productName = productName;
                lineItem.SKU = SKU;

                cart_lineitems.push(lineItem); 
            }
            cookies.set('cart_lineitems', cart_lineitems); 
            console.log('cookies.customerID:', cookies.get('customerID'));
            console.log('cookies.taxAmount:', cookies.get('taxAmount'));
            console.log('cookies.cartpage_products:', cookies.get('cart_lineitems'));
            console.log('cookies.cart_discountamount:', cookies.get('cart_discountamount'));
        });
    }
</script>
