    //Function to set JSON object values using ajax
    function setProductInfo() {
        if (window.location.href.indexOf("checkout/order-confirmation") == -1) {
            //console.log('Not orderConfirmation page');
            cookies.set('customerID', cookies.get('beacon_id'));
            cookies.set('taxAmount', '');
            cookies.set('discountAmount', '');
            cookies.set('discountCode', '');
            cookies.set('lineitems', []);

            var requestCart = new XMLHttpRequest();
            requestCart.open('GET', storeUrl+'/cart.php', false);
            requestCart.send();  
            //requestCart.onreadystatechange = function() {
            if (requestCart.status >= 200 && requestCart.status < 400) {
                var responseCartHTML = requestCart.response;
                var responseCartHTMLparser = new DOMParser();
                responseCartHTML = responseCartHTMLparser.parseFromString(responseCartHTML, "text/html");
                //console.log('responseCartHTML: ',responseCartHTML);

                var subtotal = responseCartHTML.querySelector('table.CartContents .SubTotal.subtotal-cart .ProductPrice');
                subtotal = subtotal !== null ? subtotal.innerHTML : 0
                subtotal = subtotal !== 0 ? subtotal.replace('$', '').replace(',','') : 0;
                cookies.set('subtotal', subtotal);

                var discountAmount = responseCartHTML.querySelector('table.CartContents .coupon-discount .ProductPrice');
                discountAmount = discountAmount !== null ? discountAmount.innerHTML : 0;
                discountAmount = discountAmount !== 0 ? discountAmount.replace('$', '').replace(',', '') : 0;
                cookies.set('discountAmount', discountAmount);

                var discountCode = responseCartHTML.querySelector('table.CartContents .couponcode');
                discountCode = discountCode !== null ? discountCode.innerHTML : '';
                cookies.set('discountCode', discountCode);

                var taxAmount = responseCartHTML.querySelector('table.CartContents .TaxCost .ProductPrice').innerHTML;
                //console.log('taxAmount: ',taxAmount);
                taxAmount = taxAmount !== ""  ? taxAmount : 0;
                taxAmount = taxAmount !== 0 ? taxAmount.replace('$', '') : 0;
                cookies.set('taxAmount', taxAmount);

                var cart_items = responseCartHTML.querySelectorAll('table.CartContents > tbody > tr');
                var requestProduct = [];
                var lineitems = [];
                //console.log('cart_items: ',cart_items);

                for (var i = 0; i <= cart_items.length; i++) { 
                    if (cart_items[i] !== undefined) {
                        var cart_item = cart_items[i];
                        var lineItem = [];
                        lineItem[i] = {};

                        var productName = cart_item.querySelector('.ProductName a').innerText;
                        var SKU = cart_item.querySelector('.ProductName .ProductSKU').innerText;
                        var quantity = parseInt(cart_item.querySelector('.CartItemQuantity input').value);
                       
                        var unitPrice = cart_item.querySelector('.CartItemIndividualPrice');
                        unitPrice = unitPrice !== null ? unitPrice.innerHTML : 0;
                        unitPrice = unitPrice !== 0 ? unitPrice.replace('$', '').replace(',', '') : 0;
                        unitPrice = Math.round(unitPrice * 1e2) / 1e2;
                                
                        var discountAmount = Math.round(Math.abs(cookies.get('discountAmount')) * 1e2) / 1e2;
                        //console.log('discountAmount: ',discountAmount);
                        //console.log('subtotal: ',subtotal);
                        var discountPerc = discountAmount / subtotal
                        discountPerc = Math.round(discountPerc * 1e2) / 1e2;
                        //console.log('discountPerc: ',discountPerc);
                        //console.log('unitPrice: ',unitPrice);
                        var unitPriceLessTax = unitPrice - (unitPrice * discountPerc);

                        lineItem[i].quantity = quantity !== "" ? quantity : 0;
                        lineItem[i].unitPrice = unitPrice;
                        lineItem[i].unitPriceLessTax = unitPriceLessTax;
                        lineItem[i].productName = productName !== "" ? productName : "";
                        lineItem[i].SKU = SKU !== "" ? SKU : "";

                        lineitems.push(lineItem[i]);
                        cookies.set('lineitems', lineitems);
                            
                        //console.log('lineitems: ',lineitems);
                        //console.log('lineitems cookie: ',cookies.get('lineitems'));
                    }
                }
            }
        }
        setRMTrans();
    }

    if (window.location.href.indexOf("cart.php") !== -1) {
      /* Cart page actions - START */
      //console.log('is cart page');
      setProductInfo();
      /* Cart page actions - END */
    }

    /* Checkout page actions - START */
    if (window.location.href.indexOf("/checkout") !== -1 && window.location.href.indexOf("checkout/order-confirmation") == -1) {
        console.log('checkout page');
        setTimeout(function() { setProductInfo(); }, 3000);

        document.addEventListener("click", function (e) {
          var elm = e.target;
          var parentElement1 = getParentBySelector(elm,'.cart-priceItem-link');
          var parentElement2 = getParentBySelector(elm,'.form-checklist-item');
          var parentElement3 = null;
          if(parentElement2 !== null){ parentElement3 = getParentBySelector(parentElement2,'.checkout-step--shipping'); }
          
          if (elm.id == 'applyRedeemableButton' || parentElement1 !== null || parentElement3 !== null) { 
              setTimeout(function() { setProductInfo(); }, 2000);
          }
        });    
    }
    /* Checkout page actions - END */

    /* Order confirmation page actions - START */
    if (window.location.href.indexOf("checkout/order-confirmation") !== -1) {
        console.log('orderConfirmation page');
        setTimeout(function() { setProductInfo(); }, 300); 
    }
    /* Order confirmation page actions - END */
</script>
