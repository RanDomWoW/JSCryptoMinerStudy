<script>
    if (document.body.id == 'product') {
        window.fbProductValue = document.querySelector('.ProductDetailsGrid .p-price .Value .VariationProductPrice');
        window.fbProductValue = window.fbProductValue !== null ? window.fbProductValue.innerText : 0;
        window.fbProductValue = window.fbProductValue !== 0 ? window.fbProductValue.replace('$', '') : 0;
        window.fbProductValue = Math.round(window.fbProductValue * 1e2) / 1e2;
        window.productId = new Array(document.querySelector('.productAddToCartRight form input[name="product_id"]').value);

        // Product
        fbq('track', 'ViewContent', {
            content_type: 'product',
            content_ids: window.productId,
            value: window.fbProductValue,
            currency: 'USD'
        });

        // AddToCart
        var AddCartButton = document.querySelector('.addto .AddCartButton');
        AddCartButton.addEventListener('click', 
            function() { 
                fbq('track', 'AddToCart', {
                content_type: 'product',
                content_ids: window.productId,
                value: window.fbProductValue,
                currency: 'USD' 
                });   
                console.log('window.fbProductValueOnClick: ',window.fbProductValue);       
                console.log('window.productIdOnClick: ',window.productId);
            },
            false
        );

        // WishList
        var AddWishListButton = document.querySelector('.AddToWishlistLink .wishTrigger');
        AddWishListButton.addEventListener('click', 
            function() { fbq('track', 'AddToWishlist'); }, false
        );
    }

    if (document.body.id == 'cart') {
        var cartTotal = '';
            cartTotal = cartTotal !== '' ? cartTotal : 0;
            cartTotal = cartTotal !== 0 ? cartTotal.replace('$', '') : 0;
            cookies.set('cartTotal', cartTotal);

        var fbcart_contents = [];
        var fbcart_items = document.querySelectorAll('table.CartContents > tbody > tr');

        for (var i = 0; i < fbcart_items.length; i++) {
            var cart_item = fbcart_items[i];
            var lineItem = {
                id: "",
                quantity: "",
                item_price: ""
            };
            var id = cart_item.querySelector('.ProductName').dataset.productid;
            var quantity = parseInt(cart_item.querySelector('.CartItemQuantity input').value);
            var item_price = cart_item.querySelector('.CartItemIndividualPrice');
            item_price = item_price !== null ? item_price.innerHTML : 0;
            item_price = item_price !== 0 ? item_price.replace('$', '') : 0;
            item_price = Math.round(item_price * 1e2) / 1e2;

            lineItem.id = id;
            lineItem.quantity = quantity;
            lineItem.item_price = item_price;

            fbcart_contents.push(lineItem); 
        }

        cookies.set('fbcart_contents', fbcart_contents);
        //console.log('fbcart_contents: ',fbcart_contents);

        // InitiateCheckout
        var checkoutButtons = document.querySelectorAll('.CheckoutButton .btn');
        for (var i = 0; i < checkoutButtons.length; i++) {
            var checkoutButton = checkoutButtons[i];
            checkoutButton.addEventListener('click', 
                function() { fbq('track', 'InitiateCheckout'); }, false
            );
        }
    }

    if ($('body').hasClass('search')) {

        function getUrlVars(){
            var vars = [], hash;
            var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
            for(var i = 0; i < hashes.length; i++)
            {
                hash = hashes[i].split('=');
                vars.push(hash[0]);
                vars[hash[0]] = hash[1];
            }
            return vars;
        }

        // Search
        var content_ids = [];
        var search_string = getUrlVars()["search_query"];
        var search_items = document.querySelectorAll('.ProductList.searchspring-items li');

        for (var i = 0; i < search_items.length; i++) {
            var search_item = search_items[i];
            var id = search_item.querySelector('.ProductImage.QuickView').dataset.product;
            content_ids.push(id); 
        }
        console.log('content_ids',content_ids);
        fbq('track', 'Search', { 
          search_string: search_string,
          content_ids: content_ids, // top 5-10 search results
          content_type: 'product'
        });
    }
</script>

