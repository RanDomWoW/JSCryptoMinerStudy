$('.header .PageMenu .sf-menu > li:first-child').after(miami_nav);
                $('.header .PageMenu .sf-menu').append(plballaccess);
                $(".header .PageMenu .sf-menu > li").each(function(){
                    var t = $(this);
                    t.addClass("menu-mega-nav");
                    var href = t.children("a").attr("href");
                    if (href.toLowerCase().indexOf("/accessories/") >= 0){
                        t.addClass("mega-nav-accessories");
                        t.attr("data-id","mega-nav-accessories");
                        t.children("ul").remove();
                    } else if (href.toLowerCase().indexOf("/dresses/") >= 0){
                        t.addClass("mega-nav-dresses");
                        t.attr("data-id","mega-nav-dresses");
                        t.children("ul").remove();
                    } else if (href.toLowerCase().indexOf("/clothing/") >= 0){
                        t.addClass("mega-nav-clothing");
                        t.attr("data-id","mega-nav-clothing");
                        t.children("ul").remove();
                    } else if (href.toLowerCase().indexOf("/shoes/") >= 0){
                        t.addClass("mega-nav-shoes");
                        t.attr("data-id","mega-nav-shoes");
                        t.children("ul").remove();
                    } else if (href.toLowerCase().indexOf("/monogrammed/") >= 0){
                        t.addClass("mega-nav-monogram");
                        t.attr("data-id","mega-nav-monogram");
                        t.children("ul").remove();
                    }
                    t.hover(function(){
                        var tc = $(this);
                        var menu_id = tc.attr("data-id");
                        $(".mega-nav-content").each(function(){
                            $(this).removeClass("show-mega-menu");
                        });
                        if(menu_id != null){
                            $("#"+menu_id).addClass("show-mega-menu");
                        }
                    });

                    $("body").on("mouseleave", ".mega-menu", function () {
                        $(".mega-nav-content").each(function(){
                            $(this).removeClass("show-mega-menu");
                        });
                    });
                });
            });
        </script>

