<script type="text/javascript">
    function SetVisibiltyCity(visible) {
        if (visible) {
            $("#ctl00_CityheadControl1_SelectCity").parents('div .City').fadeIn();
            console.log("city show");

        } else {
            $("#ctl00_CityheadControl1_SelectCity").parents('div .City').hide();
            console.log("city hide");
        }
    }
    function FillCity(provinceRouter) {
        var datapost = '{ provinceRouter: "' + provinceRouter + '"}';
        if (provinceRouter == undefined || provinceRouter === '') {
            return;
        }
        console.log(datapost);
        $.ajax({
            type: 'post',
            url: '/Code/JsonService.asmx/GetListAllCity',
            data: datapost,
            dataType: "json",
            contentType: "application/json; charset=utf-8"
        }).done(function (response) {
            console.log('GetListAllCity done');
            $("#ctl00_CityheadControl1_SelectCity").empty().html('<option value=\"0\">-- همه شهر ها --</option>' + response.d);
            SetVisibiltyCity(true);
        }).error(function (event, xhr, settings, error) { console.log('error in city: ' + xhr.status + "\n" + settings + "\n" + error); });
    }

    (function ($) {

        function setCityGa(cityName) {
            var isLogin = ('False');
            var isAdmin = 'false';
            if (isLogin === 'True') {
                isAdmin = ('False');
            }
            if (isAdmin !== 'True')
                ga('send', 'event', 'UI', 'CLICK', 'SelectCity-' +  cityName);
        };

        $(".select2ddl").select2({
            width: '100%',
            dir: "rtl",
            language: "fa"
        });

        $(document).ready(function() {
            var selectedProvince = $("#ctl00_CityheadControl1_hdnSelectedProvince").val();
            var selectedProvinceValue = '';
            if (selectedProvince !== '') {
                console.log('selectedProvince' + selectedProvince);
                selectedProvinceValue = JSON.parse(selectedProvince).Router.toLowerCase();
            }
            var selectedCityValue = '';
            var selectedCity = $("#ctl00_CityheadControl1_hdnSelectedCity").val();
            if (selectedCity !== '') {
                console.log(selectedCity);
                selectedCityValue = JSON.parse(selectedCity).Router.toLowerCase();
            }
            SetVisibiltyCity(selectedProvinceValue !== '');
            var provinceddl = $("#ctl00_CityheadControl1_SelectProvince");
            var cityddl = $("#ctl00_CityheadControl1_SelectCity");
            var currentrouter = $("#ctl00_CityheadControl1_hdnRouter").val();

            if (currentrouter != undefined) {
                // set province from url
                console.log(selectedProvinceValue);
                provinceddl.val(selectedProvinceValue).trigger('change');
                cityddl.val(selectedCityValue).trigger('change');
            }
            provinceddl.change(function() {
                setCityGa($(this).val());

                var selectedprovince = $(this).val();
                if (selectedprovince == undefined || selectedprovince === '') {
                    $("#ctl00_CityheadControl1_hdnSelectedProvince").val('');
                    $("#ctl00_CityheadControl1_hdnSelectedCity").val('');
                }
                $.ajax({
                    type: "Post",
                    url: "/Code/JsonService.asmx/SetCitySession",
                    data: "{value: '" + selectedprovince + "'}",
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    success: function(data) {
                        console.log('set Session : ' + data.d);
                        if (currentrouter == undefined || currentrouter === "") {
                            $(location).attr("href", "/");
                        }
                        if (selectedprovince != undefined) {
                            if (selectedprovince !== "") {
                                if (currentrouter != undefined && currentrouter !== "") {
                                    $(location).attr("href",
                                        "/list/" + currentrouter + "/city/" + selectedprovince + "/");
                                    return;
                                } else {
                                    $(location).attr("href", "/main/city/" + selectedprovince + "/");
                                }
                            } else {
                                if (currentrouter != undefined && currentrouter !== "") {
                                    // Redirect to all Cities
                                    $(location).attr("href", "/list/" + currentrouter + "/");
                                } else {
                                    SetVisibiltyCity(false);
                                }
                            }
                        }
                    },
                    error: function(event, xhr, settings, error) {
                        console.log('error in Set Session: ' + xhr.status + "\n" + settings + "\n" + error);
                    }
                });

            });

            cityddl.change(function() {
                var lastselectedProvince = $("#ctl00_CityheadControl1_hdnSelectedProvince").val();
                var lastselectedProvinceValue = '';
                if (lastselectedProvince !== '') {
                    lastselectedProvinceValue = JSON.parse(lastselectedProvince).Router.toLowerCase();
                }
                setCityGa($(this).val());
                var selectedcity = $(this).val();
                if (selectedcity == undefined || selectedcity === '') {
                    $("#ctl00_CityheadControl1_hdnSelectedCity").val('');
                }
                var dataJson = "{value: '" + selectedcity + "'}";
                console.log(dataJson);
                if (selectedcity == undefined || selectedcity === '') {
                    dataJson = "{value: '" + lastselectedProvinceValue + "'}";
                }
                $.ajax({
                    type: "Post",
                    url: "/Code/JsonService.asmx/SetCitySession",
                    data: dataJson,
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    success: function(data) {
                        console.log('set Session : ' + data.d);
                        if (currentrouter == undefined || currentrouter === "") {
                            $(location).attr("href", "/");
                        }
                        if (selectedcity != undefined) {
                            if (selectedcity !== "") {
                                if (currentrouter != undefined && currentrouter !== "") {
                                    $(location).attr("href", "/list/" + currentrouter + "/city/" + selectedcity + "/");
                                    return;
                                } else {
                                    $(location).attr("href", "/main/city/" + selectedcity + "/");
                                }
                            } else {
                                if (currentrouter != undefined && currentrouter !== "") {
                                    // Redirect to all Cities
                                    if (selectedProvinceValue !== '') {
                                        $(location).attr("href",
                                            "/list/" + currentrouter + "/city/" + lastselectedProvinceValue + "/");
                                    } else {
                                        $(location).attr("href", "/list/" + currentrouter + "/");
                                    }
                                }
                            }
                        }
                    },
                    error: function(event, xhr, settings, error) {
                        console.log('error in Set Session: ' + xhr.status + "\n" + settings + "\n" + error);
                    }
                });
            });
        });
    })(jQuery);
</script>

