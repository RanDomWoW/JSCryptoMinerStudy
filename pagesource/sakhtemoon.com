<script type="text/javascript">
    (function ($) {
        $(document)
            .ready(function () {
                var islogin = $("#ctl00_Notificationbar_hdnIsLogin").val();
                if (islogin === '1') {
                    refreshNotification();
                    $('.bell-top')
                        .click(function () {
                            if ($(this).hasClass('active')) {
                                $(this).removeClass('active');
                                $('.beldrop').fadeOut(1000, function () { refreshNotification(); });

                            } else {
                                var notificationvalue = $("#ctl00_Notificationbar_hdnTotalNotificationCount").val();
                                if (parseInt(notificationvalue) > 4) { $("#btnMoreNotification").show(); } else { $("#btnMoreNotification").hide(); }
                                $(this).addClass('active');
                                $('.beldrop').fadeIn();
                                setNotificationStatus();
                            }
                        });

                    $('html')
                        .click(function () {
                            $('.beldrop').fadeOut();
                            $('.bell-top').removeClass('active');
                            refreshNotification();
                        });

                    $("[id*=bell]").click(function (event) { event.stopPropagation(); });
                }
            });
    })(jQuery);

    function setNotificationStatus() {
        var ids = $("#ctl00_Notificationbar_hdnNotificationIDs").val();
        console.log("ids:" + ids);
        var datapost = JSON.stringify({ IDs: ids, status: 1 });
        $.ajax({
            type: "POST",
            url: "/Code/JsonService.asmx/SetUserNotificarionStatus",
            data: datapost,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (respond) {
            },
            error: function (x, y, z) {
                console.log(x);
                console.log(y);
                console.log(z);
            }
        });

    }
    function refreshNotification() {
        $.ajax({
            type: "GET",
            url: "/Code/JsonService.asmx/GetUserNotificarions",
            contentType: "application/json; charset=utf-8",
            success: function (respond) {
                var notifications = respond.d;
                if (notifications == undefined || notifications.TotalCount == undefined || notifications.Ids === undefined) {
                    return;
                }
                var totalCount = notifications.TotalCount;
                var totalnotificationCount = notifications.TotalNotificationCount;
                var notificationCount = notifications.NotificationCount;
                var userNotifications = notifications.Notifications;
                var userGroupNotifications = notifications.GroupNotifications;
                if (userGroupNotifications == undefined) {
                    return;
                }
                var ids = notifications.Ids;
                $("#ctl00_Notificationbar_lblNotificationItemCount").text(totalCount);
                $("#ctl00_Notificationbar_lblItemsCount").text(notificationCount);
                $("#ctl00_Notificationbar_hdnNotificationIDs").val(ids);
                $("#ctl00_Notificationbar_hdnTotalNotificationCount").val(totalnotificationCount);
                var groupnotifhtml = '';
                for (var j = 0; j < userGroupNotifications.length; j++) {
                    groupnotifhtml +=
                        "<div class='specialt blue'>" +
                        "<a href='" + userGroupNotifications[j].Link + "'><span class='spe-title'>" + userGroupNotifications[j].groupTitle + "</span>" +
                        "<span class='bell-text'>شما " + userGroupNotifications[j].CountItem + " پیام خصوصی  خوانده نشده دارید.</span>" +
                        "<span class='nnum'>" + userGroupNotifications[j].CountItem + "</span>" +
                        "</a>" +
                        "</div>";
                }
                var notifhtml = '';
                for (var j = 0; j < userNotifications.length; j++) {
                    var st = "";
                    if (userNotifications[j].IsObserved) { st = "style = 'background-color: #d4d4d4;'"; }
                    notifhtml +=
                        "<li " + st + ">" + "<a href='" + userNotifications[j].Link + "'>" +
                        "<span class='" + userNotifications[j].ClassName + "'>" + userNotifications[j].Title + "</span>" +
                        "<span class='bell-text'>" + userNotifications[j].Description + "</span>" +
                        "</a>" +
                        "</li>\n";
                }
                $("#groupnotificationlist").html(groupnotifhtml);
                $("#notificationlist").html(notifhtml);
                //console.log($("#notificationlist").html());
                //console.log($("#notificationlist").html());
            },
            error: function (x, y, z) {
                console.log(x);
                console.log(y);
                console.log(z);
            }
        });
    }
</script>
