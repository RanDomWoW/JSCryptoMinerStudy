                        ShowDialog('عضویت در خبرنامه', 'لطفا آدرس ایمیل صحیح وارد نمایید', 'red');
                        return false;
                    }
                    $.ajax({
                        type: 'post',
                        url: "/Code/JsonService.asmx/RegisterEmailNewsSunscribe",
                        data: JSON.stringify({ email: emailAddress }),
                        dataType: "json",
                        contentType: "application/json; charset=utf-8"
                    }).done(function (response) {
                        console.log('response : ' + JSON.stringify(response));
                        ShowDialog('عضویت در خبرنامه', response.d, 'green');
                    }).error(function (event, xhr, settings, error) {
                        ShowDialog('عضویت در خبرنامه', 'عملیات با شکست مواجه شد.\nلطفا با مدیریت سایت تماس بگیرید', 'red');
                        console.log('error : ' + xhr.status + "\n" + settings + "\n" + error);
                    });
                    e.preventDefault();
                    return false;
                });
                //$('.cityhead .citytitle').click(function () {
                //    $('.cityhead ul').fadeIn();
                //});
                //$('.cityhead').mouseleave(function () {
                //    $('.cityhead ul').fadeOut();
                //});

                $("#ctl00_btnGlobalSearch").click(function () {
                    var value = $.trim($('#ctl00_txtGlobalSearch').val());
                    ga('send', 'event', 'UI', 'CLICK', 'Search : ' + value);
                    if (value === "") { return false; }
                    else if (value.length > 40) { return false; }
                    else {
                        var searchquery = CorrectForSearch(value);
                        //$(location).attr("href", "/search?query=" + searchquery);
                        var routeLink = "all";
                        try {
                            $.ajax({
                                type: 'Post',
                                url: '/Code/JsonService.asmx/GetCitySession',
                                data: "{}",
                                dataType: "json",
                                contentType: "application/json; charset=utf-8",
                                cache: false
                            }).done(function (response) {
                                routeLink = response.d;
                                if (routeLink != undefined && routeLink != "all" && routeLink != "") {
                                    $.ajax({
                                        type: 'Post',
                                        url: '/Code/JsonService.asmx/CheckIsProvinceByRouter',
                                        data: "{Value: '" + routeLink.trim() + "'}",
                                        dataType: "json",
                                        contentType: "application/json; charset=utf-8",
                                        cache: false
                                    }).done(function (result) {
                                        if (result.d === 'yes') {
                                            $(location).attr("href", baseUrl + "search?query=" + searchquery + "&province=" + routeLink);
                                        } else if (result.d === 'no') {
                                            $(location).attr("href", baseUrl + "search?query=" + searchquery + "&city=" + routeLink);
                                        }
                                    }).error(function (event, xhr, settings, error) {
                                        console.log('error : ' + xhr.status + "\n" + settings + "\n" + error);
                                    });
                                } else {
                                    $(location).attr("href", baseUrl + "search?query=" + searchquery);
                                }
                            }).error(function (event, xhr, settings, error) {
                                console.log("error catch in search:" + searchquery);
                                console.log('error : ' + xhr.status + "\n" + settings + "\n" + error);
                            });
                        } catch (e) {
                            console.log("error catch in search:" + searchquery);
                            $(location).attr("href", "/search?query=" + searchquery);
                        }
                        return false;
                    }
                });
            });
        })(jQuery);
    </script>

