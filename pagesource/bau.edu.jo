        // setInterval(NextScroll, 5000);
    
    var theDivs;
    var theDivsLength = 0;
    var currentSlide = 0;
    var nextSlide = 0;
    var startFadeSpeed = 1000;
    var fadeSpeed = 5000;
    var delayTime = 10000;
    var IntervalId;

    function generateFadeScrolls() {

        theDivs = $('div.slide');
        theDivsLength = theDivs.length;
        currentSlide = 0;
        nextSlide = 1;
        //alert('welcome');
        //var td = $('#bauhomenewsscrolltd');
        
        //alert('theDivsLength=' + theDivsLength);

        $(theDivs).each(
            function (ind,val) {
                $(this).css("z-index", "0");
                $(this).css("opacity", "1");
                $(this).css("display", "none");
            });

        $('#div' + currentSlide).fadeIn(startFadeSpeed);

        IntervalId = setInterval(NextFadeScroll, delayTime);
    }

    function NextFadeScroll() {
        $('#div' + currentSlide).fadeOut(fadeSpeed);
        $('#div' + nextSlide).fadeIn(fadeSpeed);
        
        if (currentSlide == (theDivsLength - 1)) {
            currentSlide = 0;
        }
        else {
            currentSlide++;
        }

        if (nextSlide == (theDivsLength - 1))
        {
            nextSlide = 0;
        }
        else
        {
            nextSlide++;
        }
        
    }

    function NextBtn() {
        clearInterval(IntervalId);
        NextFadeScroll();
        IntervalId = setInterval(NextFadeScroll, delayTime);
    }

    function PrevBtn() {
        clearInterval(IntervalId);
        if (currentSlide == 0) {
            currentSlide = theDivsLength - 1;
        }
        else {
            currentSlide--;
        }

        if (nextSlide == 0) {
            nextSlide = theDivsLength - 1;
        }
        else {
            nextSlide--;
        }

        $('#div' + nextSlide).fadeOut(fadeSpeed);
        $('#div' + currentSlide).fadeIn(fadeSpeed);

        IntervalId = setInterval(NextFadeScroll, delayTime);
    }
  
</script>
                
