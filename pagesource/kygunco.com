        function populateFacet(facet, field, id, allValue) {
            //alert(allValue);
            if (allValue != 'Is Firearm') {
                var htmlStr = '';
                if (facet.Buckets.length == 1 && facet.Buckets[0].value == 'Unknown')
                    return htmlStr;
                //populate brand facet
                if (field != null && field != '') {
                    htmlStr += '<div class="finder-select-holder">';
                    htmlStr += '<select id="' + id + '" class="finder-select">';
                    htmlStr += '<option value="">' + allValue + '</option>';
                    htmlStr += '<option selected value="' + field.replace(/\"/g, "&quot;") + '">' + field.replace(/\"/g, "&quot;") + '</option>';
                    htmlStr += '</select>';
                    htmlStr += '</div>';
                }
                else {
                    htmlStr += '<div class="finder-select-holder">';
                    htmlStr += '<select id="' + id + '" class="finder-select">';
                    htmlStr += '<option value="">' + allValue + '</option>';
                    for (var i = 0; i < facet.Buckets.length; ++i) {
                        var bucket = facet.Buckets[i];
                        htmlStr += '<option value="' + bucket.value.replace(/\"/g, "&quot;") + '">' + bucket.value.replace(/\"/g, "&quot;") + '</option>';
                    }
                    htmlStr += '</select>';
                    htmlStr += '</div>';
                }
                return htmlStr;
            }

        }

        function populateSearchLink() {
            /*var htmlStr = htmlStr += '<a href ="/Search/AjaxSearch?Keyword=&Start=0&mAction=&Barrellength=&Brand=SIG%20SAUER&Caliber=&Category=&Color=&Department=&Finish=&Rating=&Type=&MinPrice=0&MaxPrice=0">Display Items</a>';*/
            var brand = typeof ($('#selbrands').val()) === 'undefined' ? '' : $('#selbrands').val().replace(/\"/g, "&quot;");
            var caliber = typeof ($('#selcaliber').val()) === 'undefined' ? '' : $('#selcaliber').val().replace(/\"/g, "&quot;");
            var department = typeof ($('#seldepartment').val()) === 'undefined' ? '' : $('#seldepartment').val().replace(/\"/g, "&quot;");
            var type = typeof ($('#seltype').val()) === 'undefined' ? '' : $('#seltype').val().replace(/\"/g, "&quot;");
            var finish = typeof ($('#selfinish').val()) === 'undefined' ? '' : $('#selfinish').val().replace(/\"/g, "&quot;");
            var barrel = typeof ($('#selbarrel').val()) === 'undefined' ? '' : $('#selbarrel').val().replace(/\"/g, "&quot;");
            var action = typeof ($('#selaction').val()) === 'undefined' ? '' : $('#selaction').val().replace(/\"/g, "&quot;");
            var category = typeof ($('#selcategory').val()) === 'undefined' ? '' : $('#selcategory').val().replace(/\"/g, "&quot;");
            var color = typeof ($('#selcolor').val()) === 'undefined' ? '' : $('#selcolor').val().replace(/\"/g, "&quot;");
            var firearm = typeof ($('#selfirearm').val()) === 'undefined' ? '' : $('#selfirearm').val().replace(/\"/g, "&quot;");

            var htmlStr = '<a class="button big" href="/Search/AjaxSearch?Keyword=&Start=0';
            htmlStr += '&Brand=' + brand;
            htmlStr += '&Caliber=' + caliber;
            htmlStr += '&Department=' + department;
            htmlStr += '&Type=' + type;
            htmlStr += '&Finish=' + finish;
            htmlStr += '&BarrelLength=' + barrel;
            htmlStr += '&mAction=' + action;
            htmlStr += '&Category=' + category;
            htmlStr += '&Color=' + color;
            htmlStr += '&IsFirearm=true';
            htmlStr += '&Rating=&Type=&MinPrice=0&MaxPrice=0">Display Items</a>';
            return htmlStr;
        }

        function populateResetLink() {
            /*var htmlStr = htmlStr += '<a href ="/Search/AjaxSearch?Keyword=&Start=0&mAction=&Barrellength=&Brand=SIG%20SAUER&Caliber=&Category=&Color=&Department=&Finish=&Rating=&Type=&MinPrice=0&MaxPrice=0">Display Items</a>';*/

            var htmlStr = '<div class="button big" id="search-reset">start over</div>';
            return htmlStr;
        }

        function populateFinder(data) {
            var container = $("#firearm-finder-container");
            var htmlStr = '<div id="firearmsFinder-Content">';


            htmlStr += populateFacet(data.brandFacet, data.queryForm.Brand, 'selbrands', 'All Brands');
            htmlStr += populateFacet(data.caliberFacet, data.queryForm.Caliber, 'selcaliber', 'All Calibers');
            htmlStr += populateFacet(data.departmentFacet, data.queryForm.Department, 'seldepartment', 'All Departments');
            htmlStr += populateFacet(data.typeFacet, data.queryForm.Type, 'seltype', 'All Types');
            htmlStr += populateFacet(data.finishFacet, data.queryForm.Finish, 'selfinish', 'All Finishes');
            htmlStr += populateFacet(data.barrelFacet, data.queryForm.Barrellength, 'selbarrel', 'All Barrel Lengths');
            htmlStr += populateFacet(data.actionFacet, data.queryForm.mAction, 'selaction', 'All Actions');
            htmlStr += populateFacet(data.categoryFacet, data.queryForm.Category, 'selcategory', 'All Categories');
            htmlStr += populateFacet(data.colorFacet, data.queryForm.Color, 'selcolor', 'All Colors');
            //htmlStr += populateFacet(data.isfirearmFacet, data.queryForm.IsFirearm, 'selfirearm', 'Is Firearm');
            htmlStr += '<div class="match-items"><span id="matching-items">' + data.queryForm.SearchResults.hits.found + '</span> : <span id="ctl00__384e4fc_matchItemLit" class="ffMatchItem">Matching Items</span></div>'
            //htmlStr += '<a href ="/Search/AjaxSearch?Keyword=&Start=0&mAction=&Barrellength=&Brand=SIG%20SAUER&Caliber=&Category=&Color=&Department=&Finish=&Rating=&Type=&MinPrice=0&MaxPrice=0">Display Items</a>';

            htmlStr += '<div id="ffSubmitHolder" class="finder-spacing">';
            htmlStr += populateSearchLink();
            htmlStr += populateResetLink();
            htmlStr += '</div>';
            htmlStr += '</div>';
            container.html(htmlStr);
        }





