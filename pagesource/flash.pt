                            $("body").addClass("bannerApp");
                        }

                        if ($(this).scrollTop() > 160) {
                            $("body").css("position", "relative");
                        } else {
                            $("body").css("position", "inherit");
                        }
                    }
                });

                $(".smartbanner-close").click(function () {
                    $("body").removeClass("bannerApp");
                });



            }

        }

        return {
            run: init(),
            init: init
        }

    })();
