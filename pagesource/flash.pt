                        .build();

                    COF.pubApi.configMastheadCheckSizes(_mastheadCheckSizes);
                });


            var kwpag = [];
            kwpag.push('homepage');
                    kwpag.push('Famosos');
                    kwpag.push('Estrelas');
                    kwpag.push('Vida');
                    kwpag.push('lifestyle');
                    kwpag.push('moda');
                    kwpag.push('beleza');
                    kwpag.push('celebridades');
                    kwpag.push('mundo');
                    kwpag.push('festas');
                    kwpag.push('paparazzi');


            function adsRenderEnded (adsEvent) {

                var slotElementID = adsEvent.slot.getSlotElementId();

                if (!adsEvent.isEmpty && slotElementID != 'OutOfPage') {
                    if (slotElementID.indexOf('BTFhorz') > -1 || slotElementID.indexOf('horzATF') > -1) {
                        if ($('#' + slotElementID).siblings('span.pubTxt').length == 0)
                            $('#' + slotElementID).after('<span class="pubTxt">pub</span>');
                    }
                    else if (slotElementID.indexOf('BTFvert') > -1 || slotElementID.indexOf('vertATF') > -1) {
                        if ($('#' + slotElementID).parent().siblings('span.pubTxt').length == 0)
                            $('#' + slotElementID).parent().after('<span class="pubTxt">pub</span>');
                    }
                    if (slotElementID.indexOf('horzATF') > -1) {
                        if ($('#horzATF').length > 0 && $('#horzATF').is(":visible")) {
                            $('body').addClass('mastheadVisible');
                        }
                    }
                }

                //validacao para carregar o script do optinmonster
                if ((adsEvent.isEmpty && slotElementID == 'OutOfPage') || (adsEvent.isEmpty && slotElementID.indexOf('horzATF') > -1)) {
                    indexOptin++;
                    if (indexOptin == 2 && loadOptinMonster() !== undefined) {
                        loadOptinMonster();
                    }
                }
            }

            var indexOptin = 0;
            googletag.cmd.push(function () {
                googletag.pubads()
                    .setTargeting('end', [window.location.href.match(/([^\?]*)\/*$/)[0]])
                    .setTargeting('kw', kwpag);

                googletag.pubads().addEventListener('slotRenderEnded', function (event) {
                    adsRenderEnded(event);
                });

                googletag.pubads().enableSingleRequest();
                googletag.pubads().enableAsyncRendering();
                googletag.pubads().collapseEmptyDivs(true, true);
                googletag.pubads().setCentering(true);
                googletag.companionAds().setRefreshUnfilledSlots(true);
                googletag.pubads().disableInitialLoad();

                googletag.enableServices();

                COF.pubApi.renderOOP($('#div-gpt-ad-1479751477335-0'));
                COF.pubApi.renderMasthead($('#div-gpt-ad-1479750981375-0'), '', 'horzATF', 'ATF', false);

                    
                        COF.pubApi.renderMrec($('#div-gpt-ad-1491234378633-0'), '', 'vertATF', 'ATF', false);
                    

                googletag.pubads().refresh();
                });
        });
</script>




