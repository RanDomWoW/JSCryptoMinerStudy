            //By Tags
            //url: "https://api.instagram.com/v1/tags/coffee/media/recent?client_id=[]&access_token=[]",
            //To get a user’s detail
            //url: "https://api.instagram.com/v1/users/usert_id/?access_token=youraccesstoken",
            dataType: "jsonp",
            cache: false,
            beforeSend: function () {
                $("#loading").show();
            },
            success: function (data) {
                $("#loading").hide();
                if (data == "") {
                    $("#PhotosDiv").hide();
