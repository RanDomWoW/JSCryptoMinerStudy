<script>
class Typewriter {
randomWait(x) {
this.multiplier = (Math.random() + 0.53) * this.multiplier;
if(this.multiplier > 1.2 || this.multiplier < 0.8) {
this.multiplier = 1.0;
}
var random = (this.multiplier + 0.1) * x;
setTimeout(this.type.bind(this), random);
}
type() {
// Go backwards until all deleted
if(this.j === 0) {
var typed = jQuery(this.typed).html();
if(typed.length > 0) {
// Update sentence by removing last character
var output = typed.substring(0, typed.length - 1);
jQuery(this.typed).html(output);
if(typed.length === 1) {
// wait around 1/2 second after sentence deletion
this.randomWait(500);
} else {
// wait around 0.045 seconds between deletes
this.randomWait(45);
}
return;
}
}
// If at end of sentence then reset for next sentence
if(++this.j > this.strings[this.i].length){
this.j = 0;
++this.i;	
if(this.i >= this.strings.length){
this.i = 0;
}
// wait around 2 seconds after completing a sentence
this.randomWait(2000);
return;
}
// Update sentence with new additional character
var element = this.strings[this.i];
var output = element.substring(0, this.j);
jQuery(this.typed).html(output);
// wait around 0.095 seconds after adding a character
this.randomWait(95);
}
blinkCursor() {
this.hidden = !this.hidden;
if(this.hidden === true) {
jQuery(this.curs).fadeTo(200, 0.0);
} else {
jQuery(this.curs).fadeTo(200, 1.0);
}
setTimeout(this.blinkCursor.bind(this), 450);
}
constructor(strings, typed, curs) {
this.strings = strings;
this.typed = typed;
this.curs = curs;
this.multiplier = 1.0;
this.i = 0;
this.j = 0;
this.hidden = true;
setTimeout(this.type.bind(this), 2000);
this.blinkCursor();
}
}
const typewriter = new Typewriter(["Printers make print work", "InDesign users convert to QuarkXPress", "QuarkXPress users convert to InDesign", "Designers convert PDF files", "make Prepress easy"], "#typed", "#curs");
