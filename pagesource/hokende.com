$(function() {
    var target = $(".weekly_ranking");
    target.find("[data-content]").hide();
    target.find("[data-category]").on("click", function() {
        var id = $(this).data('category');
        var content = target.find("[data-content=" + id + "]");
        var loading = "nowloading";
        $(this).closest('.weekly_ranking_box')
            .removeClass()
            .addClass('weekly_ranking_box')
            .addClass($(this).data('slug'));
        target.find("[data-content]").hide();
        content.show();

        if (content.children().first().hasClass(loading)) {
            $.ajax({
                url: $(this).data('api') + "?categoryId=" + id,
                type: 'get',
                contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                dataType: 'json',
                success: function(data) {
                    content.find("." + loading).remove();
                    var ol = $('<ol class="item" />');
                    $.each(data.products, function(index, product) {
                        var li = $("<li />");
                        var onlineButton = '';
                        if (product.onlineUrl) {
                            onlineButton = '<p class="request_button"><a href="' + product.onlineUrl + '" class="request-net" target="_blank">今すぐ見積り・申込み</a></p>';
                        }
                        li.html('<a href="'
                            + product.url
                            + '" target="_blank"><p class="'
                            + product.class
                            + '"><span'
                            + ' class="item_span">'
                            + product.name
                            + '</span>'
                            + product.company
                            + '</p></a>'
                            + onlineButton);
                        ol.append(li);
                    });
                    if (data.promotion.length !== 0) {
                        var promotion = data.promotion;
                        var li = $("<li />");
                        li.html('<a href="javascript:void(0)" class="modal_pr-open md-btn-area' + promotion.type + '"></a><a href="'
                            + promotion.url
                            + '" target="_blank"><p class="ranking_pr"><span class="item_span">'
                            + promotion.name
                            + '</span>'
                            + promotion.company
                            + '</p></a>');
                        ol.append(li);
                    }
                    content.append(ol);
                    var a = $('<a class="all_button" />');
                    a.attr("href", data.cart);
                    a.append('<span>まとめて資料請求する</span>');
                    content.append(a);
                    var p = $('<p class="date" />');
                    p.text("保険市場 資料請求件数調べ　" + data.term);
                    content.append(p);
                }
            });
        }
    });
    target.find("[data-category]").first().trigger("click");
});</script>
