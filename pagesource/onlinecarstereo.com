    <script type="text/javascript">
        var url = 'http://whatfitsmycar.onlinecarstereo.com/api/FitGuide/';
        

        var year, make, model, submodel, body;
        var selYear = $("select#year");
        var selMake = $("select#make");
        var selModel = $("select#model");
        var selSubmodel = $("select#submodel");
        var selBody = $("select#body");

        var btnSubmit = $("button#submit");
        var cfgForm = $('#cfg');
        var cfg = $("#divCarFitGuide");
        var trail = $("#divTrail");

        $(document).ready(function () {

            //1. year --> populate makes
            selYear.change(function () {
                year = $(this).val();

                $.ajax({
                    url: url + 'GetVehicleMakes/' + year,
                    type: 'GET',
                    dataType: 'json',
                    success: function (data) {
                        var makes = $.parseJSON(data);

                        selMake.removeAttr("disabled").find("option").remove().end();
                        selMake.append('<option value="">Select Make</option>');
                        $.each(makes, function (k, v) {
                            //selYear.append($("<option></option>").attr("value", k).text(v));
                            selMake.append($("<option></option>").attr("value", k).text(v));
                        });
                    },
                    error: function (xhr, status, error) {
                        var err = eval("(" + xhr.responseText + ")");
                        alert(err.Message);
                    },
                    beforeSend: setHeader
                });
            });

            
            function setHeader(xhr) {
                xhr.setRequestHeader('auth-token', '06627b5c-87b8-441f-a7e2-c58d923cce66');
            }

            //2. make --> populate models
            selMake.change(function () {
                //$("#divUnavailable").show();

                //$.support.cors = true; //hack for IE8 CORS support

                make = $(this).val();

                $.ajax({
                    url: url + 'GetVehicleModels/' + year + "/" + make,
                    type: 'GET',
                    dataType: 'json',
                    success: function (data) {
                        var models = $.parseJSON(data);
                        
                        selModel.removeAttr("disabled").find("option").remove().end();
                        selModel.append('<option value="">Select Model</option>');                       
                        $.each(models, function (k, v) {
                            selModel.append($("<option></option>").attr("value", v).text(k));
                        });
                    },
                    error: function () { alert('boo!'); },
                    beforeSend: setHeader
                });

            });

            //3. model --> populate sub models
            selModel.change(function () {
                model = $(this).val();

                $.ajax({
                    url: url + 'GetVehicleSubModels/' + year + "/" + model,
                    type: 'GET',
                    dataType: 'json',
                    success: function (data) {
                            var vehiclebody = $.parseJSON(data);

                            selSubmodel.removeAttr("disabled").find("option").remove().end();
                            selSubmodel.append('<option value="">Select Vehicle Sub-model</option>');
                            $.each(vehiclebody, function (k, v) {
                                selSubmodel.append($("<option></option>").attr("value", k).text(v));
                            });
                    },
                    error: function () { alert('boo!'); },
                    beforeSend: setHeader
                });
            });

            //4. sub model --> populate vehicle body
            selSubmodel.change(function () {
                submodel = $(this).val();

                $.ajax({
                    url: url + 'GetVehicleBodies/' + submodel,
                    type: 'GET',
                    dataType: 'json',
                    success: function (data) {
                            var vehicletrim = $.parseJSON(data);

                            selBody.removeAttr("disabled").find("option").remove().end();
                            selBody.append('<option value="">Select Vehicle Body</option>');
                            $.each(vehicletrim, function (k, v) {
                                selBody.append($("<option></option>").attr("value", k).text(v));
                            });

                            btnSubmit.removeAttr("disabled");
                    },
                    error: function () { alert('boo!'); },
                    beforeSend: setHeader
                });
            });

            selBody.change(function () {
                body = $(this).val();

                runFitguide();
            });

        });

        $("#submit").on("click", function (e) {
            runFitguide();
        });

        function runFitguide() {
            // e.preventDefault();

            // refresh variables
            year = $('select#year').val();
            make = $('select#make').val();
            model = $('select#model').val();
            submodel = $('select#submodel').val();
            body = $('select#body').val();

            // reset selections first
            $("#home-fit-guide select").val(0);

            // redirect to car fit guide
            var url = "/CarAudio/What-Fits-My-Car.aspx?year=" + year + "&make=" + make + "&model=" + model + "&submodel=" + submodel + "&body=" + body;
            window.location = url;
        }

    </script>    



