        });

        $(document).on("focus",".list_product.list_note", function(){
            $(".focused_listitem").removeClass('focused_listitem');
            $(this).addClass('focused_listitem');
            setFocusedListItem();
        });

        $(document).on("focus",".list_shop_content .shop_name a", function(){
            $(".focused_listitem").removeClass('focused_listitem');
            $(this).parents('.shop_name').addClass('focused_listitem');
            setFocusedListItem();
        });

        $(document).on("focus","input.list_note_add", function(){
            $(".focused_listitem").removeClass('focused_listitem');
            $(this).addClass('focused_listitem');
            setFocusedListItem();
        });


        function setFocusedListItem(){

            userList_focusedButton_id = null;
            userList_focusedButton_class = null;
            userList_focusedItem_id = null;

            focused_userlist_item = $(".focused_listitem");

            if(focused_userlist_item.attr('id')){
                userList_focusedItem_id = focused_userlist_item.attr('id');
            }

            if($(':focus').hasClass('new_list_amount')){
                userList_focusedButton_id = $(':focus').attr('href');
                userList_focusedButton_class = "." + $(':focus').attr('class').split(" ").join(".");
            }

            if($(':focus').hasClass('list_note_add')){
                userList_focusedButton_class = "list_note_add";
            }
        }

    });

