    <script>
        $('form#email_list').submit(function(){

            $('.shop_alert').removeClass('list_sended');

            var is_logged = !$('body').hasClass('no_login'),
                email = $('#list_email').val(),
                user_email = '';

            if(is_logged && email == '' && user_email != '') {
                email = user_email;
            }

            if(!validateEmail(email)) {
                $('.shop_alert').html('Vyplňte platný e-mail');
            } else {

                addTrackEvent('nakupni-listek-email-odeslan', '1');

                $.ajax({
                    type: "POST",
                    url: "/uzivatel/nakupni-seznam-email?id=0",
                    data: {
                        'email': email
                    },
                    cache: false,
                    dataType: "html",
                    beforeSend: function(){
                        $('form#email_list input[type=submit]').attr('disabled',true);
                    },
                    success: function(){
                        setTimeout("$('form#email_list input[type=submit]').attr('disabled',false);",2000);
                        $('.shop_alert').html('Nákupní seznam byl odeslán').addClass('list_sended');
                        $('#list_email').val('').removeAttr('placeholder');
                    }
               });
            }

            return false;
        });
