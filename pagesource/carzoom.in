				var select_val = $("#"+select_id).val();
				if(select_val=='#' || select_val==''){
					alert("Please select a value");
					return false;
				} else if(select_val!='') {
					$(this).attr('href',select_val);
				}
			});
			
			$(".search_select").change(function(){
				var option_val = $(this).val();
				var option_id = $(this).attr("id");
				var option_arr = option_id.split("_");
				var target_id = "redirect_"+option_arr[1];
				
				$("#"+target_id).attr('href',option_val);
			});
		});
