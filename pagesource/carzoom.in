			function get_models( make, model, variant ) {
				var cz_make = $("#cz_widget_form #"+make).val();
				
				//loading.show();
				$("#"+model+"_box").html('<select name="" id="" class="cz_select"><option value="">Loading..</option></select>');
				$("#"+variant+"_box").html('<select name="" id="" class="cz_select"><option value="">Loading..</option></select>');
				
				$.ajax
				({
					type: "POST",
					url: "http://carzoom.in/wp-content/plugins/carzoom-compare/load_data.php",
					data: { make: cz_make, car_model: model, car_variant: variant },
					timeout: 5000,
					error: function(){
						//loading.fadeOut();
						alert('Error loading results. Please refresh your page or check internet connection.');
					},
					success: function(response)
					{
						//loading.fadeOut();
						$("#"+model+"_box").html(response);
						$("#"+variant+"_box").html('<select name="'+variant+'" id="'+variant+'" class="cz_select" onchange="make_comparison();"><option value="">- No option available -</option></select>');
					}
				});
			}
			
			function get_variants( model, variant ) {
				var cz_model = $("#cz_widget_form #"+model).val();
				
				var variant_1 = $("#cz_widget_form #variant_1").val();
				var variant_2 = $("#cz_widget_form #variant_2").val();
				var exclude_variant = '';
				
				if(variant_1) {
					exclude_variant = variant_1;
				}
				else if(variant_2) {
					exclude_variant = variant_2;
				}
				else {
					exclude_variant = '';
				}
				
				//loading.show();
				$("#"+variant+"_box").html('<select name="" id="" class="cz_select"><option value="">Loading..</option></select>');
				$.ajax
				({
					type: "POST",
					url: "http://carzoom.in/wp-content/plugins/carzoom-compare/load_data.php",
					data: { model: cz_model, car_variant: variant, exclude: exclude_variant },
					timeout: 5000,
					error: function(){
						//loading.fadeOut();
						alert('Error loading results. Please refresh your page or check internet connection.');
					},
					success: function(response)
					{
						//loading.fadeOut();
						$("#"+variant+"_box").html(response);
					}
				});
			}
			
			$(document).ready(function() {
				$("#cz_widget_form").submit(function(){
					var variant_1 = $("#cz_widget_form #variant_1").val();
					var variant_2 = $("#cz_widget_form #variant_2").val();
					var action_url = "http://carzoom.in/compare/";
	
					$("#cz_widget_form #cz_submit").attr("value", 'Comparing..');
	
					var query_str = variant_1+'_vs_'+variant_2;
	
					if(variant_1=='' || variant_2=='') {
						alert('Please select both cars to make comparison');
						$("#cz_widget_form #cz_submit").attr("value", 'Compare');
						return false;
					}
					if(variant_1 == variant_2) {
						alert('Please select different cars to make comparison');
						$("#cz_widget_form #cz_submit").attr("value", 'Compare');
						return false;
					}
					else {
						$("#cz_widget_form").attr("action", action_url+"?cars="+query_str);
						return true;
					}
				});
			});
			</script>
