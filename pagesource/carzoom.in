<script type="text/javascript">
/** Hiding Blank AD from Content bottom area **/
jQuery(document).ready(function(){
	var cz_cont_ad = jQuery(".content .ad_campaign .textwidget img").length;
	var img_src = '';
	var img_arr = '';
	var img_name = '';
	
	if(cz_cont_ad > 0) {
		img_src = jQuery(".content .ad_campaign .textwidget img").attr("src");
		if(img_src != '') {
			img_arr = img_src.split('/');
			img_name = img_arr.pop();
			
			if(img_name == 'blank.gif') {
				jQuery(".content .ad_campaign .textwidget img").remove();
			}
		}
	}
});
</script>

