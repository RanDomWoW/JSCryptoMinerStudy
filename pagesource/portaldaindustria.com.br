<input type="hidden" name="api_url_next" class="api_url_next" value="https://noticias.portaldaindustria.com.br/api/noticias/?capas=1&amp;limit=3&amp;page=2">

<script>

    window.onload = function() {
        
        $('.botao_caregar_mais').on('click', function() {

            $('.botao_caregar_mais').addClass('button_loading');

            var url = $('.api_url_next').val();
            
            $.ajax({
                dataType: "json",
                url: url,
                success: function( data ) {
                    
                    if( data.next == null ) {
                        $('.botao_caregar_mais').hide();
                    } else {
                        $('.api_url_next').val(data.next.replace("http:", "https:"));
                    }

                    data.results.forEach(function(noticia) {

                        var div_noticia = `
                            <div class="col-sm-6 col-md-4">
                                <div class="card card_vertical card_space-bottom">
                                    <div class="card--media">
                                        <div class="multimedia multimedia_sized multimedia_wide">
                                                <figure class="multimedia--media">
                                                    <img src="${ noticia.imagens.destaque }" alt="" title="" class="multimedia--element">
                                                </figure>
                                                <a href="${ noticia.url }" title="" target="_blank" class="multimedia--link cover"></a>
                                        </div>
                                    </div>
                                    <div class="card--content">
                                        <div class="post-meta post-meta_md ${ noticia.classe_css_tipo }">
                                            <a href="${ noticia.url }" class="post-meta--hat ">${ noticia.subtipo }</a>
                                                <a href="${ noticia.url }" title="" target="_blank" class="post-meta--link">
                                                <h3 class="post-meta--title">${ noticia.titulo }</h3>
                                                </a>
                                        </div>
                                    </div>
                                </div>
                            </div>`;

                            $('.container-principal-noticia').append($(div_noticia).hide().fadeIn(1500));
                    });

                    $('.botao_caregar_mais').removeClass('button_loading');

                },
                error: function( data ) {
                    console.log('error');
                    $('.botao_caregar_mais').removeClass('button_loading');
                }
            });

        });
        
    };
</script>
	

	
