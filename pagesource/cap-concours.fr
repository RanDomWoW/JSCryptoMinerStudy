<script type="text/javascript">//<![CDATA[
  var urlValid = "/index.php?module=account&action=login:loginIsValid";
  var ok = true;
  var loginValidating=false;

  
  function setLoginBoxError( loginError, pwdError ) {
      $('div.loginMsg').html('');
      $('div.passwordMsg').html('');
      if( loginError != '' ) {
          $('div.loginMsg').html('<span class="loginBoxError">'+loginError+'</span>');
          $('form#loginForm input').blur();
          $('form#loginForm input#login').focus();
      }
      if( pwdError != '' ) {
          $('div.passwordMsg').html('<span class="loginBoxError">'+pwdError+'</span>');
          if( loginError == '' ) {
              $('form#loginForm input').blur();
              $('form#loginForm input.password:visible').focus();
          }
      }
  }


  jQuery(function (){
    var typeAlone = null;

    $('form#loginForm').prepend('<div style="display:none" class="loginMsg"></div>');
    $('form#loginForm').prepend('<div style="display:none" class="passwordMsg"></div>');
    $('form#loginForm input#login').attr('rel', 'div.loginMsg');
    $('form#loginForm input#password').attr('rel', 'div.passwordMsg');
    
    var cluetipOptions = {
                positionBy: 'fixed',
                arrows: true,
                local: true,
                showTitle: true,
                activation: 'focus',
                leftOffset: -405
        };

    $('form#loginForm input#login').cluetip( cluetipOptions );
    $('form#loginForm input#password').cluetip( cluetipOptions );

    $('input#password, input#login, input#rememberMe').listenForEnter().bind('pressedEnter', function() {
        if( !loginValidating ) {
                $('#loginForm').submit();
        }
    });
    $('#loginForm').submit( function() {
        if( loginValidating ) {
                return false;
        }
        loginValidating=true;
        setLoginBoxError('', '');
        $('form#loginForm input').blur();
        $('form#loginForm').trigger('removePlaceholder');
        // validité du couple login/password
        $.ajax({
              url: urlValid,
              data: {login: $('#login').val(), password: $('#password').val() },
              type: 'POST',
              success: function(data) {
                            if( data && data.fieldInError!='' ) {
                                    var loginErrorMsg='';
                                    var pwdErrorMsg='';
                                    if( data.fieldInError == 'password' ) {
                                        pwdErrorMsg = data.errorMsg;
                                    } else {
                                        loginErrorMsg = data.errorMsg;
                                    }
                                    setLoginBoxError(loginErrorMsg, pwdErrorMsg);
                                    $('form#loginForm').trigger('putBackPlaceholder');
                                    loginValidating=false;
                            } else {
                                    $('#loginForm').unbind().submit();
                            }
              },
              error: function() { $('#loginForm').unbind().submit(); }
            });
        ok = false;
        return ok;
    });
});

jQuery(function() {
    // Invoke the plugin
    $('input, textarea').placeholder();

});
  
//]]</script>

