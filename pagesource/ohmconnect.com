  <script>
   $(function() {
  
  $('#mobile-initial-btn').click(function() {
    var zip = $zip.val()
    
    if (zip.length > 4) {
      $initial_zip.hide()
      $social_sign_on.hide()
      $connect_text.hide()
      $signup_expand.show()
    } else {
      ga('send', 'event', 'Error', 'Zip Code Error', PAGE_ID);
      $('.ohm-mobile-zip').addClass('zip-error');
    }
  });
  
});

  $(document).click(function(event){
      $(event.target).closest("a, button, input, div").each(function(i, el){
      		
          var $el = $(el), category = 'click', action = $el.text().trim() || $el.attr('id');          

          if ($el.is('input') && $el.attr( "type" ) != 'submit'){
              category = 'focus'
              action = $el.attr('name')
          }
          
          var divIDs = ['mobile-initial-btn', 'fb-login', 'google-login', 'desktop_nav_burger', 'mobile-persistent-nav', 'mobile-hamburger-nav', 'mobile-nav'];
          if ($el.is('div')) {
              if ($.inArray($el.attr('id'), divIDs) == -1) {
              	return;
              }
          }

       try {
            ga('send', 'event', category, action, PAGE_ID);
        } catch (e) {
            console.log(e);
        }         
          
      })
  })
  </script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/vue/2.3.3/vue.min.js">
  </script>
  <script src="https://cdn.jsdelivr.net/npm/vue2-filters/dist/vue2-filters.min.js">
  </script>
  <script src="https://unpkg.com/axios/dist/axios.min.js">
  </script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-scrolldepth/1.0.0/jquery.scrolldepth.js" type="text/javascript">
  </script>
  <script>
   var App = new Vue({
        el: '#cashout-app',

        data: function () {
            return {
                cashouts: [],
                scroll_timer: '',
                counter: 0,
                show_items: 5
            }
        },
        created: function () {
            this.fetchEventsList();
        },
        updated: function (){
            $('.cashout-loop').slice(0, App.show_items).show();
            $('.cashout-loop').slice(App.show_items, App.cashouts.length).hide();
            App.scroll_timer = setInterval(App.scrollList, 2000);
        },
        methods: {
            fetchEventsList: function () {
                axios.get('https://login.ohmconnect.com/data/recent_cashouts?limit=120').then(function(response){
                    App.cashouts = response.data;
                    App.counter = 0;
                })

            },
            scrollList: function () {
                if (App.counter + App.show_items >= App.cashouts.length) {
                    App.cancelTimer()
                    App.fetchEventsList()
                } else {
                    $($('.cashout-loop')[App.counter]).animate({
                        height: "hide"
                    }, 400)
                    $($('.cashout-loop')[App.counter + App.show_items]).animate({
                        height: "show"
                    }, 400)
                    App.counter++;
                }
            },
            cancelTimer: function () {
                clearInterval(App.scroll_timer)
            },
        }
    })

  var track_key = Cookies.get('ohm_track_key');
  function setupABTest() {
    PageViewTracker.set_cookie('ohm_primary_page', 'signup_3_19a')
    PageViewTracker.log_user_action('signup_3_19a')
  }
  setupABTest()
  
var API_ENDPOINT = "https://login.ohmconnect.com/api/v1/"

var $form = $('form#signup'),
    $zip = $('input[name=zip_code]'),
    $email = $('input[name=email]'),
    $password = $('input[name=password]'),
    $utility = $('select[name=utility]'),
    $utility_free_text = $('input[name=utility_free_text]'),
    $account_form_div = $('.account-form-div'),
    $expandable = $('.expandable-utility'),
    $skip = $('[data-ohm-selector=skip-utility]'),
    $skip_checkbox = $('input[name=skip_utility]'),
    $connect_utility_btn = $('.js-connect-utility-button'),
    $utility_view = $('#utility-view'),
    $headline = $('#headline'),
    $initial_zip = $('#initial_zip'),
    $social_sign_on = $('#social_sign_on'),
    $connect_text = $('#connect-text'),
    $mobile_initial_btn = $('#mobile-initial-btn'),
    $signup_expand = $('#signup-expand');


var OhmSignup = {

    _choices: [],

    _validate_email: function(email){
        // Basic client-side validation. Server-side validates too.
        if (email.length > 3 && ~email.indexOf("@")){
            return true
        }

        else {
            return false
        }
    },

    _post: function(post_data, success_callback, failure_callback){
        if (track_key) {
            post_data.ohm_track_key = track_key;
        }

        $.ajax({
            type: 'POST',
            url: API_ENDPOINT + "signup",
            contentType: 'application/json',
            dataType: 'json',
            data: JSON.stringify(post_data),
            success: function(response){
                if (response.status > 0){
                    success_callback()
                }

                else {
                    failure_callback(response)
                }

            }
        })
    },

    _reveal_expanded: function(){
        $account_form_div.hide()
        $expandable.slideDown()
        $utility_view.slideDown()
        $('#hero-section').slideUp()
        $('#join-section').slideUp()
        $('#who-section').slideUp()
        $('#video-section').slideUp()
        $('#saying-section').slideUp()
        $headline.slideUp()

    },

    _failed_signup: function(response){
        alert(response.msg)
    },

    _create_account: function(){
        $form.on('submit', function(){
            var password = $password.val()
            var email = $email.val()
            var zip = $zip.val()

            if ( OhmSignup._validate_email(email) && password.length > 3 && zip.length > 4 ){
                var post_data = {
                    'email': email,
                    'password': password,
                    'zip_code': zip
                }

                OhmSignup._post(post_data, OhmSignup._reveal_expanded, OhmSignup._failed_signup)

            }

            return false;
        })

    },

    _submit_on_skip: function(){
        $connect_utility_btn.click(function(){
            $skip_checkbox.prop('checked', false)
            $form.off('submit').submit()
        })

        $skip.click(function(){
            $skip_checkbox.prop('checked', true)
            $form.off('submit').submit()
        })
    },

    _fetch_utilities_by_zip: function(){
        $zip.change(function(){
            var zip_code = $(this).val()

            $.get(API_ENDPOINT + "signup/" + zip_code, {}, function(data){
            		
                if (data.status > 0){
                    OhmSignup._choices = data.choices
				
                    $('option', $utility).remove()
                    $.each(data.choices, function(index, utility) {
                        $utility.append(`<option value='${utility.utility_id}' data-utility-display-name='${utility.display_name}'
                        data-utility-help='${utility.account_access_url}'>${utility.utility_name}</option>`)
                    });
										
                    OhmSignup._update_dynamic_elements($utility.find("option:first"))
                }

            })
        })
    },

    _change_on_utility_selection: function(){
        $utility.change(function(){

            if ($(this).val() == "999"){
                $utility_free_text.slideDown()
            }

            else {
                $utility_free_text.slideUp()
            }

            var $selected = $(this).find(":selected")
            OhmSignup._update_dynamic_elements($selected)

        })
    },

    _update_dynamic_elements: function(selected_utility_option){

        var utility_display_name = selected_utility_option.attr('data-utility-display-name')

        $("input[name=utility_username]").attr('placeholder', `Your ${utility_display_name} username`)
        $("input[name=utility_password]").attr('placeholder', `Your ${utility_display_name} password`)
        $(".utility-display-name").text(utility_display_name)

        var help_link = selected_utility_option.attr('data-utility-help')
        if (help_link == "null"){
            $("#help-click").parent().hide()
        }

        else {
            $("#help-click")
                .attr('href', help_link)
                .parent().show()
        }

    },

    _tag_dynamic_elements: function(){
        // Webflow doesn't let us insert <span> elements within a text block, so do it here based on default text on load.
        var $title = $(".utility-display-text"),
            $help = $("#help-click").parent();

        var updated_display = $title.html().replace(/(utility)/g, '<span class="utility-display-name">$1</span>')
        $title.html(updated_display)

        var updated_help = $help.html().replace(/(utility)/g, '<span class="utility-display-name">$1</span>')
        $help.html(updated_help)

        var updated_state = $("#headline").html().replace(/(California)/g, '<span class="utility-state">$1</span>')
        $("#headline").html(updated_state)
    },

    _update_from_ip: function(){
        $.get(API_ENDPOINT + "ip", function(r){
            if (r.location && r.location.state_code && r.location.state_code == "CA"){
                var updated_state = $("#headline").html().replace(/(local energy authorities)/g, 'California')
                $("#headline").html(updated_state)
            }
        });
    },

    init: function(){
        OhmSignup._tag_dynamic_elements()
        OhmSignup._update_from_ip()
        OhmSignup._fetch_utilities_by_zip()
        OhmSignup._create_account()
        OhmSignup._change_on_utility_selection()
        OhmSignup._submit_on_skip()
    }
}

OhmSignup.init()

jQuery(function() {
  jQuery.scrollDepth();
});
  </script>
 </body>
