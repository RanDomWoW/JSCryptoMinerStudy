<script>
  function trackRemoveFromCart_adobe(productCode, initialQuantity, productName,productBrandName) {
	var lcproductCode=productCode.toLowerCase();
	var lcproductName=productName.toLowerCase();
	dataLayer.event =
	{ 
	name: ['cart_remove'] 
	};
	dataLayer.products =[{ 
		'productID':lcproductCode, 
		'name':lcproductName, 
		'brand':productBrandName, 
		'subBrand':'' 
		}];
	_satellite.track('cart_addition');
}

window.mediator.subscribe('trackRemoveFromCart', function(data) {
	if (data.productCode && data.initialCartQuantity)
	{
		trackRemoveFromCart_adobe(data.productCode, data.initialCartQuantity,data.productName,data.productBrandName);
	}
}); 

function trackAddCart_adobe(productCode,productBrandName,productName) {
	var lcproductCode=productCode.toLowerCase();
	var lcproductName=productName.toLowerCase();
	dataLayer.event = {
			  name: ['cart_addition']
			  };
			dataLayer.products =[{
			             'productID':lcproductCode,
			             'name':lcproductName,
			             'brand':productBrandName
			          }];
			_satellite.track('cart_addition'); 
}

window.mediator.subscribe('trackAddToCart', function(data) {
	if (data.productCode)
	{
		
		trackAddCart_adobe(data.productCode,data.productBrand,data.productName);
	}
});
</script>
















