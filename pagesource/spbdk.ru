    function lazyLoad() {
        var images = $('.lazy');
        images.each(function () {
            var img = $(this);

            var src = img.attr('data-src');
            var width = img.attr('data-width');
            var height = img.attr('data-height');
            img.attr('width', width).attr('height', height);
            img.on('load', imgLoaded(img[0])).attr('src', src);
