<script>
    $('#present').click(function () {
        tab2();
    });

    function makePost2(number) {
        location.hash = '#present/' + number;
        $.post(
            '/api/present.php',
            {
                SECTION_ID: number,
            },
            function (data) {
                if (data != "") {
                    $(".present-wrapper").hide();
                    $('.present-goods').html(data);
                }
            }
        );
    }


    $(document).ready(function () {
        var hashes = window.location.href.slice(window.location.href.indexOf('#') + 1);
        var section = hashes.slice(8);

        if (hashes.indexOf('present', 0) != -1) {
            tab2();

            if (!(/^[^\d]*$/.test(section))) {
                makePost2(section);
            }

        }
    });

    $(window).bind('hashchange', function () {
        if ($('.tabs').find('.active').selector == ".tabs .active") {
            $('.present-goods').html("");
            $(".present-wrapper").show();
        }
    });


    function activate2(element, container) {
        container.find('.active').removeClass('active')
        element.addClass('active')
    }

    function tab2() {
        var href = $('#pres a').attr('href'),
            $ul = $('#pres a').closest('ul');

        if (/^#\w+/.test(href)) {

            if ($('#pres').hasClass('active')) {
                return
            }

            $href = $(href);

            activate2($('#pres a').parent('li'), $ul);
            activate2($href, $href.parent());
        }
    }
