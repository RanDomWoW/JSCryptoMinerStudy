<script type="text/javascript">
    require(["jquery", "jquery-ui"], function (jQuery) {
        jQuery(document).ready(function ($) {
            $(function () {
                var study_base = "//www.umt.edu/study";
                $("#term").autocomplete({
                    source: function (request, response) {
                        $.ajax({
                            url: study_base + "/areas/json",
                            dataType: "jsonp",
                            data: {
                                q: request.term
                            },
                            success: function (data) {
                                response(data);
                            }
                        });
                    },
                    minLength: 3,
                    select: function (event, ui) {
                        if (ui.item.slug != "") {
                            window.location.href = study_base + "/" + ui.item.slug;
                        } else {
                            window.location.href = study_base + "/search?term=" + ui.item.value;

                        }
                    },
                    open: function () {
                        $(this).removeClass("ui-corner-all").addClass("ui-corner-top");
                    },
                    close: function () {
                        $(this).removeClass("ui-corner-top").addClass("ui-corner-all");
                    }
                });
            });
        });
    });
</script>  


    
