<script language="javascript">
var numerolote;
var var_action; 

function chamaActionCertificado() { 

   numerolote =document.getElementById("lote");
   numerolote.disabled = false;
   //document.form.action = "ValidaCampos";
   var_action ="pesquisar.asp";
   
}  

function chamaActionFISPQ() {  
  
   numerolote =document.getElementById("lote");
   //limpa o Campo
   document.getElementById("lote").value = "";
   //desativa
   numerolote.disabled = true;
   //document.form.action = "ValidaCamposFISPQ";
   var_action = "pesquisar.asp";
  
} 

function contaCaracteres(){
  var campoLote = document.forms["formulario"]["numeroLote"].value; //(document.getElementById('numeroLote').value);
  var codigo = document.forms["formulario"]["codigo"].value;
  
     
     var numeroCaractereCampoLote = campoLote.length;
       
      if(numeroCaractereCampoLote ==9 && codigo !=""){
        
        document.formulario.action = var_action; 
    var codigoDividido = codigo.split("-");
        document.forms["formulario"]["codigo"].value = codigoDividido[0];
        this.form.submit();    
          return true;  
              
      }else if(campoLote =="" || codigo ==""){
        //Valida esta condicao somente se o radiogrupo certificado estiver
             //selecionado
             if(numerolote.disabled == false){   
             
          alert("Todos os campos devem ser preenchidos");
            return false;
        
        }else if(codigo ==""){
           
          alert("Todos os campos devem ser preenchidos");
            return false;
        
        }else if(codigo !=""){
          
          document.formulario.action = var_action; 
          var codigoDividido = codigo.split("-");
          document.forms["formulario"]["codigo"].value = codigoDividido[0];
          this.form.submit();  
          return true;  
        }
           
      }else if(campoLote !="" && codigo !="" && numeroCaractereCampoLote !=9 ){
             //Valida esta condicao somente se o radiogrupo certificado estiver
             //selecionado
           if(numerolote.disabled == false){
               alert("O lote deve conter 9 caracteres");
                return false;
             }
       } 
       
  }
  
