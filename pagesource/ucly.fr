						};
					});
					// Partage réseaux sociaux
					bindClick("body.fiche .actions-fiche .partage-reseauxsociaux__item a", function(node) {
						return {
							chapter1: 'module_reseaux_sociaux',
							chapter2: node.find('.actions-fiche__libelle').text(),
							name: $('h1').text(),
							type: 'exit'
						}
					});
					// Liens de téléchargement
					bindClick("body.fiche a.fichiers-joints__lien", function(node) {
						return {
							chapter1: 'telechargement',
							chapter2: $('h1').text(),
							name: node.find('.fichiers-joints__titre').text(),
							type: 'download'
						}
					});
					// Résultats de recherche
					if ($('body.recherche #liste_resultats').length > 0) {
						$('#liste_resultats > li a').each(function() {
							var position = $(this).parents('li').first().index();
							var pageNumber = $('#resultats_recherche_navigation > strong').text().trim().replace(/\/.*$/gi, '');
							this.onclick = function() {
								return ATTag.internalSearch.send({
									elem: this,
									keyword: $('[name="INIT_MOTS_CLEF"]').val(),
									resultPageNumber: pageNumber,
									resultPosition: position
								});
							};
						});
					}
					// Page virtuelle AT Internet : Rechercher une formation
					if ($('body.ficheaccueil .rechercheFormationCard .tag-formation').length > 0) {
						$('body.ficheaccueil .rechercheFormationCard .tag-formation').on('submit', function() {
							var customObject = {};
							if ($('.rechercheFormationCard [name=NIVEAU_RECRUTEMENT]').val() && $('.rechercheFormationCard [name=NIVEAU_RECRUTEMENT]').val() !== '0000') {
								customObject[1] = '[' + $('.rechercheFormationCard [name=NIVEAU_RECRUTEMENT]').val() + ']';
							}
							if ($('.rechercheFormationCard [name=DISCIPLINE]').val() && $('.rechercheFormationCard [name=DISCIPLINE]').val() !== '0000') {
								customObject[2] = '[' + $('.rechercheFormationCard [name=DISCIPLINE]').val() + ']';
							}
							if ($('.rechercheFormationCard [name=ACCESSIBLE_EN]').val() && $('.rechercheFormationCard [name=ACCESSIBLE_EN]').val() !== '0000') {
								customObject[3] = '[' + $('.rechercheFormationCard [name=ACCESSIBLE_EN]').val() + ']';
							}
							if ($('.rechercheFormationCard [name=CODE_RATTACHEMENT]').val() && $('.rechercheFormationCard [name=CODE_RATTACHEMENT]').val() !== '0000') {
								customObject[4] = '[' + $('.rechercheFormationCard [name=CODE_RATTACHEMENT]').val() + ']';
							}
							var pageTag = {
								level2: '2',
								chapter1: 'formations',
								name: 'nos_formations',
								customVars: {page: customObject}
							};
							ATTag.page.send(pageTag);
						});
					}
					ATTag.dispatch();
				});})(ATInternet, jQuery.noConflict());
			</script>
	</body>
</html>