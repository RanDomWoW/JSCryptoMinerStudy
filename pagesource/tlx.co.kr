function send_sms() {
    var _val = $('input[name=phone]').val();
    if (_val.length < 1) return false;
    var get_url = '/contents/ajax_appstore_sms/1?phone='+_val;

    $.ajax({
        type : 'GET',
        url : get_url,
        dataType : 'html',
        success : function(_data) {
            $('input[name=phone]').val('');
            $('.text-success').show();
        }
    });
}

$('input[name=phone]').keypress(function (event) {
    if (event.which && (event.which <= 47 || event.which >= 58) && event.which != 8) {
        event.preventDefault();
    }
});
</script>













