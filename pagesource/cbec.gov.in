<script type="text/javascript">		
	function showSecondLevel(e, y) {		
	var keyCode = e ? (e.which ? e.which : e.keyCode) : event.keyCode;		
	$(e.keyCode).delay(1);
		if (e.keyCode == 40){ 		
			$(y).children('ul').attr('style','display:block;z-index:1000;position:absolute;width:220px');
		}
		if ((e.keyCode == 38 || e.keyCode == 27)) {
			$(y).children('ul').attr('style', 'display:none;');
			$(y).children('ul ul').attr('style', 'display:none;');
		}	
	}
	
	function showLastLevel(e, y) {
		var keyCode = e ? (e.which ? e.which : e.keyCode) : event.keyCode;
		if (e.keyCode == 39) {
			$(y)
					.children('ul ul')
					.attr('style',	'display:block; z-index:1000;position:absolute; left:98%');

		}
		if (e.keyCode == 37) {
			$(y).children('ul ul').attr('style', 'display:none;');
		}
	} 

	
	

	function mouseIn(y) {
		var mydiv=$(y).children('ul').width(); 
		$(y).children('ul').attr('style', 'display:block;width:'+mydiv+'px');
	}
	
	function mouseOut(y) {
		var mydiv=$(y).children('ul').width(); 
		$(y).children('ul').attr('style', 'display:none;width:'+mydiv+'px');
	}

	$(document).click(function() {
		$('.navigation ul ul').attr('style', 'display:none;');
	})
	
	function hideSecondLevel(y) {
		
		$('.navigation ul ul').attr('style', 'display:none;');
	}
	
	function hideLastLevel(y) {
		$('.navigation ul ul ul').attr('style', 'display:none;');
	}
	function hideSecondLevel_f(y) {
		var mydiv=$(y).children('ul').width(); 
		$('.focus_menu ul ul').attr('style', 'display:none;');
	}
	
	function hideLastLevel_f(y) {
		$('.focus_menu ul ul ul').attr('style', 'display:none;');
	}
</script>


