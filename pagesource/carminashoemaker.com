          }

          function closeCookiesBanner(){
            var expirationDate=new Date();
            expirationDate.setDate(expirationDate.getDate() + 3);
            document.cookie='showCookiesPolicyWarning=0; expires='+expirationDate.toUTCString()+'; path=/';

            
            $('cookiesBannerContainer').set('tween', { onComplete: function() { $('cookiesBannerContainer').destroy(); } }).tween('bottom', -$('cookiesBannerContainer').getSize().y);
            

            
          }
