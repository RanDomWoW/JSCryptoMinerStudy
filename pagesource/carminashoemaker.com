            $('cookiesBannerContainer').setStyle('position', 'fixed').setStyle('bottom', -$('cookiesBannerContainer').getSize().y).tween('bottom', 0);

            

            
              var expirationDate=new Date();
              expirationDate.setDate(expirationDate.getDate() + 3);
