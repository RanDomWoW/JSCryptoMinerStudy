re__container"><div class="compare__item-wrapper compare__item-wrapper--left"><div class="compare__item curadoria"></div></div><div class="compare__item-wrapper compare__item-wrapper--right"><div class="compare__item ineditos"></div></div><div class="compare__controller"></div></div><div class="compare__picker"><div class="compare__picker__wrapper"><i class="compare__picker__icon icon-brackets"></i></div></div><div class="compare__labels"><span class="compare__label">Curadoria</span><span class="compare__label">Inéditos</span></div></div><script>( () => {
  function Compare() {
    this.coords = {}
    this.CONSTANTS = {
      CLASS: {
        PICKER: 'compare__picker',
        IMAGE: 'compare__item'
      }
    }

    this.components = {
      container: document.querySelector( '.compare' ),
      picker: document.querySelector( '.compare__picker' ),
      controller: document.querySelector( '.compare__controller' ),
      main: document.querySelector( '.compare__item-wrapper--left' ),
      secondary: document.querySelector( '.compare__item-wrapper--right' )
    }

    this.init()
  }

  Compare.prototype.init = function() {
    setTimeout( () => {
      this.getPositions()
      this.bindEvents()
    }, 150 )
  }

  Compare.prototype.bindEvents = function() {
    let move = this.move.bind( this )
    let events = {
      down: [ 'mousedown', 'touchstart' ],
      up: [ 'mouseup', 'touchend' ],
      move: [ 'mousemove', 'touchmove' ]
    }

    events.down.forEach( event => {
      document.addEventListener( event, e => {
        if ( e.target.classList.contains( this.CONSTANTS.CLASS.PICKER ) ) {
          e.preventDefault()
          this.components.picker.classList.add( 'moving' )
          events.move.forEach( motion => {
            window.addEventListener( motion, move, false )
          } )
        }
      } )
    } )

    events.up.forEach( event => {
      document.addEventListener( event, e => {
        events.move.forEach( motion => {
          window.removeEventListener( motion, move, false )
          this.components.picker.classList.remove( 'moving' )
        } )
      } )
    } )
  }

  Compare.prototype.move = function( e ) {
    let container = this.components.container.getBoundingClientRect()
    let containerWidth = container.width
    let containerX = container.left
    let mouseX = e.pageX || e.changedTouches[ 0 ].pageX
    let x1 = mouseX - containerX > 0 ? mouseX - containerX : 0
    let x2 = containerWidth - x1 > 0 ? containerWidth - x1 : 0
    let x3 = ( x1 > containerWidth ? containerWidth - 3 : x1 ) + 'px'

    this.components.main.style.maxWidth = x1 + 'px'
    this.components.secondary.style.maxWidth = x2 + 'px'
    this.components.controller.style.left = x3
    this.components.picker.style.left = x3
  }

  Compare.prototype.getPositions = function() {
    let images = [ this.components.main, this.components.secondary ]
    this.updateCoords()

    images.forEach( image => {
      image.querySelector( `.${this.CONSTANTS.CLASS.IMAGE}` ).style.minWidth = `${this.coords.container.width}px`
    } )

    this.components.controller.style.left = this.coords.main.width + 'px'
    this.components.picker.style.left = this.coords.main.width + 'px'
  }

  Compare.prototype.updateCoords = function() {
    Object.keys( this.components ).forEach( key => {
      this.coords[ key ] = this.components[ key ].getBoundingClientRect()
    } )
