    <script>
      var loadButton = document.getElementById('instafeed-loadmore');
      var feed = new Instafeed({
          get: 'user',
          type: 'image',
          limit: '5',
          sortBy: 'most-recent',
          resolution: 'standard_resolution',
          userId: 143966263,
          accessToken: '143966263.1677ed0.6475bd851d6548cc8704adc8795ec580',
          template: '<li><span class="instagram-post" data-url="{{link}}" data-modal="instagram-popup" data-caption="{{caption}}" data-image="{{image}}" style="background-image: url({{image}});"></span></li>',
          after: function() {
              if (!this.hasNext()) {
                  loadButton.setAttribute('disabled', 'disabled');
              }
          }
      });

      loadButton.addEventListener('click', function() {
          feed.next();
      });

      feed.run();

      var igObject,
          feedIndex;

      $(document).ready(function() {
        $('body').on('click', '.instagram-post', function() {

          if ( $(this).parent().index() === 0 ) {
            $('#instagram-popup .nav-left').hide();
          }
          else {
            $('#instagram-popup .nav-left').show();
          }
          if ( $(this).parent().index() == $('#instafeed li').length -1 ) {
            $('#instagram-popup .nav-right').hide();
          }
          else {
            $('#instagram-popup .nav-right').show();
          }

          igObject = $(this);
          var modal = igObject.data( 'modal' ),
              image = igObject.data( 'image' ),
              url = igObject.data( 'url' ),
              caption = igObject.data( 'caption' );

          $('#instagram-popup').find('.link').attr('href', url);
          $('#instagram-popup').find('.image').css('background-image', 'url(' + image + ')');
          $('#instagram-popup').find('.caption p').html(caption);
          $('#instagram-popup').fadeIn(200);
          return false;
        });

        $('#instagram-popup .nav-left').on('click', function() {
          igObject = $(igObject).closest('li').prev().find('.instagram-post'),
          modal = igObject.data( 'modal' ),
          image = igObject.data( 'image' ),
          url = igObject.data( 'url' ),
          caption = igObject.data( 'caption' );

          if ( $(igObject).parent().index() === 0 ) {
            $('#instagram-popup .nav-left').hide();
          }
          else {
            $('#instagram-popup .nav-left').show();
          }
          $('#instagram-popup .nav-right').show();

          $('#instagram-popup').find('.link').attr('href', url);
          $('#instagram-popup').find('.image').css('background-image', 'url(' + image + ')');
          $('#instagram-popup').find('.caption p').html(caption);
          return false;
        });

        $('#instagram-popup .nav-right').on('click', function() {
          igObject = $(igObject).closest('li').next().find('.instagram-post'),
          modal = igObject.data( 'modal' ),
          image = igObject.data( 'image' ),
          url = igObject.data( 'url' ),
          caption = igObject.data( 'caption' );

          if ( $(igObject).parent().index() == $('#instafeed li').length -1 ) {
            console.log('reached last item');
            $('#instagram-popup .nav-right').hide();
          }
          else {
            $('#instagram-popup .nav-right').show();
