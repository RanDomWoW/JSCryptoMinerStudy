            $(function ($) {
                var companiesString = localStorage.getItem('companies');
                if(companiesString){
                    var companies = JSON.parse(companiesString);
                    for(var i = companies.length-1; i >= 0; i--){
                        //console.log(companies[i].id);
                    }
                }


                $('.storage-add').click(function () {
                    var sId = $(this).attr('storage-id');
                    var sLabel = $(this).attr('storage-label');
                    var sCategory = $(this).attr('storage-category');
                    var sLink = $(this).attr('storage-link');
                    //console.log(sId, sLabel, sCategory, sLink);
                    var item = {id: sId, label: sLabel, link: sLink};
                    if(companies){
                        var removed = false;
                        for(var i = companies.length-1; i >= 0; i--){
                            if(companies[i].id == sId){
                                companies.splice(i, 1);
                                removed = true;
                            }
                        }
                        if(!removed){
                            companies.push(item);
                        }
                    }else{
                        companies = new Array();
                        companies.push(item);
                    }

                    localStorage.setItem(sCategory, JSON.stringify(companies));
                });
            });



