<script>
var $q_premium;
var $premium_no;
$(document).ready(function() {
	$('.is_required').focus(function() {
		var field = $(this).attr('id');
		var label = $("label[for='"+field+"']");
		label.removeClass('required');
	});

	$q_premium = $('#q_premium');
	$premium_no = $('.premium-no');

	$q_premium.click(function() {
		premium_click($(this));
	});


	big_picture_slide_text_timer();
});

function big_picture_slide_text_timer()
{
	setInterval(function () {
		big_picture_slide_text();
	}, 5000);
}

function big_picture_slide_text()
{
	var $container = $('.big-picture-container-heading');
	var $arrow = $('.big-picture-arrow');
	var $element_active;
	var $element_next;
	var height = $container.height();
	$container.height(height);
	$element_active = $('.big-picture-slide-text.active');
	$element_next = $('.big-picture-slide-text:not(.active)');
	$element_active.fadeOut(400, function() {
		$element_next.fadeIn().addClass('active');
		if ($element_next.data('arrow')) {
			$arrow.fadeIn();
		}
	}).removeClass('active');
	if ( ! $element_next.data('arrow')) {
		$arrow.fadeOut();
	}
}

function premium_click($element)
{
	var premium = $element.is(':checked');
	if (premium) {
		$premium_no.hide();
	} else {
		$premium_no.show();
	}
}

function is_valid_email(email)
{
	var pattern = new RegExp(/^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);
	return pattern.test(email);
}

function show_read_me_first()
{
	var html = '';

	html += 'Kontrollera tidigare frågor och svar.<br><br>';
	html += 'Skriv kort, förståeligt och koncist, 5-10 rader är oftast tillräckligt. Ta endast med relevant fakta. Alltför omfattande frågor besvaras ej.<br><br>';
	html += 'Kontrollera att frågan inte innehåller känslig information som du ej vill ska publiceras för allmänheten.<br><br>';
	html += 'Kontrollera att formuläret är korrekt ifyllt.<br><br>';
	html += 'Observera att när du ställt en fråga till Lawline.se så skickas det ett email till dig med en länk som du måste klicka på för att frågan ska skickas. Detta för att säkerställa att din emailadress är korrekt angiven.<br><br>';

	return site_popup_message('Läs detta först', html);
}

function show_terms()
{
	var html = '';
	html += 'Lawline AB besvarar juridiska frågor helt kostnadsfritt och det ställs inga krav på dig som frågeställare. Frågan besvaras av en juriststudent och publiceras tillsammans med svaret på lawline.se. Lawline AB åtar sig inget ansvar för eventuella förluster, kostnader eller andra konsekvenser som kan uppstå i och med ditt användande av vår gratistjänst eller rådgivning som publicerats på vår webbplats. För garanterad korrekthet i vår rådgivning, använd vår betaltjänst istället.<br><br>';
	html += 'Ange inga personuppgifter i din fråga eller information som direkt eller indirekt kan härledas till dig eller andra personer. De uppgifter, såsom namn och e-postadress, du registrer för att skicka din fråga till Lawline publiceras inte och behandlas i enlighet med personuppgiftslagen och vår <a href="/pages/integritetspolicy" target="blank">integritetspolicy</a>.<br><br>';
	return site_popup_message('Lawlines villkor', html);
}
function show_read_faq()
{
	var html = '';
	html += 'Klicka <a href="/pages/faq">här</a> för att komma till Frågor och svar</a>.<br><br>';
	html += 'OBS! Ni lämnar då sidan och er data kommer att nollställas.<br><br>';
	return site_popup_message('Frågor och svar', html);
}

function validate_form()
{
	var error_caption = 'Fel';

	var q_name = $('#q_name');
	var q_email = $('#q_email');
	var q_question = $('#q_question');
	var q_company = $('#q_company');
	var q_phone = $('#q_phone');
	var q_address = $('#q_address');
	var q_postal_code = $('#q_postal_code');
	var q_city = $('#q_city');
	var q_country = $('#q_country');
	var q_number = $('#q_number');
	var q_accept_terms = $('#q_accept_terms');

	if (q_name.is(':visible') && q_name.val() == '') {
		$("label[for='q_name']").addClass('required');
		return site_popup_message(error_caption, 'Namn måste fyllas i.');
	}

	if (q_company.is(':visible') && q_company.val() == '') {
		$("label[for='q_company']").addClass('required');
		return site_popup_message(error_caption, 'Företag måste fyllas i.');
	}

	var q_email_label = $("label[for='q_email']");
	if (q_email.val() == '') {
		q_email_label.addClass('required');
		return site_popup_message(error_caption, 'E-postadress måste fyllas i.');
	}

	if ( ! is_valid_email(q_email.val())) {
		q_email_label.addClass('required');
		return site_popup_message(error_caption, 'Ni har fyllt i en ogiltig e-postadress.');
	}

	if (q_question.val() == '') {
		$("label[for='q_question']").addClass('required');
		return site_popup_message(error_caption, 'Ni måste ange en fråga.');
	}

	if (q_phone.is(':visible') && q_phone.val() == '') {
		$("label[for='q_phone']").addClass('required');
		return site_popup_message(error_caption, 'Telefonnummer måste fyllas i.');
	}

	if (q_address.is(':visible') && q_address.val() == '') {
		$("label[for='q_address']").addClass('required');
		return site_popup_message(error_caption, 'Adress måste fyllas i.');
	}

	if (q_postal_code.is(':visible') && q_postal_code.val() == '') {
		$("label[for='q_postal_code']").addClass('required');
		return site_popup_message(error_caption, 'Postnummer måste fyllas i.');
	}

	if (q_city.is(':visible') && q_city.val() == '') {
		$("label[for='q_city']").addClass('required');
		return site_popup_message(error_caption, 'Stad måste fyllas i.');
	}

	var q_number_label = $("label[for='q_number']");
	if (q_number.val() == '') {
		q_number_label.addClass('required');
		return site_popup_message(error_caption, 'Ni måste ange spamskyddsnummer.');
	}

	if ((parseInt(q_number.val()) + 666) != parseInt($('#another_number').val())) {
		q_number_label.addClass('required');
		return site_popup_message(error_caption, 'Ni har angett fel spamskyddsnummer.');
	}

	if (q_accept_terms.is(':checked') == false) {
		return site_popup_message(error_caption, 'Ni måste acceptera Lawlines villkor.');
	}

	if ($q_premium.is(':checked')) {
		var ajax_data = '&email='+q_email.val();
		$.ajax({
			type: 'POST',
			url: '/ajax_handler',
			data: 'method=premium_check'+ajax_data,
			dataType: 'json',
			success: function(data) {
				if (data.errors.length) return alert_errors(data.errors);
				$('#form_question').submit();
				return true;
			}
		});
	} else {
		$('#form_question').submit();
	}

	return false;
}
</script>
