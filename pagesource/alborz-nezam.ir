    <script type="text/javascript">
        function SendVote() {
            var ck=0;
            var radio = document.getElementsByName("RdoVoteItem"); 
            for (var j = 0; j < radio.length; j++) { 
                if (radio[j].checked) { 
                    ck = 1;
                    AddMyVote(document.getElementById('hdnOpinion').value,radio[j].value);
                }
            }
            if (ck == 0)
                alert('لطفا یکی از گزینه ها را انتخاب نمایید');
        }

        function AddMyVote(VoteId,MyOp) {
            dvresualt.innerHTML = 'لطفا کمی صبر کنید';
            var xmlhttp;
            if (window.XMLHttpRequest) {
                xmlhttp = new XMLHttpRequest();
            }
            else {
                xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
            }
            xmlhttp.onreadystatechange = function () {
                if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
                    if (xmlhttp.responseText == '0') {
                        return;
                    }
                    else {
                        dvresualt.innerHTML = xmlhttp.responseText;
                    }
                }
            }
            var Ip = document.getElementsByName("hdnIp");
            xmlhttp.open("GET", "SiteAjax.aspx?check=65&like=" + MyOp + "&RecId=" + VoteId + "&Ip=" + Ip.value , true);
            xmlhttp.send();
            ViewLike();
        }
        function OpenVote() {
            var Id = document.getElementsByName("hdnOpinion");
