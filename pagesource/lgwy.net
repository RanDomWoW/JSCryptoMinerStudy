	$(function() {
		$('.sjh .content li').hide();
		$('.sjh .content li:eq(0)').show();
		$('.sjh .content ul li:eq(0)').show().siblings().hide();
		$(".sjh .tab a").mouseover(function() {
			$(this).addClass('on').siblings().removeClass('on');
			var index = $(this).index();
			console.log(index);
			number = index;
            if(index!=8){
                $('.sjh .content li').hide();
                $('.sjh .content li:eq(' + index + ')').show();
            }
		});

		var auto = 1; //等于1则自动切换，其他任意数字则不自动切换
		if (auto == 1) {
			var number = 0;
			var maxNumber = $('.sjh .tab a').length;
			function autotab() {
				number++;
                var number1 = number;
				number == maxNumber ? number = 0 : number;
				if(number1!=maxNumber-1){
                    $('.sjh .tab a:eq(' + number + ')').addClass('on').siblings()
                            .removeClass('on');
                    $('.sjh .content ul li:eq(' + number + ')').show().siblings()
                            .hide();
                }

			}

			var tabChange = setInterval(autotab, 3000);
			//tabChange = setInterval(autotab,1);
			clearInterval(tabChange);
			//鼠标悬停暂停切换
			$('.sjh').mouseover(function() {
				clearInterval(tabChange);
			});
			$('.sjh').mouseout(function() {
				tabChange = setInterval(autotab, 3000);

			});
		}
	});
</script>
