	$('body').prepend(scroll_header_navi);
	var numbers1 = $('#headContainer>#header>.logo+div>span:nth-child(1)').text();
	var numbers2 = $('#headContainer>#header>.logo+div>span:nth-child(2)').text();
	$('#numbers>span#numb1').append(numbers1);
	$('#numbers>span#numb2').append(numbers2);
	var global_navi_clone = $('#globalNavi').clone();
	$('#scroll_header').append(global_navi_clone);
	$('#scroll_header>.site_title>p').text($('#headContainer>h1').text());
	$('#scroll_header>.head .info').css({background:'url('+$('#headContainer>#header>div+p>img').attr('src')+') 184px 4px no-repeat'});
	scroll_header_function();
	$(window).scroll(function(){
		scroll_header_function();
	});
	$('#scroll_header #globalNavi>div>ul>li').hover(function(){
		if($(this).children('ul').length>0){
			$(this).children('ul').stop().slideDown(200);
		}
	},function(){
		if($(this).children('ul').length>0){
			$(this).children('ul').stop().slideUp(200,function(){
				$(this).removeAttr('style');
			});
		}
	});
	$('#scroll_header #globalNavi>div>ul>li img').hover(function(){
		$(this).attr('src',$(this).attr('src').replace('_off.','_on.'));
	},function(){
		$(this).attr('src',$(this).attr('src').replace('_on.','_off.'));
	});
	
});
function scroll_header_function(){
	var SP = $(window).scrollTop();
	if(SP>160 && $('#scroll_header').attr('object-active')!='on'){
		$('#scroll_header').attr('object-active','on').stop().fadeIn(200);
	}else if(SP<160 && $('#scroll_header').attr('object-active')=='on'){
		$('#scroll_header').removeAttr('object-active').stop().fadeOut(100);
	}
}
</script>

