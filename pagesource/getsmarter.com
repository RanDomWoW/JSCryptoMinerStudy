  }
  return utmCookieString.replace(/\|$/, '');
}

var campaignSource = getParam('utm_source');
var campaignMedium = getParam('utm_medium');
var campaignName = getParam('utm_campaign');
var campaignContent = getParam('utm_content');
var campaignTerm = getParam('utm_term');
var gclid = getParam('gclid');

var utmCookieString = generateUtmCookieString(campaignSource, campaignMedium, campaignName, campaignContent, campaignTerm);

if(gclid){
  var date = new Date();
  var gclid_details = {
    gclid: gclid,
    timestamp: date.toISOString()
  };
  setCookie('gclid', encodeURIComponent(JSON.stringify(gclid_details)), 90);
}

if(campaignSource && campaignMedium && campaignName){
  var date = new Date();
  var utm_details = {
    source: campaignSource,
    medium: campaignMedium,
    name: campaignName,
    timestamp: date.toISOString()
  };
  if(campaignContent) {
    utm_details.content = campaignContent
  }
  if(campaignTerm) {
    utm_details.term = campaignTerm
  }
  setCookie('utm', encodeURIComponent(JSON.stringify(utm_details)), 90);
}

if(utmCookieString){
  setCookie('__utmz', utmCookieString, 90);
} </script><script src="https://www.youtube.com/iframe_api" async="async"></script><script src="https://cdn.optimizely.com/js/7919950618.js" async="async"></script></head><body><noscript><iframe height="0" src="https://www.googletagmanager.com/ns.html?id=GTM-WLWLFTM" style="display:none;visibility:hidden" tabindex="-1" title="No user content" width="0"></iframe></noscript><div class="inner-wrap" id="first"><a class="show-on-focus" href="#mainContent"> Skip to Content</a><header id="header" role="banner"><div data-sticky-container=""><div class="no-js" data-options="marginTop:0;" data-sticky="" style="width:100%; z-index:5;"><div class="title-bar nav-title-bar no-js" data-hide-for="medium" data-responsive-toggle="getsmarter-menu"><div class="title-bar-left"><div class="getsmarter-nav-logo"><a href="/"><img alt="GetSmarter" src="/grey.gif" data-original="/assets/getsmarter-2da933614727f16c84cecb9b121eed7003733f1212d8375d134db86e5dcf1b92.png"></a></div></div><div class="title-bar-right"><span><a class="view_course_portfolio_button" href="/courses">BROWSE SHORT COURSES</a></span><div class="gs-menu-button" data-toggle="getsmarter-menu"><button class="menu-icon row"></button><span class="row">MENU</span></div></div></div><div class="top-bar" id="getsmarter-menu"><div class="top-bar-title show-for-medium"><a id="gs-logo" href="/"><img alt="GetSmarter" src="/assets/getsmarter-2da933614727f16c84cecb9b121eed7003733f1212d8375d134db86e5dcf1b92.png" /></a></div><div class="top-bar-right"><ul class="vertical medium-horizontal dropdown menu text-center" data-responsive-menu="accordion medium-dropdown"><li class="show-for-medium"><span><a class="button view_course_portfolio_button" href="/courses">BROWSE SHORT COURSES</a></span></li><li><a href="/what-we-do">WHAT WE DO</a><div class="show-for-medium menu_item_dash"></div><div class="show-for-small-only menu-items-underline"></div></li><li><a href="/what-we-do#universities">UNIVERSITIES</a><div class="show-for-medium menu_item_dash