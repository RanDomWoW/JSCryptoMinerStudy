<script type="text/javascript">
    (function (window) {
        var actionName = 'mailOffersAndDiscountsTracking';
        var actionVariableName = 'cbxMailOffersAndDiscounts';
        var actionVariableValue;

        window.logMailOffersAndDiscounts = function($checkbox) {
            if ($checkbox.is(':checked')) {
                actionVariableValue = "on";
            } else {
                actionVariableValue = "off";
            }
            logAction(actionName).addActionVariable(actionVariableName, actionVariableValue).log();
        };

    })(window);
</script><script type="text/javascript">
function validate() {
	email = document.getElementById('email');
	zip = document.getElementById('zipcode');
	fname = document.getElementById('firstname');
	lname = document.getElementById('lastname');
	emailVal = /^['_a-zA-Z0-9-\+~]+(\.['_a-zA-Z0-9-\+~]+)*@([a-zA-Z_0-9-]+\.)+(([a-zA-Z]{2})|(com|COM|org|ORG|net|NET|edu|EDU|gov|GOV|mil|MIL|biz|BIZ|info|INFO|mobi|MOBI|name|NAME|aero|AERO|asia|ASIA|jobs|JOBS|museum|MUSEUM))$/;
	zipVal   = /^\d{5}?$/g;
	nameVal = /^[a-zA-Z' -]+$/;
	
	if (fname.value != '' && nameVal.test(fname.value) && fname.value != "First Name") {
		if (lname.value != '' && nameVal.test(lname.value) && lname.value != "Last Name") {
			if (zipVal.test(zip.value) && zip.value != "Zip Code") {
				if (emailVal.test(email.value) && email.value != "Email") {
					document.form.submit();
				}
				else { alert('Please fill in a valid Email before proceeding.'); }
			}
			else { alert('Please fill in a valid 5 digit Zip Code before proceeding.'); }
		}
		else { alert('Please fill in a valid Last Name before proceeding.'); }
	}
	else { alert('Please fill in a valid First Name before proceeding.'); }
}
</script>
