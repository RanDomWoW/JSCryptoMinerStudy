<script>
    $(document).ready(function () {
        // 视差滚动
        $('.parallax-dcgs-wrapper').parallax({
            imageSrc: "https://source.vipabc.com/ext/images/website/tutorabc/official/dcgs_learn_parallax_bg.jpg",
            naturalWidth: 1.92, //背景图比例
            naturalHeight: 1,
            speed: 0.2 //背景滚动速率
        });
        $('.parallax-recommend-wrapper').parallax({
            imageSrc: "https://source.vipabc.com/ext/images/website/tutorabc/official/recommend_parallax_bg.jpg",
            naturalWidth: 2.4, //背景图比例
            naturalHeight: 1,
            speed: 0.2 //背景滚动速率
        });
        
        // Swiper组件
        var swiperContainerIndex = new Swiper('.swiper-container-index', {
            pagination: '.pagination',
            paginationClickable: true,
            autoplay: 5000,
            autoplayDisableOnInteraction : false,
            loop: true,
            lazyLoading : true,
            lazyLoadingOnTransitionStart : true
        });

        var swiperContainerMacbookDcgs = new Swiper('.swiper-container-macbook-dcgs', {
            pagination: '.pagination-macbook-dcgs',
            paginationClickable: true,
            autoplay: 5000, //可选选项，自动滑动
            autoplayDisableOnInteraction : false,
            loop: true
        });

        // 卡片数字滚动
        var optionsCountUp = {
            useEasing: false,
            useGrouping: true,
            separator: ',',
            decimal: '.',
        };
        var cardCountUp1 = new CountUp("cardCountUp1", 0, 20000, 0, 1.5, optionsCountUp);
        var cardCountUp2 = new CountUp("cardCountUp2", 0, 100, 0, 1.5, optionsCountUp);
        var cardCountUp3 = new CountUp("cardCountUp3", 0, 95, 0, 1.5, optionsCountUp);
        var cardCountUp4 = new CountUp("cardCountUp4", 0, 80, 0, 1.5, optionsCountUp);

        // 监听滚动事件
        $("#sidebarIndexOne").addClass("fadeInLeft");

        var cardInfoWrapper = $(".card-info-wrapper").offset().top - ($(window).height() - 300);
        var dcgsAnimation = $(".macbook-dcgs-container").offset().top - ($(window).height() / 2);

        $(window).scroll(function() {

            if ($(this).scrollTop() > cardInfoWrapper) {
                cardCountUp1.start();
                cardCountUp2.start();
                cardCountUp3.start();
                cardCountUp4.start();
            }

            if ($(this).scrollTop() > dcgsAnimation) {
                $(".macbook-dcgs-container").attr('id', 'animationDCGS');
            }

        });

        //视频修复
            var yaomingVideo = document.getElementById("yaomingVideo");
            var yaomingVideo_html5_api = document.getElementById("yaomingVideo_html5_api");
            var doctorLiVideo = document.getElementById("doctorLiVideo");
            var doctorLiVideo_html5_api = document.getElementById("doctorLiVideo_html5_api");
            var CEOVideo = document.getElementById("CEOVideo");
            var CEOVideo_html5_api = document.getElementById("CEOVideo_html5_api");

            $('#yaomingVideoModal').on('shown.bs.modal', function () {
                yaomingVideo_html5_api.play();
                yaomingVideo.play();
            });
            $('#yaomingVideoModal').on('hidden.bs.modal', function () {
                yaomingVideo_html5_api.pause();
                yaomingVideo.pause();
            });
            $('#doctorLiVideoModal').on('shown.bs.modal', function () {
                doctorLiVideo_html5_api.play();
                doctorLiVideo.play();
            });
            $('#doctorLiVideoModal').on('hidden.bs.modal', function () {
                doctorLiVideo_html5_api.pause();
                doctorLiVideo.pause();
            });
            $('#CEOVideoModal').on('shown.bs.modal', function () {
                CEOVideo_html5_api.play();
                CEOVideo.play();
            });
            $('#CEOVideoModal').on('hidden.bs.modal', function () {
                CEOVideo_html5_api.pause();
                CEOVideo.pause();
            });

        // 跑马灯
        function openModalWrap(object) {
            if (object.is(":visible") == true) {
                return false;
            }
            var topH = $(window).height() / 2.0 - object.height() / 2.0;
            var _h = 40;
            var _top1 = topH - _h;
            var _top2 = topH;
            object.animate({
                top: _top1 + 'px',
                opacity: 0
            }, 0, function () {
                $(this).show();
            }).animate({
                top: _top2 + 'px',
                opacity: 1
            }, 600);

        };

        var getQueryString = function (name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
            var r = window.location.search.substr(1).match(reg);
            //alert(r);
            if (r != null) {
                return decodeURIComponent(r[2]);
            }
            return null;
        };

        if (!navigator.userAgent.match(/mobile/i) || navigator.userAgent.match(/iPad/i)) {
            gotoSite();
            //close Modal Wrap lightBox
            $(".lightBox-close,.drainage-close").click(function () {

                if (document.getElementById("chk_users_site_select").checked) {
                    $.removeCookie("hiddenTen");
                    $.cookie('hiddenTen', true, { expires: 10 });
                }

                $(".lightBox-bg, .lightBox-box").fadeOut(100);

            });

            $(".drainage-div a").on('click', function (e) {
                var site_name = $(this).attr('data-name');
                if (document.getElementById("chk_users_site_select").checked) {
                    $.removeCookie("hiddenTen");
                    $.cookie('hiddenTen', true, { expires: 10 });
                }
                if (site_name == "select_adult") {
                    $(".lightBox-bg, .lightBox-box").fadeOut(100);
                    return false;
                }
            });
            addCookie('HomePageHasView', 'Y',-1);
        };
        function gotoSite() {
            //cookie页面分流
            var drainagebox = getQueryString("drainagebox");
            if (drainagebox == "false") { return false; }
            $(".lightBox-bg").show();
            openModalWrap($(".lightBox-box.site-drainage"));
            var exec = $.cookie("hiddenTen");
            var hasview = '10000';
            if(getCookie("HomePageHasView")=="Y")
            {
                hasview = '0';
            }
            if (exec === "true" || hasview != "10000") {
                $(".lightBox-bg, .lightBox-box").hide();
            }
            
        }
    });

    $(window).load(function() {

        var sidebarIndexTwo = $("#sidebarIndexTwo").offset().top - ($(window).height() + 100);
        var sidebarIndexThree = $("#sidebarIndexThree").offset().top - ($(window).height() + 100);

        $(window).scroll(function() {
            if ($(this).scrollTop() > sidebarIndexTwo) {
                $("#sidebarIndexTwo").addClass("fadeInLeft");
            }
            if ($(this).scrollTop() > sidebarIndexThree) {
                $("#sidebarIndexThree").addClass("fadeInLeft");
            }
        });
    });

