<script>
function defer(method) {
    if (typeof jQuery != "undefined")
        method();
    else
        setTimeout(function() { defer(method) }, 50);
}
defer(function(){
	var webinarproduct = "RegFox";
	var webinarurl = "https://s3-us-west-2.amazonaws.com/assets.webconnex.com/data/webinars.json";
	var demos = [];
	var organizers = {
		"John Russell": "https://1ckxmv4dba6s1dz21zkplqa1-wpengine.netdna-ssl.com/wp-content/uploads/sites/4/2016/06/heashot_jr2.png",
		"Ashley Mellott": "/wp-content/uploads/sites/2/2016/06/headshot_ash.png",
		"Katie Runyan": "https://1ckxmv4dba6s1dz21zkplqa1-wpengine.netdna-ssl.com/wp-content/uploads/sites/5/2016/06/headshot_katie.png",
                "Eric Knopf": "https://1ckxmv4dba6s1dz21zkplqa1-wpengine.netdna-ssl.com/wp-content/uploads/sites/4/2016/08/headshot_ek.png"
	}
	jQuery.getJSON(webinarurl,function(result){
		var dateFormat = "YYYY-MM-DDTHH:mm:ssZ";
		jQuery.each(result,function(i,v){
			var parts = (v.subject || "").split(" ");
			if(parts.length == 0 || parts[0] != webinarproduct || !v.registrationUrl){
				return;
			}
			var time = v.times && v.times.length > 0 ? v.times[0] : {};
			if(!time.startTime || !time.endTime){
				return;
			}
			demos.push({
				subject: v.subject,
				link: v.registrationUrl,
				start: moment(time.startTime,dateFormat),
				end: moment(time.endTime,dateFormat),
				organizer: v.organizerName,
			});
		});
		demos.sort(function(a, b) {
			if (a.start.unix() > b.start.unix()) {
				return 1
			} else if (a.start.unix() < b.start.unix()) {
				return -1
			}
			return 0;
		});
		var future_demos = [];
		jQuery.each(demos,function(i,v){
			if (v.start.unix() > moment().unix()) {
				future_demos.push(v);
			}
		});
		for (var i = 0; i < 2; i++) {
			var demo = future_demos[i];
			if (!demo) {
				break;
			}

			var start = demo.start
			var end = demo.end;

			var dowtime = start.format("dddd [at] hA").toUpperCase();
			var datetime = start.format("dddd MMMM D YYYY, h:mmA - ") + end.format("h:mmA ") + " (" + moment().tz(moment.tz.guess()).format("z") + ")";
			var hostname = "Hosted By " + demo.organizer
			var hostimg = organizers[demo.organizer] || "";
			var link = demo.link;

			var sel = "demo_" + (i + 1) + "_";

			document.getElementById(sel + "dowtime").innerHTML = dowtime;
			document.getElementById(sel + "datetime").innerHTML = datetime;
			document.getElementById(sel + "hostname").innerHTML = hostname;
			document.getElementById(sel + "hostimg").firstChild.src = hostimg;
			document.getElementById(sel + "link").href = link;
console.log(document.getElementById(sel + "link"))
		}
