										$( link ).focus();
									}, {
										scope: 'publish_actions,user_likes'
									} );
								}
							} else {
								$( link ).removeClass( 'fb-loading' );
								if ( rsp.id )
									$( link ).addClass( 'fb-liked' );
							}
						} );
					} else {
					}
				} );

