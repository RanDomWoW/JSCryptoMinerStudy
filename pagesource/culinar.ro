<script type="text/javascript" charset="utf-8" src="/assets/push/push.js?t=1521511635"></script><script type="text/javascript">
    var newsletterInterval = null;
    var sideBarNlOpen      = false;
    // The validations
    function sideNlIsValidEmail(email) {
        filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
        return filter.test(email);
    };
    // Submit the NL
    function sideNlDoSubmitDetails(formHolderId) {
        mainSubmitHolder = null;
        if (typeof formHolderId !== "undefined" && formHolderId.length > 0) {
            mainSubmitHolder = $(formHolderId);
        };
        sendUserEmail = $(".side-nl-box-email", mainSubmitHolder).val();
        if (sendUserEmail.length > 0 && sideNlIsValidEmail(sendUserEmail) === true) {
            $.ajax({
                type: "POST",
                dataType: "json",
                url: "/api/newsletter/subscribe",
                data: mainSubmitHolder.serialize(),
                success: function(data) {
                    // The finish ok
                    $('.side-nl-text').html('<span class="text-success">Ati fost abonat!<br/>Va multumim!</span>');
                },
                error: function(data) {
                    $('.side-nl-text').html('<span class="text-danger">Eroare</span>');
                }
            });
        } else {
            $('.side-nl-text').html('<span class="text-danger">Adresa de email nu este corecta</span>');
            $('.side-nl-box-email').focus();
        };
        return false;
    };
    // Start the shake
    function sideNewsletterShakeStart() {
        newsletterInterval = setInterval(function() {
            $('.sidebar-trigger').trigger('startRumble');
            setTimeout(function() {
                $('.sidebar-trigger').trigger('stopRumble');
            }, 3000);
        }, 10000);
    };
    // Stop the shake
    function sideNewsletterShakeStop() {
        $('.sidebar-trigger').trigger('stopRumble');
    };
    // Blur the sides
    function sideDoBlurBody() {
        $('.container, .main-nav, .search-nav, #main-ajax-container').addClass('side-nl-blur');
    };
    // Unblur
    function sideDoUnBlurBody() {
        $('.container, .main-nav, .search-nav, #main-ajax-container').addClass('side-nl-unblur');
        setTimeout(function() {
            $('.container, .main-nav, .search-nav, #main-ajax-container').removeClass('side-nl-blur').removeClass('side-nl-unblur');
        }, 900);
    };
    // The document start
    $(document).ready(function() {
        // Shake it
        $('.sidebar-trigger').jrumble({x: -1, y: 0, rotation: -1, speed: 1});
        sideNewsletterShakeStart();
        // The tooltips
        $('.sidebar-trigger').tooltipster({
            theme: 'tooltipster-punk',
            side: 'top',
            interactive: false,
            autoClose: false,
            trackOrigin: true,
            multiple: false,
            trigger: 'custom',
            functionInit: function (origin, tooltipEl) {

                $('.sidebar-trigger').click(function() {
                    // Open
                    if (sideBarNlOpen === false) {
                        sideBarNlOpen = true;
                        // Stop the shaker
                        sideNewsletterShakeStop();
                        window.clearInterval(newsletterInterval);
                        sideNewsletterShakeStop();
                        // Display the content
                        $('.sidebar-nl').animate({left: 0}, function() {
                            // Put focus
                            $('.side-nl-box-email').focus();
                            // Show the tooltip
                            $('.sidebar-trigger').tooltipster('open');
                        });
                        // Blur the
                        sideDoBlurBody();
                    } else {
                        // <ark the variable
                        sideBarNlOpen = false;
                        // Close the tooltip
                        $('.sidebar-trigger').tooltipster('close');
                        // Animate
                        $('.sidebar-nl').animate({left: -272}, function() {
                            // Start the shaker
                            sideNewsletterShakeStart();
                        });
                        // UnBlur the content
                        sideDoUnBlurBody();
                    };

                });
            }
        });

    });
</script>
