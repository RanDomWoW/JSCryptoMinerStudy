		//큐레이션 레이아웃
		function curation_layout(v){
			var $curation = $('.curation .inner').isotope({
				itemSelector: '.curation_item',
				layoutMode:'fitRows',
				fitRows: {
					gutter: '.curation_gutter'
				}
			});

			$curation.imagesLoaded().progress( function() {
			  $curation.isotope('layout');
			});
		}

		//모바일 스토리슬라이드
		function story_slide(v){
			if(v == 'M'){
				$('.story .inner').owlCarousel({
					loop: true,
					margin: 20,
					items: 3,
					dots: false,
					center:true
				});
			}else{
				var pv= $('#pv').val();
				var predt= $('#predt').val();
				$('.story').load('ajax_story_list.asp?pv='+pv+"&predt="+predt);
			}
		}

		//모바일 메인슬라이드
		function main_slide(v){
			if(v == 'M'){
				$('.mainMenu ul').owlCarousel({
					loop: true,
					margin: 0,
					items: 1,
					dots: true
				});
			}else{
				var pv= $('#pv').val();
				var predt= $('#predt').val();
				$('.mainMenu').load('main_slide.asp?pv='+pv+"&predt="+predt);
			}
		}

		$(document).ready(function () {
			var width = $(window).width();
			curation_layout('P');
			if(width > 740){
				//main_slide('P');
				story_slide('P'); // 비동기 방식으로 story 호출 →토익 성적 발표 시 주석처리 하여 html 생성 (2017-04-27 서윤형)
			}else{
				main_slide('M');
				//curation_layout('M');
				story_slide('M'); // 비동기 방식으로 story 호출 → // 토익 성적 발표 시 주석처리 하여 html 생성 (2017-04-27 서윤형)
			}
		})

