<script type="text/javascript">        
        var dayList = new Array(7);
        dayList[0] = "Sunday";
        dayList[1] = "Monday";
        dayList[2] = "Tuesday";
        dayList[3] = "Wednesday";
        dayList[4] = "Thursday";
        dayList[5] = "Friday";
        dayList[6] = "Saturday";

        var monthList = new Array(12);
        monthList[0] = "January";
        monthList[1] = "February";
        monthList[2] = "March";
        monthList[3] = "April";
        monthList[4] = "May";
        monthList[5] = "June";
        monthList[6] = "July";
        monthList[7] = "August";
        monthList[8] = "September";
        monthList[9] = "October";
        monthList[10] = "November";
        monthList[11] = "December";

        $(document).ready(function() {
            // var hpEventDate = jQuery.trim($(".hp-event-date").html());
			// iOS Safari Web browser treats the raw date format (YYYY-MM-dd) as a phone number and surrounds it with an HTML <a> tag
			var hpEventDate = ( $(".hp-event-date").find("a").length == 0 ) ? jQuery.trim( $(".hp-event-date").html() ) : jQuery.trim( $(".hp-event-date a").html() );
			if (hpEventDate.search("&nbsp;") >= 0) {
				var startDate = hpEventDate.split("&nbsp;")[0];
				var endDate = hpEventDate.split("&nbsp;")[1];
				var startYear = startDate.split("-")[0];
				var startMonth = startDate.split("-")[1];
				var startDay = startDate.split("-")[2];
				//get rid of '0' prefix if day is less than 10
				if (startDay.length == 2) {
					if (startDay.charAt(0) == '0') {
						startDay = startDay.charAt(1);
					}
				}
				var formattedStartDate = new Date(startYear, startMonth - 1, startDay, 0, 0, 0, 0);
				var startDayLiteral = dayList[formattedStartDate.getDay()];
				var startMonthLiteral = monthList[formattedStartDate.getMonth()];
				formattedStartDate = startMonthLiteral + " " + startDay + ", " + startYear;
				
				var endYear = endDate.split("-")[0];
				var endMonth = endDate.split("-")[1];
				var endDay = endDate.split("-")[2];
				//get rid of '0' prefix if day is less than 10
				if (endDay.length == 2) {
					if (endDay.charAt(0) == '0') {
						endDay = endDay.charAt(1);
					}
				}
				var formattedEndDate = new Date(endYear, endMonth - 1, endDay, 0, 0, 0, 0);
				var endDayLiteral = dayList[formattedEndDate.getDay()];
				var endMonthLiteral = monthList[formattedEndDate.getMonth()];
				formattedEndDate = endMonthLiteral + " " + endDay + ", " + endYear;				
				
				if(formattedStartDate != formattedEndDate) {
					$(".hp-event-date").html(formattedStartDate + "&nbsp;-&nbsp;" +  formattedEndDate);							
				} else {
					$(".hp-event-date").html(formattedStartDate);							
				}
			} else {
				var year = hpEventDate.split("-")[0];
				var month = hpEventDate.split("-")[1];
				var day = hpEventDate.split("-")[2];
				//get rid of '0' prefix if day is less than 10
				if (day.length == 2) {
					if (day.charAt(0) == '0') {
						day = day.charAt(1);
					}
				}
				var formattedDate = new Date(year, month - 1, day, 0, 0, 0, 0);
				var dayLiteral = dayList[formattedDate.getDay()];
				var monthLiteral = monthList[formattedDate.getMonth()];
				formattedDate = monthLiteral + " " + day + ", " + year
				$(".hp-event-date").html(formattedDate);			
			}
            
            //grayOut(true, '', 'Y');
        });
    </script>
