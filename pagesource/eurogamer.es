						var yafaList=document.querySelectorAll('[data-yafa-click]');
						
						for (var i=0; i < yafaList.length; i++) {
							var yafa=yafaList[i];
							var yafa_click=yafa.getAttribute('data-yafa-click');
							
							
							j('#page-wrapper').each(function () {
							this.style.setProperty( 'background', 'url('+yafa.getAttribute('data-yafa-img')+') white no-repeat 50% 0', 'important' );
							this.style.setProperty( 'padding-top', '200px', 'important' );
							});
							j('html').bind('mousemove', function(e) {
								if( (e.target==j('#page-wrapper')[0]) && (e.pageY <=1200 ) ) {
										j('html').css({ 'cursor' :'pointer' });
								} else {
										j('html').css({ 'cursor' :'auto' });
								}
							});
							j('html').bind('click', function (e) { 
								if( (e.target==j('#page-wrapper')[0]) && (e.pageY <=1200 ) ) {
									window.open(yafa_click,'_top');
								}
							});
							yafa.removeAttribute('data-yafa-img');
							yafa.removeAttribute('data-yafa-click');
						}
						j('.advertContainer').hide();
						j('#skyscraper').hide();
						j('#leaderboard').hide();
					}
