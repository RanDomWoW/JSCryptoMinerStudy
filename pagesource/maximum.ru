    function fix(){
      var height = 1000; 
      var imgs = $('.foto-gallery img'); 
      for(var index=0; index<imgs.length; index++){ 
          var h = parseInt($(imgs[index]).css('height')); 
          if(h<height)height = h; 
      } 
      var arr = new Array();
      imgs.each(function(index){ 
          var w = $(this).width();
          var h = $(this).height();
          console.log(h + " " + height);
          if(h > 3*height)
              arr.push(index);
          }
      );
      console.log(arr);
      for(var index = 0; index < arr.length; index++){ 
          var item = $(imgs[arr[index]]);
          var w = item.width()*height/item.height();
          item.width(w);	
          item.height(height);	
      }
	}
    
