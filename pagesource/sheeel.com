<script type="text/javascript">

    (function() {

        var state = "";
        enquire.register('(max-width: 800px)', {
            match: function () {
                state =  "slide";
            },
            unmatch: function () {
                state =  "more";
            }
        });


        //slide state
        if(state == "slide") {

            var scrollContainer = $("nav-container");
            var activeElement = $$("#nav li.active").first();

            //no scroll check?
            //if(scrollContainer.scrollWidth <= scrollContainer.offsetWidth) { return; }

            $("navigation-wrapper").addClassName("slide");

            //move active to middle
            var leftOffset;
            if(document.dir == "rtl") {
                leftOffset = activeElement.positionedOffset().left + scrollContainer.scrollWidth - scrollContainer.offsetWidth;
            } else {
                leftOffset = activeElement.positionedOffset().left;
            }

            scrollContainer.scrollLeft = leftOffset + activeElement.getWidth()/2 - scrollContainer.offsetWidth/2;

            var handleScroll = function() {
                //console.log(scrollContainer.scrollWidth - scrollContainer.offsetWidth);

                if(scrollContainer.scrollLeft <= 0) {
                    scrollContainer.addClassName("left-end")
                    //console.log("start");
                } else if(scrollContainer.scrollLeft >= scrollContainer.scrollWidth - scrollContainer.offsetWidth - 1) { //1px diff? padding?
                    scrollContainer.addClassName("right-end")
                    //console.log("end");
                } else {
                    scrollContainer.removeClassName("right-end");
                    scrollContainer.removeClassName("left-end");
                    //console.log("other");
                }
            };

            //react on scroll start, end, middle
            scrollContainer.observe("scroll", handleScroll);

            handleScroll();


            var ShowHint = parseInt(localStorage.getItem('shown'));

            if (ShowHint) {
                if (ShowHint < 3) {
                    $("animation-hint").addClassName("animate");
                    ShowHint++;
                    localStorage.setItem('shown', ShowHint);
                }
            } else {
                $("animation-hint").addClassName("animate");
                localStorage.setItem('shown', '1');
            }


            //more state
        } else {

            var elements = $$("#nav li");
            elements.pop(); // remove More li

            var containerWidth = $("nav-container").getWidth();
            var moreWidth = $("nav-more").getWidth();

            //menu is breaking on this widths
            var menuBreakes = $A([]);
            var width = 0;
            elements.each(function(el) {
                width += el.getWidth();
                //menuBreakes.push(el.positionedOffset().left + el.getWidth());
                menuBreakes.push(width);
            });

            //number above means no break
            var currentBreak = elements.length;

            var handleMore = function() {

                var newBreak = elements.length;

                //resolve newBreak value
                menuBreakes.each(function(breakk, i) {
                    if(breakk > containerWidth - moreWidth) {
                        newBreak = i;
                        throw $break;
                    }
                });

                //console.log(newBreak, currentBreak);

                //if currentBreak isn't changed, do nothing
                if(newBreak === currentBreak) {
                    return;

                    //restore from more
                } else if(newBreak > currentBreak) {
                    var moreElements = $$("#nav-more ul li");

                    for(var i = 0; i < newBreak-currentBreak; i++) {
                        $("nav-more").insert({
                            before: moreElements[i]
                        });
                    }

                    //move to more
                } else {

                    for(var i = currentBreak-1; i >= newBreak; i--) {
                        $$("#nav-more ul")[0].insert({
                            top: elements[i]
                        });
                    }
                }

                if(newBreak == elements.length) {
                    $("nav-more").addClassName("hidden");
                } else {
                    $("nav-more").removeClassName("hidden");
                }

                currentBreak = newBreak;
            }

            //initial state
            handleMore();

            //hadle more on resize
            var timer = false;
            Event.observe(window, "resize", function() {
                if(timer) {
                    clearTimeout(timer);
                }

                timer = setTimeout( function() {
                    containerWidth = $("nav-container").getWidth();
                    handleMore();
                }, 200);
            });


            $("show-more").observe("click", function (event) {
                event.preventDefault();
                $("hidden-links").toggle();
            });

        }


    })();

</script>    </div>

