    <!-- preloader styles -->
    <style>
      * {
        box-sizing: border-box;
      }

      .loader-wrapper {
        display: flex;
        justify-content: center;
        height: 100vh;
        align-items: center;
      }

      .loader,
      .loader:before,
      .loader:after {
        border-radius: 50%;
        -webkit-animation: load 25s infinite;
        animation: load 25s infinite;
      }
      .loader {
        color: #D6DBE0;
        font-size: 11px;
        text-indent: -99999em;
        margin: auto;
        position: relative;
        width: 3rem;
        height: 3rem;
        box-shadow: inset 0 0 0 3px;
        /*-webkit-transform: translateZ(0);
        -ms-transform: translateZ(0);
        transform: translateZ(0);*/
      }
      .loader:before,
      .loader:after {
        position: absolute;
        content: '';
      }
      .loader:before {
        width: 1.6rem;
        height: 3.2rem;
        background: white;
        border-radius: 3.2rem 0 0 3.2rem;
        top: -0.05rem;
        left: -0.05rem;
        -webkit-transform-origin: 1.6rem 1.55rem;
        transform-origin: 1.6rem 1.55rem;
        -webkit-animation: load 1.5s infinite ease 1.125s;
        animation: load 1.5s infinite ease 1.125s;
      }
      .loader:after {
        width: 1.6rem;
        height: 3.2rem;
        background: white;
        border-radius: 0 3.2rem 3.2rem 0;
        top: -0.05rem;
        left: 1.55rem;
        -webkit-transform-origin: 0 1.55rem;
        transform-origin: 0 1.55rem;
        -webkit-animation: load 1.5s infinite ease;
        animation: load 1.5s infinite ease;
      }
      @-webkit-keyframes load {
        0% {
          -webkit-transform: rotate(0deg);
          transform: rotate(0deg);
        }
        100% {
          -webkit-transform: rotate(360deg);
          transform: rotate(360deg);
        }
      }
      @keyframes load {
        0% {
          -webkit-transform: rotate(0deg);
          transform: rotate(0deg);
        }
        100% {
          -webkit-transform: rotate(360deg);
          transform: rotate(360deg);
        }
      }
    </style>
