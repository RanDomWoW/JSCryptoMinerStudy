    $(document).ready(function () {



        $("#newpost-popover").popover({ trigger: 'focus' });
        $("#newpost-popover").popover('show');

        LoadRemoteImages();

        //$(".js-centered").center(true);

        $(".square").each(function () {
            $(this).height($(this).width());
        });

        $(".ad-close-btn").click(function () {
            $("#dvModAdContainer").hide();
            $("#dvModIframeContainer").addClass("col-md-12");

            var height = $("#modSourceBody").height();

            $("#ifrSource").height(height);

        });

        $(".source-frame").click(function () {

            $("#dvModAdContainer").show();
            $("#dvModIframeContainer").removeClass("col-md-12");

            $("#ifrSource").attr("src", $(this).data("source"));

            $("#source-title").html($(this).data("title"));

            $("#modSource").modal("show");



        });

        $("#ifrSource").load(function () {
            var height = $("#modSourceBody").height();

            $("#ifrSource").height(height);
        });

        $(".ad").each(function () {

            var val = $(this).html().replace(/\r\n|\r|\n/g, '').replace(/ /g, '');

            if (val.length < 1) { $(this).remove(); }

        });

        $(".adPost").each(function () {

            var val = $(this).html().replace(/\r\n|\r|\n/g, '').replace(/ /g, '');

            if (val.length < 1) { $(this).remove(); }

        });


        if ($(".pCaption").length > 0) $(".pCaption").tweetParser();

        $(".sharer").jsSocials({
            showLabel: true,
            showCount: true,
            shareIn: "popup",
            shares: ["twitter", "facebook", "googleplus", "linkedin", "stumbleupon", "whatsapp"]
        });
    });

    $(function () {
        $("iframe").load(function () {
            if ($.trim($(this).contents().find("body").html()) == "") {
                $(this).closest("div").hide();
            }
        });

        $('#main-menu').metisMenu();

        $('[data-toggle="tooltip"]').tooltip();
        $('[data-toggle="popover"]').popover({
            trigger: 'focus'
        });
    });

    var firstParent;
    var firstTop;

    if ($(".fixed").length > 0) {

        firstParent = $(".fixed").parent();
        firstTop = firstParent.offset().top;

    }

    $("#frmSearch").submit(function (e) {
        e.preventDefault();

        location.href = "/search/" + $("#inpSearch").val();

        return false;
    });

    $(window).scroll(function () {

        if ($(document).scrollTop() > firstTop) {
            $(".fixed").addClass("top");
        }
        else {
            $(".fixed").removeClass("top");
        }
        
    });

    function LoadRemoteImages() {

        $(".rssImage").each(function (i, img) {

            if ($(img).data("src").length > 0) {
                var downloadingImage = new Image();

                downloadingImage.onload = function () {

                    $(img).attr("src", this.src);

                };

                downloadingImage.src = $(img).data("src").replace("file://", "http://");
            }
            else {
                $(img).remove();
            }

        });
    }

