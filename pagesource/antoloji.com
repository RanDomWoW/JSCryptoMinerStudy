        function SiiriBegen(siirID, puan) {

            if (siirID < 0)
                return false;


            var url = "/dinamik/Antoloji/SiirePuanVer";
            var obj = new Object();
            obj.puan = puan;
            obj.siirId = siirID;

            AjaxAntolojim(url, obj);
