    (function() {
        var loadDeferredImages = function() {
            var ATTR_LAZYIMG_SRC = 'data-lazyimg-src';
            var ATTR_LAZYIMG_SRCSET = 'data-lazyimg-srcset';
            var ATTR_LAZYIMG_PRIORITY = 'data-lazyimg-priority';

            deferredImgs = Array.prototype.slice.call(document.querySelectorAll('img['+ATTR_LAZYIMG_SRC+']'));

            if ( deferredImgs.length ) {
                deferredImgs.sort(function(a, b) {
                    var p1 = parseInt(a.getAttribute(ATTR_LAZYIMG_PRIORITY)) || 5;
                    var p2 = parseInt(b.getAttribute(ATTR_LAZYIMG_PRIORITY)) || 5;

                    if ( p1 < p2 ) return -1;
                    if ( p1 > p2 ) return 1;

                    return 0;
                });

                deferredImgs.forEach(function(next) {

                    // load the srcset first to ensure it takes priorirty in rendering
                    var srcset = next.getAttribute(ATTR_LAZYIMG_SRCSET);
                    if ( srcset ) {
                        next.setAttribute('srcset', srcset);
                        next.removeAttribute(ATTR_LAZYIMG_SRCSET);
                    }

                    next.setAttribute('src', next.getAttribute(ATTR_LAZYIMG_SRC));

                    var clearAttributes = function() {
                        next.removeAttribute(ATTR_LAZYIMG_SRC);
                        next.removeAttribute(ATTR_LAZYIMG_PRIORITY);
                    };

                    next.addEventListener('load', clearAttributes);
                    // HELP: I'm not entirely sure this is the right thing to do.  My thinking
                    // is that the data attribute should be removed regardless of the outcome
                    // of the attempt to download the image.  This is mostly since I regard this
                    // as a once off operation and don't want unneeded attributes hanging around
                    // on elements for longer than necessary
                    next.addEventListener('error', clearAttributes);
                });
            }
        };

        window.addEventListener('load', loadDeferredImages);

        var loadDeferredStyles = function() {
            var addStylesNode = document.getElementById("deferred-styles");
            if ( addStylesNode ) {
                var replacement = document.createElement("div");
                replacement.innerHTML = addStylesNode.textContent;
                document.body.appendChild(replacement)
                addStylesNode.parentElement.removeChild(addStylesNode);
            }
        };
        var raf = requestAnimationFrame || mozRequestAnimationFrame ||
            webkitRequestAnimationFrame || msRequestAnimationFrame;

        if (raf) raf(function() { window.setTimeout(loadDeferredStyles, 0); });
        else window.addEventListener('load', loadDeferredStyles);

