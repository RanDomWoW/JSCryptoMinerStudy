// cart pop up
      	    function showcartdiv(obj) {

      	        // Get the Position of Element
      	        var objparentelm = $(obj).parent();
      	        var offset = $(objparentelm).offset();
      	        var top = offset.top + $(objparentelm).height();
      	        var left = $('.view-cart a').offset().left;
      	        var cartwidth = $("#cartdetails ul.hoverCart").width();
      	        var browserwidth = $(window).width();

      	        if ((left + cartwidth) > browserwidth)
      	        {
      	            var extrawidth = (left + cartwidth) - browserwidth;
      	            left = left - extrawidth;
      	        }

      	        $('#cartdetails').css({
      	            'position': 'absolute',
      	            'left': left + "px",
      	            'top': top + "px",
                    'z-index': '999'
      	        });

      	        $('#cartdetails').show();
				$('.view-cart a').css({"background":"#333"});
      	    }

      	    function hidecartdiv() {
      	        $('#cartdetails').hide();
				$('.view-cart a').css({"background":"transparent"});
      	    }

      	    $('#cartdetails').hover(function () {
      	        $(this).show();
				$('.view-cart a').css({"background":"#333"});
      	    }, function () {
      	        $(this).hide();
				$('.view-cart a').css({"background":"transparent"});
      	    });
