<script type="text/javascript">
    $(document).ready(function () {
        /// The cartUpdated event handler
        $('#cartsummary').bind("cartUpdated", function (event, cartStatus) {

            var summaryItems = $("#cartsummaryItems");
            // Clear the line items
            summaryItems.empty();

            // Update the quantity fields to match quantities currently in the cart.
            for (var i = 0; i < cartStatus.LineItems.length; i++) {
                var line = cartStatus.LineItems[i];
                var cloned = $("#lineItemTemplate").clone();
                cloned.removeAttr("style");
                cloned.find(".item-title").html(line.Description);
                cloned.find(".item-quantity").html(line.Quantity + "/" + line.Uom + " @ $" + line.UnitPrice.toFixed(2));
                cloned.find(".item-price").html("$" + line.ExtendedPrice.toFixed(2));
                summaryItems.append(cloned);
            }

            $("#cartsummary .subtotal-amount").html(cartStatus.Subtotal);
            var savings = $("#savings");
            if (cartStatus.TotalSavings != "") {
                savings.show();
                $("#cartsummary .totalSavings").html(cartStatus.TotalSavings);
            }
            else {
                savings.hide();
            }
            //            if (postBackEnabled) {
            //                document.forms[0].submit();
            //            }
        });

    });
</script>

