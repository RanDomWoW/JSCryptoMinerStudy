    jQuery('#button-quote').on('click', function (e) {
        var cp = jQuery('#cp').val();
        var data = {'estimate_postcode': cp};
        if (cp == "" || Number(cp) == 'NaN') {
            e.preventDefault();
            e.stopPropagation();
        }
        var l = Ladda.create(this);
        l.start();
        jQuery.ajax({
            url: '/checkout/cart/estimatePost/',
            dataType: 'json',
            type: 'post',
            data: data,
            success: function (data) {
                jQuery('#minicart-content').html(data.minicart);

            }
        });
        l.stop();
    });

</script>

<script>
    jQuery('button[name="no-usar-saldo"]').on('click', function () {
        jQuery('.minicart-wrapper').addClass('loading');
        jQuery.ajax({
            type: "POST",
            url: "/sterencard/applysterencard/disable",
        }).success(function () {
            reloadMinicartModal();
        });
    });

    jQuery('button[name="usar-saldo"]').on('click', function () {
        jQuery('.minicart-wrapper').addClass('loading');
        jQuery.ajax({
            type: "POST",
            url: "/sterencard/applysterencard/enable",
        }).success(function () {
            reloadMinicartModal();
        });
    })


    function reloadMinicartModal() {
        jQuery.ajax({
            type: "POST",
            url: "/checkout/cart/reloadMinicart",
        }).success(function (response) {
            var minicart = jQuery.parseJSON(response);
            jQuery('#minicart-content').html(minicart.minicart);
        });
    }

    
    function addToCartModal() {

        jQuery('.abono-por-sc').hide();
        jQuery('.minicart-wrapper').addClass('loading');
        /*jQuery('#button-compra-steren-card-modal').button('loading');*/

        var form = {};
        var fk = '9KraLYKDF7Ywg5KJ';
        var qty = 1;
        var url = 'http://www.steren.com.mx/checkout/cart/add/uenc/aHR0cDovL3d3dy5zdGVyZW4uY29tLm14Lw,,/product/4792/form_key/9KraLYKDF7Ywg5KJ/qty/1';

        form['form_key'] = fk;
        form['qty'] = qty;
        form['product'] = 43;

        jQuery.ajax({
            type: "POST",
            url: url,
            data: form,
            success: function (data) {
                var resultJson = jQuery.parseJSON(data);
                jQuery('.cart-items').text(resultJson.output + ' productos');
                jQuery('#minicart-content').html(resultJson.minicart);

                jQuery('#button-compra-steren-card-modal').button('reset');
            }
        });
    }
    
</script>

