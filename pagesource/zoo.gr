	<div id="player"></div>
	<div id="testingPlayer"></div>
	<script>
		var player;
		var testingPlayer;
		var isReady = false;
		var isPlaying = false;
		var whenLoaded = [];
		var readyCount = 0;

		// loads youtube API (if not already loaded) and calls cb when ready
		function loadYoutube(cb) {
			if(isReady)
				return cb();

			var loading = whenLoaded.length != 0;
			whenLoaded.push(cb);
			if(!loading) {
				// load the IFrame Player API code asynchronously.
				var tag = document.createElement('script');
				tag.src = "https://www.youtube.com/iframe_api";
				var firstScriptTag = document.getElementsByTagName('script')[0];
				firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
			}
		}

		// This function creates an <iframe> (and YouTube player) after the API code downloads.
		function onYouTubeIframeAPIReady() {
			player = new YT.Player('player', {
				height: '200',
				width: '200',
				events: {
					'onReady': onPlayerReady,
					'onError': onPlayerError,
					'onStateChange': onPlayerStateChange
				}
			});

			testingPlayer = new YT.Player('testingPlayer', {
				height: '200',
				width: '200',
				events: {
					'onReady': onPlayerReady,	// same as for main player
					'onError': onTestingPlayerError,
					'onStateChange': onTestingPlayerStateChange
				}
			});
		}

		// for BOTH main and testing player
		// call all callbacks when both players are ready
		function onPlayerReady(event) {

			event.target.setVolume(event.target === player ? 100 : 0);	// mute testing player

			isReady = ++readyCount == 2;
			if(isReady) {
				console.log("youtube player is ready!");

				for(var i = 0; i < whenLoaded.length; i++)
					whenLoaded[i]();
			}
		}

		function checkYoutubeVideo(url) {
			loadYoutube(function() {
				var id = getIDfromURL(url);
				console.log("checkYoutubeVideo URL", url, "ID", id);
				testingPlayer.loadVideoById(id);
			});
		}

		function onPlayerError(event) {
			console.log("Youtube video Error: " + Object(event).data);
		}

		function onTestingPlayerError(event) {
			console.log("Youtube video TESTING Error: " + Object(event).data);
			var flashMovie = getFlashMovieObject();
			flashMovie.onYoutubeVidChecked("no");
		}

		function onTestingPlayerStateChange(event) {
			console.log("TESTING onPlayerStateChange: event.data -> " + event.data);
			switch(event.data) {
				case -1: //  -1 (unstarted)

				break;
				case 0: //0 (ended)

				break;
				case 1: //1 (playing)
				testingPlayer.stopVideo();
				console.log("This video is OK! ALLOW.");
				var flashMovie = getFlashMovieObject();
				flashMovie.onYoutubeVidChecked("ok");
				break;
				case 2: //2 (paused)

				break;
				case 3: //3 (buffering)

				break;
				case 5: //5 (video cued)
				break;
			}
		}

		function onPlayerStateChange(event) {
			console.log("onPlayerStateChange: event.data -> " + event.data);
			switch(event.data) {
				case -1: //  -1 (unstarted)
					isPlaying = false;
				break;
				case 0: //0 (ended)
					isPlaying = false;

				break;
				case 1: //1 (playing)
					isPlaying = true;
				break;
				case 2: //2 (paused)
					isPlaying = false;
				break;
				case 3: //3 (buffering)
					isPlaying = false;
				break;
				case 5: //5 (video cued)
					if(!isPlaying)
					 player.playVideo();
				break;
			}
		}

		function stopVideo() {
			console.log("stopVideo.");
			isPlaying = false;
			player.stopVideo();
		}
		function playVideo() {
			console.log("playVideo.");
			isPlaying = true;
			player.playVideo();
		}

		function setVolume(val){
			console.log("setVolume: " + val);
			if(isReady)
				player.setVolume(val);
		}

		function playerCurrentTime(){
			var flashMovie = getFlashMovieObject();

			if(isReady)
				flashMovie.onGetCurrentTime(player.getCurrentTime());
		}

		function pauseVideo(){
			console.log("pauseVideo.");
			if(isReady)
				player.pauseVideo();
		}

		function checkIfPlaying() {
			console.log("isPlaying? " + isPlaying);
			var flashMovie = getFlashMovieObject();
			flashMovie.checkIfPlaying(isPlaying);
		}

		function updateCurrentTrack(youtube_id, secToPlay) {
			loadYoutube(function() {
				console.log("updateCurrentTrack: ", youtube_id, secToPlay, "player:", player, "isPlaying", isPlaying);

				if(isPlaying)
					player.loadVideoById(youtube_id,	secToPlay)
				else
					player.cueVideoById(youtube_id,  secToPlay)
			});
		}

		function getFlashMovieObject() {
			return document.getElementById('zoo_swf');
		}

		function getIDfromURL(url){
			var regExp = /^.*((youtu.be\/)|(v\/)|(\/u\/\w\/)|(embed\/)|(watch\?))\??v?=?([^#\&\?]*).*/;
			var match = url.match(regExp);
			if (match&&match[7].length==11)
					return match[7];

			console.log("Wrong url!");
			return "";
		}
    </script>
