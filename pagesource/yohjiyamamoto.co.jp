	<script type="text/javascript">

	(function( w , d ){


		function movieTimer(){

			var config = {
				times : {
					gmtTime : function(){
						var gmt = ( new Date() ).getTime() + ( new Date() ).getTimezoneOffset() * 60 * 1000;
						return gmt;
					},
					movieTime : ( new Date( 2018, 0, 18, 16, 30, 0 ) ).getTime() ,//動画開始時間
					loadingTime : function(){
						return ( this.movieTime + 3000 );//動画開始後の3秒間
					},
					counterTime : function(){
						return ( this.movieTime + 1000 * 60 * 0 );//動画開始時間の15分後
					},
					fadeTime : 1500 ,//フェードアクション時間
				} ,
				elements : {
					liveWrap : d.getElementById('live_wrap'),
					timeWrap : d.getElementById('time_box'),
					dayWrap : d.getElementById('live_day'),
					hourWrap : d.getElementById('live_hour'),
					minWrap : d.getElementById('live_min'),
					secWrap : d.getElementById('live_sec'),
					movieWrap : d.getElementById('movie_wrap'),
					movieBox : d.getElementById('movie_box'),
					spWrap : d.getElementById('movie_sp_wrap'),
				} ,
				names : {
					doorMenuWrap : '#door-menu-wrapper' ,
					liveBtn : '#live_btn' ,
					movieBtn : '#movie_btn' ,
					movieBtn2 : '#movie_btn2' ,
					movieSpBtn : '#movie_sp_btn',
					movieInner : 'movie_inner' ,
				},
				youtubeId : 'uCsPgeuub4g',
			}

			this.times = config.times;
			this.elems = config.elements;
			this.names = config.names;
			this.youtubeId = config.youtubeId;
		}

		movieTimer.prototype.init = function(){

			var _t = this;

			if( _t.times.gmtTime() < _t.times.movieTime ){//動画開始前

				$( _t.names.doorMenuWrap ).addClass('before');

				//タイムラッパーへのクリックイベント
				$( _t.names.liveBtn ).on( 'click' ,function( e ){

					e.stopPropagation();
					$( _t.elems.liveWrap ).fadeOut( _t.times.fadeTime );

					return false;

				});

				//タイムラッパーへのクリックイベント
				$( _t.names.movieBtn2 ).on( 'click' ,function(){

					$( _t.elems.liveWrap ).fadeIn( _t.times.fadeTime );

					return false;

				});

				$( _t.elems.liveWrap ).fadeIn( _t.times.fadeTime );

			}

			_t.display();
		}

		movieTimer.prototype.isSp = function(){
			return ( ( navigator.userAgent.indexOf('iPhone') > 0 && navigator.userAgent.indexOf('iPad') == -1 ) || navigator.userAgent.indexOf('iPod') > 0 || navigator.userAgent.indexOf('Android') > 0 );
		}

		movieTimer.prototype.changeTime = function( gmt ){

			var _t = this;

			//動画開始時刻から現在の時間を引き , 一日のmsから余りを計算
			var n = ( _t.times.counterTime() - gmt ) % ( 24 * 60 * 60 * 1000 );

			// 残りの時間を計算
			var leftDay = Math.floor( ( _t.times.counterTime() - gmt ) / ( 24 * 60 * 60 * 1000 ) ) * 24; //残り日数を時に変換
			var leftHour = String( Math.floor( n / 3600000 ) + leftDay ); //時
			var leftDate = Math.floor( leftHour / 24 );
			leftHour = String( leftHour - ( leftDate * 24 ) );

			if( leftDate ){
				_t.elems.dayWrap.innerHTML = '<span class="time_' + leftDate + '"></span><div class="time_day">DAYS</div><div class="time_start">START IN</div>';
			}
			else{
				_t.elems.dayWrap.style.display = 'none';
			}

			if( leftHour.length === 1 ) leftHour = '0' + leftHour; //一桁なら0を前に足す
			_t.elems.hourWrap.innerHTML = '<span class="time_' + leftHour[0] + '"></span>' + '<span class="time_' + leftHour[1] + '"></span>';

			var leftMin = String( Math.floor( n / 60000 ) % 60 ); // 分
			if( leftMin.length === 1 ) leftMin = '0' + leftMin; //一桁なら0を前に足す
			_t.elems.minWrap.innerHTML = '<span class="time_' + leftMin[0] + '"></span>' + '<span class="time_' + leftMin[1] + '"></span>';

			var leftSec = String( Math.floor( n / 1000 ) % 60 % 60 );// 秒
			if( leftSec.length === 1 ) leftSec = '0' + leftSec; //一桁なら0を前に足す
			_t.elems.secWrap.innerHTML = '<span class="time_' + leftSec[0] + '"></span>' + '<span class="time_' + leftSec[1] + '"></span>';

		}

		movieTimer.prototype.display = function(){

			var _t = this;

			if( _t.times.gmtTime() < _t.times.movieTime ){

				_t.changeTime( _t.times.gmtTime() );

				$( ".js-pre" ).remove();

				//1秒後に再描画
				setTimeout( function(){ _t.display(); } , 1000 );

			}
			else { //期日後の記述

				if( _t.times.gmtTime() < _t.times.loadingTime() ){//期日後から一定時間はリロードを実行

					setTimeout( function(){
						location.reload();
					} , 500 );

				}else{//5秒後は動画を読み込む処理

					if( _t.times.gmtTime() > _t.times.movieTime + ( 1000 * 60 * 120 ) ){
						$( _t.names.doorMenuWrap ).addClass('after');

						$( ".js-now" ).remove();
						$( ".js-pre" ).show();
					}
					else{
						$( _t.names.doorMenuWrap ).addClass('now');
						$( '.door_brand_item:first-child' ).show();

						$( ".js-now" ).show();
						$( ".js-pre" ).remove();
					}

					$( _t.names.doorMenuWrap ).addClass('active');

					if ( _t.isSp() ){//SP

						//ムービーラッパーへのクリックイベント
						$( _t.names.movieSpBtn ).on( 'click' ,function(){

							$( _t.elems.spWrap ).fadeOut( _t.times.fadeTime );

							return false;

						});

						//ムービーラッパーへのクリックイベント
						$( _t.names.movieBtn2 ).on( 'click' ,function(){

							$( _t.elems.spWrap ).fadeIn( _t.times.fadeTime );

							return false;

						});

						$( _t.elems.spWrap ).fadeIn( _t.times.fadeTime );

					}
					else{//PC

						function onYouTubePlayerAPIReady() {

								var player = new YT.Player( _t.names.movieInner , {
										width: 960,//横幅
										height: 540,//縦幅
										videoId: _t.youtubeId ,//動画id
										playerVars: {
												'autoplay' : 1,//自動再生
												'modestbranding': 1,
												'autohide': 1,
												'controls': 1,
												'showinfo': 0,
												'rel': 0
										},
										//「イベントリスナー」
										events: {
												'onReady': function(){
													player.setVolume( 30 );
												},
										}
								});
						}

						//ムービーラッパーへのクリックイベント
						$( _t.names.movieBtn ).on( 'click' ,function(){

							$( _t.elems.movieWrap ).fadeOut( _t.times.fadeTime ,function(){

								_t.elems.movieBox.innerHTML = '<div id="' + _t.names.movieInner + '"></div>';
							});

							return false;

						});

						//ムービーラッパーへのクリックイベント
						$( _t.names.movieBtn2 ).on( 'click' ,function(){

							$( _t.elems.movieWrap ).fadeIn( _t.times.fadeTime ,function(){

								onYouTubePlayerAPIReady();

							});

							return false;

						});

						$( _t.elems.movieWrap ).fadeIn( _t.times.fadeTime );

						$( w ).on( 'load' , function(){
							onYouTubePlayerAPIReady();
						});

					}

				}

			}

		}

		var mT = new movieTimer();
		mT.init();


		$( w ).on("load",function(){
			var $brandItem = $('.door_brand_item');
			var durationTime = 1200;
			var delayTime = 750;
			
			$brandItem.each(function(i){
				$brandItem.eq( i ).delay( i * delayTime ).animate({
					left: "0px",
					opacity: 1
				} , durationTime );
			});
		});

		$( w ).on("load , resize , ready", function(){

			$doormenu = $("#door-menu-wrapper");

			var wH = $( w ).height();
			var ww = $( w ).width();

			$doormenu.css({'left' : (ww-720)/2 +'px' , 'top' : (wH-360)/2 +'px'});

		});

  })( window , document );

	</script>

	</body>

</html>