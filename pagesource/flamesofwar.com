}

function closeallages(){
    jQuery(".submenu_AgePicker_holder").each(
        function(){
             var theobject = jQuery(this);
          if (theobject.attr("ID") != "submenu_AgePicker_holder"+currentID){
              theobject.fadeOut('fast');
