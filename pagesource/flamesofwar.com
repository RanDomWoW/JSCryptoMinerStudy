function GetSubItems(obj,cgid){
    var theexpanbutton = jQuery(obj)
    var boxid = theexpanbutton.attr("subboxID")
    var btnID = theexpanbutton.attr("ID")
    var thebox = jQuery("#"+boxid)
    var parentID = thebox.attr("parentID")
    var thecontent = thebox.html()
    theexpanbutton.addClass(Nav_expedbtn)
    ResetExpandedButton(Nav_expedbtn,btnID,parentID)
    if (thecontent.length < 15 ){
        thebox.hide();
        thebox.load("online_store.aspx?CategoryID="+cgid+"&target=NavBlock5 #result")
    }
    AjaxTimeOut = AjaxLoadSteps * AjaxLoadInterval;
    window.setTimeout("DoAccordin('"+boxid+"',"+cgid+","+parentID+")",AjaxLoadInterval) 
}
function DoAccordin(boxid,cgid,parentID){
    var thebox = jQuery("#"+boxid)
    var thecontent = thebox.html()
    if (thecontent.length > 15){
        HideMenuBlock("Menublock",boxid,parentID)
        if( thebox.is(":hidden")){
            thebox.slideDown();
        }
        AjaxTimeOut = AjaxLoadSteps * AjaxLoadInterval ;
    }else{
        if ( AjaxTimeOut > 0 ){
            AjaxTimeOut = AjaxTimeOut - AjaxLoadInterval
            window.setTimeout("DoAccordin('"+boxid+"',"+cgid+","+parentID+")",AjaxLoadInterval) 
        }
    }
}
function HideMenuBlock(classtohide,IDtoIngore,parentID){
        jQuery("."+classtohide).each(
            function(){
                var theobject = jQuery(this);
                var myID = theobject.attr("id")
                var myParentID = theobject.attr("parentID")
                if (myID != IDtoIngore && myParentID == parentID){
                       if(!theobject.is(":hidden")){
                            theobject.slideUp();
                       }
                }
            }
        );
}
function ResetExpandedButton(classtohide,IDtoIngore,parentID){
    jQuery("."+classtohide).each(
        function(){
             var theobject = jQuery(this);
                var myID = theobject.attr("id")
                var myParentID = theobject.attr("parentID")
                if (myID != IDtoIngore && myParentID == parentID){
                            theobject.removeClass(classtohide)
                }
                if (myID == IDtoIngore){
                           
                }
        }
    );
}
