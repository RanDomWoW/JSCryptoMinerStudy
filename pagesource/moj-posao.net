		columns: null,

		init: function() {
			this.container = $('#featured-jobs');
			this.columns = $('ul', this.container);

			if (!this.columns.length)
			{
				return;
			}

			adsGrid.reorderAll();
			adsGrid.normalize();

			$(window).bind('resize orientationchange', function () {
				if (w != $(window).width()) {
					//do only on width resize - fixes 'bug' on mobile chrome
					w = $(window).width();
					// check if there are hidden full columns or visible empty ones
					// dont reorder on single column views
					adsGrid.reorderAll();
					adsGrid.normalize();
				}
			});
		},

		/*
		 * normalize column height
		 */
		normalize: function() {

			// dont normalize heights of single column views
			if (this.container.width() <= 480)
				return;

			var elements = [];

			// normalize only visible columns
			var $columns = this.columns.filter(':visible');

			$columns.each(function()
			{
				var $items = $(this).children('li').slice(-5);

				$items.each(function()
				{
					$(this).data('height', $(this).height());

					elements.push($(this));
				});
			});

			elements.sort(function(a, b)
			{
				return b.data('height') - a.data('height');
			});

			for (var i in elements)
			{
				// Remove elements from DOM (keeps stored data)
				elements[i].detach();
			}

			$columns.each(function()
			{
				$(this).data('height', $(this).height());
			});

			var $firstCol = $columns.first();

			for (var i in elements)
			{
				var $smallest = $firstCol;

				$columns.each(function()
				{
					if ($(this).data('height') < $smallest.data('height'))
					{
						$smallest = $(this);
					}
				});

				$smallest.append(elements[i]);
				$smallest.data('height', $smallest.data('height') + elements[i].data('height'));
			}
		},

		reorderAll: function ()
		{
			// TODO the need for this function on mobile sucks

			adsGrid.columns.each(function () {
				if (($(this).children().length == 0 && $(this).is(':visible')) ||
					$(this).children().length > 0 && $(this).is(':hidden')) {
					adsGrid.reorder();
					return false;
				}
			});
		},

		/*
		 * order elements by index
		 */
		reorder: function() {

			var $elementCount = $('li', this.columns).length;
			// only visible columns
			var $columnCount = $('ul:visible', this.container).length;
			// element index
			var $elementIndex = 1;
			// column index
			var $columnIndex = 0;

			while ($elementIndex <= $elementCount)
			{
				var column = this.columns.eq($columnIndex);

				column.append($('li[data-index=' + $elementIndex + ']', this.columns));

				//console.log($columnIndex + ' - ' + $elementIndex);

				$columnIndex++;
				$elementIndex++;

				if ($columnIndex > ($columnCount - 1))
				{
					$columnIndex = 0;
				}
			}
		}
	};
	adsGrid.init();

</script>
		
		
