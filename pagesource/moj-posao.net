<script>

	$(document).ready(function()
	{
		social.init($('#fixed-social').filter(':visible'));
	});

	var social =
	{
		url: document.URL,
		container: null,

		init: function(container) {
			if (!container.length)
			{
				return;
			}

			social.container = container;

			social.tw();
			social.fb();
			social.li();
		},
		gp: function() {
			//
		},
		tw: function()
		{
			$.getJSON("//public.newsharecounts.com/count.json?url=" + social.url + "&callback=?", function(data) {
				$('#tw-fixed .bubble', social.container).html(data.count);
			});
		},
		fb: function() {
			$.getJSON("https://graph.facebook.com/?fields=share&id=" + social.url, function (data) {
				if(data['share'])
				{
					$('#fb-fixed .bubble', social.container).html(data['share']['share_count']);
				}
			});
		},
		li: function() {
			$.getJSON("//www.linkedin.com/countserv/count/share?url=" + social.url + "&callback=?", function(data){
				$('#li-fixed .bubble', social.container).html(data.count);
			});
		}
	}

</script>

