<script>

$('.survey-page-container input[type=checkbox], .survey-page-container select').change(function(){
    // hide show additional elems
    var elems = null;
    if(	$(this).parent().parent().is('.category-container') ){
        elems = $(this).parent().parent().find('> .sub-options');
    } else if( $(this).parent().parent().is('.sub-category-container') ){
        elems = $(this).parent().parent().find('> .performer-container');
    }

    if( elems != null ){
        if( $(this).prop('checked') ){
            elems.show();
        } else {
            elems.hide();
        }
    }
});

$('#interestsForm').submit(function(e){
    e.preventDefault();

            var email = $('input[name=email]').val();
        var re = /\S+@\S+\.\S+/;
        if(!re.test(email)) {
            alert('Please enter a valid email (you@example.com) to have your interests saved and get a $5 coupon.');
            return false;
        }
    
	// persist to server
	var data = [];
	$('.survey-page-container input[type=checkbox], .survey-page-container select').each(function(e){
		if( $(this).is('select') && $(this).val() != "" ){
			data.push( {'type': 'performer', 'id': $(this).val() } );
		} else if( $(this).prop('checked') ){
			data.push( {'type': $(this).data('type'), 'id': $(this).data('value'), 'name': $(this).data('name') } );
		}	 
	});
	
	// Gather up list of parameters
	var params = {"action": "persist-interests", "data": JSON.stringify(data) , "type": "lead" };
	$('.include-data').each(function(e){
		params[ $(this).attr('name') ] = $(this).val();
	});
	
	if( data.length > 0 ){
		$.ajax({
			type: "post",
			url: "/account/interests.php", 			data: params,
            success: function(data){
                data = $.parseJSON(data);
                if(data.message) {
                    alert(data.message);
                } else {
                    alert("Thank You!");
                }
                $.cookie("noInterests", "1", { expires: 365 });
                displayLoadingDialog("interestsDialog",false);
            }
		});
	}
});
</script>

