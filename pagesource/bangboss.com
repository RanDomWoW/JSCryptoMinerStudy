<script type="text/javascript">
    head.js("/js/jquery.min.js?v=20180315112002",
            "/js/bootstrap.min.js?v=20180315112002");
    head.ready(function () {
        //$(".index-jumbotron-container").height(window.innerHeight);
        var topNav = $("#topNav");
        var img = $("img[class*=ours]");
        var logo = img.attr("src");
        var loginBtns = $("a[class*=login-div-btn]");
        var doc = $(document);
        var hasOpacity = false;
        var isMobile = false;
        function getScrollTop() {
            var scrollTop = document.body.scrollTop;
            if (document.compatMode === "BackCompat") {
                scrollTop = document.body.scrollTop;
            }
            else if(document.compatMode === "CSS1Compat") {
                scrollTop = document.documentElement.scrollTop == 0 ? document.body.scrollTop : document.documentElement.scrollTop;
            }
            return scrollTop;
        }
        window.onscroll = function(){//右侧滚动条监控事件
            var scrollTop = doc.scrollTop();//getScrollTop();
            if(scrollTop > 200){
                if(!hasOpacity) {
                    topNav.removeClass("index-nav-header");
                    topNav.addClass("nav-opacity");
                    img.attr("src", isMobile ? "/images/logo-full-mobile.png" : "/images/logo-full.png");
                    loginBtns.removeClass("login-div-btn");
                    hasOpacity = true;
                }
            } else {
                if(hasOpacity) {
                    topNav.addClass("index-nav-header");
                    topNav.removeClass("nav-opacity");
                    img.attr("src", logo);
                    loginBtns.addClass("login-div-btn");
                    hasOpacity = false;
                }
            }
        }
    });
</script>
