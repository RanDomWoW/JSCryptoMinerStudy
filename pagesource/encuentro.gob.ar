<script>
$("#search_normal").on('click', function() {
	var keywords = $('#keywords').val();
	var req = new XMLHttpRequest();
	var url = "/search/?keywords="+keywords;
	window.location.href = url;
});

$("#search_subtitles").on('click', function() {
	var keywords = $('#keywords').val();
	var req = new XMLHttpRequest();
	var url = "/search/subtitle/?keywords="+keywords;
	window.location.href = url;
});


$("#keywords").keyup(function() {

	var keywords = $('#keywords').val();

	if (keywords.length <= 2) {
		return;
	}

	var url = '/search/predictive?keywords='+keywords;

	$.getJSON(url, function(results, status) {
		if (status == "success") {
			$("#programas").html("");
			$("#guias").html("");
			$("#busqueda").html("");
			$("#busqueda").append('<h2>o tal vez estas buscando</h2>');
			for (var i in results.keywords) {
				if (i >= 5)
					break;
				$("#busqueda").append('<li><a href="/search?keywords='+results.keywords[i]+'">'+results.keywords[i]+'<span class="icn icn-ir"></span></a></li>');
			}

			if (results.programs.length == 0) {
				$("#programas").append('<h2>No se han encontrado resultados relevantes en <strong>programas</strong></h2>');
				return;
			} else { 
				$("#programas").append('<h2>Resultados relevantes en <strong>programas</strong></h2>');
			}
			for (var i in results.programs) {
				if (i >= 2)
					break;
				var image = "/images/placeholder_360x230.jpg";
				if (results.programs[i].image) {
					image = '/files/'+results.programs[i].image.file;
				}
				var title = results.programs[i].title;
				$("#programas").append('<a href="/programas/'+results.programs[i].id+'" class="relevante"><dd><div class="img-busq"><img src="'+image+'" class="img-responsive" alt="resultado"/></div><div class="text-busqueda"><h5>'+title+'</h5><p>'+results.programs[i].type+'</p></div></dd></a>');
			}

			if (results.resources.length == 0) {
				$("#guias").append('<h2>No se han encontrado resultados relevantes en <strong>gu&iacute;as</strong></h2>');
				return;
			} else { 
				$("#guias").append('<h2>Resultados relevantes en <strong>gu&iacute;as</strong></h2>');
			}
			for (var i in results.resources) {
				var image = "/images/placeholder_360x230.jpg";
				if (results.resources[i].image) {
					image = '/files/'+results.resources[i].image.file;
				}
				$("#guias").append('<a href="/enelaula/'+results.resources[i].id+'" class="relevante"><dd><div class="img-busq"><img src="'+image+'" class="img-responsive" alt="resultado"  height="230"/></div><div class="text-busqueda"><h5>'+results.resources[i].title+'</h5><p>'+results.resources[i].category.replace(',', '')+'</p></div></dd></a>');
			}

		} else {
			alert("Hubo un problema con la llamada a Educ.ar");
		}
	});
});
</script>		<section>
