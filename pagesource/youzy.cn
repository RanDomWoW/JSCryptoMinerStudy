    <script>


        var homePageControllers = {
            remainingDueNumber: '3',
            confirmDialog: "",
            messageDialog: "",
            InsertDue: function (expertId, userId, $t) {
                $t = $($t);
                if (userId <= 0) {
                    //alert('登陆后才能预约!');
                    //直接调用登录按钮的点击事件
                    $("#divLogin .user-quick-link").click();
                    return;
                }
                if (expertId <= 0) {
                    alert('请选择专家!');
                }
                //确认提示
                var name = $t.attr("title");
                var msgInfo = "优志愿仅提供用户与专家预约服务，对预约后产生的服务不承担法律责任。您的联系方式将发送至预约专家。<br/>您确定要预约专家“" + name + "”吗？<br/>";

                if (homePageControllers.remainingDueNumber == '3') {
                    msgInfo += "今天还可预约三次。";
                } else if (homePageControllers.remainingDueNumber == '2') {
                    msgInfo += "今天还可预约两次。";
                } else if (homePageControllers.remainingDueNumber == '1') {
                    msgInfo += "今天还可预约一次。";
                } else if (expertIndexcontroller.remainingDueNumber <= 0) {
                    // msgInfo += "今天预约次数用完。";
                    homePageControllers.messageDialog.open({ title: "确认预约", message: "今天预约次数用完，明天再来预约吧。", timeoutClose: 50000 });
                    return;
                }

                homePageControllers.confirmDialog.open({
                    title: '确认预约',
                    success: true,
                    message: msgInfo,
                    primary: function () {
                        ExpertDueService.InsertDue(expertId, userId, function (data) {
                            if (data.error) {
                                alert(data.error);
                                return;
                            }
                            if (data.value.Id > 0) {
                                //按钮变灰
                                alert('预约成功');
                                $('.btn-default-primary[data-val=' + data.value.ExpertId + ']').unbind("click");
                                $('.btn-default-primary[data-val=' + data.value.ExpertId + ']').attr("onclick", "");
                                $('.btn-default-primary[data-val=' + data.value.ExpertId + ']').html('已预约').removeClass('btn-default-primary').addClass('btn-forid disabled');
                                homePageControllers.remainingDueNumber = data.value.Remaining;



                            } else {
                                alert(data.value.Msg);
                                //console.log('预约失败');

                            }

                        });

                    }
                });
            }
        };//预约专家  
        require(['jquery', 'dialogs/confirm', 'dialogs/confirm', 'bootstrap', 'superSlide'], function ($, confirmDialog, messageDialog) {
            homePageControllers.confirmDialog = confirmDialog;
            homePageControllers.messageDialog = messageDialog;
            //工具提示框
            $('[data-toggle="tooltip"], .js-tooltip').tooltip({

                container: 'body',
                animation: false
            });

            //专家滑块
            window.setTimeout(function () {
                $(".uzy-index .slideBox").slide({ mainCell: ".bd ul", autoPlay: true, effect: "left" })
            }, 2000);


            //已预约的专家显示为已预约
            $("[name=aExpert_p]").each(function (i, n) {
                var userid = $("#UserId").val();

                var id = $(n).data("id");
                var name = $(n).data("name");
                var vipstatus = $(n).data("vipstatus");

                ExpertDueService.IsDueGetHtml(id, userid, name, vipstatus, function (result) {
                    if (result.value && result.value != "") {
                        var html = "";
                        if (result.value == '0') {   //未认证
                            html = '<a class="btn btn-forid disabled"  >未认证</a>';
                        } else if (result.value == '1') {
                            html = '<a class="btn btn-forid disabled"   >已预约</a>';
                        } else {
                            html = '<a class="btn btn-default-primary" target="_blank" ' + result.value + '>预约</a>';
                        }
                        $(n).html(html);
                    }
                });

            });
        });
    </script>

    
