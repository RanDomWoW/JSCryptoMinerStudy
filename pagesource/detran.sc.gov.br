		frm.submit();
	}

	function SubmeterGuia() {
		var cpf = $("#txtCPFGuia").val()
		cpf = cpf.replace(/ /g, "");
		cpf = cpf.replace(/_/g, "");
		cpf = cpf.replace(/\./g, "");
		cpf = cpf.replace(/\-/g, "");

		if(cpf.length != 11) {
			alert('CPF informado inválido.');
			$("#txtCPFGuia").focus();
			return false;
		}

		$('#txtDocPrincipal').val(cpf);
		$('#oculto').val("C");

		var frm = $("#frmServico");

		frm.attr("action",'http://consultas.detrannet.sc.gov.br/habilitacao/ConsultaCpf.asp');
