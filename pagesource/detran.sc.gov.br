	function SubmeterProva() {
		var cpf = $("#txtCPFProva").val();

		cpf = cpf.replace(/ /g, "");
		cpf = cpf.replace(/_/g, "");
		cpf = cpf.replace(/\./g, "");
		cpf = cpf.replace(/\-/g, "");

		if(cpf.length != 11) {
			alert('CPF informado inválido.');
			$("#txtCPFProva").focus();
			return false;
		}

		var frm = $("#frmServico");
