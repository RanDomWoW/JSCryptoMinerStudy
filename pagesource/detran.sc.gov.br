	function SubmeterAndamentoProcesso() {
		var cpf = $("#txtCPFAndamentoProcesso").val()
		cpf = cpf.replace(/ /g, "");
		cpf = cpf.replace(/_/g, "");
		cpf = cpf.replace(/\./g, "");
		cpf = cpf.replace(/\-/g, "");

		var renach = $("#txtNuRenach").val();

		if(cpf.length != 11) {
			alert('CPF informado inválido.');
			$("#txtCPFAndamentoProcesso").focus();
			return false;
		}

		if(renach.length != 9) {
			alert('Número Renach informado inválido.');
			$("#txtNuRenach").focus();
			return false;
		}

		$('#txtDocPrincipal').val(cpf);
		$('#NumeroRenach').val(renach);
		$('#oculto').val("AvancarC");

		var frm = $("#frmServico");
		frm.attr("action",'http://consultas.detrannet.sc.gov.br/habilitacao/ConsultaProcesso.asp');
		frm.submit();

	}

	function SubmeterPontuacaoCNH() {
		var cpf = $("#txtCPFCNH").val()
		cpf = cpf.replace(/ /g, "");
		cpf = cpf.replace(/_/g, "");
		cpf = cpf.replace(/\./g, "");
		cpf = cpf.replace(/\-/g, "");
		var docCNH = $("#txtNuRegistroCNH").val();

		if(cpf.length != 11) {
			alert('CPF informado inválido.');
			$("#txtCPFCNH").focus();
			return false;
		}
		if(docCNH == "") {
			alert('Número do registro informado inválido.');
			$("#txtNuRegistroCNH").focus();
			return false;
		}

		$('#txtDocPrincipal').val(cpf);
		$('#txtDocCNH').val(docCNH);
		$('#oculto').val("C");

		var frm = $("#frmServico");
		frm.attr("action",'http://consultas.detrannet.sc.gov.br/servicos/ConsultaPontuacaocondutor.asp');
		frm.submit();
	}

	function SubmeterEmissaoCNH() {
		var cpf = $("#txtCPFEmissao").val()
		cpf = cpf.replace(/ /g, "");
		cpf = cpf.replace(/_/g, "");
		cpf = cpf.replace(/\./g, "");
		cpf = cpf.replace(/\-/g, "");
		var docCNH = $("#txtCNHEmissao").val();

		if(cpf.length != 11) {
			alert('CPF informado inválido.');
			$("#txtCPFEmissao").focus();
			return false;
		}
		if(docCNH == "") {
			alert('Número do registro informado inválido.');
			$("#txtCNHEmissao").focus();
			return false;
		}

		$('#txtDocPrincipal').val(cpf);
		$('#txtDocCNH').val(docCNH);
		$('#oculto').val("C");

		var frm = $("#frmServico");
		frm.attr("action",'http://consultas.detrannet.sc.gov.br/servicos/ConsultaHistoricoEmissaoCNH.asp');
		frm.submit();
	}

</script>
