<script type="text/javascript">
    $(document).ready(function () {
        var id = "#" + "mainNav1_searchBox_ctl00_ctl00_searchTextBox";
        var indexId = "#" + "mainNav1_searchBox_ctl00_ctl00_searchIndexName";
        var indexName = $(indexId).val();
        var origControlID = $("#" + "mainNav1_searchBox_ctl00_ctl00_originalControlID").val();
        

        var pageURL = window.location.href;
        var url = "";
        if (origControlID == "intSearchBox" || (origControlID == "topSearchBox" && pageURL.indexOf("international") > 0) || (origControlID == "C007" && pageURL.indexOf("international") > 0)) {
            url = "/Sitefinity/Public/Services/InternationalCourseAutoComplete/InternationalCourseAutoCompleteService.svc/GetKeywords?catalogueName=" + indexName;
            $(id).attr("placeholder", "Search International Courses");
        } else {
            url = "/Sitefinity/Public/Services/SearchCompletion/AutoCompleteService.svc/GetKeywords?catalogueName=" + indexName;
            if (indexName == "tafesacourses") {
                $(id).attr("placeholder", "Search Courses");
            } else {
                $(id).attr("placeholder", "Search ...");
            }
        }
        
        $(id).autocomplete({
            minLength: 3,
            source: url,
            select: function (event, ui) {
                $(id).val(ui.item.value);
                $("#" + "mainNav1_searchBox_ctl00_ctl00_searchButton")[0].click();
            }
        });
    });
</script>

