<script>
  $(function(){
    var video_arr = $("video");
    var action_name = $(".action_name");
    var video_current_time;
    var video_current_time_interval;
    var has_updata = 0;

    if( action_name.attr("ct") !== "/index.php/Home/Index"){
      var ac = deal_url(action_name.attr("ac"));
      var ct = deal_url(action_name.attr("ct"));
      var vi = deal_url(video_arr.attr("src"));
    }
    video_current_time_interval = setInterval(updata_video_hits,10);

    function updata_video_hits(){
      if(video_arr.length !== 1) return;
      if(has_updata) return;
      video_current_time = video_arr.get(0).currentTime;
      if(video_current_time > 5){
        clearInterval(video_current_time_interval);
        $.ajax({
          url: '/index.php/Home/Works/upvh',
          type: 'GET',
          dataType: 'json',
          data: {type: ct+"/"+ac , wid:action_name.attr("wid") , tit:action_name.attr("tit") , vi:vi},
        })
        .done(function(data) {
          if(data == "f"){
            console.log("视频统计次数失败");
          }
          has_updata = 1 ;
        })
        .fail(function() {
          console.log("视频统计次数失败");
        })
        .always(function() {
          video_current_time_interval = setInterval(updata_video_hits,10);
        });
      }
      if(video_current_time < 0.5){
        has_updata = 0 ;
      }
    }
  })
  // 截取url末位变量
  function deal_url(st){
    if(!st) return;
    st = st.split('/').reverse();
    return st[0];
  }

  // MP3播放处理
  var _video = $("video");
  var _video_src = $("video").attr("src");
  // _video.each(function(){
  //   _video_src = $(this).attr("src");
  //   if( validate(_video_src)[0] === ".mp3" || validate(_video_src)[0] === ".MP3" ){
  //     // $(this).css({"height":"100px","width":"200px"});
  //     $(this).css("height","0px");
  //     $(this).css("position","relative");
  //     $(this).css("top","-15px");
  //     $(this).parent().prev().css("margin-bottom","70px");
  //   }
  // });
  _video.each(function(){
    _video_src = $(this).attr("src");
    if( validate(_video_src)[0] === ".mp3" || validate(_video_src)[0] === ".MP3" ){
      var _index = $(this).index();
      var _width = $(this).attr("width");
      var _height = $(this).attr("height");
      var _str = "<audio src="+_video_src+" height='"+_height+"' width='"+_width+"' class='audio_"+_index+"' controls>";
      $(this).parent().empty().append(_str).prev().css("margin-bottom","20px");
      // $(".audio_"+_index).get(0).play();
      $(".audio_"+_index).css({"width":"100%","position":"relative","top":"-10px"});
    }
  });
  function validate(str){ 
  //var importUrl= $("#importurl").val(); 
  //str="test.xlsx";
  var d=/\.[^\.]+$/.exec(str); 
  // alert(d);
  return d; 
  }
</script>
