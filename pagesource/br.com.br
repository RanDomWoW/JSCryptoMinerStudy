<script>


var delete_cookie = function(name) {
    document.cookie = name + '=;expires=Thu, 01 Jan 1970 00:00:01 GMT;';
};


//Categorias default.
function gravaCookieDefault(){
 var categoriasDefault = "Programa De Olho no Combustivel#Siga Bem#Cartão Petrobras#Lubrificantes Automotivos Lubrax";
 setCookiePzn("brwcmcat", categoriasDefault, 365);
}

//Preenche selação
function preencheSelecao(){
	var cont = 10;	
	if(getCookie("brwcmcatfav")!=undefined){
		var categoriasPreferencia = decodeURIComponent(getCookie("brwcmcatfav")).split('#')
		$("[id*='h_pzn_item-']").each(function() {
			for(var i=0; i<categoriasPreferencia.length; i++) {
				if($(this).val() == categoriasPreferencia[i]){
					$(this).parent().addClass("checked");                                				
					var divId = $(this).parent().parent().parent().parent().attr('id');
					//Contador
					if(divId == undefined || divId != 'content-tab-0'){
						cont = cont-1;					
					}				
				}
			}	
		});
	}
	//tags
	$('.list-items-to-choose li.checked').each(function(){
		customizeHome.checkList($(this), '1');
	});
	
	//chama funções do js da agencia.
	//customizeHome.countItems();
	//customizeHome.drawArc();
			
}

window.onload = function(){
        var coockie1 = getCookie("brwcmcat");
	gravaCookieDefault();
	var coockie2 = getCookie("brwcmcat");
	if(coockie1 != coockie2){
		location.reload(true);
	} else {
		preencheSelecao();
	}
}
<!-- ### FIM JAVASCRIPT PZN -->      
</script> 



