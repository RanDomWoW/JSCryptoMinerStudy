<!-- JAVASCRIPT PARA GRAVAÇÃO E LEITURA DO COOKIE DE PERSONALIZAÇÃO DA PÁGINA -->
<script language="javascript">  
  function gravaCookie(form){
    var expires = new Date();  
   	expires.setTime(expires.getTime() + 31536000000);
   	var objCheckBox = form.elements["item1"];
  	for (i=0; i < objCheckBox.length; i++) {
	var selecao = "";
       if (objCheckBox[i].checked) {	 		  
	       selecao += objCheckBox[i].value+",";
  		}
  	}
	document.cookie = 'BR_WCM_FAVORITOS=' + selecao + ';expires=' + expires.toUTCString()+"; path=/";  
}
	function leCookie(){
		var username=getCookie("BR_WCM_FAVORITOS");
		if (username!="") {
			alert(username);
			
			var ids = username.split(',');			
			for(var i=0; i<ids.length; i++) {
				var id = ids[i];
				if(id !="" ){
					mostraDiv(id);
				}			
			}			
		}		
	}
	function getCookie(cname) {
		var name = cname + "=";
		var ca = document.cookie.split(';');
		for(var i=0; i<ca.length; i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1);
			if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
		}
		return "";
	}
	 function mostraDiv(numeroDiv){ 
		
		var divv = 'div'+numeroDiv;	
		document.getElementById(divv).style.display = "block";  
	}     
</script> 
