			    function navOpacity(){
			    	var oNav = jq("#nav_bar").height();
			    	var this_scrollTop = jq(this).scrollTop();
			        if(this_scrollTop>oNav ){
			            jq("#nav_bar").css('opacity','0.95');
			        }else{
			        	jq("#nav_bar").css('opacity','0.8');
			        };
			    };
			    function throttle(method,context){
			        clearTimeout(method.tId);
			        method.tId=setTimeout(function(){
			            method.call(context);
			        },500);
			    };
			},
