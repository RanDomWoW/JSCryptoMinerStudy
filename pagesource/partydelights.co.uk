	border:0;
	padding:1rem;
	font-size:1rem;
	line-height:1rem;
	width:100%;
	color:#fff;
	-webkit-appearance: none;
}

.email-capture .panel ::-webkit-input-placeholder{color:#000}
.email-capture .panel ::-moz-placeholder{color:#000}
.email-capture .panel :-ms-input-placeholder{color:#000}
.email-capture .panel :-moz-placeholder{opacity:1;color:#000}

.side-content{display:none}

.headline{position:relative}
.headline::before{content:"";display:inline-block;padding-top:55%}
.headline svg{width:100%;height:auto;position:absolute;left:0;top:0}

@media (min-width:47em){
