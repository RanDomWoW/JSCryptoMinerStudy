<script>
emailCapture();

function emailCapture() {
var referrer = document.referrer.split('/')[2];
	if(referrer == "blog.partydelights.co.uk"){
		setCookie("pageViews", "0", 365);
	} 
	else {
		var pageViews = getCookie("pageViews");
		if (pageViews !== "") {
			if (pageViews === "1") {
				document.body.classList.add('show-email-capture');
				setCookie("pageViews", "0", 365);
			} 
		}
		else {
			setCookie("pageViews", "1", 365);
		}
	}
}

function closeEmailCapture(){
    document.body.classList.remove('show-email-capture');
    setCookie("pageViews", "0", 365);
}

function setCookie(cname, cvalue, exdays) {
  var d = new Date();
  d.setTime(d.getTime() + exdays * 24 * 60 * 60 * 1000);
  var expires = "expires=" + d.toUTCString();
  document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
}

function getCookie(cname) {
  var name = cname + "=";
  var decodedCookie = decodeURIComponent(document.cookie);
  var ca = decodedCookie.split(";");
  for (var i = 0; i < ca.length; i++) {
    var c = ca[i];
    while (c.charAt(0) == " ") {
      c = c.substring(1);
    }
    if (c.indexOf(name) === 0) {
      return c.substring(name.length, c.length);
    }
  }
  return "";
}
</script>

