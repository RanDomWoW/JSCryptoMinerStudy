.email-capture{
	position:fixed;
	right:0;
	top:0;
	bottom:0;
	left:0;
	z-index:999;
	display:none;
}
.email-capture *{box-sizing:border-box}
.email-capture .panel{
	box-sizing:border-box;
	position:fixed;
	width:calc(100% - 2rem);
	top:50%;
	left:50%;
	-webkit-transform: translate3d(-50%,-50%,0);
	transform: translate3d(-50%,-50%,0);
	background:#9cf1f2;
	padding:2rem 2rem 0;
	max-width:700px;
	z-index:5000;
}
.show-email-capture .email-capture{display:block}

.email-capture .overlay{
	position:fixed;
	right:0;
	top:0;
	bottom:0;
	left:0;
	width:100%;
	height:100%;
	background:#fff;
	background:rgba(255,255,255,.75);
}

.email-capture .panel .close-button{
	position:absolute;
	right:0;
	top:0;
	background:0;
	border:0;
	padding:1rem;
}

.email-capture .panel p{
	margin:0 0 1rem;
	font-size:.8rem;
	text-align:center;
	color:#000;
}

.email-capture .panel a{
	font-size:.8rem;
	display:block;
	text-align:center;
	color:#000;
	padding:1rem
}

.email-capture .panel a:focus{text-decoration:underline}

.email-capture .panel input[type=email]{
	background:none;
	border:1px solid #000;
	padding:1rem;
	font-size:1rem;
	width:100%;
	color:#000;
	margin-bottom:.5rem;
	-webkit-appearance: none;
}
.email-capture .panel button[type=submit]{
