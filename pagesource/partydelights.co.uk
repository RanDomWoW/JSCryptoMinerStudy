	.email-capture .panel input[type=email]{flex-grow:1;width:100%;margin:0 .5rem 0 0;font-size:.9rem;padding:.75rem}
	.email-capture .panel button[type=submit]{width:auto;font-size:.9rem;line-height:.9rem;padding:.75rem 1rem}
}
</style>

