<script type="text/javascript">
    $(".modalClick").click(function (event) {
        event.preventDefault();
        var src = $(this).attr("data-href");
        //console.log("clicked" + src);
        var iframe = $("#iframeModal iframe");
        $(iframe).attr("src", src);
        setTimeout(function () { autoResize($(iframe).attr("id")) }, 1000);
    });
    function modalClick(obj) {
        var src = $(obj).attr("data-href");
        //console.log("clicked" + src);
        var iframe = $("#iframeModal iframe");
        $(iframe).attr("src", src);
        setTimeout(function () { autoResize($(iframe).attr("id")) }, 1000);
    }
    function autoResize(id) {
        var newheight;

        if (document.getElementById) {
            newheight = document.getElementById(id).contentWindow.document.body.scrollHeight;
        }

        document.getElementById(id).height = (newheight + 10) + "px";
    }
    // Stop video on modal close
    $('#iframeModal').on('hidden.bs.modal', function () {
        $("#iframeModal iframe").attr("src", "");
    });
</script>
