        <div class="farapopup__content" style="overflow: hidden; background-color: #fff; max-width: 900px">
            <img alt="" src="/Portals/0/price-contact.jpg" width="544" height="544" />
        </div>
    </div>
    <script>
        if (typeof $.cookie('farapopup') == 'undefined') {
            var $faraPopup = $('.farapopup__wrap');
            $faraPopup.find('img').css('max-width', '100%');

            $(window).on('load', function () {
                setTimeout(function () {
                    var option = {};
                    option.width = 'auto';
                    option.height = 'auto'
                    option.html = $faraPopup.html();
                    option.onLoad = function () {
                        $('body').addClass('no-scroll');
                    }
                    option.onClosed = function () {
                        $('body').removeClass('no-scroll');
                    }

                    if (window.innerWidth <= 900)
                        option.width = '100%';

                    $.colorbox(option);
                }, 400);
            });

			var date = new Date();
			var minutes = 720;
			date.setTime(date.getTime() + (minutes * 60 * 1000));			
            $.cookie('farapopup', 'set', { expires: date, path: '/' });
        }
    </script>
