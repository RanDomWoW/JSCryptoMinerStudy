        function featuredDealsNumber() {
            var boxWidth = parseInt($('#featuredDeals .box').css('width')) + parseInt($('#featuredDeals .box').css('margin-left')) + parseInt($('#featuredDeals .box').css('margin-right'));
            if($('#mainLeft').css('display')=='none'){
                var count = parseInt(parseInt($('body').css('width')) /boxWidth);
            }else{
                var count = parseInt((parseInt($('body').css('width'))-36-parseInt($('#mainLeft').css('width'))) /boxWidth);

            }
            $('#featuredDeals .carousel-inner').css('width', count * boxWidth);
            $('#mainRight').css('width', count * boxWidth);
            /*
             *计算左边距
             */
            if($('#mainLeft').css('display')!='none'){
                var bodyWidth = parseInt($('body').css('width'));
                $('#mainRight').css('margin-left', (bodyWidth-34 - count * boxWidth - parseInt($('#mainLeft').css('width'))) /2);
                $('#mainRight').css('margin-right', 34);
                $('#mainRight').removeClass();
                $('#mainRight').addClass('pull-left');
            }else{
                $('#mainRight').css('margin-left', 0);
                $('#mainRight').css('margin', '0 auto');
                $('#mainRight').removeClass();

            }

            var html = '';
            var htmlindicators = '';
            for (var i = 0; i < Math.ceil(featuredDealsData.length / count); i++) {
                if(i == (Math.ceil(featuredDealsData.length / count)-1)&&((featuredDealsData.length-i * count)<count)&&(i!=0)){
                    break;
                }
                if (i == 0) {
                    html += '<div class="item active clearfix">';
                    htmlindicators += '<li data-target="#carousel-example-generic" data-slide-to="' + i + '" class="active"></li>'
                } else {
                    html += '<div class="item clearfix">';
                    htmlindicators += '<li data-target="#carousel-example-generic" data-slide-to="' + i + '"></li>'
                }
                var itemNumber = (Math.ceil(featuredDealsData.length / count) == (i + 1) ? featuredDealsData.length : (i + 1) * count);
                for (var j = i * count; j < itemNumber; j++) {
                    if (featuredDealsData[j].final_price <= 0) {
                        var $price = "FREE";
