	function validateStep1()
	{
	
			var form = $("#step1");
            Cy.clearError(form);
            var checkValidate = true;

			
            var firstName = form.find('[name="FirstName"]');
            if (!firstName.val()) {
                Cy.addErrorRow(form, firstName, firstName.attr('validate-error'));
                checkValidate = false;
            }
            

            var email = form.find('[name="Email"]');
            var emailRegex = new RegExp(email.attr('validate-regex'));
            if (!emailRegex.test(email.val())) {
                Cy.addErrorRow(form, email, email.attr('validate-error'));
                checkValidate = false;
            }


            var mobilePhone = form.find('[name="Mobile"]');
            var mobilePhoneRegex = new RegExp(mobilePhone.attr('validate-regex'));
            if (!mobilePhoneRegex.test(mobilePhone.val())) {
                Cy.addErrorRow(form, mobilePhone, mobilePhone.attr('validate-error'));
                checkValidate = false;
            }
		return checkValidate;
	}
	
	
	function validateStep2()
	{
	
			var form = $("#step2");
            Cy.clearError(form);
            var checkValidate = true;

			var address = form.find('[name="Address"]');
			var addressRegex = new RegExp("/[-!$%^&*()_+|~=`\\#{}\[\]:;'<>?,.\/]/");
            if (!address.val()) {
                Cy.addErrorRow(form, address, address.attr('validate-error'));
                checkValidate = false;
            }
			
			if (/[!$%^&*_+|~=`\\#{}\[\]:";'<>?]/.test(address.val())) {
                Cy.addErrorRow(form, address, address.attr('validate-error'));
                checkValidate = false;
            }
			
			var city = form.find('[name="City"]');
            if (city.val() == 0) {
                Cy.addErrorRow(form, city, city.attr('validate-error'));
                checkValidate = false;
            }
            
			var district = form.find('#District');
            if (district.val() == 0) {
                Cy.addErrorRow(form, district, district.attr('validate-error'));
                checkValidate = false;
            }
				  
			var memberDescription = form.find('#aboutMom');
            if (memberDescription.val() == 0) {
                Cy.addErrorRow(form, memberDescription, memberDescription.attr('validate-error'));
                checkValidate = false;
            }
				  
			if (memberDescription.val() > 1) {
                var maxdate = new Date();
                maxdate.setDate(maxdate.getDate() + 294);

                form.find('.birth-date-holder').each(function () {
                    var child = $(this);
                    var contentError = child.find('div.birth-date');
                    var dob = new Date(child.find('[name="childYear"]').val(), parseInt(child.find('[name="childMonth"]').val()) - 1, child.find('[name="childDay"]').val());
                    if (dob > maxdate) {
                        Cy.addErrorRow(form, contentError, contentError.attr('validate-error'));
                        checkValidate = false;

                    }
                });
            }
				  
			if (memberDescription.val() > 2) {
                form.find('[name="gender"]').each(function () {
                    var value = $(this).val();
                    if (value == 0) {
                        Cy.addErrorRow(form, $(this), $(this).attr('validate-error'));
                        checkValidate = false;
                    }
                });
            }	  
            
		return checkValidate;
	}
	</script>


