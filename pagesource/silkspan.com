<script type="text/javascript">
//Enter "frombottom" or "fromtop" จะให้อยู่บนหรือล่าง
var verticalpos="fromtop";
function JSFX_FloatTopDiv()
{
	var sectionFlag = true;
	var boxHeight = 839;  // ขนาดความสูง (A)

	var mybrowser=navigator.userAgent;
	if(mybrowser.indexOf('Firefox')>0)
	{
		var plsBottom = 16;
	}
	else
	{
		var plsBottom = 0;
	}

	var startX = document.body.scrollLeft + document.body.clientWidth - 198;
	if (verticalpos=="fromtop")
	{
		var startY = plsBottom;
	}
	else
	{
		var startY = boxHeight + plsBottom;
	}

	var ns = (navigator.appName.indexOf("Netscape") != -1);
	var d = document;

	function ml(id)
	{
		var el=d.getElementById?d.getElementById(id):d.all?d.all[id]:d.layers[id];
		if(d.layers)el.style=el;
/*		if(mybrowser.indexOf('Firefox')>0)
		{  */
			el.sP=function(x,y){this.style.left=x + 'px';this.style.top=y + 'px';};
/*		}
		else
		{
			el.sP=function(x,y){this.style.left=x;this.style.top=y;};
		}  */
		el.x = startX;
		if (verticalpos=="fromtop")
		el.y = startY;
		else{
		el.y = ns ? pageYOffset + innerHeight : document.body.scrollTop + document.body.clientHeight;
		el.y -= startY;
		}
		return el;
	}
	window.stayTopLeft=function()
	{
		if (verticalpos=="fromtop")
		{
			var pY = ns ? pageYOffset : document.documentElement.scrollTop;
			if (sectionFlag == true)
			{
				ftlObj.y += (pY + startY - ftlObj.y)/8;
			}
			else
			{
				ftlObj.y += (pY + (18 + plsBottom) - ftlObj.y)/8;
			} 
		}
		else
		{
			var pY = ns ? pageYOffset + innerHeight : document.body.scrollTop + document.body.clientHeight;
			if (sectionFlag == true)
			{
				ftlObj.y += (pY - startY - ftlObj.y)/8;
			}
			else
			{
				ftlObj.y += (pY - (18 + plsBottom) - ftlObj.y)/8;
			}
		}		
		ftlObj.sP(ftlObj.x, ftlObj.y);		
		setTimeout("stayTopLeft()", 10);		

		
	}
	
	ftlObj = ml("divStayTopLeft");
	stayTopLeft();  
}

JSFX_FloatTopDiv();
</script>
