            var formCadastro = $(this).closest('.form-cadastro');

            if (formCadastro.length != 0) {
                formCadastro.addClass('not-validade');
            } else {
                $(this).closest('.form-group').addClass('not-validade');
            }
        }).focusout(function () {
            var formCadastro = $(this).closest('.form-cadastro');

