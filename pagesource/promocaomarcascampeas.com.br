        function enviarContato() {
            var form = $('#form-contato').serialize();
            var url = $('#form-contato').attr('action');

            $.ajax({
                url: url,
                data: form,
                type: 'post',
                success: function (resp) {
                    if (resp.Success) {
                        digdin.flashMessage(resp.Message);
                        $('#form-contato').data('bootstrapValidator').disableSubmitButtons(false);
                        $('#form-contato input').val('');
                        $('#form-contato textarea').val('');
                        closeModal('faleConoscoModal');
                    } else {
                        digdin.flashMessage(resp.Message, 'error');
                    }
                },
                error: function () {
                    $('#form-contato').data('bootstrapValidator').disableSubmitButtons(false);
                }
            });

        }
