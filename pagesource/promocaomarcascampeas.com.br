    <script src="/Scripts/countUp.js"></script>
        <script>
            var options = {
                useEasing: true,
                useGrouping: true,
                separator: '.',
                decimal: ','
            };

            var quantidadeCamisetas =  1585;
            var countDown = new CountUp('camisetas', 0, quantidadeCamisetas, 0, 3, options);

            $(function () {
                if (quantidadeCamisetas >= 1000) {
                        $("#camisetas").css({ "font-size": "3em" });
                    }
                else if (quantidadeCamisetas >= 100) {
                        $("#camisetas").css({ "font-size": "4.5em" });
                    } else {
                        $("#camisetas").css({ "font-size": "6em" });
                }
                CheckScrollCountUp(window);
            })

            $(window).scroll(function () {
                CheckScrollCountUp(window);
            });

            function CheckScrollCountUp(window) {
                var hT = $('#camisetas').offset().top,
                    hH = $('#camisetas').outerHeight(),
                    wH = $(window).height(),
                    wS = $(this).scrollTop();
                if (wS > (hT + hH - wH)) {
                    countDown.start();
                }
            }
        </script>



