    <script type="text/javascript">
        function popularCidades(nomeDaCidade) {
            $.ajax({
                url: '/home/GetEmpresaByCidade?cidade=' + nomeDaCidade,
                type: 'POST',
                success: function (result) {
                    $('#cidades').empty();

                    result.map(function (cidade) {
                        var item = $('<div class="row cidade-detalhe"><div class="col-md-12"><p>' +
                            cidade.Nome +
                            '</p></div></div>');
                        $('#cidades').append(item);
                    });
                }
            });
        }

        $(function () {
            $('#busca_cidade').keypress(function (event) {
                if (event.keyCode !== 13) {
                    return;
                }

                $('#cidades_loading').show();
                $('#cidades').hide();

                $.ajax({
                    url: '/home/GetEmpresaByCidadeBusca?cidadebusca=' + encodeURIComponent($(this).val()),
                    type: 'POST',
                    success: function (result) {
                        $('#cidades').empty();

                        result.map(function (cidade) {
                            var item = $('<div class="row cidade-detalhe"><div class="col-md-12"><p>' +
                                cidade.Nome +
                                '</p></div></div>');
                            $('#cidades').append(item);
                        });

                        $('#cidades_loading').hide();
                        $('#cidades').show();
                    }
                });
            });
        });
    </script>


