            $(document).on('keypress keyup keydown', 'input[type=text]', function (e) {

                var obj = $(this);
                var maxLen = obj.attr('maxlength');

                if (obj.data() && obj.data().mask) {
                    maxLen = obj.data().mask.mask.length;
                }


                if (maxLen != null && maxLen != '') {
                    maxLen = +maxLen;
                    var val = obj.val();



                    if (e != null && 'keyCode' in e && (e.keyCode < 8 || e.keyCode > 46))
                        if (val != null && val.length == maxLen) {
