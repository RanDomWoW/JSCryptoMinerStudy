<script>
    (function() {
            setTimeout(check, 100);

            function check() {
                if (!window['$'] || typeof $('').elastislide !== 'function') {
                    setTimeout(check, 100);
                } else {
                    setup();
                }
            }

            function setup() {
            $(function() {
                    var stopped = false;

                $('#carousel')
                    .elastislide({
                        orientation: 'vertical',
                        minItems: 4,
                        autoplay: true,
                        autoplaydelay: 5000,
                        paused: function() {
                            return stopped;
                        }
                    });

                $('.img-carrosel')
                    .mouseenter(function(event) {

                        var offset = $(event.currentTarget).offset();
                        var height = $(event.currentTarget).height();
                        var width = $(event.currentTarget).width();

                        offset.left += 2;
                        offset.top += 1;
                        
                        $('.img-carrosel-popover div div').text($(event.currentTarget).data('title'));
                        $('.img-carrosel-popover div span').text($(event.currentTarget).data('descr'));
                        $('.img-carrosel-popover').offset(offset);
                        $('.img-carrosel-popover').height(height);
                        $('.img-carrosel-popover').width(width);

                        //$('.img-carrosel-popover').show();
                        
                        $('.img-carrosel-popover').click(function() {
                            document.location.assign($(event.currentTarget).data('url'));
                        });

                        stopped = true;
                    });

                $('.img-carrosel-carrosel')
                    .mouseleave(function() {
                        //$('.img-carrosel-popover').hide();

                        stopped = false;
                    });

                $('.flutuante')
                    .mouseleave(function() {
                        //$('.img-carrosel-popover').hide();
                        stopped = false;
                    });
                });
        }
    }());
</script>


