<script>
$('#home__search-input').autocomplete('/ajax/search_keywords', {
	minChars: 2,
	max: 50,
	delay: 10,
	cacheLength: 0,
	matchSubset: false,
	matchContains: false,
	selectFirst: false,
	scrollHeight: 300,
	resultsClass: 'ac_results ac_results--home-search'
});
$('#home__search-input').result(function(event, data, formatted) {
	if (data) { // Ensures we don't run into a JavaScript error when no data is present
		$('<input type="hidden" />')
			.attr('class', 'search__vendor-type-id')
			.attr('name', 'search__vendor-type-id')
			.val(data[1])
			.appendTo('#home__search-form');
		$('#home__search-form').submit();
	}
});

// Only animate the home page images when they have all fully loaded
$('.js-home__header-container').imagesLoaded(function() {
	$('.js-home__header-container').addClass('loaded');
});

$('.js-category-slider').slick({
	infinite: true,
	slidesToShow: 4,
	slidesToScroll: 1,
	speed: 250,
	draggable: false, // Disables desktop dragging
	prevArrow: '<button type="button" class="slick-prev category-slider__previous-arrow"><img src="/images/svg/standalone/thin-arrow.svg"></button>',
	nextArrow: '<button type="button" class="slick-next category-slider__next-arrow"><img src="/images/svg/standalone/thin-arrow.svg"></button>',
	responsive: [
		{
			breakpoint: 1024,
			settings: {
				slidesToShow: 3
			}
		},
		{
			breakpoint: 600,
			settings: {
				slidesToShow: 2
			}
		},
		{
			breakpoint: 480,
			settings: {
				slidesToShow: 1
			}
		}
		// You can unslick at a given breakpoint now by adding:
		// settings: "unslick"
		// instead of a settings object
	]
});
</script>
<script>
if (!Cookies.get('hide_cta_promokit_signup'))
{
	setTimeout(function() {
		$( "#js-top-cta-dropdown" ).slideDown( "fast" );
	}, 1000);

	$( document ).on( "click", ".js-top-cta-dropdown__set-cookie", function() {
		Cookies.set("hide_cta_promokit_signup", true, { expires: 5184000 });
	});
	$( document ).on( "click", ".js-top-cta-dropdown__close", function() {
		$( "#js-top-cta-dropdown" ).slideUp( "fast" );
	});
}
</script>
