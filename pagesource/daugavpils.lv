<script type="text/javascript">
function zeroPad(number, length) {
  var str = '' + number;
  while (str.length < length) {
    str = '0' + str;
  }
  return str;
}

$(function() {
  var selectedDates = new Array();
  $.ajax({
    url: "/kalendars/dates_in_month",
    dataType: "json",
    async: false,
    success: function(data){
               selectedDates = data.dates;
             }
  });
  $("#eventsCalendar").datepicker({
    dateFormat: 'yy-mm-dd',
    beforeShowDay: function(date) {
      //alert(selectedDates);
      sDate = date.getFullYear() + '-' + zeroPad(date.getMonth() + 1, 2) + '-' + zeroPad(date.getDate(), 2);
      if($.inArray(sDate, selectedDates) > -1) {
        return [true];
      }
      else {
        return [false];
      }
      //return [false,"ui-state-active","Event Name"];
      //return [true,"ui-state-highlight",""];//enable all other days
    },
    onChangeMonthYear: function(year, month) {
      $.ajax({
        url: "/kalendars/dates_in_month/"+year+zeroPad(month,2),
        dataType: "json",
        async: false,
        success: function(data){
                   selectedDates = data.dates;
                 }
        });
    },
    onSelect: function(dateText) {
      window.location.href = "/lv/kalendars/by_date/"+dateText;
    }
  });
  $("#eventsCalendar").css("padding-left", "2px");

});
</script>


			


