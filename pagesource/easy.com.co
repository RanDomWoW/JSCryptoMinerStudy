				$("#prod_compare_bar").show("bounce", { times: 3, direction: "down" }, "slow");
				var list = $(".prod_compare input[name='prod_compare[]']:checked").map(function () {return this.value;}).get().join(",");
				$("#prod_compare_list").val(list);
				$("#bottom_promo").hide("slide", { direction: "down"});
			} else {
				$("#prod_compare_bar").hide("slide", { direction: "down"});
				$("#bottom_promo").show("slide", { direction: "down"});
			}
		}
		
		$(".prod_compare input:checkbox").on("change", function() {
			var len = $(".prod_compare input[name='prod_compare[]']:checked").length;
			if ( len > 1 ) {
				$("#prod_compare_bar_button").prop("disabled", false);
			} else {
				$("#prod_compare_bar_button").prop("disabled", true);
			}
			if ( len > 4 ) {
				this.checked = false;
			} else {
				updateCounter();
			}
		});
		
		$(document).ready(updateCounter());
