        // QUITA LA PAGINACION SI SOLO HAY UN PUNTO
        function muestroPaginacion(){
        	var totalPaginacion = $('.swiper-pagination span').length;
        	// alert(totalPaginacion);
        	if(totalPaginacion==1){
        		$('.swiper-pagination').hide();
        	}else{
