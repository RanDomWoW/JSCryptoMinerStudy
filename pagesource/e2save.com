        <script type="text/javascript">
    
$('document').ready(function() {
    /* NEWSLETTER SIGNUP - FOOTER
 ================================================== */
$('.newsletter form#pre-newletter').on('submit', function(event) {
    event.preventDefault();
    $("input#email_field").val($('input#email').val());
    $("input#confirm").val('');
    $("#newsletter-signup").modal('show');
});

$('#newsletter-signup form').validator().on('submit', function(event) {
    $('#pre-newletter-register-error').hide();
    $('#pre-newletter-register-success').hide();
    if (event.isDefaultPrevented()) {
        event.preventDefault();
    } else {
        event.preventDefault();
        $.post($(this).attr('action'), $(this).serialize(), function () {
        }).done(function (data) {
            if (typeof(data.success) !== 'undefined') {
                $('#pre-newletter-register-success').fadeIn();
                setTimeout(function () {
                    $('#newsletter-signup').modal('hide');
                }, 3000);
            } else if (typeof(data.invalid) !== 'undefined') {
                $('#pre-newletter-register-error').html('<p>' + data.invalid + '</p>');
                $('#pre-newletter-register-error').show();
            } else {
                $('#pre-newletter-register-error').html('<p> Unable to complete your request</p>');
                $('#pre-newletter-register-error').show();
            }
        }).fail(function (jqXHR, textStatus, errorThrown) {
            $('#pre-newletter-register-error').html('<p> Unable to process your request</p>');
            $('#pre-newletter-register-error').show();
            //console.log(jqXHR, textStatus, errorThrown);
        });
    }
});
});
</script>

