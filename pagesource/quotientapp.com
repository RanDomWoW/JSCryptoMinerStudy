<script>
    
    var maxErrorCount = 0;
    window.onerror = function(errorMessage, url, line) {
        if(maxErrorCount > 2) {
            return;
        }
        maxErrorCount++;
        var text = 'URL:' + window.location.href;
        text += ' in:' + url + ' on line:'+line;
        if($ && $.browser) {
            text += ' Device:' + $.os.name + ' ' + $.browser.name + ' ' + $.browser.version;
        }
        // trim our message to 500 chars max
        text += ' Message:' + (errorMessage.substr(0,500));
        if($.ajax) {
            $.ajax({
                type : 'post',
                url : quotient.url('Go','/service/log/js/message'),
                data : { q : text }
            });
        }
    };
</script>
