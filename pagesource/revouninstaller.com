    <script>
      $('#subscribe-form').submit(function(e){
      
        e.preventDefault();
      
        var email = $.trim($('#subscribe-email').val());
        if(email.length == 0 || !validateEmail(email)){
          $('#subscribe-msg').html('<p class="alert alert-danger"><i class="fa fa-exclamation-circle" aria-hidden="true"></i> Please enter a valid email address</p>');
        }else{
          $.ajax({
            url: 'includes/process.php',
            method: 'POST',
            data: {email: email},
            success: function(data){
              $('#subscribe-msg').html(data);
      
              $('#subscribe-form')[0].reset();
            }
          });
        }
      });
      
      function validateEmail(email) {      
        var validRegExp = /^[^@]+@[^@]+.[a-z]{2,}$/i;       
              
        if (email) {
          if (email.search(validRegExp) == -1) {return false;}
          }else { return false; }
          
        return true;      
      }
    </script>
