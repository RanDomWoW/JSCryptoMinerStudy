<script type='text/javascript'>
    document.addEventListener("DOMContentLoaded", function (event) {
        var $e = document.createElement('div');
        var $e_t = document.createTextNode("Hizmetlerimizden en iyi şekilde faydalanabilmemiz için çerezler kullanıyoruz. KariyerAkademi.net'i kullanarak çerezlere izin vermiş olursunuz. ");
        $e.className = "cookie-policy";
        $e.appendChild($e_t);

        var $l = document.createElement('a');
        var $l_t = document.createTextNode("Çerez politikamız için tıklayın.");
        $l.href = "http://www.kariyer.net/veri-politikasi/cerez-politikasi";
        $l.setAttribute("target", "_blank");
        $l.appendChild($l_t);

        var $c = document.createElement('a');
        var $c_t = document.createTextNode("X");
        $c.href = "javascript:;";
        $c.className = "cookie-policy-close";
        $c.setAttribute("onclick", "closeCookiePolicy()");
        $c.appendChild($c_t);

        $e.appendChild($l);
        $e.appendChild($c);

        document.body.appendChild($e);

        if (getCookiePolicy("CookiePolicy") == "" && getCookiePolicy("CookiePolicy") != "1") {
            document.getElementsByClassName('cookie-policy')[0].style.display = 'block';
        }
    });

    function closeCookiePolicy() {
        setCookiePolicy('CookiePolicy', "1");
        document.getElementsByClassName('cookie-policy')[0].style.display = 'none';
    }

    function setCookiePolicy(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + d.toGMTString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }

    function getCookiePolicy(cname) {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }
</script>




