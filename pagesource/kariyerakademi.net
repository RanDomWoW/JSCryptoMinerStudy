/**
 * Call this function when a user clicks on a promotion. This function uses the eventCallBack
 * datalayer variable to handle navigation after the ecommerce data is sent to Google Analytics.
 *
 * @param {Object} promoObj An object representing an internal site promotion.
 */
    function onPromoClick(promoObj) {
        console.log("promoObject");
  dataLayer.push({
    'event': 'promotionClick',
    'ecommerce': {
      'promoClick': {
        'promotions': [
         {
             'id': promoObj.Id, 				// Promosyonun ID’si
             'name': promoObj.Name,    // Promosyonun Adı
            'creative': '',			        // Promosyonu Görseli
            'position': ''				// Promosyonu Sırası
         }]
      }
    },
    'eventCallback': function() {
      document.location = promoObj.url; // Tıklandıktan sonar yönlenilen URL
    }
  });
}
