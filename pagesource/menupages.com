    //for clickstream
      function setup_tags(){
        function addTags(divId, identifier) {
          identifier = typeof identifier !== 'undefined' ? identifier : 'id';

          // grubhub and seamless order urls are done separately

          if (identifier==='id') {
            var base = $('#'+divId).not(".seamless-order-url").not(".grubhub-order-url");
          }
          else {
            var base = $('.'+divId).not(".seamless-order-url").not(".grubhub-order-url");
          }
          base.find('a').attr('data-clickstream', '');
          var temp_divId = divId;
          if (['list9GH', 'list9newa'].indexOf(divId) >= 0) {
            base.find('a').attr('data-cs-fires-on-click-link', 'clicked_' + 'partner_link');
          } else if (['list9OT'].indexOf(divId) >= 0) {
            base.find('a').attr('data-cs-fires-on-click-link', 'clicked_' + 'OpenTable');
          }
          else {
            base.find('a').attr('data-cs-fires-on-click-link', 'clicked_' + divId);
          }
          var data_str = '';
          var city_str = 'city: "new-york-city"';
          data_str += city_str;

          


          if (['list-by-cuisine', 'list-by-neighborhood'].indexOf(divId) >= 0) {
            base.find('a').each(function(){
              $(this).attr('data-cs-with-property-text', data_str + ', value: ' + $(this).text());
            });
          }
          base.find('a').each(function(){
            $(this).attr('data-cs-with-property-text', data_str);
          });
        }

        addTags('breadcrumbs');
        addTags('popularRestaurants');
        addTags('list-by-cuisine');
        addTags('list-by-neighborhood');
        addTags('hot-menus');

        addTags('areaDims');
        addTags('neighborhoodDims');
        addTags('mealTagDims');
        addTags('featureTagDims');

        // addTags('searchResults');
        // addTags('searchresults');

        addTags('menu-tab');

        // addTags('grubhub-order-url_from_search_results', 'class');
        // addTags('seamless-order-url_from_search_results', 'class');

        addTags('list9GH');
        addTags('list9newa');

        addTags('list9OT');

        addTags('grubhub-order-url_from_profile_page');
        addTags('seamless-order-url_from_profile_page');

        addTags('opentable_from_profile_page');

        $('#image-map-wrp').find('area').attr('data-clickstream', '');
        $('#image-map-wrp').find('area').attr('data-cs-fires-on-click-link', 'clicked_hp_map');

        // $('#Form1').attr('data-cs-fires-on-submit', 'test_submitted_search_form1');
