function loopClass(e, c, t) {
  // e = element
  // c = class
  // t = time in ms
  i = 1;
  setInterval(function() {
    items = e.children(), items.removeClass(c), items.eq(i).addClass(c);

    if (i == items.length - 1) {
      i = 0;
    } else {
      i++;
    }
  }, t);
}

