<script>
$(document).ready(function() {
	var coursesCity = $('a.header__city').text();
	courses(coursesCity, 'prod');
	//	dirty hack!!! Это сделано, потому что раз в минуту запрос уходит в 10-секундный пендинг из-за сброса кеша. 
	// Первый герой расчищает дорогу настоящему запросу и погибает, как и подобается в таких случаях
	courses(coursesCity, 'prod', function() {
		var jsonData = JSON.parse(window.coursesdata);
		$('span.resultCourse').html("");
		var date = new Date();
		var a = date.getDate();
		var b = date.getMonth()+1;
		if (a < 10) a = "0"+a;
		if (b < 10) b = "0"+b;
		var nowDate = a+"."+b;
		
		$.each(jsonData.dataFiz, function( index, value ) {		
			
		  var coursesName = jsonData.dataFiz[index].NAME;
		 
		  if (coursesName == "USDRUR" || coursesName == "EURRUR") {
	  		 
			  var coursesCodeIn = jsonData.dataFiz[index].CODE_IN;
			  
			  var coursesBuy = parseFloat(jsonData.dataFiz[index].BUY_VALUE);
			  var coursesSell = parseFloat(jsonData.dataFiz[index].SELL_VALUE);
			  var coursesCode = jsonData.dataFiz[index].CODE;
			  var coursesdate = jsonData.dataFiz[index].DATE;
			  
			  var coursesdate = coursesdate.substring(0,5);
			  
			  var coursesBuyCh = jsonData.dataFiz[index].BUY_CHANGE;
			  var coursesSellCh = jsonData.dataFiz[index].SELL_CHANGE;
			  
			  if (coursesBuyCh < 0) {var rowtobuy = 'up';}else{var rowtobuy = 'down';}
			  if (coursesSellCh < 0) {var rowtosell = 'up';}else{var rowtosell = 'down';}
				  
			  $('span.resultCourseDate').html("<b>"+nowDate+" :</b>");
			  $('span.resultCourse').append("<span class='header__USD'>"+
											 "<b>"+coursesCodeIn+"</b>"+coursesBuy.toFixed(2)+" <span class='arrow_"+rowtobuy+"'></span>"+
											 " —&nbsp;&nbsp; "+coursesSell.toFixed(2)+" <span class='arrow_"+rowtosell+"'></span>"+
										 "</span>");
		  }
		});
	});	
});
</script>            

