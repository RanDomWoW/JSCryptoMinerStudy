<script>

function setActiveDots() {
    $("a[data-slide-index]").removeClass('active');
	$('.owl-dots .owl-dot').each(function(i,elem) {
		if ($(this).hasClass("active")) {
           $("a[data-slide-index='" + i + "']").addClass('active');
			return false;
		} 
		/*else {
           $("a[data-slide-index='" + i + "']").removeClass('active');
		}
        */
	});
}


var owl = $('.owl-carousel');

owl.owlCarousel({
	loop:true, //Зацикливаем слайдер
	autoplay: true,
	autoplayHoverPause: true,
	stagePadding: 0,
	loop:true,
	dots:true,
	dotsEach:true,
	margin:10,
	nav:true,
	navText:false,
	responsive:{
		0:{
			items:1
		},
		600:{
			items:1
		},
		1000:{
			items:1
		}
	  }
});


owl.on('changed.owl.carousel', function() { setActiveDots(); });
owl.on('dragged.owl.carousel', function() { setActiveDots(); });
$(".owl-next").click(function() {  setActiveDots(); });
$(".owl-prev").click(function() {  setActiveDots(); });



