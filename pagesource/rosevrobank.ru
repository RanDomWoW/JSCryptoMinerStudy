  },
  function() {
	  $('.owl-prev').not(this).stop(true, true).animate({"background-position-x":"18px", opacity: "0.0"}, 350); //0
	  $('.owl-next').not(this).stop(true, true).animate({"background-position-x":"18px", opacity: "0.0"}, 350); //0
  }
);

