<script>

var now = new Date();
var h = now.getHours();
var m = now.getMinutes();
var tasks = new Array();
var value = new Array();


$('select[name="form_dropdown_date"] option').each(function() {  
   tasks.push( $(this).text().split("-") ); 
   value.push( $(this).val() );
})

var disabled = true; //блокируем выбор времени из списка
for(var row = 0; row < tasks.length; row++) {

    //console.log(tasks[row][0] +"-"+ tasks[row][1]);
    var h_list = parseInt(tasks[row][0].replace(':00', ''),10);
    var h_list_max = parseInt(tasks[tasks.length-1][0].replace(':00', ''),10);


     if ( h > h_list && h < h_list_max  ) { // текущее время больше 
        //console.log( h +'>'+h_list+' && '+h+' < '+h_list_max);
        disabled = true;   // запретим выбрать из списка
     }else {
        disabled = false;  // разрешим выбрать из списка
     }

     if ( h == h_list  ) { // совпадение по часам

         if ( (60-parseInt(m,10)) <= 20 ) { // двадцать минут до закрытия часа
			 //console.log( '1');
            disabled = true; // запретим выбрать из списка

         }else {
			 //console.log( '2');
           var value = value[row];
           disabled = false; // разрешим выбрать из списка

         }
      }

    $('select[name="form_dropdown_date"] option[value="'+ value[row] +'"]').prop('disabled', disabled);
    $('select[name="form_dropdown_date"] option[value="'+ value +'"]').prop('selected', true);
}

</script></div>   </div>

