    <script type="text/javascript">
        $(document).ready(function () {
            $(function showanimation() {
                var supportInview = new Waypoint.Inview({
                    element: $('#support'),
                    enter: function (direction) {
//                        alert('Enter triggered with direction ' + direction)
                        $("#support ul>li").removeClass().addClass('wow animated bounceIn').show();
                        $("#support div.map").removeClass('wow animated fadeIn').addClass('wow animated fadeIn').show();
                    },
                    entered: function (direction) {
//                        alert('Entered triggered with direction ' + direction)
                        $("#support ul>li").removeClass().addClass('wow animated bounceIn').show();
                        $("#support div.map").removeClass('wow animated fadeIn').addClass('wow animated fadeIn').show();
                    },
                    exit: function (direction) {
//                        alert('Exit triggered with direction ' + direction)
                    },
                    exited: function (direction) {
//                        alert('Exited triggered with direction ' + direction)
                        $("#support ul>li").removeClass().hide();
                        $("#support div.map").removeClass('wow animated fadeIn').hide();
                    }
                });
                var productInview = new Waypoint.Inview({
                    element: $('#product'),
                    enter: function (direction) {
//                        alert('Enter triggered with direction ' + direction)
                        $("#product div.product_tv").removeClass('wow animated bounceIn').addClass('wow animated bounceIn').show();
                        $("#product div.product_box").removeClass('wow animated bounceIn').addClass('wow animated bounceIn').show();
                        $("#product ul.list").removeClass('wow animated bounceIn').addClass('wow animated bounceIn').show();
                    },
                    entered: function (direction) {
//                        alert('Entered triggered with direction ' + direction)
                        $("#product div.product_tv").removeClass('wow animated bounceIn').addClass('wow animated bounceIn').show();
                        $("#product div.product_box").removeClass('wow animated bounceIn').addClass('wow animated bounceIn').show();
                        $("#product ul.list").removeClass('wow animated bounceIn').addClass('wow animated bounceIn').show();
                    },
                    exited: function (direction) {
//                        alert('Exited triggered with direction ' + direction)
                        $("#product div.product_tv").removeClass('wow animated bounceIn').hide();
                        $("#product div.product_box").removeClass('wow animated bounceIn').hide();
                        $("#product ul.list").removeClass('wow animated bounceIn').hide();
                    }
                });

                var serviceInview = new Waypoint.Inview({
                    element: $('#service'),
                    enter: function (direction) {
                        $("#service div.list").removeClass('wow animated fadeIn').addClass('wow animated fadeIn').show();
                        $("#product div.khach_hang_moi").removeClass('wow animated fadeInLeft').addClass('wow animated fadeInLeft').show();
                        $("#product div.khach_hang_hien_tai").removeClass('wow animated fadeInLeft').addClass('wow animated fadeInLeft').show();
                    },
                    entered: function (direction) {
                        $("#service div.list").removeClass('wow animated fadeIn').addClass('wow animated fadeIn').show();
                        $("#product div.khach_hang_moi").removeClass('wow animated fadeInLeft').addClass('wow animated fadeInLeft').show();
                        $("#product div.khach_hang_hien_tai").removeClass('wow animated fadeInLeft').addClass('wow animated fadeInLeft').show();
                    },
                    exited: function (direction) {
                        $("#service div.list").removeClass('wow animated fadeIn').hide();
                        $("#product div.khach_hang_hien_tai").removeClass('wow animated fadeInLeft').hide();
                        $("#product div.khach_hang_moi").removeClass('wow animated fadeInLeft').hide();
                    }
                });

                var recruitmentInview = new Waypoint.Inview({
                    element: $('#recruitment'),
                    enter: function (direction) {
                        $("#recruitment div.content").removeClass('wow animated bounceInDown').addClass('wow animated bounceInDown').show();
                    },
                    entered: function (direction) {
                        $("#recruitment div.content").removeClass('wow animated bounceInDown').addClass('wow animated bounceInDown').show();
                    },
                    exited: function (direction) {
                        $("#recruitment div.content").removeClass('wow animated bounceInDown').hide();
                    }
                });
            });
            function onScroll(event) {
                var scrollPos = $(document).scrollTop();
                $('#top_menu a.is-croll').each(function () {
                    var href = $(this).attr("href"),
                            top = ($(href).offset() || {"top": NaN}).top;
                    if (!isNaN(top) && typeof top !== "undefined") {
                        var offsetTop = top - 80,
                                offsetHieght = $(href).outerHeight();
                        if ((offsetTop <= scrollPos) && (offsetTop + offsetHieght > scrollPos)) {
                            $('#top_menu li a').removeClass("active");
                            $(this).addClass("active");
                        } else {
                            $(this).removeClass("active");
                        }
                    }
                });
            }
            $(document).on("scroll", onScroll);
        });
        $(window).bind("load", function () {
            var urlHash = window.location.href.split("#")[1];
            if ((urlHash !== null) || (urlHash !== 'home'))
            {
                var top = ($('#' + urlHash).offset() || {"top": NaN}).top;

                if (!isNaN(top) && typeof top !== "undefined") {
                    $('html,body').animate({scrollTop: top - 80}, 800);
                }
            }
        });
    </script>

