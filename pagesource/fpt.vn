<script type="text/javascript">

    $(window).load(function () {
        var pagewidth = $(document).width();
        $('footer,#banner').width(pagewidth);
    });
    $(window).resize(function () {
        var win_width = $(window).innerWidth();
        if (win_width < 1024) {
            $('footer,#banner').width(1024);
        } else {
            $('footer,#banner').width(win_width);
        }
    });
    $('.fix_link_mega').click(function () {
        var url = $(this).attr('href').substr(0, 1);
        if (url == '#') {
            url = $(this).attr('href');
            $('#content-ps-adsl>div').removeClass('display');
            $(url).addClass('display');
            url = url.substr(1, url.length - 1);
            var zindex = $('#ps-adsl').attr('index');
            var str_width = $('#ps-adsl>div img').css('width').toString();
            var str_height = $('#ps-adsl>div img').height();
            $('#ps-adsl>div').each(function () {
                $(this).removeClass('inselect');
                $(this).css('z-index', $(this).attr('index'));
                $(this).children('img:nth-child(1)').css('z-index', zindex);
                if ($(this).attr('herf') == url) {
                    $(this).addClass('inselect');
                    $(this).css('z-index', zindex);
                    $(this).children('img:nth-child(1)').css('z-index', $(this).children('img:nth-child(1)').attr('index'));
                    var str_left = $(this).css('left').toString();
                    $('#content-ps-adsl>img').css('left', (parseInt(str_left.substring(0, str_left.length - 1)) + parseInt(str_width.substring(0, str_width.length - 2)) / 2 - 9) + 'px');
                }
            });
            var urlHash = window.location.href.split("#")[1];
            if ((urlHash !== null) || (urlHash !== 'home'))
            {
                var top = ($('#' + url).offset() || {"top": NaN}).top;
                $('html,body').animate({scrollTop: top - 120 - str_height}, 800);
            }
        }
    });
</script>
