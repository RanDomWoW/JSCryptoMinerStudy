	<script type="text/javascript">
		$(function(){
			if ($("#box").length)
			{
				$('#box').bounceBox();
				$('#box').bounceBoxToggle();
				
				var closeBox = function(){
					$('#box').bounceBoxHide();
				};
				var timerInt = setTimeout(closeBox, 10000);
				
				$('#box').hover(function(){
					$(this).find(".close").show();
					clearTimeout(timerInt);
				}, function(){
					$(this).find(".close").hide();
					timerInt = setTimeout(closeBox, 10000);
				});
				
				$("#box").find('.close').bind('click', function(e){
					e.preventDefault();
					closeBox();
				});
			}
		});
	</script>
