<script type="text/javascript">	
	var booking_buddy_url;
	var booking_buddy_params;
	var flights_param;
	var hotels_param;
	var packages_param;
	var cruises_param;
	function cookieProcess(cookie) {
		if ($.cookieBar && inArray(cookie.code, ["XX", "AT","BE","BG","CY","CZ","DE","DK","EE","IE","ES","FI","FR","GB","GR","HR","HU","IT","LT","LU","LV","MT","NL","PL","PT","RO","SE","SI","SK"])) {
			//cookiebar
			$.cookieBar({
				message: 'We use cookies to track usage and preferences, and for advertising purposes.',
				acceptText: 'Okay, I Understand',
				acceptButton: true,
				policyText: 'View cookie policy',
				policyURL: '/deals/cookiepolicy.php',
				policyButton: true,
				cookieName: 'cb-enabled'
			});
		}
		
				
		//search form params
		if (cookie.code == 'GB') {
			booking_buddy_url = "http://www.bookingbuddy.co.uk";
			booking_buddy_params = "af=21818648&oppLanderId=373&source=52732";
			booking_buddy_short_params = "af=21818648&source=52732";
			flights_param = '&search_mode=air';
			hotels_param = '&search_mode=hotel';
			packages_param = '&search_mode=holiday';
			cruises_param = '&search_mode=cruise';
		}
		else {
			booking_buddy_url = "http://www.bookingbuddy.com";
			booking_buddy_params = "af=14061334&oppLanderId=257&source=47032";
			booking_buddy_short_params = "af=14061334&source=47032";
			flights_param = '&search_mode=air';
			hotels_param = '&search_mode=hotel';
			packages_param = '&search_mode=vacation';
			cruises_param = '&search_mode=cruise';
		}
		
		//from/to city placeholders
		if (cookie.code == 'GB') {
			departure_city = 'LON or London, United Kingdom';
			arrival_city = 'PAR or Paris, France';
		}
		else {
			departure_city = 'MIA or Miami, FL';
			arrival_city = 'LAX or Los Angeles, CA';
		}
		
		$('input[name^="departure_city"]').attr('placeholder', departure_city);
		$('input[name^="arrival_city"]').attr('placeholder', arrival_city);
		
		//datepicker jquery ui function
		if (cookie.code == 'GB') {
			date_format = 'dd/mm/yy';
		}
		else {
			date_format = 'mm/dd/yy';
		}
		
		var date1 = new Date();
		var date2 = new Date();
		
		date1.setDate(date1.getDate() + 7);
		if (cookie.code == 'GB') {
			departure_date = ("0" + date1.getDate()).slice(-2) + '/' + ("0" + (date1.getMonth() + 1)).slice(-2) + '/' + date1.getFullYear();
		}
		else {
			departure_date = ("0" + (date1.getMonth() + 1)).slice(-2) + '/' + ("0" + date1.getDate()).slice(-2) + '/' + date1.getFullYear();
		}
		
		date2.setDate(date2.getDate() + 14);
		if (cookie.code == 'GB') {
			return_date = ("0" + date2.getDate()).slice(-2) + '/' + ("0" + (date2.getMonth() + 1)).slice(-2) + '/' + date2.getFullYear();
		}
		else {
			return_date = ("0" + (date2.getMonth() + 1)).slice(-2) + '/' + ("0" + date2.getDate()).slice(-2) + '/' + date2.getFullYear();
		}
		
		$('input[name^="departure_date"]').datepicker({minDate: 0, dateFormat: date_format, 
			beforeShow: function (input, inst) {
				setTimeout(function () {
					position = $(input).offset();
				}, 0);
			},
			onSelect: function (dateText, inst) {
				var date = $(this).datepicker('getDate');
				date.setTime(date.getTime() + (1000*60*60*24));
				$(this).parent().parent().find('input[name^="return_date"]').datepicker("option", "minDate", date);
				if ($(this).parent().parent().find('input[name^="return_date"]').datepicker("getDate") == null) {
					$(this).parent().parent().find('input[name^="return_date"]').datepicker("setDate", date);
				}
			}
		});
		$('input[name^="return_date"]').datepicker({minDate: 0, dateFormat: date_format, 
			beforeShow: function (input, inst) {
				setTimeout(function () {
					position = $(input).offset();
				}, 0);
			}
		});
		
		$('input[name^="departure_date"]').attr('placeholder', departure_date);
		$('input[name^="return_date"]').attr('placeholder', return_date);
		
		//hide cruises form for GB
		if (cookie.code == 'GB') {
			$("#FlightsForm1").show();
			$("#FlightsForm2").show();
			$("#PackagesForm1").show();
			$("#PackagesForm2").show();
			$("#HotelsForm1").show();
			$("#HotelsForm2").show();
			$("#CruisesForm1").remove();
			$("#CruisesForm2").remove();
		}
		else {
			$("#FlightsForm1").show();
			$("#FlightsForm2").show();
			$("#PackagesForm1").show();
			$("#PackagesForm2").show();
			$("#HotelsForm1").show();
			$("#HotelsForm2").show();
			$("#CruisesForm1").show();
			$("#CruisesForm2").show();
		}
		
		$(".sponsored h5 a").tooltip();
		$(".sponsored h5 a").on('touchend click', function(event) {
			$(".sponsored h5 a").tooltip('close');
			$(this).tooltip('open');
			event.stopImmediatePropagation();
			event.preventDefault();
		});
		$(document).on('touchend click', function(event) {
			$(".sponsored h5 a").tooltip('close');
		});
	}
	
	$(document).ready(function() {
		var cookie = '';
		cookie = decodeURIComponent(getCookie('glm_country'));
		try {
			cookie = unserialize(cookie);
		}
		catch (err) {
			cookie = null;
		}
		
		if (cookie) {
			$.get("//www.golastminute.com/geoip/ip.php", function(ipAddress) {
					if (cookie.long && cookie.long == ipAddress) {
						cookieProcess(cookie);
					}
					else {
						$.get("//www.golastminute.com/geoip/index.php", function(cookie) {
								setCookie('glm_country', encodeURIComponent(cookie), 365, '.golastminute.com');//domain=.golastminute.com
								cookie = unserialize(cookie);
								cookieProcess(cookie);
							}
						);
					}
				}
			);
		}
		else {
			$.get("//www.golastminute.com/geoip/index.php", function(cookie) {
					setCookie('glm_country', encodeURIComponent(cookie), 365, '.golastminute.com');//domain=.golastminute.com
					cookie = unserialize(cookie);
					cookieProcess(cookie);
				}
			);
		}
	
		$('.flexslider').flexslider({
			animation: "fade",
			controlsContainer: $(".custom-controls-container"),
			customDirectionNav: $(".custom-navigation a")
		});
	});	
</script>
