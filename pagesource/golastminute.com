function showContinuePopup(id) {
	if (!$("#overlay").length) {
		$("body").append("<div id='overlay'></div>");
	
		var docHeight = $(document).height();
		$("#overlay").height(docHeight).css({
			//'opacity' : 0.4,
			'position': 'absolute',
			'top': 0,
			'left': 0,
			'background-color': 'rgba(0,0,0,0.6)',
			'width': '100%',
			'z-index': 102
		});
		$(window).resize(function() {
			var docHeight = $(document).height();
			$("#overlay").height(docHeight);
		});
	}
	else {
		$("#overlay").show();
	}
	
	var scroll1 = 0;
	var scroll2 = 0;
	if ($('.rb-enabled').length) {
		scroll1 += $('.rb-enabled').outerHeight();
	}
	if ($('.cb-enabled').length) {
		scroll1 += $('.cb-enabled').outerHeight();
	}
	scroll2 += $('#topheader').outerHeight();
	scroll2 += $('#header').outerHeight();
	scroll2 += 31; //margin-top compensation
	
	var element = $('#' + id).detach();
	element.css('marginTop', scroll1 + scroll2 + 'px');
	$('#overlay').append(element);
	
	$('#' + id).show();
	return false;
}		
function hideContinuePopup() {
	$("#overlay").hide();
	$(".emailPopup").hide();
	return false;
}

function compareFare(oForm) {
	var placement;
	if (oForm.indexOf("1") > 0){
		placement = "ATF";
	}
	else{
		placement = "BTF";
	}
	
	var departure_city = '';
	var arrival_city = '';
	var departure_date = document.forms[oForm]["departure_date"].value;
	var return_date = document.forms[oForm]["return_date"].value;
	departure_city = $(document.forms[oForm]["departure_city"]).attr('data-code');
	departure_city_code = departure_city.substr(0,3);
	if (departure_city) {
		for (i=0; i < compareAvailableTags.length; i++) {
			if (compareAvailableTags[i].code == departure_city.substr(0,3)) {
				departure_city = compareAvailableTags[i].label;
				break;
			}
		}
	}
	arrival_city = $(document.forms[oForm]["arrival_city"]).attr('data-code');
	arrival_city_code = arrival_city.substr(0,3);
	if (arrival_city) {
		for (i=0; i < compareAvailableTags.length; i++) {
			if (compareAvailableTags[i].code == arrival_city.substr(0,3)) {
				arrival_city = compareAvailableTags[i].label;
				break;
			}
		}
	}
	
	//one-way / roundtrip
	var roundtrip = getRadioValue(document.forms[oForm]["search_type"]);
	if (return_date.length == 0) {
		roundtrip = 'oneway';
	}
	
	var ddepdate = $("#" + document.forms[oForm]["departure_date"].id).datepicker("getDate");
	departure_date_ymd = ddepdate.getFullYear() + '-' + ("0" + (ddepdate.getMonth() + 1)).slice(-2) + '-' + ("0" + ddepdate.getDate()).slice(-2);
	if (roundtrip == 'roundtrip') {
		var dretdate = $("#" + document.forms[oForm]["return_date"].id).datepicker("getDate");
		return_date_ymd = dretdate.getFullYear() + '-' + ("0" + (dretdate.getMonth() + 1)).slice(-2) + '-' + ("0" + dretdate.getDate()).slice(-2);
	}
	else {
		return_date_ymd = '';
	}
	
	var url = [];
	if (booking_buddy_url == 'http://www.bookingbuddy.com') {
		url.push(booking_buddy_url + "/en/partner/hero/?tab_limit=6&mode=air&source=47032&time1=anytime&time2=anytime&flight_class=economy_coach&oneway="+(roundtrip=='oneway'?'true':'false')+"&origin="+encodeURIComponent(departure_city_code)+"&destination="+encodeURIComponent(arrival_city_code)+"&travelers="+document.forms[oForm]["num_travelers"].value+"&date1="+departure_date_ymd+"&date2="+return_date_ymd+"&traqparam="+departure_city_code+","+arrival_city_code);
	}
	else {
		url.push(booking_buddy_url + "/en/partner/hero/?tab_limit=6&mode=air&source=52732&time1=anytime&time2=anytime&flight_class=economy_coach&oneway="+(roundtrip=='oneway'?'true':'false')+"&origin="+encodeURIComponent(departure_city_code)+"&destination="+encodeURIComponent(arrival_city_code)+"&travelers="+document.forms[oForm]["num_travelers"].value+"&date1="+departure_date_ymd+"&date2="+return_date_ymd+"&traqparam="+departure_city_code+","+arrival_city_code);
	}
	url.push("http://travel.mediaalpha.com/hosted/form.html?placement_id=Nlwv3WS0TB9ZrtxJgHHOI7rEgM-Fmg&type=ad_unit&data.flights[0].date="+departure_date_ymd+"&data.flights[0].origin="+encodeURIComponent(departure_city_code)+"&data.flights[0].destination="+encodeURIComponent(arrival_city_code)+"&data.num_adults=="+document.forms[oForm]["num_travelers"].value+"&sub_1=golastminute&sub_2=site-dt-homepage-flight-"+placement.toLowerCase()+"&sub_3=&bg=0&title=FFFFFF&w=1000");
	if (roundtrip == 'roundtrip') {
		url.push("http://travel.mediaalpha.com/hosted/form.html?placement_id=Nlwv3WS0TB9ZrtxJgHHOI7rEgM-Fmg&type=ad_unit&data.flights[0].date="+departure_date_ymd+"&data.flights[1].date="+return_date_ymd+"&data.flights[0].origin="+encodeURIComponent(departure_city_code)+"&data.flights[1].destination="+encodeURIComponent(departure_city_code)+"&data.flights[0].destination="+encodeURIComponent(arrival_city_code)+"&data.flights[1].origin="+encodeURIComponent(arrival_city_code)+"&data.num_adults="+document.forms[oForm]["num_travelers"].value+"&sub_1=golastminute&sub_2=site-dt-homepage-flight-"+placement.toLowerCase()+"&sub_3=&bg=0&title=FFFFFF&w=1000");
	}
	openPopup(departure_city_code, arrival_city_code, roundtrip, url, 'nsrc', 'Compare Fare - Home Page - Flights - ' + placement);
	
	window.criteo_q.push(
		{ event: "setAccount", account: 43619 },
		{ event: "setSiteType", type: deviceType },
		{ event: "trackTransaction", id: 'site-' + (new Date()).getTime(), item: [
			{ id: $(document.forms[oForm]["departure_city"]).attr('data-code') + '-' + $(document.forms[oForm]["arrival_city"]).attr('data-code'), price: (deviceType != 'm') ? 0.40 : 0.10, quantity: 1 }
		]}
	);
}
