$(document).ready(function() {
	$('input[name=search_type]').on('click', function () {
		if ($(this).val() == 'roundtrip') {
			$(this).closest('.postarea-details-box-bottom').find('input[name=return_date]').parent().show();
		}
		else {
			$(this).closest('.postarea-details-box-bottom').find('input[name=return_date]').parent().hide();
		}
	});
	
	function getAvailableTag(code, element) {
		$(element).val('');
		$(element).removeAttr("data-code");
		$.ajax({
			url: "/apiservices/autosuggest/v1.0/US/USD/en-US/?includeAirports=true&includeCities=true&includeCountries=false",
			dataType: "json",
			data: {
				query: code
			},
			success: function( data ) {
				for (i=0; i < data['Places'].length; i++) {
					airport_code = data['Places'][i]['PlaceId'].replace('-sky', '');
					city_code = data['Places'][i]['CityId'].replace('-sky', '');
					label = data['Places'][i]['PlaceName'];
					if (data['Places'][i]['RegionId']) {
						label += ', ' + data['Places'][i]['RegionId'];
					}					
					label += ', ' + data['Places'][i]['CountryName'].replace(/ \([A-Z]+\)/g, "");
					label += ' (' + code + ')';
					if (code.length == 4) {
						label += ' - All Airports';
					}
					//hacky, should not validate
					if (city_code == code) {
						$(element).val(label);
						$(element).attr('data-code', city_code);
						$(element).trigger('change');
					}					
					if (airport_code == code) {
						$(element).val(label);
						$(element).attr('data-code', airport_code);
						$(element).trigger('change');
					}
				}
			}
		});
	}
	
	var availableTags = function( request, response ) {
		$.ajax({
			url: "/apiservices/autosuggest/v1.0/US/USD/en-US/?includeAirports=true&includeCities=true&includeCountries=false",
			dataType: "json",
			data: {
				query: request.term
			},
			success: function( data ) {
				resp = [];
				for (i=0; i < data['Places'].length; i++) {
					code = data['Places'][i]['PlaceId'].replace('-sky', '');
					label = data['Places'][i]['PlaceName'];
					if (data['Places'][i]['RegionId']) {
						label += ', ' + data['Places'][i]['RegionId'];
					}					
					label += ', ' + data['Places'][i]['CountryName'].replace(/ \([A-Z]+\)/g, "");
					label += ' (' + code + ')';
					if (code.length == 4) {
						label += ' - All Airports';
					}
					resp.push( {label: label, code: code} );
				}
				response( resp );
			}
		});
	};	
	
