		$(document).ready(function(){
			var cookieExist = jQuery.cookie('logo');
			if (cookieExist) {
				$('#logoAnimate').remove();
				$('#logo').show();
			}
			else {
				$.cookie('logo', 'value',{ expires: null });
				$('#logo').remove();
				$('#logoAnimate').show();
			}
			
			$('#nav>li>ul').removeAttr('style').hide();
			$('#nav li').addClass('activeLi');
			
			if($('#nav>li').hasClass('current')){
				$('.current').find('.top-triangle').show();
				$('.current>ul').show();
