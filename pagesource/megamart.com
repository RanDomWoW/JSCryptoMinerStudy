<script type="text/javascript">
$(function(){

	//getOneSearchKeyword();

	$("input[name=one_search_input]").keydown(function(e) {
		if(e.keyCode==13) {
			e.preventDefault();
			var rownum=$(this).attr("rownum");
			if(rownum==10) {
				oneKeywordSearch();
				return false;
			} else {
				var next=parseInt(rownum)+1;
				$("#one_search_input_"+next).focus();
				return false
			}
		}
	});
});


function oneKeywordSearch() {

	var arrOneSearchKeyWords = "";
	var firstSarchKeyWord = "";
	var tempKeyword = "";

	for(var i=0; i<10; i++ ) {
		tempKeyword = $.trim( $("#one_search_input_"+(i+1) ).val() );
		if( tempKeyword != "" ) {
			tempKeyword = tempKeyword.replace(/\;/g,"");			// 세미콜론 지우기!!!
			//console.log( $("#one_search_input_"+(i+1) ).val() );
			if(arrOneSearchKeyWords!="") {
				//arrOneSearchKeyWords += "||";
			} else {
				firstSarchKeyWord = tempKeyword;
			}
			arrOneSearchKeyWords += "||"+ tempKeyword;
		}
	}

	if( arrOneSearchKeyWords == "" ) {
		alert('text.account.keyword.bookmark.write');
		return;
	}

	//console.log("arrOneSearchKeyWords : "+arrOneSearchKeyWords);
	//console.log("one_search : "+$("#one_search").val());

	$("#exhibitionCode").val('');

	$("#one_search").val(arrOneSearchKeyWords);

	//console.log("one_search : "+$("#one_search").val());

	$("input[name=one_search]").val(arrOneSearchKeyWords);
	$("#input_SearchBox").val(firstSarchKeyWord);
	megaKeywordSearch();

}

function getOneSearchKeyword() {
	$("#onepunch_list").html();
}

function setOneSearchKeyword() {
	
	
	alert('로그인 후 이용가능합니다.');
	
}

</script>
