<script type="text/javascript">

	$(function(){
		
		//íì ëì°ê¸°
		var siteType = getSystemName();
		var url = contextRoot +"/customerCenter/popupList.ajax";
		var param = {'siteType' : siteType};

		commonAjax(url, param, "json", "POST").done(function( data, textStatus, jqXHR ) {

			var popupList = data.resultList;

			for(var idx = 0; idx < popupList.length; idx++){
				if ("2731" == "2731") {
					break;
				}
				
				if("true" != $.cookie( 'noticePop-' + popupList[idx].POP_ID )){

					var locationOrigin = "http://" + window.location.hostname;

					if(locationOrigin.indexOf("local") > 1 || locationOrigin.indexOf("dev") > 1){
						locationOrigin = "http://www.megamart.com";
					}

					var attachUrl = "/attach/board_images/";

					var imgName		= locationOrigin + attachUrl +popupList[idx].IMAGE_NAME;

					if(popupList[idx].POP_GUBUN == '1' && "mobileApp" != "mobileApp"){

						var feature = {
								width : popupList[idx].SIZE_WIDTH ,
								height :popupList[idx].SIZE_HEIGHT+30,
								top: popupList[idx].Y_POSITION+200,
								left: popupList[idx].X_POSITION,
								menubar: 0,
								location : false,
								winname : "popupList"+idx
							};


						var imgWidth	= popupList[idx].SIZE_WIDTH - 15;
						var imgHeight	= popupList[idx].SIZE_HEIGHT - 45;
						//var imgName		= popupList[idx].IMAGE_NAME;
						var cookieName = "'noticePop-" + popupList[idx].POP_ID + "'";
						var imgUrl = "'" + popupList[idx].IMAGE_URL + "'";
						var strFunction = "'" + "function" + "'";

						strFeatures = JSONToString(feature).replace(/{|}/g, "").replace(/:\s*/g, "=").replace(/\"/g, "");

						var popUp = window.open("", 'noticePop-' + popupList[idx].POP_ID, strFeatures, false);
						var popDoc = popUp.document;

						popDoc.open();
						popDoc.write('<style>html,body{margin:0px; padding:0px; width:100%;}</style>');
						popDoc.write('<form name="popupForm" style="margin:0px; padding:0px; display:block; width:100%; height:100%;">');
						popDoc.write('	<div style="margin:0px; padding:0px; display:block; width:100%; height:auto;">');
						popDoc.write('		<div style="margin:0px; padding:0px; display:block; width:100%; height:100%;">');
						if (popupList[idx].IMAGE_URL != "" && popupList[idx].IMAGE_URL != "#" && popupList[idx].IMAGE_URL != null) {
							popDoc.write('			<a href="#" style="margin:0px; padding:0px; display:block; width:100%; height:100%; border:none;" onClick="javascript:opener.location.href=' + imgUrl + ';window.close();">');
							popDoc.write('				<img src="' + imgName + '" alt="image" style="position:relative; height:auto; width:100%; margin:0 auto;" border="0"/>');
							popDoc.write('			</a>');
						} else {
							popDoc.write('				<img src="' + imgName + '" alt="image" style="position:relative; height:auto; width:100%; margin:0 auto;" border="0"/>');
						}
						popDoc.write('		</div>');
						popDoc.write('		<div>');
						popDoc.write('			<div style="margin:0px auto;position:absolute; bottom:0px; left:0px; background:#d1d3d4; width:100%; height:30px; line-height:30px; font-size:12px;">');
						popDoc.write('				<input type="checkbox" id="cookieCheck" value="true" style="vertical-align:middle;margin-top:0px;" /> ');
						popDoc.write('				<label for="cookieCheck">closed</label>');
						
						popDoc.write('				<div onClick="javascript:if(document.popupForm.cookieCheck.checked && typeof window.opener.openPopCookie == ' + strFunction + '){window.opener.openPopCookie(' + cookieName + ');}window.close();" style="cursor:pointer; position:relative;float:right; right:10px;">');
						popDoc.write('					Close');
						popDoc.write('					<img src="/_ui/desktop/theme-megamart/images/close_btn.gif" alt="" style="width:14px; height:14px; margin-top:-3px; vertical-align:middle;"/>');
						popDoc.write('				</div>');
						popDoc.write('			</div>');
						popDoc.write('		</div>');
						popDoc.write('	</div>');
						popDoc.write('</form>');

						popDoc.close();
						popUp.focus();

					}else if(popupList[idx].POP_GUBUN == '2'){

						$("#layerPop"+idx).css("width", popupList[idx].SIZE_WIDTH);
						$("#layerPop"+idx).css("height", popupList[idx].SIZE_HEIGHT + 25);
						$("#layerPop"+idx).css("top", popupList[idx].Y_POSITION);
						$("#layerPop"+idx).css("left", popupList[idx].X_POSITION);
						$("#popImageDiv"+idx).css("height", popupList[idx].SIZE_HEIGHT);
						//$("#layerPopTitle"+idx).text(popupList[idx].TITLE);

						$("#layerPopClose"+idx).val('noticePop-' + popupList[idx].POP_ID);

						$("#popImage"+idx).attr("src", imgName);
						$("#popImage"+idx).css("width", popupList[idx].SIZE_WIDTH);
						$("#popImage"+idx).css("height", popupList[idx].SIZE_HEIGHT);
						
						if (popupList[idx].IMAGE_URL != "" && popupList[idx].IMAGE_URL != "#" && popupList[idx].IMAGE_URL != null) {
							$("#popImageLink"+idx).attr("href", popupList[idx].IMAGE_URL );
						}

						$("#layerPop"+idx).show();
					}

				}
			}
		}).fail(function( jqXHR, textStatus, errorThrown ) {
			//alert("System error. Please try again later.");
		}).always(function( a, textStatus, b ) {
		});

		$('#layerPopClose0').click(function() {
			var weekChk = $('input:checkbox[id="weekChkBox0"]:checked').val();
			if(weekChk) {
				$.cookie($(this).val(), "true", { expires: 1} );
			}
			$("#layerPop0").hide();
		});

		$('#layerPopClose1').click(function() {
			var weekChk = $('input:checkbox[id="weekChkBox1"]:checked').val();
			if(weekChk){
				$.cookie($(this).val(), "true", { expires: 1} );
			}
			$("#layerPop1").hide();
		});
		
		$('#layerPopClose2').click(function() {
			var weekChk = $('input:checkbox[id="weekChkBox2"]:checked').val();
			if(weekChk){
				$.cookie($(this).val(), "true", { expires: 1} );
			}
			$("#layerPop2").hide();
		});

	});

</script>