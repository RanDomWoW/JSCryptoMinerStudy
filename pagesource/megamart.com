function deleteAllRecentKwd() {
	var cName = ''.toLowerCase()+'-recentKeyword';
	var cDomain = '.megamart.com'; // 운영
	var locationOrigin = window.location.hostname;
	if (locationOrigin.indexOf("local") > -1) {
		cDomain = '.local.com'; // 로컬
	}
	else if(locationOrigin.indexOf("dev") > -1) {
		cDomain = '.megamartdev.com'; // 개발
	}

	deleteAllCookie(cName, cDomain);
	var noRecentKeywordHtml = '<li class="none_page" id="noKeyword">최근 검색어가 없습니다.</li>';
