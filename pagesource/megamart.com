	<script type="text/javascript">
	/*<![CDATA[*/
	var postErrorText = "에러가 발생하였습니다.";
	var postCallBackName = "postCallBack";
	function postPopupOpen(callBackName) {
		if(callBackName != "") {
			postCallBackName = callBackName;
		}
		selectSidoList("/megamart/en/post/sido.ajax",postErrorText);
		$("#postPopup").show();
	}

	function postStreet() {
		var choiceText = "Choice";
		var sidoMsg = "시/도를 선택해 주세요.";
		var gunguMsg = "시/군/구를 선택해 주세요.";
		var streetMsg = "Enter street name.";
		var bldgNoMsg = "Enter building number.";
		selectStreetList("/megamart/en/post/street.ajax",postErrorText,choiceText,sidoMsg,gunguMsg,streetMsg,bldgNoMsg,postCallBackName);
	}

	function postDong() {
		var choiceText = "Choice";
		var dongMsg = "동(읍/면)을 입력해 주세요.";
		selectDongList("/megamart/en/post/dong.ajax",postErrorText,choiceText,dongMsg,postCallBackName);
	}

	function postClose() {
		postTab(1);
		$("#streetListDiv").hide();
		$("#dongListDiv").hide();
		$("#postPopup").hide();
	}

	function postTab(gubun) {
		if(gubun == 1) {
			$("#postTabLi2").removeClass("current");
			$("#postTabLi1").addClass("current");
			$("#street").val("");
			$("#bldgNo").val("");
			$("#postTab1").show();
			$("#postTab2").hide();
			$("#dongListDiv").hide();
			$("#dongList").html("");
		} else {
			$("#postTabLi1").removeClass("current");
			$("#postTabLi2").addClass("current");
			$("#postTab2").show();
			$("#postTab1").hide();
			$("#streetListDiv").hide();
			$("#streetList").html("");
			$("#dong").val("");
			selectSidoList("/megamart/en/post/sido.ajax",postErrorText);
		}
