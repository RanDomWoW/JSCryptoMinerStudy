	function searchAddr() {
		var choiceText = "Choice";
		var postalText = "Enter postalcode number.";
		var cityText = "Enter city name.";
		
		if ($("#postcode").val() == "" && $("#city").val() == "") {
			alert(postalText);
			$("#postcode").focus();
			return;
		}
		
		$.ajax({
		    url:"/login/ups/addressValidationRequest"
		    , type:"post"
		    , timout:1000
		    , contentType:"application/x-www-form-urlencoded; charset=UTF-8"
		    , datatype : "xml"
		    , data: "strCity="+$("#city").val()+"&strPostCode="+$("#postcode").val()+"&strState="+$("#stateSel").val()
		    /* ,success:function(data){
		       alert(data);
		    } */
		    , success: function(data, textStatus){
		       // alert(textStatus);
				if(textStatus == "success") {
					var xmlDoc = $.parseXML(data.result);
			        //alert(xmlDoc);
					$xml = $( xmlDoc );

					if( $xml.find( "ResponseStatusCode" ).text() == "1" ) {
						// success
						$addressList = $xml.find( "AddressValidationResult" );

						var addHeml = "";
						var addressCount = $addressList.length;
						//alert("count : " + addressCount);
						// for
						$addressList.each(function(){
							var city = "";
							var stateProvinceCode = "";
							var postalCode = "";
// 							alert("Rank : " + $(this).find("Rank").text() );
// 							alert("Quality : " + $(this).find("Quality").text() );
// 							alert("City : " + $(this).find("City").text() );
// 							alert("StateProvinceCode : " + $(this).find("StateProvinceCode").text() );
// 							alert("PostalCodeLowEnd : " + $(this).find("PostalCodeLowEnd").text() );
// 							alert("PostalCodeHighEnd : " + $(this).find("PostalCodeHighEnd").text() );
							
							city = $(this).find("City").text();
							stateProvinceCode = $(this).find("StateProvinceCode").text();
							postalCode = $(this).find("PostalCodeLowEnd").text();
							
							addHeml += "<li>" + city + " " + stateProvinceCode  + " " + postalCode + 
							" <a href=\"javascript:;\" onclick=\""+postCallBackName+"('"+stateProvinceCode+"','"+postalCode+"','"+city+"');\">"+choiceText+"</a></li>";
						});

						$("#addressList").html(addHeml);
						$("#addressListDiv").show();
					} else {
						// failure
						//alert("ErrorCode : "+$xml.find( "ErrorCode" ).text() );
						var noDataText = "Data not found. Please try again!!";
						alert(noDataText);
					    // alert("ResponseStatusDescription : "+$xml.find( "ResponseStatusDescription" ).text() );
					}

				} else {
					// httpclient error
					alert( textStatus );
				}
			}
			,error:function(xhr,status,e){
		        alert(xhr.responseText);
		    }
		});
	}

