            statusCode: {
              401: function() {
                bootbox.alert("Ten\u00e9s que estar registrado como usuario");
              },
              405: function() {
                bootbox.alert("Ya est\u00e1s siguiendo el \u00e1lbum");
              }
            },
            complete: function() {
              $button.removeClass('do-subscribe');
            }
          });
        }
      });
    });
  });
</script>
