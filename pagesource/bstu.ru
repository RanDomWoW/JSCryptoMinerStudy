<script type="text/javascript" src="http://tihov.fitdev.ru/countdown/flipcounter.min.js"></script>
<script src="http://tihov.fitdev.ru/countdown/moment.min.js"></script>
<script src="http://tihov.fitdev.ru/countdown/ru.js"></script>

<script>
var timer;


function declOfNum(number, titles) {
	cases = [2, 0, 1, 1, 1, 2];
	return titles[(number % 100 > 4 && number % 100 < 20) ? 2 : cases[(number % 10 < 5) ? number % 10 : 5]];
}

function getDates() {
	var countdowns = $('.countdown');
	var dates = [];
	countdowns.each(function() {
		dates.push($(this).attr('date'));
	});

	return dates;
}


$(function() {
	moment.lang('ru');
	var dates = getDates();
	var $message = $('.countdown');

	console.log('dates',dates)

	dates.forEach(function(date) {
	//'', 'YYYYMMDDhhmmss'
		var start = moment(Number(date));
		console.log('start',start)
		//var start = moment('20130810130000', 'YYYYMMDDhhmmss');
		var diff = start.diff(moment('20180317194035', 'YYYYMMDDhhmmss'), 'seconds');
		var days = Math.floor(diff / 86400);
		if (days < 10) $('#d-' + date).show();
		var delta = (days * 86400);
		var hours = Math.floor((diff - delta) / 3600);
		if (hours < 10) $('#h-' + date).show();
		delta += hours * 3600;
		var minutes = Math.floor((diff - delta) / 60);
		if (minutes < 10) $('#m-' + date).show();
		delta += minutes * 60;
		var seconds = diff - delta;
		if (seconds < 10) $('#s-' + date).show();
		var days = new flipCounter('days-' + date, {value: days, inc: -1, auto: false});
		var hours = new flipCounter('hours-' + date, {value: hours, inc: -1, auto: false});
		var mins = new flipCounter('minutes-' + date, {value: minutes, inc: -1, auto: false});
		var secs = new flipCounter('seconds-' + date, {value: seconds, inc: -1, auto: false});
		new flipCounter('d-' + date, {value: 0, auto: false});
		new flipCounter('h-' + date, {value: 0, auto: false});
		new flipCounter('m-' + date, {value: 0, auto: false});
		new flipCounter('s-' + date, {value: 0, auto: false});

		var countdown = function() {
			if (secs.getValue() == 10)
				$('#s-' + date).show();
			if (secs.getValue() == 0) {
				$('#s-' + date).hide();
				secs.setValue(60);
				mins.step();
			}
			secs.step();
			if (mins.getValue() < 10)
				$('#m-' + date).show();
			if (mins.getValue() == 0) {
				$('#m-' + date).hide();
				mins.setValue(59);
				hours.step();
			}
			if (hours.getValue() < 10)
				$('#h-' + date).show();
			if (hours.getValue() == 0 && days.getValue() > 0) {
				$('#h-' + date).hide();
				hours.setValue(23);
				days.step();
			}
			if (days.getValue() < 10)
				$('#d-' + date).show();
			//if (days.getValue() == 0) {
			//	$('#d0').hide();
			//}
			//clearInterval(timer);
			$('.days.time-text').html(declOfNum(days.getValue(), ['День', 'Дня', 'Дней']));
			$('.hours.time-text').html(declOfNum(hours.getValue(), ['Час', 'Часа', 'Часов']));
			$('.minutes.time-text').html(declOfNum(mins.getValue(), ['Минута', 'Минуты', 'Минут']));
			$('.seconds.time-text').html(declOfNum(secs.getValue(), ['Секунда', 'Секунды', 'Секунд']));
		};

		setTimeout(function() {
			countdown();
			timer = setInterval(countdown, 1000);
		}, 1000);

	});
});
</script>
