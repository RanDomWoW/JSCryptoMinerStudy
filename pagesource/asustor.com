<script>
	
	$(document).ready(function(){
		
		
			/*=========search_bar================*/


	
	
		/*=========================*/	
		
		$('.search_input').focus(function(){
			 $(".search").addClass("search_active");
		  	 $("#lan").fadeOut(200);	
			
		}).focusout(function() {
        	$(".search").removeClass("search_active");
		    $("#lan").fadeIn(400);    
        });
		
		
		
		
		$("#menu li").click(function(){
			var _id = $(this).children("a").attr('id');
			
			var _active = $(this).attr('class');
			
			
			$(".nav_sub").hide();			
			
			
			if(_active == 'flow active')
			{
				$("#nav_sub"+_id).fadeIn("fast");	
			}
			
			
			

				
    	});
		
		$(document).click(function(event) 
		{	
			$(".nav_sub").hide();		
			$("#menu ul").stop().fadeOut(200);
			$("#menu li").removeClass("active");
			$("#menu li").find("a").css('color', '');
		});
		
		
		


	});
	
</script>

