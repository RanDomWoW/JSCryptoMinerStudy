                         function inViewPort(el) {
                              var viewport = {};
                             viewport.top = $(window).scrollTop();
                             viewport.bottom = viewport.top + $(window).height();
                             var bounds = {};
                             bounds.top = el.offset().top;
                             bounds.bottom = bounds.top + el.outerHeight();
