          <h2 class="blockTitle">OpenIDログイン</h2>
      <div class="blockContent"><style>
img.openid_button {
 border-style: none;
 cursor: pointer;
}
#openid_identifier {
 background: #fff url(https://blender.jp/modules/openid/images/login-bg.gif) no-repeat left;
 color: #000;
 padding-left: 18px;
 width: 50%;
}
</style>
<script type="text/javascript">
function changeServer(s, start, size) {
    var o = document.getElementById('openid_identifier');
    o.value = s;
    o.parentNode.style.display = 'block';
    o.focus();

    if (o.createTextRange) {
        try {
            var tr = o.createTextRange();    
            tr.collapse(true);
            tr.moveStart('character', start);
            tr.moveEnd('character', size);
            tr.select();
        } catch (e) {
        }
    } else {
        o.setSelectionRange(start, start + size);
    }
}
</script>
<p>OpenIDを入力</p>
