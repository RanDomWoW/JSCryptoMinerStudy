<script type="text/javascript">
	$('.dangkyhocthu').click(function(){
		var top_scroll = 0;
		top_scroll = $("#box_dangkyhocthu").offset().top;
		var topPage = $(window).scrollTop();
		var gotop = 50;
		if(topPage < 100) {
			gotop = 150
		}
		$('html,body').animate({
			scrollTop: top_scroll -(gotop)
		}, 'slow');
	});

	$(document).ready(function() {
		function showScroll() {
			var topPage = $(window).scrollTop();
		    var top_box_course = $('.block_header').outerHeight() + $('.block_slide').outerHeight() - 250;
			if(topPage >= top_box_course) {
				$('.controls_dangky').fadeIn();
				$('.box_scroll_right').fadeIn();
			} else {
				$('.controls_dangky').fadeOut();
				$('.box_scroll_right').fadeOut();
			}
		}
		showScroll();
		
		$(window).scroll(function () {
			showScroll();
		});
	});

	 $.get( "/api/json/coso", function( data ) {
				data = JSON.parse(data);
				var htmlFooter = '<div class="container"><div class="row">';
				var htmlSelect = '';
				if(data.length > 0) {
					for (var i = 0; i < data.length; i++) {
						htmlFooter += '<div class="col-sm-3"><div class="bottom">';
							htmlFooter += '<p class="title">'+ data[i]['title'] +'</p>';
							htmlFooter += '<p class="text"><i class="fa fa-map-marker"></i>'+ data[i]['address'] +'</p>';
							htmlFooter += '<p class="text"><i class="fa fa-phone"></i>'+ data[i]['tel'] +'</p>';
						htmlFooter += '</div></div>';

						htmlSelect += '<option value="'+ data[i]['id'] +'">'+ data[i]['name'] +': '+ data[i]['address'] +'</option>';
					};
				}
				htmlFooter += '</div></div>';
				$('.api-coso-list').html(htmlFooter);
				$('.api-coso').append(htmlSelect);
			});

	  function dangkytrainghiem(frmItem) {
				var urlAjax = '/daotao/ajax/dangkytrainghiem';
				if(frmItem == undefined || frmItem == null) {
					var frmItem = '#frmDangkyTrainghiem';
				}
				$.ajax({
					url: urlAjax,
					type: 'POST',
					data: $(frmItem).serializeArray(),
					beforeSend: function () {
						$(frmItem + ' #btn-gui').val('Đang gửi thông tin...');
					},
					success: function (string) {
						var getData = $.parseJSON(string);
						if (getData.ketqua == 1) {
							$(frmItem + ' #btn-gui').val('Gửi thông tin');
							if (getData.name) {
								alert('Bạn chưa nhập họ tên');
								$(frmItem + ' #name').focus();
								return false;
							}
							if (getData.phone) {
								alert('Bạn chưa nhập số điện thoại');
								$(frmItem + ' #phone').focus();
								return false;
							}
							if (getData.email) {
								alert('Địa chỉ email bạn nhập không đúng');
								$(frmItem + ' #email').focus();
								return false;
							}
							if (getData.id_coso) {
								alert('Bạn cần chọn ít nhất 1 cơ sở');
								return false;
							}
						} else {
							alert('Thông tin của bạn đã được gửi thành công');
							window.location = "";
							$(frmItem).html('<div class="thongbao" style="text-align: center; padding: 20px; font-size: 20px;">Thông tin của bạn đã được gửi thành công</div>');
						}
					}
				});
			}
</script>
	<script type="text/javascript">
    $(window).load(function () {
        var h_center = $('.center_content').outerHeight();
        var h_right = $('.right_content').outerHeight();
        var h_scroll = $('#right_scroll').outerHeight();
        var right_scroll = $('#right_scroll');
        var offset = right_scroll.offset();
        var h_mxh = $('.block_mxh').outerHeight();
        var mxh_scroll = $('.block_mxh');
        var mxh_offset = mxh_scroll.offset();
        var center_scroll = $('.center_content');
        var offset_center = center_scroll.offset();
        var footer_scroll = $('.box_footer');
        var offset_footer = footer_scroll.offset();
        var stop = offset_footer.top - h_scroll - 50;
        var stop_mxh = offset_footer.top - h_mxh - 120;

        $(window).scroll(function () {
            if (h_right < h_center) {
                var topPage = $(window).scrollTop() + 170;

                if (topPage >= mxh_offset.top) {
                    topPage = topPage + 50 - offset_center.top;
                    if (topPage <= stop_mxh) {
                        $(".block_mxh").css({'top': topPage + 'px'});
                    } else {
                        $(".block_mxh").css({'top': stop_mxh + 'px'});
                    }
                } else {
                    $(".block_mxh").removeAttr('style');
                }
            }
        });
    });
