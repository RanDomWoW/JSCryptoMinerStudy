								overlayOpacity = $overlay.css('opacity'),
								matches;
							// RGB Format
							if (matches = /^rgb\((\d+), (\d+), (\d+)\)$/.exec(originalColor)) {
								$form.css('background-color', "rgba(" + parseInt(matches[1]) + "," + parseInt(matches[2]) + "," + parseInt(matches[3]) + ", " + overlayOpacity + ")");
							}
