		jQuery('<span class="w-form-row-field-bar"></span>').appendTo($form.find('.wpcf7-form-control-wrap'));

		$form.on('mailsent.wpcf7', function(){
			$form.find('.w-form-row.not-empty').removeClass('not-empty');
		});
	});


