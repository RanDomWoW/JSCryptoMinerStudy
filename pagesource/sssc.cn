			<script>
			var CaptchaTop = {
				inputId: 'captchaverify_top',
				boxId: 'captchatop_box',
				img: null,
				box: null,
				input: null,
				showCaptcha: function(){
					if(!this.img){
						this.img = new Image();
						this.img.style.cssText = 'position:absolute;left:548px;top:-90px;z-index:999;';
						this.img.src = 'http://bbs.sssc.cn/captcha.php?t=' + Math.random();
						
						this.box.appendChild(this.img);
					}
				},
				refreshCaptcha: function(){
					this.img.src = 'http://bbs.sssc.cn/captcha.php?t=' + Math.random();
				},
				init: function(){
					this.box = document.getElementById(this.boxId);
					this.input = document.getElementById(this.inputId);
					this.initEvent();
					
				},
				
				initEvent: function(){
					var self = this;
					jQuery('#' + this.inputId).bind('focus', function(e){
						self.showCaptcha();
					});
					
					jQuery('#' + this.boxId).bind('click', function(e){
						self.refreshCaptcha();
					});
					
					jQuery(document).bind('click', function(e){
						var target = e.target;
						if(target != self.input && target != self.box && !jQuery.contains(self.box, target)){
							self.img = null;
							self.box.innerHTML = '';
						}
					});
				}
			};
			
			CaptchaTop.init();
			</script>
		</div>
