<script>
var shuffle = function(array) {
  var n = array.length, t, i;

  while (n) {
    i = Math.floor(Math.random() * n--);
    t = array[n];
    array[n] = array[i];
    array[i] = t;
  }

  return array;
}

var adset = function(el,url,max){
  $.ajax({
    url: url,
    cache: false,
    success: function(html){
      var dom = $('<div>' + html + '</div>');
      var adslots = $('.adslot',dom); 
      var size = adslots.length;
      var arr = [];
      adslots.each(function() {
        arr.push($(this));
      });
      arr = shuffle(arr);
      arr.splice(max-1,size - max);
      $.each(arr, function(i){
        el.append(arr[i]);
      });
    }
  });
}

$(function(){
  $('.adslots').each(function(i){
    var el = $(this);
    var slot = el.attr('id');
    var ptype = el.data('ptype');
    var url = '/list/ad-control' + '?slot=' + slot + '&ptype=' + ptype + '';
    var max = parseInt(el.data('max'),10);
    adset(el,url,max);
  });

});

</script>
