	function lingqulb(id){
		var id = id;
		$.ajax({
			type:'POST',
			url:'/card/ajax',
			data:{id:id},
			success:function(data){
				if(data.msg == '1' ||data.msg == '2'){
					layer.open({
						type: 1,
						skin: 'layui-layer-zdy', //样式类名
						anim: 2,
						title:data.name+'礼包',
						shadeClose: true, //开启遮罩关闭
						content: '<div class="laybox">' +
						'<label>礼包兑换码：</label><input type="text" value="'+data.info+'" id="libaocdk"><a class="copy poin" onclick="contentCopy()">复制</a>' +
						'</div>'
					});
				}
			},error:function(data){
				layer.alert('礼包错误');
			}
		});
	}
	function contentCopy(){
		//获取到对象
		var obj = document.getElementById("libaocdk");
		//选择文本框中得所以内容
		obj.select();
		//执行复制功能
		document.execCommand("copy");
		layer.alert('复制成功！');
	}
</script>
