<script type="text/javascript">

var loaded = false;

window.onload = function() 
{
   startFade();
}

function pad(num, size) {
    var s = "000000000" + num;
    return s.substr(s.length-size);
}

function startFade()
{
   if(loaded)
      return;

   loaded = true;

   imgBG = document.getElementById("slideShowBG");
   imgFG = document.getElementById("slideShowImg");

   currImg = 1;
   nextImg = Math.floor(Math.random() * 10);
   imgBG.style.background = "url('/frontpage/frontpage-shaders-" + pad(nextImg, 2) + ".jpg')";

   window.setTimeout("fadeOut(100)", 3000);
}

function fadeOut(val)
{
   setOpacity(imgFG, val);

   if(val > 0)
   {
      val -= 2;
      window.setTimeout("fadeOut(" + val + ")", 30);
   }
   else
   {
      imgFG.src = "/frontpage/frontpage-shaders-" + pad(nextImg, 2) + ".jpg";
      setOpacity(imgFG, 100);
   
      currImg = nextImg;
      nextImg = Math.floor(Math.random() * 10);

      imgBG.style.background = "url('/frontpage/frontpage-shaders-" + pad(nextImg, 2) + ".jpg')";

      window.setTimeout("fadeOut(100)", 3000);
   }

   
}

