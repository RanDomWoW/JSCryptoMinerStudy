    <div class="w1200">
        <div class="s_fixed_l">
            <img src="http://ecimg.happigo.com/resource/web/images/search_fixed_logo.png" />
        </div>
        <div class="s_fixed_r">
            <form id="form2">
                <input autocomplete="off" placeholder="面膜" class="txt_f_sb" type="text" name="keyword" id="keyword_fix"/>
                <input type="button" value="搜索" hc="面膜" class="f_btn_search" style="cursor:pointer">
            </form>
            <div class="search_list" style="display: none;"><ul></ul></div>
        </div>
    </div>
</div>
<script type="text/javascript">
$(function() {
    $('.f_btn_search').click(function(){
        var keyword = $('#keyword_fix').val();
        var c_keyword = $('.txt_f_sb').val();

        if(c_keyword!==''){
            $('#keyword').val(c_keyword);
            $('#form1').submit();
        }else{
            var de_word = $(this).attr('hc');
            if(de_word!==''){
                $('#keyword').val(de_word);
                $('#form1').submit();
            }else{
            }
        }
    });

    $('.txt_f_sb').keyup(function(e) {
        var _container = $('.search_list ul');
        var _lis = _container.find('li');
        var _keyword = $('.txt_f_sb');
        var len = $(".search_list ul").find('li').length;
        var index = 0;
        var new_index = 0;
        var keyword = '';
        
        if (e.keyCode != 40 && e.keyCode != 38) {
            search_keyword = _keyword.val();
        }
        
        if (e.keyCode == 40) {
            //向下键               
            if ($('.choose').length > 0) {
                index = $('.choose').attr('index');
                
                if (index != (len - 1)) {
                    new_index = parseInt(index) + 1;
                } else {
                    new_index = index;
                }
                
                keyword = _lis.eq(new_index).find('a').text();
                if (keyword == '在全球购下搜索') {
                    $('#search_act').val('go_mall');
                    keyword = search_keyword;
                } else {
                    $('#search_act').val('index');
                }
                _keyword.val(keyword);
                _lis.eq(new_index).addClass('choose').siblings().removeClass('choose');
            } else {
                keyword = search_keyword;
                _keyword.val(keyword);
                _lis.eq(0).addClass('choose');
            }               
        } else if (e.keyCode == 38) {
            //向上键
            if ($('.choose').length > 0) {
                index = $('.choose').attr('index');
                
                if (index != 0) {
                    new_index = parseInt(index) - 1;
                } else {
                    new_index = index;
                }
                
                keyword = _lis.eq(new_index).find('a').text();
                if (keyword == '在全球购下搜索') {
                    $('#search_act').val('go_mall');
                    keyword = search_keyword;
                } else {
                    $("#search_act").val('index');
                }
                _keyword.val(keyword);
                _lis.eq(new_index).addClass('choose').siblings().removeClass('choose');
            } else {
                keyword = search_keyword;
                _keyword.val(keyword);
                _lis.eq(0).addClass('choose');
            }               
        } else {
            var keyword = $(this).val();

            $.ajax({
                type:"post",
                url:"/index.php?act=search&op=search_suggest",
                async:true,
                data:{"keyword":keyword},
                dataType:"json"
            }).done(function(data) {
                if (data) {
                    var str = '';
                    
                    if (data.mall_sum > 0) {
                        str += '<li index="0" class="mall_key"><a class="s_h_a_key2" href="javascript:void(0);">在<b class="important">全球购</b>下搜索</a><span class="s_h_span_count">约' + data.mall_sum + '个结果</span></li>';
                        str += '<hr style="height: 1px;border: none;border-top: 1px solid #ccc;margin: 0" />';
                    }
                    
                    if (data.ec_data != '') {
                        for (var i in data.ec_data) {
                            if (data.mall_sum > 0) {
                                j = parseInt(i) + 1;
                            } else {
                                j = i;
                            }
                            
                            str += '<li index="' + j +'" class="ec_key"><a class="s_h_a_key" href="javascript:void(0);">' + data.ec_data[i]['keyword'] + '</a><span class="s_h_span_count">约' + data.ec_data[i]['object_count'] + '个结果</span></li>';
                        }
                    }

                    _container.html('');
                    _container.append(str);
                    $('.search_list').css('display', 'block');
                }
            });
        }
    });

    $('.search_his_list ul, .search_list ul').on("hover", "li", function() {
        $(this).addClass('choose').siblings().removeClass('choose');
    });
    
    $(".search_list ul").on("click", ".mall_key", function() {
       var text = search_keyword;
       $('.txt_f_sb').val(text);
       $('#search_act').val('go_mall');
       $('.f_btn_search').click();
    });

    $(".search_list ul").on("click", ".ec_key", function() {
        var text = $(this).find('a').text();
        $('.txt_f_sb').val(text);
        $('#search_act').val('index');
    $('.f_btn_search').click();
});
});
</script><!--底部-->
