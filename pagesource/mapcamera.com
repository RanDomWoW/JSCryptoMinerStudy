<script>
    // 検索用オブジェクト
    var ukSearch = new uksearchengine("", j$("#ukkdwrSearchBoxWrapper"));
    ukSearch.params = {};
    // スペック検索側エレメント
    var specElm = j$("#ukkdwrSearchBoxWrapper");

    var isClickImageElm = false;
    var isClickSpecElm = false;

    (function ($) {
        $(function () {
            // searchresult,addnaviのサブレイアウトがなければ初期化
            if ($("#UKSearchResult_Wrapper,#UKAddNavi_Wrapper").size() == 0){
                ukSearch.getdrilldown();
            }

            // 左ペインナビゲーション イメージ検索 クリックイベント
            $(".search_img").bind("click", function (e) {
                if($(".ImageSearchDetialPopupWreapper").size() == 0){
                    initPopupList();
                }

                // スペック検索が開いていたら閉じる
                if($(".specSearchBoxPopupWreapper").dialog("isOpen") === true) {
                    $(".specSearchBoxPopupWreapper").dialog("close");
                    $('.search_spec').css({'-ms-filter':'','filter':'','opacity':''});
                }

                isClickSpecElm = false;

                if (isClickImageElm === false) {
                    $(".imageSearchBoxPopupWreapper").dialog("option", "position", { my: "left top", at: "right top", of: $(".navigation"), collision: "none none" });
                    $(".imageSearchBoxPopupWreapper").dialog("open");
                    $('.search_img').css({'-ms-filter':'"alpha(opacity=70)"','filter':'alpha(opacity=70)','opacity':'0.7'});
                    isClickImageElm = true;
                } else {
                    $(".imageSearchBoxPopupWreapper").dialog("close");
                    $('.search_img').css({'-ms-filter':'','filter':'','opacity':''});
                    isClickImageElm = false;
                }
            });

            // 左ペインナビゲーション イメージ検索 マウスオーバーイベント
            var isLeaveImageElm = true;
            $(".search_img").bind("mouseenter", function (e) {
                if($(".ImageSearchDetialPopupWreapper").size() == 0){
                    initPopupList();
                }

                var _elm = e;
                var _this = this;
                isLeaveImageElm = false;

                sethover = setTimeout(function(){
                    if (isClickImageElm === false && isClickSpecElm === false) {
                        // スペック検索が開いていたら閉じる
                        if($(".specSearchBoxPopupWreapper").dialog("isOpen") === true) {
                            $(".specSearchBoxPopupWreapper").dialog("close");
                        }
                        if ($(_elm.fromElement).closest(_this).length === 0 && isLeaveImageElm === false) {
                            if ($(".imageSearchBoxPopupWreapper").dialog("isOpen") === false) {
                                $(".imageSearchBoxPopupWreapper").dialog("option", "position", { my: "left top", at: "right top", of: $(".navigation"), collision: "none none" });
                                $(".imageSearchBoxPopupWreapper").dialog("open");
                                $('.search_img').css({'-ms-filter':'"alpha(opacity=70)"','filter':'alpha(opacity=70)','opacity':'0.7'});
                                isLeaveImageElm = true;
                            }
                        }
                    }
                }, 375);
            });
            $(".search_img").bind("mouseleave", function (e) {
                if (isClickImageElm === false && isClickSpecElm === false) {
                    $('.search_img').css({'-ms-filter':'','filter':'','opacity':''});
                    isLeaveImageElm = true;
                }
            });

            // イメージ検索リストダイアログ
            $(".imageSearchBoxPopupWreapper").dialog({
                dialogClass: 'noTitleDialog',
                autoOpen: false,
                draggable: false,
                resizable: false,
                width: 790
            });

            // イメージ検索リストダイアログ 閉じる クリックイベント
            $(".imageSearchBoxPopupWreapper .closeBtn").bind("click", function (e) {
                isClickImageElm = false;
                isClickSpecElm = false;
                $('.search_img').css({'-ms-filter':'','filter':'','opacity':''});
                $('.search_spec').css({'-ms-filter':'','filter':'','opacity':''});
                $(".imageSearchBoxPopupWreapper").dialog("close");
            });


            // 左ペインナビゲーション スペック検索 クリックイベント
            $(".search_spec").bind("click", function (e) {
                // イメージ検索が開いていたら閉じる
