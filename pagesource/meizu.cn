<div class="index-container">
	<div class="index-phone-center" id="index-phone-center"></div>
	<div class="index-accessory-center" id="index-accessory-center"></div>
	<div class="index-service-center" id="index-service-center"></div>
	<div class="index-video-center" id="index-video-center"></div>
</div>

<script type="text/html" id="CarouselFigure">
	<div class="swiper-container-banner">
		<div class="swiper-wrapper">
			{{each data.CarouselFigure.data as item index}}
			<div data-theme="{{ item.theme }}" class="slider-1 swiper-slide"
					 style="background-image: url('{{ item.img }}');
							background-image: -webkit-image-set(url('{{ item.img }}') 1x, url('{{ item.imgRetina }}') 2x);
							background-color:{{ item.color }}">
				<div class="button-wrapper">
					<a style="position:absolute;left:0;top:0;bottom:0;right:0;" href="{{ item.href }}" target="_blank" data-mtype="wmz_index_lb_{{ index +1 }}"></a>
				</div>
			</div>
			{{/each}}
		</div>
		<div class="pagination"></div>
	</div>
</script>

<script type="text/html" id="PhoneFloor">
	<div class="section-box section-phone-box">
		<div class="section-box-adv">
			<a href="{{ data.PhoneFloor.adv.url }}" class="box-img box-img-retina" style="background-color:{{ data.PhoneFloor.adv.bgColor }};" lazy-src="{{ data.PhoneFloor.adv.img }}" lazy-srcset="{{ data.PhoneFloor.adv.imgRetina }}"></a>
		</div>
		<div class="section-box-row">
			<div class="index-center-wrapper">
				<ul>
					{{each data.PhoneFloor.data as item}}
						<li>
							<div class="section-item-box phone-box">
								<a href="{{ item.url }}">
									<span class="svg-logo svg-logo-{{ item.name }}"></span>
									<span class="box-img-text">{{ item.desc }}</span>
									<span class="box-img box-img-retina box-img-bg" lazy-src="{{ item.img }}" lazy-srcset="{{ item.imgRetina }}"></span>
								</a>
							</div>
						</li>
					{{/each}}
				</ul>
			</div>
		</div>
	</div>	
</script>

<script type="text/html" id="AccessoryFloor">
	<div class="section-title">
		<h3>Accessories</h3>
	</div>
	<div class="section-box section-accessory-box">
		<div class="section-box-adv">
			<a href="{{ data.AccessoryFloor.adv.url }}" class="box-img box-img-retina" style="background-color:{{ data.AccessoryFloor.adv.bgColor }};" lazy-src="{{ data.AccessoryFloor.adv.img }}" lazy-srcset="{{ data.AccessoryFloor.adv.imgRetina }}"></a>
		</div>
		<div class="section-box-row">
			<div class="index-center-wrapper">
				<ul>
					{{each data.AccessoryFloor.data as item }}
					<li>
						<div class="section-item-box acc-box">
							<a class="box-img box-img-retina" href="{{ item.url }}" lazy-src="{{ item.img }}" lazy-srcset="{{ item.imgRetina }}">
								<span class="box-info">
									<span>{{ item.name }}</span>
									<em>{{ item.subName }}</em>
								</span>
							</a>
						</div>
					</li>
					{{/each}}
				</ul>
			</div>
		</div>
	</div>
</script>

<script type="text/html" id="ServiceFloor">
	<div class="section-box section-service-box">
		<div class="service-title">
			<h3>Support</h3>
		</div>
		<div class="section-box-row">
			<div class="index-center-wrapper">
				<ul>
					{{each data.ServiceFloor.data as item index}}
					<li>
						<div class="section-item-box service-box">
							{{if item.type == 'code'}}
							<div class="card-box">
								<a class="box-img box-img-retina" href="{{ item.url }}" target="_blank" lazy-src="{{ item.img }}" lazy-srcset="{{ item.imgRetina }}"></a>
								<img class="box-img-card" src="{{ item.imgCode }}" alt="">
							</div>
							{{ else }}
							<a class="box-img box-img-retina" href="{{ item.url }}" target="_blank" lazy-src="{{ item.img }}" lazy-srcset="{{ item.imgRetina }}"></a>
							{{/if}}
							<p class="box-img-text">{{ item.desc }}</p>
						</div>
					</li>
					{{/each}}
				</ul>
			</div>
		</div>
	</div>
</script>

<script type="text/html" id="VideoFloor">
	<div class="section-title">
		<h3>Videos</h3>
	</div>
	<div class="section-box section-video-box">
		<div class="section-box-row">
			<ul class="clearfix">
				{{each data.VideoFloor.data as item index}}
				<li><a href="javascript:;" class="jpVideo" data-video="{{ item.video }}" style="background-image:url('{{ item.img }}')"><span class="desc">{{ item.title }}</span></a></li>
				{{/each}}
			</ul>
		</div>
	</div>
</script>

