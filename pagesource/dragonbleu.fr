<script type="text/javascript">
jQuery(document).ready(function($){
	/* roll over cart */
	var cart_block = new HoverWatcher('.cart_block');
	var button_cart = new HoverWatcher('.button-cart');

	$(".button-cart").hover(
			function(){
				if ($(".cart_block").hasClass('no-items')) {
					return;
				}
				$(".cart_block").addClass('display-cart');
			},
			function(){
				setTimeout(function(){
					if (!button_cart.isHoveringOver() && !cart_block.isHoveringOver())
						//$(".cart_block").stop(true, true).slideUp(450);
// 						$(".cart_block").removeAttr("style");
						$(".cart_block").removeClass('display-cart');
				}, 200);
			}
		);

		$(".cart_block").hover(
			function(){
				if (typeof cartVarToHide != 'undefined') { clearTimeout(cartVarToHide); }
			},
			function(){
				setTimeout(function(){
					if (!button_cart.isHoveringOver()) {
						if ($(".cart_block")[0].hasAttribute("style")){ $(".cart_block").removeAttr("style"); }
						//$(".cart_block").stop(true, true).slideUp(450);
						$(".cart_block").removeClass('display-cart');
					}
				}, 200);
			}
		);

		$('#close-dropdown').click(
				function(){
					$('#minicart-resp').hide();
				}
		);
});
function HoverWatcher(selector)
{
	this.hovering = false;
	var self = this;

	this.isHoveringOver = function(){
		return self.hovering;
	}

	jQuery(selector).hover(function(){
		self.hovering = true;
	}, function(){
		self.hovering = false;
	})
}
/**
 * Function to delete ajax cart items 
 */
function deletecartitem(id) {
	jQuery('#item_' + id).fadeOut(500, function() { $(this).remove(); });
	jQuery.ajax({
		type: "GET",
        url:"https://www.dragonbleu.fr/dragonbleuajaxcart/add/deleteCartItem/",
        data:{id : id},
        dataType:'json',
        success:function(e){
        	if (e.status == 'success') {            		
        		jQuery('#cartqty').html(e.countitems);
				jQuery('#carttotalprice').html(e.totalprice);
				location.reload();
        		if (e.countitems == 0) {
        			jQuery('#cart_block').hide();
        			jQuery('#cart_block').addClass('no-items');
            	}
			}
        }
    });
}
</script>
