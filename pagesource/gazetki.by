<script>
    $(function(){
        var windowWidth = $(window).width();
        var updateAdsPerScreenSize = function() {
            // remove right banner if width less than 960 cause it's not loading anyway
            if (windowWidth <= 960) {
                var premiumRight = $(".premium-list").find('li:last-child').detach();
            }
            // run ads slider only on small tablet screens where 2 items do not fit [>640]
            if (windowWidth >= WIDTH_XS && windowWidth <= 640) {
                // init the slider of 2 elements
                $(".premium-list").lightSlider($.extend(window.sliderSettings, {
                    item: 1,
                    autoWidth: false,
                    slideMove: 1,
                    speed: 500,
                    pause: 10000,
                    auto: true,
                    loop: true,
                    responsive: []
                }));
            }
            // on mobiles move premium ads above page title
            if (windowWidth < WIDTH_XS) {
                // premium left right above title
                $('<div class="mobile-ad"></div>').append($(".premium-list").find('li.ad .item')).insertBefore($('h1'));
                // premium right after first 6 promotions or papers if the exist
                var promotions = $('.promotions-section .promotion');
                var papers = $('.papers-section .paper');
                var insert = premiumRight.addClass('ads').height(250);
                if(promotions.length > 6) {
                    insert.insertAfter(promotions.eq(5))
                }
                if(papers.length > 6) {
                    insert.insertAfter(papers.eq(5))
                }
            }
        };
        updateAdsPerScreenSize();
    });
</script>
