    function TcnRate() {
        jQuery.post('/wp-admin/admin-ajax.php', {'action':'my_action'}, function (response) {
            var coin_info = JSON.parse(response);
            var avg = ((coin_info.BTC_TCN.high-coin_info.BTC_TCN.low)/coin_info.BTC_TCN.low)*100;
            console.log(avg);
            jQuery('#change').html(avg.toFixed(2));
            jQuery('#high').html(coin_info.BTC_TCN.high);
            jQuery('#low').html(coin_info.BTC_TCN.low);
            jQuery('#volume').html(coin_info.BTC_TCN.volume);
            console.log(coin_info);
        });
    }
