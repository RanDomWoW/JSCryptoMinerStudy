<script type="text/javascript">
  
  jQuery("#worldModal").modal({show: false});
  jQuery("#worldHeaderSuccess, .alert").hide();
  jQuery("#world_signup").trigger("reset");

  /* Checks for Cookie and shows lightbox if cookie doesn't exist */ 
  if (jQuery.cookie('CMAWlightbox') == null) {

      setTimeout(function() {
        jQuery("#worldModal").modal("show");
      }, 10000);
      /* The value directly above controls how long the site waits before showing the form
         5000 ms = 5 second. Adjust as needed */
  }

  /* Check if the cookie exists */
  if (jQuery.cookie('CMAWlightbox') == null) {
      // If the cookie doesn't exist, save the cookie with the value of 1
      jQuery.cookie('CMAWlightbox', '1', {
          expires: 30
      });
  }

jQuery("#world_signup").validate({
  errorLabelContainer: jQuery("#world_signup div.alert"),
  rules: {
    email: {
      required: true,
      email: true
    },
    postal: "required",
    first_name: "required",
  },
  messages: {
    email: "Please enter a valid email address",
    first_name: "Please enter your First Name",
    postal: "Please enter your Zip Code"

  },
    errorClass: 'has-error',
    validClass: 'has-success',
    errorElement: 'span',
    highlight: function(element, errorClass, validClass) {
      jQuery(element).parents("div.form-group").addClass(errorClass).removeClass(validClass);
    },
    unhighlight: function(element, errorClass, validClass) {
      jQuery(element).parents(".has-error").removeClass(errorClass).addClass(validClass);
    },
  submitHandler: function(form) {
    
          jQuery("#world_signup").addClass("loading");
          var email = jQuery("#world_signup #id_email").val();
          var fname = jQuery("#world_signup #id_first_name").val();
          var zip = jQuery("#world_signup #id_postal").val();

          /* Emma Account ID */
          var accountId = "1766749";
          var groups = "2743645";

          var data = "{\"first_name\": \""+ fname +"\",\"postal_code\": \""+ zip +"\",\"source\": \"cmaworld-lightbox\",\"email\": \""+ email +"\",\"optInConfirmation\": false,\"accountId\": \""+accountId+"\",\"groups\": ["+groups+"]}";

          jQuery.ajax({
              type: "POST",
              cache: false,
              url : 'https://signup-collector.e2ma.net/signup',
              data: data,
              contentType: 'application/json',
              dataType: 'text',
              success: function(data, textStatus, jqXHR)
                {
                    jQuery("#world_signup").removeClass("loading");
                    jQuery(".replace").hide();
                    jQuery("#worldHeaderSuccess").fadeIn("slow");
                    jQuery.cookie('CMAWlightbox', '1', {expires: 365});
                },
                error: function (jqXHR, textStatus, errorThrown)
                {
             
                }
          }); 

  }
});
  
