<script>
    var leads = 0;
    var newLeads = 0;
    var customers = 0;
    var revenue = 0;
    var customers_per_lead = 0;
    
    function update_ticker(elem, value) {
        $(elem).html(parseInt(value));
    }

	function update_odometer(elem, value, duration) {
	    var el = document.querySelector(elem);
        od = new Odometer({
          el: el,
          value: value,
          animation: 'count',
          format: '(,ddd)', 
          duration: duration
        });
        od.update(value)
	}
	
	function get_leads_since_midnight(lpm) {
	    var e = new Date(new Date());
        var ms = new Date() - e.setHours(0,0,0,0);
        var minutes = Math.floor(ms / 60000);
        return minutes * lpm;   
	}
	
    function millisToMinutesAndSeconds(millis) {
      var minutes = Math.floor(millis / 60000);
      var seconds = ((millis % 60000) / 1000).toFixed(0);
      return minutes + ":" + (seconds < 10 ? '0' : '') + seconds;
    }
    
    function get_random_lead_jump() {
        var ms = Math.floor(Math.random() * 5500) + 550;
        // console.log('random jump: '+ ms);
        return ms;
    }
    
    function get_random_revenue_jump() {
        // for a random amount:
        // var revenue_jump = Math.floor(Math.random() * 6500) + 4750
        var revenue_jump = 11500;
        return revenue_jump;
    }
    
    function flipNumbers() {
        var lead_difference = leads - newLeads;
        // 
        // 
        // this number set manualy by mgmt
        if(lead_difference == customers_per_lead) {
            newLeads = leads;
            customers = customers + 1;
            revenue = revenue + get_random_revenue_jump();
            update_odometer('#customers', customers, 8000);
            update_odometer('#revenue', revenue, get_random_lead_jump());
        }
        update_odometer('#leads', Math.ceil(leads), 500);
    }
    
    // the only way I could figure out how to do a set timeout with a random interval
    // loop only exists within the immediately invoked function context, so it can recursively call itself.
    function begin_ticking() {
        (function loop() {
            leads = leads + 1;
            var rand = get_random_lead_jump();
            setTimeout(function() {
                flipNumbers();
                loop();  
            }, rand);
        }());
    }

	//Number of days between the EPOCH - 01/01/1970 and 03/15/2016
	var startDate = 16874;
	//Total number of leads generated by S2M by 03/15/2016
	var startLeadsNumber = 508978;
	//Number to increase leads by after each day based on an average. I change this number up or down anytime new averages are noted
	var leadsEachDay = 558;
	var leadsEachMinute = 558 / 1440;
	var leadsEachSecond = 558 / 86400;
	var secondsPerLead = 1 /leadsEachSecond;
	var leadsSinceDayStarted = get_leads_since_midnight(leadsEachMinute);
	
	var minutes = 1000*60;
	var hours = minutes*60;
	var days = hours*24;
	var d = new Date();
	var t = d.getTime();
	
	//Number of days between the EPOCH - 01/01/1970 and the current day
	var todaysDate = Math.round(t/days);
	
	//Current day minus day number on 08/01/2014 to generate how many days have passed since the original lead number was generated
	var NumberOfDaysSince = todaysDate - startDate;
	
	//Multiply how many days have passed by estimated number of leads generated per day
	var LeadsToAdd = NumberOfDaysSince * leadsEachDay;

	//Generates new leads number with commas at the appropriate decimal points
	newLeads = startLeadsNumber + LeadsToAdd + leadsSinceDayStarted;
	leads = newLeads;
	
	//Adds number to start animated ticker with for the day and separate script ticks number up every few milliseconds 
// 	console.log('leads per second: '+leadsEachSecond)
    
    setTimeout(function(){
        $('#leads').show();
        update_odometer('#leads', Math.ceil(newLeads), 4000);
        setTimeout(function(){
            $('#customers').show();
            customers = Math.ceil(newLeads * .021);
            customers_per_lead = newLeads / customers;
            console.log('cpl: '+customers_per_lead)
            update_odometer('#customers', customers, 6500);
            setTimeout(function(){
                $('#revenue').show();
                revenue = Math.ceil(customers * 11500);
                update_odometer('#revenue', revenue, 9000);
                begin_ticking();     
            }, 1000);
        }, 1000);
    }, 1000);
    
        

</script>
