<script>
$(document).ready(function(){
	/* Moves the second menu into the main menu  and adds functionality to menu */
    var mega_menu_extended = $('#how-we-do-it ul').parent().html();
    $('#how-we-do-it').remove();
    $('#hs_menu_wrapper_main_menu > ul > li.hs-item-has-children:eq(0) > ul:eq(0)').wrap('<div class="mega-dropdown mega-dropdown-large" />');
    $('#hs_menu_wrapper_main_menu > ul > li.hs-item-has-children:eq(1) > ul:eq(0)').wrap('<div class="mega-dropdown mega-dropdown-small" />');
    $('.mega-dropdown-large').append(mega_menu_extended);
    
    $('#hs_menu_wrapper_main_menu > ul > li').on('mouseenter', function() {
        $(this).addClass('hover');
        $('#hs_menu_wrapper_main_menu > ul > li > a').css('color', '#cccccc');
        $(this).on('mouseleave', function() {
            $(this).removeClass('hover');
            $('#hs_menu_wrapper_main_menu > ul > li > a').css('color', '#ffffff');
        });
        
    });
    
}); 
$(window).load(function() {
    // grab an element
    /* this activates the disappearing header
    var my_header = document.querySelector("header");
    var headroom  = new Headroom(my_header, {
        "offset": 200,
        "tolerance": 5,
        "classes": {
            "initial": "animated",
            "pinned": "slideDown",
            "unpinned": "slideUp"
        }
    });
    headroom.init(); 
    */
    
    $('#mobile-menu-trigger button').on('click touchstart', function(e) {
        $(this).toggleClass('is-active');
        if($(this).hasClass('is-active')) {
            $(this).find('.hamburger-label').html('CLOSE');    
        } else {
            $(this).find('.hamburger-label').html('MENU');    
        }
        
    });
    
    // setup navigation to use the regular menu
    $('#mobile-menu-trigger').sidr({
		name: 'sidr-main',
		source: '#primary-navigation',
		side: 'right'
	});
	
	// add cta to top of menu
	var cta = $('.menu-cta-container').html();
	$('.sidr-inner ul').eq(0).prepend('<li id="mobile-cta">' + cta + '</li>')
    $('.sidr-class-hs-item-has-children').prepend('<span class="child open-child"><div class="horizontal"></div><div class="vertical"></div></span>');
    
    $('.sidr-class-hs-item-has-children').on('click', function() {
        var drop_button = $(this).find('.child');
        if(drop_button.hasClass('open-child')) {
            drop_button.removeClass('open-child').addClass('close-child')
            $(this).addClass('active').find('.sidr-class-mega-dropdown').slideDown();    
        } else {
            drop_button.removeClass('close-child').addClass('open-child')
            $(this).removeClass('active').find('.sidr-class-mega-dropdown').slideUp();
        }
    	
    });
    $('.mega-dropdown li a').each(function() {
    	var text = $(this).html();
    	if(text == '&nbsp;') $(this).css('margin-bottom', '20px')
    });

    $('.sidr-inner a').each(function() {
        // clean up menu for mobile view
        var url = $(this).attr('href');
        var text = $(this).html();
        if(url == 'javascript:;' && (text == '&nbsp;' || text == '')) {
            $(this).parent().remove();
        }
        if(url == 'javascript:;' && $(this).parent().hasClass('sidr-class-hs-menu-depth-2')) {
            $(this).parent().remove();
        }
        if(text == 'How We Do It') {
            $(this).parent().remove();
        }
    });
    
})
</script>
