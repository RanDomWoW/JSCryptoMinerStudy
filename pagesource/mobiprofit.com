        <script>
            $("#contact-form").submit(function(event) {
                var url = "http://" + "mobiprofit.com" + "/contacts";
                var inputs = {};
                var form = this;
                $.each($(form).serializeArray(), function(i, field) {
                    inputs[field.name] = field.value;
                });

                $.ajax({
                    url: url,
                    method: 'POST',
                    data: inputs,
                    success: function(data){
                        $(form).hide();
                        $('#feedback_thx').show();
                    },
                    error: function(data){
                    }
                });

                event.preventDefault();
            });
        </script>
