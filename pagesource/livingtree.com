$(document).ready(function(){
	
	// hide #back-top first
	$("#back-top").hide();
	
	// fade in #back-top
	$(function () {
            $(window).scroll(function () {
                if ($(this).scrollTop() > 100) {
                    $('a.brand').fadeIn();
                } else if ($(this).scrollTop() > 300) {
                    $('#back-top').fadeIn();
                } else {
                    $('#back-top').fadeOut();
                    $('a.brand').fadeOut();
                }
            });
            
            // scroll body to 0px on click
            $('#back-top a').click(function () {
                $('body,html').animate({
                    scrollTop: 0
                }, 1000);
                return false;
            });
	});
	
        $('#home_username, #home_password').keydown(function(e) {
            if ($("#error_message").html() != "") $("#error_message").html("");
            if (e.which == '13') {
               $('.home_login_form').submit();
            }
        });
        $('.home_login_form').validate({
            rules: {
                home_username: {
                    required: true,
                    email: true
                },
                home_password: {
                    required: true,
                    minlength: 5
                }
            },
            messages: {
                home_username: {
                    required: "Please enter an email",
                    email: "Please enter an valid email"
                },
                home_password: {
                    required: "Please enter a password",
                    minlength: "Please enter a valid password"
                },
            },
            submitHandler: function (form) {
                var username = $('#home_username').val(),
                    password = $('#home_password').val(),
                    persistent = $('#home_persistent').is(':checked') ? true : false;
                var tips   =  1;
                if($('#hiddenTips').val()){
                    tips = $('#loginfrm #hiddenTips').val();
                }
                if($('#hiddenTips3').val()){
                    tips = $('#loginfrm #hiddenTips3').val();
                }
                $('#error_message').html('<font style="color:#5fb13b;font-size:14px;">Verifying credentials...</font>');
                elgg.action('action/userlogin', { 
                    data : {username: username, password: password, tips : tips, persistent:persistent},
                    success : function(result) {
                        if(result.error != '') {
                            $('#error_message').html(result.error);
                        } else {
                            $('#error_message').html('<font style="color:#5fb13b;font-size:14px;">Logging in ...</font>');
                            window.location.href = elgg.get_site_url() + result.forward_url;
                        }
                    }
                });
                return false;
            }        
        });
});
</script>

