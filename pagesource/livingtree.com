            if (element.attr("name") == "telephonesss")
                error.appendTo("#telephoneErr");
            else
                error.insertAfter(element);
        }

    });

    $('#login_dropdown_form').submit(function () {
        if($('#login_dropdown_form').valid()) {
            if(!$('#login_dropdown_form #cbPersistent').is(':checked')){
                $("#login_dropdown_form #cbPersistent").val("false");
            }
            var username = $('#login_dropdown_form #txtEmail').val(),
                password = $('#login_dropdown_form #txtPassword').val(),
                persistent = $("#login_dropdown_form #cbPersistent").val()
            var tips   =  1;
            if($('#login_dropdown_form #hiddenTips').val()){
                tips   =    $('#login_dropdown_form #hiddenTips').val();
            }
            if($('#login_dropdown_form #hiddenTips3').val()){
                tips   =    $('#login_dropdown_form #hiddenTips3').val();
            }
            $('#invalid_status_message').html('<font style="color:#5fb13b;font-size:14px;">Verifying credentials...</font>');
            elgg.action('action/userlogin', { 
                data : {username: username, password: password, tips : tips, persistent:persistent  
                },
                success : function(result) {
                    if(result.error != '') {
                        $('#invalid_status_message').html(result.error);
                    } else {
                        $('#invalid_status_message').html('<font style="color:#5fb13b;font-size:14px;">Logging in ...</font>');
                        window.location.href = elgg.get_site_url() + result.forward_url;
                    }
                }
            });
        }
        return false;
    });

    $('#forgot_password_form').validate({
        rules:{
            username: {
                required : true,
                email : true
            }
        },
        messages:{
            username:{
                required:"Please enter an email",
                email:"Please enter a valid email"
            }
        }
    });
    
    $('#forgot_password_form').submit(function () {
        $('#status_message').html('');
        if($('#forgot_password_form').valid()) {
            var username = $('#forgot_password_form #txtEmail').val();
            
            elgg.action('action/user/requestuserpassword', { 
                data : {username: username     
                },
                success : function(result) {
                    if(result.error != '') {
                        $('#status_message').html(result.error);
                    } else {
                        $('#status_message').html('<font style="color:#00f">' + result.message + '</font>');
