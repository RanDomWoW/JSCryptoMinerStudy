              	align: 'center',
        			verticalAlign: 'top',
        			y: 110,
              	x: 0
           },
           
            tooltip: {
               /* pointFormat: '{point.y}: <b>{point.percentage:.1f}%</b>' */
               pointFormat: '<strong>{point.percentage}%</strong>'
            },
            plotOptions: {
                pie: {
                    allowPointSelect: true,
                    cursor: 'pointer',
                    dataLabels: {
                        enabled: cLabels,
                        distance: 20,
                        /* format: '<b>{point.name}</b> ({point.y:,.0f})', */
