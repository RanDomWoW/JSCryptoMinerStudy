                                <animate attributeName="opacity" from="1" to="0" dur="1s" begin="0.5s"
                                         repeatCount="indefinite"/>
                            </rect>
                            <rect x="44" y="44" width="12" height="12" rx="12" ry="12" fill="#fff"
                                  transform="rotate(-135 45.03 62)">
                                <animate attributeName="opacity" from="1" to="0" dur="1s" begin="0.625s"
                                         repeatCount="indefinite"/>
                            </rect>
                            <rect x="44" y="44" width="12" height="12" rx="12" ry="12" fill="#fff"
                                  transform="rotate(-90 38 62)">
                                <animate attributeName="opacity" from="1" to="0" dur="1s" begin="0.75s"
                                         repeatCount="indefinite"/>
                            </rect>
                            <rect x="44" y="44" width="12" height="12" rx="12" ry="12" fill="#fff"
                                  transform="rotate(-45 21.03 62)">
                                <animate attributeName="opacity" from="1" to="0" dur="1s" begin="0.875s"
                                         repeatCount="indefinite"/>
                            </rect>
                        </svg>
                    </button>
                </form>
            </div>
        </li>
        {{/each}}
    </ul>
</script>


<script id="productreco-template-products-special-offer" type="text/x-handlebars-template">
    {{#each items}}
        {{#compare @index 8 operator="<"}}
        <li class="list-item">
            <a class="media" href="{{../languagePrefix}}{{detailURL}}" title="{{name}}">
                <div class="aside display-list-image wide">
                    <img src="{{pictureURL picturePath "140"}}" alt="{{name}}"/>
                </div>
                <div class="body">
                    <h3 class="beta">{{name}}</h3>
                    <span class="star-rating star-rating-{{ratingValue}}"
                          title="{{ratingValue}} out of 5 stars"></span>
                    <span class="hidden">{{ratingValue}} out of 5 stars</span>

                    <div class="price-display">

                        {{#compare reducedPrice null operator="!="}}
                            <p>{{standardPriceText}}</p>
                            <p class="highlight">{{reducedPriceLabelText}} <span>{{reducedPriceFormatedValue}}</span></p>
                        {{else}}
                            <p>{{standardPriceText}}</p>
                        {{/compare}}
                        {{#compare unit null operator="!="}}
                            {{#compare pricePerUnit null operator="!="}}
                                <span class="smalltext">
                                  <span style="white-space:nowrap;">
                                      {{pricePerUnitPriceTextNoBrackets}}
                                  </span>
                                </span>
                            {{/compare}}
                        {{/compare}}
                    </div>
                </div>
            </a>
        </li>
        {{/compare}}
    {{/each}}
</script>


