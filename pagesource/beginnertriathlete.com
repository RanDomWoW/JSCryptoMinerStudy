    function FormatBeginnersSection() {
        jQuery(".BeginnersBlockTitle").each(function () {
            if (jQuery(this).innerHeight() < 40) {
                jQuery(this).css("line-height", "40px");
            }
        });
    }

    function SetColumnHeights() {
        var $divLeftColumnOuter = jQuery("#divLeftColumnOuter");
        var $divRightColumnOuter = jQuery("#divRightColumnOuter");
        var $divLeftColumnInner = jQuery("#divLeftColumnInner");
        var $divRightColumnInner2 = jQuery("#divRightColumnInner2");
        var leftColumnOuterHeight = $divLeftColumnOuter.height();
        var rightColumnOuterHeight = $divRightColumnOuter.height();

        if (leftColumnOuterHeight > rightColumnOuterHeight) {
            var diff = leftColumnOuterHeight - rightColumnOuterHeight;
            var $divRaceReportLast = jQuery("#divRaceReportLast");

            if (diff >= $divRaceReportLast.height()) {
                $divRaceReportLast.show();

                leftColumnOuterHeight = $divLeftColumnOuter.height();
                rightColumnOuterHeight = $divRightColumnOuter.height();

                if (leftColumnOuterHeight > rightColumnOuterHeight) {
                    $divRightColumnInner2.height($divRightColumnInner2.height() + (leftColumnOuterHeight - rightColumnOuterHeight));
                }
                else if (leftColumnOuterHeight < rightColumnOuterHeight) {
                    $divLeftColumnInner.height($divLeftColumnInner.height() + (rightColumnOuterHeight - leftColumnOuterHeight));
                }
            }
            else {
                $divRightColumnInner2.height($divRightColumnInner2.height() + (leftColumnOuterHeight - rightColumnOuterHeight));
            }
        }
        else if (leftColumnOuterHeight < rightColumnOuterHeight) {
            $divLeftColumnInner.height($divLeftColumnInner.height() + (rightColumnOuterHeight - leftColumnOuterHeight));
        }
    }
</script>
