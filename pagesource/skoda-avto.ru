<script type="text/javascript">
    $(document).ready(function(){
        $('.accordion .accordion_header').click(function(){
            var title = $(this).find('h2.title');
            var active = title.hasClass('active');

            if((active === false) && ($('.accordion').find('div.accordion_panel:visible').length > 0)){
                $('.accordion').find('div.accordion_panel:visible').each(function(){
                    $(this).slideUp('slow');
                    $(this).prev('div.accordion_header').find('h2.title').removeClass('active');
                });
            }

            title.toggleClass('active');
            $(this).next('div.accordion_panel').slideToggle("slow");
        })
    })
</script>

