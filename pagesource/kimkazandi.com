<script>

    $(".modal-gorus .firstDiv a").on('click',function(e){
        e.preventDefault();
        if (!$(this).hasClass('active')){
            var url = $(this).attr('href');

            var urlOld =  $(".modal-gorus .firstDiv a.active").attr('href');
            $(urlOld).fadeOut();
            $(".modal-gorus .firstDiv a.active").removeClass('active');
            $(url).fadeIn();
            $(this).addClass('active');
        }
    });

    $(".modal-gorus .seconDiv form").on('submit',function(e){
        e.preventDefault();
        var email = $(this).find('input[name=email]').val();
        if (is_email(email)){
            emailFalse = true;
            $(this).find('input[name=email]').css('border-color','#dbdbdb');
        }else{
            emailFalse = false;
            $(this).find('input[name=email]').css('border-color','#E26E6E');
        }
        var buTton = $(this).find('button');
        var thisForm = $(this);

        if (emailFalse){
            $(this).find('button').attr('disabled', 'disabled');
            $.ajax({
                type: "POST",
                url: "/form/iletisimForm.php",
                data: $(this).serialize(),
                success: function (msg) {
                alert(msg);
                    $(thisForm).find('input[name=email]').val('');
                    $(thisForm).find('textarea').val('');
                    $(buTton).removeAttr('disabled');
                }
            });
        }
    });

