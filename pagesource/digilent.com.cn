            if(!l){
                l = 13;
            }
            var  x="0123456789qwertyuioplkjhgfdsazxcvbnm";
            var  key="";
            var timestamp = new Date().getTime();
            for(var  i=0;i<  l;i++)  {
                key  +=  x.charAt(Math.ceil(Math.random()*100000000)%x.length);
            }

            if(keyArr.length>0){
                //key重复了
                while($.inArray(key, keyArr)>0){
                    getKey(l);
                }
            }
            keyArr[keyArr.length] = key;
            return keyPrefix+key;
        }
