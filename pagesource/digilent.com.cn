        <script type="text/javascript">
        var timeoutid = null;
            $(document).ready(function() {
                $("#header .navbar-nav").find('li').mouseenter(function() {
                    $(this).find('ul').show();
                    /* Act on the event */
                });
                $("#header .navbar-nav").find('li').mouseleave(function() {
                    $(this).find('ul').hide();
                    /* Act on the event */
                });
                $(".gzwx").mouseenter(function() {
                    $(this).find('a').addClass('gzxwa');
                });
                $(".gzwx").mouseleave(function() {
                    $(this).find('a').removeClass('gzxwa');
                });
                    $(".userjdt .progress").mouseenter(function() {
                        $(".pr-mask").addClass('userjdt-select');
                    });
                    $(".userjdt .progress").mouseleave(function() {
                       $(".pr-mask").removeClass('userjdt-select');
                    });
                })
            
            //开始搜索
            function star(title) {timeoutid = setTimeout("getalldigilent('"+title+"')", 500);}
            //清空setTimeout
            function stop() {clearTimeout(timeoutid); }
            $(document).ready(function() {
                $(".user").mouseenter(function() {
                    $(this).children("ul").addClass('open');
                });
                $(".user").mouseleave(function() {
                    $(this).children("ul").removeClass('open');
                });
                $("#usercenterbanner").mouseenter(function() {
                    $("#edit_profile_btn").addClass('edit_profile_btn_show')
                    /* Act on the event */
                });
                $("#usercenterbanner").mouseleave(function() {
                    $("#edit_profile_btn").removeClass('edit_profile_btn_show')
                    /* Act on the event */
                });
                $("#header .form-control").focus(function(){
                        $(".navbar-nav").fadeOut();
                        $(this).next(".fa-times").addClass("opactiey");
                        var title = $.trim($(this).val());
                        if(title != ''){
                            $("#search-slide").slideDown();
                        }
                    })
                 $("#header .form-control").blur(function(){
                        $(".navbar-nav").fadeIn();
                        $(this).next(".fa-times").removeClass("opactiey");
                        setTimeout( function(){$("#search-slide").slideUp()},1000);
                    })
                $("#header .form-control").keypress(function(e) {
                    // 回车键事件
                    var title = $.trim($(this).val());
                    if(title != '' && e.which == 13) {
                        window.location.href = "/search/all/"+title+".html";
                    }
                });
                //主要用于退格 把文本框东西删除完毕后
                $("#header .form-control").keydown(function(event){
                    // $(this).css("background-color","orange");
                    var title = $.trim($(this).val());
                    if(event.which == 8 && title.length < 2){
                        if(timeoutid != null){
                            stop();
                        }
                    }
                });

                $("#header .form-control").keyup(function(event){
                    event.preventDefault();
                    var title = $.trim($(this).val());
                    if(title != ''){
                        if(timeoutid != null){
                            stop();
                        }
                        star(title);
                    }else{
                        if(!$("#search-slide").is(":hidden")){
                            setTimeout( function(){$("#search-slide").slideUp()},1000);
                        }
                    }
                });
                $("#header .fa-times").click(function(){
                    $("#header .form-control").val("");
                });
            });
            //搜索AJAX
            function getalldigilent(title){
                $('.search-none').css('display','none');
                $('.sear-slide-ul').addClass('hidden');
                $("#search-slide").slideDown();
                $('.sear-load').css('display','block');
                $('.rsearch').css('display','none');
                //POST BIGEN
                $.post("/public/getalldigilent.html", {'title': title}, function(data, textStatus, xhr) {
                    var node = $(".sear-slide-ul");
                    if(data!=''){
                        // alert(JSON.stringify(data));
                        $('.sear-slide-ul').removeClass('hidden');
                        $('.sear-load').css('display','none');
                        $('.rsearch').css('display','inline-block');
                        node.removeClass('hidden');
                        if(!data['xiangmu'] && !data['kecheng'] && !data['product'] && !data['members'] && !data['shequ']){
                            $('.sear-load').css('display','none');
                            $('.rsearch').css('display','inline-block');
                            $('.sear-slide-ul').addClass('hidden');
                            $('.search-none').css('display','block');
                        }else{
                            var temp = '';
                            //相关项目
                            node = $(".sear-slide-list");
                            if(data['xiangmu']){
                                node.parent().removeClass('hidden');
                                $.each(data['xiangmu'],function(index, el) {
                                    temp += '<li class="col-xs-4 col-sm-4 col-md-4" title="'+el.title+'"><a target="_blank" href="'+el.href+'" class="sear-slide-pic"><img src="'+el.img+'!19196" alt=""></a><div class="sear-slide-tit"><h4><a target="_blank" href="'+el.href+'">'+el.title+'</a></h4></div></li>';
                                });
                                node.html(temp).parent().children('h3').children('a').attr('href',"/search/project.html?searchcn="+title);
                            }else{
                                node.parent().addClass('hidden');
                            }
                            //相关课程
                            node = $(".sear-slide-ke");
                            if(data['kecheng']){
                                node.parent().removeClass('hidden');
                                temp = '';
                                $.each(data['kecheng'],function(index, el) {
                                    temp += '<li class="col-xs-4 col-sm-4 col-md-4" title="'+el.title+'"><a target="_blank" href="'+el.href+'" class="sear-ke-pic"><img src="'+el.images+'!185111" alt=""><span class="sear-ke-tit">'+el.title+'</span></a></li>';
                                });
                                node.html(temp).parent().children('h3').children('a').attr('href',"/search/study.html?searchcn="+title);
                            }else{
                                node.parent().addClass('hidden');
                            }
                            //相关产品
                            node = $(".sear-slide-chanpin");
                            if(data['product']){
                                node.parent().removeClass('hidden');
                                temp = '';
                                $.each(data['product'],function(index, el) {
                                    temp += '<li class="col-xs-3 col-sm-3 col-md-3" title="'+el.title+'"><a target="_blank" href="'+el.href+'" class="sear-cp-pic"><img src="'+el.img+'!128x128" alt=""></a><div class="sear-cp-tit"><a target="_blank" href="'+el.href+'">'+el.title+'</a></div></li>';
                                });
                                node.html(temp).parent().children('h3').children('a').attr('href',"/search/products.html?searchcn="+title);
                            }else{
                                node.parent().addClass('hidden');
                            }
                            //相关用户
                            node = $(".sear-slide-user");
                            if(data['members']){
                                node.parent().removeClass('hidden');
                                temp = '';
                                $.each(data['members'],function(index, el) {
                                    temp += '<li  title="'+el.nickname+'"><a target="_blank" href="'+el.href+'"><img src="'+el.headimg+'!65x65" alt=""></a><a target="_blank" href="'+el.href+'">'+el.nickname+'</a></li>';
                                });
                                node.html(temp).parent().children('h3').children('a').attr('href',"/search/members.html?searchcn="+title);
                            }else{
                                node.parent().addClass('hidden');
                            }
                            //相关资讯
                            node = $(".sear-zxli");
                            if(data['shequ']){
                                node.removeClass('hidden');
                                temp = '';

                                $.each(data['shequ'],function(index, el) {
                                    temp += '<p><a href="javascript:window.open(\'/community/'+el.id+'\');"  title="'+el.title+'">'+el.title+'</a></p>';
                                });
                                node.children('p').remove();
                                node.append(temp).children('h3').children('a').attr('href',"/shequsearch/"+title+"/shequ/0/0/0");
                            }else{
                                node.addClass('hidden');
                            }
                        }
                    }else{
                        $('.sear-load').css('display','none');
                        $('.rsearch').css('display','inline-block');
                        $('.search-none').css('display','none');
                    }
                });
            }
        </script>
        
