    var liketurn = true;
function likeorguanzhu(obj,type){
    var uid="";
    var tid=$(obj).attr('data-id');
    if(uid){
        if(liketurn){
            liketurn = false;
            var cla = $(obj).hasClass('aui-icon-likefill');
            var tp = 1;
            if(cla){
                //取消点赞
                tp = 2;
            }
            var url;
            if(type == 'cp'){
                type = 1;
                url = "/cplike.html";
            }else if(type == 'kc'){
                type = 2;
                url = "/kecheng/kclike.html";
            }else{
                type = 9;
                url = "/cplike.html";
            }
            $.ajax({
                type:'post',
                url:url,
                data:{'tid':tid,'mid':uid,'type':type,'tp':tp},
                dataType:"json",
                cache:false,
                success:function(data){
                    if(data){
                        if(tp == 1){
                            //喜欢
                            $(obj).removeClass('aui-icon-like');
                            $(obj).addClass('aui-icon-likefill');
                        }else{
                            //取消喜欢
                            $(obj).removeClass('aui-icon-likefill');
                            $(obj).addClass('aui-icon-like');
                        }
                        $(obj).next('like').text(data);
                    }else{
                        noticeMessage('error','','操作失败!');
                    }
                    liketurn = true;
                },
                error:function(){
                    liketurn = true;
                    noticeMessage('error','','操作失败!');
                },
            });
        }
    }else{
        $('.bs-example-modal-login').modal('show');
