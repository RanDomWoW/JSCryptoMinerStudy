        }
        else {

            blockAdBlock.check();
            
            blockAdBlock.onDetected(function() {
                ga('send', 'event', 'Adblock', 'Sí Detectado', '1');
                document.body.innerHTML = '<p style="padding: 30px; font-weight: bold; font-family: StainlessRegular,sans-serif; font-size: 16px; line-height: 26px;">Hemos detectado que tienes activado el Adblocker. Para poder disfrutar al máximo de la página deberías desactivarlo. ¡Muchas gracias!</p>';
                mpAdblockOpts.adblock = true;
                MPIB.tracking.follow(mpAdblockOpts);
            });
            
            blockAdBlock.onNotDetected(function() {
                MPIB.tracking.follow(mpAdblockOpts);
            });
        }
    </script>




    



