<script>
<!-- 最新情報/お知らせ整形 -->
$(function() {
	// 箱を作る
	$('#newcont .newsblock').after('<div class="newsblock2 newsblock-num1" style="display: none;"><ul></ul></div>');
	$('#newcont .newsblock-num1').after('<div class="newsblock2 newsblock-num2" style="display: none;"><ul></ul></div>');
	$('#newcont .newsblock-num2').after('<div class="newsblock2 newsblock-num3" style="display: none;"><ul></ul></div>');

	// 最新情報を一旦全消し
	$('#newcont .newsblock .newcont_whats_new_list').css('display','none');

	// お知らせのlast
	$('#newcont .newsblock .newcont_notification_list').last().addClass('last');

	// 1つ目のblockを埋める
	var count1 = $('#newcont .newsblock .newcont_notification_list').length;
	var new_count = 0;
	while($('#newcont .newsblock .newcont_whats_new_list:eq('+new_count+')').length > 0 && count1 < 4){
		$('#newcont .newsblock .newcont_whats_new_list:eq('+new_count+')').appendTo('#newcont .newsblock .newcont_notification').css('display','block');
		count1++;
		new_count++;
	}

	// 2つ目のblockを埋める
	var count2 = 0;
	while($('#newcont .newsblock .newcont_notification_list:eq(4)').length > 0 && count2 < 4){
		$('#newcont .newsblock .newcont_notification_list:eq(4)').appendTo('#newcont .newsblock-num1 ul');
		count2++;
	}
	while($('#newcont .newsblock .newcont_whats_new_list:eq('+new_count+')').length > 0 && count2 < 4){
		$('#newcont .newsblock .newcont_whats_new_list:eq('+new_count+')').appendTo('#newcont .newsblock-num1 ul').css('display','block');
		count2++;
	}

	// 3つ目のblockを埋める
	var count3 = 0;
	while($('#newcont .newsblock .newcont_notification_list:eq(4)').length > 0 && count3 < 4){
		$('#newcont .newsblock .newcont_notification_list:eq(4)').appendTo('#newcont .newsblock-num2 ul');
		count3++;
	}
	while($('#newcont .newsblock .newcont_whats_new_list:eq('+new_count+')').length > 0 && count3 < 4){
		$('#newcont .newsblock .newcont_whats_new_list:eq('+new_count+')').appendTo('#newcont .newsblock-num2 ul').css('display','block');
		count3++;
	}

	// 4つ目のblockを埋める
	var count4 = 0;
	while($('#newcont .newsblock .newcont_notification_list:eq(4)').length > 0 && count4 < 4){
		$('#newcont .newsblock .newcont_notification_list:eq(4)').appendTo('#newcont .newsblock-num3 ul');
		count4++;
	}
	while($('#newcont .newsblock .newcont_whats_new_list:eq('+new_count+')').length > 0 && count4 < 4){
		$('#newcont .newsblock .newcont_whats_new_list:eq('+new_count+')').appendTo('#newcont .newsblock-num3 ul').css('display','block');
		count4++;
	}

	$('#newcont .newsblock .newcont_whats_new').remove();

	// lastクラス処理
	$('#newcont .newsblock ul li:last-child, #newcont .newsblock2 ul li:last-child').addClass('last');
	$('#newcont .newsblock .newcont_notification_list:eq(3)').css('border-bottom','none');
	$('#newcont .newsblock2').each(function(){
		$(this).find('.newcont_notification_list:eq(3)').css('border-bottom','none');
	});

	// 文字数処理
	$('.nn_title').each(function(){
		cutText(this, 50);
	});
});

function cutText(_this, _length){
	var afterTxt = "…";
	var textLength = $(_this).text().length;
	var textTrim = $(_this).text().substr(0,_length);

	if(_length < textLength) {
		$(_this).html(textTrim + afterTxt).css({visibility:'visible'});
	} else if(_length >= textLength) {
		$(_this).css({visibility:'visible'});
	}
}
</script> 

