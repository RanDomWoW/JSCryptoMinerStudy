<script>
$(document).ready(function() {
var rememberScroll = 0;
var heroOffset = 200;
if($('.hero').first().length) {
heroOffset = $('.hero').first().height();
}
// Header
$(window).on('scroll', function() {
if($(window).scrollTop() >= heroOffset) {
$('header').addClass('sticky');
} else {
$('header').removeClass('sticky');
}
});
// Mobile menu
$('body').on('click', '.mobile-menu-trigger', function(e) {
e.preventDefault();
var $body = $('body');
if ($body.hasClass('mobile-menu-visible')) {
$body.removeClass('mobile-menu-visible');
$('html, body').scrollTop(rememberScroll);
} else {
rememberScroll = $(window).scrollTop();
$('html, body').scrollTop(0);
$body.addClass('mobile-menu-visible');
} 
});
})
