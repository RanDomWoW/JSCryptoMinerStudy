// play and closed controls
function onPlayerReady(event) {
  var playButton = document.getElementById("play-youtube-main-video");
  var youtube_iframe_wrap = document.getElementById("youtube-iframe-wrap");

  playButton.addEventListener("click", function() {
    player_1.playVideo();
    $(youtube_iframe_wrap).removeClass('hide');
    $(".orbit-bullets-container").addClass('hide');
    function stopAutoFlipping(event) {
      if (player_1.getPlayerState() == 1 || $(youtube_iframe_wrap).hasClass('hide') == false) {
        $(frontpage_slider).trigger("mouseover");
      }
    }
    setInterval(stopAutoFlipping, 1000);
  });

  $('#closed-youtube-video').click(function(){
    player_1.pauseVideo();
    $(youtube_iframe_wrap).addClass('hide');
    $(".orbit-bullets-container").removeClass('hide');
  });
  $('.orbit-prev').click(function(){
    player_1.pauseVideo();
  });
  $('.orbit-next').click(function(){
    player_1.pauseVideo();
  });
}

