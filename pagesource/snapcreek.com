<script type="text/javascript">
	var autoRenewMode = false;
	
	function DProSwitchPricingMode(autoRenew) 
	{
		ga('send', {
				 hitType: 'event',
				 eventCategory: 'EDD',
				  eventAction: 'DupPro ARMode Change',
				  eventLabel: autoRenew.toString(),
		});	

		autoRenewMode = autoRenew;
		
		if(autoRenew) {
			jQuery("#business-price").html("107");
			jQuery("#freelancer-price").html("71");
			jQuery("#personal-price").html("35");

			jQuery("#gold-autorenew").show();
			jQuery("#business-autorenew").show();
			jQuery("#freelancer-autorenew").show();
			jQuery("#personal-autorenew").show();
			
			jQuery("#pricing-autorenew-on").hide();
			jQuery("#pricing-autorenew-off").show();			
			
			jQuery("#autorenew-description").show();
								
		} else {
			jQuery("#business-price").html("119");
			jQuery("#freelancer-price").html("79");
			jQuery("#personal-price").html("39");

			jQuery("#gold-autorenew").hide();
			jQuery("#business-autorenew").hide();
			jQuery("#freelancer-autorenew").hide();
			jQuery("#personal-autorenew").hide();
			
			jQuery("#pricing-autorenew-on").show();
			jQuery("#pricing-autorenew-off").hide();
			
			jQuery("#autorenew-description").hide();
		}		
		
	}
	
	function DProPurchaseClicked(buttonIndex) {
				
		var priceIndex = buttonIndex;
		
		if(autoRenewMode && (priceIndex != 7)) {
			// The AR version of a given license is always 3 ahead of the non AR version
			priceIndex += 3;
		}

		// Let Analytics know that something was added to the cart
		ga('send', {
	 		 hitType: 'event',
	 		 eventCategory: 'EDD',
			  eventAction: 'AddItemToCart',
			  eventLabel: 'Duplicator Pro ' + priceIndex + ' gold',
			  transport: 'beacon'	// So event won't get lost when we change pages
			});
		
		if(priceIndex != 7) {
			window.location.href = "https://snapcreek.com/checkout?edd_action=add_to_cart&download_id=31&edd_options[price_id]=" + priceIndex;		
		} else {					
			// Gold bundle
			window.location.href = "https://snapcreek.com/checkout?edd_action=add_to_cart&download_id=138787";
		}
		
		return false;
	}
</script>
