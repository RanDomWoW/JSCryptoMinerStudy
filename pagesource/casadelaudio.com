<script type="text/javascript">

    var clocks = [];
  $(function(){
		var countdown = 0;
        if(HasCountDown){
            FlipClock.Lang.Custom = { days:'Días', hours:'Horas', minutes:'Minutos', seconds:'Segundos' };
            var opts = {
                clockFace: 'DailyCounter',
                countdown: true,
                language: 'Custom'
            };
            opts.classes = {
                active: 'flip-clock-active',
                before: 'flip-clock-before',
                divider: 'flip-clock-divider',
                dot: 'flip-clock-dot',
                label: 'flip-clock-label',
                flip: 'flip',
                play: 'play',
                wrapper: 'flip-clock-wrapper'
            };
            $.each( Deadlines, function( index, element ){
                var deadline = element.date;
                countdown = (Date.parse(deadline)/1000+7200) - ((new Date().getTime())/1000);
                countdown = Math.max(1, countdown);


			    $('.'+element.counter+' '+'.clock-builder-output2').each(function() {
			        var clock = $(this),
			            date = countdown;
			        clock.FlipClock(date, {
			            clockFace: 'DailyCounter',
			            countdown: true,
			            language: 'Custom'
			        });
			        clocks.push(clock);
			    });
			});
        };
    });
    </script>

