<script type="text/javascript">
function getTopOffset(e) { 
	var y = 0;
	do { y += e.offsetTop; } while (e = e.offsetParent);
	return y;
}
var block = document.getElementById('fixblock'); /* fixblock - значение атрибута id блока */
if ( null != block ) {
	var topPos = getTopOffset( block );
	window.onscroll = function() {
		var newcss = (topPos < window.pageYOffset) ? 
			'top:20px; position: fixed;' : 'position:static;';
		block.setAttribute( 'style', newcss );
	}
}
</script>
<script>

var skey={};
var q='';
var act='';
jQuery(document).ready(function(){
	act=jQuery('form[name=search]').attr('action');
	jQuery.post('/ajax/keys.php',function(data){
		skey=data;
	},'json');
		jQuery('form[name=search]').submit(function(){
			jQuery('form[name=search]').attr('method','GET');
			jQuery('form[name=search]').attr('action',act);
			q=jQuery('input[name=q]').val();
			jQuery.post('/ajax/sea.php',jQuery(this).serialize(),function(){});
			jQuery.each(skey,function(key,val){
				//console.log(q,key,val);
				//console.log(substr_count(q,key));
				if(substr_count(q,key)>0){
					//console.log('!!!');
					jQuery('form[name=search]').attr('action',val);
					jQuery('form[name=search]').attr('method','POST');
				}
			});
		});
		
});
function substr_count( haystack, needle, offset, length ) {
	haystack=haystack.toLowerCase();
	needle=needle.toLowerCase();
 
    var pos = 0, cnt = 0;
 
    if(isNaN(offset)) offset = 0;
    if(isNaN(length)) length = 0;
    offset--;
 
    while( (offset = haystack.indexOf(needle, offset+1)) != -1 ){
        if(length > 0 && (offset+needle.length) > length){
            return false;
        } else{
            cnt++;
        }
    }
 
    return cnt;
}

</script>
