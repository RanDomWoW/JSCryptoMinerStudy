<script>
let btn = document.getElementsByClassName('shop-btn')[0]

let hoveredOnBtn = false;
let hoveredOnDropdown = false;

btn.addEventListener('mouseover', (e) => {
    hoveredOnBtn = true;
    addVisible()
})

btn.addEventListener('mouseleave', (e) => {
  // This should only be called if the cursor hasn't left the
  // btn either. What can I add to create that condition?
  hoveredOnBtn = false;
  removeVisible();
})

let dropDownCotainer = document.getElementsByClassName('dropdown-container')[0]
dropDownCotainer.addEventListener('mouseleave', (e) => {
    let hoveredOnDropdown = false;
    removeVisible()
})

dropDownCotainer.addEventListener('mouseover', (e) => {
    let hoveredOnDropdown = true;
    addVisible()
})

function toggleVisible() {
  let el = document.getElementsByClassName('dropdown-container')[0]
  if (el.classList.contains('visible')) { 
      el.classList.remove('visible')
  } else {
    el.classList.add('visible') 
  }
}

function addVisible() {
     let el = document.getElementsByClassName('dropdown-container')[0]
      el.classList.add('visible')
}

function removeVisible() {
  let el = document.getElementsByClassName('dropdown-container')[0]
    if (hoveredOnBtn == false || hoveredOnDropdown || false ) {  el.classList.remove('visible') }
}
</script>



