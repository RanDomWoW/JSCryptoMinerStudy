<script type="text/javascript">
  $('#contact-response').hide();
  $('#btn-asistente-contact').on('click', function(e) {
    var form = $('#asistente-contact');
    $.ajax({
      url: form.attr('action'),
      data: form.serialize(),
      method : "POST",
      success : function(data){
        $('#contact-response').text('');
        $('#contact-response').hide();
        $('#contact-response').removeClass('alert-success alert-danger');
        if (data.success) {
          button = '<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>';
          $('#contact-response').append(button + '' + data.success);
          $('#contact-response').addClass('alert-success');
          $('#contact-response').show();

          setTimeout(function() { $('#contacto-popup').modal('hide'); }, 1000);
        } else {
          var errors = data.error;
          error = "Se encontraron los siguientes errores: ";
          lista = "<ul>";
          Object.keys(errors).forEach(function(key) {
            if (key !== 'captcha') {
              lista += "<li>" + errors[key]; + "</li>";
            } else {
              $('.recaptcha-contact').text(errors[key]);
            }
          });
          lista += "</ul>";
          error += lista;
          button = '<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>';
          $('#contact-response').append(button + '' + error);
          $('#contact-response').addClass('alert-danger');
          $('#contact-response').show();
        }
      }
    });
  });
</script>
