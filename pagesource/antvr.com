	$(".cancel-energy-tip").click(function(){
		$(".energy-tip").hide();
	});
	
     var url=window.location.search;  	  
	url=url.toString(); 
	var array_url=new Array();
	array_url=url.split("=");
        if(array_url[0]=="?view"){
			if(array_url[1]=="share"){
			      window.location.href="/share.do";
			}
			if(array_url[1]=="resource"){
			      window.location.href="/browser.do";
			}
        }
       	//中英文切换
	 if(array_url[0]=="?lang"){
		if(array_url[1]=="en"){
			window.location.href="/en?lang=en";
		}
	}else if(array_url[0]==""&&arrUrl[3]==""){
		//检测浏览器语言
		var currentLang = navigator.language;   //判断除IE外其他浏览器使用语言
		if(!currentLang){//判断IE浏览器使用语言
		    currentLang = navigator.browserLanguage;
		}
		console.log(currentLang);
		if(!(currentLang=="zh-CN")){
			window.location.href="/en";
		}
	}
	 	var nickname=$.cookie("USER_NICKNAME");	
   	var headImg=$.cookie("USER_HEADIMG");
// 	alert(nickname);
//var nickname="${user.nickName}";	
//var headImg="${user.headImg}";
	if(nickname!= "undefined" && nickname != ''&&nickname !=null){
		$(".un-login").hide();
		$(".complete-login").show();
		$(".complete-login>a").html(nickname+'<b class="caret"></b>');
		$(".complete-login .person-nic>a:first-child").html('<img src='+headImg+' width="100" height="100" /><i>'+nickname+'</i>');
	}else{
		$(".un-login").show();	
		$(".complete-login").hide();
	}
	
	if($("body").width()<767){
		$(".menu li.resouce").click(function(){
			
			$(".menu li.resouce span").css("display:block","opacity:1");
		});
		
		$(".icon_menu").click(function(){
			if($("nav").height()<=60){
				$("nav").css("height","350px");
				$("nav").css("max-height","350px");
			}else{
				$("nav").css("max-height","60px");	
			}
			
		});
	}else{
		$(".glass,.m_headset").hover(function(){
			$(".m_headset").addClass("drop-down-hover");
		});
		$(".glass").hover(function(){
			$(".m_headset .wrapper02").hide();
			$(".m_headset .wrapper01").show();
		});
		$(".glass,.m_headset").mouseleave(function(){
			$(".m_headset").removeClass("drop-down-hover");
		});
		
		$(".pc,.m_headset").hover(function(){
			$(".m_headset").addClass("drop-down-hover");
		});
		$(".pc").hover(function(){
			$(".m_headset .wrapper01").hide();
			$(".m_headset .wrapper02").show();
		});
		$(".pc,.m_headset").mouseleave(function(){
			$(".m_headset").removeClass("drop-down-hover");
		});
	}
	
	
	var scrollFunc = function(e) {
		var srcTop;
		document.body.scrollTop ? srcTop = document.body.scrollTop : srcTop = document.documentElement.scrollTop; //鏍囧噯
		if(srcTop>=$(".top-bar1").height()*2){		
			if(parseInt($(".top-bar1").css("top"))==0){
				$(".top-bar1").addClass("top-active");
			}		
		}else{
			$(".top-bar1").removeClass("top-active");
		}
	}  
	if (document.addEventListener) {		
		document.addEventListener("DOMMouseScroll", scrollFunc, false);
	}
	//IE/Opera/Chrome
	window.onmousewheel = document.onmousewheel = scrollFunc;
	$(".closelogin").click(function(){
		window.location.href="/antUser/logout.do";
	});
</script>
