	<script>
		function slideShow(el, urls)
		{
			var timeOut = 1 * 1000;
			var url = urls.shift();
			var img = new Image(1, 1);
			var iconTV = document.createElement('div');
			iconTV.className = 'ap_icon';

			el.onmouseleave = function ()
			{
				el.timeOut = 1;
			};

			if (url === undefined)
			{
				el.onmouseenter = function ()
				{
					clearInterval(el.timeOut);
					el.timeOut = setInterval(function ()
					{
						el.style.backgroundImage = 'url(' + el.urls[(el.urls.indexOf(el.style.backgroundImage.split(/\("?|"?\)/)[1]) + 1) % el.urls.length] + ')';
					}, timeOut);
					el.appendChild(iconTV);
					$(".viac_videi").css("font-weight", "bold").css("text-transform", "uppercase");
				};

				el.onmouseleave = function ()
				{
					$(".viac_videi").css("font-weight", "normal").css("text-transform", "none");
					clearInterval(el.timeOut);
					el.style.backgroundImage = 'url(' + el.urls[0] + ')';
					iconTV.remove ? iconTV.remove() : iconTV.removeNode();
				}

				if (!el.timeOut)
					el.onmouseenter();
				return;
			}

			if (el.urls === undefined)
			{
				el.urls = [el.style.backgroundImage.split(/\("?|"?\)/)[1]];
			}

			img.onload = function ()
			{
				el.urls.push(this.src);
				slideShow(el, urls);
				document.body.removeChild(img);
			};

			img.src = url;
			document.body.appendChild(img);
		}
	</script>

