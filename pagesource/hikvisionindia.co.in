<script>
    function validateLogin()
    {
        var username = document.getElementById('modlgn-username').value.trim();
        var password = document.getElementById('modlgn-passwd').value.trim();
        var err_msg = "";
        var emailreg = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
        if (username == "")
        {
            err_msg = "Please enter email\n";
        }
        else
        {
            if (!emailreg.test(username))
            {
                err_msg += "Please enter valid email\n";
            }
        }
        if (password == "")
        {
            err_msg += "Please enter password\n";
        }
        if (err_msg != "")
        {
            alert(err_msg);
            return false;
        }
    }
</script>
