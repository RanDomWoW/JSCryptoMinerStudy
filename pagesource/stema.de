        var J = jQuery.noConflict();

        jQuery(document).ready(function ($) {

            var slideCount = $('#slider-start-sonderangebote ul li').length;
            var slideWidth = $('#slider-start-sonderangebote ul li').width();
            var slideHeight = $('#slider-start-sonderangebote ul li').height();
            var sliderUlWidth = slideCount * slideWidth;

            $('#slider-start-sonderangebote').css({width: slideWidth, height: slideHeight});

            $('#slider-start-sonderangebote ul').css({width: sliderUlWidth, marginLeft: -slideWidth});

            $('#slider-start-sonderangebote ul li:last-child').prependTo('#slider-start-sonderangebote ul');

            function moveLeft() {
                $('#slider-start-sonderangebote ul').animate({
                    left: +slideWidth
                }, 200, function () {
                    $('#slider-start-sonderangebote ul li:last-child').prependTo('#slider-start-sonderangebote ul');
                    $('#slider-start-sonderangebote ul').css('left', '');
                });
            };

            function moveRight() {
                $('#slider-start-sonderangebote ul').animate({
                    left: -slideWidth
                }, 200, function () {
                    $('#slider-start-sonderangebote ul li:first-child').appendTo('#slider-start-sonderangebote ul');
                    $('#slider-start-sonderangebote ul').css('left', '');
                });
            };

            $('a.control_prev_sonderangebote').click(function () {
                moveLeft();
            });

            $('a.control_next_sonderangebote').click(function () {
                moveRight();
            });

        });
    </script>
				
