function validLen(obj,len,desc,flag){
  var objText=obj.value.trim();
  if(flag){
    if(objText.lenbyte()==0){
      alert("提示："+desc+"不能为空！");
      obj.select();
      return false;
    }
  }
  if(objText.lenbyte()>len){
    alert("提示："+desc+"的长度不能超过"+len+"位！");
    obj.select();
    return false;
  }
  return true;
}	
String.prototype.trim  =  function()
{
        //  用正则表达式将前后空格
        //  用空字符串替代。
        return  this.replace(/(^\s*)|(\s*$)/g,  "");
}

/*** 返回字节数 中文算二位，英文算一位 ***/
String.prototype.lenbyte = function() {
  return this.replace(/[^\x00-\xff]/g,"**").length;
