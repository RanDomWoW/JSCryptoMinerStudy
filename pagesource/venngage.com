    <script>

		// Handles all events for changing template thumbnails
        var templateView = {
            templates: ['infographic', 'report', 'poster', 'promo', 'social'],
            /**
             * Changes the current template thumbnail category in view using
             * css attributes and classes.
             * @param {String} templateName One of: ['infographic','report','poster','promo','social']
             */
            changeTemplate: function(templateName) {
                if (
                    $('li.active').data('template') === templateName ||
                    this.templates.indexOf(templateName) === -1
                ) return;
                var newTemplate = $('li.' + templateName);
                var curTemplate = $('li.active');
                var newTemplateRow = $('#' + templateName);
                var curTemplateRow = $('#' + curTemplate.data('template'));
                newTemplate.addClass('active');
                curTemplate.removeClass('active');
                curTemplateRow.addClass('animate-out');
                setTimeout(function() {
                    curTemplateRow.css({'display':'none','opacity':0});
                    newTemplateRow.css({'display':'flex','opacity':1});
                    newTemplateRow.addClass('animate-in');
                }, 185);
                setTimeout(function() {
                    newTemplateRow.removeClass('animate-in');
                    curTemplateRow.removeClass('animate-out');
                }, 600);
            },
            // Changes height of template-container div for proper hover states
            calculateProperHeight: function() {
                $('.template-image-container').each(function() {
                    var containerHeight = $(this).height();
                    $(this).parent().css('height', containerHeight+2);
                });
            },
            // Changes height of a specific template-containern div for proper hover states
            calculateProperHeightChild: function(img) {
                var imageContainer = img.parent('.template-image-container');
                var containerHeight = imageContainer.height();
                imageContainer.parent().css('height', containerHeight+2);
            }
        }

        document.addEventListener("DOMContentLoaded", function(event) {
			trackPageVisit('Homepage');
            // Calculate proper height for templates on first load and further ones
            templateView.calculateProperHeight();
            $(window).on('load resize', templateView.calculateProperHeight);

            // Show signup buttons onhover for template thumbnails
            $('.template-container').mouseover(function() {
                $(this).find('.template-overlay-container').css('opacity', 1);
            }).mouseleave(function() {
                $(this).find('.template-overlay-container').css('opacity', 0);
            });

            // Change current template thumbail on click
            $('.template-titles li').click(function() {
                templateView.changeTemplate($(this).data('template'));
                setTimeout(templateView.calculateProperHeight, 185);
            });
        });

        $(document).ready(function() {
            function lazyLoadImages(size) {
                // on appropriate screen sizes, load imgs that have not already been loaded
                var $imgs = $("img.lazy-load-" + size + "[src='']:not([data-src=''])");
                $imgs.each(function(index) {
                    $img = $(this);
                    $img.attr('src', $img.attr('data-src'));
                });
            }
            $(window).on("resize", function(){
                if (window.matchMedia('(min-width: 992px)').matches) {
                    lazyLoadImages('md');
                }
                if (window.matchMedia('(min-width: 768px)').matches) {
                    lazyLoadImages('sm');
                }
                if (window.matchMedia('(min-width: 481px)').matches) {
                    lazyLoadImages('xs');
                }
            }).resize();

            /** 
             * Once a template image is done loading, calculate its correct height. 
             * Instead of showing all signup buttons once all templates are loaded, show all the ones that are done loading so far
             */
            $('img.template-image').on('load',function() {
                templateView.calculateProperHeightChild($(this));
            });
        });
    </script>

