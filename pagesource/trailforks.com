    $("#ac_trailname").autocomplete({
        source: function(request, response) {
            pb.rmsSend({mod: 'trailforks', op: 'ac_trailname', term: request.term}, function (oRmsR) {
                var suggestions = [];
                $.each(oRmsR['rmsD']['response'], function(i, val) {
                    suggestions.push(val);
                });
                response(suggestions);
            });
        },
        select: function(event, ui) {
            $('#trailsearch_id').val(ui.item.id);
        },
        close: function() {
            $('form[name="trailsearch"] input[type="submit"]').click();
        },
        minLength: 2
    });

