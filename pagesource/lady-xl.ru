<script type="text/javascript">
	var searchForm;

	function updateFilterSubmitBtn(el) {
		var container = el.parents('.select');
		var sel = container.find('ol:not([data-selector]) li.selected');
		var btn = container.find('input[type="submit"]');
		if (sel.length > 0) {
			btn.val(btn.data('apply-text')).removeClass('close').addClass('apply');

			if (typeof container.data('allow-multiple') === 'undefined') {
				if (container.hasClass('select-listbox')) {
					var caption = container.children('div:first-child');
					var captionText = $.trim(sel.first().text());
					if (captionText && captionText.length > 0)
						caption.text(captionText);
				}
			}
		} else {
			btn.val(btn.data('close-text')).removeClass('apply').addClass('close');
		}
	}
	function updateFilter() {
		searchForm = $('#AdvancedSearchInCategory');
		searchForm.find('div.select-listbox > div, div.select-checkbox > div').each(function() {
			updateFilterSubmitBtn($(this));
		});

		searchForm.find('div.select ol[data-selector] li:first').each(function() {
			$(this).trigger('click');
		});
	}
	$(document).ready(function() {
		searchForm = $('#AdvancedSearchInCategory');
		searchForm.live('submit',function(e) {
			if (productsLoading) return false;
			productsLoading = true;

			var self = $(this);

			var variantNames = ['param_color', 'param_size', 'param_height'];
			var variants = [];

			self.find('input[type="hidden"][data-fake]').remove();

			self.find('.filter-select').each(function() {
				var name = $(this).data('name');
				var isSpecial = variantNames.indexOf(name) >= 0;
				if (name && name.length > 0) {
					var options = $(this).find('ol:not([data-selector]) li.selected');
					if (name == 'price_range') {
						var priceFrom = self.find('input[name="search_price_from"]');
						var priceTo = self.find('input[name="search_price_to"]');
						if (options.length > 0) {
							var price = options.first().data('value');
							var priceRange = price.split('-');
							if (priceRange.length == 2) {
								priceFrom.val(priceRange[0]);
								priceTo.val(priceRange[1]);
							} else {
								priceFrom.val(price);
								priceTo.val('');
							}
						} else {
							priceFrom.val('');
							priceTo.val('');
						}
					} else if (name == 'subcategory') {
						var subcategory = self.find('input[name="use_search_in_new_products"]');
						if (options.length > 0) {
							var categoryId = options.first().data('value');
							subcategory.val(categoryId);
						} else {
							subcategory.val('1');
						}
					} else {
						var values = [];
						if (isSpecial) {
							options.each(function() {
								var valueList = $(this).data('value').toString().split(',');
								var tmps = [];
								for (var i = 0; i < valueList.length; i++) {
									var parts = valueList[i].split(':'), value = null;
									if (parts.length > 1)
										value = parts[1];
									else if (parts.length == 1)
										value = parts[0];
									if (value !== null)
										tmps.push(value);
								}
								tmps = tmps.unique();
								if (tmps.length > 0)
									values.push(tmps.join(','));
							});
						} else {
							options.each(function() {
								var valueList = $(this).data('value').toString().split(',');
								for (var i = 0; i < valueList.length; i++) {
									var parts = valueList[i].split(':'), value = null;
									if (parts.length > 1)
										value = parts[1];
									else if (parts.length == 1)
										value = parts[0];
									if (value !== null)
										values.push(value);
								}
							});
						}
						values = values.unique();
						if (values.length > 0) {
							if (isSpecial) {
								for (var i = 0; i < values.length; i++)
									variants.push(values[i]);
							} else {
								var curValue = (values.length > 1 ? values.join(',') : values[0]);
								self.append('<input type="hidden" name="' + name + '" value="' + curValue + '" data-fake/>');
								
							}
						}
					}
				}
			});

			if (variants.length > 0)
				self.append('<input type="hidden" name="variants" value="' + (variants.length > 1 ? variants.join(';') : variants[0]) + '" data-fake/>');

			var height_model = $(this).find('div.search-height-button a.search-height-button-sel');
			if (height_model.length > 0) {
				var height_value = height_model.first().data('value');
				self.append('<input type="hidden" name="param_height_model" value="' + height_value + '" data-fake/>');
			}

			hideSelect();
			$('#product_wrapper ul.product_list').animate({ opacity: 0.0 }, 500);
			var filterSerialize = searchForm.serialize();
			if (supports_history_api()) {
				var newUrl = get_current_url_without_querystring();
				if (filterSerialize.length > 0)
					newUrl += '?' + filterSerialize;
				window.history.replaceState(null, '', newUrl);
			} else {
				location.hash = '#'+filterSerialize;
			}
			var imgLoad = $('<div class="loading-spinner loading-spinner-top"></div>');
			$('#filter_wrapper ul.product_list:first').before(imgLoad);
			$('#filter_wrapper ul.product_list').animate({ opacity: 0.0 }, 500);
			$.ajax({
				type: 'GET',
				url: searchForm.attr('action'),
				data: filterSerialize+'&ajax_call=1',
				success: function(data) {
					imgLoad.remove();
					filterWrapperUpdate(data);
					rebuildProductList();
				}
			});
			return false;
		});

		searchForm.find('.filter-select.select-checkbox .checkbox').live('click',function(e) {
			var ol = $(this).parents('ol');
			var container = ol.parent();
			if (typeof container.data('allow-multiple') !== 'undefined') {
				if ($(this).is(':checked')) {
					$(this).parents('li').addClass('selected');
				} else {
					$(this).parents('li').removeClass('selected');
				}
			} else {
				ol.find('li.selected:not(.submit) .checkbox').attr('checked', false);
				ol.find('li.selected:not(.submit)').removeClass('selected');
				$(this).parents('li').addClass('selected');
			}
			updateFilterSubmitBtn($(this));
		});

		searchForm.find('.select input[type="submit"]').live('click',function(e) {
			if ($(this).hasClass('close')) {
				var select = $(this).parents('.select').children('ol');
				select.hide();
			} else if ($(this).hasClass('apply')) {
				searchForm.submit();
			}
			return false;
		});

		searchForm.find('.filter-select').live('click',function(e) {
			e.stopPropagation();
		});

		searchForm.find('div.select-listbox li:not(.submit)').live('click',function(e) {
			var li = $(this);
			var ol = li.parents('ol');
			var isSelector = typeof ol.data('selector') !== 'undefined';
			var container = ol.parent();

			if (!isSelector) {
				if (typeof container.data('allow-multiple') !== 'undefined') {
					li.toggleClass('selected');
				} else {
					ol.find('li.selected:not(.submit)').removeClass('selected');
					li.addClass('selected');
				}

				updateFilterSubmitBtn($(this));
			} else {
				ol.find('li.selected:not(.submit)').removeClass('selected');
				li.addClass('selected');
			}
		});

		searchForm.find('div.select ol[data-selector] li:not(.selected)').live('click',function(e) {
			var container = $(this).parents('.select');
			var optionIds = $(this).data('oids').toString().split(',');
			var li = container.find('ol:not([data-selector]) li:not(.submit)');

			li.removeClass('selected');

			li.each(function() {
				var value = $(this).data('value').toString();
				for (var i = 0; i < optionIds.length; i++) {
					if (value.indexOf(optionIds[i] + ':') >= 0)
						$(this).show();
					else
						$(this).hide();
				}
			});
		});

		$('div.filter > div.filter-select > div:first-child').live('click',function(e) {
			var el = $(this).parent().children('ol');

			if (!el.is(':visible')) {
				hideSelect(), lastSelect = el;

				updateFilterSubmitBtn($(this));
				lastSelect.show();
			} else {
				hideSelect();
			}

			return false;
		});
		$('div.search-sort div.select-listbox > div:first-child').live('click', function(e) {
			var el = $(this).parent().children('ol');

			if (!el.is(':visible')) {
				hideSelect(), lastSelect = el;
				el.show();
			} else {
				hideSelect();
			}

			return false;
		});

		$('div.search-sort > a, div.search-sort li').live('click', function(e) {
			var input = searchForm.find('input[name="sort"]');
			input.val($(e.target).data('value'));

			searchForm.submit();

			return false;
		});

		$('div.search-height-button > a').live('click', function(e) {
			var buttons = $(this).parent().children('.search-height-button-sel');
			buttons.removeClass('search-height-button-sel');
			$(this).addClass('search-height-button-sel');

			searchForm.submit();

			return false;
		});

		updateFilter();

		$('ul.filter-line li').live('click', function() {
			if (productsLoading) return false;
			productsLoading = true;

			var queryObj1 = $.decodeQueryString(location.href);
			var queryObj2 = $.decodeQueryString('?'+location.hash.substr(1));
			var queryObj = $.extend(queryObj2, queryObj1);
			if (queryObj !== null && typeof queryObj === 'object') {
				if (typeof $(this).data('clear-all') !== 'undefined') {
					for (var key in queryObj) {
						key = key.toString();
						if (key.match(/^(subcategory|season|search_price_from|search_price_to|tag_group_|variants|param_height_model|manufacturerID)/))
							delete queryObj[key];
					}
				} else {
					var type = $(this).data('type');
					if (type == 'subcategory') {
						delete queryObj['subcategory'];
					} else if (type == 'season') {
						delete queryObj['season'];
					} else if (type == 'price') {
						delete queryObj['search_price_from'];
						delete queryObj['search_price_to'];
					} else if (type == 'height_model') {
						delete queryObj['param_height_model'];
					} else if (type == 'manufacturerID') {
						delete queryObj['manufacturerID'];
					} else if (type == 'size' || type == 'height') {
						if ('variants' in queryObj) {
							var id = $(this).data('id');
							var variants = queryObj['variants'].toString().split(';');
							for (var i = variants.length - 1; i >= 0; i--) {
								if (id == variants[i])
									variants.splice(i, 1);
							}
							queryObj['variants'] = variants.join(';');
							if (queryObj['variants'].length == 0)
								delete queryObj['variants'];
						}
					} else if (type == 'tag') {
						var parts = $(this).data('id').toString().split(':');
						if (parts.length == 2) {
							var id = parts[0], value = parts[1];
							for (var key in queryObj) {
								if (key.indexOf('tag_group_' + id) === 0) {
									var variants = queryObj[key].toString().split(',');
									for (var i = variants.length - 1; i >= 0; i--) {
										if (value == variants[i])
											variants.splice(i, 1);
									}
									queryObj[key] = variants.join(',');
									if (queryObj[key].length == 0)
										delete queryObj[key];
									break;
								}
							}
						}
					}
				}
				var newQueryString = $.encodeQueryString(queryObj).substr(1);
				if (newQueryString && newQueryString.length > 0) {
					var imgLoad = $('<div class="loading-spinner loading-spinner-top"></div>');
					$('#filter_wrapper ul.product_list:first').before(imgLoad);
					$('#filter_wrapper ul.product_list').animate({ opacity: 0.0 }, 500);
					var newUrl = get_current_url_without_querystring() + '?' + newQueryString;
					if (!supports_history_api())
						location.hash = '#'+newQueryString;
					else
						window.history.replaceState(null, '', newUrl);
					$.ajax({
						type: 'GET',
						url: newUrl + '&ajax_call=1',
						success: function(data) {
							imgLoad.remove();
							filterWrapperUpdate(data);
							rebuildProductList();
						}
					});
				}
			}
		});
	});
</script>
														
