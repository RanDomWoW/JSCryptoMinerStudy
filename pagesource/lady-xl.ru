			$(document).ready(function() {
				var pushedBannerIds = [];

				$('#banner-slider > ul').show().bxSlider({
					mode: 'horizontal',
					controls: false,
					auto: true,
					autoHover: true,
					pause: 4000,
					responsive: false,
					touchEnabled: false,
					buildPager: function(slideIndex) {
						return '';
					},
					onSliderLoad: function(curIndex) {
						var cat = getCategory();
						if (cat != 'home') return;
						var name = $('#banner-slider .bxslider img:eq(' + (curIndex+1) + ')').data('name');
						var slideId = name+'_'+cat;
						if (pushedBannerIds.indexOf(slideId) >= 0)
							return;
						dataLayer.push({
							event: 'carousel_flipping',
							ecommerce: {
								promoView: {
									promotions: [{
										id: slideId,
										name: name,
										creative: name,
										position: 'home_carousel_1'
									}]
								}
							}
						});
						pushedBannerIds.push(slideId);
					},
					onSlideAfter: function($slide,oldIndex,newIndex) {
						var cat = getCategory();
						if (cat != 'home') return;
						var name = $slide.find('img').data('name');
						var slideId = name+'_'+cat;
						if (pushedBannerIds.indexOf(slideId) >= 0)
							return;
						dataLayer.push({
							event: 'carousel_flipping',
							ecommerce: {
								promoView: {
									promotions: [{
										id: slideId,
										name: name,
										creative: name,
										position: 'home_carousel_'+(newIndex+1)
									}]
								}
							}
						});
						pushedBannerIds.push(slideId);
					}
				});
			});
		</script>

