var serverdate;
$(function () {
    if (mtc_frame.ua.indexOf('Safari') > 0) {
        var date_action = 'GET';
    }else{
        var date_action = 'HEAD';
    }
    $.ajax({
        type    : date_action,
        url     : '/date.html',
        cache   : false,
        success : function (data, status, xhr) {
            serverdate = new Date(xhr.getResponseHeader('Date'));
            importantViewCheck(serverdate);
        }
    });
});
function compareDate(year1, month1, day1, year2, month2, day2) {
    var dt1 = new Date(year1, month1, day1);
    var dt2 = new Date(year2, month2 - 1, day2);
    var diff = dt1 - dt2;
    var diffDay = diff/86400000;
    return diffDay;
}
function importantViewCheck(date) {
    $('#mainContent div.important dl').each(function(){
            var diffDay = compareDate(date.getFullYear(),date.getMonth(),date.getDate(),$(this).attr('data-year'),$(this).attr('data-month'),$(this).attr('data-day'));
            if(diffDay>0){
                $(this).remove();
            }else{
                $(this).show();
            }
    });
    if($('#mainContent div.important dl').size()<=0){
        $('#mainContent div.important').hide();
    }
}
/* ]]> */
