<script>
 



    $(function () {
        prepareImageGallery();
        prepareVideoGallery();
        preparePublications();
        $("a.media-img").click(function (event) {
           
            if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
                event.preventDefault();

                var _this = $(this);
                var clicks = 0;

                if (!_this.attr('data-clicks')) {
                    _this.attr('data-clicks', clicks + 1);
                } else {
                    clicks = parseInt(_this.attr('data-clicks')) + 1;
                    _this.attr('data-clicks', clicks);
                }


                if (clicks == 2) {
                    _this.attr('data-clicks', 0);
                    $.prettyPhoto.open(gals[id], titles, descs);
                    return false;
                } else {
                    return false;
                }
            } else {
                $.prettyPhoto.open(gals[id], titles, descs);
                return false;
            }

              
         
        });
    });
    function prepareImageGallery() {
        prepareImageGalleryInit();
    }

    function prepareVideoGallery() {
        $(".vidgal a[rel^='prettyPhoto']").prettyPhoto({ animation_speed: 'fast', slideshow: 10000, hideflash: true, social_tools: false });
    }

    function preparePublications() {
        if (!isTouchDevice) {
            $('.product, .employee')
            .hover(function (e) {
                e.preventDefault();

                $(this).addClass('hover');
            }, function (e) {
                e.preventDefault();

                $(this).removeClass('hover');
            });
        }

        $('body').on('touchstart', function (e) {
            e.stopPropagation();

            if ($(e.target).parents('.product, .employee').length == 0)
                $('.product, .employee').removeClass('hover');
        });

        $('.product, .employee').on('touchend', function () {
            if ($(this).hasClass('hover')) {
                $(this).removeClass('hover');
            } else {
                $('.product, .employee').removeClass('hover');
                $(this).addClass('hover');
            }
        });

        preparePdfLinks();
    }

    var $ = jQuery,
		isTouchDevice = navigator.userAgent.match(/(iPhone|iPod|iPad|Android|BlackBerry|Windows Phone)/);

    var gals = new Array();
    var titles = new Array();
    var descs = new Array();
    var defaultWidth = '100%';
    function prepareImageGalleryInit() {
        $("area[rel^='prettyPhoto']").prettyPhoto({ animation_speed: 'fast', slideshow: 10000, hideflash: true, social_tools: false, });

        var allImages = $("a[data-album]");

        allImages.each(function () {
            var albumId = $(this).data("album");
            var imgSrc = $(this).find("img").attr("src");

            var gal = gals[albumId];
            if (gal == null) {
                gals[albumId] = [imgSrc];
                //titles[albumId] = '';
                //descs[albumId] = '';
            } else {
                if (!gals[albumId].some(function (x) { return x == imgSrc })) {
                    gals[albumId][gals[albumId].length] = imgSrc;
                    //titles[albumId] += "," + '';
                    //descs[albumId] += "," + '';
