<script>
    $(".audiance").click(function () {
        var showPop = $(this).data("pop") != null;
        selectAudience($(this).data("id"), true,showPop);
    });


    function selectAudience(id, reload,showPoup) {

        $("#btnClosseAudience").click();


        if (showPoup)
            $("#dvAudienceInfo").show('Bounce');

        $.ajax({
            cache: false,
            type: "GET",
            url: "/Home/SetAudiance",
            data: { id: id },
            success: function() {
                setTimeout(function() {

                    var remember = $("#chkRememberAudience")[0].checked;

                    if(remember)
                    {
                        setCookie("selectAudience",id,100);
                    }

                    if (reload)
                        window.location.reload(true);
                    else {
                        //$("#dvAudienceInfo").hide();

                    }

                }, showPoup ? 0 : 0);
            },
            error: function() {

            }
        });


    }




    $(function() {
        $.validator.unobtrusive.parse("#form0");

    });

    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays*24*60*60*1000));
        var expires = "expires="+d.toUTCString();
        document.cookie = cname + "=" + cvalue + "; " + expires;
    }

    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i=0; i<ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1);
            if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
        }
        return "";
    }
    function showDialogMessage(message)
    {
        $("#dvMessageText").html(message);
        $("#messageModal").modal("show");
    }
</script>

