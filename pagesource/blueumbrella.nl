        url: 'https://www.blueumbrella.nl/wordpress/wp-admin/admin-ajax.php',
        data: { action: 'set_year', theyear: jQuery('#year').val() },       
        success: function(data) { 
            jQuery('.theyear').html( data );
        } 
    });

}
</script>

<script type="text/javascript">
document.addEventListener( 'wpcf7mailsent', function( event ) {
    if ( 179 == event.detail.contactFormId ) {
    	jQuery('div.wpcf7-mail-sent-ok').remove();
	    var inputs = event.detail.inputs;
	    var firstName;
	    var lastName;
	    var email;
	    for ( var i = 0; i < inputs.length; i++ ) {
	        if ( 'firstName' == inputs[i].name ) {
	            firstName = inputs[i].value;
	        }
	        if ( 'lastName' == inputs[i].name ) {
	            lastName = inputs[i].value;
	        }
	        if ( 'email' == inputs[i].name ) {
	            email = inputs[i].value;
	        }
	    }
		var selection = document.getElementById('select-product').value;
		switch (selection) {
			case "Income Tax":
				url = 'thank-you-blue-tax';
				var product = 'bluetax';
				break;

			case "Childcare Allowance":
				url = 'thank-you-blue-daycare';
				var product = 'bluedaycare';
				break;

			case "Tax Mail":
				url = 'thank-you-blue-mail';
				var product = 'bluemail';
				break;
			default:
				url = 'thank-you-contact';
		}

		if(product) {
			var url_api =  "/api/v1/login/register?firstName="+firstName+"&lastName="+lastName+"&email="+email+"&product="+product;
			$form = jQuery('form#blue-umbrella-register');
			console.log(url_api);
			jQuery.ajax({
				url:  url_api,
				type: 'POST',
			})
			.done(function(result){
		    	if(result.status == "OK") {

		    		location.replace(url);
		    	} else if(result.status == "UsernameExists") {
		    		var $e = jQuery( '<div class="wpcf7-response-output"></div>' ).text( result.message );
					$form.append( $e );
			
		    	} else if(result.status == "ValidationError") {
					var $e = jQuery( '<div class="wpcf7-response-output"></div>' ).text( result.message );
					$form.after( $e );
	
		    	}
			})
			.fail(function(error){
	    		var $e = jQuery( '<div class="wpcf7-response-output"></div>' ).text( error );
				$form.after( $e );

			});
		} else {
			jQuery('div.wpcf7-mail-sent-ok').hide();
			location.replace("/"+url);
		}
	} else {
		location.replace("/thank-you-contact");
	}		
}, false );
</script>
