		<script type="text/javascript">
		  $(function(){
			var TIMEOUT_SLIDE = 6000;
			var TIMEOUT_SLIDE_FAST = 1000;
			var timer;
			var forward = true;
			$('.slide-images').each(function() {
			  var slides = $('.slide-image', this);

			  /// Slide Navigation ///
			  var currentSlideNum = 0;
			  var goToSlide = function(slideNum) {
				slides.eq(currentSlideNum = slideNum).addClass('current').siblings().removeClass('current');
			  };
			  var nextSlide = function() {
				slides.eq(currentSlideNum).addClass('previous').siblings().removeClass('previous');
				goToSlide(currentSlideNum+1>=slides.size() ? 0 : currentSlideNum+1);
			  };
			  var prevSlide = function() {
				goToSlide(currentSlideNum<1? slides.size()-1 : currentSlideNum-1);
				slides.eq(currentSlideNum<1? slides.size()-1 : currentSlideNum-1).addClass('previous').siblings().removeClass('previous');
			  };
			  goToSlide(0);

			  /// Navigation binding ///
			  $('.prevSlide').mouseout(function(){
					forward = true;
					clearInterval(timer);
					timer = setInterval(timerFn, TIMEOUT_SLIDE);
			  });
			  $('.prevSlide').mouseover(function(){
					forward = false;
					clearInterval(timer);
					timerFn();
					timer = setInterval(timerFn, TIMEOUT_SLIDE_FAST);
			  });
			  $('.nextSlide').mouseout(function(){
					forward = true;
					clearInterval(timer);
					timer = setInterval(timerFn, TIMEOUT_SLIDE);
			  });
			  $('.nextSlide').mouseover(function(){
					forward = true;
					clearInterval(timer);
					timerFn();
					timer = setInterval(timerFn, TIMEOUT_SLIDE_FAST);
			  });

			  var timerFn = function() {
				if(forward){nextSlide();}else {prevSlide();}
			  };
			  timer = setInterval(timerFn, TIMEOUT_SLIDE);
			});
		  });
		</script>

