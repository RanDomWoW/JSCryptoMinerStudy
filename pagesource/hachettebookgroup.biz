  <script type="text/javascript" src="/_b2b/static/core/js/plugins/twitter-text.js"></script>


  <script type="text/javascript">
    // requires jQuery, Twitter-Widget, Twitter-Text-JS
    (function($, twt) {
      'use strict';

      // fix time from now per twt display requirements
      Date.prototype.twitterTimeFromNow = (function() {

        var timeFromNowFunc = function() {

          var now = new Date();

          var timeDiff = now - this;

          timeDiff = Math.abs(timeDiff);


          var unit_key = 'now';

          for (var key in conversions) {

            if (timeDiff < conversions[key]) {
              break;
            }

            unit_key = key;
            timeDiff = timeDiff / conversions[key];

          }

          if (unit_key === 'hour' && timeDiff/24 >= 1) {
            return calculateTime(this, 'month', true);
          }

          timeDiff = Math.floor(timeDiff);

          return calculateTime(timeDiff, unit_key);
        };

        var timeUnit = function(key) {
          return timeChoices[key];
        };

        var calculateTime = function(timeDiff, unit_key) {

          var unit = timeUnit(unit_key);

          if (unit_key === 'month') {
            unit = unit[timeDiff.getMonth()];
            timeDiff = timeDiff.getDate();

          } else if (unit_key !== 'now') {
              unit = timeDiff === 1 ?
                timeUnit(unit_key)['one'] :
                timeUnit(unit_key)['other'];
          }

          return unit.replace('%n', timeDiff+"");
        };

        var conversions = {
          now: 1,
          second: 1000,
          minute: 60,
          hour:   60
        };

        var timeChoices = {
          now: 'Now',
          second: {one: '%ns', other: '%ns'},
          minute: {one: '%nm', other: '%nm'},
          hour:   {one: '%nh', other: '%nh'},
          month: ['%n Jan','%n Feb','%n Mar',
                  '%n Apr','%n May','%n June',
                  '%n July','%n Aug','%n Sept',
                  '%n Oct','%n Nov','%n Dec']
        };

        return timeFromNowFunc;

      }());

      $(function(){
        var rootContainers$ = $('#twitter_timeline');

        if (rootContainers$.length > 0) {
          var dateXformRex = /^\w+ (\w+) (\d+) ([\d:]+) \+0000 (\d+)$/;
          var dateStructure = "$1 $2 $4 $3 UTC";
          var intentBaseURL = "https://twitter.com/intent/user?screen_name=";

          $.each(rootContainers$, function(index, container) {
              var rootContainer$ = $(container);
              rootContainer$.attr('id', this.id + index);


              var tweetContainers$ = $('.tweet', rootContainer$);
              var tweetTimestamps$ = $('.tweet-timestamp', rootContainer$);
              var tweetTexts$ = $('.tweet-text', rootContainer$);
              var userVerified$ = $('.user-verified', rootContainer$);
              var webIntents$ = $('.web-intent', rootContainer$);
              var timelineOptions = {
                    targetBlank: true,
                    hashtagUrlBase:  intentBaseURL,
                    usernameUrlBase: intentBaseURL,
                    usernameIncludeSymbol: true
                  };

              if (rootContainer$.outerWidth() < 240) {
                var profileImages$ = $('.profile-image', rootContainer$);
                var tweets$ = $('.tweet', rootContainer$);
                var userFullNames$ = $('.user-full-name', rootContainer$);
                var userScreenNames$ = $('.user-screen-name', rootContainer$);

                profileImages$.css({
                  'position' : 'static',
                  'padding-bottom' : '4px',
                  'display' : 'block',
                  'clear' : 'both'
                });

                tweets$.css({
                  'position' : 'static',
                  'padding' : '2px'
                });

                userVerified$.css({
                  'display' : 'none'
                });

                userFullNames$.css({
                  'position' : 'static',
                  'padding' : '2px',
                  'display' : 'block',
                  'clear' : 'both'
                });

                userScreenNames$.css({
                  'position' : 'static',
                  'padding' : '2px',
                  'display' : 'block',
                  'clear' : 'both'
                });

              }


              $.each(tweetTimestamps$, function(index, timestamp) {
                var timestamp$ = $(timestamp);
                var timeFromNow = new Date(
                      timestamp$.text().replace(dateXformRex, dateStructure)
                    ).twitterTimeFromNow();

                timestamp$.text(timeFromNow);
              });

              $.each(tweetTexts$, function(index, tweet) {
                var tweet$ = $(tweet);
                var tweetText = (tweet$.text());
                    // twttr.txt magic here
                var newText = twt.autoLink(tweetText, timelineOptions);

                tweet$.html(newText);
              });


              tweetContainers$.hover(
                function(event){
                  $(this).find('.tweet-actions').show();
                },
                function(event){
                  $(this).find('.tweet-actions').hide();
                }
              );

              /* overriding styling higher in the cascade */
              webIntents$.css({
                'background' : 'none',
                'padding-left' : 7
              });

          });

        }

      });

    }(jQuery, twttr.txt));

  </script>

  




