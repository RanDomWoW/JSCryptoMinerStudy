    function checkSearch() {
        if (!this.nodeName || this.nodeName.toUpperCase() != "FORM") return;
        var searchCriteriaSupplied = false;
        var searchCriteriaField = $(this).find(".catalogSearchCriteria").get(0);
        if (!searchCriteriaSupplied) {
            var searchCriteriaValue = $(searchCriteriaField).attr('value');
            var defaultValue = $(searchCriteriaField).attr('defaultValue');
            if (searchCriteriaValue.length > 1 && searchCriteriaValue != defaultValue) {
                searchCriteriaSupplied = true;
            }
        }

        if (!searchCriteriaSupplied) {
            return false;
        } else {
            $(searchCriteriaField).attr('value', $(searchCriteriaField).attr('value').replace($(searchCriteriaField).attr('defaultValue'),""));

            var formAction = $(this).attr('action');
            var targetProtocol = (formAction.indexOf('https:') == 0) ? 'https:' : ((formAction.indexOf('http:') == 0) ? 'http:' : window.location.protocol);

            // only do the workaround if the current protocol is https and the target is http
            if (window.location.protocol == 'http:' || targetProtocol == 'https:') return true;

            var href="";
            var delimiter="?";

            $('#CatalogSearchForm input[type != "image"]').each(function() {
                href += delimiter + $(this).attr('name') + '=' + encodeURIComponent($(this).attr('value'));
                delimiter = "&";
            });

            href = formAction + href;

            // If the url is too long then let the form submit normally otherwise IE has problems.
            if (href.length < 2000) {
                window.location.href= href;
                return false;
            } else {
                return true;
            }
        }
    }

    $.fn.prompt = function(defaultValue) {
        
        $(this).attr("value", defaultValue);
        $(this).attr("defaultValue", defaultValue);

        $(this).focus(function() {
            if( this.value == defaultValue ) {	this.value = ""; }
        }).blur(function() {
            if( !this.value.length ) { this.value = defaultValue; }
        });
    };

