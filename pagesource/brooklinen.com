<script>
  // Helper function to add commas to yotpo reviews counts
  function numberWithCommas(x) {
      return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
  }

  jQuery(function($){
    // On mobile, move middle product (luxe bundle) first (on top)
    $(window).on('ready load resize',function(e) {
        if( $(window).width() < 768 ){
            $('.grid__item--2').insertBefore('.grid__item--1');
        }else{
            $('.grid__item--1').insertBefore('.grid__item--2');
        }
    });

    // Set the height of the spotlight product based on breakpoint
    var spotlight_table = $('.spotlight .table');
    var spotlight_img = $('.spotlight .product');
    $(window).on('load resize',function(){
        if( $(window).width() > 767) {
            spotlight_table.css({ height: spotlight_img.height()+'px' });
        }else{
            spotlight_table.css({ height: ' ' });
        }
    });

    // Add commas to yotpo reviews counts
    $(window).on('load',function(){
      $('a.text-m').each(function(){
        var self = $(this);
        var array = self.text().split(' ');
        var num = parseInt(array[0]);
        var comma_num = numberWithCommas(num);
        self.text(comma_num + ' Reviews');
      });
    });
  });
</script>
