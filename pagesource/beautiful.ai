<script type="text/javascript">


    $(document).ready(function () {



        startVideoTimer = new Date();


        $.fn.isOnScreen = function () {

            var win = $(window);

            var viewport = {
                top: win.scrollTop(),
                left: win.scrollLeft()
            };
            viewport.right = viewport.left + win.width();
            viewport.bottom = viewport.top + win.height();

            var bounds = this.offset();
            bounds.right = bounds.left + this.outerWidth();
            bounds.bottom = bounds.top + this.outerHeight();

            return (!(viewport.right < bounds.left || viewport.left > bounds.right || viewport.bottom < bounds.top || viewport.top > bounds.bottom));

        };




   //story template controls----------------------------------------------
        $(".template-item").click(function () {


            var deck = "";
            $("#player-iframe").attr("src", deck);
            
						var link = $(this).children(".sample-deck-data").children().data("link");
            var name = $(this).children(".sample-deck-data").children().data("name");
            ga('send', {
                hitType: 'event',
                eventCategory: 'Example Decks',
                eventAction: 'View',
                eventLabel: name
            });

            $("#player-iframe").attr("src", link + "?autoPlay&loop&time=5");

            $(".embed-popup-container").fadeIn("fast");
            $('html').css('overflow', 'hidden');



        });



        $(".close-button, .embed-popup-container").click(function () {
            $(".embed-popup-container").fadeOut("fast");
            $('html').css('overflow', 'visible');

            $("#player-iframe").attr("src", "");

        });
        


        $("#getdeck-button").click(function () {
            var newLink = $("#player-iframe").attr("src").replace("player", "deck");
            $(this).attr("href", newLink);
            console.log(newLink);

        });



        $(document).bind("scroll.videoScroll", function () {
            if (!$('#video-frame').isOnScreen()) {
                getVideoViewedSeconds();
            }
        });

        var getVideoViewedSeconds = (function () {
            var executed = false;
            return function () {
                if (!executed) {
                    executed = true;
                    var endVideoTimer = new Date();
                    var videoViewedSeconds = (endVideoTimer - startVideoTimer) / 1000;
										
                    
                    ga('send', {
                        hitType: 'event',
                        eventCategory: 'Page Scroll',
                        eventAction: 'Scroll Past Main Video',
                        eventLabel: Math.round(videoViewedSeconds)
                        //eventValue: Math.round(videoViewedSeconds)
                    });
                    
                    //debugger;


                    $(document).unbind('.videoScroll');
                }
            };
        })();
        
        
        
        
    });
//END DOCUMENT READY


    //video handling----------------------------------------------
    $(".video-overlay").click(function () {
        var element = document.getElementById("screenflow");

        if (element.mozRequestFullScreen) {
            element.mozRequestFullScreen();
            //iphone needs this otherwise unmute even doesnt fire after video goes fullscreen
            $(element).prop('muted', false);
        } else if (element.webkitEnterFullScreen) {
            element.webkitEnterFullScreen();
            $(element).prop('muted', false);
        }
    });

    document.addEventListener("fullscreenchange", onFullScreenChange, false);
    document.addEventListener("webkitfullscreenchange", onFullScreenChange, false);
    document.addEventListener("mozfullscreenchange", onFullScreenChange, false);

    function onFullScreenChange() {
        var state = document.fullScreen || document.mozFullScreen || document.webkitIsFullScreen;
        var event = state ? 'FullscreenOn' : 'FullscreenOff';
        var element = document.getElementById("screenflow");
        if (event === "FullscreenOn") {
            $(element).prop('muted', false);
        } else {
            $(element).prop('muted', true);
        }
    }




 
 </script></body></html>