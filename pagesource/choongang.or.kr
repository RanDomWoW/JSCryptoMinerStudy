              var $megaSubMenu =  $megaMenuMegamenu.find('.mega-sub-menu');
                  $megaSubMenu.css({ 'box-shadow':'none'});
                  $megaSubMenu.find('>li').css({ 'line-height':'1.2'});
                  $megaSubMenuLink = $megaSubMenu.find('>li>a').css({ 'line-height':'1.2'});
       
           
             $megaSubMenuLink.each(function(){ 
               
               
                      var elem = $(this).get(0);
                      var theCSSprop = window.getComputedStyle(elem,  ':before').getPropertyValue('vertical-align'); 
                
                
                      if(theCSSprop === 'text-bottom' ){

                             $(this).addClass('mega-menu-icon').css({'line-height':'2.6'});
                      }
             });       
          

   
              console.log("$megaSubMenu.offset().top", $megaSubMenu.offset().top);
 
              var $subMenuWrap = $('<div class="sub-menu-wrap"></div>');
          
              var submenuWrapTop = $('#ch-solution').length? 87:83;
          
                  $subMenuWrap.insertBefore ($megaSubMenu.closest('.row'));
          
                  $subMenuWrap.css({
                      display: 'block',
                      visibility:'hidden',
                      position: 'absolute',
                      top: submenuWrapTop+'px',
                      left: 0,
                      right: 0,
                      background:  $megaSubMenu.css('background-image'),
                      height: '0',
                      zIndex: '2',
                    
                       opacity: '0',
                      'transition-duration': '200ms',
                      'transition-timing-function': 'ease-in',
                      'transition-property': 'opacity, visibility,display'
                  });

          
               //event
                $megaMenuMegamenu.on('mouseenter', function(){
                       
       
                      $subMenuWrap.css({  height:  $(this).find($megaSubMenu).outerHeight() +'px' ,opacity: '1',  visibility:'visible' });
                });
          
                $megaMenuMegamenu.on('mouseleave', function(){
                      $subMenuWrap.css({  opacity: '0', visibility:'hidden'});
                });


        }
        function init(){

            //menu init
            $mythemesHeaderMenu = $megaMenuMegamenu.closest('.mythemes-header-menu');
             if($mythemesHeaderMenu.length>0){

                  setMenu();
             }
        }     
          init();
    });
 })(jQuery);

 /*end: fullmenu setting*/</script>

