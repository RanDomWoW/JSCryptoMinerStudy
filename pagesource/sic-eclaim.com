                var infoPanelIsHidden = true;
                function toggleInfoPopup(button, args) {
                    var infoPanel = $telerik.$(".supportInfo");
                    var buttonElement = $telerik.$(button.get_element());
                    if (infoPanelIsHidden) {
                        infoPanel.addClass("supportInfoOpen");
                        button.addCssClass("supportHideInfo");
                        infoPanel.animate({ width: 350, height: 555 }, 1000, 'swing', function () { infoPanelIsHidden = !infoPanelIsHidden; });
                    }
                    else {
                        infoPanel.animate({ width: 35, height: 71 }, 1000, 'swing', function () { infoPanelIsHidden = !infoPanelIsHidden; buttonElement.animate({ opacity: 0 }, 'medium', function () { $telerik.$(this).removeClass("supportHideInfo"); $telerik.$(this).animate({ opacity: 1 }, "medium", function () { infoPanel.removeClass("supportInfoOpen"); }); }); });
                    }
                }
            </script>
