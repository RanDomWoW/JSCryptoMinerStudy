			var $filteredPortfolio = $portfolioClone.find("div");
		} else {
			var $filteredPortfolio = $portfolioClone.find("div[data-type~=" + $filterClass + "]");
		}
		
		// Call quicksand
		jQuery("#ice_filter_items_312 .row-fluid").quicksand( $filteredPortfolio, { 
			duration: 600, 
			retainExisting:true,
			useScaling : false,
			easing: 'swing', 
