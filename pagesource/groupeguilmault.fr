	var $portfolioClone = jQuery(".ice_filter_items").clone();
	
	// Attempt to call Quicksand on every click event handler
	jQuery("#ice_filter_nav_312 a").click(function(e){
		
		jQuery("#ice_filter_nav_312 li").removeClass("current");	
		
