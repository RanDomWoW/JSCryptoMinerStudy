    <!-- Шаблоны  кнокаут -->
    <script type="text/html" id="templateInput">
        <div class="input-field">
            <input type="text" data-bind="value: value, attr: { id: fieldname, name: fieldname}, tooltip: tooltip">
            <label data-bind="text: label, attr: { for: fieldname }" data-error=""></label>
        </div>
    </script>
    <script type="text/html" id="templateInputFile">
        <div class="file-field input-field">
            <div class="btn">
                <span>Загрузить</span>
                <input type="file" data-bind="fileInput: value, attr: { name: fieldname}, tooltip: tooltip">
            </div>
            <div class="file-path-wrapper ">
                <input class="file-path" type="text" data-bind="validationElement: value, attr: { 'placeholder': label}">
                <span class="validationMessage" data-bind="validationMessage: value"></span>
            </div>
        </div>
    </script>
    <script type="text/html" id="templateSelect">
        <div class="input-field">
            <select class="materialize" data-bind="options: value, optionsText: 'name', optionsValue: 'id', value: selected_id, tooltip: tooltip, attr: { name: fieldname, id: fieldname}, enable: enable">
            </select>
            <label data-bind="text: label, attr: { for: fieldname }"></label>
        </div>
    </script>
    <script type="text/html" id="templateTextarea">
        <div class="input-field">
            <textarea data-bind="text: value, attr: { name: fieldname, id: fieldname}" class="materialize-textarea"></textarea>
            <label data-bind="text: label, attr: { for: fieldname }"></label>
        </div>
    </script>
    <script type="text/html" id="templateCheckbox">
        <div class="input-field">
            <input type="checkbox" class="filled-in" data-bind="value: value, checked: checked, attr: { name: fieldname, id: fieldname}" />
            <label data-bind="text: label, attr: { for: fieldname }"></label>
        </div>
    </script>
    <script type="text/html" id="templateInputHidden">
        <input type="hidden" data-bind="value: value, attr: { name: fieldname, id: fieldname}" />
    </script>

