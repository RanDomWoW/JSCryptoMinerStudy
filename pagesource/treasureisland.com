<script>


	jQuery(document).ready(function($) {

		$('a[data-toggle="check-hotel-availability"]').on('click', function(e) {
			e.preventDefault();
			trackEvent('Booking Widget', 'Hotel', 'Check Availability');
			var url = getUrl();
			window.open(url, '_blank');
			return false;
		});

		$('.date-summary[data-toggle="arrival"]').on('click', function(e) {
			toggleDatePicker($(this));
			e.preventDefault();
		});

		$('.date-summary[data-toggle="departure"]').on('click', function(e) {
			toggleDatePicker($(this));
			e.preventDefault();
		});

		$('.date-summary[data-toggle="guests"]').on('click', function(e) {
			toggleGuestPicker($(this));
			e.preventDefault();
		});

		$('ul.guests-selector li').bind('click', function(e) {
			$('form#hotel-booking-form input[name="guests"]').val($(this).text());
			$('.date-summary[data-toggle="guests"] .date-day').html($(this).text());
			$('.date-summary.active').removeClass('active');
			$(this).parents('ul').hide();
		});




		function toggleDatePicker(elem)
		{
			$('.datepicker-box').datepicker('destroy');

			if (elem.hasClass('active'))
			{
				elem.removeClass('active');
				$('div#'+elem.attr('data-toggle')).hide();
			}
			else
			{
				$('.date-summary.active').removeClass('active');
				$('.guests-selector').hide();
				elem.addClass('active');
				$('div#'+elem.attr('data-toggle')).show();

                if (elem.attr('data-toggle')=='departure'){
                    var dataToggle = 'departure';
                    }
                else{
                    var dataToggle = 'arrival';
                    }

				$('div#'+elem.attr('data-toggle')).find('.datepicker-box').datepicker({
					altField: $('div#'+elem.attr('data-toggle')).find('input'),
					defaultDate: $('div#'+elem.attr('data-toggle')).find('input').val(),
					onSelect: function(date, elem) {
						var summaryElem = $('.date-summary.active');

                        if (getNights()>14){
                            alert("Oops! Online reservations are limited to 14 consecutive days.");
                            resetDates(dataToggle);
                        }

                        else{
                            updateSummary(summaryElem, date);
                            updateDefaultDepartureDate();
                        }

                        $('.datepicker-box').datepicker('destroy');
			            // update the default departing date

			        }
				});
			}
		}

		function toggleGuestPicker(elem)
		{
			$('.datepicker-box').datepicker('destroy');

			if (elem.hasClass('active'))
			{
				elem.removeClass('active');
				$('div#'+elem.attr('data-toggle')).find('input.calendar-input').blur();
				$('.guests-selector').hide();
			}
			else
			{
				$('.date-summary.active').removeClass('active');
				elem.addClass('active');
				$('.guests-selector').show();
			}
		}

		function updateSummary(elem, date)
		{
			// var year = date.substr(0, 4);
			// var month = date.substr(0, 2);
			// var day = date.substr(8, 2);

			var d = new Date(date+' PST');

			elem.removeClass('active');
			elem.find('.date-month').html(getMonth(d.getMonth()));
			elem.find('.date-day').html(d.getDate());
		}

		function getArrivalDate()
		{
			var date = $('input.calendar-arrival').val();
			var year = date.substr(8, 2);
			var month = date.substr(0, 2);
			var day = date.substr(3, 2);

			return month+day+year;
		}

		var defaultNumDays = 3;
		function updateDefaultDepartureDate()
		{
			if (getNights() <= 0)
            {
            	var d = new Date($('input.calendar-arrival').val()+' PST');
            	d.setDate(d.getDate() + defaultNumDays);
            	var date = (d.getMonth()+1)+'/'+(d.getDate())+'/'+d.getFullYear();

				$('input.calendar-departure').val(date);
				updateSummary($('.date-summary[data-toggle="departure"]'), date);
            }
		}

        function resetDates(toggle){
            if (toggle=="arrival"){
                var d = new Date($('input.calendar-departure').val()+' PST');
                d.setDate(d.getDate() - 14);
                var date = (d.getMonth()+1)+'/'+(d.getDate())+'/'+d.getFullYear();
				$('input.calendar-arrival').val(date);
				updateSummary($('.date-summary[data-toggle="arrival"]'), date);
            }
            else{
                var d = new Date($('input.calendar-arrival').val()+' PST');
                d.setDate(d.getDate() + 14);
                var date = (d.getMonth()+1)+'/'+(d.getDate())+'/'+d.getFullYear();
				$('input.calendar-departure').val(date);
				updateSummary($('.date-summary[data-toggle="departure"]'), date);
            }
        }

		function getNights()
		{
			var date1 = new Date($('input.calendar-arrival').val()+' PST');
			var date2 = new Date($('input.calendar-departure').val()+' PST');
			return daysBetween(date1, date2);
		}

		function getGuests()
		{
			return $('form#hotel-booking-form input[name="guests"]').val();
		}

		function getRoom()
		{
			return $('form#hotel-booking-form select[name="room_type"] option:selected').val().substr(2);
		}

		function getUrl()
		{
			var baseUrl = "https://bookings.treasureisland.com/102892";

			// old reservations url
			// baseUrl = baseUrl.substr(0, baseUrl.length-3);
			// if(/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)){
			// 	var url = baseUrl + "/m;";
			// }
			// else{
			// 	var url = baseUrl+';';
			// }
			// url += getArrivalDate()+';';
			// url += getNights()+';';
			// url += getGuests()+';';
			// url += ';';
			// url += getRoom()+';';
			// url += ';?/';

			var date_in = $('input.calendar-arrival').val();
			var date_out = $('input.calendar-departure').val();
			var guests = getGuests();

			var url = baseUrl+'?DateIn='+date_in+'&DateOut='+date_out+'&Rooms=1&Adults='+guests+'&Children=0';

			return url;
		}

		function getMonth(n)
		{
			var month=new Array();
			month[0]="Jan";
			month[1]="Feb";
			month[2]="Mar";
			month[3]="Apr";
			month[4]="May";
			month[5]="Jun";
			month[6]="Jul";
			month[7]="Aug";
			month[8]="Sep";
			month[9]="Oct";
			month[10]="Nov";
			month[11]="Dec";
			return month[n];
		}

		function daysBetween(first, second) {

			// Copy date parts of the timestamps, discarding the time parts.
			var one = new Date(first.getFullYear(), first.getMonth(), first.getDate());
			var two = new Date(second.getFullYear(), second.getMonth(), second.getDate());

			// Do the math.
			var millisecondsPerDay = 1000 * 60 * 60 * 24;
			var millisBetween = two.getTime() - one.getTime();
			var days = millisBetween / millisecondsPerDay;

			// Round down.
			return Math.floor(days);
		}

	});
</script>
