<script>
	jQuery(document).ready(function($) {

        //set LAX as default
        $('.dropdown-toggle[data-id=gsOrigin] .filter-option').text($("select[name=gsOrigin] option[value='LAX']").text());
        $('select[name=gsOrigin]').val("LAX");
        $('.bootstrap-select').selectpicker('refresh');

		// attach submit handler
		$('a[data-toggle="check-air-hotel-availability"]').bind('click', function(e) {
			$('form#air-hotel-booking-form').submit();
			e.preventDefault();
		});

		$('form#air-hotel-booking-form').submit( function(e) {
			trackEvent('Booking Widget', 'Air+Hotel', 'Search');

			var num_adults = $('select[name="num_adults"] option:selected').val();
			var num_kids = $('select[name="num_kids"] option:selected').val();

			// reset ages
			$('input[name*="gsAge"]').val('');

			for (var i=0; i<num_kids; i++)
			{
				$('input[name="gsAge'+(i+1)+'"]').val((i+1));
				num_adults++;
			}

			$('input[name="gsNumberofTravelers"]').val(num_adults);

			return true;
		});

        function getNights()
		{
			var date1 = new Date($('#gsDepartureDate').val()+' PST');
			var date2 = new Date($('#gsReturnDate').val()+' PST');
			return daysBetween(date1, date2);
		}

        function daysBetween(first, second) {

			// Copy date parts of the timestamps, discarding the time parts.
			var one = new Date(first.getFullYear(), first.getMonth(), first.getDate());
			var two = new Date(second.getFullYear(), second.getMonth(), second.getDate());

			// Do the math.
			var millisecondsPerDay = 1000 * 60 * 60 * 24;
			var millisBetween = two.getTime() - one.getTime();
			var days = millisBetween / millisecondsPerDay;

			// Round down.
			return Math.floor(days);
		}

        var defaultNumDays = 3;
        function updateDefaultReturnDate()
		{
			if (getNights() <= 0)
            {
            	var d = new Date($('#gsDepartureDate').val()+' PST');
            	d.setDate(d.getDate() + defaultNumDays);
            	var date = (d.getMonth()+1)+'/'+(d.getDate())+'/'+d.getFullYear();
				$('#gsReturnDate').val(date);
            }
		}

        function resetDeparting(){
            if (getNights()>14){
                alert("Oops! Online reservations are limited to 14 consecutive days.");
                var d = new Date($('#gsReturnDate').val()+' PST');
                d.setDate(d.getDate() - 14);
                var date = (d.getMonth()+1)+'/'+(d.getDate())+'/'+d.getFullYear();
                $('#gsDepartureDate').val(date);
            }
        }

        function resetReturning(){
            if (getNights()>14){
                alert("Oops! Online reservations are limited to 14 consecutive days.");
                var d = new Date($('#gsDepartureDate').val()+' PST');
                d.setDate(d.getDate() + 14);
                var date = (d.getMonth()+1)+'/'+(d.getDate())+'/'+d.getFullYear();
                $('#gsReturnDate').val(date);
            }
        }


        setTimeout(function(){
            $('#gsDepartureDate').datepicker("option", "onSelect", function(date, elem){
                resetDeparting();
                updateDefaultReturnDate();
            });

            $('#gsReturnDate').datepicker("option", "onSelect", function(date, elem){
                resetReturning();
                updateDefaultReturnDate();
            });
            $("#ui-datepicker-div").hide();
        }, 1000);
	});
</script>
