function change_ad_b_select(i)
{
	var length;	
	$('#ad_b_select_' + i).attr('class','select');
	for(length = 0;length <= 4; length++)
	{
		if(length != i)
		{
			$('#ad_b_select_' + length).removeAttr('class','select');
		}
	}
}

//換B區顯示圖
function change_mab_pic(num)
{
	$("#ad_b_" + $("#ad_b_flag").html()).toggle();
	$("#ad_b_" + num).css("display", "block");
	$("#ad_b_flag").html(num);
	change_ad_b_select(num);
}

//輪換時間間隔
var mab_time_interval = 8000;
var mab_timer;
//輪換B區顯示圖
function rotate_mab_pic(curnum)
{
	var max_num = '4';
	//alert(curnum);
	if(curnum == max_num)
	{
		change_mab_pic(0);
	}
	else
	{
		change_mab_pic(++curnum);
	}
	
	mab_timer = setTimeout(function(){rotate_mab_pic($("#ad_b_flag").html())},mab_time_interval);
}

