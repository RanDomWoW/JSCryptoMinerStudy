//抓廣告
function display(id,area)
{
	//先隱藏所有指定區域的廣告
	$(".ad_class_" + area).hide();
	//先刪除所有class內容
	$(".tab_class_" + area).removeAttr("class"); 
	
	//顯示被點擊的頁籤廣告
	$("#ad_" + id).show();
	//顯示被點擊的頁籤
	if(area == 'F')
	{
		if(id == 67 || id == 66 || id == 65)
		{
			$("#tab_" + id).attr("class","none select tab_class_" + area); 
		}
		else
		{
			$("#tab_" + id).attr("class","none tab_class_" + area); 
		}
	}
	else
	{
		$("#tab_" + id).attr("class","select tab_class_" + area); 
	}
	
	if(area == 'A' || area == 'B' || area == 'C' || area == 'D')
	{
		$(".data-ad-" + id).each(function(){
			var img_url = $(this).attr("data-ad");
			$(this).attr("data-original",img_url);
			$(this).attr("src",img_url);
		});
	}

}

