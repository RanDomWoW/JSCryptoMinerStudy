<script type="text/javascript">
$( document ).ready(function() {

    var lista_sub = $('.sub-menu');

    // iterar en la lista de sub-menus
    for (k = 0; k < lista_sub.length; k++) {
        // asignar id a sub menus
        lista_sub[k].setAttribute("id","sub"+k);
    }
    // iterar en la lista de submenus
    for (k = 0; k < lista_sub.length; k++) {

        // obviar si el elemento padre tiene "una-col" de clase
        if ( (lista_sub[k].parentElement.className).search('una-col') == -1) {

            var ul = $('#sub'+k),
                li = ul.children('li');

            i=0;
            a=0;

            mitad = Math.round(li.length/2);

            while(i < mitad) {
                li[i].setAttribute("data-orden", a);
                a=a+2;
                i++;
            }

            a=1;

            while(i < li.length) {
                li[i].setAttribute("data-orden", a);
                a=a+2;
                i++;
            }

            li.detach().sort(function(a,b) {
                return $(a).data('orden') - $(b).data('orden');  
            });

            ul.append(li);
        }
    }

    // establecer alturas correctas
    for (k = 0; k < lista_sub.length; k++) {

        if ( (lista_sub[k].parentElement.className).search('una-col') == -1) {
            var ul = $('#sub'+k),
                li = ul.children('li');

            ul.css("opacity", 0);
            ul.css('display','block');        

            i=0;
            l_height = 0;

            while(i < li.length) {
                if ( i%2 == 0) {
                    l_height = $('#'+li[i].id).height();
                }
                else {
                    if ( l_height != 0 ) {
                        $('#'+li[i].id).height(l_height);
                    }                
                }
                i++;
            }

            ul.css('display', '');
            ul.css("opacity", 'initial');
        }
    }

    // Establecer ancho de filas de una sola columna
    $('.una-col ul').css('width','140px');

    // $('.una-col ul li').css('clear','both');
    $('.una-col ul li').css('width','140px');
});
</script>
