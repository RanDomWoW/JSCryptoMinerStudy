					$.cookie( "indexAccess_01" , "true" , {expires: 7});
                    new Opening ();
                }

                function Opening (){

                    var stage = new createjs.Stage("opening");
                    var txt_tange;
                    var copy;
                    
                    var path = "https://www.tangeweb.com/wp-content/themes/tange";
                    var manifest = [
                        {src: path + "/images/index/header_parts-tange.png", id:"txt_tange"},
                        {src: path + "/images/index/header_parts-read.png", id:"copy"}
                    ];

                    var queue = new createjs.LoadQueue();
                    queue.loadManifest(manifest, false);
                    queue.addEventListener("fileload", fileloadHandler);
                    queue.addEventListener("complete", completeHandler);
                    queue.load();

                    function fileloadHandler(evt) {
                        var bitmap;
                        switch(evt.item.id) {
                            case "txt_tange" :
                                txt_tange = createObject (evt, 63, 38 + 20);
                                break;
                            case "copy" :

                                var bitmap = new createjs.Bitmap(evt.result);
                                var bounce = bitmap.getBounds();
                                bitmap.x = -bounce.width / 2;
                                bitmap.y = -bounce.height / 2;
                                copy = new createjs.Container ();
                                copy.addChild(bitmap);
                                copy.x = 291;
                                copy.y = 138;
                                copy.alpha = 0;
                                copy.scaleX = copy.scaleY = 0.9;
                                break;
                        }
                    }

                    function createObject (evt, x, y){
                        var bitmap = new createjs.Bitmap(evt.result);
                        var obj = new createjs.Container ();
                        obj.addChild(bitmap);
                        obj.x = x;
                        obj.y = y;
                        obj.alpha = 0;
                        return obj;
                    }

                    function completeHandler(evt) {
                        queue.removeEventListener("fileload", fileloadHandler);
                        queue.removeEventListener("complete", completeHandler);
                        intro();
                    }

                    function intro (){
                        $(".visualArea__sliderList").animate( {opacity: '0'},{duration: 0, easing: 'linear'});


                        createjs.Ticker.setFPS(60);
                        createjs.Ticker.addEventListener("tick", stage);

                        stage.addChild( txt_tange );
                        stage.addChild( copy );

                        createjs.Tween.get(txt_tange).wait(400).to({ 'alpha': 1, 'y': 38 }, 1600, createjs.Ease.quartOut);
                        createjs.Tween.get(copy).wait(1800).to({ 'scaleX': 1, 'scaleY': 1, 'alpha': 1 }, 1600, createjs.Ease.cubicOut);

                        $(".visualArea__scene--01").delay(6000).animate( {opacity: '0'},{duration: 1200, easing: 'easeOutQuart', complete: handleComplete});
                    }

                    function handleComplete() {
                        $.cookie( "indexAccess" , "true" , {expires: 7});
                        $(".header__block").delay(100).animate( {top:"0"},{duration: 1200, easing: 'easeOutQuart'}).promise().done(function () {});
                        $(".visualArea__sliderList").delay(800).animate( {opacity: '1'},{duration: 1500, easing: 'easeOutQuart'}).promise().done(function () {
                            slider.startAuto();
                        });
                        $(".visualArea__scene--01").hide();
                    }
                }
            });


        </script>

