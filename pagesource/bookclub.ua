		<script type="text/javascript">
	var arrayMessageSborEmailError = new Array();
	arrayMessageSborEmailError[1] = 'Спасибо! Вы успешно подписаны';
	arrayMessageSborEmailError[2] = 'Некорректный email';
	arrayMessageSborEmailError[3] = 'Такой email уже был введен';
	arrayMessageSborEmailError[4] = 'Введите email в поле';
	arrayMessageSborEmailError[5] = 'Ошибка отправки. Попробуйте ещё раз';
	arrayMessageSborEmailError[6] = 'Спасибо! Ваш email принят';

	$(function() {
		createError = function(num) {
			var er = $('.foot_subscr_block .ai_inp_email .error_eml_foot');
			er.text(arrayMessageSborEmailError[num]);
			if (er.hasClass('disp_n')) {
				er.removeClass('disp_n');
			}
		}
		checkEmail = function(email) {
			if (!email) {
				createError(4);
				return false;
			}
			var reg = /^[_\.0-9a-z-]+@([0-9a-z][0-9a-z-]*\.)+[a-z]{2,}$/i;
			if (!email.match(reg)) {
				createError(2);
				return false;
			}
			return true;
		}
		$('.foot_subscr_block input[name=email_aiem]').bind('input', function() {
			var er = $('.foot_subscr_block .ai_inp_email .error_eml_foot')
			if ($(this).val() && !er.hasClass('disp_n')) {
				er.addClass('disp_n');
			}
		});
		submEmailFunc = function(){
			var email = $('.foot_subscr_block').find('input[name=email_aiem]').val();
			if (!checkEmail(email)) { return; }
			$.ajax({
				url: '/infocenter/sbor/ajax/ajax_set_email.php',
				type: "POST",
				data: {email : email, lng : 1},
				success: function(num) {
					createError(num);
					if (num != 3) {
						$('.foot_subscr_block input[name=email_aiem]').val('');
					}
					if (num % 5 == 1) {
						$('.foot_subscr_block .ai_inp_email .error_eml_foot').addClass('success-mess');
					} else {
						if ($('.foot_subscr_block .ai_inp_email .error_eml_foot').hasClass('success-mess')){
							$('.foot_subscr_block .ai_inp_email .error_eml_foot').removeClass('success-mess');
						}
					}
					/*else if (num == 4) {
						$('.foot_subscr_block .ai_inp_email .error_aiem').text('Уже');
					} else if (num == 2 || num == 3 || num == 5) {
						createError(num);
					}*/
				}
			});
		}
		$('.foot_subscr_block .email_aiem').keypress(function(e){
	      if(e.keyCode==13){
				submEmailFunc();
			}
		});
		$('.foot_subscr_block .send_email').click(function(){
			submEmailFunc();
		});		
		
	})
</script>

