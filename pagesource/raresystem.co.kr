			<script>
					function Selection_brandF(brandF) {
						document.all.brandF_1.style.display="none";
						document.all.brandF_2.style.display="none";
						document.all.brandF_3.style.display="none";
				
						if(brandF=='1') {document.all.brandF_1.style.display="block";}
						if(brandF=='2') {document.all.brandF_2.style.display="block";}
						if(brandF=='3') {document.all.brandF_3.style.display="block";}
					}

					var r_delay = 3000;
					var r_status = 'Play';
					var nowCount_TypeF = 0;
					var count_TypeF = 3;

					function select_TypeF(tabno) {
						if(tabno=='0') {Selection_brandF('2')}
						if(tabno=='1') {Selection_brandF('3')}
						if(tabno=='2') {Selection_brandF('1')}
						
						for(Tno=0; Tno<count_TypeF; Tno++){
							document.all['tabTypeF_'+Tno].style.backgroundColor="#9b9b9b";
							if(tabno==Tno) {
									document.all['tabTypeF_'+Tno].style.backgroundColor="#7f1d1a";
									MM_showHideLayers('bnrTypeF_1','','hide','bnrTypeF_2','','hide','bnrTypeF_3','','hide','bnrTypeF_4','','hide','bnrTypeF_5','','hide','bnrTypeF_6','','hide','bnrTypeF_7','','hide');
									MM_showHideLayers('bnrTypeF_'+Tno,'','show');
									nowCount_TypeF = Tno;
							}
						}						
					}										
			
					main_rolling_TypeF();
					function main_rolling_TypeF(){
						if( r_status == 'Play' ){
							if(nowCount_TypeF==0){select_TypeF('0');}
							if(nowCount_TypeF==1){select_TypeF('1');}
							if(nowCount_TypeF==2){select_TypeF('2');}

							nowCount_TypeF++;
							if(count_TypeF < nowCount_TypeF){nowCount_TypeF = 0;}
						}
						setTimeout("main_rolling_TypeF()",r_delay);
					}
				</script>
