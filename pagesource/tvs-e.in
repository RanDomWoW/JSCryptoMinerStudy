    <script>
        function TrackingNo() {
            $('#error_message_id').hide();
            var TrackingNumer = $('#txtTrackingno').val();
            if (TrackingNumer == '') {
                alert('Please enter your Tracking No');
                $('#txtTrackingno').focus();
                return false;
            }
            var hummus = { 'TrackingNo': TrackingNumer };
            $.ajax({
                type: "POST",
                url: 'api-test.aspx?trackingno='+TrackingNumer,
                success: function (response) {
                    //alert(response);
                    if (response == "No")
                    {
                        $('#form_track').show();
                        $('#error_message_id').show();
                    }
                    else
                    {
                        //var responsereulst=
                        $('#form_track').hide();
                        $('#error_message_id').hide();
                        $('#myDiv').show();
                        //$('#myDiv').append(response);
                        $('#myDiv').html(response);
                    }
                    
                    
                }
            });
            return;

        }
        
        
    </script>
    
