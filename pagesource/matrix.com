                if (scrollHeight > contentHeight) {
                    jQuery(".brand-left-menu").addClass("banner-below")
                } else {
                    jQuery(".brand-left-menu").removeClass("banner-below")
                }
            });

            //GTM Implementation
            jQuery(".home-banner-slider a").on('click', function (e) {
                GTMTagging.GTMVar.linkName = "";
                //GTMTagging.GTMVar.contentName = "contentName";
                var carouselData = $(".carousel").data('bs.carousel');
                if (carouselData.$element.find('.item.active') != null) {
                    var currIndex = carouselData.getItemIndex(carouselData.$element.find('.item.active'));
                    GTMTagging.GTMVar.contentName = jQuery("#" + currIndex).val();
                    //alert(" postion" + jQuery("#" + currIndex).val());
                    GTMTagging.GTMVar.contentPosition = ++currIndex;
                    GTMTagging.carousel_Click();
                }
            });

            var taggedBanners = [];
            jQuery('#carousel-example-generic').bind('slide.bs.carousel', function (e) {
                var triggerTagging = true;
                var carouselData = $(".carousel").data('bs.carousel');
                var currIndex = carouselData.getItemIndex(carouselData.$element.find('.item.active'));
                //alert("position : " + currIndex);
                for (var i = 0; i != taggedBanners.length; i++) {
                    if (taggedBanners[i] == currIndex) {
                        triggerTagging = false;
                        break;
                    }
                }
                if (triggerTagging) {
                    GTMTagging.GTMVar.contentName = jQuery("#" + currIndex).val();
                    GTMTagging.GTMVar.contentPosition = currIndex + 1;
                    GTMTagging.carousel_View();
