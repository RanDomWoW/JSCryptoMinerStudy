<script type="text/javascript" src="http://hatubai.net/css/recently_add.js"></script>
<script>

/* []をHTML出力 */
recentry_add = array_shuffle(recentry_add);
var Output_recentry_add = "";
var cnt = 1;
for(var i in recentry_add){
    var tmp = "";
    var img = "<img src='http://ecx.images-amazon.com/images/P/"+recentry_add[i]['asin']+".09._SCTZZZZZZZ_CR0,20,70,90_.jpg' height='90px' align='left' />";
    tmp = tmp + '<a href="http://hatubai.net/item/'+recentry_add[i]['id']+'">'+img+recentry_add[i]['title']+'</a>';
    tmp = tmp + "<span class='release'>" + recentry_add[i]['release'] + "&nbsp;発売予定</span>";
    //tmp = tmp + "<span class='update'>" + recentry_add[i]['update'] + "&nbsp;追加!!</span>";
    Output_recentry_add = Output_recentry_add + "<li>" + tmp + "</li>\n";
    cnt++;
}

$(function(){

    $("#recently-item ul.list").html(Output_recentry_add);

    //[最近追加されたアイテム] 自動表示するコ－ド
    var setElm = $('.ticker');
    var effectSpeed = 1000;
    var switchDelay = 6000;
    var easing = 'swing';

    //非表示を解除
    $("#recently-item ul").css("display","block");

    setElm.each(function(){
        var effectFilter = $(this).attr('rel'); // 'fade' or 'roll' or 'slide'

        var targetObj = $(this);
        var targetUl = targetObj.children('ul');
        var targetLi = targetObj.find('li');
        var setList = targetObj.find('li:first');

        var ulWidth = targetUl.width();
        var listHeight = targetLi.height();
        targetObj.css({height:(listHeight)});
        targetLi.css({top:'0',left:'0',position:'absolute'});

        if(effectFilter == 'fade') {
            setList.css({display:'block',opacity:'0',zIndex:'98'}).stop().animate({opacity:'1'},effectSpeed,easing).addClass('showlist');
            function timerFade(){
                setTimerFade = setInterval(function(){
                    var activeShow = targetObj.find('.showlist');
                    activeShow.animate({opacity:'0'},effectSpeed,easing,function(){
                        $(this).next().css({display:'block',opacity:'0',zIndex:'99'}).animate({opacity:'1'},effectSpeed,easing).addClass('showlist').end().appendTo(targetUl).css({display:'none',zIndex:'98'}).removeClass('showlist');
                    });
                },switchDelay);
            }
            timerFade();
            targetObj.hover(function(){
                clearInterval(setTimerFade);
            },function(){
                timerFade();
            });
        } else if(effectFilter == 'roll') {
            setList.css({top:'3em',display:'block',opacity:'0',zIndex:'98'}).stop().animate({top:'0',opacity:'1'},effectSpeed,easing).addClass('showlist');
            function timerRoll(){
                setTimerRoll = setInterval(function(){
                    var activeShow = targetObj.find('.showlist');
                    activeShow.animate({top:'-3em',opacity:'0'},effectSpeed,easing).next().css({top:'3em',display:'block',opacity:'0',zIndex:'99'}).animate({top:'0',opacity:'1'},effectSpeed,easing).addClass('showlist').end().appendTo().css({zIndex:'98'}).removeClass('showlist');
                },switchDelay);
            }
            timerRoll();
            targetObj.hover(function(){
                clearInterval(setTimerRoll);
            },function(){
                timerRoll();
            });
        } else if(effectFilter == 'slide') {
            setList.css({left:(ulWidth),display:'block',opacity:'0',zIndex:'98'}).stop().animate({left:'0',opacity:'1'},effectSpeed,easing).addClass('showlist');
            function timerSlide(){
                setTimerSlide = setInterval(function(){
                    var activeShow = targetObj.find('.showlist');
                    activeShow.animate({left:(-(ulWidth)),opacity:'0'},effectSpeed,easing).next().css({left:(ulWidth),display:'block',opacity:'0',zIndex:'99'}).animate({left:'0',opacity:'1'},effectSpeed,easing).addClass('showlist').end().appendTo(targetUl).css({zIndex:'98'}).removeClass('showlist');
                },switchDelay);
            }
            timerSlide();
            targetObj.hover(function(){
                clearInterval(setTimerSlide);
            },function(){
                timerSlide();
            });
        }
    });
    
    $("form input[name=pub-select]:radio").change(function(){
        changePubSelect();
    });
    
    if(getObj['k']){
        var dom = $("#sidebar-search input[type=text][name=k]");
        if(getObj['k'].length>0){
            dom.val(getObj['k']);
        }
    }
    
    if(getObj['g']){
        var dom = $("#sidebar-search input[type=radio][name=g][value="+getObj['g']+"]");
        if(dom.length>0){
            dom.prop("checked",true);
        }
    }
    
    if(getObj['y']){
        var dom = $("#sidebar-search select.d-select[name=y]");
        dom.val(getObj['y']);
    }
    if(getObj['m']){
        var dom = $("#sidebar-search select.d-select[name=m]");
        dom.val(getObj['m']);
    }
    
    if(getObj['pub-select']){
        var dom = $("#sidebar-search input[name=pub-select][value="+getObj['pub-select']+"]");
        if(dom.length>0){
            dom.prop("checked",true);
            changePubSelect();
            if(getObj['pub']){
                var dom = $("#sidebar-search select#"+getObj['pub-select']);
                dom.val(getObj['pub']);
            }
        }
    }
});

function changePubSelect(){
    var d = $("#sidebar-search input[name=pub-select]:checked").val();
    if(d == 'pub-book'){
        $("#sidebar-search #pub-book").css("display","block");
        $("#sidebar-search #pub-game").css("display","none");
        $("#sidebar-search #pub-game").val("");
    }else if(d == 'pub-game'){
        $("#sidebar-search #pub-book").css("display","none");
        $("#sidebar-search #pub-game").css("display","block");
        $("#sidebar-search #pub-book").val("");
    }
}
</script>




