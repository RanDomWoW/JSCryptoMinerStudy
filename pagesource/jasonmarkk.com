<script>

    jQuery(document).ready(function($){

        /* DEFINE */
        var AddToCart = function() {

            this.$button = $(".atc-button");

            this.url      = "/shop/checkout/cart/add/";
            this.form_key = "cZdeL6SyhuWB1akS";

            return this;
        };


        /* METHODS */
        AddToCart.prototype.add = function(e){
            e.preventDefault();
            var $this = ( typeof e === "undefined" ) ? this : window.add_to_cart;
            
            var id = $(e.target).attr("data-id");

            var qty = 1;

            $this.url += "product/" + id + "/";
            $this.url += "qty/" + qty + "/";
            $this.url += "form_key/" + $this.form_key + "/";

            $.ajax({
                url: $this.url,
                cache: false,
                beforeSend: function(){
                    // add preloader
                    $(e.target).html('Added To Cart');
                    // show cross sell products
                    // $('.js-pdp__related').addClass('active');
                },
                success: function(){
                    // remove preloader
                    // update cart icon in header
                    var cart_count = parseInt($('.js-cart__count').text());
                    var count = 1;
                    $(e.target).html('Add to cart');
                    if (isNaN(cart_count)) {
                        $('.js-cart__count').text(count);
                    } else {
                        $('.js-cart__count').text(cart_count + 1);
                    }
                }
            });
        };

        /* INITIALIZE */
        window.add_to_cart = new AddToCart();


        /* EVENTS */
        $(document).on("click", ".atc-button", window.add_to_cart.add);

    });

</script>


