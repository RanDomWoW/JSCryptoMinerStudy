    line-height: 1.1!important;
    letter-spacing: 6px;
    color: #000;
    text-transform: uppercase!important;
}

.td-page-content h2 {
    font-size: 40px;
    font-weight: 700;
    letter-spacing: 4px;
}

.td-page-content h3 {
    font-size: 30px;
    font-weight: 500;
    letter-spacing: 2px;
}

.td-page-content h4 {
    font-size: 25px;
