    overflow: hidden;
}

.yousupport .heading {
    text-align: center;
    font-size: 14px;
    color: #333;
    font-weight: 300;
    margin-bottom: 15px;
}

.yousupport .content .right {
    font-size: 13px;
    color: #333;
    text-align: left;
    font-weight: 300;
    padding-left: 5px;
    padding-right: 5px;
}


.yousupport .content .left {
    font-size: 14px;
    padding-top: 5px;
    padding-left: 5px;
    padding-right: 5px;
}

.yousupport .content {
    text-align: center;
    font-size: 13px;
    color: #333;
    font-weight: 300;
    margin-bottom: 15px;
}

.sidebartext h3 {
    font-size: 17px;
    text-align: left;
    font-weight: 400;
    border-left: 4px solid #ec282c;
    padding-left: 15px;
    overflow: hidden;
}

.sidebartext h4 {
    font-size: 15px;
    font-weight: 400;
    margin-top: 5px;
}

.sidebartext img {
    margin-bottom: 10px;
}

@media screen and (max-width: 480px) {
    .ourpick.nodesc {
        width: 90%;
        margin: 10px auto;
        text-align: center;
    }
    
    .ourpick.nodesc.left {
        float: unset;
    }

    .ourpick.nodesc.right {
        float: unset;
    }
    
}

/* Resource pages */

.group, .topcontent {
    padding-left:0!important;
    padding-right: 0!important;
}

.topcontent {
    padding-top: 30px;
}

.grouph2 {
    border-left: 4px solid #ec282c;
    padding-left: 10px;
}

.subgroup {
    padding-left: 0!important;
    padding-right: 15px!important;;
}

.subimg {
    padding: 10px!important;
    text-align: center;
    border-left: 3px solid #ec282c;
    border-right: 3px solid #ec282c;
    border-top: 3px solid #ec282c;
}

.subimg img {
    margin: 0 auto;
    width: auto;
    max-height: 120px;
    height: 120px;
    border-radius: 10px;
}

.subcontent{
    padding: 10px!important;
}

.subimg:hover img {
    opacity: .8;
}

.subhead {
    text-align: left;
    background: #eee;
    border-right: 3px solid #ec282c;
    border-left: 3px solid #ec282c;
    border-bottom: 3px solid #ec282c;
}

.subhead.noimg {
    border-top: 3px solid #ec282c;
}

.subhead h2 {
    margin-top: 10px;
    margin-bottom: 10px;
    padding-left: 10px;
}

.subhead a {
    text-decoration: none;
}

.subhead a:hover {
    text-decoration: none;
    color: #ec282c;
}

.sharing {
    position: absolute;
    left: 10px;
    bottom: 10px;
    opacity: .5;
    float: left;
    width: 100%;
    transition: 0.4s all ease;
}

.crumbtitle {
    position: absolute;
    right: 0;
    top: 10px;
    padding: 15px;
    background: rgba(0,0,0,0.7);
    color: #fff;
    text-align: right;
    transition: 0.4s all ease;
}

.restitle {
    margin-bottom: 0;
    margin-top:0;
    color: #fff!important;
}

.page-template-page-resources .imgshare .image {
    max-height: 400px;
    overflow: hidden;
}

/* find your accessory page template */

.findaccessory .td-page-header h1 {
    text-align: center;
}

.findaccessory .td-page-header p {
    text-align: center;
}

@media screen and (max-width: 600px) { 
    .td-footer-bottom-full .footer-logo-wrap {
        margin-top: 10px;
        margin-bottom: 10px;
        width: 100%;
        text-align: center;
        float: unset;
    }
    
}

/* multi product review template */
.cproduct {
    margin-bottom: 0px;
    position: relative;
}

.cproduct_first {
    top: 0;
    left: 0;
    color: #fff;
    z-index: 9999;
    border-bottom: 2px solid #444;
    overflow: hidden;
}

.cproduct_first h3 {
    background: #444;
    color: #fff!important;
    line-height: 1!important;
    padding: 0;
    font-weight: 700;
    margin: 0;
    padding: 10px;
    display: inline;
}

.cproduct_first h2 {
    background: #ec282c;
    color: #fff;
    padding: 0;
    font-weight: 500;
    margin: 0;
    padding: 10px;
    display: inline;
}

.cproduct_first h5 {
    color: #fff;
    padding: 0;
    margin: 0;
    margin-bottom: 5px;
}


.cproduct_first a:hover {
    text-decoration: none;
}

.cproduct .cproduct_image img {
    max-width: 300px;
    height: auto;
}

.cproduct .cproduct_image {
    padding-top: 15px;
}

.ctitle {
    font-weight: bold;
    border-bottom: 3px solid #ec282c;
}

.cproduct .cblock {
    padding-left: 15px!important;
    padding-right: 15px!important;
}

.cproduct_sixth {
    text-align: center!important;
    float: unset!important;
    overflow: hidden;
    display: block!important;
}

.bestprice {
    background: #ec282c;
    color: #fff;
    padding: 10px;
    text-align: center;
    display: block;
    max-width: 280px;
    font-family: Roboto;
    font-size: 19px!important;
    font-weight: 700;
    margin: 0 auto;
    margin-bottom: 35px;
    transition: 0.4s ease background;
}

.bestprice:hover {
    background: #eee;
    color: #ec282c;
    text-decoration: none!important;
    transition: 0.4s ease background;
}

.cproduct .cblock.single {
    padding-left: 15px!important;
    padding-right: 15px!important;
}

.cproduct_image img {
    width: 250px;
    height: auto;
}

.cproduct_features {
    max-width: 220px;
}

.powered-by-sumo {
    display: none;
}

.td-page-content h1, .td-page-content h2, .td-page-content h3, .td-page-content h4, .td-page-content h5, .td-page-content h6 {
    font-family: 'Eczar', serif!important;
