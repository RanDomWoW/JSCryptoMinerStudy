}

function show_domainr_loading()
{
	$('#buy_domainr_loading').show();
}
function hide_domainr_loading()
{
	$('#buy_domainr_loading').hide();
}
function create_domainr_wishlist_json()
{
	var reg_dmns = $('#register_domainr_wislist').val();
	var register_array = reg_dmns.split(',');
	
	var transfer_dmns = $('#transfer_domainr_wislist').val();
	var transfer_array = transfer_dmns.split(',');
	
	$(".wishist_heart.active").each(function () {		
		var domain = $(this).attr('rel');
		var type = $(this).attr('rel2');
		if(type=='Register')
		{
			register_array.push(domain);
		}
		if(type=='Transfer')
		{
			transfer_array.push(domain);
		}
		});
		var regs = register_array.join();
		var trans = transfer_array.join();
	$('#register_domainr_wislist').val(regs);
	$('#transfer_domainr_wislist').val(trans);
}
function remove_domain_from_list(domain)
{
	var reg_dmns = $('#register_domainr_wislist').val();
	reg_dmns_str = reg_dmns.replace(domain, '');
	$('#register_domainr_wislist').val(reg_dmns_str);
	
	var transfer_dmns = $('#transfer_domainr_wislist').val();
	transfer_dmns_str = transfer_dmns.replace(domain, '');
	$('#transfer_domainr_wislist').val(transfer_dmns_str);
}
