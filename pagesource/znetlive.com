				else
				{
					$('#doaminr_search_response').html(response);
					$('#doaminr_search_response').slideDown();				
				}
			}
		});
		}
}
function add_doaminr_in_cart(domain,i)
{
	show_domainr_loading();
	$.ajax({
				type: "POST",
				url: 'https://www.znetlive.com/domainr_actions.php',
				dataType: "json",
				data: 'action=adddomain&domain='+domain,			
