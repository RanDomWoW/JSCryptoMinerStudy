{
	$('#domainr_search_domain').val('');
	$('#register_domainr_wislist').val('');
	$('#transfer_domainr_wislist').val('');
	$('#doaminr_search_response').hide();
	$('#doaminr_search_response').html('');
}
function domainr_billing_cycle_update(cycle,id)
{
	var previous_text = $('.price_loading_'+id).html();
	$('.price_loading_'+id).html('<img src="https://www.znetlive.com/img/loading.gif">');
	$.ajax( {
		type: "POST",
		url: 'https://www.znetlive.com/domainr_actions.php', 
		data: 'action=domainr_billing_cycle_update&cycle='+cycle+'&domainid='+id,
		dataType: "json",
		success: function( response )
		{
			if(response.status=='Success')
			{
				$('.price_loading_'+id).removeClass("right-a");
				$('.price_loading_'+id).removeClass("offer-r");
				$('.price_loading_'+id).addClass(response.pricebox_class);
				$('.price_loading_'+id).html(response.pricebox);
			}
			else
			{
				alert(response.message);
				$('.price_loading_'+id).html(previous_text);
			}						
		}
	});
}
