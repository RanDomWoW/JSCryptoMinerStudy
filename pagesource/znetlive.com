{
	$('#domainr_ajax_content').html('<div style="text-align: center; margin: 50px;">Please wait<br><img src="https://www.znetlive.com/img/loading.gif"></div>');
	var reg_domains = $('#register_domainr_wislist').val();
	var trans_domains = $('#transfer_domainr_wislist').val();
	$.ajax( {
		type: "POST",
		url: 'https://www.znetlive.com/domainr_actions.php', 
		data: 'action=show_data_in_pop&reg_doamins='+reg_domains+'&trans_doamin='+trans_domains,
		success: function( response )
		{
			$('#domainr_ajax_content').html(response);				
		}
	});
}
