function check_validate_exit()
{
	var test_submit='';
	var oemail = $('#email_optin').val();
	var onumber = $('#num_optin').val();
	var oname = $('#name_optin').val();
	
	//var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
	var reg = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
	var alpha = /^[a-zA-Z ]*$/;
	
	var nmber = /^[0-9]+$/;
	
	olen=onumber.length;
				
	//otest=isNaN(onumber);
	
	if(oemail=='' || onumber=='' || oname=='')
	{
		test_submit='All fields are required.';
		
	}
	else 
	{
		
		if(alpha.test(oname) == false)
		{
			
			test_submit='Name field only accepts alphabets values.';
		}
		else if(reg.test(oemail) == false)
		{
			test_submit='Please enter a valid email ID.';
		}
		else
		{
			if(nmber.test(onumber) == false)
			{
				test_submit='Number field only accepts numeric values.';
			}
			else if(olen>20)
			{
				test_submit='Your Number could not exceed 20 digits.';
				
					
			}
			
		}
		
		
	}
	
	
	
	if(test_submit=='')
	{
		$('#cart_exit_optimonster').submit();
	}
	else
	{
		alert(test_submit);
	}
}

