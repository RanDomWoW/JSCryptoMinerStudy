		else{
		$.ajax( {
			type: "POST",
			url: 'https://www.znetlive.com/doaminr_result.php', 
			data: 'domain='+searchkey,
			success: function( response )
			{
				$('#go_button_loading').html('<input class="search_go" type="button" value="GO" onclick="domain_search_domainr_function();" />');
