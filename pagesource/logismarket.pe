<script type="text/template" id=products>
        <div class="row">
            <div class="col-md-12">
                <!-- Wrapper for slides -->
                <div class="carousel-inner" role="listbox">
                    <%  var totalProducts = _.size(products); 
            var productsPerDiv = 4;
            var totalDivs = totalProducts/productsPerDiv;
            var divCounter = 0;
            while(divCounter < totalDivs){ %>
                        <div class="item <% if(divCounter == 0){%> active <% } %>">
                            <ul class="productos clean-list row">
                                <% var sublist = products.slice(divCounter*productsPerDiv, (divCounter+1)*productsPerDiv);
                  _.each( sublist, function( item ){ %>
                                    <li class="col-sm-3">
                                        <a href="<%=item.url%>" class="bold text-center" data-track-event-category="Test AB" data-track-event-label="Nuevos-productos-<%=item.name%>">
                                <img src="<%=item.urlImage%>" alt="" />
                                <br/><%=item.name%>
                                <br/>
                                <img src="<%=item.companyUrlImage%>" alt="">
                            </a>
                                    </li>
                                    <% }); %>
                            </ul>
                        </div>
                        <% divCounter++; } %>
                </div>
            </div>
        </div>
        <div class="row relative">
            <!-- Indicators -->
            <ol class="carousel-indicators">
                <% divCounter = 0;
            while(divCounter < totalDivs){ %>
                    <li data-target="#carousel-example-generic" data-slide-to="<%=divCounter%>" <% if(divCounter==0 ){%>class="active"
                        <% } %> data-track-event-category="Test AB" data-track-event-label="Nuevos-productos-next"></li>
                    <% divCounter++; } %>
            </ol>
        </div>
    </script>
</div>
