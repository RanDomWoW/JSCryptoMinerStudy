<script>
    window.addEventListener('load', function () {
        var home = window.location.pathname == '/';
        var product = jQuery('#product').length > 0;
        var cartf = window.location.pathname.indexOf('/cart/order') != -1;
        var carts = window.location.pathname.indexOf('/dutyfree/checkout') != -1
        var category = jQuery('.view-product-list').length > 0;
        var id = '';
        var price = '';
        var pagetype = '';
        if (home) {
            var pagetype = "home"
        }
        else if (category) {
            var pagetype = "category"
        }
        else if (product) {
            var id = jQuery('.modal-no').text().split(":")[1].trim("");
            var pagetype = "product"
            if (jQuery('.price:contains("USD")').is(":visible")) {
                var price = parseFloat(jQuery('.price').text().replace(/[^0-9,.]/g, ''))
            }
            else {
                var price = parseFloat(jQuery('.product-price-group .price').text().split("Rs.")[1].replace(/[^0-9.]/g, ''))
            }
        }
        else if (cartf) {
            var id = new Array();
            var len = jQuery('.cart-disc label:odd').length;
            for (i = 0; i < len; i++) {
                id.push(jQuery('.cart-disc label:odd')[i].innerText.split(":")[1].trim(""))
            }
            var pagetype = "cart"
            var price = parseFloat(jQuery('#total').text().replace(/[^0-9.]/g, ''));
        }
        else if (carts) {
            var id = new Array();
            var len = jQuery('.cart-disc tbody').length;
            for (i = 0; i < len; i++) {
                id.push(jQuery('.cart-disc tbody')[i].innerText.split(":")[1].split("Item")[0].trim(""))
            }
            var pagetype = "cart"
            var price = parseFloat(jQuery('#full_total').text().replace(/[^0-9.]/g, ''));
        }
        window.google_trackConversion({
            google_conversion_id: 852338964,
            google_custom_params: {
                ecomm_prodid: id,
                ecomm_pagetype: pagetype,
                ecomm_totalvalue: price
            },
            google_remarketing_only: true
        });
    });
</script>
