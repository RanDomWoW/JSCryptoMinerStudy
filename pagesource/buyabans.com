    $('.carousel').carousel({
        interval: 3000,
        cycle: true
    });

    $('#scale-down').on('click', function (e) {
        e.preventDefault();
        $('.scale').removeClass('scale-up').removeClass('scale-reset').addClass('scale-down');
    });

    $('#scale-reset').on('click', function (e) {
        e.preventDefault();
        $('.scale').removeClass('scale-up').removeClass('scale-down');
    });

    $('#scale-up').on('click', function (e) {
        e.preventDefault();
        $('.scale').removeClass('scale-reset').removeClass('scale-down').addClass('scale-up');
    });

    $('#subscribe').on('click', function (e) {
        e.preventDefault();

        if (!$('#email').val().trim()) {
            sweetAlert("Oops...", "Please provide your email!", "error");
        } else {
            $.ajax({
                url: 'https://buyabans.com/subscribe',
                dataType: 'text',
                method: 'POST',
                data: {'email': $('#email').val(), '_token': $('#_token').val()},
                success: function (data) {
                    if (data === 'true')
                        swal("Good job!", "You have subscribe with buyabans", "success");
                    else
                        sweetAlert("Oops...", "Something went wrong!", "error");
                },
                error: function (err) {
                    sweetAlert("Oops...", "Something went wrong!", "error");
                }
            });
        }
    });

