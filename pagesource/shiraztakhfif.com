        function closeMenu() {
            setTimeout(function () {
                document.getElementById("menu").style.transform = "translate(100%, 0)";
                document.getElementById("fix-back").style.opacity = "0";
                $("#menu-toggle").prop('checked', false);
                $('body').removeClass('no-scroll');
            }, 50);
            setTimeout(function () {
                $('.burger-menu-right').addClass('hidden')
            }, 900)
        }

        $('.toggle-search-js').click(function () {
            $('.toggle-hidden').addClass('active');
            $('.toggle-hidden').removeClass('hidden');
            $('.searchPhraseMobile').focus();

        })

        $('.searchPhraseMobile').keyup(function () {
            if ($(this).val() !== '') {
                $('#closeSearchResult').removeClass('hidden');
            } else {
                $('#closeSearchResult').addClass('hidden');
            }
        })

        $('#closeSearchResult').click(function () {
            $('.searchPhraseMobile').val(null);
            $('.searchPhrase').val(null);
            $('.toggle-hidden').removeClass('active');
            $('.toggle-hidden').addClass('hidden');
        })

        $.ajaxSetup({headers: {'X-CSRF-TOKEN': $('meta[name="_token"]').attr('content')}, cache: false});
        $(function () {
            // site search
            $('.searchForm').submit(function () {
                var phrase = $('.searchPhrase').val();
                if (phrase) {
                    if (phrase.length < 3) {
                        $(this).find('.input-group').addClass('has-error');
                        return false;
                    }
                    $('.flickity-enabled').hide();
                    $( "#container" ).html("<center><img src='/assets/plugins/colorbox/example1/images/loading.gif' /></center>")
                    $( "#container" ).load( "https://shiraztakhfif.com/search/"+phrase+" #container");
                } else {
                    $('#closeSearchResult').trigger('click');
                }
                return false;
            });


            $('.searchMobileForm').submit(function () {
                var phrase = $('.searchPhraseMobile').val();
                if (phrase) {
                    if (phrase.length < 3) {
                        $(this).find('.input-group').addClass('has-error');
                        return false;
                    }
                    window.location.href = "https://shiraztakhfif.com/search" + '/' + phrase;
                } else {
                    $('#closeSearchResult').trigger('click');
                }
                return false;
            });


            $('.prevent-to-submit-again').click(function () {
                $this = $(this);
                $this.attr('disabled', 'disabled');
                if ($this.is('input[type=submit]')) {
                    $this.data('before-submit-text', $this.value);
                    $this.val('در حال ارسال ...');
                } else {
                    $this.data('before-submit-text', $this.text());
                    $this.html('در حال ارسال ...');

                }
                $.ajaxSetup({
                    complete: function (jqXHR, textStatus) {
                        if ($this.is('input[type=submit]'))
                            $this.val($this.data('before-submit-text'));
                        else
                            $this.html($this.data('before-submit-text'));
                        $this.removeAttr('disabled');
                    }
                });
                $this.closest('form').submit();
            });
            // sticky menu
            $("#sticky-menu").sticky({topSpacing: 0});
            var myLazyLoad = new LazyLoad();

        })
