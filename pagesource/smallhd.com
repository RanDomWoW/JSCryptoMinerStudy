                for(var x=1; x <= data.items.length; x++) 
                { 
                    var html = '<div class="media"><div class="media-left"><a href="'+data.items[x-1].url+'"><img src="'+data.items[x-1].thumbnail+'" id="product_thumbnail_' + x + '"></a></div><div class="media-body"><div class="pull-left"><h4 title="'+data.items[x-1].title+'"><a href="'+data.items[x-1].url+'" id="product_title_' + x + '">'+data.items[x-1].title+'</a></h4><p id="product_part_id_' + x + '">Part# '+data.items[x-1].itemid+'</p></div><div class="pull-right"><p><span id="product_price_' + x + '">$'+data.items[x-1].rate+'</span>  x  <span id="product_quantity_' + x + '">'+data.items[x-1].quantity+'</span></p></div></div></div>';                 
 
                    $('#cart-dropdown .center').append(html); 		    	
                } ; 
		$('.cssload-container').hide();
		$('.btn-lg').hide();
		$('#cart-text').show();
            }else{ /* Condition 2 (User logged in to the store and no item added in the cart)*/
		$('.cssload-container').hide();
		$('.btn-lg').hide();
		$('#cart-text').show();
                $('#cart-dropdown .center').append('<div class="media"><div class="media-body"><div class="pull-left"><h4>Your Cart is empty!</h4></div></div></div>'); 
            } 
	 }else{ 
		if(data.items.length>0){ /* Condition 4 (User not logged in to the store and items added in cart)*/
                for(var x=1; x <= data.items.length; x++) 
                { 
                    var html = '<div class="media"><div class="media-left"><a href="'+data.items[x-1].url+'"><img src="'+data.items[x-1].thumbnail+'" id="product_thumbnail_' + x + '"></a></div><div class="media-body"><div class="pull-left"><h4 title="'+data.items[x-1].title+'"><a href="'+data.items[x-1].url+'" id="product_title_' + x + '">'+data.items[x-1].title+'</a></h4><p id="product_part_id_' + x + '">Part# '+data.items[x-1].itemid+'</p></div><div class="pull-right"><p><span id="product_price_' + x + '">$'+data.items[x-1].rate+'</span>  x  <span id="product_quantity_' + x + '">'+data.items[x-1].quantity+'</span></p></div></div></div>';                 
 
                    $('#cart-dropdown .center').append(html); 		    	
                } 
		$('#btnID').show(); 
		$('.cssload-container').hide();
		$('.btn-lg').hide();
		$('#cart-text').show();
            }else{ /* Condition 3 (User not logged in to store and no item added in cart)*/
		$('#btnID').hide(); 
		$('#cart-text').hide();
		$('.cssload-container').hide();
		$('.btn-lg').show();
	   }		
	 }	
            console.log('https://checkout.na1.netsuite.com:  received response:  ',event.data); 
        });  
    });
</script>


