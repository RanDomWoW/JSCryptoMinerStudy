      <script type="text/javascript">
      $.get('http://ems.stvincent.edu/MasterCalendar/RSSFeeds.aspx?data=mL1noRCrG1I%2fLTuo7OuVK5qkln9Q1doT ', function (data) {
        var $container = $('#feed_bbbfbd3a-cb69-6798-9f18-ff0000f502eb');
        var limit = 3 || 100;
        var link, el, markup, title, day_sp, day_cc, day_dd, name, location, date, day, month, time, $newItem;
        
        var months = ['Jan','Feb','Mar','Apr','May','Jun','Jul','Aug','Sep','Oct','Nov','Dec'];
        
  	var events = $(data).find("item").length;
        if (events > 0) {
          var ec = 0;
          $(data).find("item").each(function () {
            ++ec;
            el = $(this);
            
            if (el.find("title").length > 0) {
              markup = '';
              link = el.find("link").text();
              title = el.find("title").text();
              day_sp = title.split(' - ');
              if (day_sp.length > 2) {
                day_cc = day_sp[1];
              }
              else {
                day_cc = day_sp[0];
              }
              day_dd = day_cc.substr(day_cc.lastIndexOf(' '));
              name = title.substr(0, title.indexOf(' - ')).replace(day_dd,'');
              location = title.substr(title.indexOf(' - ')+3).replace(day_dd + ' -','');
              time = el.find("description").text();
              if (time.indexOf('M:') != -1) {
              	time = time.substr(0, time.indexOf('M:') + 1);
              }
              
              time = time.replace(/AM/g, 'a.m').replace(/PM/g, 'p.m.');
              date = new Date(Date.parse(day_dd));
              day = date.getDate();
              month = months[date.getMonth()];
          
              //console.log("------------------------", el);
              $newItem = $('<li class="mdl-list__item mdl-list__item--two-line"></li>');
  
              markup = '<a href="' + link + '" target="_blank">';
              markup += '<span class="mdl-list__item-primary-content">';
              markup += '<span class="date mdl-list__item-avatar">';
              markup += '<span class="month">' + month + '</span><span class="day">' + day + '</span>';
              markup += '</span>';
              markup += '<span>' + name + '</span>';
              markup += '<span class="mdl-list__item-sub-title">' + time + ', ' + location + '</span>';
              markup += '</span>';
              markup += '</a>';
              
              $newItem.append(markup);
              $container.append($newItem);
            } else if (items == 1) {
              $('#feed_bbbfbd3a-cb69-6798-9f18-ff0000f502eb').append('<p>There are upcoming events.</p>');          
            }
            if (ec == limit) {
            	return false;
            }
          });
      	} else {
          $('#feed_bbbfbd3a-cb69-6798-9f18-ff0000f502eb').append('<p>There are no upcoming events.</p>');
        }
      });
      </script>
    
	</div>
