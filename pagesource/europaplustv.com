<script type="text/javascript">
	$(document).ready(function() {
		var news_page_not_tele = 1;
		var news_count_not_tele = 14684;
		var count_pages_not_tele = 10;

		$('.in-news[mode=not_tele] .paginator_center div').click(function(){
			var _t = $(this);
			var news_category = _t.parent().attr('news_category');

			var loaded_page = news_page_not_tele;
			if(_t.hasClass('paginator_left')) {
				if(news_page_not_tele<=1) {
					news_page_not_tele = count_pages_not_tele;
				}
				else {
					news_page_not_tele--;
				}

				if(news_page_not_tele<=1) {
					loaded_page = count_pages_not_tele;
				}
				else {
					loaded_page = news_page_not_tele - 1;
				}
			}
			else if(_t.hasClass('paginator_right')) {
				if(news_page_not_tele<count_pages_not_tele) {
					news_page_not_tele++;
				}
				else {
					news_page_not_tele = 1;
				}

				if(news_page_not_tele<count_pages_not_tele) {
					loaded_page = news_page_not_tele + 1;
				}
				else {
					loaded_page = 1;
				}
			}
			else {
				return;
			}

			// Проверка есть ли уже следующий блок или его нужно грузить
			$('.hb-content.category_'+news_category+' .block[page="'+loaded_page+'"] img').each(function(){
				if($(this).attr('realsrc')) {
					$(this).attr('src', $(this).attr('realsrc')).removeAttr('realsrc');
				}
			});

			// Включаем нужный блок
			$('.hb-content.category_'+news_category+' .block').hide();
			$('.hb-content.category_'+news_category+' .block[page="'+news_page_not_tele+'"]').show();
		});
	});
</script>
