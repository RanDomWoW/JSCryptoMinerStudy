    <script type="text/javascript">
    $(function () {
        $("#setaEsquerda").click(IrParaAnterior);
        $("#setaDireita").click(IrParaProximo);

        totalSegmentos = $("#containerSuplemento li").length;

        if (totalSegmentos < 5) {
            $("#setaEsquerda").css("display", "none");
            $("#setaDireita").css("display", "none");
        }
    })

    var totalSegmentos;

    var segmentoAtual = 0;

    function IrParaAnterior() {

        if (segmentoAtual != 0) {
            segmentoAtual--;

            IrPara();
        }


    }

    function IrParaProximo() {
        if (segmentoAtual < totalSegmentos - 4) {
            segmentoAtual++;

            IrPara();
        }

    }

    function IrPara() {
        var tamanhoPrimeiro = NumberTransform($("#containerSuplemento li").css("width")) + NumberTransform($("#containerSuplemento li").css("marginRight"));

        var calcPosX = -(tamanhoPrimeiro * segmentoAtual);

        var posX = calcPosX.toString() + "px";

        $("#containerSuplemento").animate({ marginLeft: posX });
    }

    function NumberTransform(str) {
        return Number(str.replace("px", ""));
    }

</script>
