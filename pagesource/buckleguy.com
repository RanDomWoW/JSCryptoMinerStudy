                <script>
                    $(document).ready(function(){
                        $('.featuredCategoryBox li').filter(function(){return $(this).find('.fcimg').children().length > 1}).hover(function(){
                            startFadeImg($(this).find('.fcimg'));
                        },function(){
                            stopFadeImg($(this).find('.fcimg'));
                        });
                        $('.featuredCategoryBox li').click(function(){
                            var url = $(this).find('.viewCatButton a').attr("href");
                            window.location = url;
                        });
                    });

                    var fadeInterval;

                    function startFadeImg(currentBox)
                    {
                        stopFadeImg(currentBox);
                        currentBox.find("img:first-child").addClass("current");
                        fadeInterval = window.setInterval(function(){
                            fadeNextImg(currentBox);
                        },1500);
                        fadeNextImg(currentBox);
                    }

                    function fadeNextImg(currentBox)
                    {
                        var currentImg = currentBox.find('.current').removeClass("current");
                        var nextImg = currentImg.next();

                        if(currentImg.is(":last-child"))
                            nextImg = currentBox.find("img:first-child");

                        nextImg.addClass('current');

                        currentImg.fadeOut(600);
                        nextImg.fadeIn(600);
                    }

                    function stopFadeImg(currentBox)
                    {
                        window.clearInterval(fadeInterval);

                        var currentImg = currentBox.find('.current').removeClass("current");
                        var defaultImg = currentBox.find("img:first-child").addClass("current");

                        currentBox.find('img').stop(false,true);

                        if(!currentImg.is(':first-child'))
                            currentImg.fadeOut(600);

                        defaultImg.fadeIn(600);
                    }
                </script>

