<script type="text/javascript">
function setCookie(c_name,value)   // c_name: cookie name, value: cookie value, expiredays: cookie 有效天數 
{
	expiredays=999;
	var exdate=new Date()                        // 宣告 exdate
	exdate.setDate(exdate.getDate()+expiredays)  // 將 exdate 設成今天加上過期天數
	document.cookie=c_name+ "=" +escape(value)+  // 產生 cookie 內容  c_name=escape(value);expires=exdate
	((expiredays==null) ? "" : ";expires="+exdate)
}

function getCookie(c_name)
{
   if (document.cookie.length>0)
  {
     var c_list = document.cookie.split("\;");
	 for ( i in c_list )
	 {
	     var cook = c_list[i].split("=");
		 if ( cook[0] == c_name )
		 {
		     return unescape(cook[1]);
		}
	} 
  }
  return null
}

function checkCookie()
{
	lang=getCookie('lang');
  url=getCookie('keep');
  if (url!=null) {
    //location.href=url;
		//window.open(url);
  }
}

function goto(url){
	lang="";
	for(i=0;i<3;i++){
		if(document.form1["lang"][i].checked){lang=document.form1["lang"][i].value;}
	}
	
	if(url=="http://web.asia.edu.tw"){
		url="http://web.asia.edu.tw/bin/home.php?Lang=zh-TW";
		if(lang=="gb"){url="http://web.asia.edu.tw/bin/home.php?Lang=zh-cn";}
		if(lang=="en"){url="http://web.asia.edu.tw/bin/home.php?Lang=en";}
	}
	if(url=="http://www.asia.edu.tw"){
		if(lang=="gb"){url="http://www.asia.edu.tw/CN/";}
		if(lang=="en"){url="http://www.asia.edu.tw/EN/";}
	}
	if(url=="http://m.asia.edu.tw"){
		url="http://m.asia.edu.tw/index-tc.htm";
		if(lang=="gb"){url="http://m.asia.edu.tw/index-sc.htm";}
		if(lang=="en"){url="http://m.asia.edu.tw/index-en.htm";}
	}

	if(document.getElementById('keep').checked){
		setCookie("keep",url);
		setCookie("lang",lang);
	}

	location.href=url;
	//window.open(url);
}

