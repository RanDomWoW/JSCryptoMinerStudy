<script>
    var l = $( '.ladda-button' ).ladda();
	l.click(function(){
		l.ladda( 'start' );
		setTimeout(function(){
			l.ladda('stop');
		},2000)
	});
	
	$(".btn-submit").click(function(){
		var form = $(this).data("form");
		$(form).submit();
	});
	
	$(".disable").click(function(e){
		e.preventDefault();
		swal({
			title: "Ops!",
			text: "Recurso desabilitado temporariamente, volte mais tarde.",
			type: "error"
		});
	});
	
	
    $(".btn-submit-ajax").click(function(){
		var form = $(this).data("form");
		$.post($(form).attr("action"), $(form).serialize()).done(function( data ) {
			data = $.parseJSON(data);
			if (data.Status == false) {
				swal({
					title: "Ops!",
					text: data.Message,
					type: "error"
				});
			} else {
				window.location='index.html';
			}
		});
	});
</script>

