			// Item Store
			$(function(){
				var list  = $('#store-panel .content .list ul');
				var count = list.find('li').length;
				var left  = $('#store-panel .arw-left');
				var right = $('#store-panel .arw-right');
				var index = 0;

				list.css('width', (75 * list.find('li').length) + 'px' );
				right.click(function(){
					index = Math.min( index+1, count - 7 );
					right.css('opacity', ( index === count - 7 ) ? 0.5 : 1.0 );
					left.css('opacity', ( index === 0 ) ? 0.5 : 1.0 );
					list.animate( { marginLeft:-(index*75) + 'px' }, 400 );
					return false;
				});
