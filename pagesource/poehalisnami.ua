<script src="/js/tagit.js?v=170725" type="text/javascript"></script>
<script type="text/javascript">

    var ulMultiSelect;

    function initTagit() {
        var parentControlId = '';
        var childControlId = '';

        var hdnResultIds = $("#ctl00_ctl00_ctl00_ctl00_ctl00_center_center_center_banner_TOS_ulCountryMultipleSelect_hdnResultIds");
        var hdnResultNames = $("#ctl00_ctl00_ctl00_ctl00_ctl00_center_center_center_banner_TOS_ulCountryMultipleSelect_hdnResultNames");
        ulMultiSelect = $("#ctl00_ctl00_ctl00_ctl00_ctl00_center_center_center_banner_TOS_ulCountryMultipleSelect_ulMultiSelect");

        ulMultiSelect.tagit({
            minLength: 0,
            allowNewTags: false,
            tagSource: function (search, showChoices) {
                var parentIds = '';
                if (parentControlId != '') {
                    var hdnParentResultIds = $("input[type='hidden'][id*='" + parentControlId + "_hdnResultIds']");
                    parentIds = hdnParentResultIds.val();
                }
                $.ajax({
                    url: '/Tour/WebServices/TourService.asmx/GetCountriesForTourIndividualRequest',
                    type: 'POST',
                    //contentType: "application/x-www-form-urlencoded; charset=utf-8",
                    contentType: "application/json; charset=uft-8",
                    dataType: 'json',
                    //                    data: {
                    //                        term: search.term,
                    //                        ids: parentIds
                    //                    },
                    data: '{"term": "' + encodeURIComponent(search.term) + '", "ids": "' + encodeURIComponent(parentIds) + '"}',
                    success: function (data) {
                        if (data.d)
                            data = data.d;
                        showChoices(data);
                    },
                    error: function (e) {
                        alert(e);
                    }
                });
            },
            tagsChanged: function (tagValue, action, element) {
                var tags = ulMultiSelect.tagit('tags');
                var resultIds = '';
                var resultNames = '';

                changePlacehoder(tags);
                
                for (var i in tags) {
                    if (tags[i].value != undefined) {
                        resultIds += resultIds == '' ? tags[i].value : ("," + tags[i].value);
                        resultNames += resultNames == '' ? tags[i].label : ("," + tags[i].label);
                    }
                }
                hdnResultIds.val(resultIds);
                hdnResultNames.val(resultNames);

                if (action == "popped") {
                    if (childControlId != '') {
                        var childMultiSelect = $("ul[id*='" + childControlId + "']");
                        // search child controls
                        if (childMultiSelect.length > 0) {
                            var childTags = childMultiSelect.tagit('tags');
                            var childList = [];
                            $.each(childTags, function (i, o) {
                                if (o.parent != tagValue) {
                                    childList.push(o);
                                }
                            });
                            childMultiSelect.tagit('fill', childList);
                        }
                    }
                }
            }
        });

        function changePlacehoder(tags) {
            if (tags.length === 0) {
                ulMultiSelect.find(".tagit-input").attr("placeholder", "");
            } else {
                ulMultiSelect.find(".tagit-input").attr("placeholder", "");
            }
        }
        
        var tags = ulMultiSelect.tagit('tags');
        changePlacehoder(tags);

        $('.tagit-wrapper .d-arr').click(function () {
            $("#ctl00_ctl00_ctl00_ctl00_ctl00_center_center_center_banner_TOS_ulCountryMultipleSelect_ulMultiSelect" + " input").click();
        });
    }
    
    function destroyTagit() {
        ulMultiSelect.empty();
    }

    $(document).ready(function () {
        initTagit();
    });
</script>
