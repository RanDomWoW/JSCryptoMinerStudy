<script type="text/javascript">
    var isPost = false;
    var popover;
    function initOrderForm() {
        $("input[key='quickPhone']").mask({ mask: "+38(999) 999-99-99" });
        $("input[key='quickCountry']").Watermark("Страна:", undefined, "black");
        $("input[key='quickName']").Watermark("Имя*:", undefined, "black");
        $("input[key='quickPhone']").Watermark("Телефон*:", undefined, "black");
        $("input[key='quickEmail']").Watermark("E-mail:", undefined, "black");
        $("input[key='quickName']").bind("blur", function () {
            if ($(this).val() == 'Имя*:' && !isPost) {
                $("span[key='ValidateQuickNamekey']").html("");
            }
        });

        var $checkbox = $('.jsTourOrderSubscribe input[type="checkbox"]');

        if ($checkbox.parent().hasClass("prettycheckbox")) {
            var checkboxClone = $checkbox[0].cloneNode();
            var $subscribe = $checkbox.parents(".jsTourOrderSubscribe");
            $subscribe.empty();
            $subscribe.removeClass("has-pretty-child");
            $subscribe.append(checkboxClone);
            $checkbox = $(checkboxClone);
        }

        $checkbox.prettyCheckable({
            color: "subscribe"
        });

        $('.js-move-cursor-on-focus').each(function () {
            var input = $(this),
                placeholder = input.mask('option', 'placeholder'),
                firstPlaceholderMatch;

            $(this).on('click', function () {
                var nextAvailablePosition = input.val().indexOf(placeholder, 0),
                    notFull = nextAvailablePosition !== -1,
                    currentCaretPosition = input[0].selectionStart;

                if (typeof firstPlaceholderMatch == 'undefined') {
                    firstPlaceholderMatch = input.val().indexOf(placeholder, 0);
                }

                if (notFull) {
                    if ((currentCaretPosition > nextAvailablePosition) || (currentCaretPosition < firstPlaceholderMatch)) {
                        input[0].setSelectionRange(nextAvailablePosition, nextAvailablePosition);
                    }
                }
                else if (currentCaretPosition < firstPlaceholderMatch) {
                    input[0].setSelectionRange(firstPlaceholderMatch, firstPlaceholderMatch);
                }
            });
        });

        popover = $('[data-toggle="popover"]').popover({
            trigger: 'manual',
            template: '<div class="popover popover--no-wrap popover--validation popover--subscribe" role="tooltip">' +
                        '<div class="arrow"></div>' +
                        '<h3 class="popover-title"></h3>' +
                        '<div class="popover-content"></div>' +
                      '</div>'
        });
    }

    $(document).ready(function () {
        initOrderForm();
    });

    function SendClick(element) { 
        isPost = true;
        ClickOrer();
        Page_ClientValidate("Send");
        if (Page_IsValid) {
            var formId = element.getAttribute("data-form-id");
            if (!formId) formId = TRACKID.SideFormIndividualRequestSend;

            CommonTrackClick(formId);
            return PreSendOrder('preSend', 'Send');
        }
        return false;
    }

    function ValidateQuickName(source, arguments) {
        $(source).removeAttr("style");
        var target = $("input[key='quickName']");
        if ((target.val() == '' || target.val() == 'Имя*:') && isPost) {
            arguments.IsValid = false;
            // $(source).html("Необходимо ввести имя");
            target.popover('show');
            $(source).html('');
        } else {
            $(source).html('');
            arguments.IsValid = true;
            target.popover('hide');
        }
    }

    function ValidateQuickPhone(source, arguments) {
        var message = '';
        var target = $("input[key='quickPhone']");
        //message = "Формат номера (YYY) ХХХ-ХХ-ХХ";
        if ((target.val() == '' || target.val() == 'Телефон*:') && isPost) {
            // message = "Телефон - обязательное поле";
            target.popover('show');
            isPost = false;
            arguments.IsValid = false;
        } else {
            if (target.val() != 'Телефон*:') {
                var expr = /\+38\(\d{3}\)\s{1}\d{3}-\d{2}-\d{2}/i;
                var isValid = expr.test(target.val());
                if (!isValid) {
                    // message = "Формат номера +38(YYY) ХХХ-ХХ-ХХ";
                    target.popover('show');
                    isPost = false;
                    arguments.IsValid = false;
                } else {
                    target.popover('hide');
                }
            }
        }
        $(source).html(message);
        $(source).removeAttr("style");
        // arguments.IsValid = message == '';
    }

    function ValidateQuickEmail(source, arguments) {
        var message = '';
        var target=$("input[key='quickEmail']");
        if (target.val() != '' && target.val() != 'E-mail:') {
            var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            var isValid = re.test(target.val());
            if (!isValid) {
                // message = "Некорректный E-mail";
                target.popover('show');
                isPost = false;
                arguments.IsValid = false;
            } else {
                target.popover('hide');
            }
        }
        $(source).html(message);
        $(source).removeAttr("style");
        // arguments.IsValid = message == '';
    }
</script>

