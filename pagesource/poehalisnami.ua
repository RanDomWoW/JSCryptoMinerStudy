<script type="text/javascript" >
    $(document).ready(function() {
        if (document.URL.indexOf("/admin") == -1) {
            //$.cookie('psn.ss', '0', { path: '/', expires: 10000, domain: 'poehalisnami.ua' });
            var ss = $.cookie('psn.ss');
            var sc = $.cookie('psn.sc');
            if (sc != null) {
                if ((ss == null) || (ss == 0)) {
                    var isAllowed = 0;
                    var hdnIsSubscribePopupAllowed = $("input[id$='hdnIsSubscribePopupAllowed']");
                    if (hdnIsSubscribePopupAllowed.length > 0)
                        isAllowed = hdnIsSubscribePopupAllowed.val();
                    sc++;
                    $.cookie('psn.sc', sc, { path: '/', expires: 10000, domain: 'poehalisnami.ua' });
                    if ((sc > 5) && (isAllowed == 1)) {
                        $("div.jsSubscribePopup").show();
                    }
                }
            } else {
                $.cookie('psn.sc', '1', { path: '/', expires: 10000, domain: 'poehalisnami.ua' }); // subscribe counter
            }
        }
    });
</script>


