    function initDirectory(){
        var columnCount = getColumnCount();

        var columnList = {
            1: $('#directory-wide-block .column-1'),
            2: $('#directory-wide-block .column-2'),
            3: $('#directory-wide-block .column-3'),
            4: $('#directory-wide-block .column-4')
        }

        redistribute();

        $(window).resize(function () {
            var newColumnCount = getColumnCount();
            if(newColumnCount && newColumnCount != columnCount){
                columnCount = newColumnCount;
                redistribute();
            }
        });


        function getColumnCount() {
            return $('#directory-wide-block .column:visible').length;
        }

        function redistribute() {
            if(columnCount) {
                $("#directory-wide-block .directoryBlock").each(function () {
                    var block = $(this);
                    var column = block.data('column'+columnCount);
                    columnList[column].append(block);
                });
            }
        }

    }
