    <script type="text/javascript">
      $(document).ready(function() {
 
          var sync1 = $("#sync1");
          var sync2 = $("#sync2");
 
          sync1.owlCarousel({
            singleItem : true,
            slideSpeed : 1000,
            navigation: false,
            pagination:false,
            afterAction : syncPosition,
            responsiveRefreshRate : 200,
          });
 
          sync2.owlCarousel({
            items :3,
