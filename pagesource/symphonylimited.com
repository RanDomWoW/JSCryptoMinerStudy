              navigation: false,
              pagination:false,
              autoPlay:true,
              slideSpeed : 800,
              autoPlayTimeout:5000,
              autoPlayHoverPause:true
        });
 
          // Custom Navigation Events
          $(".next").click(function(){
            owl.trigger('owl.next');
          })
          $(".prev").click(function(){
            owl.trigger('owl.prev');
          })

 

