<script type="text/javascript">
  /* SlideToggle */
  jQuery(function(){
    if (jQuery('[data-toggle]').length){
      jQuery('[data-toggle]').click(function(){
        if (jQuery(this).hasClass('bordered')){
          if (jQuery('body').innerWidth() > 700){
            return;
          }
        }
        if (jQuery(this).hasClass('f-title') && jQuery('body').innerWidth() > 980){
          return;
        }        
          if (jQuery(this).attr('data-mm')){
            jQuery('[data-mm]').not(jQuery(this)).removeClass('__opened');
            jQuery('.' + jQuery('[data-mm]').not(jQuery(this)).attr('data-toggle')).slideUp();
          }
          if (jQuery(this).attr('data-toplinks') && jQuery('.b-header-link.__opened').length){
            jQuery('.' + jQuery('.b-header-link.__opened').not(jQuery(this)).data('toggle')).slideUp();
            jQuery('.b-header-link.__opened').not(jQuery(this)).removeClass('__opened');
          }
          jQuery('.' + jQuery(this).attr('data-toggle')).slideToggle();
          jQuery(this).toggleClass('__opened');          
      });
    }
    
    
    /* Fixed menu */
    var menuPosition = jQuery('.nav-container').offset();
    jQuery(window).bind('scroll', function () {
      if (jQuery('body').innerWidth() > 980){
        if (jQuery(window).scrollTop() > menuPosition.top) {
            jQuery('.page').addClass('nav_fixed');
        } else {
            jQuery('.page').removeClass('nav_fixed');
        }        
      }
    });
    
    jQuery('.b-header-link-search').click(function(){
      jQuery('.form-search').slideDown();
      jQuery('#search_mini_form #search').focus();
      
      if (jQuery('.b-header-link.__opened').length){
        jQuery('.' + jQuery('.b-header-link.__opened').data('toggle')).slideUp();
        jQuery('.b-header-link.__opened').removeClass('__opened');        
      }
      
    });
    jQuery('#search_mini_form #search').blur(function(){
      setTimeout(function(){ jQuery('.form-search').slideUp() }, 200);
    });
    
  });
  

  
</script>

