          .removeClass('links')
          .append($toggle, $ul);

  }

  var uncollapse = function($section) {

    var $ul = $section.find('ul.dropdown-menu').removeClass('dropdown-menu').removeAttr('role'),
        $toggle = $section.find('a[data-toggle="dropdown"]').remove(),
        $titleTag = $('<span>').addClass('title').text($toggle.text());

        $section
          .removeClass('dropdown')
          .addClass('links')
          .append($titleTag, $ul);

