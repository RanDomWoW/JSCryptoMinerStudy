<script type="text/javascript">
  jQuery(function($) {

    $('.do-subscribe').on('click', function(ev) {
      ev.preventDefault();
      var $button = $(ev.target);

      if (!$button.attr('data-album-id')) {
        $button = $button.closest('button');
      }

      if ($button.hasClass('disabled')) {
        return;
      }
      $button.addClass('disabled');

      Login.pop({
        'title': "Ficamos encantados por gostar deste \u00e1lbum <small>Precisamos que entre na sua conta ou esteja registado para poder seguir o \u00e1lbum<\/small>",
        'source_page': '',
        'source_action' : 'subscribe_album_aside',
        'onsuccess': function () {
          var album_id = $button.data('album-id');
          var album_url = $button.data('album-url');

          $.ajax({type:"GET",
            url:     '/do_ajax/add_subscribe_album/' + album_id + '?show_alert=true',
            success: function(){
              window.location.href = album_url;
            },
            statusCode: {
              401: function() {
                bootbox.alert("Deves estar registado como usu\u00e1rio");
              },
              405: function() {
                bootbox.alert("J\u00e1 est\u00e1s seguindo o \u00e1lbum");
              }
            },
            complete: function() {
              $button.removeClass('do-subscribe');
            }
          });
        }
      });
    });
  });
</script>
