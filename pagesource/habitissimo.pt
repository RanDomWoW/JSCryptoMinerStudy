  jQuery(function($) {
    var $navbar = $('#main-navbar');

    $navbar.wrap('<div id="main-navbar-wrapper"></div>');

    resizeMainMenu();

    window.onresize = function() {
      resizeMainMenu();
    };

    function resizeMainMenu() {
      if ($navbar.hasClass('navbar-affix')) {
        $('#main-navbar-wrapper').css('height', $navbar.css('height'));
      }
    }
  });
</script>
  <script>
  jQuery(function($) {
    var $navbar = $('#main-navbar');
    var $search = $navbar.find('.navbar-search');
    var $navbarNotification = $(".navbar-notification");

    toggleBudgetNavbarButton();

    $(window).scroll(function() {
      toggleBudgetNavbarButton();
    });

    $navbar.on('mouseenter', '.menu-content, #user-menu', closeNotifications);

    $navbar.on('touchstart', '.menu .menu-toggle', function() {
      closeNotifications();
    });

    $navbar.find('form.search').on('submit', function(ev) {
      var $q = $(this).find('[name=q]');

      if ($q.val().trim() === '') {
        ev.preventDefault();
        $q.focus();
      }
    });

    $search.find('.search-toggle').on('click', function() {
      var $this = $(this),
        $search = $this.closest('.navbar-search');

      var $prev = $navbar.find('.navbar-header'),
        left = $prev.outerWidth() + $prev.position()['left'],
        width = $this.position()['left'] + $this.outerWidth() - left;

      $search.css({
        left: left,
        width: width
      });

      $navbar.find('#main-menu').css('visibility', 'hidden');

      $search.addClass('active');
      $search.find('[name=q]').focus();
      $(window).one('resize', closeSearchForm);
    });

    $search.find('button.close').on('click', function(ev) {
      closeSearchForm();
      ev.preventDefault();
    });

    $search.find('[name=q]').on('keyup', function(ev) {
      if (ev.which == 27) {
        closeSearchForm();
      }
    });

    function closeSearchForm() {
      $search.removeClass('active');
      $navbar.find('#main-menu').css('visibility', 'visible');

      $search.css({
        left: '',
        width: ''
      });
    }

    function closeNotifications() {
      if($navbarNotification.hasClass('active')) {
        $(".navbar-notification.active").removeClass('active');
      }
    }

    function toggleBudgetNavbarButton() {
      if (isScrolledIntoView($('[data-action=toggle-navbar-budget-button]'))) {
        $('.quotation-add-navbar').addClass('hide-fixed');
      } else {
        $('.quotation-add-navbar').removeClass('hide-fixed');
      }
    }

    function isScrolledIntoView(elem) {
      var $elem = $(elem);

      if (!$elem.length) {
        return false;
      }

      var $window = $(window);

      var docViewTop = $window.scrollTop();
      var docViewBottom = docViewTop + $window.height();

      var elemTop = $elem.offset().top;
      var elemBottom = elemTop + $elem.height();

      return ((elemBottom <= docViewBottom) && (elemTop >= docViewTop));
    }
  });
</script>
  <script>
  jQuery(function($) {
    $('body').on('click', '.user-signin', function(ev) {
      var that = $(this);
      var options = {
        back: 'https://www.habitissimo.pt/usuario',
        default_action: 'register',
        source_action: 'login_navbar',
      };

      var addOptionIfDataExists = function(option, prefix) {
        var option_data = that.data(option);
        var option_underscore;

        if (prefix) {
          option = prefix + option;
        }

        if (option_data) {
          option_underscore = option.replace('-', '_');
          options[option_underscore] = option_data;
        }
      };

      addOptionIfDataExists('action', 'default_');
      addOptionIfDataExists('title');
      addOptionIfDataExists('source-page');
      addOptionIfDataExists('source-action');

      try {
        Login.pop(options);
        ev.preventDefault();

      } catch (e) {}
    });
  })
</script>
  <script>
  jQuery(function($) {
    var menu_selector_classes = [".menu .menu-toggle",".menu .accordion-menu-toggle"];

    menu_selector_classes.forEach(function(menu_selector_class) {
      var $navbar = $('#main-navbar');

      $navbar.on('touchstart', menu_selector_class , function(ev) {
        var $menu = $(this).closest('.menu'),
          $hover = !$menu.hasClass('active hover');

        $menu.toggleClass('active hover', $hover);

        if ($hover) {
          $menu.add($menu.parentsUntil($navbar, '.menu')).siblings().removeClass('active hover');
        }

        if (!$(this).hasClass('noncollapsible')) {
          ev.preventDefault();
        }

        var onBodyClick = function(ev) {
          if (!$(ev.target).closest('.menu.hover').length) {
            $navbar.find('.menu').removeClass('hover');
            $(document.body).off('touchstart', onBodyClick);
          }
        };

        $(document.body).on('touchstart', onBodyClick);
      });

      $navbar.on('mouseenter touchenter', menu_selector_class, function() {
        var $menu = $(this).closest('.menu');

        $menu.addClass('hover');
        $menu.add($menu.parentsUntil($navbar, '.menu')).siblings('.menu').removeClass('hover');
      });

      $navbar.on('mouseleave', '.menu.hover', function() {
        $(this).removeClass('hover');
      });

      $navbar.on('click', menu_selector_class, function() {
        var $menu = $(this).closest('.menu');

        $menu.toggleClass('active');

        if ($menu.hasClass('active') || $menu.hasClass('hover')) {
          $menu.add($menu.parentsUntil($navbar, '.menu')).siblings('.menu').removeClass('active hover');
        }
      });
    })
  });
</script>
  <script>
    jQuery(function($) {

  var $body = $(document.body),
      $notifications = $('.navbar-notifications');

  $notifications.on('click', 'li.navbar-notification > a', function(ev) {
    ev.preventDefault();

    var $this = $(this),
        $li = $this.closest('li'),
        $target = $($this.attr('href'));

    $('li.navbar-notification').not($li).removeClass('active');

    $li.toggleClass('active');

    if ($li.hasClass('active')) {
      var onMouseDown = function(ev) {
        if ($target[0] !== ev.target && !$.contains($target[0], ev.target)) {
          $li.removeClass('active');
          $body.off('mousedown', onMouseDown);
        }
      };

      setTimeout(function() {
        $body.on('mousedown', onMouseDown);
      }, 0);
    }
  });

  $notifications.on('touchstart', function(ev) {
    var onBodyClick = function(ev) {
      if (!$(ev.target).closest('.navbar-notifications').length) {
        $(".navbar-notification.active").removeClass('active');
        $(document.body).off('touchstart', onBodyClick);
      }
    };

    $(document.body).on('touchstart', onBodyClick);
  });
});
