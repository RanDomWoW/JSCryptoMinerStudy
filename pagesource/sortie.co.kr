							searchOpen = true;
							$(".search-container").addClass("search-container-active");
							$("#search-textbox").show();
							$( "#search-textbox" ).animate({
								width: "155px"
							}, 300, function() {
//								$( "#search-textbox" ).focus();
							});
						}
					}
					var oncloseSearch = function() {
						freeze = true;
						searchOpen = false;
						$( "#search-textbox" ).animate({
							width: "20"
						}, 300, function() {
							$(".search-container").removeClass("search-container-active");
							$("#search-textbox").hide();
//							$( "#search-textbox" ).focusout();

							setTimeout(function() {
								freeze = false;
							}, 1000)
						});
					}
					var onclickShowSearch = function() {

					}
					var checkSearchForm = function() {
						
						if ( $("#search-textbox").val() == "" ) {
							oncloseSearch();
							return false;
						}

						return true;
					}
					var closeGlobal = false;
					var preventClosing = function() {
						closeGlobal = true;
						var e  = window.event;
						e.preventDefault();
						e.stopPropagation();
					}
