			slidesToScroll: 1
		});
	});
	var closeProductPreview = function() {
		$(".product-preview").hide();

		for(var i = totalCount; i > -1; i--) {
			$('.product-preview-images').slick('slickRemove', i);
		}
	}
	var productPreviewLeft = function() {
		$('.product-preview-images').slick('slickPrev');
	}
	var productPreviewRight = function() {
		$('.product-preview-images').slick('slickNext');
	}
	var onclickAddImagesToPreview = function(imageString) {
		var imageArray = imageString.split(",");
		totalCount = imageArray.length;
		for ( var i = 0; i < imageArray.length; i++) { 
			$('.product-preview-images').slick('slickAdd','<div class="preview-items"><img src="'+imageArray[i]+'"/></div>');
		}
	}
</script>

		
