<script type="text/javascript">
$(function() {
	setAppBanner();		
});
var isAPP = false;

function setAppBanner(){
	if (/(iPhone|iPad|iPod|iOS)/i.test(navigator.userAgent)) {
		$('a.app_download').attr('href','https://itunes.apple.com/tw/app/id465020202?mt=8');
		isAPP = true;
	} else if (/(Android)/i.test(navigator.userAgent)) {
		$('a.app_download').attr('href','https://play.google.com/store/apps/details?id=com.holiday&feature=search_resultn');
		isAPP = true;
	}
	
	if(isAPP){
		$('#app').show();
		$('#Form1').css('top', $('#app').outerHeight());
		$('.app_close').bind('click',function(){
			$('#app').slideUp(200);
			$('#Form1').css('top', 0);
			isAPP = false;
		});
	}
	
	$(window).scroll(function() {
		if(isAPP){
			var targetTop = $('#app').outerHeight()- $(this).scrollTop() > 0 ? $('#app').outerHeight()- $(this).scrollTop() : 0;
			$('#Form1').css('top', targetTop);
		}
	});
}
</script>

