<script type="text/javascript" language="javascript">
    var speed = 20//数值越大速度越慢
    var demo = document.getElementById("demo");
    var demo1 = document.getElementById("demo1");
    var demo2 = document.getElementById("demo2");
    demo2.innerHTML = demo1.innerHTML
    function Marquee() {
        if (demo2.offsetWidth - demo.scrollLeft <= 0)
            demo.scrollLeft -= demo1.offsetWidth
        else {
            demo.scrollLeft++
        }
    }
    var MyMar = setInterval(Marquee, speed)
    demo.onmouseover = function () { clearInterval(MyMar) }
    demo.onmouseout = function () { MyMar = setInterval(Marquee, speed) } 
</script>
<script type="text/javascript" language="javascript">
    var speed1 = 20; //数值越大速度越慢
    var Div1 = document.getElementById("Div1");
    var Td1 = document.getElementById("Td1");
    var Td2 = document.getElementById("Td2");
    Td2.innerHTML = Td1.innerHTML;
    function Marquee1() {
        if (Td2.offsetWidth - Div1.scrollLeft <= 0) {
            Div1.scrollLeft -= Td1.offsetWidth;
        }
        else {
            Div1.scrollLeft++;
        }
    }
    var MyMar1 = setInterval(Marquee1, speed1)
    Div1.onmouseover = function () { clearInterval(MyMar1) }
    Div1.onmouseout = function () { MyMar1 = setInterval(Marquee1, speed1) }
</script>