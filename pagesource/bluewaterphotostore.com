<script>
var popupid = null;
jQuery(function(){
	var curuid = 0;
	var _uid = jQuery.cookie('msguid');
	var _msgboxshowtime = jQuery.cookie('msgboxshowtime');
	
	if(_uid == null || _msgboxshowtime == null){
		checkMsgbox();
	}else if(_uid != curuid || (_msgboxshowtime < (new Date().getTime() + 10000))){
		checkMsgbox();
	}
	
});

function checkMsgbox(){
	jQuery.getJSON("/check-msgbox?path="+window.location.pathname+"&r="+new Date().getTime()+"-"+Math.random(), function(data){
		//console.log(data);
		if(data && data.show){
			jQuery("#msgbox").show();
			popupid = data.id;
		}
		
		if(data.msgboxremain){
			jQuery.cookie("msguid", data.msguid);
			//jQuery.cookie("msgboxremain", data.msgboxremain);
			jQuery.cookie("msgboxshowtime", new Date().getTime() + data.msgboxremain*1000);
		}
	});
}

function closeMsgBox(){
	jQuery("#msgbox").hide();
	if(popupid){
		jQuery.get("/close-msgbox?id="+popupid+"&r="+new Date().getTime()+"-"+Math.random());
	}
}
</script>


