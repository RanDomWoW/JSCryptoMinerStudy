<script type="text/javascript">


function GetTypes(dt) 
{ 

    // Попытка отправки запроса серверу 
    try 
    {		

	//dt=dt.replace(/"/g,""));
				if(dt.length>5)
				{
					
				$.ajax({
                url:"http://minjust.gov.kg/ky/news/dots/",
				async: true,
				type:'POST',
				data:{date:dt},
                success: function(data) {
		
				var ob = JSON.parse(data, function(key, value) {
  					return value;
				});

				if(ob!=null)
				{
				var keys = Object.keys(ob);
				
				
				
				for(var i =0;i<keys.length; i++)
				{
					var itm=document.getElementById('td-'+keys[i]);
					if(itm!=null)
					{	
						var strs=ob[keys[i]].split(',');
						for(j=0;j<strs.length;j++)
						{
							if(strs[j]==1)
							{
								///itm.innerHTML+='<div class="round-green"></div>';
								$(itm).css("background-color","rgba(20, 200, 20, 0.4)");
							}
							else if(strs[j]==2)
							{
								$(itm).css("background-color","rgba(20, 120, 200, 0.4)");
						    }
							else if(strs[j]==3)
							{itm.innerHTML+='<div class="round-red"></div>';}
							else if(strs[j]==4)
							{itm.innerHTML+='<div class="round-gray"></div>';}
							else if(strs[j]==5)
							{itm.innerHTML+='<div class="round-black"></div>';}
							else if(strs[j]==6)
							{itm.innerHTML+='<div class="round-yellow"></div>';}
							
						}
	
					}
    				
				}
				
				}
				
				}
				});
				}
		
	}
	// Сообщение об ошибке в случае неудачи 
    catch (e) 
    {
		//alert(e);
	} 
} 


</script>

