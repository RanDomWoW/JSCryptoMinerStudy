                        <img ng-if="(item.Status == 'AWAITING_APPROVAL' || item.Status == 'SAVED') && IsBlogWorkflowEnable == 'True'" title='{{item.Status}}' id="blogpost166_10_{{item.Id}}_Pending" class="BlogPendingImage showAwaitingApprovalToolTip" src="//fluencycontent2-schoolwebsite.netdna-ssl.com/Releases/3805.1/Content/images/pending-small.png">
                        <img ng-if="item.Status == 'PUBLISHED' && IsBlogWorkflowEnable == 'True'" title='{{item.Status}}' id="blogpost166_10_{{item.Id}}_Published" class="BlogPublishedImage showEntryPubishedToolTip" src="//fluencycontent2-schoolwebsite.netdna-ssl.com/Releases/3805.1/Content/images/approved-small.png">
                        <img ng-if="item.Status == 'APPROVAL_REJECTED' &&  IsBlogWorkflowEnable == 'True'"  title='{{item.Status}}' id="blogpost166_10_{{item.Id}}_Reject" class="BlogRejectedImage showEntryRejectedToolTip" src="//fluencycontent2-schoolwebsite.netdna-ssl.com/Releases/3805.1/Content/images/reject-small.png" >
