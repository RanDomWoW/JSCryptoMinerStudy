<script>
$('.winners [data-id], .indicado [data-id]').click(function(e){
    e.preventDefault();
    var post_id = $(this).data().id;
    $.ajax({
        type: 'post',
        url: "/wp-admin/admin-ajax.php",
        async: true,
        dataType: 'json',
        data: {
            action : 'do_ajax',
            post_id : post_id
        },
        success: function(results) {
            //console.log(results[0]);
            $('body').addClass("modalopen");
            results[0].thumbnail ? results[0].thumbnail : '<img class="thumbnail" src="http://fakeimg.pl/140x140/"/>';
            $('.modal').remove();
            $('body').append(
                "<div class='modal'><div><span class='legend'>"+results[0].thumb_caption+"</span>"+results[0].thumbnail+"<ul class='social-media'></ul></div><div><h2 class='title'>"+results[0].name+"</h2><p class='cargo'>"+results[0].cargo+"</p><div class='desc'>"+results[0].content+"</div></div><i class='fa fa-times modalclose' aria-hidden='true'></i></div>"
            );
            $.each(results[0].social_name, function(i, val) {
                $('.social-media').append("<li class='item'><a class='link' href='"+results[0].social_link[i]+"'><i class='icon fa "+results[0].social_icon[i]+"'></i>"+val+"</a></li>");
            });
            return results;
        },
        error: function( jqXHR, textStatus, errorThrown ) {
            console.log( 'Could not get posts, server response: ' + textStatus + ': ' + errorThrown );
        }
    }).responseJSON; // <-- this instead of .responseText
});

$('input.checkbox').on('change', function(evt) {
   if($(this).parent().parent().siblings('.indicado').children().children(':checked').length >= 3) {
       this.checked = false;
       alert("Você pode selecionar somente até 3 candidatos");
   }
});

$('body').on('click', '.modalclose', function(e) {
    $('.modal').remove();
    $('body').removeClass("modalopen");
});

$('body').click(function(e){
    if($(e.target).closest('.modal').length != 0) {
        return false;
    } else if($('.modal').length){
        $(this).removeClass("modalopen");
        $('.modal').remove();
    }
});

$('.btn._confirmar').click(function(e) {
    e.preventDefault();
    e.stopPropagation();
    $('body').addClass("modalopen");
    $('body').append(
        "<div class='modal _finalizar'><div><h2 class='title'>Tem certeza que deseja confirmar?</h2></div><div style='text-align:center;'><a class='btn btn-danger _finalizar' href='/premio-votacao/finalizar-votacao/'>Finalizar votação</a></div><div><i class='fa fa-times modalclose' aria-hidden='true'></i></div>"
    );
});

$('body').on('click', '.btn._finalizar', function(e) {
    var href = $('.btn._finalizar').attr('href');
    window.location.href = href;
});



</script>

</body>
</html>