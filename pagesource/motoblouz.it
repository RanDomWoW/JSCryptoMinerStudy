        <script type="text/javascript">
            whenAvailable('$', function($) {
    $(function () {
        if (typeof algoliasearch == 'function') {
            var client = algoliasearch("UR2G8A3VQW", "d5c5dd9cf4218d80b080e58baed812fe"); // public credentials
        }
        var nbCategory = 3;
        var nbBrand = 3;
        var nbProduct = 5;
        var nbEtrContent = 2;
        var nbContent = 4;

        $('#search_engine').keyup(function (event) {
            if (27 === event.which) return; // if Escape => break
            var query = $(this).val();
            if (!query) {
                searchMultiCallback(true, null);
            }
            else {
                if (typeof algoliasearch == 'function' && typeof client == "object") {
                    client.search([
                        {
                            indexName: 'motoblouzit-category',
                            query: query,
                            params: {hitsPerPage: nbCategory}
                        },
                        {
                            indexName: 'motoblouzit-brand',
                            query: query,
                            params: {hitsPerPage: nbBrand}
                        },
                        {
                            indexName: 'motoblouzit-product',
                            query: query,
                            params: {hitsPerPage: nbProduct}
                        },
                        {
                            indexName: 'motoblouzit-etr_content',
                            query: query,
                            params: {hitsPerPage: nbEtrContent}
                        },
                        {
                            indexName: 'motoblouzit-content',
                            query: query,
                            params: {hitsPerPage: nbContent}
                        }
                    ], searchMultiCallback);
                }
            }
        });
    });
});        </script>

        <script type="text/javascript">
            whenAvailable('$', function($) {
                $(function () {
                    $('#search_result_algolia').on('click', '#search_result_product a.more', function () {
                        $('#search_form').submit();
                        return false;
                    });
                });
            });

            function searchMultiCallback(err, content) {
                $('#search_result_algolia').hide();
                if (err) return; // Return if no results
                $('#search_result_algolia').show();

                var categories = content.results[0];
                var re = /([\s\w]+)>([\s\w]+)/;
                for (var i = 0; i < categories.hits.length; i++) {
                    if ((m = re.exec(categories.hits[i].fullpathname)) !== null) {
                        if (m.index === re.lastIndex) re.lastIndex++;
                        categories.hits[i].level0 = undefined !== m[1] ? m[1].trim() : '';
                    }
                    categories.hits[i].avatar = categories.hits[i].nom.charAt(0);
                }

                var colCount = 0;
                if (content.results[0].hits.length) {
                    colCount++;
                    $('#search_result_algolia #search_result_category').html(tmpl('category_tmpl', content.results[0]));
                } else {
                    $('#search_result_algolia #search_result_category').html('');
                }
                if (content.results[1].hits.length) {
                    colCount++;
                    $('#search_result_algolia #search_result_brand').html(tmpl('brand_tmpl', content.results[1]));
                } else {
                    $('#search_result_algolia #search_result_brand').html('');
                }
                if (content.results[2].hits.length) {
                    colCount++;
                    $('#search_result_algolia #search_result_product').html(tmpl('product_tmpl', content.results[2]));
                } else {
                    $('#search_result_algolia #search_result_product').html('');
                }
                if (content.results[3].hits.length) {
                    colCount++;
                    $('#search_result_algolia #search_result_etr').html(tmpl('etr_tmpl', content.results[3]));
                } else {
                    $('#search_result_algolia #search_result_etr').html('');
                }
                if (content.results[4].hits.length) {
                    colCount++;
                    $('#search_result_algolia #search_result_content').html(tmpl('content_tmpl', content.results[4]));
                } else {
                    $('#search_result_algolia #search_result_content').html('');
                }

                $('#search_result_algolia').data('col-count', colCount);
                $('#search_result_algolia').removeClass().addClass('col-count-' + colCount);
            }
        </script>

        <div id="search_result_algolia">
            <div id="search_result_product" class="col"></div>
            <div id="search_result_brand" class="col"></div>
            <div id="search_result_category" class="col"></div>
            <div id="search_result_etr" class="col"></div>
            <div id="search_result_content" class="col"></div>
            <div class="clearfix"></div>
        </div>

        <script type="text/html" id="product_tmpl">
            <% if (hits.length) { %>
            <p class="entete">Prodotti</p>
            <ul>
                <% for (var i = 0; i < hits.length; i++) { %>
                <li>
                    <div class="product result">
                        <div class="image">
                            <a href="<%=hits[i].url%>"><img src="<%=hits[i].picture%>" alt="<% if (hits[i]._highlightResult.productname) { %><%=hits[i].productname.value%> <% } else { %> <%=hits[i]._highlightResult.nomCategorie.value%><%=hits[i]._highlightResult.marque.value%> <% } %>" /></a>
                        </div>
                        <div class="name">
                            <a href="<%=hits[i].url%>">
                                <%=hits[i]._highlightResult.nomCategorie.value%>
                                <%=hits[i]._highlightResult.marque.value%>
                                <% if (hits[i]._highlightResult.productname) { %>
                                    <%=hits[i]._highlightResult.productname.value%>
                                <% } %>
                            </a>
                        </div>
                    </div>
                </li>
                <% } %>
            </ul>
            <% } %>
            <a href="#" class="more">Vedi tutti i prodotti</a>
        </script>

        <script type="text/html" id="brand_tmpl">
            <% if (hits.length) { %>
            <p class="entete">Marche</p>
            <ul>
                <% for (var i = 0; i < hits.length; i++) { %>
                <li>
                    <div class="brand result">
                        <div class="image">
                            <a href="<%=hits[i].url%>"><img src="<%=hits[i].picture%>" alt="<%=hits[i].marque%>" /></a>
                        </div>
                        <div class="name">
                            <a href="<%=hits[i].url%>"><%=hits[i]._highlightResult.marque.value%></a>
                        </div>
                    </div>
                </li>
                <% } %>
            </ul>
            <% } %>
            <a href="/marche.html" class="more">Vedi tutte le marche</a>
        </script>

        <script type="text/html" id="category_tmpl">
            <% if (hits.length) { %>
            <p class="entete">Categorie</p>
            <ul>
                <% for (var i = 0; i < hits.length; i++) { %>
                <li>
                    <div class="category result">
                        <% if (hits[i].avatar) { %>
                        <div class="left"><div class="avatar"><%=hits[i].avatar%></div></div>
                        <% } %>
                        <div class="name">
                            <a href="<%=hits[i].url%>">
                                <span class="level1"><%=hits[i]._highlightResult.nom.value%></span>
                                <span class="level0">in <%=hits[i].level0%></span>
                            </a>
                        </div>
                    </div>
                </li>
                <% } %>
            </ul>
            <% } %>
        </script>

        <script type="text/html" id="content_tmpl">
            <% if (hits.length) { %>
            <p class="entete">ALGOLIA.SEARCH.CONTENT_TITLE</p>
            <ul>
                <% for (var i = 0; i < hits.length; i++) { %>
                <li>
                    <div class="category result">
                        <div class="left"><div class="avatar"><%=hits[i].title.substr(0, 1)%></div></div>
                        <div class="name">
                            <a href="<%=hits[i].url%>">
                                <span class="level0"><%=hits[i].title%></span>
                            </a>
                        </div>
                    </div>
                </li>
                <% } %>
            </ul>
            <% } %>
        </script>

        <script type="text/html" id="etr_tmpl">
            <% if (hits.length) { %>
            <p class="entete">Il blog</p>
            <ul>
                <% for (var i = 0; i < hits.length; i++) { %>
                <li>
                    <div class="etr result">
                        <div class="image">
                            <a href="<%=hits[i].link%>" title="<%=hits[i].title%>"><img src="<%=hits[i].image%>" alt="<%=hits[i].title%>" /></a>
                        </div>
                        <div class="name">
                            <a href="<%=hits[i].link%>" title="<%=hits[i].title%>"><%=hits[i]._highlightResult.title.value%></a>
                        </div>
                    </div>
                </li>
                <% } %>
            </ul>
            <% } %>
            <a href="<%='https://www.motoblouz.it/enjoytheride/?s=%keyword%'.replace('%keyword%', query)%>" title="Vedi tutti gli articoli" class="more">Vedi tutti gli articoli</a>
        </script>
