<script type="text/javascript">

    var favoritesProductIds = [];
    var favoritesBrandIds = [];

    function Callback_favorites_listener_customer_add_favorite_executed(d3t_block, d3t_trigger) {
        if (d3t_block.context_data.response != 'success') {
            $.magnificPopup.open({
                items: {
                    src: d3t_block.context_data.response,
                    type: 'inline'
                },
                closeOnBgClick: false
            }, 0);
        }
        else {
            $.magnificPopup.close();
            var text = "";
            if (d3t_trigger.idproduit != "") {
                favoritesProductIds.push(parseInt(d3t_trigger.idproduit));
                text = "Prodotto aggiunto ai preferiti";
            }
            if (d3t_trigger.idfabricant != "") {
                favoritesBrandIds.push(parseInt(d3t_trigger.idfabricant));
                text = "Marca aggiunta ai preferiti";
            }
            if ('function' === typeof updateFavoritesButtons)
                updateFavoritesButtons();

            $().toastmessage('showToast', {
                text: text,
                stayTime: 4000,
                sticky: false,
                type: 'success',
                position: 'middle-center',
                inEffectDuration: 300
            });

            $.eventDispatcher.trigger('customer_add_favorite_executed_success');
        }
    }

    function updateFavoritesButtons() {

        $(".favorites.product.btn").each(function () {
            if (favoritesProductIds.indexOf(parseInt($(this).attr("data-id-product"))) != -1) {
                $("#product_like_" + $(this).attr("data-id-product")).find(".not-added").hide();
                $("#product_like_" + $(this).attr("data-id-product")).find(".added").show();
            }
        });

        $(".favorites.brand.btn").each(function () {
            if (favoritesBrandIds.indexOf(parseInt($(this).attr("data-id-brand"))) != -1) {
                $("#brand_like_" + $(this).attr("data-id-brand")).find(".not-added").hide();
                $("#brand_like_" + $(this).attr("data-id-brand")).find(".added").show();
            }
        });
    }

</script>

    
