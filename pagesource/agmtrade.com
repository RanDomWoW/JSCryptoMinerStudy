    <script>
        var Qiniu_URL = "https://qiniu.agm18.com/"; // 显示七牛图片的url
        var master_session = "";
        if (master_session) {
            $('.open-account').attr('href', '/account/open');
            $('.followProduct .floatLeft .title a').attr('href', '/vote#WindUp');
            $('.followProduct .floatRight .title a').attr('href', '/vote');
        }
        var is_TraderStar = "0";

        if (is_TraderStar == '1') {
            $("#TraderStar").attr("dataType", 1);
            $(".slide").css("margin-bottom", 0);
            $("#shortcutMenu").hide();
            $('.clearlist').hide();
            $('.footer').css('position', 'inherit');
        }

        //固定菜单
        $(window).scroll(function() {
            var $tt = $(this).scrollTop();
            if ($tt > 40) {
                $("nav.js-stick").addClass("navFixed");
                $(".mobile_menu_switcher").css("top", "33%");
            } else {
                $("nav.js-stick").removeClass("navFixed");
                $(".mobile_menu_switcher").css("top", "50%");
            }
        });
        // 判断当前菜单高亮显示
        var CRM_URL = "https://interface.agmtrade.com",
            path = location.pathname,
            CheckPhoneName = '',
            ADMIN_EMAIL = "compliance@agmtrade.com", // 用于验证手机号码的用户名
            Qiniu_URL = "https://qiniu.agm18.com/"; // 显示七牛图片的url

        var outime = false;

        if (window.localStorage.getItem('Acc') && window.localStorage.getItem('Acc') != 'undefined') {
            $('#OrderUsername').val(window.localStorage.getItem('Acc'));
            $('#OrderPassword').val(window.localStorage.getItem('AccPas'));
        }

        // 输入邮箱订阅网站信息 BEGIN
        $('#Subscribe').click(function(e) {
            if (e && e.preventDefault) {
                e.preventDefault();
            } else {
                window.event.returnValue = false;
            }

            var res =
                /^([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+@([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+\.[a-zA-Z]{2,3}$/;

            if (!res.test($('input[name="keyword"]').val())) {
                layer.msg('请填写正确的邮箱地址');
                return false;
            }

            $.ajax({
                url: '/Public/Subscribe',
                type: 'post',
                dataType: 'json',
                async: true,
                data: {
                    keyword: $('input[name="keyword"]').val()
                },
                success: function(res) {
                    if (res.Status == '') {
                        layer.msg(res.Content);
                    } else {
                        layer.msg(res.Content);
                    }
                }
            });
        });
        // 输入邮箱订阅网站信息 END

        // 添加页面切换栏目的激活状态 BEGIN
        $.each($("#navBar li a"), function() {
            if ($(this).attr('href') == '/agmindex' && path == '/') {
                $(this).addClass('active');
            } else {
                if (path == $(this).attr('href')) {
                    $(this).addClass('active');
                } else {
                    $(this).removeClass('active');
                }
            }
        });
        // 添加页面切换栏目的激活状态 END

        // 点击登录按钮进行登录 BEGIN
        $('#LoginButton').click(function(e) {
            if (e && e.preventDefault) {
                e.preventDefault();
            } else {
                window.event.returnValue = false;
            }

            var username = $('#OrderUsername').val(),
                password = $('#OrderPassword').val();

            if (username == '' || password == '') {
                layer.msg('账号或者密码不能为空!');
                return false;
            }

            layer.load(1, {
                shade: [0.3, '#fff'] //0.1透明度的白色背景
            });

            var data = JSON.stringify({
                username: username,
                password: password
            });

            $.ajax({
                url: CRM_URL + '/api/auth/login',
                type: 'post',
                contentType: 'application/json: charset=UTF-8',
                dataType: 'json',
                async: true,
                data: data,
                success: function(res) {
                    if (res.token) {
                        window.localStorage.setItem('IM_account', res.account);

                        if (res.show) {
                            CheckPhoneName = username; // 验证手机号码时的用户名赋值

                            $('#OrderUsername').val(''),
                                $('#OrderPassword').val('');

                            layer.closeAll();

                            $('#BalanceUserModal').modal({
                                show: true,
                                backdrop: 'static'
                            });

                            $('#ModifyInfo').on('click', function() {
                                window.localStorage.setItem('token', res.token.token); //存入本地缓存
                                window.localStorage.setItem('cloud_key', res.cloud_key);
                                window.localStorage.setItem('email_to', res.email_to);

                                if (!ModifyNickname) {
                                    layer.msg('用户名已被占用或格式不正确请重新填写');
                                    return false;
                                }

                                if (!InitPass) {
                                    layer.msg('请检查初始密码是否正确');
                                    return false;
                                }

                                if (!(ModifyNewPass && ModifySurePass)) {
                                    layer.msg('请检查新密码的格式或者两次输入密码是否一致');
                                    return false;
                                }

                                if (!(CheckPhone && CheckMessage)) {
                                    layer.msg('请验证手机号码或者短信验证码是否正确');
                                    return false;
                                }

                                var AcceptInfo = $('#AcceptInfo').get(0).checked;

                                if (!AcceptInfo) {
                                    layer.msg('请仔细阅读相关协议');
                                    return false;
                                }

                                var AModifyNickname = $('#ModifyNickname').val(),
                                    AInitPass = $('#InitPass').val(),
                                    AModifyNewPass = $('#ModifyNewPass').val(),
                                    AModifySurePass = $('#ModifySurePass').val();

                                var NewData = JSON.stringify({
                                    username: username,
                                    password: AInitPass,
                                    NewNickname: AModifyNickname,
                                    NewPass: AModifyNewPass
                                });

                                layer.load(1, {
                                    shade: [0.3, '#fff'] //0.1透明度的白色背景
                                });

                                $.ajax({
                                    url: CRM_URL + '/api/auth/AddNewInfo',
                                    type: 'post',
                                    contentType: 'application/json: charset=UTF-8',
                                    dataType: 'json',
                                    async: true,
                                    data: NewData,
                                    success: function(info) {
                                        if (info.Status == '') {
                                            $.ajax({
                                                url: '/Public/login',
                                                data: {
                                                    username: AModifyNickname,
                                                    password: AModifyNewPass,
                                                    token: window.localStorage.getItem('token')
                                                },
                                                type: 'post',
                                                success: function(res) {
                                                    if (res.url) {
                                                        if ($('#Rem_word:checked').val() == 'None') {
                                                            window.localStorage.setItem('Acc', username);
                                                            window.localStorage.setItem('AccPas', password);
                                                        };

                                                        window.localStorage.setItem('login_count_click', true);

                                                        if (document.location.pathname == '/follow') {
                                                            window.location = document.location.origin + '/trade/master';
                                                        } else {
                                                            window.location = res.url;
                                                        }
                                                    } else {
                                                        layer.closeAll();

                                                        window.localStorage.clear();
                                                        layer.msg('账号或密码错误!');
                                                        // 登录失败删除class避免回车无法登录
                                                        $('#LoginButton').removeClass('AGMLogin');
                                                    }
                                                }
                                            });

                                            // 客户首次登录发送信息 BEGIN
                                            var ask_json = JSON.stringify({
                                                Name: AModifyNickname,
                                                Account: username,
                                                Date: '',
                                                Type: 'SignContract',
                                                Email: ADMIN_EMAIL,
                                                Status: 2
                                            })
                                            $.ajax({
                                                url: CRM_URL + '/api/OrderSMS',
                                                type: 'POST',
                                                data: ask_json,
                                                contentType: 'application/json: charset=UTF-8',
                                                dataType: 'json',
                                                headers: {
                                                    Authorization: 'Bearer ' + localStorage.getItem('token')
                                                }
                                            });
                                            // 客户首次登录发送信息 BEGIN
                                        } else {
                                            layer.closeAll();

                                            layer.msg(info.Content);
                                        }
                                    }
                                })
                            });
                            $('#LoginFieldChange').on('click', function() {
                                $('#BalanceUserModal').modal('hide');
                                $.ajax({
                                    url: '/Public/login_count',
                                    data: {
                                        username: username
                                    },
                                    type: 'POST'
                                });
                            });
                        } else {
                            window.localStorage.setItem('token', res.token.token); //存入本地缓存
                            window.localStorage.setItem('cloud_key', res.cloud_key);
                            window.localStorage.setItem('email_to', res.email_to);

                            if (res.nick_name == '' || res.nick_name == null || res.nick_name == 'undefined') {
                                layer.closeAll();

                                layer.prompt({
                                    title: '欢迎来到AGMTrade社交化交易终端,首次登陆,请补添用户名哦!',
                                    formType: 3
                                }, function(pass, index) {
                                    var b = /^[0-9a-zA-Z\_\u4e00-\u9fa5]*$/g;
                                    var zb = /^[0-9a-zA-Z\_]*$/g;
                                    var xb = /^[\u4e00-\u9fa5]*$/g;
                                    var yb = /^[0-9a-zA-Z\_\u4e00-\u9fa5]*$/g;
                                    var NumberPattern = /^[0-9]+$/g;

                                    if (pass == '') {
                                        layer.msg("请填写昵称");
                                        return false;
                                    } else if (!b.test(pass)) {
                                        layer.msg("用户名只允许数字、字母、中文和下划线");
                                        return false;
                                    } else if (pass.length > 16 || pass.length < 6 && zb.test(pass)) {
                                        layer.msg("用户名长度介于6-16个英文字符");
                                        return false;
                                    } else if (pass.length > 8 && xb.test(pass)) {
                                        layer.msg("用户名长度不能大于8个文字");
                                        return false;
                                    } else if (pass.replace(/[^\x00-\xff]/g, "01").length > 16 && yb.test(pass)) {
                                        layer.msg("用户名混合长度不能大于16个字符");
                                        return false;
                                    } else if (NumberPattern.test(pass)) {
                                        layer.msg("用户名不能使用纯数字");
                                        return false;
                                    }

                                    layer.load(1, {
                                        shade: [0.3, '#fff'] //0.1透明度的白色背景
                                    });

                                    $.ajax({
                                        url: '/Public/checkName',
                                        data: {
                                            nickname: pass
                                        },
                                        type: 'post',
                                        dataType: 'json',
                                        success: function(res) {
                                            if (res.status == '') {
                                                $.ajax({
                                                    url: '/Public/login',
                                                    data: {
                                                        username: username,
                                                        password: password,
                                                        nick_name: pass,
                                                        token: window.localStorage.getItem('token')
                                                    },
                                                    type: 'post',
                                                    success: function(res) {
                                                        if (res.url) {
                                                            if ($('#Rem_word:checked').val() == 'None') {
                                                                window.localStorage.setItem('Acc', username);
                                                                window.localStorage.setItem('AccPas', password);
                                                            };

                                                            window.localStorage.setItem('login_count_click', true);

                                                            if (document.location.pathname == '/follow') {
                                                                window.location = document.location.origin + '/trade/master';
                                                            } else {
                                                                window.location = res.url;
                                                            }
                                                        } else {
                                                            layer.closeAll();

                                                            window.localStorage.clear();
                                                            layer.msg('账号或密码错误!')

                                                            // 登录失败删除class避免回车无法登录
                                                            $('#LoginButton').removeClass('AGMLogin');
                                                        }
                                                    }
                                                });
                                            } else {
                                                layer.msg(res.info);
                                            }
                                        }
                                    });
                                });
                            } else {
                                //登陆
                                $.ajax({
                                    url: '/Public/login',
                                    data: {
                                        username: username,
                                        password: password,
                                        token: window.localStorage.getItem('token')
                                    },
                                    type: 'post',
                                    success: function(res) {
                                        if (res.url) {
                                            if ($('#Rem_word:checked').val() == 'None') {
                                                window.localStorage.setItem('Acc', username);
                                                window.localStorage.setItem('AccPas', password);
                                            }

                                            window.localStorage.setItem('login_count_click', true);

                                            if (document.location.pathname == '/follow') {
                                                window.location = document.location.origin + '/trade/master';
                                            } else {
                                                window.location = res.url;
                                            }
                                        } else {
                                            layer.closeAll();

                                            window.localStorage.clear();
                                            layer.msg(res.info);

                                            // 登录失败删除class避免回车无法登录
                                            $('#LoginButton').removeClass('AGMLogin');
                                        }
                                    }
                                });
                            }
                        }
                    } else {
                        layer.closeAll();

                        // 登录失败删除class避免回车无法登录
                        $('#LoginButton').removeClass('AGMLogin');

                        layer.msg(res.info);
                        return false;
                    }
                }
            });
        });
        // 点击登录按钮进行登录 END

