        $("#closeNation").click(function() {
            $("#TopBannerForm, #closeNation").hide();
            $("#TopBannerForm").parent().css("height","75px");
        });
        // 隐藏头部信息 END

        //设置AJAX的全局默认选项 BEGIN
        $.ajaxSetup({
            headers: {
                AGM_CLIENT: '54.80.1.73'
            }
        });
        //设置AJAX的全局默认选项 END
