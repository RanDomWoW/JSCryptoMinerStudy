                    var slide = slides[slideIndextemp];
                    slideTimer = setTimeout(ShowSlide, slide.WAITTIME * 1000);
                });

                $('.slider div.goLeft').click(function () {
                    StopSliderTransition();
                    ShowSlide();
                });

                $('.slider div.goRight').click(function () {
                    StopSliderTransition();

                    if (slideIndex == 0)    //we are displaying the last slide
                        slideIndex = slides.length - 2;
                    else if (slideIndex == 1)   //we are displaying the first slide
                        slideIndex = slides.length - 1;
                    else
                        slideIndex -= 2;

                    ShowSlide();
                });
                ShowSlide();
            }
        }

        function ShowSlide() {
            var slide = slides[slideIndex];
            var image2Show = $('.slider .image' + frameIndex);
            image2Show.attr("src", "/Slides/ReadSlideImage.aspx?qsSlideID=" + slide.SLIDEID);
            image2Show.attr("alt", slide.SLIDENAME);
            if (slide.SLIDELINK != null) {
                image2Show.css('cursor', 'pointer');
                image2Show.click(event, function () {
                    if (slide.TARGET == 1)
                        window.location.href = slide.SLIDELINK;
                    else
                        window.open(slide.SLIDELINK)
                    event.stopPropagation();
                });
            } else {
                image2Show.css('cursor', 'default');
                image2Show.prop('onclick', null).off('click');
            }
            image2Show.prop('title', slide.COMMENTS);
            $(image2Show).fadeIn(slideTransitionTime, function () {
            });

            frameIndex = frameIndex == 0 ? 1 : 0;
            var image2Show = $('.slider .image' + frameIndex);
            image2Show.prop('onclick', null).off('click');
            image2Show.prop('title', '');
            $(image2Show).fadeOut(slide.slideTransitionTime, function () {
            });

            if (++slideIndex == slides.length)
                slideIndex = 0;

            if (slidePlay && slides.length > 1)
                slideTimer = setTimeout(ShowSlide, slide.WAITTIME * 1000);
        }
        function StopSliderTransition() {
            clearTimeout(slideTimer);
            $('.slider .image0').stop(true, true);
            $('.slider .image1').stop(true, true);
        }
    </script>

