<script type="text/javascript">
//ページ内リンク調整
$(window).on('load', function() {
  var headerHeight = 140;
  var url = $(location).attr('href');
  if(url.indexOf("?id=") != -1){
    var id = url.split("?id=");
    var $target = $('#' + id[id.length - 1]);
    if($target.length){
      var pos = $target.offset().top-headerHeight;
      $("html, body").animate({scrollTop:pos}, 400);
    }
    }
  });
</script>

