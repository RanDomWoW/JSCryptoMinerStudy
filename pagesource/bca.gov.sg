				window.open("https://www.google.com/cse?cx=008333683202955833228%3Ai2mutbii3eg&ie=UTF-8&siteurl=www.google.com%2Fcse%2Fhome%3Fcx%3D008333683202955833228%3Ai2mutbii3eg&ref=www.google.com%2Fcse%2Fmanage%2Fall&ss=240j44288j3&q=" + inputTextValue);
			}
			
			function changeFontSize(target) {
				var x1 = document.getElementById("changeSize1");
				var x2 = document.getElementById("changeSize2");
				var x3 = document.getElementById("changeSize3");
				var x4 = document.getElementById("changeSize4");
				var x5 = document.getElementById("changeSize5");
				var x6 = document.getElementById("changeSize6");
				var x7 = document.getElementById("changeSize7");
				var x8 = document.getElementById("changeSize8");
				var x9 = document.getElementById("changeSize9");
				var x10 = document.getElementById("changeSize10");
				//var x = document.getElementsByClassName("changeSize");
			  var computedStyle = window.getComputedStyle
					? getComputedStyle(x1) // Standards
					: x1.currentStyle;     // Old IE
			  var fontSize;

			  if (computedStyle) { // This will be true on nearly all browsers
				  fontSize = parseFloat(computedStyle && computedStyle.fontSize);

				  if (target == document.getElementById("btnLarge")) {
					if (fontSize < 21 ) fontSize += 2;
				  } else if (target == document.getElementById("btnSmall")) {
					if (fontSize > 10 ) fontSize -= 2;
				  } else if (target == document.getElementById("btnNormal")) {
					fontSize = 14;
				  }
				  x1.style.fontSize = fontSize + "px";
				  x2.style.fontSize = fontSize + "px";
				  x3.style.fontSize = fontSize + "px";
				  x4.style.fontSize = fontSize + "px";
				  x5.style.fontSize = fontSize + "px";
				  x6.style.fontSize = fontSize + "px";
				  x7.style.fontSize = fontSize + "px";
				  x8.style.fontSize = fontSize + "px";
				  x9.style.fontSize = fontSize + "px";
				  x10.style.fontSize = fontSize + "px";
			  }
			}
			
		</script>

