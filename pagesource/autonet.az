<script type="text/javascript">
        $(document).ready(function() {


         $('#ProfileLogin').submit(function(){

          $('#login_loading').html("Xahiş edirik gözləyin...");

          $.ajax({
            type: 'POST',
            url: '/app/ajax_login.php?language=az&action=index',
            data: $(this).serialize()
          })
          .done(function(data){

           if($.trim(data)=== "Error"){
             $('#login_loading').html("İstifadəçi adı və ya şifrə yalnışdır");

           }else{
             $('#login_response').html(data);
             $('#modal_login').removeClass("md-show");
             $('#login_loading').empty().html();
             $('#modal_login').find("input[type=text], input[type=password]").val("");

           }

         })
          .fail(function() {

           alert("Naməlum səhv baş verdi...");

         });

          return false;

        });

       });


        function profile_logout(){
          $('#login_response').html("Xahiş edirik gözləyin...");

          $.post( "/app/ajax_logout.php?language=az&action=index", function( data ) {
           if($.trim(data)=== "Error"){
            $('#login_response').html("İstifadəçi adı və ya şifrə yalnışdır");

          }else{
           $('#login_response').html(data);
         }
       });

          return false;
        }
        function additem_btn_link(){
          $.post( "/app/ajax_loggon.php", function( data ) {
            if($.trim(data)=== "Error"){
             $('#modal_login').addClass("md-show");
             $('#modal_login').find("input[name=location]").val("additem");


           }else{
             window.location.href='/az/additem';
           }
         });

          return false;
        }
        function subscription_btn_link(){
          $.post( "/app/ajax_loggon.php", function( data ) {
            if($.trim(data)=== "Error"){
             $('#modal_login').addClass("md-show");
             $('#modal_login').find("input[name=location]").val("subscription");


           }else{
             window.location.href='/az/subscription';
           }
         });

          return false;
        }

      </script>
