<script type="text/javascript">
  $(document).ready(function() {


    var bashla = new Date().getFullYear();
    var son = 1900;
    var options = "";
    var bosh = "<option class='sort_other' value=''></option>";
    var boshm = "<option class='sort_other' value=''></option>";
    for(var year = bashla ; year >=son; year--){
