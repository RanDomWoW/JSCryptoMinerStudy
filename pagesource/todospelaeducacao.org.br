                                    var nTam = nTam - 2;
                                    jQuery(".content").css("font-size", nTam, 'margin', '5%');
                                    clikDim =  clikDim + 1;
                                    clikAum = 1;
                            }
                    }
            });

        });

        function changeFocus(id){
            var v = "#" + id;
            $(v).focus();
        }


        </script>


