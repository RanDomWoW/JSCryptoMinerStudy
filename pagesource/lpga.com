<script type="text/x-underscore" id="right-rail-social-twitter-post">
	<article class="social-item twitter-tweet">
		<% if (typeof(ImageUrl) != "undefined" && ImageUrl != null && ImageUrl.length > 0) { %>
		<figure class="social-featured-image">
			<img src="<%= ImageUrl %>" />
		</figure>
		<% } %>
		<header class="social-header">
			<div class="social-avatar">
				<img alt="<%= UserName %>" src="<%= UserProfileImageUrl %>">
			</div>
			<h3 class="social-name">
				<a href="<%= AuthorUrl %>" target="_blank"><%= UserName %></a>
			</h3>
			<div class="twitter-name">@<%= UserScreenName %></div>
			<!-- TODO: Add Follow button - ensure username is updated and javascript is included somewhere on the page -->
			<a class="social-follow-button" target="_blank" href="<%= FollowUrl %>">Follow</a>
		</header>
		<p class="social-copy">
			<%= ParsedText %>
		</p>
		<footer class="social-footer">
			<div class="social-meta">
				<abbr class="timeago" title="<%= CreatedAt %>"></abbr>
			</div>
			<div class="social-functions">
				<a class="reply" href="<%= ReplyUrl %>" target="_blank"></a>
				<a class="retweet" href="<%= RetweetUrl %>" target="_blank"></a>
				<a class="favorite" href="<%= FavoriteUrl %>" target="_blank"></a>
			</div>
		</footer>
	</article>
</script>

