  </script><script>	FBA = {
		getCookie: function(name) {
			var nameEQ = name + "=";
			var ca = document.cookie.split(';'); //把cookie分割成组
			for (var i = 0; i < ca.length; i++) {
				var c = ca[i]; //取得字符串
				while (c.charAt(0) == ' ') { //判断一下字符串有没有前导空格
					c = c.substring(1, c.length); //有的话，从第二位开始取
				}
				if (c.indexOf(nameEQ) == 0) { //如果含有我们要的name
					return unescape(c.substring(nameEQ.length, c.length)); //解码并截取我们要值
				}
			}
			return false;
		},
		setCookie: function(name, value, seconds, path, domain) {
			seconds = seconds || 0; //seconds有值就直接赋值，没有为0，这个根php不一样。
			var expires = "";
			if (seconds != 0) { //设置cookie生存时间
				var date = new Date();
				date.setTime(date.getTime() + (seconds * 1000));
				expires = "; expires=" + date.toGMTString();
			}
			domain = domain || G.TOP_DOMAIN;//''document.domain;
			path = path || '/';
			document.cookie = name + "=" + escape(value) + expires + ";domain=" + domain + ";path=" + path; //转码并赋值
		},
		formatParams: function(data) {
			var arr = [];
			for (var name in data) {
				arr.push(encodeURIComponent(name) + '=' + encodeURIComponent(data[name]));
			}
			return arr.join('&');
		},
		sendJsonp: function(options){
			try{
				options = options || {};
				//创建 script 标签并加入到页面中
				var callbackName = ('jsonp_' + Math.random()).replace(".", ""),
					oHead = document.getElementsByTagName('head')[0],
					params = "";
				if(options.data){
					options.data[options.callback] = callbackName;
					params += this.formatParams(options.data);
				}else{
					params += options.callback + "=" + callbackName;
				}
				var oS = document.createElement('script');
				oHead.appendChild(oS);

				//创建jsonp回调函数

				window[callbackName] = function (json) {
					oHead.removeChild(oS);
					clearTimeout(oS.timer);
					window[callbackName] = function(){};
					options.success && options.success(json);
				};
				//发送请求

				oS.src = options.url + (options.url.indexOf('?') != -1 ? '&' : '?') + params;
				//超时处理
				if (options.time) {
					oS.timer = setTimeout(function () {
						window[callbackName] = function(){};
						oHead.removeChild(oS);
						options.error && options.error();
					}, options.time);
				}
			}catch(e){}
		},
		jsonp: function(obj){
			this.sendJsonp({
				url: obj.url,
				callback: "callback",
				data: obj.data || {},
				time: 3000,
				success: function(data){
					obj.success && obj.success(data);
				},
				error: function(){
					obj.error && obj.error();
				}
			});
		},
		isUS: function(){
			/*
				(ip) + _site_f  表示是否弹过窗 空为没弹过窗 1为页面跳转之后要弹窗 2为已弹窗
				window['ip_layer'] 表示接口请求完 才能弹窗  
				(ip) + '_access_site' 表示当前用户访问的是国际站还是当前ip站点
				_this.setCookie('ip_code');  存贮用户的ip 当浏览器关闭或者弹过窗 清除
			*/		
			var _this = this;

						var ipPopConfig = JSON.parse('{"IP":["US","GB","CA","AE","OM","SA"],"SITE":["US","GB","CA"]}');
			var shipSite = '';
				_this.isShow(1);
				_this.jsonp({
					url: 'https://www.dresslink.com/ProductAjax-getCountryCode.html',
					data: {'data_type': 'country'},
					success: function(data){
						if(!shipSite && data.info && data.info.country_ip && ipPopConfig['IP'].indexOf(data.info.country_ip) != -1 && data.info.country_name){
							window['ip_site'] = data.info;
						}

						if(data && data.info && data.info.country_code){
							window['ip_code'] = data.info.country_code.toLowerCase();
							if(data.info.site){
								var s = _this.getCookie(window['ip_code'] + '_site_f');
								if(shipSite == '' &&  !s){
									//新用户 跳转到相对应的ip站点
									_this.ipTurn(window['ip_code'],1);
								}else{
									//老用户 跳转至用户上一次的访问记录
									if(s == '1'){
										_this.isShow();//页面跳转中
										return;
									}else{
										var c = shipSite || 'www',
											b = _this.getCookie(window['ip_code'] + '_access_site');
										if(b == c || c != 'www'){ 
											_this.dealLanguage();//站点与访客记录一致 或者访问其他站点
										}else{
											b ? _this.ipTurn(b) : _this.dealLanguage();
										}
										_this.setCookie(window['ip_code'] + '_access_site',c,60 * 24 * 60 * 60);
									}
								}
							}else if(ipPopConfig['IP'].indexOf(window['ip_code'].toUpperCase()) != -1){
								var f = false;
								if(!_this.getCookie(window['ip_code'] + '_site_f')){
									_this.setCookie('ip_code',window['ip_code']);
									window['ip_layer'] = 1;
									_this.setCookie(window['ip_code'] + '_site_f', 1);
									f = true;
								}
								_this.dealLanguage(f);
							}else{
								_this.dealLanguage();
							}
						}else{
							_this.dealLanguage();
						}
					},
					error: function(){
						_this.dealLanguage();
					}
				})
		},
				ipTurn: function(b,f){
			var _this = this;
			_this.jsonp({
				url: "https://www.dresslink.com/ProductAjax-getLocationUrl.html",
				data: {url: location.href,user_type: _this.getCookie(b + '_access_site') ? 'old' : 'new' ,access_site: b},
				success: function(data){
					if(data.result == 'success' && data.info && data.info.url){
						if(f && !_this.getCookie(b + '_site_f')){
							_this.setCookie('ip_code',b);//空为没弹过窗 1为页面跳转之后要弹窗 2为已弹窗
							window['ip_layer'] = 1;
							_this.setCookie(b + '_site_f', 1);
						}
						location.href = data.info.url;
					}else{
						_this.dealLanguage();
					}
				},
				error: function(){
					_this.dealLanguage();
				}
			})
		},
				dealLanguage: function(flag){
			var _this = this;
			_this.jsonp({
				url: "https://www.dresslink.com/ProductAjax-locationLang.html",
				data: {current_url: location.href},
				success: function(data){
					if(data && data.info && parseInt(data.info.type) == 1){
						location.href = data.info.location_url;
					}else{
						flag ? location.reload() : _this.isShow();
					}
				},
				error: function(){
					flag ? location.reload() : _this.isShow();
				}
			})
		},
		isShow: function(flag){
			var body = document.getElementsByTagName('body');
			if(body && body.length){
				body[0].style.cssText = "visibility: " + (flag ? 'hidden' : 'visible');
			}else{
				document.write('<style>body{visibility: ' + (flag ? 'hidden' : 'visible') + ';}</style>');
			}
		}
	}
	FBA.isUS();
