<script src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.6.0/slick.min.js"></script><script type="text/javascript" src="https://static.rocketpunch.com/bundle/card.e24cd7528fb6d0812206.js"></script>
<script>
$().ready(function(){
  if (typeof ajax_tiles !== "undefined") { $rocket.fetch_tiles(ajax_tiles); }
  $('#main-content').on('click', '.bookmark-icon', function(e) {
    e.preventDefault();
    e.stopPropagation();
    var form = $(e.target).closest('form'), icon = $(e.target);
    $rocket.toggleBookmark(form, icon);
  });$('#banner-list').slick({
    lazyLoad: 'ondemand',
    slidesToShow: 2,
    slidesToScroll: 1,
    autoplay: true,
    autoplaySpeed: 3500,
    arrows: true,
    prevArrow: '<i class="ic-chevron-left icon slick-prev"></i>',
    nextArrow: '<i class="ic-chevron-right icon slick-next"></i>',
    infinite: true,
    responsive: [
      {
        breakpoint: 767,
        settings: {
          slidesToShow: 1,
          slidesToScroll: 1,
        }
      },
    ]
  });

  function ifie() {
    var ua = window.navigator.userAgent;
    var msie = ua.indexOf("MSIE ");
    if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./))
      return true;
    else
      return false;
  }
  var slides = [];
  var actives = [];
  var setHeight = function() {
    $('#banner-list').height($('#banner-list .slick-active img').height());
    $('#banner-list button').show();
  }
  $('#banner-list').on('init', function(event, slick){
    if(slick.$slides.length>0){
      slides = slick.$slides;
    }
    var id0 = $(slides[0]).data('id');
    var id1 = $(slides[1]).data('id');
    ga('send', 'event', 'banner', 'main load', 'main banner load '+id0, 1, {nonInteraction: true});
    ga('send', 'event', 'banner', 'main load', 'main banner load '+id1, 1, {nonInteraction: true});
    actives.push(id0, id1);
    if(ifie()){
      $('#banner-list button').hide();
      $('#banner-list .slick-active img').on('load', function(e){
        setHeight();
      });
      $(window).resize(function() {
        setHeight();
      });
    }
  });

  $('#banner-list').on('afterChange', function(event, slick, currentSlide){
    var active = $('#banner-list .slick-active');
    $.each(active, function(i, a){
      var id = $(a).attr('data-id');
      var arr = $.inArray( id, actives );
      if(arr<0){
        actives.push(id);
        ga('send', 'event', 'banner', 'main load', 'main banner load '+id, 1, {nonInteraction: true});
      } else {
        actives.splice(arr-1, 1);
      }
    });
  });

  $('#banner-list').on('click', 'a', function(e){
    var id = $(this).attr('id');
    if(!id) return false;
    ga('send', 'event', 'banner', 'main click', id+' main banner', 1, {nonInteraction: true});
  });

  $('.slick-prev').on('click', function(e){
    ga('send', 'event', 'banner', 'main arrow click', 'main banner prev', 1, {nonInteraction: true});
  });
  $('.slick-next').on('click', function(e){
    ga('send', 'event', 'banner', 'main arrow click', 'main banner next', 1, {nonInteraction: true});
  });});
</script>
