    <div class="row input-wrapper">
        <div class="input-field col l6" data-error="Selecione a data de partida">
            <i class="icon-calendar"></i>
            <input id="widget-vertical-departure-date" placeholder="Data de ida" class="datepicker departure-date" type="text" data-error="" value="">
        </div>
        <div class="input-field col l6" data-error="Selecione a data de volta">
            <i class="icon-calendar"></i>
            <input id="widget-vertical-return-date" placeholder="Data de volta" class="datepicker return-date" type="text" value="" disabled>
        </div>
    </div>
    <div class="row">
        <div class="col l12">
            <button class="search-btn btn-primary btn-main-cta"><i class="icon-search"></i>Buscar passagens</button>
        </div>
    </div>
</form>

