    <div class="row input-wrapper">
        <div class="input-field col l12" data-error="Selecione sua origem">
            <i class="icon-origin"></i>
            <input id="widget-vertical-origin-place" class="origin-place " type="text" placeholder="Digite aqui sua origem" value="" data-slug="">
        </div>
    </div>
    <div class="row input-wrapper">
        <div class="input-field col l12" data-error="Selecione o destino">
            <i class="icon-destination"></i>
