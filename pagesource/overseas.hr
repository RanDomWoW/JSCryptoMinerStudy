<script type="text/javascript">
    function handleLogin() {
        FB.getLoginStatus(function (response) {
            if (response.status === 'connected') {
                var uid = response.authResponse.userID;
                var accessToken = response.authResponse.accessToken;
                $.ajax({
                    type: "GET",
                    contentType: "application/json; charset=utf-8",
                    url: "/Shipping/FacebookLogin/" + accessToken,
                    success: function (data) {
                        if (data.registered && data.redirUrl != null) {
                            _gaq.push(['_setCustomVar', 1, 'Registriran', 'Da', 1]);
                            _gaq.push(['_trackEvent', 'Login', 'Facebook']);
                            parent.window.open(data.redirUrl, '_newtab');
                            return;
                        }

                        $.fancybox({
                            width: 490,
                            height: 300,
                            padding: 5,
                            href: "/ShippingFacebookLogin",
                            type: "iframe",
                            centerOnScroll: true,
                            overlayColor: "#000",
                            autoScale: false,
                            titleShow: false
                        });
                    }
                });
            }
        });
    }

    function faceConnect() {
        FB.getLoginStatus(function (response) {
            if (response.status === 'connected') {
                var uid = response.authResponse.userID;
                var accessToken = response.authResponse.accessToken;
                var face = $('#face');
                var connector = $('#connector');
                var fb_pic = $('#fb_pic');
                var name = $('Name');
                var contact_person = $('#ContactPerson');
                var pic_url = 'https://graph.facebook.com/' + uid + '/picture'
                if (face != null) {
                    face.hide(400);
                    connector.show();
                    fb_pic.attr("src", pic_url);
                }

                $.ajax({
                    type: "GET",
                    contentType: "application/json; charset=utf-8",
                    url: "/Shipping/FacebookLogin/" + accessToken,
                    success: function (data) {
                        if (data.registered && data.redirUrl != null) {
                            _gaq.push(['_setCustomVar', 1, 'Registriran', 'Da', 1]);
                            _gaq.push(['_trackEvent', 'Login', 'Facebook']);
                            return;
                        }
                    }
                });
            }
        });
    }
</script>
