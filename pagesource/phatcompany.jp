<script>

  // スムーススクロールを起動する

  smoothScroll.init() ;



  // ナビゲーションを固定

  $(function () {

    var $body = $('body'),

        $localNav = $('.local-nav'),

        localNavOffsetTop = $localNav.offset().top;


  
    $(window).on('scroll', function () {

      if($(this).scrollTop() > localNavOffsetTop) {

        $body.addClass('is-fixed');

      } else {

        $body.removeClass('is-fixed');

      }

    });

  });



  // スクロール位置でナビゲーションの画像を変更

  $(function() {

    /**

     * 現在スクロール位置によるグローバルナビのアクティブ表示

     */

    var scrollMenu = function() {

      // 配列宣言

      // ここにスクロールで点灯させる箇所のidを記述する

      // 数値は全て0でOK

      var array = {

        '#top': null,

        '#scale': null,

        '#parfom': null,

        '#minicchu': null,

        '#figma': null,

        '#others': null

      };



      var $globalNavi = new Array();



      // 各要素のスクロール値を保存

      for (var key in array) {

        if ($(key).offset()) {

          array[key] = $(key); // 数値丁度だとずれるので10px余裕を作る

          $globalNavi[key] = $('#local-nav ul li a[href="' + key + '"]');

        }

      }



      // スクロールイベントで判定

      $(window).scroll(_.throttle(function () {

        for (var key in array) {
          if ($(window).scrollTop() > array[key].offset().top - 80) {

            $('#local-nav ul li a').each(function() {

              $(this).removeClass('active');
            });

            $globalNavi[key].addClass('active');

          }

        }

      }, 100));

    }

    // 実行

    scrollMenu();
  });




</script>

