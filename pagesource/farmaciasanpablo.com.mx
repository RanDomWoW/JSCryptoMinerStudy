<script id="recommendation-engine-js-tmpl" type="text/x-jquery-tmpl">
<section class="fsp-wrapper">
    <div class="recommendation-engine-title">
        <hr>
        <div class="content">
            <h3>{{= title}}</h3>
        </div>

        <section class="slider owl-carousel owl-carousel-ranking">
            {{each recommendations}}
                <div class="owl-item product-item">
                    <a href="/p/{{= code}}">
                        <img src="{{= imageUrl}}" class="img-responsive item-center slider-image" alt="">
                    </a>
                    <hr/>
                    <a href="/p/{{= code}}" >
                        <h5 class="home-item-title">
                            {{= name}}
                        </h5>
                    </a>
                    <p class="home-item-subtitle">{{= description}}</p>
                    <p class="item-prize">{{= price.formattedValue}}<span class="currency"></span></p>
                    <form id="addToCartForm_{{= code}}" class="" action="/cart/add" method="post">
                        <input type="hidden" maxlength="3" size="1" id="qty" name="qty" class="qty js-qty-selector-input" value="1">
                        <input type="hidden" name="productCodePost" value="{{= id}}">
                        <button id="addToCartButton" type="button" class="btn btn-primary slider-btn recommendation-form js-add-to-cart" onclick="FSP.recEng.addToCart('{{= id}}')">Agregar</button>
                    </form>
                </div>
            {{/each}}
        </section>

        <hr>
    </div>
</section>
</script>
