<script type="text/javascript">
if (!Browser.Engine.trident){
	new Autocompleter.Request.JSON($('id_l'), '/_ajax/locationform/',{
		'selectFirst':true,
		'postVar':'l',
		'selectMode':'pick',
		'onSelection': function(token){
			$('id_l_id').value =  this.selected.getElement('input[name=id]').value;
			$('id_l_info').value = this.selected.getElement('input[name=prefix]').value;
			$('id_l_name').value = this.selected.getElement('span').get('text');
		},
		'injectChoice':function(token){
			var name = token['name'];
			var id = token['id'];
			var prefix = token['prefix'];
		
			var choice = new Element('li');
			choice.inputValue = token['name'];

			new Element('span', {'html': this.markQueryValue(name)}).inject(choice);
			new Element('input', {'type':'hidden','name':'id','value':id}).inject(choice);
			new Element('input', {'type':'hidden','name':'prefix','value':prefix}).inject(choice);
			this.addChoiceEvents(choice).inject(this.choices);
		}
	});

	$('id_search_form').addEvent('submit',function(){
		if ($('id_l').value != $('id_l_name').value){
			$('id_l_id').value = '';
			$('id_l_info').value = '';
		}
	});
}
</script>

