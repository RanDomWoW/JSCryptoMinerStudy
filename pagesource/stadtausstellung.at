<style type="text/css">
div.info_bar {width:936px;height:76px;position:relative;z-index:0}
div.info_bar .item {display:block;height:76px;width:232px;position:absolute;top:0px;z-index:5;background:url(http://assets.sta.io/site_media/img/spacer.gif);}
div.info_bar .bg_bar {position:absolute;width:936px;height:76px;top:0px;left:0px;z-index:-1;}
div.info_bar .bar_buttons {position:absolute;top:0px;left:0px;width:936px;height:76px;z-index:3}
div.info_bar img {position:absolute;top:0px;left:0px;z-index:-1}
#bar_allgemeine_informationen {left:0px}
#bar_jobs {left:232px}
#bar_news {left:464px}
#bar_kontakt {left:696px}</style>
<div class="info_bar">
	<div class="bar_buttons">
		<a href="/land/allgemeine-informationen/" id="bar_allgemeine_informationen">Allgemeine Informationen</a> <a href="http://programm.stadtausstellung.at/jobs/" id="bar_jobs">Wir stellen ein (Jobs)</a> <a href="http://programm.stadtausstellung.at/news1/" id="bar_news">Aktuelle News</a> <a href="http://www.stadtausstellung.at/kontakt/" id="bar_kontakt">Kontakt</a></div>
	<div class="bg_bar">
		&nbsp;</div>
</div>
<script type="text/javascript">
window.addEvent('load',function(){
	var mainimg = new Element('img',{'src':'http://assets.sta.io/site_media/u/si/2011/12/08/leiste210911.png'});
	var info = new Element('img',{'src':'http://assets.sta.io/site_media/u/si/2011/12/08/leiste_rollover1_210911.png'});
	var jobs = new Element('img',{'src':'http://assets.sta.io/site_media/u/si/2011/12/08/leiste_rollover2_210911.png'});
	var news = new Element('img',{'src':'http://assets.sta.io/site_media/u/si/2011/12/08/leiste_rollover3_210911.png'});
	var kontakt = new Element('img',{'src':'http://assets.sta.io/site_media/u/si/2011/12/08/leiste_rollover4_210911.png'});
	var bg_bar = document.getElement('div.info_bar .bg_bar');
	var current_overlay = null;
        var info_bar = document.getElement('div.info_bar div.bar_buttons');

	mainimg.inject(bg_bar);


	info.inject(bg_bar);
	info.get('morph').set({'opacity':0});

	jobs.inject(bg_bar);
	jobs.get('morph').set({'opacity':0});

	news.inject(bg_bar);
	news.get('morph').set({'opacity':0});

	kontakt.inject(bg_bar);
	kontakt.get('morph').set({'opacity':0});

document.getElements('div.info_bar a').each(function(a){
var d = new Element('div',{'class':'item','id':a.id});
d.inject(info_bar);
var href = a.href;
a.destroy();
d.addEvent('click',function(){ self.location.href=href; });
});
	
	$('bar_allgemeine_informationen').addEvent('mouseenter',function(){
		mainimg.morph({'opacity':0});
		info.morph({'opacity':1});
		current_overlay = info;
	});
	$('bar_jobs').addEvent('mouseenter',function(){
		mainimg.morph({'opacity':0});
		jobs.morph({'opacity':1});
		current_overlay = jobs;
	});
	$('bar_news').addEvent('mouseenter',function(){
		mainimg.morph({'opacity':0});
		news.morph({'opacity':1});
		current_overlay = news;
	});
	$('bar_kontakt').addEvent('mouseenter',function(){
		mainimg.morph({'opacity':0});
		kontakt.morph({'opacity':1});
		current_overlay = kontakt;
	});
	
	document.getElements('div.info_bar .item').addEvent('mouseleave',function(){
		if (current_overlay != null){
			current_overlay.morph({'opacity':0});
			mainimg.morph({'opacity':1});
			current_overlay=null;
		}
	});
});
</script>
</div>







