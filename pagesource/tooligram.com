function setCookie(name, value, minutes, domain) {
    var expires = "";
    if (minutes) {
        var date = new Date();
        date.setTime(date.getTime() + (minutes*60*1000));
        expires = "; expires=" + date.toUTCString();
    }
    document.cookie = name + "=" + (value || "")  + expires + "; path=/; domain=" + domain;
}
var isRoistatReferrerCookieSet = document.cookie.indexOf('roistat_referrer=') !== -1
if (!isRoistatReferrerCookieSet) {
    setCookie('roistat_referrer', document.referrer, 10, '.tooligram.com');
}
