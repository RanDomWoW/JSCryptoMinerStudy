<script>
	$(function() {
		setInterval(cambiaImmaginiAffiliazioni, 1500);
	});

	var boxCorrente = 1;
	var affCorrente = 4;
	var NUM_AFFILIAZIONI = 27;

	function cambiaImmaginiAffiliazioni() {

		//Trovo il box da cambiare
		var idAffiliazione = "#aff"+boxCorrente;

		//Determino l'affiliazione da mettere
		affCorrente++;
		if (affCorrente > NUM_AFFILIAZIONI) {
			affCorrente = 1;
		}
		var nomeAffCorrente = "";
		if (affCorrente < 10) {
			nomeAffCorrente = "0"+affCorrente;
		} else {
			nomeAffCorrente = ""+affCorrente;
		}

		//Codice che cambia l'immagine
		$(idAffiliazione).fadeOut(400, function() {
            $(idAffiliazione).attr("src","immagini/affiliazioni/aff"+nomeAffCorrente+".jpg");
        }).fadeIn(400);

		//Preparo per cambiare il prossimo
		boxCorrente++;
		if (boxCorrente == 5) {
			boxCorrente = 1;
		}
	}
</script>
