<script>
	function cercaVeloce() {
		var q = document.getElementById('searchBox').value;
		if (q.length > 0) {
			var q = encodeURIComponent(q);
			var indirizzo = "answerCerca.php?q="+q;
			var response = $.ajax({ type: "GET",   
				url: indirizzo,   
				async: true, 
				success : function (responseText) {
					$("#risultatiRicerca").html(responseText);
					if (responseText.length > 0) {
						$("#risultatiRicerca").show("slow");
					}
				}
			}).responseText;
		} else {
			$("#risultatiRicerca").hide("fast");
			$("#risultatiRicerca").html("");
		}
	}
</script>

