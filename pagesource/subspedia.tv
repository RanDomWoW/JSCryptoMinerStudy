		<script>

			//Cosa stai a guardare la sorgente della pagina?

			//Ti senti figo perchè stai leggendo questi commenti?

			//Vuoi HACKERARE il sito? U.U

			//Trovati di meglio da fare. Irriconoscente che non sei altro.

			grazies = [];
			var ultimoscaricato = "";

			function grazie(serie, stagione, numero) {

				//Controllo che non abbia già ringraziato
				var trovato = false;
				for	(i=0; i<grazies.length; i++) {
					if (grazies[i] == serie+'-'+stagione+'-'+numero) { trovato = true; }
				}

				if (!trovato) {

					//Aggiorno la casellina
					var idCasella = 'GRZ'+serie+'-'+stagione+'-'+numero;
					var idCuore = 'CR'+serie+'-'+stagione+'-'+numero;
					var numGrazie = parseInt($('#'+idCasella).html());
					numGrazie++;
					$('#'+idCasella).html(numGrazie)
					$('#'+idCuore).attr('class', 'fa fa-heart');

					//Invio la richiesta del grazie
					var richiesta = $.ajax({
						type: 'GET',
					  	url: "intoGraph.php",
					  	data: { grazie : 1,
					  			serie : serie,
					  			stagione : stagione,
					  			numero : numero
					  	}
					});

					//Mi assicuro che non ri-ringrazi
					grazies[grazies.length] = serie+'-'+stagione+'-'+numero;
				}
			}

			function downloadSub(link, serie, stagione, numero) {

				var chiaveTemporanea = link+serie+stagione+numero;
				if (ultimoscaricato != chiaveTemporanea) {

					//Mi annoto il download
					var richiesta = $.ajax({
						type: 'GET',
					  	url: "intoGraph.php",
					  	data: { download : 1,
					  			serie : serie,
					  			stagione : stagione,
					  			numero : numero
					  	}
					});

				}

				ultimoscaricato = chiaveTemporanea;

				//Faccio partire il download
				downloadUrl = "scaricaSub.php?path="+link+"&serie="+serie+"&stagione="+stagione+"&numero="+numero;
				var downloadFrame = document.createElement("iframe"); 
				downloadFrame.setAttribute('src', downloadUrl);
				downloadFrame.setAttribute('class', "screenReaderText"); 
				document.body.appendChild(downloadFrame); 
			}
		</script>
