			function listaNoticia(lista) {
				var uls = '';
				$('ul.listagem').html('');
				for (var i in lista) {
					uls +=	'<li><span>'+lista[i].data_f+'</span> - <a href="/noticia/ntc-'+lista[i].id_noticias+'.html">'+lista[i].titulo+'</a></li>';
				}
				$('ul.listagem').hide().append(uls).fadeIn('slow');
			}

			function pagina(p){
				$.ajax({
					type:		"POST",
					url:		"app/json.php",
					data:		"pagina="+p,
					dataType:	"json",
					success: function(data){
						listaNoticia(data.noticia);
