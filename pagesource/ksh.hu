<script type="text/javascript">
    function loadHetKerdese()
    {
        $('#hetkerdese').load('ajax.php?act=load_poll&lang=hu', function () {
            $('#kerdes').submit(function () {
                if ($('#hetkerdese ul').attr('id')) {
                    setCookie("hetkerdese", $('#hetkerdese ul').attr('id'), 7);
                }
                $.ajax ({
                    type:"POST",
                    url: "ajax.php",
                    data: $("#kerdes").serialize() + "&action=" + $("#kerdes").attr('action') + "&act=send_poll",
                    success: function(data){
                        $("#hetkerdese").html(data);
                        $('#rosszvalasz').submit(function () {
                            loadHetKerdese()
                            return false;
                        });
                    }           
                })
                return false;
            });            
        });
    }
        
    function setCookie(c_name,value,exdays)
    {
        var exdate=new Date();
        exdate.setDate(exdate.getDate() + exdays);
        var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
        document.cookie=c_name + "=" + c_value;
    }
    
    $(document).ready(function () {
        loadHetKerdese();
    });
