<script type="text/javascript">
	jQuery(function($)
	{
		// ********************************************************************************************************************************************
		// Permitimos que funcione el click del primer elemento del menú y que las opciones de menú se abran al hacer hover sobre este primer elemento
		// ********************************************************************************************************************************************
		$('.navbar .dropdown > a').click(function(){
			var dispositivo = navigator.userAgent.toLowerCase();
			if( dispositivo.search(/iphone|ipod|ipad|android/) > -1 )
			{
				if ($(this).parent('.dropdown').hasClass("open")){
					location.href = this.href;
				}
			}

			else
			{
				location.href = this.href;
			}
		});
		// ********************************************************************************************************************************************
	});
</script>

<script type="text/javascript">
	// ********************************************************************************************************************************************
	// TRATAMIENTO DE COOKIES / Establecemos cookies al pulsar el botón CERRAR y ocultamos panel informativo
	// ********************************************************************************************************************************************
	$(function(){
		$("#cerrar_cookies").click(function() {

			var d = new Date();
			d.setTime(d.getTime() + (365*24*60*60*1000));
			var expires = d.toUTCString();

			//creamos la cookie llamada tc_cookie (con un valor inicial de s)
			document.cookie='tc_cookie2=s;path=/;expires='+expires + ';domain=.travelclub.es;path=/';

			//ocultamos la capa que contiene el aviso de uso de cookies
			$(".site-header-cookies").hide("slow");
		})
	});
</script>

<script type="text/javascript">
	// ********************************************************************************************************************************************
	// CERRAR VENTANA DESCARGA APP
	// ********************************************************************************************************************************************
	$(function(){
		// Cerrar ventana descarga de la APP
		$('.a-qv-close').on('click', function(e){
			// 
			var d = new Date();
			d.setTime(d.getTime() + (24*60*60*1000));
			var expires = d.toUTCString();

			//creamos la cookie llamada capaApp (con un valor inicial de s)
			document.cookie='capaApp=s;path=/;expires='+expires + ';domain=.travelclub.es;path=/';

			$('#quickview-content').fadeOut();

		})

	});
</script>

<script type="text/javascript">
	// ********************************************************************************************************************************************
	// ANIMACIONES / Formulario de búsqueda
	// ********************************************************************************************************************************************
	jQuery(function($) {

		$(".animate-click").click(function(){

			if ( $(".form-horizontal").is(":visible") ) {
				var animateIn = $(this).data("animate-in");
				var animateOut = $(this).data("animate-out");
				var animatedElement = $(this).find(".animate-wr");

				//console.log(animateIn+'-'+animateOut);

				if(animateIn != undefined){
					//console.log('incepem animatia');
					if(animatedElement.is(":hidden")){
						//console.log('start IN');
						animatedElement.addClass(animateIn);
						animatedElement.one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function() {
							animatedElement.removeClass(animateIn);
						});
					}
				}
			}
		});

		$(".animate-hover").hover(function(){
			var animation = $(this).data("animate-in");
			if(animation != undefined || animation != ""){
				$(this).find(".animate-wr").addClass(animation);
			}
		});

		$(".animate-click").click(function(){
			var animation = $(this).data("animate-in");
			if(animation != undefined || animation != ""){
				$(this).find(".animate-wr").addClass(animation);
			}
		});

	})
</script>







	
