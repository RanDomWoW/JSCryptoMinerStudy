    <script src="https://s3-eu-west-1.amazonaws.com/awaprod/js/jquery.slideme2.js"></script>
    <script>
        $(function(){
			if ($('html').hasClass('oldie')) {
				$('#svg-icons').hide();
				$('#png-icons').show();
			}
			
			$('#home-strip-1 .icons li a').click(function(){
				$(this).addClass('clicked');
				$(this).css('background', 'url(https://s3-eu-west-1.amazonaws.com/awaprod/img/home/cercle-active.png) no-repeat center transparent');
			});
			
			$('#home-strip-1 .buttons li a').click(function(){
				var cl = $(this).parent().data('cl');
				$('.icons li.' + cl + ' a').addClass('clicked').css('background', 'url(https://s3-eu-west-1.amazonaws.com/awaprod/img/home/cercle-active.png) no-repeat center transparent');
			});
			
			$('#home-strip-1 .icons li').hover(function(){
				var cl = $(this).data('cl');
				$('.buttons li.' + cl + ' a').addClass('hover');
				$('img', this).addClass('zoomed');
			}, function(){
				var link = $('a', this);
				
				if (!link.hasClass('clicked')) {
					var cl = $(this).data('cl');
					$('.buttons li.' + cl + ' a').removeClass('hover');
					$('img', this).removeClass('zoomed');
				}
			});
			
			$('#home-strip-1 .buttons li').hover(function(){
				var cl = $(this).data('cl');
				$('.icons li.' + cl + ' img').addClass('zoomed');
				$('a', this).addClass('hover');
			}, function(){
				var cl = $(this).data('cl'),
					link = $('.icons li.' + cl + ' a');
				
				if (!link.hasClass('clicked')) {
					$('.icons li.' + cl + ' img').removeClass('zoomed');
					$('a', this).removeClass('hover');
				}
			});
			
			// Load on scroll
			var strip3_loaded = false;
			var strip5_loaded = false;
			
			function isVisible(el)
			{
				var $el = $(el),
					$window = $(window),
					winTop = $window.scrollTop(),
					winBottom = winTop + $window.height(),
					elTop = $el.offset().top;
					
				return (elTop <= winBottom - 100);
			}
			
			function setBGs()
			{
				$('#home-strip-3-nourriture').css('background-image', 'url(https://s3-eu-west-1.amazonaws.com/awaprod/img/bg-home-strip-3-nourriture.jpg)');
				$('#home-strip-3-abris').css('background-image', 'url(https://s3-eu-west-1.amazonaws.com/awaprod/img/bg-home-strip-3-abris.jpg)');
				$('#home-strip-3-couvertures').css('background-image', 'url(https://s3-eu-west-1.amazonaws.com/awaprod/img/bg-home-strip-3-couvertures.jpg)');
				$('#home-strip-3-granules').css('background-image', 'url(https://s3-eu-west-1.amazonaws.com/awaprod/img/bg-home-strip-3-granules.jpg)');
			}
			
			function loadBGs()
			{
				if (!strip3_loaded && isVisible('#home-strip-3-nourriture'))
				{
					setBGs();
					strip3_loaded = true;
				}
				if (!strip5_loaded && isVisible('#home-strip-5'))
				{
					$('#home-strip-5').css('background-image', 'url(https://s3-eu-west-1.amazonaws.com/awaprod/img/bg-home-strip-5.jpg)');
					strip5_loaded = true;
				}
			}
			
			$(window).scroll(function(){ loadBGs(); });
			$(window).resize(function(){ loadBGs(); });
			loadBGs();
			
			// Slideshow
			var slideshow = $('#slideshow'),
				slide = 0;
				
			slideshow.slideme({
				arrows: true,
				labels : {
					next: '',
					prev: '',
				},
				loop: true,
				touch: false,
				swipe: false,
				nativeTouchScroll: false,
				onStartCallback: setBGs,
				onEndCallback: setBGs
			});
			
			function resizeSlideshow()
			{
				var width = $(window).width();
				$('#home-strip-3-nourriture').css('background-size', (width > 600)? width : 600);
				$('#home-strip-3-abris').css('background-size', (width > 1010)? width : 1010);
				$('#home-strip-3-couvertures').css('background-size', (width > 800)? width : 800);
				$('#home-strip-3-granules').css('background-size', (width > 1101)? width : 1101);
			}
			$(window).resize(function() { resizeSlideshow(); });
			resizeSlideshow();
        });
    </script>

    <script>
        $(function(){
            $('a.user-menu').hover(function(){
                var img = $('img', this);
                img.attr('src', img.data('hover'));
            }, function(){
                var img = $('img', this);
                img.attr('src', img.data('src'));
            });
			
			$(document).on('click', 'ul.menu > li > a', function(){
				var $this = $(this);
				var alreadyClicked = $this.hasClass('clicked');
				if (alreadyClicked) {
					return false;
				}
				$this.addClass('clicked');
			});
			$(document).on('click', 'ul#svg-icons > li > a', function(){
				var $this = $(this);
				var alreadyClicked = $this.hasClass('clicked2');
				if (alreadyClicked) {
					return false;
				}
				$this.addClass('clicked2');
			});
			$(document).on('click', 'ul#png-icons > li > a', function(){
				var $this = $(this);
				var alreadyClicked = $this.hasClass('clicked2');
				if (alreadyClicked) {
					return false;
				}
				$this.addClass('clicked2');
			});
			
			$('#hamburger').click(function(){
				$(this).toggleClass('is-active');
				$('#navigation-mobile').css('height', $(window).height() - 103).slideToggle();
			});
		
			$('#id_phone,#id_president_phone,#id_president_phone2,#id_contact_phone,#id_delivery_phone').on('keydown', function(e){
				if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 110, 190]) !== -1 ||
				   (e.keyCode == 65 && (e.ctrlKey === true || e.metaKey === true)) ||
				   (e.keyCode == 67 && (e.ctrlKey === true || e.metaKey === true)) ||
				   (e.keyCode == 88 && (e.ctrlKey === true || e.metaKey === true)) ||
				   (e.keyCode >= 35 && e.keyCode <= 39)) {
						return;
			   }
			   if (((e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
				   e.preventDefault();
			   }
			});
			
			// SUBSCRIBTION FORM
			$('#subscription-banner-form').submit(function(e){
                e.preventDefault();
                
                var email = $('#subscription-banner-email').val();
                
				if (email)
				{
					$('#subscription-banner-form-wrapper').hide();
					$('#subscription-banner-loading').show();
					
					$.ajax('/comptes/rappel/ajax/', {
						method: 'POST',
						crossDomain: true,
						data: {
							email: email,
							footer: 1
						}
					}).done(function(data) {
						$('#subscription-banner-loading').hide();
					
						if (data.success) {
							$('#subscription-banner-success').show();
						} else {
							$('#subscription-banner-form-wrapper').show();
							alert('Impossible d\'enregistrer votre adresse email');
						}
					});
				} else {
					alert('Merci de saisir une adresse email valide');
				}
            });
        });
		
		if (!document.cookie.indexOf('cookielaw_accepted=') >= 0) {
			Cookielaw.createCookie('cookielaw_accepted', '1', 10 * 365);
		}
    </script>

