<script>
	$(document).ready(function(){
	
	
		var def_wrap = $('#wrapper').height();
		$('.catalog_drop').click(function(){
			$(this).parent().next().slideToggle();
			var cat_height = $(this).parent().next().height();
			if (def_wrap < 1230) {
				if (cat_height < 50) $('#wrapper').animate({height : 1230}, 500);
				else $('#wrapper').animate({height : def_wrap}, 500);
			}
		})
		
		
		$(function() {
			var pull 		= $('#pull');
				menu 		= $('nav>ul');
				menuHeight	= menu.height();

			$(pull).on('click', function(e) {
				e.preventDefault();
				menu.slideToggle();
			});

			$(window).resize(function(){
        		var w = $(window).width();
        		if(w > 320 && menu.is(':hidden')) {
        			menu.removeAttr('style');
        		}
    		});
		});
	})
		
	$('#catalog_scroll').tinycarousel({duration: 200});
	
	$(function() {
 $.fn.scrollToTop = function() {
  $(this).hide().removeAttr("href");
  if ($(window).scrollTop() >= "300") $(this).fadeIn("fast")
  var scrollDiv = $(this);
  $(window).scroll(function() {
   if ($(window).scrollTop() <= "300") $(scrollDiv).fadeOut("fast")
   else $(scrollDiv).fadeIn("fast")
  });
  $(this).click(function() {
   $("html, body").animate({scrollTop: 0}, "fast")
  })
 }
});

$(function() {
 $("#go_top").scrollToTop();
	$(".book_block .book_title" ).hover(
		function() {
			if($(this).data('fullname').length > 50) {
				$('<div class="full_text">' + $(this).data('fullname') + '</div>').hide().appendTo($(this).parent()).fadeIn(300);
			
			}
		}, function() {
			$('.full_text').fadeOut(300, function(){ $('.full_text').remove();});
		}
	);
})
</script>
