	function setImgShow(sObj) {
		for (var tmpI = 0; tmpI < imgBtnDiv.length; tmpI++) {
			if (imgBtnDiv[tmpI] == sObj) {
				imgBtnDiv[tmpI].className="textbox_over";
				bigImg.appendChild(imgShow[tmpI]);
			} else {
				imgBtnDiv[tmpI].className="textbox";
				try {
					bigImg.removeChild(imgShow[tmpI]);
				} catch (e) {
				}
			}
		}
		if (!imgShowSelected) {
			imgShowIndex++;
			if (imgShowIndex >= imgShow.length) {
				imgShowIndex = 0;
			}
			timer = setTimeout("setImgShow(imgBtnDiv[" + imgShowIndex + "])", interval);
		} else {
			clearTimeout(timer);
		}
	}
</script>

