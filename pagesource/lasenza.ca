    <script>
        var PayPalIncontextConfig = '{"isActive":"1","environment":"production","merchantid":"BBQ4CKTYHZ3WN","setExpressCheckout":"https:\/\/www.lasenza.ca\/incontext\/express\/start\/"}';

        var IWD = IWD || {};

        IWD.InContext = {

            // method after save start In-Context Checkout Experience
            reinitDefaultMethods: function () {
                // rewrite on save method in Payment class.
                if (typeof(Payment) == "undefined") {
                    return;
                }
            },

            // Fire up window given token response
            startFlow: function() {
                var urlConnect = PayPalIncontextConfig.setExpressCheckout;

                paypal.checkout.initXO();
                jQuery.support.cors = true;
                jQuery.ajax({
                    url: urlConnect,
                    type: "GET",
                    async: true,
                    crossDomain: false,

                    success: function (response) {
                        if (!response.token) {
                            paypal.checkout.closeFlow();
                            setLocation(response.errorurl);
                        } else {
                            paypal.checkout.startFlow(response.url);
                        }
                    },
                    error: function (responseData, textStatus, errorThrown) {
                        alert("Error in ajax post" + responseData.statusText);
                        paypal.checkout.closeFlow();
                    }
                });
            }
        };

        function initPaypalButton(buttonId) {
            if (typeof(IWD.OPC) != "undefined" || typeof(IWD.ES) != "undefined" || typeof(IWD.LIPP) != "undefined") {
                return;
            }

            if (typeof(PayPalIncontextConfig) == "undefined") {
                return;
            }
            if (typeof(PayPalIncontextConfig) != "object") {
                PayPalIncontextConfig = JSON.parse(PayPalIncontextConfig);
            }
            if (PayPalIncontextConfig.isActive == 0) {
                return;
            }

            paypal.checkout.setup(PayPalIncontextConfig.merchantid, {
                environment: PayPalIncontextConfig.environment,
                button: buttonId,
                click: function (e) {
                    e.preventDefault();
                    if ($('paypal-save-button') != undefined) {
                        if (payment.currentMethod != 'paypal_express') {
                            return;
                        }
                    }

                    //return if click by button in add to cart form
                    if (jQuery(e.target).closest('.add-to-cart').length > 0) {
                        return;
                    }

                    IWD.InContext.startFlow();
                }
            });
        }

        function initCheckoutPaypalButton() {
            if (typeof(IWD.OPC) != "undefined" || typeof(IWD.ES) != "undefined" || typeof(IWD.LIPP) != "undefined") {
                return;
            }

            if (typeof(PayPalIncontextConfig) == "undefined") {
                return;
            }
            if (typeof(PayPalIncontextConfig) != "object") {
                PayPalIncontextConfig = JSON.parse(PayPalIncontextConfig);
            }
            if (PayPalIncontextConfig.isActive == 0) {
                return;
            }

            paypal.checkout.setup(PayPalIncontextConfig.merchantid, {
                environment: PayPalIncontextConfig.environment
            });

            IWD.InContext.startFlow();
        }


        /**
         * Wrap the forcedSubmit function to layer in the in-context popup instead of
         * the redirect to paypal for paypal express
         */
        function applyInContext()
        {
            EcomDev.CheckItOut.addMethods({
                /**
                 * Submit order without performing validation
                 * and configuration check
                 *
                 * @return void
                 */
                forcedSubmit: function() {
                    this.showMask();

                    var currentMethod = EcomDev.CheckItOut.instance.getStep('payment').currentMethod;

                    if (currentMethod == "paypal_express") {
                        this.hideMask();
                        initCheckoutPaypalButton();
                        return;
                    }

                    if (this.paymentRedirect) {
                        // Support for payment methods,
                        // that have own checkout process
                        window.location = this.paymentRedirect;
                    } else {
                        new Ajax.Request(this.config.save, {
                            parameters: this.getParameters(),
                            method: 'POST',
                            onComplete: this.submitComplete.bind(this)
                        });
                    }
                }
            });
        }

        document.observe("dom:loaded", function () {
            if (typeof(IWD.OPC) != "undefined" || typeof(IWD.ES) != "undefined" || typeof(IWD.LIPP) != "undefined") {
                return;
            }
            if (typeof(PayPalIncontextConfig) != "undefined") {
                if (typeof(PayPalIncontextConfig) != "object") {
                    PayPalIncontextConfig = JSON.parse(PayPalIncontextConfig);
                }
                if (PayPalIncontextConfig.isActive == 0) {
                    return;
                }
                IWD.InContext.reinitDefaultMethods();
            }

            // Listen for when paypal is selected and override default behavior
            document.body.observe('payment-method:switched', function(event){
                applyInContext();
            });
        });
    </script>
