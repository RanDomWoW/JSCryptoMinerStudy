		(function($) {
			
			var mailingList = document.getElementById('mailinglist');

			mailingList.addEventListener('submit', submitMailingList);

			function submitMailingList(e) {
				e.preventDefault();

				if ($('#mailinglistemail').val() == '') {
					$('#tabbed-3 .error').text('Please enter your email address');

					return false;
				}

				var email = $('#mailinglistemail').val();

				if (email.indexOf("@") == -1 || email.indexOf(".") == -1) {
					$("#tabbed-3 .error").text('Please enter a valid email address.');

					return false;
				}

				var data = {
					action: 'save_mailinglist_options',
					option: $(this).serialize()
				};

				$('#mailinglistsubmit').hide();
				$('.ajaxsave').show();

				$.post("https://a2zhomeschooling.com/wp-admin/admin-ajax.php", data, function(response) {
					$('.ajaxsave').hide();
					$('#mailinglistsubmit').show();
					$('#mailinglistemail').val('');
					$('#tabbed-3 .message').html(response);
				});

				return false;
			}
		})(jQuery);
	</script>
