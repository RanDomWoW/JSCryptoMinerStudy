		<script type="text/javascript">
			$(document).ready(function() {
				$("#createBlogBtn").click(function() {
					$.ajax({
							dataType: "jsonp",
							url: "https://accounts.blogdiario.com/ajax/check_name?method=?",
							type: 'GET',
							data: "name="+encodeURIComponent($('#blogName').val()),
							success: function(json){
								if (json.response==1){
									window.location="https://accounts.blogdiario.com/registro/"+$('#blogName').val()+'/'+$('#blogDomain').val();
								} 
								else{
									$('#div-errors').html(json.msg);
									$('#div-errors').show();
								}
							}
						});
				});
			});
