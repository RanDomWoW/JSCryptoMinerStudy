<script type="text/javascript">
    require(['jquery','mage/validation'], function ($) {
        'use strict';
        $('#newsletter-validate-detail').submit(function() {
            var form = $(this);
            var messageElementId = 'newsletter-error';
            ajaxNewsletters(form,messageElementId);
            return false;
        });
        $('#newsletter-validate-detail-2').submit(function() {
            var form1 = $(this);
            var messageElementId1 = 'newsletter-error-2';
            ajaxNewsletters(form1,messageElementId1);
            var modalPopupClass = $(document).find('.modal-popup.confirm');
            if(modalPopupClass.length > 0 && modalPopupClass.hasClass('_show')) {
                modalPopupClass.find('.action-close').trigger( "click" );
            }
            return false;
        });
        function ajaxNewsletters(form, messageElementId) {
            var messageElement = $('#'+messageElementId);
            if(form.validation() && form.validation('isValid')) {
                var formAction = form.attr('action');
                var param = form.serialize();
                messageElement.text('').hide();
                $.ajax({
                    showLoader: true,
                    url: formAction,
                    data: param,
                    type: "POST",
                    dataType: 'json'
                }).done(function (data) {
                    if(data.message_type == 'success') {
                        if(messageElement.length > 0) {
                            messageElement.addClass('mage-success').removeClass('mage-error').text(data.messages).show();
                        } else {
                            form.find('.control').append('<div for="newsletter" generated="true" class="mage-success" id="'+ messageElementId +'">'+ data.messages +'</div>')
                        }
                    } else if(data.message_type == 'error') {
                        if(messageElement.length > 0) {
                            messageElement.addClass('mage-error').removeClass('mage-success').text(data.messages).show();
                        } else {
                            form.find('.control').append('<div for="newsletter" generated="true" class="mage-error" id="'+ messageElementId +'">'+ data.messages +'</div>')
                        }
                    }
                });
            }
        }
    });
</script></div>
