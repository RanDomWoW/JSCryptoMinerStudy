            'mage/validation',
            'mage/translate'
        ],function($, custom){
            // Add password strength test to jquery util object
            $.util= $.util || {};
            $.util.passwordStrengthLevel = function(pw){
                var passLevel = [
                    'red',
                    'red',
                    'yellow',
                    'blue',
                ];

                var flag = 0;
                /[a-zA-Z]/.test(pw)     && flag++;
                /[0-9]/.test(pw)        && flag++;
                /[!@#$%^&*]/.test(pw)   && flag++;

                var testResult = {
                    level: null,
                    status: null
                };

                if(passLevel[flag]){
                    testResult = {
                        level: passLevel[flag],
                        status: flag >= 2 ? true : false
                    }
                }

                return testResult;
            }

            $.validator.addMethod('passwordstrength', function (value, element, params) {
                var pwTestResult = $.util.passwordStrengthLevel(value);
                if(!pwTestResult.status){
                    return false;
                }
                return pwTestResult.status;
            }, "Yếu, nên có thêm số và ký tự đặc biệt.");



            var ee = $('.form-update-account-ajax').mage('validation', {
                submitHandler: function(){
                    $('#email_updated').removeProp('disabled');
                    custom.showLoadingProgress();
                    cleanLpointCookie();
                    $.ajax({
                        url: "https://www.lotte.vn/smcustomer/account/updatepost/",
                        type: "POST",
                        data: $(".form-update-account-ajax").serializeArray(),
                        xhrFields: {
                            withCredentials: true
                        },
                        dataType: 'json',
                        success: function (data) {
                            if (data.error) {
                                if(data.err_code == '262'){
                                    //custom.closeModalPopup('#register-modal');
                                    custom.hideLoadingProgress();
                                    $('#lpoint-password-checking-fail-modal').modal('openModal');
                                    $.cookie('lpoint_phone_number', '', { domain: baseUrl, path: '/', expires:-1 });
                                    cleanLpointCookie();
                                }else{
                                    $('.updated-err').html(data.message);
                                    $('.updated-err').show();
                                    custom.hideLoadingProgress();
                                }
                            } else {
                                if(typeof data.lpoint_phone_check !== "undefined"){
                                    $('#lpoint-phone-checking-modal').modal('openModal');
                                    $('.phone-number-text').html(data.phone);
                                    custom.hideLoadingProgress();

                                }else{
                                    custom.closeModalPopup('#update-modal');
                                    custom.showRegisterSuccessfullyModal('');
                                    custom.hideLoadingProgress();
                                    //window.location.reload(true);
                                }
                            }
                        },
                        error: function () {
                        }
                    });

                },
                highlight: function (element, errorClass, validClass) {
                    if (element.type === 'radio') {
                        this.findByName(element.name).addClass(errorClass).removeClass(validClass);
                    } else {
                        $(element).addClass(errorClass).removeClass(validClass);
                    }
                    if(element.name === 'password_updated') {
                        $(element).parent().attr('class', function (i, c) {
                            return c.replace(/(^|\s)pw-level-\S+/g, '');
                        }).find('.pw-success-message').addClass('hide');
                    }

                },
                unhighlight: function (element, errorClass, validClass) {
                    if (element.type === 'radio') {
                        this.findByName(element.name).removeClass(errorClass).addClass(validClass);
                    } else {
                        $(element).removeClass(errorClass).addClass(validClass);
                    }
                    if(element.name === 'password_updated'){
                        var pwTestResult = $.util.passwordStrengthLevel(element.value);
                        if(pwTestResult.status === null){
                            return;
                        }
                        $(element).parent().attr('class', function(i, c){
                            return c.replace(/(^|\s)pw-level-\S+/g, '');
                        });
                        $(element).parent().addClass('pw-level-' + pwTestResult.level);
                        $(element)
                            .siblings('.pw-success-message').addClass('hide')
                            .filter('.pw-' + pwTestResult.level).removeClass('hide');
                    }
                }
            });

            var facebookLoginBehavior = custom.facebookLoginChecking('custom_cookie');
            if(facebookLoginBehavior){
                $('.register-link').trigger('click');
                $('.facebook-login-err').html('Tài khoản facebook của bạn không có địa chỉ email - thông tin bắt buộc khi đăng ký thành viên tại Lotte.vn.Bạn có thể bấm vào nút Đăng ký bằng email để đăng ký tài khoản với địa chỉ email của bạn');
                var baseUrl = custom.getBaseUrlWithoutHttp('https://www.lotte.vn/');
                $.cookie('custom_cookie', '', { domain: baseUrl, path: '/', expires:-1 });
            }

            var cleanLpointCookie = function(){
                $.cookie('lpoint_card_no', '', { domain: baseUrl, path: '/', expires:-1 });
                $.cookie('lpoint_phone_number', '', { domain: baseUrl, path: '/', expires:-1 });
                $.cookie('lpoint_customer_id', '', { domain: baseUrl, path: '/', expires:-1 });
                $.cookie('login_id', '', { domain: baseUrl, path: '/', expires:-1 });
            }

            $('.form-update-account .other-info-label').on('click',function(){
                var _this     = $(this);
                var _othes    = $('.form-update-account .others-info-field');
                var _checkbox = $('.form-update-account #othes-info1');
                if(_this.hasClass('clicked')){
                    _checkbox.attr('clicked');
                    _this.removeClass('clicked');
                    _othes.addClass('hidden');
                }else{
                    _checkbox.removeAttr('clicked');
                    _this.addClass('clicked');
                    _othes.removeClass('hidden');
                }
            });
            setTimeout(function() {
                if ($("input").val().length > 0) {
                    $(this).parent('label').addClass('form-label-active');
                }
            }, 100);

            $('.update-account-popup .action-close').on('click',function(){
                location.reload();
            });
            $('body').keydown(function(e){
                if (e.which==27){ location.reload(); }
            });
        });
    </script>
