<script type="text/javascript">
    require([
        'jquery',
        'custom',
        'mage/validation'
    ],function($, custom){
        var dataForm = $('.lpoint-phone-checking-ajax');
        var baseUrl = custom.getBaseUrlWithoutHttp('https://www.lotte.vn/');
            dataForm.submit(function(event) {
                if(dataForm.validation() && dataForm.validation('isValid')) {
                    custom.showLoadingProgress();
                    $.ajax({
                        url: "https://www.lotte.vn/customer/account/createpost/",
                        type: "POST",
                        data: $(".lpoint-phone-checking-ajax, .form-create-account-ajax").serializeArray(),
                        dataType: 'json',
                        success: function (data) {
                            if (data.error) {
                                switch (data.err_code){
                                    case 1:
                                        custom.closeModalPopup('#lpoint-phone-checking-modal');
                                        custom.closeModalPopup('#register-modal');
                                        custom.hideLoadingProgress();
                                        $('#lpoint-phone-checking-fail-modal').modal('openModal');
                                        $.cookie('lpoint_phone_number', '', { domain: baseUrl, path: '/', expires:-1 });
                                        cleanLpointCookie();
                                        break;
                                    default:
                                        $('.lpoint-phone-checking-err').html(data.message);
                                        $('.lpoint-phone-checking-err').show();
                                        custom.hideLoadingProgress();
                                        break;
                                }
                            } else {
                                custom.showRegisterSuccessfullyModal('');
                                custom.hideLoadingProgress();
                            }
                        },
                        error: function () {
                        }
                    });
                    event.preventDefault();
                }else{
                    event.preventDefault();
                    return false;
                }
            });

            $(document).on('click', '#lpoint-phone-checking-modal .action-close', function (e) {
                custom.closeRegisterField();
                custom.resetForm();
                var baseUrl = custom.getBaseUrlWithoutHttp('https://www.lotte.vn/');
                $.cookie('lpoint_phone_number', '', { domain: baseUrl, path: '/', expires:-1 });
                cleanLpointCookie();
                if(dataForm.length){
                    if(dataForm.validation()) {
                        dataForm.validation('clearError');
                    }
                }
            });

            $(document).on('click', '.register-redirect-btn', function (e) {
                e.preventDefault();
                custom.closeRegisterField();
                custom.resetForm();
                custom.closeModalPopup('#lpoint-phone-checking-fail-modal');
                custom.closeModalPopup('#lpoint-password-checking-fail-modal');
                var baseUrl = custom.getBaseUrlWithoutHttp('https://www.lotte.vn/');
                $.cookie('lpoint_phone_number', '', { domain: baseUrl, path: '/', expires:-1 });
                cleanLpointCookie();
                if(dataForm.length){
                    if(dataForm.validation()) {
                        dataForm.validation('clearError');
                    }
                }
            })

        var cleanLpointCookie = function(){
            $.cookie('lpoint_card_no', '', { domain: baseUrl, path: '/', expires:-1 });
            $.cookie('lpoint_phone_number', '', { domain: baseUrl, path: '/', expires:-1 });
            $.cookie('lpoint_customer_id', '', { domain: baseUrl, path: '/', expires:-1 });
            $.cookie('login_id', '', { domain: baseUrl, path: '/', expires:-1 });
        }

    });
</script>
