        <script type="text/javascript" src="/js/main.js?v=2.3"></script>
        <script type="text/javascript">
            // Grid container
            var IsScrollLeftGraterThanZiro = true;
            var bgr = document.getElementsByClassName('row')[0];
            if (bgr.scrollLeft === 0)
                IsScrollLeftGraterThanZiro = false;

            var IsFirstPageWidthSmallerThan767 = false;
            if (window.innerWidth <= 768)
                IsFirstPageWidthSmallerThan767 = true;

            window.onresize = function () {
                if (window.innerWidth >= 767 && IsFirstPageWidthSmallerThan767) {
                    ResetGridBackground();
                }

                if (bgr.scrollLeft === 0)
                    IsScrollLeftGraterThanZiro = false;
                else
                    IsScrollLeftGraterThanZiro = true;
            }
            function GridBackgroundOnScroll(el) {
                var slowdownBackgroundImageTransparencySpeed = 2;
                var elGridBackground = document.getElementById(el.getAttribute('data-id'));
                var gridBackgroundClientWith = elGridBackground.clientWidth;

                var scrollLeft = Math.abs(el.scrollLeft);
                var scrolledValue;
                if (IsScrollLeftGraterThanZiro)
                    scrolledValue = el.scrollWidth - (scrollLeft + gridBackgroundClientWith);
                else
                    scrolledValue = scrollLeft;

                var percentOfScroll = (100 / gridBackgroundClientWith) * scrolledValue;
                var backgroundImageOpacity;
                backgroundImageOpacity = (100 - (percentOfScroll * slowdownBackgroundImageTransparencySpeed)) / 100;
                if (backgroundImageOpacity < 0.1)
                    backgroundImageOpacity = 0.05;

                var backgroundPositionValue = scrolledValue / 200;
                if (backgroundPositionValue > 2)
                    backgroundPositionValue = 2;

                ChangeGridBackground(elGridBackground, backgroundImageOpacity, backgroundPositionValue);
            }

            function ChangeGridBackground(elGridBackground, backgroundImageOpacity, backgroundPositionValue) {
                var el = elGridBackground.getElementsByClassName('background-image')[0];
                if (typeof (el) != 'undefined' && el != null) {
                    console.log(backgroundImageOpacity);
                    el.style.opacity = backgroundImageOpacity;
                    el.style.transform = "translate3d(" + (backgroundPositionValue.toFixed(3)) + "%, 0, 0)";
                }
            }

            function ResetGridBackground() {
                var els = document.getElementsByClassName("grid-container");
                Array.prototype.forEach.call(els,
                    function (el) {
                        ChangeGridBackground(el, 1, 0);
                        if (IsScrollLeftGraterThanZiro)
                            el.getElementsByClassName('row')[0].scrollLeft = 0;
                        else
                            el.getElementsByClassName('row')[0].scrollLeft = 10000;
                    });
            }
        </script>
    

    <script type="text/javascript">
