        <script>
            // Quick link navigation
            if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
                $('.ql-nav').css({ 'margin': ' 5px 0', 'overflow-x': ' scroll','height': ' auto', 'padding': ' 10px 2px' });
                //$('.ql-nav li:last-child').css({ 'margin-right': ' 10px' });
                $('.ql-nav').css({ 'margin': '0 0' });
                //$('.category-books .ql-nav').css({ 'margin': '0 -10px' });
                $('.ql-nav li h2').css({ 'padding': ' 0 10px' });
                $('.ql-nav-content').css({ 'position': 'initial', 'right': 'initial', 'padding': ' 0' });
                $('.ql-nav-scroll-to-right').css({ 'display': ' none' });
                $('.ql-nav-scroll-to-left').css({ 'display': ' none' });
            } else {
                $(window).resize(function () {
                    if ($('.ql-nav-content').width() > $('.ql-nav').width())
                        $('.ql-nav-scroll-to-left').fadeIn();
                    else
                        $('.ql-nav-scroll-to-left').fadeOut();
                });

                $(document).ready(function () {
                    if ($('.ql-nav-content').width() < $('.ql-nav').width()) {
                        $('.ql-nav-scroll-to-left').css('display', 'none');
                    }
                    $('.eab').on('click', function () {
                        var contentWidhh = $('.ql-nav-content').width();
                        var easyAccessBooks = $('.ql-nav').width();
                        //Hidden part of the ql-nav-content div
                        var hiddenPart = contentWidhh - easyAccessBooks;
                        var rightPosition = $('.ql-nav-content').css('right');
                        rightPosition = rightPosition.replace('px', '');
                        var id = $(this).attr('id');

                        if (id == 'eab_left') {
                            $('.ql-nav-scroll-to-right').fadeIn();
                            var value = rightPosition - (easyAccessBooks / 2);
                            if (hiddenPart < (rightPosition - (easyAccessBooks / 2)) * -1) {
                                value = hiddenPart * -1;
                                $('.ql-nav-scroll-to-right').fadeIn();
                                $('.ql-nav-scroll-to-left').delay(900).fadeOut('fast');
                            }
                            $('.ql-nav-content').css('right', value);
                        } else {
                            $('.ql-nav-scroll-to-left').fadeIn();
                            var value2 = $('.ql-nav-content').css('right').replace('px', '');
                            value2 = Math.round(value2);
                            value2 = value2 + (easyAccessBooks / 2);
                            if (value2 > 0) {
                                value2 = 2;
                                $('.ql-nav-scroll-to-right').delay(900).fadeOut('fast');
                                $('.ql-nav-scroll-to-left').delay(1000).fadeIn();
                            }
                            $('.ql-nav-content').css('right', value2);
                        }
                    });
                });
            }
            SetLtrBlockScroll('ql-nav');
        </script>
    
    
    

