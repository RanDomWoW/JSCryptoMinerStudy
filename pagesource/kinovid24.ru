<script type="text/javascript">
    var srubric_id = '';

$(document).ready(function(){
    $( "#start-date" ).datepicker({
      defaultDate: "+1w",
      numberOfMonths: 2,
      dateFormat: 'yy-mm-dd',
      maxDate: 0,
      showButtonPanel: true,
