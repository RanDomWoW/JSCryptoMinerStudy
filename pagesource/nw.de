<!-- Begin Article From Localstorage Template -->
<script id="article-from-localstorage-template" type="text/template">
<div id="new-content" class="row margin-bottom-30" style="display: none;">
<a class="close-new-content" href="javascript:void(0);" onclick="jQuery('#new-content').slideUp();">
<i class="icon_cross"></i>
</a>
<div class="sep-text single centered">
<span class="sep-text-before"><span class="sep-text-line"></span></span>
<div class="content">
<h2 class="regular-title-wrapper">
Letzte neue Artikel auf nw.de
</h2>
</div>
<span class="sep-text-after"><span class="sep-text-line"></span></span>
</div>
<div class="col-xs-12">
{{#articles}}
<div class="new-content-item" data-id="{{id}}" data-timestamp="{{timestamp}}">
<div class="new-content-item-time" title="{{date}} Uhr">{{timeago}}</div>
<a class="new-content-item-image" href="{{permalink_url}}">
<img src="/no.gif" alt="{{value}}" width="35" height="35" style="background: #c2c2c2 url({{image}}) center center no-repeat; background-size: cover;">
</a>
<a class="new-content-item-headline" href="{{permalink_url}}">
{{value}}
</a>
</div>
{{/articles}}
</div>
</div>
</script>
<!-- End Article From Localstorage Template -->

<!-- Begin New Content Template -->
<script id="new-content-template" type="text/template">
<div class="update-headline text-center">{{updateHeadline}}</div>
<div class="update-text text-center">{{updateText}}</div>
<div class="update-btn-cta text-center">
<a href="/?newsticker" class="btn btn-primary"><i class="fa fa-refresh" aria-hidden="true"></i> {{updateTextBtnCTA}}</a>
</div>
<div class="update-btn-cancel text-center">
<a href="javascript: void(0);" onclick="jQuery.fancybox.close();" class="update-close">{{updateTextBtnCancel}}</a>
</div>
</script>
<!-- End New Content Template -->

