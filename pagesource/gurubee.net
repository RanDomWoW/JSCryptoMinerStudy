<script type="text/javascript" >

	function moveMaincommentPageNavigation(mPage){
		jQuery.ajax({
            type:"GET",
            url:"/async/maincomments?pageNo="+mPage,         
            cache : false,
            success : function(data) {
                $("#mainCommentList").html(data);
            },             
            error : function(xhr, status, error) {
            	alert("오류가 발생했습니다! 운영자에게 문의해 주세요!");
            }
       });	   
	}

    
$(document).ready(function() {	
	
	$('#myTabs a[href="#lecture"]').click(function (e) {
		  e.preventDefault();
		  if(jQuery.trim($("#lecturePanel").html()).length < 10){
	            var lectureUrl = '/async/main/lectures';
	            jQuery.ajax({
	                type : "POST",
	                url : lectureUrl,
	                cache : false,
	                success : function(data) {
	                    $("#lecturePanel").html(data);
	                    $(this).tab('show');
	                },             
	                error : function(xhr, status, error) {
	                    alert('오류가 발생하였습니다. \n 다시 시도해주시거나 운영자에게 문의해 주세요!');
	                }
	           });         
	        }      
		  
	});
	
	$('#myTabs a[href="#comment"]').click(function (e) {
		e.preventDefault();
        if(jQuery.trim($("#commentPanel").html()).length < 10){
              var commentUrl = '/async/main/comments';
              jQuery.ajax({
                  type : "POST",
                  url : commentUrl,
                  cache : false,
                  success : function(data) {
                      $("#commentPanel").html(data);
                      $(this).tab('show');
                  },             
                  error : function(xhr, status, error) {
                      alert('오류가 발생하였습니다. \n 다시 시도해주시거나 운영자에게 문의해 주세요!');
                  }
             });         
          } 
          
    });

	$('#myTabs a[href="#dbpost"]').click(function (e) {
        e.preventDefault();
        if(jQuery.trim($("#dbpostPanel").html()).length < 10){
            var dbpostUrl = '/async/main/posts';
            var parameters = 'rssType=db';
            jQuery.ajax({
                type : "POST",
                url : dbpostUrl,
                data : parameters,
                cache : false,
                success : function(data) {
                    $("#dbpostPanel").html(data);
                    $(this).tab('show');
                },             
                error : function(xhr, status, error) {
                    alert('오류가 발생하였습니다. \n 다시 시도해주시거나 운영자에게 문의해 주세요!');
                }
           });         
        }        
    });
	
	$('#myTabs a[href="#devpost"]').click(function (e) {
        e.preventDefault();
        if(jQuery.trim($("#devpostPanel").html()).length < 10){
            var devpostUrl = '/async/main/posts';
            var parameters = 'rssType=dev';
            jQuery.ajax({
                type:"POST",
                url:devpostUrl,
                data:parameters,
                cache : false,
                success : function(data) {
                    $("#devpostPanel").html(data);
                    $(this).tab('show');
                },             
                error : function(xhr, status, error) {
                    alert('오류가 발생하였습니다. \n 다시 시도해주시거나 운영자에게 문의해 주세요!');
                }
           });         
        }
        
    });
	
	$('#myTabs a[href="#studyArticle"]').click(function (e) {
        e.preventDefault();
        if(jQuery.trim($("#studyArticlePanel").html()).length < 10){
	        var studyArticleUrl = '/async/main/articles';        
	        jQuery.ajax({
	            type:"POST",
	            url:studyArticleUrl,
	            cache : false,
	            success : function(data) {
	                $("#studyArticlePanel").html(data);
	                $(this).tab('show');
	            },             
	            error : function(xhr, status, error) {
	                alert('오류가 발생하였습니다. \n 다시 시도해주시거나 운영자에게 문의해 주세요!');
	            }
	       });         
        }
    });
	
	$('#replyCommentLayout').on('show.bs.modal', function(e) {		
		$("#mainCommentGroupSeq").val($(e.relatedTarget).data('commentseq'));		
	});
	
	$(document).on("click","#mainCommentList .btn",function() {		
		if($(this).text()=='삭제'){			
			if(confirm('댓글을 삭제 하시겠습니까?')){
                var commentSeq = $(this).data('commentseq');                
	            var removeUrl = '/async/maincomment/remove';
	            var parameters = 'commentSeq='+commentSeq;
	            jQuery.ajax({
	                type:"POST",
	                url:removeUrl,
	                data:parameters,
	                cache : false,
	                success : function(data) {
	                    $("#mainCommentList").html(data);
	                    alert('댓글이 삭제 되었습니다!');
	                },             
	                error : function(xhr, status, error) {
	                    alert('오류가 발생하였습니다. \n 다시 시도해주시거나 운영자에게 문의해 주세요!');
	                }
	           });	            
	        }else{
	            return false;
	        }
		}
	});
	
	$(document).on("click","#addMainComment",function() {
        
    	var content = encodeURIComponent($("#mainContent").val());
    	var ocontent = $("#mainContent").val();
    	
    	 if(jQuery.trim(ocontent).length < 5){
    		 alert('내용을 다섯자 이상 입력해 주세요!');
    		 $("#mainContent").focus();
             return false;
    	 }else if(jQuery.trim(ocontent).length > 140){
             alert('내용을 140자 이하로만 입력 가능합니다!');
             $("#mainContent").focus();
             return false;
         }else if(confirm('등록 하시겠습니까?')){

            var writeUrl = '/async/maincomment/write';
            var parameters = 'content='+content;
            
            jQuery.ajax({
                type:"POST",
                url:writeUrl,
                data:parameters,
                cache : false,
                success : function(data) {                	
                	$("#mainContent").val('');
                	$("#mainCommentList").html(data);                    
                },             
                error : function(xhr, status, error) {
                	alert('오류가 발생하였습니다. \n 다시 시도해주시거나 운영자에게 문의해 주세요!');
                }
           });            
            
        }else{
            return false;
        }            
     }); 
    
	$(document).on("click","#replyMainComment",function() {    
    	var content = encodeURIComponent($("#recontent").val());
    	var ocontent = $("#recontent").val();
        
        if(jQuery.trim(ocontent).length < 5){
            alert('내용을 다섯자 이상 입력해 주세요!');
            $("#recontent").focus();
            return false;
        }else if(jQuery.trim(ocontent).length > 140){
            alert('내용을 140자 이하로만 입력 가능합니다!');
            $("#recontent").focus();
            return false;
        }else if(confirm('등록 하시겠습니까?')){
           var writeUrl = '/async/maincomment/reply';
           var parameters = 'content='+content+'&groupSeq='+$("#mainCommentGroupSeq").val()+'&curPage='+$("#mainCommentCurPage").val();           
           jQuery.ajax({
               type:"POST",
               url:writeUrl,
               data:parameters,
               cache : false, 
               success : function(data) {    
            	   $('#replyCommentLayout').modal('hide');
            	   $("#recontent").val('');
                   $("#mainCommentList").html(data);
               },             
               error : function(xhr, status, error) {
                   alert('오류가 발생하였습니다. \n 다시 시도해주시거나 운영자에게 문의해 주세요!');
               }
          });           
       }else{
           return false;
       }
    });    
});

</script>
            
