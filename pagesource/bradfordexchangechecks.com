function handleClick(e)
{


        e = e || event;
        var target = e.srcElement || e.target;
        if(target.parentNode.parentNode.className=='recentlyViewedInfo' || target.parentNode.className=='recentlyViewedInfo' || target.parentNode.parentNode.className=='recentlyViewedItem')
		{setCookie("recentlyViewedCM","Recently Viewed",cookieExpDays);}
	if(target.type=='submit' && pageURL.indexOf("/products/")>0)
		{
		storeDataForCartPage();
		}
//	else if(target.type=='submit' && pageURL.search(/\/reorder\/reorderCheck-crossSell.jsp\?action=/)>=0)
//	{
//		storeDataForCartPage();
//	}
	else if(!(typeof target.parentNode.name=='undefined') && target.parentNode.name.indexOf("form")==0)
		{
			if (pageURL.indexOf("reorderCheck-crossSell.jsp")>0){
				storeUpsellDataForReorderPage(target.parentNode.name);
			} else{
				storeUpsellDataForCartPage(target.parentNode.name);
			}
		}
    else if(target.parentNode.parentNode.className=="coordinatedItem")
    	{
    	storeDataForCoordinatedItems(target);
    	}


}

document.body.onclick = handleClick;




