<script>
    
    
    
// open the dialog 

setTimeout(function(){ $("#nombre").focus(function(){
  $("#tip1").attr("aria-hidden","false");
  $("#tip1").removeClass("hidden");
  
  });
    $("#nombre").mouseover(function(){
  $("#tip1").attr("aria-hidden","false");
  $("#tip1").removeClass("hidden");
  });
   // repeat for the keyboard accessibility for all buttons, enter makes it click...
   $("#nombre").blur(function(){
  $("#tip1").attr("aria-hidden","true");
  $("#tip1").addClass("hidden");
  });
  
    $("#nombre").mouseleave(function(){
  $("#tip1").attr("aria-hidden","true");
  $("#tip1").addClass("hidden");
  });
  
  $("#nombre").keydown(function(ev){
  if (ev.which ==27)  {
       $("#tip1").attr("aria-hidden","true");
       $("#tip1").addClass("hidden");
       ev.preventDefault(); 
       return false;
       }
  });},300);
  
 
   </script>          
                      
