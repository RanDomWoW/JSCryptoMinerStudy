jQuery(document).ready(function() {
    var messages = null;
    var regexLogout = new RegExp('\\?logout$');
    var isLogout = regexLogout.exec(window.location.href);
    if (isLogout !== null) {
        messages = {"type":"success","title": "Vous êtes maintenant déconnecté(e)."};
    }
    var regexCart = new RegExp('cart$');
    var isCart = regexCart.exec(window.location.href);
    if(messages != null && messages != ''){
        if(messages.page == 'cart' && isCart == null){
            messages = null;
        }
    }
    if(messages != null && messages != '') {
        var type = messages.type;
        var title = messages.title;
        var fields = messages.fields;
        var content = title;
        if(fields != null) {
            for(var key in fields) {
                content += '<br/> - ' + fields[key];
            }
        }
        Layout.showGrowl(type, content);
    }
});
</script>
