			var input_autocomplete = (document.getElementById('zip-code-field-block-magasins'));

			var options_autocomplete = {
				types: ['(regions)'],
				componentRestrictions: {country: "fr"}
			};

			loadJsMap();

			var execIntval;
			var exec = function() {

				var autocomplete = new google.maps.places.Autocomplete(input_autocomplete, options_autocomplete);

				// place autocomplete
				google.maps.event.addListener(autocomplete, 'place_changed', function () {

					var place = autocomplete.getPlace();
					if (!place.geometry) {
						return;
					}

					// If the place has a geometry, then present it on a map.
					var lat = place.geometry.location.lat();
					var lng = place.geometry.location.lng();

					greyBoxShow('http://www.lavieclaire.com/store/index/popup/livrable/1/lat/' + lat + '/lng/' + lng, 955, 'get', {className: 'gmap-steps'});
				});

				var $golocbtn = jQuery('.quich-search-stores--location');
				$golocbtn.unbind('click');
				$golocbtn.bind('click', function(){
					if(Modernizr.geolocation){
						navigator.geolocation.getCurrentPosition(function(position){
							greyBoxShow('http://www.lavieclaire.com/store/index/popup/livrable/1/lat/' + position.coords.latitude + '/lng/' + position.coords.longitude, 955, 'get', {className: 'gmap-steps'});
						});
					}
				});
			};
			execIntval = setInterval(function(){
				if (!(typeof(google) == 'undefined' || typeof(google.maps) == 'undefined')) {
					clearInterval(execIntval);
					exec();
				}
			}, 250);
		</script>
