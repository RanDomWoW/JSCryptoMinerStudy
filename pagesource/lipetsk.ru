function ytVidId(url) {
	error = 0;
  var p = /^(?:https?:\/\/)?(?:www\.)?(?:youtu\.be\/|youtube\.com\/(?:embed\/|v\/|watch\?v=|watch\?.+&v=))((\w|-){11})(?:\S+)?$/;
  var regexp = /(ftp|http|https):\/\/(\w+:{0,1}\w*@)?(\S+)(:[0-9]+)?(\/|\/([\w#!:.?+=&%@!\-\/]))?/;
  if (regexp.test(url) == false)
  {
	error++;
  }
  if(url.match(p) == false)
  {
	error++;
  }
  if(error == 0)
  {
	return true;
  }
  else
  {
	return false;
  }
