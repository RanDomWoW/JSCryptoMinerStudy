    <script type="text/javascript">
        function CreateFailure() {
            //AceAlert("#Messages", "error", "خطا ! دوباره تلاش نمایید .", null, false, false);
            AceAlert("#Messages", "error", "خطا ! دوباره تلاش نمایید", null, false, false);
        };
        function CreateSucces(data) {
            if (data.error == 1) {
                window.location = "/fa/DashBoard/Default";
            }
            else {
                AceAlert("#Messages", "error", data.message, null, true, false);
            }
        };
        function ResendEmail(email) {
            $("#Loading").css("display", "block");
            $.ajax({
                url: '/fa/Account/ResendEmail',
                data: { email: email},
                type: "POST",
                success: function(data) {
                    if (data.error == 1) {
                        //AceAlert("#Messages", "success", "یک نامه حاوی لینک فعال سازی به ایمیل شما ارسال شد", null, false, false);
                        AceAlert("#Messages", "success", "", null, false, false);
                        //AceAlert("#Info", "info", "شما برای استفاده از حساب کاربری نیاز به فعال کردن نام کاربری خود دارید . در صورتی که ایمیل خود را صحیح وارد کردید به ایمیل خود مراجعه کنید و روی لینک فعال سازی کلیک کنید .", null, false, false);
                        AceAlert("#Info", "info", "شما برای استفاده از حساب کاربری نیاز به فعال کردن نام کاربری خود دارید . در صورتی که ایمیل خود را صحیح وارد کردید به ایمیل خود مراجعه کنید و روی لینک فعال سازی کلیک کنید", null, false, false);
                        $("#Loading").css("display", "none");
                    } else {
                        AceAlert("#Messages", "error", data.message, null, false, false);
                        $("#Loading").css("display", "none");

                    }
                },
                error: function(reponse) {
                    //AceAlert("#Messages", "error", "خطا ! دوباره تلاش نمایید .", null, false, false);
                    AceAlert("#Messages", "error", "خطا ! دوباره تلاش نمایید", null, false, false);
                }
            });
        }
    </script>

    <script type="text/javascript">
        jQuery(function ($) {
