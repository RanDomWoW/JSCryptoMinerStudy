     <script type="text/javascript">                                     

       var map; 
       var infoWindow;

       function plotQuake(map, lat, long, regiao, magnitude, prof, dia, tnt , bombas) {         
       
       //var url= "http://localhost/painel_global/satmap_terremotos.php?mapa=" + dia ;
       
       var url= "http://www.painelglobal.com.br/satmap_terremotos.php?mapa=" + dia ;
          
          // Define o ícone de acordo com a magnitude
	    if ( magnitude > 8 ){
   	      // Alvo vermelho grande
	      var image = new google.maps.MarkerImage('http://www.painelglobal.com.br/imagens/mapa_global_m8.gif',
	      new google.maps.Size(133, 133),      
	      new google.maps.Point(0, 0),
	      new google.maps.Point(66, 66))
	      }
      
	      else if (magnitude > 7){
   	      // Alvo vermelho   	            
	      var image = new google.maps.MarkerImage('http://www.painelglobal.com.br/imagens/mapa_global_m7a.gif',
	      new google.maps.Size(70, 70),      
	      new google.maps.Point(0, 0),
	      new google.maps.Point(35, 35))
	      }      

   	      else {    
   	      // Alvo azul   	      
	      var image = new google.maps.MarkerImage('http://www.painelglobal.com.br/imagens/mapa_global_m7.gif',
	      new google.maps.Size(70, 70),      
	      new google.maps.Point(0, 0),
	      new google.maps.Point(35, 35))
      	      }    

         
	  // plota o ícone
             var point = new google.maps.LatLng(lat,long);
             var ponto = new google.maps.Marker({
             position: point,
             map: map,
             icon: image,                 
             optimized: false})


            google.maps.event.addListener(ponto, 'click', function() {
                window.open(url,'janela', 'width=778, height=502, top=20, left=120, scrollbars=yes, status=no, toolbar=no, location=no, directories=no, menubar=no, resizable=no, fullscreen=no, scrollbars=no');
	    });




      }   // Fecha a função



       
       function load() {
       
        var centro_mapa = new google.maps.LatLng(-30.18, -177.71);
        
        var myOptions = {
          center: new google.maps.LatLng(-30.18, -177.71),
          zoom: 2,       
          disableAutoPan: false,
          scaleControl: false,
          mapTypeId: google.maps.MapTypeId.HYBRID
        };

        map = new google.maps.Map(document.getElementById("map"), myOptions);
        
        plotQuake(map,'-30.18', '-177.71','Nova Zelândia, a 103 km de Raoul Islan','5.4','10','22/0307:265.4','1875','0.09375');                              
        
        
        
                  
        
        
              //--------------  EVENTOS ---------
	          google.maps.event.addListener(map, 'mouseup', function(event){
		     latLngStr=event.latLng.lat() + ',' + event.latLng.lng();
		     var latlon = latLngStr.split(",");		 
		     zoom=5;
		  });

		  
              //--------------
              		  
		  
        
      }

