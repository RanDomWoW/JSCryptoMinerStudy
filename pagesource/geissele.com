				if (iOS) {show_vid = false;}

			if (show_vid) {
				jQuery('.slideshow').on('cycle-initialized', function(event) {
					if (jQuery(this).find('.cycle-slide-active video').length) {
						var video = jQuery(this).find('.cycle-slide-active video').get(0).play();
					}
				});
				jQuery('.slideshow').on('cycle-after', function(event, optionHash, outgoingSlideEl, incomingSlideEl, forwardFlag) {
					if (jQuery(outgoingSlideEl).find('video').length) {
						var video = jQuery(outgoingSlideEl).find('video').get(0).pause();
					}
					if (jQuery(incomingSlideEl).find('video').length) {
						var video = jQuery(incomingSlideEl).find('video').get(0).play();
					}
				});
			} else {
				jQuery('.slideshow video').hide();
				jQuery('.slideshow a').height(jQuery('.slideshow').height());
			}
		</script>
