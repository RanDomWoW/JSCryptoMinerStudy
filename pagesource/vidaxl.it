<script type="text/javascript">
    var menuDelay;
    $('#choose-category').hover( function(){
        menuDelay = setTimeout("$('#categories-popup').show()",300);
    }, function() {
        if(menuDelay){
            clearTimeout(menuDelay);
        }
    });
    $("#main-header .logo, #search, #sticky-header").hover(function(){
        $('#categories-popup').hide();
    });
    $('#categories-popup').mouseleave(function(){
        $('#categories-popup').hide();
    });
    function setNewCategory() {
        var index = 1, liNumb = 27;
        $('#categories-popup .left-list li a').each(function() {
            $(this).find('span').before("<span class='icon'></span>");
            $(this).find('.icon').css({backgroundPosition : "-" + liNumb + 'px' + ' 0px'});
            liNumb += 27;
        });

        $('.level-top-container:first-child').addClass('border');
        var $menu = $("#categories-popup .left-list");
        $menu.menuAim({
            activate: function(li){
                $('#categories-popup .left-list li').removeClass('border');
                $('.submenu').hide();
                var submenuId = $(li).attr('data-submenu-id');
                $('#'+ submenuId).show();
                $(li).addClass('border');
            },
            deactivate: function(li){
                var submenuId = $(li).attr('data-submenu-id');
                $('#'+ submenuId).hide();
                $(li).removeClass('border');
            }
        });
    }
    $(function(){
        if ($(window).width() > 780)
        {
            $('#topmenu').addClass('topmenu-position');
            setNewCategory();
        }
    });

    $(window).resize(function() {
        if ($(window).width() > 780) {
            $('#topmenu').addClass('topmenu-position');
        } else {
            $('#topmenu').removeClass('topmenu-position');
        }
    });
</script>
