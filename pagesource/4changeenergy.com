    <script type="text/javascript">
        function getPageType2(pageName) {
            var pageType = "";
            if (pageName === "home") {
                pageType = "home";
            } else if (pageName.indexOf("plans") > -1) {
                pageType = "plans_and_pricing_page";
            } else if (pageName.indexOf("step1") > -1) {
                pageType = "step_1";
            } else if (pageName.indexOf("step3") > -1) {
                pageType = "step_3";
            } else if (pageName.indexOf("complete") > -1) {
                pageType = "completion_page";
            } else if (pageName.indexOf("confirmation") > -1) {
                pageType = "confirmation_page";
            } else if (pageName.indexOf("causes") > -1) {
                pageType = "causes";
            } else if (pageName.indexOf("questions") > -1) {
                pageType = "faqs";
            } else if (pageName.indexOf("account-login") > -1) {
                pageType = "account_login";
            } else if (pageName.indexOf("billing-usage") > -1) {
                pageType = "account_summary";
            } else if (pageName.indexOf("account-settings") > -1) {
                pageType = "account_settings";
            } else if (pageName.indexOf("my-4friends-referrals") > -1) {
                pageType = "4friends_referral_program";
            } else if (pageName.indexOf("payment-details") > -1) {
                pageType = "payment_methods";
            } else if (pageName.indexOf("manage-my-service") > -1) {
                pageType = "manage_service";
            }
            return pageType;
        };

        function setPlansAndSkusUTags() {
            if (window.location.pathname.indexOf("personalized-plans-and-rates") >= 0) {
                $('.plan2 h2').each(function (i, e) {
                    utag_data.plan_names_on_page.push(e.innerHTML);
                });
                $('.choose a').each(function (i, e) {
                    utag_data.plan_skus_on_page.push(e.attributes["productcode"].textContent);
                });
            }
        }

        function getPageName2() {
            var pathName = window.location.pathname.substring(1);
            var pageName;
            if (pathName === "") {
                pageName = "home";
            } else {
                if (pathName.indexOf("/") < 0 && pathName.indexOf("en/") < 0) {
                    pageName = pathName;
                } else {
                    var parts = pathName.substring(0).split("/");
                    pageName = parts[1];
                }
            }

            return pageName;
        }

        function setUTag2(zip, acctNum, planNames, planSkus, planTdu) {
            utag_data.page_name = getPageName2();
            utag_data.page_type = getPageType2(utag_data.page_name);
            utag_data.customer_zip = zip;
            utag_data.customer_id = acctNum;
            utag_data.plan_names_on_page = planNames;
            utag_data.plan_skus_on_page = planSkus;
            utag_data.plan_tdu = planTdu;
        };

        function setUTags() {
            var planNames = [];
            var planSkus = [];
            var planTdu = "";
            var zip = "";
            var acctNum = "";
            var products;

            var hasSession = 'True';
            if (hasSession === "True") {

                var isEnrollmentCustomer = 'False';
                if (isEnrollmentCustomer === "True") {
                    zip = '';
                }

                var isActiveCustomer = 'False';
                if (isActiveCustomer === "True") {
                    acctNum = '';
                }

                var hasLightup = 'True';
                if (hasLightup === "True") {
                    planTdu = '';
                    var prods = '[]';
                    var prodsFixup = prods.replace(/\"TDU\"/g, "");
                    products = JSON.parse(prodsFixup);
                    for (var i = 0; i < products.length; i++) {
                        planNames.push(products[i]._productName);
                        planSkus.push(products[i]._productCode);
                    };
                }

                var isLoggedIn = 'False';
                if (isLoggedIn === "True") {
                    acctNum = '';
                    zip = '';
                    planTdu = '';
                }
            }

            setUTag2(zip, acctNum, planNames, planSkus, planTdu);
        };

        (function () {
            var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
            po.src = 'https://apis.google.com/js/plusone.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);

            var po2 = document.createElement('script'); po2.type = 'text/javascript'; po2.async = true;
            if (-1 < window.location.hostname.toLowerCase().indexOf("www.4changeenergy.com")) {
                //-1 < window.location.hostname.toLowerCase().indexOf("www.myexpressenergy.com")) {
                po2.src = 'https://cdn.optimizely.com/js/3193360324.js';
            } else {
                po2.src = 'https://cdn.optimizely.com/js/3263630133.js';
            }
            s.parentNode.insertBefore(po2, s);
        })();

        setErrorTags = function() {
            if (document.location.pathname.indexOf("/enroll/step1-service-info") > -1 || document.location.pathname.indexOf("/enroll/step3-payment") > -1) {
                if ($('#pnlErr').length && ($('#errorMessageList li').length || $('#lblError').length)) {
                    var errorMsg = $('#errorMessageList li').text() || $('#lblError').text() || '';
                    var eventLabel = 'OTHER_ERROR_Msg';
                    try {
                        if (errorMsg.indexOf('PRTPIN') > 0) {
                            eventLabel = '30Day_ESIID';
                        } else if (errorMsg.indexOf('2001') > 0) {
                            eventLabel = '12Month_Experian-PIN';
                        } else if (errorMsg.indexOf('4001') > 0) {
                            eventLabel = 'PastDue';
                        } else if (errorMsg.indexOf('6001') > 0) {
                            eventLabel = 'BadDebt';
                        } else if (errorMsg.indexOf('8001') > 0) {
                            eventLabel = 'PINEmail';
                        } else if (errorMsg.indexOf('9001') > 0) {
                            eventLabel = '1Customer_ADD';
                        }
                    }
                    catch(exception) {}
                    utag.link({
                        ga_event_category: "Fraud Table",
                        ga_event_label: eventLabel,
                        ga_event_action: "Error_popup"
                    });
                }
            }            
        }

    </script>
	
