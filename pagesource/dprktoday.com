function sel_imo(nos){
	var item;
	for(i = 1; i <= 7; i++){
		item = document.getElementById("imo" + i);
		if(i == parseInt(nos)){
			item.style.border = "2px solid green";
			item.style.padding = "1px";
			item.width="26";
			item.height="26";
		}else{
			item.style.border = "none";
			item.width="23";
			item.height="23";
		}
	}
	document.getElementById("imo").value = nos;
}
