function ShowServerTime(h,m,s)
{
	s = s + 1;
	if( s < 10)
	{
	  s = "0" + s;
	} 
	if(s >= 60)
	{
		s = "0" + 0;
		m = m + 1;
	}
	if( m < 10)
	{
		m = "0" + m;
	}
	if(m >= 60)
	{
		m = "0" + 0;
		h = h + 1;
		if(h >= 24)
		{
			h = 0;
		}
	}
				my_time.innerHTML = "<font color='#000bf8' style='font-size:10pt;' >" + "평양시간" + "&nbsp" + h + ":" + m + ":" + s +"</font>";  
								 
	setTimeout("ShowServerTime(" + h + "," + m + "," + s + ")",1000);
}
