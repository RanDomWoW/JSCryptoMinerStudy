function fsearch_submit(f)
{
	if (f.stx.value.length < 2) 
	{
		alert("검색어는 두글자이상 입력해야 합니다.");
		f.stx.select();
		f.stx.focus();
		return false;
	}
	// 검색에 많은 부하가 걸리는 경우 이 주석을 제거
	var cnt = 0;
	for (var i=0; i<f.stx.value.length; i++) 
	{
		if (f.stx.value.charAt(i) == ' ')
			cnt++;
	}

	if (cnt > 1) 
	{
		alert("검색어에 공백은 한개만 입력할수 있습니다.");
		f.stx.select();
		f.stx.focus();
		return false;
	}
}
