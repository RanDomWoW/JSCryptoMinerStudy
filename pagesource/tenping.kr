                    var addHeight = $("#facebookLayer").contents().find(".area-keyword").height();

                  //  $("#layerFacebook").css({ 'height': (730 + addHeight) + "px", "margin-top": -(730 + addHeight)/2 });
                }
            }
        } catch (e) {
            $("#facebookLayer").contents().find(".area-keyword").hide();
        }


            _elem.contents().find("#picture").attr("src", image);

