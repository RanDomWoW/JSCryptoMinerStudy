    //게시하기
    function fn_facebook_post() {
        uiModalClose("Modal_Facebook_Confirm");

        var msg = $("#message").val();

        var channel = $("#facebookLayer").contents().find("#postChannel").val();
        var channelName = $("#facebookLayer").contents().find("#postChannel option:selected").text();
        var privacy = { "value": $("#facebookLayer").contents().find("#openChannel").val() };
        var description = _meta_description;
        var image = _facebook_Info;

        var content = $("#facebookLayer").contents().find("#u_0_11").val();



        var url = channel == "self" ? "/me/feed" : "/" + channel + "/feed";
        if (_facebookType == "cpv") {
            url = channel == "self" ? "/me/videos" : "/" + channel + "/videos";
        }

        var fbContent = {
            //enum{'EVERYONE', 'ALL_FRIENDS', 'FRIENDS_OF_FRIENDS', 'CUSTOM', 'SELF'}
            message: content,
            link: _Invite_Url,
            fbID: channel == "self" ? userID : channel,
            MemberID: "",
            CampaignID: _CampaignID,
            CampaignType: _Campaign_Type
        };


        if (channel != "self") {
            for (var i = 0 ; i < pageData.data.length; i++) {
                if (pageData.data[i].id == channel) {
                    fbContent.access_token = pageData.data[i].access_token;
                    break;
                }
            }
        } else {
            fbContent.access_token = accessToken;
            fbContent.privacy = privacy;
        }

        if (_facebookType == "cpv") {
            fbContent.file_url = _Campaign_Landing_Url;
            fbContent.description = content;
            var fbID = channel == "self" ? userInfo.id : channel;



            facebookPostingCheck(fbID, url, fbContent, channel, channelName, false);
        } else {
            facebookPosting(url, fbContent, channel, channelName);
        }
    }

    function facebookPostingCheck(fbID, url, fbContent, channel, channelName, IsMobile) {
        var jData = {};

        jData.MemberID = "";
        jData.CampaignID = _CampaignID;
        jData.FbId = fbID;

        $.ajax({
            url: "/Home/Facebook_Posting_Cnt_Check",
            type: "POST",
            data: jData,
            dataType: "JSON",
            success: function (data) {

                if (IsMobile == false) {
                    if (data.ResultCode < 2) {
                        facebookPosting(url, fbContent, channel, channelName);
                    } else {
                        alert("같은 소문에 대해 일일 최대 2회까지 포스팅 할 수 있습니다");
                        uiModalClose("layerFacebook");
                    }
                } else {
                    var checkResult = {};
                    checkResult.IsAble = data.ResultCode < 2;
                    checkResult.msg = "같은 소문에 대해 일일 최대 2회까지 포스팅 할 수 있습니다";

                    if (isANDROID) {
                        window.Android.postFacebookCPVCheck(JSON.stringify(checkResult, null, 4));
                    } else if ("N" == "Y") {

                    }
