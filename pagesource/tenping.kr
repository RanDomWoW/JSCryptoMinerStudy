        _elem.contents().find("#caption").html(extractHostname(_Invite_Url));
        _elem.contents().find("#description").html(fn_subString(_meta_description, 0, 200));

        try {
            var essential_words = _Essential_Word.split("∫");
            var target = "#u_0_11";
            var wordCount = 0;
            $("#facebookLayer").contents().find("#essential_words").empty();
            for (var word in essential_words) {
                if (essential_words[word].length > 0) {
                    $("#facebookLayer").contents().find("#essential_words").append("<li onClick=\"parent.pasteWord('" + target + "','" + essential_words[word] + "')\" style=\"cursor:pointer;\">" + essential_words[word] + "</li>");
                    wordCount++;
                }
            }
            if (wordCount == 0) {
