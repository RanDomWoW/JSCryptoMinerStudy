    $(document).on('click', '.select-wrap > span, .select-wrap > strong', function () {        
        var p = $(this).parent();
        var ul = p.find('>ul');
        var r = p.width() - $(this).outerWidth();
        var bg = $('<div class="select-bg"></div>');
        if (p.hasClass('open')) {
            p.removeClass('open');
            $('.select-bg').remove();
        } else {
            p.addClass('open').after(bg);
            ul.css('right', (r + 5) + 'px');
        }
        ul.click(function () {
            p.removeClass('open');
            $('.select-bg').remove();
        });
        $('.select-bg').click(function () {
            p.removeClass('open');
            $('.select-bg').remove();
        });
    });

