                    for (i = 0; i < arrayList.length; i++) {
                        buildString = buildString + "<li><button type=\"button\" onclick=\"javascript:fn_SearchHistoryClick('" + arrayList[i] + "');\">" + arrayList[i] + "</button>";
                        buildString += "<span class=\"form-delete\"><button type=\"button\" class=\"form-btn-delete04\" onclick=\"javascript:fn_historyDelete(this);\">작성한 검색어 지우기</button></span>";
                        buildString += "</li>";
                    }
                    $(".form-recent").html(buildString);
                }
            }
        }

        function fn_Auto_Search_List() {
            var SearchWord = $("#txtSearchWord").val();
            var SearchList = getCookie("GNB_Search_List");
            if (SearchList == "undefined" || SearchList == "" || SearchList == null) {
                $("#Auto_Search_History").html("");
            } else {
                var buildString = "";
                var arrayList = SearchList.split('|');

                if (arrayList.length == 0) {
                    $("#Auto_Search_History").html("");
                } else {
                    var i = 0;
                    var k = 0;
                    for (i = 0; i < arrayList.length; i++) {
                        if (k == 5) {
                            break;
                        }
                        if (arrayList[i].indexOf(SearchWord) >= 0) {
                            buildString = buildString + "<li><button type=\"button\" onclick=\"javascript:fn_SearchHistoryClick('" + arrayList[i] + "');\">" + arrayList[i] + "</button></li>";
                            k = k + 1;
                        }
                    }
                    $("#Auto_Search_History").html(buildString);
                }
            }
        }

        function fn_SearchHistoryClick(val) {
            var nation_code = "";
            var category_code = "0";
            var campaign_type = "2047";
            var ordertype = "";
            if (typeof ($(":radio[name=Sort_Select]:checked").val()) != "undefined") {
                ordertype = $(":radio[name=Sort_Select]:checked").val();
            }
            var searchword = val;
            var favoriteStatus = $("#hdFavoriteStatus").val();

            var queryString = fn_SearchQuerystring(nation_code, category_code, campaign_type, ordertype, encodeURIComponent(searchword), favoriteStatus);

            location.href = "/Home/List?" + queryString;
        }

        function fn_historyDelete(e) {
            var gnb = getCookie("GNB_Search_List").split('|');
            var temp = "";
            if (gnb.length > 0) {
                for (var i = 0; i < gnb.length; i++) {
                    if (gnb[i] == $(e).parent().parent().children(0)[0].innerText) {
                        $(e).parent().parent().remove();
                        if (gnb.length == 1) $(".form-norecent").addClass('show')
                    }
                    else {
                        if (temp != "") temp += "|";
                        temp += gnb[i];
                    }
                }
            }
            else {
                $(".form-recent").html("");
            }
            setCookie("GNB_Search_List", temp, 3650);
        }

        function fn_SearchQuerystring(nation_code, category_code, campaign_type, ordertype, searchword, favoriteStatus) {
            var queryString = "";
            var sort_value = "8001";
            var member_grade = "0";

            if (nation_code != "") {
                queryString = queryString + (queryString != "" ? "&" : "") + "Nation_Code=" + nation_code;
            }
            if (category_code != "" && nation_code == "") {
                queryString = queryString + (queryString != "" ? "&" : "") + "Campaign_Category=" + category_code;
            }
            if (campaign_type != "") {
                queryString = queryString + (queryString != "" ? "&" : "") + "CampaignType=" + campaign_type;

                setCookie("C_CampaignType", campaign_type, 1);

                if (member_grade == 0)
                    sort_value = "8001";
                else {
                    if (campaign_type == "2047")
                        sort_value = "8001";    //전체 : 최신 순
                    else if (campaign_type == "1153")
                        sort_value = "8002";    //클릭 : 단가 순
                    else if (campaign_type == "288")
                        sort_value = "8001";    //재생 : 최신 순
                    else if (campaign_type == "8")
                        sort_value = "8004";    //설치 : 설치 수 순
                    else if (campaign_type == "578")
                        sort_value = "8005";    //참여 : 승인율 순
                    else
                        sort_value = "8001";    //그 외 : 최신 순
                }

                setCookie("C_SortName", sort_value, 1);
            }
            if (ordertype != "") {
                queryString = queryString + (queryString != "" ? "&" : "") + "FavoriteStatus=" + favoriteStatus;
                setCookie("C_SortName", ordertype, 1);
            }
            if (searchword != "") {
                queryString = queryString + (queryString != "" ? "&" : "") + "SearchWord=" + searchword;
            }

            return queryString;
        }

        $("#btnCpsurl").click(function () {
            btnCpsurl_Click();
        });

        $("#btn_cpsdelete").click(function () {
            $('#CPS_Url').val('').focus();
        });

        $(".ui-btn").click(function () {
            var AdvertiserID = $(this).attr("advertiserid");
            $(".child" + AdvertiserID).toggle();
            var parent = $(this).parent();

            if (parent.hasClass("open")) {
                parent.removeClass("open");
                $(this).removeClass("ui-btn-open");
            } else {
                parent.addClass("open");
                $(this).addClass("ui-btn-open");
            }
        });

        //var content_more_text = $("#contents-more").html();

        

        var oneClick = true;
        var CPS_CopyMessage = "";

        function btnCpsurl_Click() {
            var cpsUrl = $("#CPS_Url").val();
            if (cpsUrl.indexOf("https") != -1) {
                cpsUrl = cpsUrl.replace("https://", "http://");
            }
            var hasLogin = "True";

            if (cpsUrl.trim().length == 0) {
                alert("소문낼 상품 URL을 입력해주세요");
                return;
            }

            if (cpsUrl == _targetUrl) {
                alert("이미 소문내기 URL이 생성된 상품입니다");
                return;
            }
            //생성한 URL 담아두기
            _targetUrl = cpsUrl;

            if (hasLogin == "True") {
                location.href = "/Member/Send_Campaign_Login?SearchUrl=" + encodeURI($("#CPS_Url").val()) + "ReturnUrl=" + encodeURI("/Home/List?CampaignType=4") + "&CampaignID=0&CampaignType=4&DistributionType=SearchUrl";
                return;
            }
            if (cpsUrl == "") {
                alert("소문낼 상품 URL을 입력해주세요.");
            }

            if (cpsUrl != "") {
                cpsUrl = encodeURIComponent(cpsUrl);

                $("#loading").show();

                $.ajax({
                    url: "/Home/CPS_Url_Create",
                    type: "Post",
                    data: {
                        CPS_Url: cpsUrl,
                    },
                    beforeSend: function (jqXHR, opts) {
                        //전송중일때
                        if (!oneClick) {
                            jqXHR.abort();
                        }
                        else { //전송중이 아닐때
                            oneClick = false;
                        }
                    },
                    success: function (data) {
                        if (data.Result == 1) {
                            CPS_CampaignID = data.campaignID;
                            CPS_Campaign_Name = data.campaign_Name;
                            CPS_Campaign_Memo = data.campaign_Memo;
                            CPS_Googlge_Url = data.googlge_Url;
                            CPS_Short_Url = data.short_Url;
                            CPS_SendMessage = data.sendMessage;
                            CPS_CopyMessage = data.copyMessage;
                            CPS_ImgSrc = data.campaign_Images_Select[0].Image_Url;
                            CPS_ImgWidth = data.campaign_Images_Select[0].Image_Width;
                            CPS_ImgHeight = data.campaign_Images_Select[0].Image_Height;
                            CPS_Image_Url_Server = data.campaign_Images_Select[0].Image_Url_Server;
                            CPS_Image_Type = data.campaign_Images_Select[0].Image_Type;

                            distributionChar = CPS_Short_Url;
                            campaign_Name = CPS_Campaign_Name;
                            campaign_Memo = CPS_Campaign_Memo;
                            invite_Url = CPS_Googlge_Url;
                            kakao_url = data.kakao_Url;
                            facebook_Info = "http:" + CPS_Image_Url_Server + "?RS=" + CPS_ImgWidth + "x" + CPS_ImgHeight;
                            image_Info = "http:" + CPS_Image_Url_Server + "?RS=320x213";

                            _CampaignID = data.campaignID;
                            _Campaign_Name = CPS_Campaign_Name;
                            _Campaign_Memo = data.campaign_Memo;
                            _meta_title = CPS_Campaign_Name;
                            _meta_description = CPS_Campaign_Memo;
                            _image_Info = image_Info;
                            _facebook_Info = facebook_Info;
                            _Invite_Url = CPS_Googlge_Url;
                            _DistributionChar = CPS_Short_Url;
                            _Short_Url = CPS_Short_Url;
                            _kakao_url = data.kakao_Url;
                            _kakao_message = "(광고)지만 유익한 정보" + "\n\n" + _Campaign_Name + "\n\n" + _Campaign_Memo + "\n\n" + "이용문의" + " : https://goo.gl/C5b87K" + "\n" + "수신거부" + " : " + "친구차단";

                            SNS_Share_Make();
                        }
                        else if (data.Result == 800) {
                            //미로그인
                            location.href = "/Member/Send_Campaign_Login";
                            return;
                        }
                        else if (data.Result == 900) {
                            alert("소문내기를 지원하지 않는 페이지입니다.");
                            return;
                        }
                        else if (data.Result == 600) {
                            alert("제휴 쇼핑몰의 상품이 아닙니다. \n상품 URL을 확인해주세요.");
                            return;
                        }
                        else {
                            alert("소문내기를 지원하지 않는 페이지입니다.");
                        }
                    },
                    error: function () {
                        alert("소문내기를 지원하지 않는 페이지입니다.");
                    },
                    complete: function (results) {
                        oneClick = true;
                        $("#loading").hide();
                    }
                });
            }
        }

        function SNS_Share_Make() {
            $("#cps_sns_memo").val(CPS_CopyMessage);
            $("#div_cps_sns_share").show();
        }

        var standalone = window.navigator.standalone,
        userAgent = window.navigator.userAgent.toLowerCase(),
        safari = /safari/.test(userAgent),
        ios = /iphone|ipod|ipad/.test(userAgent);
        var isVisibleScrollToUp = true;

        if (ios) {
            if (!standalone && safari) {
                //browser
                $('#liCPI').show();
            } else if (standalone && !safari) {
                //standalone
                $('#liCPI').show();
                //$('#liCPI').hide();
            } else if (!standalone && !safari) {
                //uiwebview
                isVisibleScrollToUp = false;
                $(".btn-top").hide();
            };
        } else {
            //not iOS
            $('#liCPI').show();
        };

    </script>

    

