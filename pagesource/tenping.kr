    //페이스북 로그인 체크
    function fbLogin() {
        FB.login(function (response) {
            if (response.status == "connected") {
                doNextInLogin(response);
            } else {
                alert("페이스북 로그인에 실패하였습니다 로그아웃 후 다시 시도해주세요");
            }
        }, {
            scope: 'publish_actions,user_posts,publish_pages,manage_pages,publish_stream',
            return_scopes: true,
            auth_type: 'rerequest'
        });
    }

    //로그인 처리
    function doNextInLogin(response) {
        if (typeof response.authResponse != 'undefined') {
            accessToken = response.authResponse.accessToken;
            userID = response.authResponse.userID;

            FB.api('/me?fields=id,name,gender,email,picture', function (response) {
                userInfo = response;
                getFacebookPageList();
            });
        } else {
            alert("페이스북 로그인에 실패하였습니다");
        }
    }

    var pageData;
    function getFacebookPageList() {
        FB.api(
            "/me/accounts",
            function (response) {
                if (response && !response.error) {
                    pageData = response;
                }
                //페이스북 공유 레이어 팝업 호출
                send();
            }
        );
    }

