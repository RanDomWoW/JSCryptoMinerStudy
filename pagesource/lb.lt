                </div>
                                        
                                <div id="youtube_play" data-videoid="tRrHvDNOQfI"></div>
                    </div>
    </div>
    <div class="video-buttons container-fluid">
        <a class="vid_button stop btn btn-default" href="" title="Suskleisti" style="display: none;"><i class="fa fa-step-forward"></i>  Suskleisti</a>
        <a class="vid_button play btn btn-default" href="" title="Video" style="display: none;"><i class="fa fa-video-camera"></i> Išskleisti</a>
        <div class="clear"></div>
    </div>
</div>
<script type="text/javascript">
    function VideoController()
    {
        var that  = this;
        var player;
        var initialized = false;
        var isLoaded = false;
        var cookieName = 'shown_video_2d5d03c9c4801d847fef26cd76d1d38a';

        that.initialize = function()
        {
            if(!initialized)
            {
                initControls();
                if(!isVideoShown())
                {
                    initVideo();
                }
                else
                {
                    $('.vid_button.play').show();
                    $('.vid_button.stop').hide();
                }
            }

            initialized = true;
        };

        that.isInitialized = function() {
            return initialized;
        };

        that.initYouTubeVideo = function () {
            youTubePlayerAPIReady();
        };

        function initControls()
        {
            $('.vid_button.play').on('click', function(){
                if(!isLoaded)
                {
                    initVideo();
                }
                $('.video_box').slideDown(500);
                $('.vid_button.play').hide();
                $('.vid_button.stop').show();

                return false;
            });

            $('.vid_button.stop').on('click', function(){
                if($('#introvideo').length > 0)
                {
                    document.getElementById('introvideo').pause();
                }
                else if($('.flv-video').length>0)
                {
                    window["jw_video"].stop();
                }
                else if($('#youtube_play').length > 0 && player)
                {
                    player.stopVideo();
                }

                $('.video_box').slideUp(500, function(){ $('.first_frame').fadeIn(500) });
                $('.vid_button.stop').hide();
                $('.vid_button.play').show();

                return false;
            });

            $('.first_frame').on('click', function(){
                $(this).fadeOut(500);
                if($('#introvideo').length > 0)
                {
                    document.getElementById('introvideo').play();
                }
                else if($('.flv-video').length>0)
                {
                    window["jw_video"].play();
                }
                else if($('#youtube_play').length > 0 && player)
                {
                    player.playVideo();
                }

                return false;
            });
        }

        function isVideoShown()
        {
            var shownVideo = parseInt(Cookies.get(cookieName));
            return !isNaN(shownVideo);
        }

        function setVideoShown()
        {
            Cookies.set(cookieName, 1, { expires: 365, path: '/' });
        }

        function initVideo()
        {
            var jIntroVideo = $('#introvideo');

            if(jIntroVideo.length > 0)
            {
                jIntroVideo.show();

                jIntroVideo.click(function () {
                    this.paused ? this.play() : this.pause();
                });

                jIntroVideo.bind("ended", function () {
                    hideVideo();
                });
            }
            else if($('.flv-video').length > 0)
            {
                var file = $('.flv-video').data('file');

                window["jw_video"] = jwplayer("video_jw").setup({
                    "file": file,
                    "height": "100%",
                    "width": "100%"
                }).onComplete(function(){
                    hideVideo();
                });
            }
            else if($('#youtube_play').length > 0)
            {
                var tag = document.createElement('script');

                tag.src = "//www.youtube.com/player_api";
                var firstScriptTag = document.getElementsByTagName('script')[0];
                firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
            }

            isLoaded = true;

            $('.vid_button.play').hide();
            $('.vid_button.stop').show();

            setVideoShown();
        }

        function youTubePlayerAPIReady()
        {
            var video_id = $('#youtube_play').data('videoid');

            player = new YT.Player('youtube_play', {
                height: '480',
                width: '853',
                videoId: video_id,
                playerVars: {
                    rel: 0
                },
                events: {
                    onReady: onYouTubePlayerReady,
                    onStateChange: onYouTubePlayerStateChange
                }
            });
        }

        // autoplay video
        function onYouTubePlayerReady(event)
        {
            resizeIframe('.video_box');
        }

        // when video ends
        function onYouTubePlayerStateChange(event)
        {
            if(event.data === 0)
            {
                hideVideo();
            }
        }

        function hideVideo()
        {
            $('.video_box').slideUp(500);
            $('.vid_button.stop').hide();
            $('.vid_button.play').show();
        }
    }

    function onYouTubePlayerAPIReady()
    {
        if(vpc && vpc.isInitialized())
        {
            vpc.initYouTubeVideo();
        }
    }

    var vpc;

    $(function(){
        vpc = new VideoController();
        vpc.initialize();
    });
</script>

