<script>
    //获取消息数
    function updateMsg() {
        var GET_UNREAD_PATH = '/home/message/get-unread-count';
        $.post(GET_UNREAD_PATH, function (data) {
            var $messCount = $('.J_message span');
            if ($messCount.length > 0) {
                $messCount.text(data.count);
            }
        }, 'JSON');
    }
    updateMsg();
    setInterval(function(){
        updateMsg();
    }, 30000);
</script>



    <script type="text/javascript">
