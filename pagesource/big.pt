  <script>
  $(document).ready(function () {
    function mobileAppAlert() {
      if (navigator) {
        if (navigator.userAgent.match(/Android/i) && !readCookie("hasSOAndroid")) {
          var qes = confirm("A App BiGMobile está disponível para o seu dispositivo! Clique OK para aceder à app BiGMobile através do Google Play.");
          var exdate = new Date();
          createCookie("hasSOAndroid", 1, exdate.getDate() + 90);
          if (qes) {
            window.location = "https://play.google.com/store/apps/details?id=pt.bigonline.BiGMobile";
          }
        }
      }
    }
    mobileAppAlert();
  });
