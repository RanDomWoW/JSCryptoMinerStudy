	function cek_login(){
		$('.input_error').removeClass('input_error');
		$('#wrong_login').hide();
		
		var email = $('#login_email').val();
		var password = $('#login_password').val();
		
		var a = '1';
		if($.trim(email) == ''){
			$('#login_email').addClass('input_error');
			a = '0';
		}
		if($.trim(password) == ''){
			$('#login_password').addClass('input_error');
			a = '0';
		}
		if(a == '1'){
			$.ajax({
				type: "POST",
				url: "http://www.acehardware.co.id/ajax/ajax_login",
				data: { email: email, password: password }
			}).done(function( msg ) {
				if(msg == '1'){
					var link = $(location).attr('href');
					window.location = link;
				}
				else{
					$('#wrong_login').show();
				}
			});
		}
		return false;
	}
	
	function change_log(value){
		if(value == '1'){
			$('#login_area').hide();
			$('#subscribe_area').hide();
			$('#register_area').show();
			$('#register_firstname').focus();
			$('#forgot_password_area').hide();
			$('#ui-dialog-title-loginlet').html('Register');
		}
		else if(value == '2'){
			$('#login_area').show();
			$('#subscribe_area').hide();
			$('#register_area').hide();
			$('#forgot_password_area').hide();
			$('#ui-dialog-title-loginlet').html('Login');
		}
		else if(value == '3'){
			$('#login_area').hide();
			$('#subscribe_area').hide();
			$('#register_area').hide();
			$('#forgot_password_area').show();
			$('#ui-dialog-title-loginlet').html('Forgot Password');
		}
		else{
			return false;
		}
		
		$('.holderLabel').hide();
	//	$('input[placeholder]').placeholder();

		if($.browser.msie){ 
			$('input[placeholder]').each(function(){  
				var input = $(this);        
				$(input).val(input.attr('placeholder'));
				$(input).focus(function(){
					if (input.val() == input.attr('placeholder')) {
						input.val('');
					}
				});
			        
				$(input).blur(function(){
					if (input.val() == '' || input.val() == input.attr('placeholder')) {
						input.val(input.attr('placeholder'));
					}
				});
			});
		};

		$('label[for=recaptcha_response_field]').css('top', '-7px');
		
	}

