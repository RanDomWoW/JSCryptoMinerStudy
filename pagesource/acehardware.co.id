		$('#registerbidding_area_inside').hide();
		$('#btn_registerbidding').val('Sending data...');

		var name = $('#registerbidding_name').val();
		var no_telp = $('#registerbidding_notelp').val();
		var no_identitas = $('#registerbidding_noidentitas').val(); 
		var membercard_id = $('#registerbidding_membercardid').val();
                var ace_reward_password = $('#ace_reward_password').val();
		
		var challengeField = $("input#recaptcha_challenge_field").val();
		var responseField = $("input#recaptcha_response_field").val();
                
                $('#registerbidding_name').removeClass('input_error');
                $('#registerbidding_notelp').removeClass('input_error');
                $('#registerbidding_noidentitas').removeClass('input_error');
                $('#registerbidding_membercardid').removeClass('input_error');
                $('#ace_reward_password').removeClass('input_error');
                
                var error = '';
		var a = '1';
		if($.trim(name) == ''){
			$('#registerbidding_name').addClass('input_error');
                        error+='Name field is required \n';
			a = '0';
		}
		if($.trim(no_telp) == ''){
			$('#registerbidding_notelp').addClass('input_error');
                        error+='No telp field is required \n';
			a = '0';
		}
                
                if($.trim(membercard_id) != ''){

                        if($.trim(ace_reward_password) != ''){
                            
                            $.ajax({
				type: "POST",
                                async: false,
				url: "http://www.acehardware.co.id/ajax/cek_rewardid",
				data: { member_id: membercard_id, strPasskey: ace_reward_password, }
				
                            }).done(function( msg ) {
                                
                                if(msg == '0'){
                                        a = '0';
                                        error+='Your ACE Rewards Not Valid! \n';
                                        console.log(a);
					$('#registerbidding_membercardid').addClass('input_error');
                                        
				}
                            });
                            
                        }else{
                            $('#ace_reward_password').addClass('input_error');
                            error+='Password reward is required! \n';
                            a = '0';
                        }

		}

		if(a == '1'){
			$.ajax({
				type: "POST",
				url: "http://www.acehardware.co.id/ajax/ajax_registerbidding",
				data: { name: name, no_telp: no_telp, no_identitas: no_identitas, membercard_id:membercard_id, challengeField: challengeField, responseField: responseField }
				
			}).done(function( ) {
                                $('#registerbidding_form')[0].reset();
                                $('#registerbidding_form').hide();
                                $('#registerbidding_area_inside').html('Register Sukses');
                                $('#registerbidding_area_inside').show();
                                $('#registerbidding_area').show();
				var link = $(location).attr('href');
                                window.location = link;		
			});
		} else {
                        window.alert(error);
 
			$('#btn_registerbidding').val("register");
		}
		
		return false;
	}
	
