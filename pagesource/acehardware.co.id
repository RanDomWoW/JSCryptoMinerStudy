	function cek_forgot_password(){
		$('#wrong_forgot_password').hide();
		$('#suc_forgot_password').hide();
		$('#btn_forgot').val('Sending data...');
		$('.input_error').removeClass('input_error');
		
		var forgot_email = $('#forgot_email').val();
		
		var a = '1';
		if($.trim(forgot_email) == ''){
			$('#forgot_email').addClass('input_error');
			a = '0';
		}
		if(validateEmail($.trim(forgot_email)) == false){
			$('#forgot_email').addClass('input_error');
			a = '0';
		}
		
		if(a == '1'){
			$.ajax({
				type: "POST",
				url: "http://www.acehardware.co.id/ajax/ajax_forgot",
				data: { email: forgot_email }
			}).done(function( msg ) {
				if(msg == '1'){
					$('#suc_forgot_password').html('New password has been sent to your email');
					$('#suc_forgot_password').show();
					$('#btn_forgot').val('reset password');
				}
				else if(msg == '2'){
					$('#wrong_forgot_password').html('Email not found, please enter valid email');
					$('#wrong_forgot_password').show();
					$('#btn_forgot').val('reset password');
				}
				else{
					$('#wrong_forgot_password').html('Something wrong with reset password, please try again' + msg);
					$('#wrong_forgot_password').show();
					$('#btn_forgot').val('reset password');
				}
			});
		}
		else{
			$('#btn_forgot').val('reset password');
		}
		return false;
	}
	
