<script>
    $(document).ready(function () {
        var cartMenuHeight = window.innerHeight * .75;
        $("#cartmenu").css('max-height', cartMenuHeight);

        $(".remove-from-cart").click(function(e) {
            e.preventDefault();
            var params = $(this).attr("id").split("^^^");
            var id = params[1];
            $.blockUI(window);
            $.ajax({
                type: "POST",
                url: '/cart/remove',
                data: 'inventoryId=' + id + '&cartid=' + 67394228,
                success: function (htmldata) {
                    //console.log(htmldata);
                    if (htmldata.AddResult) {
                        location.reload();
                    } else {
                        alert("Error removing item from cart");
                    }
                },
                error: function (e) {
                    alert("Error removing item from cart");
                    $.unblockUI(window);
                },
                complete: function () {
                    //$.unblockUI(window);
                }
            });
        });
    });
</script>


