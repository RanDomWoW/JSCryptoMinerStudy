<script>
    $(document).ready(function() {
        $(document).on('click', '#qvbidBtn', function (e) {
            $.blockUI(window);
            window.location= $("#qvbidBtn").attr("href");
        });

        $(".add-to-cart").click(function(e) {
            e.preventDefault();
            var params = $(this).attr("id").split("^^^");
            var id = params[1];
            var price = params[2];
            $.blockUI(window);
            $.ajax({
                type: "POST",
                url: '/cart/add',
                data: 'inventoryId=' + id + '&cartid=' + 67394228 + '&price=' + price,
                success: function (htmldata) {
                    //console.log(htmldata);
                    if (htmldata.AddResult) {
                        location.reload();
                    } else {
                        alert("Error adding item to cart");
                    }
                },
                error: function (e) {
                    alert("Error adding item to cart");
                    $.unblockUI(window);
                },
                complete: function () {
                    //$.unblockUI(window);
                }
            });
        });
    });
</script>


