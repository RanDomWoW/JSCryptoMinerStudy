.title-with-sep{background:url("https://www.thebrokebackpacker.com/wp-content/themes/simplemag/images/section-header.png") repeat-x 0 50%;}
/* Custom CSS */
.logo img{width: 100%;}

.column-1 .related-posts .entry-image{height: auto !important;}

.posts-slider .read-more{display: none !important;}
.tag-box{display: none;}

#footer .footer-sidebar a.widget-post-title{font-family: Oswald;font-size: 18px; text-align: center; text-decoration: none; margin-bottom: 10px;}

.base-image img{width: 100%; height: auto;}

.single .base-image img.default-featured-img{display: none !Important;}

.page .page-title.title-with-bg{background-image: none !Important;  padding: 10px 0px; }
.page .page-title.title-with-bg *{color: #000 !Important;}

.main-menu > ul > .sub-links-only {
   position:static;
}

.main-menu > ul > .sub-links-only > .sub-menu {
   width:100%;
   left:0;
   margin-left:0;
   padding:30px 0;
   text-align:center;
}

.main-menu > ul > .sub-links-only > .sub-menu li {
   display:inline-block;
}

.main-menu .sub-links-only .sub-menu .sub-links a:after {
   display:none;
}

.sub-menu li{font-size: 14px;}

.mc4wp-form input{margin-bottom: 5px; width: 100%;}

.posts-slider .entry-meta{display: none;}

/*.posts-slider .read-more{display: none;}*/

.posts-slider .entry-header{bottom: 25% !important; }

.posts-slider .entry-title{visibility: hidden; width: 100%; height: 100%;}
.posts-slider .entry-title a{display: block; width: 100%; height: 100%;}

.posts-slider header{width: 100%; height: 100%; display: block;}

a.widget-post-title{font-family: 'Oswald', Arial, Verdana, 'Helvetica Neue', Helvetica, sans-serif !important;
    font-weight: 400;}

/*
.entry-image {background-image: url("https://barn2media.co.uk/thebrokebackpacker/wp-content/uploads/2014/06/thebrokebackpacker.jpg"); background-size: cover;}
*/




.single .default-featured-img, .page .default-featured-img {display: none !important;}

.category .default-featured-img,
.home .default-featured-img,
 .attachment-medium-size.default-featured-img{display: block !Important;}

.widget_categories select{width: 100%; padding: 12px;
-moz-appearance: none; -webkit-appearance: none; appearance: none; border: solid 1px #eee;
font-family: Lato;
}
#masthead #branding{margin-top: 0px;}
.widget_categories select option{font-family: Lato; }








.page .page-content li,
.single .entry-content li {
	margin-bottom:6px;
}

.instagram-pics li {
    float: left;
    width: 12%;
    padding: 2px 2px 2px 2px;
}

/* Start additions by Nelson, Once Coupled June 2017 */

.single .grid-8.column-1 {
  max-width: calc(100% - 410px);
}

.single .grid-4.column-2 {
  min-width: 350px;
} 

@media only screen and (max-width: 1023px) {
  .single .grid-8.column-1 {
    max-width: initial;
    width: calc(100% - 20px);
  }

  .single .grid-4.column-2 {
    padding-top: 5%; 
    display: block;
    margin: 0 auto;
    float: none;
    clear: both;
  }
}

@media only screen and (max-width: 750px) {  
  .single .grid-4.column-2 {
    max-width: 330px;
  }
}

@media only screen and (max-width: 480px) {
  .single .grid-4.column-2 {
    min-width: inherit;
  }
}

/* End additions by Nelson, Once Coupled June 2017 */

/* CSS for Links */

.entry-content a {
   color: #1c74bf;
   font-weight: bold;
   text-decoration: none;
}

/* When the link is hovered over. */
.entry-content a:hover {
   color: #000000;
}

/*End of CSS code for links */

/*Code to Hide categories on homepage*/

.entry-category a{
display: none;
}

/*End of Category hiding code*/

.title-text h1, .page .page-content h1, .single .entry-content h1 {
    font-size: 42px !important;
}</style>
