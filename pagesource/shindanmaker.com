var FirstAsyncFlag = false;
$(function(){
    setTimeout(function(){
    if (!FirstAsyncFlag) {
  	  FirstAsyncFlag = true;
  	  var async_target = [];
  	  async_target.push('recommend');
  	  async_target.push('pickup');  	  async_target.push('hot');
  	    	    	  async_target.push('new');
  	  async_target.push('ranking');
  	    	    	  async_target.push('chart');  	  async_target.push('favhot');
  	  GetAsync(async_target);
    }
    },50);
});

function GetAsync(async_target){
    //$('#async_'+m).html('<div style="text-align: center; margin-bottom: 20px; color: #CCC;"><i class="fas fa-spinner fa-spin fa-2x"></i></div>');
	$.ajax({
    type: 'GET',
    url: '/c/shindan_async',
    dataType: 'json',
      data: {
      shindan_id: '',
      async_target: async_target,
      author: '',
      ac:'',
      ac_f:'',
      ac_m:'',
      favorite:'',
      theme:'',
      pickuprank:'',
      hotrank:'',
      piccheck:'',
      movcheck:'',
      chart_flag:'1',
      },
    success: function(data ,datatype){
      $.each( data, function( key, value ) {
        $(value).prependTo('#async_'+key).css("display","none").fadeIn('slow');
      });
    },
	});
}
</script>
<script type="text/javascript">
var WindowActive = true;
window.onfocus=function(){
  WindowActive = true;
}
window.onblur=function(){
  WindowActive = false;
}

$(document).ready(function(){
  FirstSearchFlag = true;
  TweetSearch('index', 'first');
});

$(function(){
  	setInterval(function(){
        showRelativeTime();
      	},10000);
});

function TweetSearch(fm, sm){
  
  $("#loading1").html('<div class="loading"><span style="color: #AAA;"><i class="fas fa-spinner fa-spin fa-2x"></i><span class="sr-only">Loading...</span></span></div>');
  refresh_url_block = '#refresh_url-'+fm;
  refresh_url = $(refresh_url_block).text();
  
	$.ajax({
    type: 'GET',
    url: '/c/tweet_search' + refresh_url,
    dataType: 'html',
       data: {
       filemode : fm,
       searchmode : sm,
    },
    success: function(data) {
      $("#loading1").empty();
  		$(refresh_url_block).remove();
      showRelativeTime();
      $(data).prependTo('#tweet-'+fm).css("display","none").fadeIn('slow');
      if ($('table', '#tweet-'+fm).length > 10) {
       $('table:last', '#tweet-'+fm).remove();
       $('div:last', '#tweet-'+fm).remove();
      }
    },
	});
}
</script>


