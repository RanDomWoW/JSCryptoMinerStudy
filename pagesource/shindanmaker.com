function showRelativeTime()
{
		var month = new Array();
	month['Jan'] = '1月';
	month['Feb'] = '2月';
	month['Mar'] = '3月';
	month['Apr'] = '4月';
	month['May'] = '5月';
	month['Jun'] = '6月';
	month['Jul'] = '7月';
	month['Aug'] = '8月';
	month['Sep'] = '9月';
	month['Oct'] = '10月';
	month['Nov'] = '11月';
	month['Dec'] = '12月';
	
	
$.each($('div[name=hiddentime]'), function(idx, o) {
  
  var html = $(o).html();
  html = html.replace(/^\[/, '');
  html = html.replace(/\]$/, '');
  
  var arr = html.split('] [');
  //var timestamp = parseInt(Date.parse(arr[2]) / 1000);
	var timestamp = arr[3];
	
  var now = new Date();
	var nowstamp = parseInt(now.getTime() / 1000);
	
  var diff = nowstamp - timestamp;

	var str = '';

	if (diff < 0) {
		str = '';
	} else if (diff < 60) {
		str = diff + '秒前';
	} else if (diff < 3600) {
		str = parseInt(diff/60) + '分前';
	} else if (diff < 3600 * 24) {
		str = parseInt(diff/3600) + '時間前';
	} else {
		//var jpstamp = Date.parse(arr[2]);
		//var jpdate = new Date(jpstamp);
		var jpdate = new Date(arr[3] * 1000);
		var dateArr = jpdate.toString().split(' ');
		str = month[dateArr[1]]+dateArr[2]+'日';
	}
	
	$('a', '#showtime_'+arr[1]).html(str);
});
}






function SendEventGa(eventCategory,eventAction,eventLabel){
  
  if(eventAction === undefined){
    eventAction = "";
  }

  if(eventLabel === undefined){
    eventLabel = "";
  }

  if(eventCategory !== undefined) {
    ga('send', 'event', eventCategory, eventAction, eventLabel);
  }
}


