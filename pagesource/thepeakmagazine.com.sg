.gra16-alpha-index a:hover { background-color: #eb9812; text-decoration: none; }
.gra16-alpha-index a:last-child { letter-spacing: 3px; margin-right: 0; }


/* logo styling on mobile */
@media screen and (max-width: 1024px) {
    #logo {
        margin-bottom: 20px !important;
        margin-top: 20px !important;
    }
}

/* content font styling */
.entry-content {
    line-height: 24px !important;
}

/* font styling for related articles */
.ob-rec-text {
    text-transform: none !important;
    font-size: 12px !important;
    font-weight: normal !important;
}

/* Anchorblock arrow styling */
.cb-slider-block .flex-direction-nav a {
	opacity: 0.6 !important;
}

/* Brand Special styling */
.brandspecial_label {
	background-color: rgba(32, 32, 32, 0.8) !important;
	display:none !important;
}

.brandspecial_label_anchorblock,
.brandspecial_label_byline {
    display:none;
}

/* Wine Awards Styling */
.cb-module-header {
    border-bottom-color:#333 !important;
}

.cb-box .cb-module-title {
    margin: 0 0 5px;
    text-transform: uppercase;
    color: #333;
    font-size: 20px;
}

.wineawards {
    text-align: center;
    margin: 0 0 23px;
    font-size: 2.2em;
    text-transform: uppercase;
    border-bottom: 3px solid #333333;
    color:#333333;
    padding: 0 0 24px;
}

.wineawards a {
color:#333;
}

.wineawards-nav {
text-transform:uppercase;
font-family: 'Fjalla One', serif;
font-size:1.2em;
padding:0 0 24px;
text-align: center;
}

/* Dropdown Button */
.dropbtn {
    background-color: #333333;
    color: white;
    padding: 8px 16px;
    font-family: 'Fjalla One', serif;
    font-size: 0.8em;
    font-weight: 500;
    border: none;
    cursor: pointer;
    text-transform: uppercase;
    letter-spacing: 1px;
}

.dropbtn a {
    color:#FFF;
}

.dropbtn.selected {
    background-color:#aaa;
}

.dropbtn:visited, .dropbtn a:visited {
    color: #fff;
}

a.dropbtn { display:block; }

/* The container <div> - needed to position the dropdown content */
.dropdown {
    position: relative;
    display: inline-block;
    font-family: 'Fjalla One', serif;
    margin-bottom:2px;
}

/* Dropdown Content (Hidden by Default) */
.dropdown-content {
    display: none;
    position: absolute;
    background-color: #f9f9f9;
    min-width: 160px;
    box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
    z-index:2; font-size: 14px;
}

/* Links inside the dropdown */
.dropdown-content a {
    color: black;
    padding: 12px 16px;
    text-decoration: none;
    display: block;
}

/* Change color of dropdown links on hover */
.dropdown-content a:hover {background-color: #f1f1f1}

/* Show the dropdown menu on hover */
.dropdown:hover .dropdown-content {
    display: block;
}

/* Change the background color of the dropdown button when the dropdown content is shown */
.dropdown:hover .dropbtn {
    background-color: #555555;
}

/* CATEGORIES MODULE */
.wineawards-categ {
    display: block;
    width: 830px;
    margin: 0 auto;
    clear: both;
    padding: 10px 0;
    text-align: center;
}

.wineawards-categ li {
    width: 377px;
    height: 252px;
    /*float: left;*/
    display: inline-block;
    margin: 15px;
}

.wineawards-categ li img {
    width: 377px;
    height: 252px;
}

