<div class="modal fade" id="forgotPasswordPopup" tabindex="-1"
	role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" ng-controller="signInCtrl" >
	<div class="modal-dialog" style="display: inline-block;width: 100%;margin: 0;">
	<div class="modal-content modalContentRetrieveYPw">
	<div class="ngdialog-close custom-forgotPopupClose" data-dismiss="modal"></div>
	
		<div class="modal-body custom-modal-body">			
			 <div><div class="thankYouTxt">Forgot Your Password?</div>
				<div class="notice-pop-retrieveYPw">Please enter your email address below. You will receive a link to reset your password.
				
					</div>
			
				<form ng-submit="retriveYourPassword(retrivePasswordForm.$valid,'Y')" class="validate ng-pristine ng-invalid ng-invalid-required" method="POST" name="retrivePasswordForm">

					<input type="email" value="" placeholder="Email" name="emailId" ng-model="emailId" required="required" id="emailId" class=" ng-pristine ng-invalid ng-invalid-required ng-valid-email retrievYPwInput">
							<span ng-show="isRegisteredEmail" style="color:red" class="ng-hide">
						      						Please enter a registered email.
						    				</span>
						    		<div class="buttons-retrieveYourPw-success">
						    				
						<input class="btn-success btn-wide createJobsButton" type="submit"
								name="submit" id="submit" value="Submit" />
								</div>
				</form>
		</div>
		
		</div>
		</div>
	</div>
</div>

<script type="text/ng-template" id="signInregisterPopUp"> 
	<style type="text/css">
.ngdialog{
z-index:10000;
}
</style>
				<div  ng-controller="signInCtrl">
			<div ng-show="showModalOne" class="signNRegisterBtns" id="signNRegisterBtns1">
				<a href="" id="signInBtn" id="signInBtn" ng-click="toggleModal();focusOnRegister=false" style="color: #333;">Sign in<div class="underline"></div></a>
				<a href="" id="registerBtn" ng-click="registerPopUp();focusOnRegister=false" style="color: #999;">Register</a>
			</div>
			<div ng-show="showModalTwo" class="signNRegisterBtns" id="signNRegisterBtns2">
				<a href="" id="signInBtn" id="signInBtn" ng-click="toggleModal();focusOnRegister=false" style="color: #999;">Sign in</a>
				<a href="" id="registerBtn" ng-click="registerPopUp();focusOnRegister=false" style="color: #333;">Register<div class="underline"></div></a>
				<div ng-class="{'focusOnRegister':focusOnRegister ==true}"></div>
			</div>
				
    				<form  ng-show="showModalOne" id="signInId" name="form.signIn" method="POST"  class="contact-us"
								ng-submit="login()"> 
						<div class="customLoginForm" style ="">
								<input type="text" required="required"
									ng-model="signIn.emailId" name="emailId" id ="emailId" 
									placeholder="Email" value="" />
								
						<input
									type="password" required="required"
									ng-model="signIn.password" name="password"
									placeholder="Password" value="" />  		<input class="signInSubmit"
									type="submit" id="signInId" name="submit" value="Sign in" />
									<a href="" class="loginForgtPw" ng-click="closeSignInRegisterPopup();" data-toggle="modal" data-target = "#forgotPasswordPopup"><span>Forgot Password ?</span> </a>
								<br>
						</div>
				  </form>

			<form autocomplete="false" ng-show="showModalTwo" id="registerId" name="registrationForm" class="contact-us" method="POST"
						ng-submit="saveRegistration(registrationForm.$valid,'websiteDefaultForm')">
				<div class="customLoginForm custom-register-form-padding">
							
							<input id="uname" class="ovrlay-inputtext" type="text" name="Username"
								placeholder="Username" ng-model="registrationForm.register.userName" required/>
							
							<input class="ovrlay-inputtext email-style regFromMarginBotRemv" type="email" name="email" id="regEmailId" ng-pattern="/^([\w-\.]+@(?![A-Z])(?!gmail)(?!yahoo)(?!hotmail)(?!rediffmail)(?!aol)(?!outlook)(?!gmx)(?!zoho)(?!lycos)(?!inbox)(?!hushmail)(?!hush)(?!mac.hush)([\w-]+\.)+[\w-]{2,25})?$/"
								placeholder="Email" ng-model="registrationForm.register.emailId" required ng-change="emailExistsErr= ''">
							<span class="validationErrorMsg" ng-show="(registrationForm.email.$dirty || submitted) && registrationForm.email.$error.pattern">Please provide your work email.</span>
							<span class="validationErrorMsg" >{{emailExistsErr}}</span>
							<input class="ovrlay-inputtext" type="tel" required="required" name="phoneNo" id="phoneNo" ng-blur="validatePhoneNo(registrationForm)" ng-pattern="/^[0-9-+ ]{1,25}$/"
								ng-model="registrationForm.register.phoneNo" placeholder="Phone No"/>
							<span class="validationErrorMsg" ng-show="(registrationForm.phoneNo.$dirty || submitted) && registrationForm.phoneNo.$error.pattern">Invalid Phone Number.</span>

						<input id="companyWebsite" class="ovrlay-inputtext regFromMarginBotRemv" type="text" ng-pattern="/^(http(s)?:\/\/)?(www\.)?[a-zA-Z0-9]+([\-\.]{1}[a-zA-Z0-9]+)*\.[a-zA-Z]{2,20}(:[0-9]{1,5})?(\/.*)?$/"
								name="companyWebsite" placeholder="Company Website" ng-model="registrationForm.register.companyWebsite" required ng-change="onChangeSetErrNull();"/>
							<span class="validationErrorMsg" >{{websiteInvalidErr}}</span>
						    <span class="validationErrorMsg" ng-show="(registrationForm.companyWebsite.$dirty || submitted) && registrationForm.companyWebsite.$error.pattern">Please provide valid address</span>
							<span class="validationErrorMsg">{{companyExistsErr}}</span>
							
							<input class="ovrlay-inputtext" type="text" id="companyName"
								name="companyName" placeholder="Company Name" ng-model = "registrationForm.register.companyName" required/>

							<input class="ovrlay-inputtext" type="password"  id="password"
								name="password" placeholder="Password" ng-model="password" ng-minlength="6" required="required"/> 
							<span class="validationErrorMsg" ng-show="passwordMinLenght">{{passwordMinLenght}}</span>
							
							<input type="password" id="confirmPassword" name="passwordConfirmation" ng-model="passwordConfirmation" id="confirmPassword"
							placeholder="Confirm Password" class="ovrlay-inputtext"  password-confirm match-target="password" required="required"/> 
							
							
							<span class="validationErrorMsg" ng-show="registrationForm.password.$error.minlength">
								Sorry! Password should be at least 6 characters long. </span> 
							
						<span class="validationErrorMsg"  ng-show="passwordConfirmation && registrationForm.passwordConfirmation.$error.match">
								<span ng-show = "registrationForm.password.$error.minlength == false">Sorry! 
Your passwords do not seem to match. Please re-enter.</span></span> 

							<span class="errormsg" ng-show="validatePassword">{{validatePassword}}</span>

							<input class="signInSubmit" type="submit"  id="createAccId" value="Create Account" ng-click="registrationErr = ''"/> 
							<span class="validationErrorMsg">{{registrationErr}}</span>
						
					</div>
		</form>

</div>
</script>

