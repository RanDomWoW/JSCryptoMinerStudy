<script type="text/javascript">
	$(document).ready(function(){
		var currentSlide = 1;
		var slideNumber = 0;
		var maxSlideNumber = 4;
		function doSlide(slideNumberIndex,slideCommand){
			if(slideNumber == slideNumberIndex){
				if(slideCommand == "next"){
					if(currentSlide < maxSlideNumber){
						currentSlide++;
					}else{
						currentSlide = 1;
					}
				}else if(slideCommand == "pre"){
					if(currentSlide > 1){
						currentSlide--;
					}else{
						currentSlide = maxSlideNumber;
					}
				}else{
					currentSlide = slideCommand;
				}
				$(".coveredSlideItem").fadeOut("slow");
				$(".slideTbImage").removeClass("activeItem");
				$("#coveredSlideItem_"+currentSlide).fadeIn("slow");
				$("#slideTbImage_"+currentSlide).addClass("activeItem");
				setTimeout(function() {
					  doSlide(slideNumberIndex,"next");
				}, 10000);
			}
		}
		doSlide(0,1);
		
		$(".nextBtn").click(function(){
			slideNumber++;
			doSlide(slideNumber,"next");
		});
		
		$(".preBtn").click(function(){
			slideNumber++;
			doSlide(slideNumber,"pre");
		});
	});
</script>			
