    $("#search-select-category").change(function(e)
    {
        var catId = $(this).val();
        blockScreen();

        $.ajax({
            url  : "process/pr_ajax_calls.php",
            type : "POST",
            dataType : "JSON",
            data : {
                catId : catId,
                action : "find-subcategory-for-search"
            },
            success : function(data)
            {
                unblockScreen();

                removeInput('#subcategory-placeholder', '#search-select-subcategory');
                removeInput('#subsidiary-placeholder', '#search-select-subsidiary');
                removeInput('#search-custom-fields');

                if(data.status === true)
                {
                    showInput('#subcategory-placeholder', '#search-select-subcategory', data.result)
                } else {
                    if(data.custom_field_status === true) {
                        $("#search-custom-fields").html(data.custom_field_result).slideDown();
                        $('.selectpicker').selectpicker();
                    }
                }
            }
        })
    })

    $("#search-select-subcategory").change(function(e)
    {
        var catId = $("#search-select-category").val();
        var subcatId = $(this).val();

        blockScreen();

        $.ajax({
            url  : "process/pr_ajax_calls.php",
            type : "POST",
