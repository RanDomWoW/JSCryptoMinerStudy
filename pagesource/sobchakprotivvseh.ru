        hashToObject: function(hash){

            hash = hash.substr(1);

            var params = hash.split('&');

            var navi_params = {} ;

            for (var i = 0; i < params.length; i++){
                if (typeof(params[i]) == "undefined") continue;

                var tmp = params[i].split('=');

                if (typeof(tmp[1]) == "undefined") tmp[1] = '';

                navi_params[tmp[0]] = tmp[1];

            }

            return navi_params ;

        },

        getCenterPosition: function(){

            if (gdMap.is_authed == true && gdMap.my_info.user.b_id != 0){
                var building = gdMap.findInArrayById('b_id', gdMap.my_info.user.b_id, gdMap.buildings);

                if (building !== false)
                    return {lat: parseFloat(building.lat), lng: parseFloat(building.lng)};
            }

            return {lat: 55.75199484565539, lng: 37.617576160018885}; // city center
            return {lat: 55.80758869866716, lng: 37.46141795117189};
        },

        ifMac: function(){

            return mac = navigator.platform.match(/(Mac|iPhone|iPod|iPad)/i) ? true : false;

        },

        initMap: function(){

            var styledMap = new google.maps.StyledMapType(gdMap.mapStyled,
                {name: "mapSimplifiedStyle"});

            var center_position = gdMap.getCenterPosition();


            var scrollwheel = false ;


            gdMap.map = new google.maps.Map(document.getElementById('map'), {
                zoom: 5,
                center: center_position,
                scrollwheel : scrollwheel,
                navigationControl: false,
                scaleControl: false,
                mapTypeControl: false,
            });

            gdMap.map.mapTypes.set('mapSimplifiedStyle', styledMap);
            gdMap.map.setMapTypeId('mapSimplifiedStyle');

            google.maps.Polygon.prototype.my_getBounds=function(){
                var bounds = new google.maps.LatLngBounds()
                this.getPath().forEach(function(element,index){bounds.extend(element)})
                return bounds
            };

            google.maps.event.addListener(gdMap.map, 'zoom_changed', gdMap.onZoomChanged);
            //google.maps.event.addListener(gdMap.map, 'bounds_changed', gdMap.onBoundsChanged);

            google.maps.event.addListenerOnce(gdMap.map, 'tilesloaded', function(){
                //this part runs when the mapobject is created and rendered

                var hash = navi.hashToObject(document.location.hash);

                if (hash.hasOwnProperty('b_id')){
                    var building = mainSite.findInArrayByValue('b_id', hash.b_id, gdMap.buildings);

                    gdMap.map.setCenter({lat: parseFloat(building.lat), lng: parseFloat(building.lng)});
                    gdMap.map.setZoom(17);
                }


                var hash = gdMap.hashToObject(document.location.hash);
                if (hash.hasOwnProperty('authMessage')){
                    hash.authMessage = decodeURIComponent(hash.authMessage).replace(/\+/g, ' ');

                    mainSite.postError(hash.authMessage, '');
                }

                google.maps.event.addListener(gdMap.map, 'click', gdMap.onMapClicked);
                google.maps.event.addListener(gdMap.map, 'click touchstart', gdMap.onMapClicked);


                // load markers by ajax
                //google.maps.event.addListener(gdMap.map, 'idle', gdMap.showMarkers);

                google.maps.event.addListener(gdMap.map, 'idle', gdMap.renderMapContent);

                /*
                 Срабатывает сразу перед перемещением
                 */
                google.maps.event.addListener(gdMap.map, 'idle', function(){

                });

                gdMap.renderMapContent();

                gdMap.subjects.drawList();
                gdMap.extraObjects.drawList();

                //gdMap./

                if (typeof(gdMapAdmin) != "undefined")
                    gdMapAdmin.onMapInited();

                //if (typeof(mundepForm) != "undefined")
                //mundepForm.updateDeputyCircle('Академический');

            });
        },


        extraObjects: {

            drawList: function(){

                gdMap.drawnObjects = {} ;

                $.each(gdMap.extra_objects_list, function(key, item){

                    var marker = gdMap.extraObjects.drawMarker(item);

                    gdMap.drawnObjects[item.id] = {
                        marker: marker,
                    };



                });

            },

            drawMarker: function(item){

                var geo = item.geo.split(',');

                var lat = geo[0] ;
                var lng = geo[1] ;

                var params = {};

                var icon_file = 'pin@2x.png'; //pin_time
                var icon_size = new google.maps.Size(23, 47);

                var icon = {
                    url: '/static/img/map/' + icon_file,
                    scaledSize: icon_size,
                    origin: new google.maps.Point(-0,0),
                };

                var marker = new MarkerWithLabel({
                    position: {lat: parseFloat(lat), lng: parseFloat(lng)},
                    draggable: false,
                    raiseOnDrag: false,
                    map: gdMap.map,
                    clickable: true,
                    icon: icon,
                    labelContent: '<div class="label"><div>' + item.object_title + '</div></div>',
                    labelClass: "gmap_labels", // the CSS class for the label
                    labelInBackground: false,
                    visible: true,
                    labelVisible: false,
                    item: item,

                });

                google.maps.event.addListener(marker, 'click', gdMap.extraObjects.onDotClicked);
                google.maps.event.addListener(marker, 'click touchstart', gdMap.extraObjects.onDotClicked);
                google.maps.event.addListener(marker, 'click touchend', gdMap.extraObjects.onDotClicked);

                google.maps.event.addListener(marker, 'mouseout', gdMap.polygonDotUtils.mouseout);
                google.maps.event.addListener(marker, 'mouseover', gdMap.polygonDotUtils.mouseover);

                /*




                */


                return marker ;


            },

            onDotClicked: function(){

                if (this.item.object_type == "yurta")
                    gdMap.extraObjects.drawYurtaPopup(this.item);

            },

            drawYurtaPopup: function(item){

                var popup = $('#map-popup');

                var address_h = item.address_h ;

                $('.map-popup-content').addClass('shtab-gi');
                $('.map-popup-content').addClass('hide-name');

                if (item.time_h != ''){

                    //address_h += ' ('+ item.time_h + ')';
                    //$('.map-popup-content .hq-hours').text(item.time_h);
                    //$('.map-popup-content .hq-hours').show();
                } else {
                    $('.map-popup-content .hq-hours').hide();
                }

                $('.map-popup-content .hq-social').hide();
                $('.map-popup-content .hq-email').hide();

                popup.find('.hq-title').html(item.title);
                popup.find('.hq-address_h').html(address_h);

                popup.find('.hq-phone').html('<a href="tel://'+item.phone+'">'+formatPhoneNumber(item.phone)+'</a>');

                popup.fadeIn(250);

