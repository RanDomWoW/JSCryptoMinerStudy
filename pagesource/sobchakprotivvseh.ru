        },

        subjects: {

            drawList: function(){

                gdMap.drawnPolygons = {} ;
                gdMap.drawnSubjects = {} ;

                //console.log(gdMap.regions);

                // Define the LatLng coordinates for the polygon's path.

                $.each(mainSite.regions_geometry, function(key, region){

                    /*
                     Отрисовка границ полигона
                     */

                    var isFound = false ;
                    for (var i = 0; i < gdMap.subjects_list.length; i++){

                        if (gdMap.subjects_list[i].code_oktmo == region.code_oktmo){

                            console.log('found');

                            $.extend(region, gdMap.subjects_list[i]);

                            gdMap.subjects_list[i] = region;

                            isFound = true ;

                            break ;
                        }

                    }

                    gdMap.drawnSubjects[region.object_name] = true;
                    region.geometry = JSON.parse(region.geometry);


                    if (region.hasOwnProperty('hq_is_visible') == false || region.hq_is_visible == 0) {

                        console.log('No hq at ' + region.object_name);

                        return;
                    }

                    if (region.hasOwnProperty('geometry') == false || region.geometry.length != 2 || isFound == false){

                        console.log('Not found geometry for ' + region.object_name);
                        return ;
                    }

                    region.hq_data = JSON.parse(region.hq_data);

                    var coords = region.hq_geo.split(', ');

                    region.center_lat = coords[0];
                    region.center_lng = coords[1] ;

                    var regionPolygons = [] ;

                    // для каждого полигона в районе
                    $.each(region.geometry[1].geometries, function(){

                        var coords = [];

                        var polygon_geometry = this ;

                        $.each(this['coordinates'][0], function(key, value1){
                            coords.push({'lat': value1[1], 'lng': value1[0]});
                        });

                        // Construct the polygon.
                        var regionPolygon = new google.maps.Polygon({
                            paths: coords,
                            strokeColor: '#14425e',
                            strokeOpacity: 0,
                            strokeWeight: 1,
                            fillOpacity: 0,
                            poly_name: region.name,
                        });

                        google.maps.event.addListener(regionPolygon, 'click', gdMap.polygonEvents.polygonClicked);
                        google.maps.event.addListener(regionPolygon, 'click touchstart', gdMap.polygonEvents.polygonClicked);
                        google.maps.event.addListener(regionPolygon, 'click touchend', gdMap.polygonEvents.polygonClicked);

                        google.maps.event.addListener(regionPolygon, 'mouseout', gdMap.polygonEvents.mouseout);
                        google.maps.event.addListener(regionPolygon, 'mouseover', gdMap.polygonEvents.mouseover);

                        if (typeof(gdMapAdmin) != "undefined") google.maps.event.addListener(regionPolygon, 'click', gdMapAdmin.onMapClicked);

