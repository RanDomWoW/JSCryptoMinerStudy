<script>

    var onloadCallback = function () {
        var widget = grecaptcha.render(document.getElementById("re-captcha"), {
            'sitekey' : "6LdR7jQUAAAAAP55ZursfCTfrdg9UfuBeIqHKmGE",
            'theme': "light",
            'callback' : function(){

                if (pIndex.isMobile())
                    mainSite.waveScroll($("#cb-residence").offset().top, $('html, body'), 250);

            },
        });
    }

    pIndex = {

        isBinded: false,

        isSendingInProgress: false,

        onSuccessCallback: function(){},
        onBeforeSendCallback: function(){},

        ignoreUsualCallback: false,

        initForm: function(){

            pIndex.container = $('.indexsb-form');

            var target = pIndex.container.find('.ajax_arg[name="living_subject"]');
            gdAddressAutocomplete.init(target);

            var target = pIndex.container.find('.ajax_arg[name="living_addr"]');
            gdAddressAutocompleteCity.init(target);


            pIndex.prefillFormFields();

            //var target = pIndex.container.find('.ajax_arg[name="foreign_country"]');
            //gdAddressAutocompleteCountry.init(target);

            pIndex.bindings();

        },

        bindings: function(){


            if (pIndex.isBinded == true)
                return ;

            pIndex.isBinded = true ;

            pIndex.container.find('.btn-sendData').on('click touchstart', pIndex.saveData);

            pIndex.container.find('.ajax_arg').click(function(){

                if ($(this).hasClass('isRedLabeled'))
                    $(this).removeClass('isRedLabeled');

            });

            pIndex.container.find('.ajax_arg').change(pIndex.handlers.onAjaxFieldChanged);

            pIndex.container.find('.indexfb-form-foreign_country').on('click', '.selectize-control', function(){
                $('select[name="foreign_country"]').data().selectize.clear();
            });

            $('.pseudo-likely-block span').click(function(){

                var likely_real = $(this).attr('data-likely_real');

                $('.grey-line .' + likely_real).click();
            });

            $('#mailgun-email').mailgun_validator({
                api_key: 'pubkey-85b5bfaec9b8cfb0142c3d93fd9c660b',
                in_progress: function(){}, // called when request is made to validator
                success: pIndex.handlers.onEmailValidationSuccess,         // called when validator has returned
                error: function(){},           // called when an error reaching the validator has occured
            });


        },

        prefillFormFields: function(){

            if (navi.getParams().hasOwnProperty('auth_credentials')){

                if (navi.getParams().auth_credentials.search('@') == -1){
                    pIndex.container.find('.ajax_arg[name="phone"]').val(formatPhoneNumber(navi.getParams().auth_credentials));
                }
                else {
                    pIndex.container.find('.ajax_arg[name="email"]').val(navi.getParams().auth_credentials);
                }

            }
            
        },



        saveData: function(){


            if ($(this).hasClass('disabled'))
                return false ;

            if (pIndex.isSendingInProgress)
                return ;

            pIndex.isSendingInProgress = true ;

            setTimeout(function(){
                pIndex.isSendingInProgress = false;
            }, 1000);

            var data = {};

            data.context = 'add_supporter';
            data.form = mainSite.parseFormArgs(pIndex.container);
            data.form.helpOptions = [] ;
            data.is_mobile = pIndex.isMobile();
            data.recaptcha = $('.g-recaptcha-response').val();
            pIndex.container.find('div.circle').each(function(){

                if ($(this).hasClass('active') == false)
                    return ;

                data.form.helpOptions.push($(this).attr('data-option'));

            });

            if (data.form['cb-pers'] == false){

                mainSite.swal('Вам нужно согласиться с обработкой ваших персональных данных');
                return ;
            }
            if (pIndex.ignoreUsualCallback == false)
                mainSite.swalLoading.enable();

            if (pIndex.hasOwnProperty('onBeforeSendCallback'))
                pIndex.onBeforeSendCallback.apply(null, [data]);

            data.form.pathname = document.location.pathname ;

            //console.log(data);

            smartAjax('/ajax/ajax_index.php?context=add_supporter', data, function(msg){

                if (pIndex.ignoreUsualCallback == false){

