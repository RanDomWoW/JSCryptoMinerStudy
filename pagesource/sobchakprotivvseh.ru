beautyAg = {

    getText: function(){

        return $('#beautyag_text').val();

    },

    shareWith: function(social_type){


        beautyAg.social_type = social_type ;

        mainSite.swalLoading.enable();

        var data = {};

        data.context = 'save__onimgText';
        data.onimg_text = beautyAg.getText();

        smartAjax('/ajax/ajax_opengraph.php', data, function(msg){

            beautyAg.sharingLink = beautyAg.getSharingLink(beautyAg.social_type, msg.ogOjbect.id) ;



            if (beautyAg.social_type == 'facebook'){

                var opengraph_preloader = $('#sb-opengraph-preloader') ;

                opengraph_preloader.attr('src', function(){

                    var img_fb_preloader = $('#facebook-sharer-preloader') ;

                    img_fb_preloader.attr('src', function(){

                        setTimeout(function(){

                            beautyAg.openSharingPopup(beautyAg.sharingLink);

                        }, 5000);

                        swal({'title': 'Готовим изображение', 'showConfirmButton': false, 'customClass': 'swal-gd-loader'});
                        swal.showLoading();

                        setTimeout(function(){
                            swal({'title': 'Еще чуть-чуть', 'showConfirmButton': false, 'customClass': 'swal-gd-loader'});
                            swal.showLoading();
                        }, 2000);

                        setTimeout(function(){
                            swal({'title': 'Почти готово', 'showConfirmButton': false, 'customClass': 'swal-gd-loader'});
                            swal.showLoading();
                        }, 3500);



                    });

                    img_fb_preloader.attr('src', beautyAg.sharingLink);

                });

                opengraph_preloader.attr('src', 'https://sobchakprotivvseh.ru/ajax/ajax_opengraph.php?context=get__indexImage&onimg_id=' + msg.ogOjbect.id + '.png');




            }
            else {
                beautyAg.openSharingPopup(beautyAg.sharingLink);
            }



        }, function(msg){

            mainSite.swal(msg.error_text);

        }, '', 'GET');

    },

    openSharingPopup: function(sharingLink){

        var popup = {};

        if (beautyAg.social_type == 'facebook')
            popup = {width: 500, height: 600};

        if (beautyAg.social_type == 'vk')
            popup = {width: 550, height: 330};

        if (beautyAg.social_type == 'twitter')
            popup = {width: 600, height: 450};


        var left = Math.round(screen.width / 2 - popup.width / 2),
            top = 0;

        screen.height > popup.height && (top = Math.round(screen.height / 3 - popup.height / 2));

        var windowParams = "left=" + left + ",top=" + top + ",width=" + popup.width + ",height=" + popup.height + ",personalbar=0,toolbar=0,scrollbars=1,resizable=1"

        beautyAg.sharingLink = sharingLink ;

        beautyAg.window = window.open(beautyAg.sharingLink, 'example', windowParams);

        beautyAg.window ? (beautyAg.window.focus(), beautyAg.window) : (location.href = sharingLink, null);

        mainSite.swalLoading.disable();

    },

    getSharingLink: function(social_type, id){

        var siteLink = 'https://sobchakprotivvseh.ru/against/' + id ;
        var sharingText = $('meta[property="og:title"]').attr('content');

        if (typeof(sharingText) == "undefined")
            sharingText = 'Ксения Собчак — кандидат «против всех»';

        var link = '';

        if (social_type == 'twitter')
            link = 'https://twitter.com/intent/tweet?url={sharing_link}&text={sharing_text}';

        if (social_type == 'vk')
            link = 'https://vk.com/share.php?url={sharing_link}&title={sharing_text}';

        if (social_type == 'facebook')
            link = 'https://www.facebook.com/sharer/sharer.php?u={sharing_link}';


        link = link.replace('{sharing_link}', siteLink);
        link = link.replace('{sharing_text}', sharingText);

        return link ;
    }

};

