            setIconBlue: function(){

                return ;

                var icon_file = 'shtab@2x.png';

                var icon = {
                    url: '/static/img/map/' + icon_file,
                    scaledSize: new google.maps.Size(32, 32),
                    origin: new google.maps.Point(0,0),
                };

                this.setIcon(icon);

            },

            calculateDotKey: function(object){

                if (object.hasOwnProperty('code_oktmo')) {
                    return object.code_oktmo + '_' + object.object_name;
                }
                else {
                    return object.object_name;
                }
            },

            onDotClicked: function(){

                if (this.labelVisible == false) {
                    gdMap.polygonDotUtils.hideAllLabels();
                    this.labelVisible = true;

                    if (gdMap.polygonDotUtils.hasOwnProperty('lastDot'))
                        gdMap.polygonDotUtils.setIconBlue.apply(gdMap.polygonDotUtils.lastDot, []);

                    gdMap.polygonDotUtils.setIconRed.apply(this, []);


                }
                else {
                    this.labelVisible = false ;
                    gdMap.polygonDotUtils.setIconBlue.apply(this, []);
                }

                gdMap.drawShtabPopup(this.region.object_name);
                
                this.setVisible(true);
                gdMap.polygonDotUtils.lastDot = this ;

            },

            hideAllLabels: function() {

                $.each(gdMap.drawnPolygons, function(key, value){
                    value.custom_label.labelVisible=false;
                    value.custom_label.label.setVisible(false);
                });

            }

        },

