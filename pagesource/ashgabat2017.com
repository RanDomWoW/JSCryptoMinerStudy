					<div class="news_masonry">
			<div class="holder">
				<div class="filter-group" style="display:none;">
                	<input class="form-control" id="ms-complex-templating">
              	</div>
              	<div class="news_masonry_ajaxload"></div>
				<div class="clear"></div>
			</div>
		</div>
		<script>
			$(document).ready(function(){
				
				$(".lazy").lazyload();

			   	var ms = $('#ms-complex-templating').magicSuggest({
			    	allowFreeEntries: false,
			    	allowDuplicates: false,
			    	displayField: 'title',
			    	maxSelection: 6,
			    	hideTrigger: true,
			    	method: 'GET',
			    	typeDelay: 0,
			    	placeholder: 'Sports & NOCs...',
			        data: '/site/en/tags/autocomplete.json',
			        renderer: function(data){
			        	if(data.icon == "")
							return '<div>' + data.title + '</div><div class="clear"></div>';
			        	else
			        		return '<div><img src="//media-cdn.ashgabat2017.com/thumb/60/0/' + data.icon + '" />' + data.title + '</div><div class="clear"></div>';
			        },
			        selectionRenderer: function(data){
			        	if(data.icon == "")
			        		return data.title;
			        	else	
			        		return '<img src="//media-cdn.ashgabat2017.com/thumb/60/0/' + data.icon + '" />' + data.title;
			        }
			    });

			   	//click tag to filter
			    $(".news_masonry").on("click",".news_masonry_tags a",function(e){

			    	e.preventDefault();

					title = $(this).html();
					type = $(this).attr("class");
					icon = $(this).attr("icon");

					var current_selection = ms.getSelection(); 
					
					duplicate = 0;

					$.each(current_selection,function(index){
						if(current_selection[index].title == title)
							duplicate = 1;
					});

					if(duplicate == 0)
					{
						if(type == "sport" || type == "country" || type == "featured")
						{

							$('html, body').animate({
						        scrollTop: $(".news_masonry").offset().top-100
						    }, 1000);

							ms.addToSelection([{title:title, type:type, icon:icon}]);
						}
					}
						
					return false;
				});


			    //click to open news
				$(".news_masonry").on("click",".news_open",function(e){
					document.location = $(this).attr("news-src");
				});

			   	//color tags
			    $(ms).on('collapse keydown selectionchange blur', function(e,m){

			    	var current_selection = this.getSelection(); 

			    	current_selection_string = JSON.stringify(current_selection);
			    	setCookie("news_filter_en",escape(current_selection_string));

				  	setTimeout(function(){

				  		$(".ms-sel-item").each(function(index){
					  		$(this).addClass(current_selection[index].type);
					  	});

				  	},200);

				  	$(".news_masonry .filter-group").slideDown(200);

				});

			    loading = 0;
			    page = 2;

			    //homepage load more & when filter not found
			    $(window).scroll(function() {
				   	
				    if($(window).scrollTop() > ($(document).height() - $(window).height()) - 1000) 
				    {
				    	current_selection = ms.getSelection();

				    	if(loading == 0 && current_selection.length == 0)
				    	{
				    		news_order = $(".news_masonry_ajaxload .news_masonry_item:last-child").attr("news-order");
				    		
				    		if(news_order > 0)
				    		{
					    		loading = 1;
					    	 
					    		$(".news_masonry_ajaxload").append("<div class='masonry_loading'></div>");

					    		$.ajax({
						            url: '/ajaxload/news_grid/category/home/'+page+'/'+news_order,
						            type: 'GET',
						            dataType: 'html',
						            success: function(html) {
						                
						                $(".news_masonry_ajaxload").append(html);
										$(".lazy").lazyload();

										page = page+1;
										loading = 0;

										if(html == "")
						                	loading = 1;

										$(".masonry_loading").remove();
						             		
						            }
						        });
				    		}
				    	} 
				    }
				});

			    //load news
			    $(ms).on("selectionchange",function(e,m){

			    	//countdown fix
			    	$('.news_masonry .days, .news_masonry .hours, .news_masonry .minutes, .news_masonry .seconds').countdown('destroy');

					tags = "";

					var current_selection = this.getSelection(); 
					
					if(current_selection.length > 0)
					{
						$(".news_masonry_ajaxload").html("<div class='masonry_loading'></div>");

						$.each(current_selection,function(index){
							tags += current_selection[index].title+",";
						});

						$.ajax({
				            url: '/ajaxload/news_grid/tags/'+tags,
				            type: 'GET',
				            dataType: 'html',
				            success: function(html) {
				                
				                $(".news_masonry_ajaxload").html(html);
								$(".lazy").lazyload();
				             	
				             	if(html == "")
				             		$(".news_masonry_ajaxload").html("<div class='news_masonry_not_found'>No related news or tags were found.</div>");

				            }
				        });

					} else {
						
						$(".news_masonry_ajaxload").html("<div class='masonry_loading'></div>");

						$.ajax({
				            url: '/ajaxload/news_grid/category/home/1',
				            type: 'GET',
				            dataType: 'html',
				            success: function(html) {
				                
				              	$(".news_masonry_ajaxload").html(html);
								$(".lazy").lazyload();
				             		
				             	page = 2;
				            }
				        });
					}

				});


				//load from cookie
			    saved_selections = getCookie("news_filter_en");
			    saved_selections = JSON.parse(unescape(saved_selections));
			    
			    if(saved_selections && saved_selections.length > 0)
			    {
			    	ms.setSelection(saved_selections);
			    } else {

		    		$(".news_masonry_ajaxload").html("<div class='masonry_loading'></div>");

					$.ajax({
			            url: '/ajaxload/news_grid/category/home/1',
			            type: 'GET',
			            dataType: 'html',
			            success: function(html) {
			                
			              	$(".news_masonry_ajaxload").html(html);
							$(".lazy").lazyload();
			             		
			             	page = 2;
			            }
			        });
			    }

			});
		</script>
