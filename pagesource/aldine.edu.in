		function getImageCSV(imgCsv, nos, divCss){		
			var img = '';
			imgArr = imgCsv.split(",");
			
			colCss='';
			if(!nos){ nos = imgArr.length; colCss = "col-xs-4";}
			
			for(i=0; i < nos; i++){
				if( imgArr[i] != '' ){
					ig = '<img class="img-fluid '+ colCss +'" src="/repository/cms/'+imgArr[i]+'" alt="image" />';
					if(divCss){ ig = '<div class="'+divCss+'">' + ig + '</div>'; }
					
					img += ig;
				}
			}
			
			// if(divCss){ img = '<div class="'+divCss+'">' + img + '</div>'; }
			return img;
		}
