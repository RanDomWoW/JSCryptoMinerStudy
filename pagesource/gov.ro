<script type="text/javascript">
    var url_param = '';
    $(".div_zi_eveniment_sedinte").live("click",function(){
		$(".selected").removeClass("selected");
		id_div = $(this).attr('id');
		$(this).addClass("selected");
		$(".div_evenimente_sedinte").css('display','none');
		$("#events_sedinte_"+id_div).css('display','block');
                url_param = $("#data_"+id_div).val();
                $("#veziMaiMulteSedinte").prop('href',$("#url_sedinte_lista").val()+'?s='+url_param );
    
                //alert(url_param );
                
    });

    $("#sedinte_prev").click(function(){
        if ($("#sedinte_prev").prop('disabled') !== true)
        {
            contor = 0;
            first = 0;
            nextFirst = '';
            firstDiv = '';
            $("#sedinte_prev").prop('disabled', true);
            $(".div_zi_eveniment_sedinte").each(function(){
                if (contor === 0)
                    firstDiv = $(this).attr('id'); 
                if (contor === 0 && $(this).attr('first') === undefined)
                    {
                           nextFirst = $(this).attr('id'); 
                    }

                if ($(this).attr('first') === '1')
                    {
                          contor++;
                          first=1;
                          $(this).show().removeAttr('first');
                    }
                if (contor > 0)     
                    contor++;

                if (contor > 8)
                {
                    if (contor === 9)
                        $(this).attr('last','1');
                    else
                        $(this).removeAttr('last');
                    $(this).hide();

                }

            });
            
            if (nextFirst === '')
            {
                lastDate = $("#data_"+firstDiv).val();
                if (lastDate!== undefined)
                {
                        $.ajax({
                                type: "GET",
                                url: "http://gov.ro/ro/ajax/sedinte/create_dates_index",
                                data: "type=3&last_date="+lastDate,
                                success: function(msg){
                                        $("#sedinte_prev").after($(msg));
                                       $('.sedinteSlide').bxSlider({
                                                            minSlides: 1,
                                                            slideWidth: 890,
                                                            slideMargin: 10,
															pause: 4000,
                                                            responsive: true,
                                                            adaptiveHeight: true,
                                                            infiniteLoop: false,
															auto:true
                                                            });
                                }
                        });
                        $.ajax({
                                type: "GET",
                                url: "http://gov.ro/ro/ajax/sedinte/create_dates_index",
                                data: "type=4&last_date="+lastDate,
                                success: function(msg){
                                        $("#sedinte_detalii").prepend($(msg));
                                       $('.sedinteSlide').bxSlider({
                                                            minSlides: 1,
                                                            slideWidth: 890,
                                                            slideMargin: 10,
															pause: 4000,
                                                            responsive: true,
                                                            adaptiveHeight: true,
                                                            infiniteLoop: false,
															auto:true
                                                            });
                                }
                        });
                }
            }
            else
            {
                $("#"+nextFirst).attr('first','1');
            }
             window.setTimeout( function(){ $("#sedinte_prev").prop('disabled', false)} , 500 );
        }
    });

    $("#sedinte_next").click(function(){
        if ($("#sedinte_next").prop('disabled') !== true)
        {
            contor = 0;
            first = 0;
            nextLast = '';
            nextFirst = '';
            maybeLast = '';
            $("#sedinte_next").prop('disabled', true);
            $(".div_zi_eveniment_sedinte").each(function(){
                if (first === 1)
                {
                     $(this).hide();
                     first = 0;
                     contor++;
                     $(this).attr('first','1');
                }
                if ($(this).attr('first') === '1' && contor === 0)
                {
                      first=1;
                      $(this).removeAttr('first');
                }
                if (contor > 0)     
                    contor++;

                if (contor === 9)
                {
                    $(this).show();
                    $(this).removeAttr('last');
                    maybeLast = $(this).attr('id');

                }
                if (contor === 10)    
                    nextLast = $(this).attr('id'); 
            });

            if (nextLast === '')
            {
                    lastDate = $("#data_"+maybeLast).val();
                    if (lastDate!== undefined)
                    {
                        $.ajax({
                            type: "GET",
                            url: "http://gov.ro/ro/ajax/sedinte/create_dates_index",
                            data: "type=1&last_date="+lastDate,
                            success: function(msg){
                                $(msg).insertBefore('#sedinte_next');
                                $('.sedinteSlide').bxSlider({
                                                            minSlides: 1,
                                                            slideWidth: 890,
                                                            slideMargin: 10,
															pause: 4000,
                                                            responsive: true,
                                                            adaptiveHeight: true,
                                                            infiniteLoop: true,
															auto:true
                                                            });
                            }
                        });
                        $.ajax({
                            type: "GET",
                            url: "http://gov.ro/ro/ajax/sedinte/create_dates_index",
                            data: "type=2&last_date="+lastDate,
                            success: function(msg){
                                $("#sedinte_detalii").append($(msg));
                                $('.sedinteSlide').bxSlider({
                                                            minSlides: 1,
                                                            slideWidth: 890,
                                                            slideMargin: 10,
															pause: 4000,
                                                            responsive: true,
                                                            adaptiveHeight: true,
                                                            infiniteLoop: true,
															auto:true
                                                            });
                            }
                        });
                    }
            }    
            else    
                $("#"+nextLast).attr('last','1');  
                
           window.setTimeout( function(){ $("#sedinte_next").prop('disabled', false)} , 500 );     
        }
    });
    
    
$(window).resize(function() 
{
	var height_banner = $(".sedinteSlide").height();
	$(".div_evenimente .bx-wrapper .bx-viewport").css('height',height_banner);
});   
   
 
 $(document).ready(function(){  
	$('.sedinteSlide').bxSlider({
		controls:true,
		minSlides: 1,
		slideWidth: 890,
		slideMargin: 10,
		pause: 4000,
		responsive: true,
		adaptiveHeight: true,
		infiniteLoop: false,
		auto:true
	});

});   
</script>
