<script type="text/javascript">
    $(".div_zi_eveniment").live("click",function(){
		$(".selected").removeClass("selected");
		id_div = $(this).attr('id');
		$(this).addClass("selected");
		$(".div_evenimente").css('display','none');
		$("#events_"+id_div).css('display','block');
    });

    $("#agenda_prev").click(function(){
        if ($("#agenda_prev").prop('disabled') !== true)
        {
            contor = 0;
            first = 0;
            nextFirst = '';
            firstDiv = '';
            $("#agenda_prev").prop('disabled', true);
            $(".div_zi_eveniment").each(function(){
                if (contor === 0)
                    firstDiv = $(this).attr('id'); 
                if (contor === 0 && $(this).attr('first') === undefined)
                    {
                           nextFirst = $(this).attr('id'); 
                    }

                if ($(this).attr('first') === '1')
                    {
                          contor++;
                          first=1;
                          $(this).show().removeAttr('first');
                    }
                if (contor > 0)     
                    contor++;

                if (contor > 8)
                {
                    if (contor === 9)
                        $(this).attr('last','1');
                    else
                        $(this).removeAttr('last');
                    $(this).hide();

                }


            });

            if (nextFirst === '')
            {
                            lastDate = $("#data_"+firstDiv).val();
                            if (lastDate!== undefined)
                            {
                                    $.ajax({
                                            type: "GET",
                                            url: "http://gov.ro/ro/ajax/agenda/create_dates_index",
                                            data: "type=3&last_date="+lastDate,
                                            success: function(msg){
                                                    $("#agenda_prev").after($(msg));
                                                    $('.agendaSlide').bxSlider({
																	minSlides: 1,
																	slideWidth: 250,
																	slideMargin: 10,
																	responsive: true,
																	adaptiveHeight: true,
																	infiniteLoop: false
                                                            });
                                            }
                                    });
                                    $.ajax({
                                            type: "GET",
                                            url: "http://gov.ro/ro/ajax/agenda/create_dates_index",
                                            data: "type=4&last_date="+lastDate,
                                            success: function(msg){
                                                    $("#agenda_detalii").prepend($(msg));
                                                    $('.agendaSlide').bxSlider({
																	minSlides: 1,
																	slideWidth: 250,
																	slideMargin: 10,
																	responsive: true,
																	adaptiveHeight: true,
																	infiniteLoop: false
                                                            });
                                            }
                                    });
                            }
            }
            else        
                $("#"+nextFirst).attr('first','1');
            
             window.setTimeout( function(){ $("#agenda_prev").prop('disabled', false)} , 500 );
        }
    });

    $("#agenda_next").click(function(){
        if ($("#agenda_next").prop('disabled') !== true)
        {
            contor = 0;
            first = 0;
            nextLast = '';
            nextFirst = '';
            maybeLast = '';
            $("#agenda_next").prop('disabled', true);
            $(".div_zi_eveniment").each(function(){
                if (first === 1)
                {
                     $(this).hide();
                     first = 0;
                     contor++;
                     $(this).attr('first','1');
                }
                if ($(this).attr('first') === '1' && contor === 0)
                {
                      first=1;
                      $(this).removeAttr('first');
                }
                if (contor > 0)     
                    contor++;

                if (contor === 9)
                {
                    $(this).show();
                    $(this).removeAttr('last');
                    maybeLast = $(this).attr('id');

                }
                if (contor === 10)    
                    nextLast = $(this).attr('id'); 
            });

            if (nextLast === '')
            {
                    lastDate = $("#data_"+maybeLast).val();
                    if (lastDate!== undefined)
                    {
                        $.ajax({
                            type: "GET",
                            url: "http://gov.ro/ro/ajax/agenda/create_dates_index",
                            data: "type=1&last_date="+lastDate,
                            success: function(msg){
                                $(msg).insertBefore('#agenda_next');
                                                            $('.agendaSlide').bxSlider({
																	minSlides: 1,
																	slideWidth: 250,
																	slideMargin: 10,
																	responsive: true,
																	adaptiveHeight: true,
																	infiniteLoop: false
                                                            });
                            }
                        });
                        $.ajax({
                            type: "GET",
                            url: "http://gov.ro/ro/ajax/agenda/create_dates_index",
                            data: "type=2&last_date="+lastDate,
                            success: function(msg){
                                $("#agenda_detalii").append($(msg));
                                                            $('.agendaSlide').bxSlider({
																	
																	minSlides: 1,
																	slideWidth: 250,
																	slideMargin: 10,
																	responsive: true,
																	adaptiveHeight: true,
																	infiniteLoop: false
                                                            });
                            }
                        });
                    }
            }    
            else    
                $("#"+nextLast).attr('last','1');  
                
           window.setTimeout( function(){ $("#agenda_next").prop('disabled', false)} , 500 );     
        }
    });
   
$(window).resize(function() 
{
	var height_banner = $(".agendaSlide").height();
	$(".div_evenimente .bx-wrapper .bx-viewport").css('height',height_banner);
});   
   
 $(document).ready(function(){  
	$('.agendaSlide').bxSlider({
		controls:true,
		minSlides: 1,
		slideWidth: 250,
		slideMargin: 10,
		responsive: true,
		adaptiveHeight: true,
		infiniteLoop: false
	});

});
</script>
