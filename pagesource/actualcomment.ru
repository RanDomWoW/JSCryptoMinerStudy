<script type="text/javascript" defer async>



	var lockPagesLoad = false;

	function loadBlock(objPager, sBlock) {
		var data = {getnextblock: sBlock};
		var url = objPager+'&getnextblock='+sBlock;
		history.pushState({}, '',objPager.href.replace('getnextblock=innerblocks', ''));
		
		var curUrl = String(window.location);
		var tmpIndex = curUrl.indexOf('archive=');

		if (tmpIndex > -1){ // если имеется параметр archive
			var nSimbol = curUrl.substr(tmpIndex+14, 1);
			if (nSimbol == '&') var monthMrk = curUrl.substr(tmpIndex+8, 6);
			else var monthMrk = curUrl.substr(tmpIndex+8, 7);
			url += '&archive='+monthMrk;
		}

		if (lockPagesLoad) return false;
		lockPagesLoad = true;
		
		
		$.ajax({
			url: url,
			type: 'POST',
			data: data,
			success: function (response) {
				var $container = $( BX(sBlock) );
				var $data = $(response);
				
				$container.replaceWith($data);
				
				lockPagesLoad = false;
			},
			dataType: 'text'
		});
	}

	function loadBlockContent(objPager, sBlock, sPrevPager) {
		var data = {getnextblock: sBlock};
		var url = objPager+'&getnextblock='+sBlock;
		history.pushState({}, '',objPager.href.replace('getnextblock=innerblocks', ''));

		var curUrl = String(window.location);
		var tmpIndex = curUrl.indexOf('archive=');

		if (tmpIndex > -1){ // если имеется параметр archive
			var nSimbol = curUrl.substr(tmpIndex+14, 1);
			if (nSimbol == '&') var monthMrk = curUrl.substr(tmpIndex+8, 6);
			else var monthMrk = curUrl.substr(tmpIndex+8, 7);
			url += '&archive='+monthMrk;
		}

		if (lockPagesLoad) return false;
		lockPagesLoad = true;
		
		
		$.ajax({
			url: url,
			type: 'POST',
			data: data,
			success: function (response) {
				var buffer = document.createElement('DIV');
				buffer.innerHTML = response;
				
				var $buffer = $(buffer);
				var $container = $('.'+sBlock);
				var $paging = $buffer.find('.b_hottopics__loadmore');
				var $oldPager = $(objPager).parent();
				

				// Партия плашек в карточке материала
				var $newRow = $buffer.find('.similarPart').clone();
				if ($newRow.length > 0){
					$oldPager.remove();
					
					$newRow.css('display', 'none');
					$newRow.appendTo($container);
					$newRow.fadeIn(500);
					
					$paging.insertAfter($newRow);
					
					lockPagesLoad = false;
					
					return true;
				}

				
				// для каталогов новостей [/kommentarii/]
				var tmpItems = $buffer.find('.b_hottopics__rt_item').toArray();
				var baners = new Array();
				var banersContent = new Array();
				var items = new Array();
				
				for (var i in tmpItems){
					if (BX.hasClass(tmpItems[i], 'b_hottopics__rt_item')) {
						items.push(tmpItems[i]);
					}
				}
				
				if (items.length > 0){
					$container.append(items).isotope('appended', items).isotope('layout');
				}
				
				
				// Элементы у эксперта на детальной [/experts/expert_name/]
				var $exponePostsItems = $buffer.find('.b_expone__posts_item').clone();
				if ($exponePostsItems.length > 0){
					$exponePostsItems.css('display', 'none');
					$exponePostsItems.appendTo($container);
					$exponePostsItems.fadeIn(500);
				}
				
				
				// Элементы в поиске [/search/?q=search_words]
				var $searchResultItems = $buffer.find('.b_src__reslt_item').clone();
				if ($searchResultItems.length > 0){
					$searchResultItems.css('display', 'none');
					$searchResultItems.appendTo($container);
					$searchResultItems.fadeIn(500);
				}
				
				
				// Замена пагинатора
				$paging.insertAfter($oldPager);
				$oldPager.remove();
				
				lockPagesLoad = false;
			},
			dataType: 'text'
		});
	}
