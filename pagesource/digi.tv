<html>
	<head>
		<script type="text/javascript" src="https://www.google.com/jsapi"></script>
		<script type="text/javascript">
			window.addEventListener('load', function(data) {
				document.getElementById('loading').remove();
			});
			google.load('visualization', '1', {packages:['geochart']});
			google.setOnLoadCallback(drawRegionsMap);
			function drawRegionsMap() {
				var data = google.visualization.arrayToDataTable([
					['Country',	 'Latitude'],
					['Hungary', 'Hungary'], ['Croatia', 'Croatia'], ['Czech Republic', 'Czech Republic'], ['Romania', 'Romania'], ['Serbia', 'Serbia'], ['Slovakia', 'Slovakia']
				]);

				var options = {
					region: '150',
					colorAxis: {colors: []},
					backgroundColor: {
						fill: '#DFF6FF',
						stroke: 'white',
						strokeWidth: 3
					},
					datalessRegionColor: 'white',
					defaultColor: '#009ee0',
					enableRegionInteractivity: true,
					legend: 'none',
					/*tooltip: {trigger: 'none'},*/
				};

				var chart = new google.visualization.GeoChart(document.getElementById('map'));
				google.visualization.events.addListener(chart, 'regionClick', function(data) {
					var links = {HU: 'http://www.digi.hu', CZ: 'http://cz.digi.tv', SK: 'http://www.digislovakia.sk', RO: 'http://www.rcs-rds.ro', RS: 'http://www.digi.rs/web/', HR: 'http://hr.digi.tv/'}
					if (links[data.region]) location.href = links[data.region];
					console.log(data.region);
				});
				chart.draw(data, options);
			};
		</script>
		<style>
			body {
				text-align: center;
			}
			#logo {
				margin: 50px auto;
			}
			#frame {
				width: 800px;
				height: 450px;
				overflow: hidden;
				margin: 0 auto;
				position: relative;
				border: 1px solid #f5f5f5;
			}
			#map {
				width: 1500px;
				top: -380px;
				left: -500px;
				position: absolute;
			}
			.loading {
				text-align: center;
				position: absolute;
				top: 40%;
				left: 0;
				right: 0;
				height: 50px;
				margin: auto;
			}
			@media (max-width: 767px) {
				#logo {
					width: 250px;
					margin: 25px auto;
				}
				#frame {
					width: 250px;
					height: 350px;
				}
				#map {
					width: 1200px;
					top: -320px;
					left: -590px;
				}
			}

			.loading span {
				display: inline-block;
				vertical-align: middle;
				width: 20px;
				height: 20px;
				margin: 50px auto;
				background: rgb(0,158,224);
				border-radius: 100px;
					-o-border-radius: 100px;
					-ms-border-radius: 100px;
					-webkit-border-radius: 100px;
					-moz-border-radius: 100px;
				animation: loading 1.105s infinite alternate;
					-o-animation: loading 1.105s infinite alternate;
					-ms-animation: loading 1.105s infinite alternate;
					-webkit-animation: loading 1.105s infinite alternate;
					-moz-animation: loading 1.105s infinite alternate;
			}

			#loading_1 {
				animation-delay: 0s;
					-o-animation-delay: 0s;
					-ms-animation-delay: 0s;
					-webkit-animation-delay: 0s;
					-moz-animation-delay: 0s;
			}

			#loading_2 {
				animation-delay: 0.3295s;
					-o-animation-delay: 0.3295s;
					-ms-animation-delay: 0.3295s;
					-webkit-animation-delay: 0.3295s;
					-moz-animation-delay: 0.3295s;
			}

			#loading_3 {
				animation-delay: 0.659s;
					-o-animation-delay: 0.659s;
					-ms-animation-delay: 0.659s;
					-webkit-animation-delay: 0.659s;
					-moz-animation-delay: 0.659s;
			}



			@keyframes loading {
				0% {
					width: 20px;
					height: 20px;
					background-color:rgb(0,138,204);
					transform: translateY(0);
				}

				100% {
					width: 48px;
					height: 48px;
					background-color:rgb(255,255,255);
					transform: translateY(-42px);
				}
			}

			@-o-keyframes loading {
				0% {
					width: 20px;
					height: 20px;
					background-color:rgb(0,138,204);
					-o-transform: translateY(0);
				}

				100% {
					width: 48px;
					height: 48px;
					background-color:rgb(255,255,255);
					-o-transform: translateY(-42px);
				}
			}

			@-ms-keyframes loading {
				0% {
					width: 20px;
					height: 20px;
					background-color:rgb(0,138,204);
					-ms-transform: translateY(0);
				}

				100% {
					width: 48px;
					height: 48px;
					background-color:rgb(255,255,255);
					-ms-transform: translateY(-42px);
				}
			}

			@-webkit-keyframes loading {
				0% {
					width: 20px;
					height: 20px;
					background-color:rgb(0,138,204);
					-webkit-transform: translateY(0);
				}

				100% {
					width: 48px;
					height: 48px;
					background-color:rgb(255,255,255);
					-webkit-transform: translateY(-42px);
				}
			}

			@-moz-keyframes loading {
				0% {
					width: 20px;
					height: 20px;
					background-color:rgb(0,138,204);
					-moz-transform: translateY(0);
				}

				100% {
					width: 48px;
					height: 48px;
					background-color:rgb(255,255,255);
					-moz-transform: translateY(-42px);
				}
			}
		</style>
	</head>
	<body>
		
		<img id="logo" src="data:image/png;base64,"/>
		<div id="frame">
			<div class="loading" id="loading">
				<span id="loading_1">
				</span>
				<span id="loading_2">
				</span>
				<span id="loading_3">
				</span>
			</div>
			<div id="map"></div>
		</div>
	</body>
</html>