    $('.close-redirect-btn').click(function(e){
      e.preventDefault();
      $('#PageContainer, #siteNav, #productInfoBar').removeClass('site-banner-shift');
    });

    jQuery(document.body).on('afterCartLoad.ajaxCart', function(evt, cart) {
        // Bind to 'afterCartLoad.ajaxCart' to run any javascript after the cart has loaded in the DOM
        timber.RightDrawer.open();
        window.updateToFreeShippingMessage();
    });
    jQuery(document.body).on('afterChangeItem.ajaxCart', function(evt, line, quantity, cart) {
      adjustCart(cart)
      window.updateToFreeShippingMessage();
    });
    jQuery(document.body).on('afterAddItem.ajaxCart', function(evt, product, form) {
      var z = [];
      // var pdp = /(products)/g
      $('.selectAddon').each(function(){

        var selectedAddon = this.options[this.selectedIndex].getAttribute('id');
        // selectedAddon ? selectedAddon = selectedAddon.getAttribute('id') : null;
        selectedAddon ? z.push(selectedAddon) : null;
      });
      $('.js-addon:checked').each(function(){
        z.push($(this).val());
      });
      var pdp = window.location.pathname.indexOf('products');
      window.gettAllViableIds(z, product.id.toString(), pdp);

      window.updateToFreeShippingMessage();
    });
    jQuery(document.body).on('errorUpdateCartNote.ajaxCart', function(evt, XMLHttpRequest, textStatus){
      snackbar.show({"message": textStatus });
    });
    jQuery(document.body).on('errorChangeItem.ajaxCart', function(evt, XMLHttpRequest, textStatus){
      snackbar.show({"message": textStatus });
    });
    </script>

    
