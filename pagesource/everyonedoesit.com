<script>
function calculateCartTotal() {
  var calcShipping = document.querySelector('.calc-shipping');
  var continueButton = document.querySelector('.continue_btn');
  var shippingText = document.querySelector('p.text-center');

    if(document.querySelector('.ajaxcart__totalprice > p') != null) {
      var totalPrice = document.querySelector('.ajaxcart__totalprice > p').innerText;
      var totalPriceNumber = Number(totalPrice.split(/ /)[0].replace(/[^\d]/g, ''));
    }
    if(totalPriceNumber < 8000){
      var total_price = document.querySelector('.ajaxcart__totalprice');
      if(total_price != null){
          total_price.setAttribute('style','margin-bottom:23px;');
      }
      var checkoutButton = document.querySelector('.cart__checkout');
      calcShipping.setAttribute('style', 'display:none');
      continueButton.setAttribute('style', 'display:none');
      shippingText.innerText = "Shipping calculated at checkout";
      shippingText.setAttribute('style', 'position:relative;top:70px;display:block; font-size: 14px;')
      checkoutButton.setAttribute('style', 'position:relative; bottom: 22px;')
    }
    if(totalPriceNumber >= 8000 && totalPriceNumber < 10000) {
      var total_price = document.querySelector('.ajaxcart__totalprice');
      if(total_price != null){
          total_price.setAttribute('style','margin-bottom:15px;');
      }
      var taxes = document.querySelector('p.text-center');
      taxes.setAttribute('style', 'position:relative; top:135px; font-size: 14px;');
      calcShipping.innerHTML = "<span style=\"font-weight:bold\">$" + ((10000 - totalPriceNumber) / 100) + "</span> more for FREE discreet shipping";
      calcShipping.setAttribute('style', 'text-align:center;display:block; position:relative; bottom: 35px; font-size: 14px;');
      continueButton.setAttribute('style', 'width:100%;display:block; position:relative; bottom: 35px; background-color:#f79a2d !important; box-shadow: 0px 3px 1px -2px rgba(0, 0, 0, 0.2), 0px 2px 2px 0px rgba(0, 0, 0, 0.14), 0px 1px 5px 0px rgba(0, 0, 0, 0.12);color: #fff;font-weight: bold;height: 40px;align-items: center;justify-content: center;display: flex;');
      continueButton.onclick = function(ev){
      	ev.preventDefault();
        location.reload();
      }
      var checkoutButton = document.querySelector('.cart__checkout');
      checkoutButton.setAttribute('style', 'position:relative; top: -30px;');
      shippingText.innerText = 'Shipping calculated at checkout';
    }
    if(totalPriceNumber >= 10000) {
      var total_price = document.querySelector('.ajaxcart__totalprice');
      if(total_price != null){
          total_price.setAttribute('style','margin-bottom:15px;');
      }
      calcShipping.setAttribute('style', 'display:none');

      continueButton.setAttribute('style', 'display:none');
      var checkoutButton = document.querySelector('.cart__checkout');
      checkoutButton.setAttribute('style', 'position:relative; top: -25px;');
      shippingText.innerText = 'FREE discreet shipping for your order.';
      shippingText.setAttribute('style', 'position:relative;top:70px;display:block; font-size: 14px;')
    }
}

function testEvent () {
  setTimeout(function() {
    var calcShipping = document.querySelector('.calc-shipping');
    var continueButton = document.querySelector('.continue_btn');
    var checkoutButton = document.querySelector('.cart__checkout');

    if(document.querySelector('.continue_btn') !== null && document.querySelector('.calc-shipping') !== null) {
      calculateCartTotal();
    } else {
      var cartDrawer = document.querySelector('#CartDrawer');

      var aLink = document.createElement('a');
      	  aLink.setAttribute('href', window.location.href);
      	  aLink.setAttribute('class', 'continue_btn');
      	  aLink.innerText = 'Continue Shopping';
          aLink.setAttribute('style', 'width:100%;display:none');

      var calcShipping = document.createElement('p');
          calcShipping.innerText = '';
          calcShipping.setAttribute('class', 'calc-shipping');
          calcShipping.setAttribute('style', 'text-align:center;display:none');

      if(checkoutButton != null) {
        var ajaxcart = document.querySelector('.ajaxcart__footer');
        checkoutButton.before(calcShipping);
        checkoutButton.before(aLink);
      }

      calculateCartTotal();
    }
  }, 1300);
}

var addToDrawer = document.querySelector('#CartDrawer');
	addToDrawer ? addToDrawer.addEventListener('click', testEvent) : null;
var addToCart = document.querySelector('#AddToCart');
	addToCart ? addToCart.addEventListener('click', testEvent) : null;
var openCart = document.querySelector('.site-header__cart--toggle');
	openCart ? openCart.addEventListener('click', testEvent) : null;
var adjustButtons = document.querySelectorAll('.ajaxcart__qty-adjust');
if(adjustButtons != null){
  for(var i=0;i<adjustButtons.length;i++){
      adjustButtons[i].addEventListener('click', testEvent);
  }
}
</script>

