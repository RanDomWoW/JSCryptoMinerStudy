var __top2016key = "INDEX2016_TOP_CMS_OPEN";
var _rg = /(([^.]+\.(com|cn|net|org|biz|info|cc|tv))|((\d+\.){3}(\d+)))((?=:)|)/;
var _dm = window.location.host;
var _hs = _rg.exec(_dm)[0];

function check2016TopCms(){
	var date1 = AICAI_TOP_INFO.dateStart;
	var date2 = AICAI_TOP_INFO.dateEnd;
	var t = checkTime(date1.replace(/-/g,   "/"),date2.replace(/-/g,   "/"));
	if(!t)return;
	
	var color = AICAI_TOP_INFO.rgb;
	if(color==null || color=='' || color.split(',').length!=3)color = '203,242,247';	
	
	var _x24 = AICAI_TOP_INFO.x24;
	var isShow = false;
	if(_x24==-1){
		isShow = true;
		//setTop2016Cookie(__top2016key,"close1",24*60,_hs);//24小时		
	}else{
		var topKey = getTop2016Cookie(__top2016key);
		if(typeof (topKey) == "undefined" || topKey == ""||typeof (topKey)!= "string"|| topKey == null ){
			isShow = true;
			//setTop2016Cookie(__top2016key,"close1",24*60,_hs);//24小时					
		}else if(topKey.indexOf('close')!=-1){
			var count = topKey.replace('close','');
			count = count*1;
			if(count<_x24){
				isShow = true;												
				//setTop2016Cookie(__top2016key,"close"+(count+1),24*60,_hs);//24小时
			}			
		}		
	}
	
	if(isShow){
		var _html = '<div class="topads"><a href="'+AICAI_TOP_INFO.url+'"><img src="'+AICAI_TOP_INFO.img+'" width="1080" height="130" alt=""></a><a href="javascript:;" class="top_ads_close" onclick="top2016AdsClose();" href="">&times;</a></div>'
		$('#jq_index_2016_top_cms').css('background','rgb('+color+')');
		$('#jq_index_2016_top_cms').html(_html);
		$('#jq_index_2016_top_cms').show();
	}
}

/**是否在指定的2个时间内, 时间格式:2014/5/8 00:00:00  2014/5/12 23:59:59*/
function checkTime(startStr,endStr){		
	var starttime = new Date(startStr);
	var endtime = new Date(endStr);
    var today = new Date();
    
    var todayTimes = today.getTime();
    var starttimes = starttime.getTime();
    var endTimes = endtime.getTime();
    
    if(todayTimes>starttimes&&todayTimes<endTimes){
    	return true;
    }

    return false;
}


function top2016AdsClose(){
	$('#jq_index_2016_top_cms').hide();
	var topKey = getTop2016Cookie(__top2016key);
	if(typeof (topKey) == "undefined" || topKey == ""||typeof (topKey)!= "string"|| topKey == null ){
		setTop2016Cookie(__top2016key,"close1",24*60,_hs);//24小时
	}else if(topKey.indexOf('close')!=-1){
		var count = topKey.replace('close','');
		count = count*1+1;
		if(count<100)setTop2016Cookie(__top2016key,"close"+count,24*60,_hs);//24小时		
	}	
}

