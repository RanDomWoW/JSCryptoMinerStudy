}

function checkHomePopup(){
	var img = AICAI_HOME_INFO.img;
	if(img==null || ''==img)return;
	
	var date1 = AICAI_HOME_INFO.dateStart;
	var date2 = AICAI_HOME_INFO.dateEnd;	
	
	var t = popupTime(date1.replace(/-/g,   "/"),date2.replace(/-/g,   "/"));
	if(!t)return;		
		
	var needLgn = AICAI_HOME_INFO.needLogin;
	var next = true;
	var isShow = false;
	if('y'==needLgn){
		var loginStr = getHomeIndexCookie("CPLOGIN_INFO");
		if(loginStr != null && loginStr != 'undefined' && loginStr != ''){//已登录
			next = true;
		}else{//未登录
			next = false;
		}
	}
	
	if(!next)return;
	
	var cloTime = AICAI_HOME_INFO.closeTime;	
	if(cloTime==0){
		isShow = true;			
	}else{
		var openKey = getTop2016Cookie(_home2016key);
		if(typeof (openKey) == "undefined" || openKey == ""||typeof (openKey)!= "string"|| openKey == null ){
			isShow = true;							
		}else if('close'==openKey){
			isShow = false;				
		}		
	}
	
	if(isShow){
		setTimeout(function(){indexPopup();},1000);
		popAdsClose(cloTime);
		
		$('#jq_popup_href').live('click', function(){
			dataStat('Aaicaisyweb006');
		}); 
		
		$('#jq_popup_close').live('click', function(){
			dataStat('AaicaisyCloseweb001');
		}); 		
	}
}

/**是否在指定的2个时间内, 时间格式:2014/5/8 00:00:00  2014/5/12 23:59:59*/
function popupTime(startStr,endStr){		
	var starttime = new Date(startStr);
	var endtime = new Date(endStr);
    var today = new Date();
    
    var todayTimes = today.getTime();
    var starttimes = starttime.getTime();
    var endTimes = endtime.getTime();
    
    if(todayTimes>starttimes&&todayTimes<endTimes){
    	return true;
    }

    return false;
}

function popAdsClose(times){
	setTop2016Cookie(_home2016key,"close",times,_hst);
}

//埋点的请求操作
function dataStat(dataId){	
	userClickTrack(dataId);
}

$(document).ready(function(){
	try{checkHomePopup();}catch(e){}
});
</script>

