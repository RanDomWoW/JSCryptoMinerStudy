<script type="text/javascript">
var _home2016key = "INDEX2016_HOME_OPEN";
var _hrg = /(([^.]+\.(com|cn|net|org|biz|info|cc|tv))|((\d+\.){3}(\d+)))((?=:)|)/;
var _hdm = window.location.host;
var _hst = _hrg.exec(_hdm)[0];

function indexPopup(config){
	if(!(this instanceof indexPopup)){
		return new indexPopup(config);
	}		
	this.config =config || {};
	for (var i in indexPopup.defaults) {
		if (this.config[i] === undefined) {this.config[i] = indexPopup.defaults[i];}
	}
	this.maskDiv;
	this.adWrap;
	this.adClose;
	this.adLink;
	this.hdAction;

	this._init();		
}

indexPopup.prototype={
	constructor:indexPopup,
	docEle:document.documentElement,
	_init:function(){
		var that=this;
		var objHeight = (document.documentElement.clientHeight-that.config.height)/2;//-80
		if(objHeight<0){objHeight = 0;};
		var objWidth = (document.documentElement.clientWidth-that.config.width)/2;
		
		if (window.VBArray && !window.XMLHttpRequest) {
			objHeight = objHeight+document.documentElement.scrollTop;
		}

		that.maskDiv = document.createElement("div");
		that.maskDiv.style.cssText="position:absolute;left:0;top:0;width:100%;background:#000;opacity:0.5;filter:alpha(opacity:50); z-index:"+that.config.zIndex;
		that.maskDiv.style.height = Math.max(this.docEle.scrollHeight,this.docEle.clientHeight)+"px";
		document.getElementsByTagName("body")[0].appendChild(that.maskDiv);
		
		//广告容器外层
		that.adWrap =document.createElement("div");
		that.adWrap.style.cssText ='position:fixed;_position:absolute; width:490px; height:450px; background:url('+that.config.img+') center center no-repeat; top:'+objHeight+'px; left:'+objWidth+'px; width:'+that.config.width+'px; height:'+that.config.height+'px; z-index:'+that.config.zIndex+2;

		//关闭按钮
		that.adClose=document.createElement("a");
		that.adClose.href='javascript:void(0)';
		that.adClose.setAttribute('close','true');
		that.adClose.setAttribute('id','jq_popup_close');
		that.adClose.style.cssText = 'position:absolute; top:0; right:0; z-index: 10; width:'+that.config.btnWidth+'px; height:'+that.config.btnHeight+'px;';		
		that.adWrap.appendChild(that.adClose);

		//链接按钮
		that.adLink=document.createElement("a");
		that.adLink.href = that.config.adLink;
		that.adLink.target = '_blank';				
		that.adLink.setAttribute('id','jq_popup_href');
		that.adLink.style.cssText =' position:absolute; top:0; left:0; width:100%; height:100%';
		that.adWrap.appendChild(that.adLink);
		
		document.getElementsByTagName("body")[0].appendChild(this.adWrap);

		this._addEvent();
	},
	_addEvent:function(){
		var that = this;
		this.adWrap.onclick = function(e){
			e = e || window.event;
			ele = e.target || e.srcElement;		
			if(ele.getAttribute("close") && ele.getAttribute("close").toLowerCase()=="true"){				
				that.adWrap.onclick = null;
				that.adWrap.onmousedown = null;
				that.adWrap.cssText ='';
				that.adWrap.parentNode.removeChild(that.adWrap);
				that.maskDiv.parentNode.removeChild(that.maskDiv);
				if(that.config.closeFn){that.config.closeFn(); that.config.closeFn=null};
			}
		}
		this.adWrap.onmousedown = function(e){
			e = e || window.event;
			ele = e.target || e.srcElement;
			var mX = e.clientX-that.adWrap.offsetLeft;
			var mY = e.clientY-that.adWrap.offsetTop;
			
			if(e.preventDefault){
	            e.preventDefault();
	            e.stopPropagation();
	        }
	        if (navigator.userAgent.indexOf("MSIE")>0){
	            e.returnValue = false;
	            e.cancelBubble = true;
	        }
			document.onmousemove = function(e){
				e = e || window.event;
				that.adWrap.style.left=e.clientX-mX+"px";
				that.adWrap.style.top=e.clientY-mY+"px";
				return false;			
			}
			document.onmouseup = function(){
				document.onmousemove=null;
				document.onmouseup=null;
			}
		}

	}

}
