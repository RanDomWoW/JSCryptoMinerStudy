            $("#query_what").mkAutosuggest({ 
       api_url: '/api/what?', 
       container_id: "autosuggest_what",
       category_id_element: 'where_category_id',
       where_id_element: 'where_geo_id',
       where_name_element: 'query_where',
       next_input_element: '#query_where',
       result_groups: ["category_results", "entity_results"],
       result_group_labels: ["category", "entity"],
       no_results: 'No result found'
    });


    var current_ajax_req_where, trueFocus = true;
    $("#query_where").autocomplete({
        source: function(request, response) {
            if(current_ajax_req_where)
                current_ajax_req_where.abort();
            var term = $.trim(request.term.replace(new RegExp(/\(|\)/gi), ""));
            current_ajax_req_where = $.ajax({
                url: "/api/where?count=12",
                dataType: "json",
                data: {
                    name: term
                },
                success: function(data) {
                    if(data != null) {
                        response($.map(data.items, function(item) {
                            return {
                                id:    item.id,
                                default_radius: item.default_radius,
                                label: item.name,
                                value: item.name
                            }
                        }));
                    }
                }
            })
        },
        minLength: 1,
        appendTo: "#autosuggest_where",
        focus: function(event, ui) {
           console.event;
            return false;
        },
        open: function() {
            // make room for stupid ie7
            $("#ieAutocompleteFix").show();
        },
        close: function() {
            // avoid double-pop-up issue
            setTimeout(function() { trueFocus = true; }, 300);
            $("#ieAutocompleteFix").hide();
        },
        select: function(event, ui) {
            $("#where_geo_id").val(ui.item.id);
            // not needed for kapi
            //$("#where_default_distance").val(ui.item.default_radius > 0 ? Math.max(Math.round(parseInt(ui.item.default_radius)),1) : 30);
            $("#where_default_distance").val('');
        }
    }).blur(function() {
        setTimeout( function() { $(this).autocomplete("close"); }, 150);
    }).data("autocomplete")._renderItem = function (ul, item) {
        item.label = item.label.replace(new RegExp("(" + $.trim(this.term.replace(new RegExp(/\(|\)/gi), "")).replace(/\s/gi,"|") + ")", "gi"), "<em>$1</em>");

        return $("<li></li>")
            .data("item.autocomplete", item)
            .append($("<a></a>").html(item.label))
            .appendTo(ul);
    };

    $("#query_where").on("keyup", function(e){
        e = e || window.event;
        var key = e.keyCode || e.which;
        if ((key > 45 && key < 91) || key === 8) {
            $("#where_geo_id").val("");
            // not needed for kapi
            $("#where_default_distance").val("");
        }
    });
    $("#query_what").on("keyup", function(e){
        e = e || window.event;
        var key = e.keyCode || e.which;
        if ((key > 45 && key < 91) || key === 8) {
            $("#what_category").val("");
        }
    });

    setTimeout(function() {
        $("#query_where").bind("focus", function(event, ui) {
            if (trueFocus) { 
                $(this).autocomplete("search");
                trueFocus = false;
            }
        });
    }, 500);
