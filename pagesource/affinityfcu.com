<script type="text/javascript">
    function refactorSearch(searchType) {
        if (searchType == "desktop") {
            console.log("desktop search called");
            var searchQ = $("#header-search-input").val();
            if ($("#header-search-input").val().length > 0) {
                window.location.href = "https://www.affinityfcu.com/search.aspx?q=" + searchQ;
            } else {
                console.log("search not called due to empty query: '" + searchQ + "'");
            }
        } else {
            console.log("mobile search called");
            var searchQ = $("#header-search-input-mobile").val();
            if ($("#header-search-input-mobile").val().length > 0) {
                window.location.href = "https://www.affinityfcu.com/search.aspx?q=" + searchQ;
            } else {
                console.log("search not called due to empty query: '" + searchQ + "'");
            }
        }
    }
    $("#searchSubmit").click(function (event) {
        refactorSearch("desktop");
    });
    $(document).ready(function () {
        $(".touch-sub").hide();
    });
    $("#header-search-input").keypress(function (e) {
        if (e.which == 10 || e.which == 13) {
            refactorSearch("desktop");
        }
    });
    $("#searchSubmit-mobile").click(function (event) {
        refactorSearch("mobile");
    });
    $("#header-search-input-mobile").keypress(function (e) {
        if (e.which == 10 || e.which == 13) {
            refactorSearch("mobile");
        }
    });
    $(function () {
        $(".footer-logo-refactor a").tooltip();
    });
    //$("input").focus(function () {
        //this.value = "";
