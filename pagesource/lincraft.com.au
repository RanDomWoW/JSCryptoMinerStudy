        var productAddToCartForm_1 = new VarienForm('product_addtocart_form_1');
    productAddToCartForm_1.submit = function (button, url) {
        if (this.validator.validate()) {
            jQuery(button).attr("disabled", true);

            var form = this.form;
            var oldUrl = form.action;
            if (url) {
                form.action = url;
            }
            if (jQuery('.wishlist-index-configure, .checkout-cart-configure').length) {
                this.form.submit();
            } else {
                //   button.addClassName('loading');
                new Ajax.Request(oldUrl, {
                    method: 'post',
                    parameters: form.serialize(),
                    onSuccess: function (response) {
                        var data = response.responseText.evalJSON();
                        if (data.status == 'error') {
                            var $errorMsg = jQuery('<div class="error-message" />').text(data.message);
                            jQuery($errorMsg).appendTo(jQuery('#messages_product_view'));
                            jQuery("html, body").animate({scrollTop: 0}, 700);
                            // jQuery('.modal-error').modal({keyboard: true});

                        } else {
                            jQuery('.top-cart-wrapper').html(data.mini_cart);

                            if (Foundation.utils.is_small_only()) { // if mobile
                                jQuery('.modal-mini-cart').modal({keyboard: true});
                            } else if ($('topCartContent')) {
                                Enterprise.TopCart.initialize('topCartContent');
                                Enterprise.TopCart.showCart(7);
                                jQuery("html, body").animate({scrollTop: 0}, 700);
                            }
                        }
                    },
                    onComplete: function () {
                        if (url) {
                            form.action = oldUrl;
                        }

                        jQuery(button).removeAttr("disabled");
                        //scroll to top
                        // jQuery("html, body").animate({ scrollTop: 0}, 700);

                    }
                });
            }
        } else {
            //reflow after validation message appear
            setTimeout(function () {
                jQuery.colorbox.resize();
            }, 100);
        }

    }.bind(productAddToCartForm_1);

    productAddToCartForm_1.submitLight = function (button, url, ajax) {
        if (this.validator.validate()) {
            var form = this.form;
            var oldUrl = form.action;
            if (url) {
                form.action = url;
