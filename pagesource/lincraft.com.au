    jQuery(document).ready(function ($) {

        $('input').focusin(function () {
            input = $(this);
            input.data('place-holder-text', input.attr('placeholder'));
            input.attr('placeholder', '');
        });
        $('input').focusout(function () {
            input = $(this);
            input.attr('placeholder', input.data('place-holder-text'));
        });
    });

    var RegPopUpSession = 'RegPopUp_Australia_Session';
    var RegPopUpStore = 'RegPopUp_Australia';

    var expiryDate = new Date();
    expiryDate.setDate(expiryDate.getDate() + 60);

    var sessionExpiryDate = '3600';
    var sessionDate = new Date();
    sessionDate.setTime(sessionDate.getTime() + (parseInt(sessionExpiryDate) * 1000));

    var ckRegPopUpSession = Mage.Cookies.get(RegPopUpSession);

    if (ckRegPopUpSession == null) {
        var valSession = 1;
        Mage.Cookies.set(RegPopUpSession, valSession, sessionDate);
        var ckRegPopUpStore = Mage.Cookies.get(RegPopUpStore);
        if (ckRegPopUpStore == null) {
            Mage.Cookies.set(RegPopUpStore, valSession, expiryDate);
        } else {
            var countVal = parseInt(ckRegPopUpStore, 10) + parseInt(valSession, 10);
            Mage.Cookies.set(RegPopUpStore, countVal, expiryDate);
        }
    }
