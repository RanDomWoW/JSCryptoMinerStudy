function autoCambia()
{
if(1 > 2)
{
   if(document.getElementById('campanacambiada').value == 0)
   {
		var campanaactual = document.getElementById('campananumero').value;
		
		if(campanaactual == 1)
		{
		var campananueva = 1;
		}
		else
		{
		campananueva = Number(campanaactual) + Number('1') ;
		}
		
		var miniImagen = "miniImagen"+campananueva;
		var imagen = document.getElementById(miniImagen).value;
		FadeInImage('noticiaimagen',imagen,'asd',campananueva);
		document.getElementById('campananumero').value = campananueva;
		setTimeout("autoCambia()",2000);
	}
	else
	{
	document.getElementById('campanacambiada').value = 0;
	setTimeout("autoCambia()",5000);	
	}
	
	}
}  
function addCommas(nStr)
{
	nStr += '';
	x = nStr.split('.');
	x1 = x[0];
	x2 = x.length > 1 ? '.' + x[1] : '';
	var rgx = /(\d+)(\d{3})/;
	while (rgx.test(x1)) {
		x1 = x1.replace(rgx, '$1' + ',' + '$2');
	}
	return x1 + x2;
}
function despliegaVisitante(recibido){
visitas=((recibido - 23714711)+5877330);
visitas= addCommas(visitas);
document.getElementById('contadorFinal').innerHTML  = visitas;
document.getElementById('cargando2').style.display= 'none';
}
function cargadoDePagina()
{
new Ajax.Request("contador.php", { 
onSuccess : function(resp) { 
despliegaVisitante(resp.responseText); 
}, 
onFailure : function(resp) { 
alert("Oops, there's been an error."); 
}
});
