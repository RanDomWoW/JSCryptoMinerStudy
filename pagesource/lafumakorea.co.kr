	<script>
	function setCookie(name, value, expiredays)
	{
	    var todayDate = new Date();
	    todayDate.setDate( todayDate.getDate() + expiredays );
	    document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";"
	}

	function getCookie(cname) {
		var name = cname + "=";
		var ca = document.cookie.split(';');
		for(var i=0; i<ca.length; i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1);
			if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
		}
		return "";
	}

	function wOPEN(U, N, W, H, T, L) {
		if(!L) {L=0;}
		if(!T) {T=0;}

		window.open(U, N,'width='+ W +',height='+ (H+30) +',top='+ T +',left='+ L +',toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no');
	}

	$(".close-popup").click(function(e){
		e.preventDefault();
		$(".layer-popup").hide();
	});

	$(".hide-popup").click(function(e){
    	setCookie("POPUP_20161129", "done" , 1);
    	$(".close-popup").trigger('click');
