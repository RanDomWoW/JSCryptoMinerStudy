@import url('https://fonts.googleapis.com/css?family=Dancing+Script:700');
@-webkit-keyframes simpleBannerSlideInFromBottom {
    0% {
        -webkit-transform: translateY(100%);
        transform: translateY(100%);
    }
    100% {
        -webkit-transform: translateY(0);
        transform: translateY(0);
    }
}

@keyframes simpleBannerSlideInFromBottom {
    0% {
        -webkit-transform: translateY(100%);
        transform: translateY(100%);
    }
    100% {
        -webkit-transform: translateY(0);
        transform: translateY(0);
    }
}

.fixed-banner-minimized {
    display: none;
}

.fixed-bubble-banner-wrapper {
    -webkit-animation: 1s ease-in 0s 1 simpleBannerSlideInFromBottom;
    animation: 1s ease-in 0s 1 simpleBannerSlideInFromBottom;
    background: #000;
    background: linear-gradient(#333, #000);
    left: 0;
    margin: 0 auto;
    padding: 7px;
    position: fixed;
    right: 0;
}

.fixed-bubble-banner-bottom {
    bottom: 0;
}

.fixed-bubble-banner-top-copy {
    background-color: #fff;
    border-top-right-radius: 0.33em;
    bottom: 47px;
    font-size: 20px;
    font-weight: bold;
    left: -33px;
    line-height: 1;
    padding: 0.25em 0.5em 0.33em 33px;
    position: absolute;
}

.fixed-bubble-banner-close {
    background-color: #999;
    border: 4px solid #fff;
    border-radius: 50%;
    color: #fff;
    cursor: pointer;
    font-size: 30px;
    height: 1em;
    line-height: 1;
    text-align: center;
    font-weight: bold;
    position: absolute;
    right: -50px;
    top: -25px;
    width: 1em;
}

.fixed-bubble-banner-close:hover,
.fixed-bubble-banner-close:focus {
    background-color: #f00;
}

.fixed-bubble-banner-close:active {
    background-color: #900;
}

.fixed-bubble-banner-copy {
    display: table;
    margin: 0 auto;
    position: relative;
}

.fixed-bubble-banner-copy .headline {
    color: #fff;
    display: table-cell;
    font-size: 18px;
    line-height: 1.5;
    text-align: center;
    text-shadow: 0 0 5px rgba(0, 0, 0, 0.9);
    vertical-align: middle;
}

.fixed-bubble-banner-copy .fixed-bubble-banner-cta-wrapper {
    display: table-cell;
    padding: 0 0 0 10px;
    vertical-align: middle;
}

.fixed-bubble-banner-copy .fixed-bubble-banner-cta:before {
    background: -webkit-linear-gradient(left top, left bottom, from(rgba(255, 255, 255, 0.6)), to(rgba(255, 255, 255, 0.1)));
    background: linear-gradient(rgba(255, 255, 255, 0.6), rgba(255, 255, 255, 0.1));
    border-radius: 100px;
    content: '';
    height: 50%;
    left: 0;
    margin: auto;
    position: absolute;
    right: 0;
    top: 4px;
    width: 90%;
}

.fixed-bubble-banner-copy .fixed-bubble-banner-cta {
    background: #f86a01;
    background: linear-gradient(#fe862e, #f86a01, #f86a01, #cf5901);
    border-radius: 100px;
    color: #fff;
    display: block;
    font-family: 'Open Sans Condensed', Verdana, Helvetica, sans-serif;
    font-size: 15px;
    font-weight: bold;
    line-height: 1.2;
    padding: 10px 35px 12px 25px;
    position: relative;
    text-align: left;
    text-shadow: 0px -1px rgba(0, 0, 0, 0.33);
}

.fixed-bubble-banner-copy .fixed-bubble-banner-cta>.fixed-bubble-banner-cta-text {
    position: relative;
    z-index: 10;
}

.fixed-bubble-banner-copy .fixed-bubble-banner-cta>.fa {
    bottom: 0;
    font-size: 18px;
    height: 50%;
    margin: auto;
    position: absolute;
    right: 15px;
    top: 0;
    z-index: 10;
}

.fixed-bubble-banner-copy .fixed-bubble-banner-cta:after {
    border-radius: 100px;
    content: '';
    height: 100%;
    left: 0;
    position: absolute;
    top: 0;
    width: 100%;
}

.fixed-bubble-banner-copy .fixed-bubble-banner-cta:hover:after,
.fixed-bubble-banner-copy .fixed-bubble-banner-cta:focus:after {
    background-color: rgba(255, 255, 255, 0.15);
}

.fixed-bubble-banner-copy .fixed-bubble-banner-cta:active:after {
    background-color: rgba(0, 0, 0, 0.15);
}

.fixed-bubble-banner-icon {
    bottom: 5px;
    position: absolute;
    right: 102%;
}

.fixed-bubble-banner-icon>.fa {
    background-color: #fff;
    border: 4px solid #000;
    border-radius: 50%;
    color: #000;
    font-size: 1.5em;
    padding: 0.3em 0.4em 0.35em;
}

.fixed-bubble-banner-icon>img {
    border: 4px solid #fff;
    border-radius: 50%;
    display: block;
}


