	<script>
	$(document).ready(function(){
	
		$("#modal-login .btn_auth").click(function(){
			if($("#modal-login .btn_auth").attr("data-ctrl") != "Y") {
				var LOGIN=$(".LOGIN_auth").val();
				var PASSWORD=$(".PASSWORD_auth").val();
				$("#modal-login .btn_auth").attr("data-ctrl", "Y");

				$.post("/ajax/ulogin.php", {"ACTION":"AUTH", "LOGIN":LOGIN, "PASSWORD":PASSWORD}, function(data){
					$(".show_bl").slideUp(1);
					$("#modal-login .btn_auth").attr("data-ctrl", "N");
					if(data.indexOf("ERROR_") != -1){
						$(".error_show").empty().append(data.replace("ERROR_", "")).slideDown();
					}else if(data.indexOf("OK_") != -1){
						$(".ok_show").empty().append(data.replace("OK_", "")).slideDown();	
						setTimeout(function(){
							location.reload();
						},1000);
					}
					setTimeout(function(){
						$(".show_bl").slideUp(1);
					},5000);
				});	
			}
		});
		
		$("#modal-registration .btn_reg").click(function(){
			if($("#modal-registration .btn_reg").attr("data-ctrl") != "Y") {
				var NAME=$(".NAME_reg").val();
				var LOGIN=$(".LOGIN_reg").val();
				var PASSWORD=$(".PASSWORD_reg").val();
				var AGREEMENT=$(".AGREEMENT_reg:checked").val();
				if(!AGREEMENT) AGREEMENT="";
				
				$("#modal-registration .btn_reg").attr("data-ctrl", "Y");				
				$.post("/ajax/ulogin.php", {"ACTION":"REG", "NAME":NAME, NAME:NAME, "LOGIN":LOGIN, "PASSWORD":PASSWORD, AGREEMENT:AGREEMENT}, function(data){
					$(".show_bl").slideUp(1);
					$("#modal-registration .btn_reg").attr("data-ctrl", "N");
					if(data.indexOf("ERROR_") != -1){
						$(".error_show").empty().append(data.replace("ERROR_", "")).slideDown();
					}else if(data.indexOf("OK_") != -1){
						$(".ok_show").empty().append(data.replace("OK_", "")).slideDown();	
						setTimeout(function(){
							location.reload();
						},1000);
					}
					setTimeout(function(){
						$(".show_bl").slideUp(1);
					},5000);
				});
			}
		});
		
		$("#modal-checkemail .btn_checkemail").click(function(){
			var CODE=$("#modal-checkemail .CODE_checkemail").val();
			var TOKEN=$("#modal-checkemail .TOKEN_checkemail").val();
			if(CODE){
				uloginCallback(TOKEN, CODE);
			}else{
				$(".show_bl").slideUp(1);
				$(".error_show").empty().append("Введите код подтверждения").slideDown();
				setTimeout(function(){
					$(".show_bl").slideUp(1);
				},5000);
			}
		});
		
		
		
		
	});
	function uloginCallback(token, code){
		if(token){
			if(!code) code="";
		
			$.post("/ajax/ulogin.php", {ACTION:"TOKEN", TOKEN:token, CODE:code}, function(data){
				$(".show_bl").slideUp(1);
				if(data.indexOf("CHECKEMAIL_") != -1){
				
					$("#modal-checkemail .TOKEN_checkemail").val(token);
					$("#modal-checkemail .txt_checkemail").empty().append(data.replace("CHECKEMAIL_", ""));

					$("#modal-login").modal('hide');
					$("#modal-registration").modal('hide');
					setTimeout(function(){
						$("#modal-checkemail").modal('show');
					}, 200);

				}else if(data.indexOf("ERROR_") != -1){
					$(".error_show").empty().append(data.replace("ERROR_", "")).slideDown();
				}else if(data.indexOf("OK_") != -1){
					$(".ok_show").empty().append(data.replace("OK_", "")).slideDown();	
					setTimeout(function(){
						location.reload();
					},1000);
				}
				setTimeout(function(){
					$(".show_bl").slideUp(1);
				},5000);
			});
		}
	}	
