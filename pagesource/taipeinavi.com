				objArray = new Array('div1','div2','div3');
				objArray_title = new Array('ホテル予約','エステ予約','ツアー予約');
				liArray = new Array('tab01','tab02','tab03');
			}
			
		 for(var i = 0; i < liArray.length; i++)
		 {
		 	var text = objArray_title[i];
