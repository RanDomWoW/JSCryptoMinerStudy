<script>
$(document).ready(function(){
  htmlObj=$.ajax({url:"https://www.ticket-easy.cn/member/loginInfoNav",async:false});
  $(".m-loginbar").html(htmlObj.responseText);  
  
  	$.ajax({
        type: "GET",
        contentType: "application/json; charset=utf-8",
        data: {},
        url: "https://www.ticket-easy.cn/index/showinfo", 
		async: true, 
		dataType: "json",
        success: function(data){
	        var collectionShowIds=data[0];
		  	var orderShowIdMap=data[1];
		  	if(collectionShowIds!=''||orderShowIdMap!=null){
			  	$(".monOne li[mid!='']").each(function(){
			  		var showIdAndGroupIdStr=$(this).attr('mid').split('_');
			  		var showIdStr=showIdAndGroupIdStr[0];
			  		var showGroupIdStr=showIdAndGroupIdStr[1];
			  		var showDate=$(this).attr('id');
			  		var isBuy=false;
			  		if(orderShowIdMap!=null){
			  			var showIdArray=showIdStr.split(',');
				  		for(var i=0,len=showIdArray.length;i<len;i++){
				  			if(orderShowIdMap[showDate+"-"+showIdArray[i]]!=null){
				  				$(this).children('b').removeClass('gray').addClass('green');
				  				isBuy=true;
				  				break;
				  			}
				  		}
			  		}
			  		if(collectionShowIds!=''&&isBuy!=true){
				  		var showGroupIdArray=showGroupIdStr.split(',');
				  		for(var i=0,len=showGroupIdArray.length;i<len;i++){
				  			if(collectionShowIds.indexOf(","+showGroupIdArray[i]+",")>-1){
				  				$(this).children('b').removeClass('gray').addClass('red');
				  				break;
				  			}
				  		}
			  		}
			  	})
		  	}
		  	
		}
    });
    
    <!-- 获取猜你喜欢数据 -->
    $.ajax({
        type: "GET",
        contentType: "application/json; charset=utf-8",
        data: {},
        url: "https://www.ticket-easy.cn/index/getGuessShowGroup", 
		async: true, 
		dataType: "json",
        success: function(data){
        	if(data != null && data.length > 0){
        		for(var i = 0; i < data.length; i++){
        			var obj = data[i];
        			
        			var sgNameLength = obj.sgName.length;
        			var sgName = obj.sgName;
        			if(sgNameLength > 23){
        				sgName = sgName.substring(0, 20) + "...";
        			}
        			
        			var str = "<li><img class='tag' src='https://images.ticket-easy.cn/static/images/banner/pic_tag.png' />";
        				str+= "<img src='https://images.ticket-easy.cn/res"+ obj.imgPath+ "' alt='111'  class='pic'>";
        				str+= "<a href='https://www.ticket-easy.cn/item/"+ obj.sgId +"' title='"+ obj.sgName +"' target='_blank' style='display: none;'>";
        				str+= sgName;
        				str+= "<br/>票价： <mark>RMB" + obj.minPrice + "</mark>起</a>   </li>";
        			$("#guessUl").append(str);
        		}
        		
        		$('.picList li').hover(function(){
					$('a',$(this)).css('display','block');
				},function(){
					$('a',$(this)).css('display','none');
				})
        	}
		}
    });
});
