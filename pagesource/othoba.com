<script type="text/javascript">
    $(document).ready(function () {
        if ($(window).width() <= 768) {
            $('.hasChild>a').attr('href', '#');
            $(".dvToggler").click(function () {
                $("div.dvCategories div.dvMenu").fadeToggle();
            });

            $(".dvMenu a").click(function (e) {
                var menuli = $(e.target).closest('li');
                var dvMenu = $(".dvMenu");
                var setLelt = '-100%';
                if ($(menuli).hasClass('hasChild')) {
                    if ($(dvMenu).offset().left != 0) {
                        setLelt = '-200%';
                        var categories = $(".hasCategory");
                        for (var i = 0; i < $(categories).length; i++) {
                            $(categories[i]).css('top', '');
                        }
                    }
                    $(dvMenu).css('left', setLelt);
                    var submenu = $(menuli).find('.hasCategory').get(0);
                    $(submenu).show();
                    var caltop = $(submenu).offset().top - $(".dvheader").height() - 37;
                    $(submenu).css('top', '-' + caltop + 'px')
                }
            });
            $(".liBackMe").click(function (e) {
                var windowWidth = window.innerWidth;
                var dvMenuLeft = $(".dvMenu").offset().left * -1; dvMenuLeft
                var level = Math.ceil(dvMenuLeft / windowWidth); level
                var dvCategory = $(e.target).parent().parent().parent();
                if (level == 1) {
                    $(dvCategory).css('top', '');
                    $(dvCategory).hide();
                    $(".dvMenu").css('left', '');
                }
                else if (level == 2) {


                    $(dvCategory).css('top', '');
                    $(dvCategory).hide();
                    $(".dvMenu").css('left', '-100%');

                    var parentCategory = $(dvCategory).parent().parent().parent();
                    var caltop = $(parentCategory).offset().top - $(".dvheader").height() - 37;
                    $(parentCategory).css('top', '-' + caltop + 'px')
                }

            });
        }
    });
</script>
