        });

        //Menu script for mobile mode
        if ($(window).width() <= 768) {
            $(".hasChild").each(function () {
                $(this).children(":first").attr("href", "#");
            });

            $('.dvToggler').click(function () {
                $('#dvPrimaryNav').toggle();
            });


            $('.dvCategorylnk a').click(function () {
                var subCategory = $('.dvSubCategory.open');
                        $.each(subCategory, function (i) {
                            $(subCategory[i]).removeClass("open").toggle();
                            $(this).next('.dvSubCategory').removeClass("open").toggle();
                        });
                $(this).children(":first").attr("href", "#");
                $(this).next('.dvSubCategory').addClass("open").toggle();
            });

         
        }
    });
</script>







