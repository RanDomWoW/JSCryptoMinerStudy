function monthObj(monthname,monthdays)

{

this.m_month=monthname;

this.m_days=monthdays;

}

var now2=new Date();

var monthdays=new Object(12);

var year=now2.getFullYear();

var month=now2.getMonth();

var day=now2.getDate();

function isLeapyear(year)

{

if(year%4==0)

   {

   if ((year%100==0)&&(year%400!=0)){return 0;}else{return 1;}		

   }

return 0;

}

function stndrdth(n_ordinal)

{

var ordtext="";

var nmm=n_ordinal;

while(nmm>=100){nmm-=100;}

if((nmm>=11)&&(nmm<=19)){ordtext="th";}

else{while(nmm>=10){nmm-=10;}}

if(nmm==0){ordtext="th";}

if(nmm==1){ordtext="st";}

if(nmm==2){ordtext="nd";}

if(nmm==3){ordtext="rd";}

if((nmm>=4)&&(nmm<=10)){ordtext="th";}

return "<b>"+parseInt(n_ordinal)+"</b>"+ordtext;

}
