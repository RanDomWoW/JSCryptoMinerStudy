	function validateRMSForm(position) {		
		var error_count = 0;
		// id values for fields to validate
		var required_fields = new Array('fname-rms', 'lname-rms', 'email-rms', 'phone-rms', 'disease-rms');
		var focus_field;
		// loop the required fields and check their value
		for(i=0; i<required_fields.length; i++) {
			var required_field = required_fields[i];
			// alter id value if the bottom version of form
			if (position == 'bottom') {
				required_field = 'bottom-' + required_field;
			}
			var error = checkRMSField(required_field);
			if (error) {
				error_count = error_count + 1;	
				if (focus_field == '') {
					var field_selector = '#' + required_field;
					jQuery(field_selector).focus();	
				}
			}
		}
		// if errors found, display message, kill form submit
		if (error_count > 0) {
			var field_message = error_count == 1 ? 'field' : 'fields';
			var error_message = 'Please complete the required<br>' + field_message + ' highlighted below.';
			var elem_prefix = '#rms-form-wrapper #form ';
			if (position == 'bottom') {
				elem_prefix = '#bottom-rms-form-wrapper #form ';
			}
			jQuery(elem_prefix + 'p.title').addClass('form-error').html(error_message);
			jQuery(elem_prefix + 'p.sub-title').addClass('hidden');
			return false;			
		} else {
			// update the bot_check field value here, bots won't be smart enough to post the valid value for this field
			if (position == 'bottom') {
				jQuery('#bottom-bot-check').val('yes');				
			} else {
				jQuery('#bot-check').val('yes');
			}
			jQuery('form').preventDoubleSubmission();
			
			// now we need to check if in after-hours mode, and if so, submit the lead to our database via Ajax with queued_ah status then show popup
			var is_after_hours = jQuery('input[name=after_hours]').val();
			if (is_after_hours == '1') {
				var handler_url = '/wp-content/plugins/rms-forms/form-processor.php';
				var elem_prefix = '#rms-form-wrapper #form ';
				if (position == 'bottom') {
					elem_prefix = '#bottom-rms-form-wrapper #form ';
					var form_position = 'Bottom';
				}
				var fname = jQuery(elem_prefix + 'input[name=fname]').val();
				var lname = jQuery(elem_prefix + 'input[name=lname]').val();
				var phone = jQuery(elem_prefix + 'input[name=phone]').val();
				var email = jQuery(elem_prefix + 'input[name=email]').val();
				var disease = jQuery(elem_prefix + 'select[name=disease]').val();
				var email_optin = jQuery(elem_prefix + 'input[name=email_optin]').is(':checked') ? 1 : 0;
				var inquiry_method = jQuery(elem_prefix + 'input[name=inquiry_method]').val();
				var lead_status = jQuery(elem_prefix + 'input[name=lead_status]').val();
				var return_url = jQuery(elem_prefix + 'input[name=return_url]').val();
				var refer_url = jQuery(elem_prefix + 'input[name=refer_url]').val();
				var assignment_type = jQuery(elem_prefix + 'input[name=assignment_type]').val();
				var notify_owner = jQuery(elem_prefix + 'input[name=notify_owner]').val();
				var notify_visitor = jQuery(elem_prefix + 'input[name=notify_visitor]').val();
				var owner_template = jQuery(elem_prefix + 'input[name=owner_template]').val();
				var visitor_template = jQuery(elem_prefix + 'input[name=visitor_template]').val();
				var lead_source = jQuery(elem_prefix + 'input[name=lead_source]').val();
				var seminar_date = jQuery(elem_prefix + 'input[name=seminar_date]').existsWithValue() ? jQuery(elem_prefix + 'input[name=seminar_date]').val() : '';
				var seminar_location = jQuery(elem_prefix + 'input[name=seminar_location]').existsWithValue() ? jQuery(elem_prefix + 'input[name=seminar_location]').val() : '';
				var how_attending = jQuery(elem_prefix + 'select[name=how_attending]').existsWithValue() ? jQuery(elem_prefix + 'select[name=how_attending]').val() : '';
				if (how_attending == '') {
					how_attending = jQuery(elem_prefix + 'input[name=how_attending]').existsWithValue() ? jQuery(elem_prefix + 'input[name=how_attending]').val() : '';	
				}
				var seminar_time = jQuery(elem_prefix + 'select[name=seminar_time]').existsWithValue() ? jQuery(elem_prefix + 'select[name=seminar_time]').val() : '';
				if (seminar_time == '') {
					seminar_time = jQuery(elem_prefix + 'input[name=seminar_schedule_time]').existsWithValue() ? jQuery(elem_prefix + 'input[name=seminar_schedule_time]').val() : '';	
				}
				
				jQuery.post( handler_url, 
					{ submit_ah: 1, fname: fname, lname: lname, phone: phone, email: email, disease: disease, email_optin: email_optin, inquiry_method: inquiry_method, lead_status: lead_status, return_url: return_url, refer_url: refer_url, assignment_type: assignment_type, notify_owner: notify_owner, notify_visitor: notify_visitor, owner_template: owner_template, visitor_template: visitor_template, lead_source: lead_source, seminar_date: seminar_date, seminar_location: seminar_location, how_attending: how_attending, seminar_time: seminar_time, form_position: form_position }
				)
				.done(function(result) {
					if (result != '') {
						// check to see if the result is a dupe
						if (result == 'dupe') {
							// in this case, just redirect to the thank you page, do not show new form
							window.location.href = return_url;
						} else {
							// we have a new ID, so set the hidden ID field in the popup to this value
							jQuery('#ah-lead-id').val(result);
							window.location.href = '#popup-after-hours';
						}
					} else {
						// something went wrong, just send along to the thank you page
						window.location.href = return_url;	
					}
				});

				// don't push the form through since lead queued up for now and showing the modal
				return false;
			} else {
				return true;
			}
		}
	}
	
	// funtion for checking value of individual form fields
	function checkRMSField(field_id) {
		// set the jQuery selector based on the form field's id attribute
		var field_selector = '#' + field_id;
		var error = false;
		var field_value = jQuery(field_selector).val().replace(/^\s+|\s+$/g, '');
		// check to see if the field is using the jquery placeholder fallback and if so, don't allow that value or blank value
		var placeholder_value = '';	
		if (jQuery(field_selector).is('[placeholder]')) {
			placeholder_value = jQuery(field_selector).attr('placeholder');	
		}
		var phone_len = 0;
		// if a phone number, clean out the value and set the length, should be at least 10 characters
		if (field_id == 'phone') {
			var phone_clean = field_value.replace(/\D/g,'');
			var phone_len = phone_clean.length;
		}
		// if field empty or equal to the placeholder, add class that highlights the field and triggers error
		if (field_value == '' || field_value == placeholder_value || (field_id == 'phone' && phone_len < 10)) {
			jQuery(field_selector).addClass('field-error');
			error = true;
		} else {
			jQuery(field_selector).removeClass('field-error');	
		}
		
		return error;
	}
</script>

