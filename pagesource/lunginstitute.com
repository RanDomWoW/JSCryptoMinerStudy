                jQuery.fn.preventDoubleSubmission = function() {
                    jQuery(this).on('submit',function(e){
                        var $form = jQuery(this);

                        if ($form.data('submitted') === true) {
                            // Previously submitted - don't submit again
                            e.preventDefault();
                        } else {
                            // Mark it so that the next submit can be ignored
                            $form.data('submitted', true);
                        }
                    });

                    // Keep chainability
                    return this;
                };

                jQuery('form').submit(function(){
                    $('input[type="submit"]', 'form').button('loading');
                    $(this).find(':submit').attr('disabled','disabled');
                });

                jQuery.fn.existsWithValue = function() {
                    return this.length && this.val().length;
                }

                function handleAfterHours(goto_form) {
                    var ah_lead_id = jQuery('#ah-lead-id').val();
                    var handler_url = '/wp-content/plugins/rms-forms/afterhours-form-processor.php';
                    if (ah_lead_id != '') {
                        if (!goto_form) {
                            // if not continuing, finalize submit of queued_ah lead
                            window.location.href = handler_url + '?submit_final=' + ah_lead_id;
                        } else {
                            // continue to the form but through an intermediary file that will set the lead ID as session
                            window.location.href = '/after-hours/';
                        }
                    } else {
                        // if there's no generated lead ID, then something is awry
                    }
                }
