					$expander.toggleClass('fa-toggle-down',!isExpanded).toggleClass('fa-toggle-up',isExpanded);
				}
				
				function CategoryListWidget__init(widgetSelector){
					var $widget = $(widgetSelector);
										
					$widget.find('.category-list-widget__expander').each(function(ind, elem){
						var $item = $(elem);
						CategoryListWidget__itemRefreshExpanded($item);
					});
					
					$widget.find('.category-list-widget__expander').each(function(i,elem){
						var $expander = $(elem);
						var $item = $expander.closest('.category-list-widget__item');
						$expander.click(function(){
							$item.toggleClass('category-list-widget__item--expanded');
							CategoryListWidget__itemRefreshExpanded($item);
						});
					})
				}
				
				$(function(){
					CategoryListWidget__init('#w0');
				});
