<script type="text/javascript">
	function show_password_popup(cms_page_id, redirect_url, hide_close) {
		$(document).find('#PasswordPageModalBox').modal('show');
		if (hide_close == "1") {
			$('#PasswordPageModalBox').find("button.close").remove();
		} else {
			$('#PasswordPageModalBox').find("a.back-to-home").hide();
		}
		$("#cms_pwd_page_submit").click(function () {
			$('#PasswordPageModalBox').find("div.error").hide();
			if ($("#cms_pwd_page_input").val() == "") {
				$('#PasswordPageModalBox').find("div.error").html("Please enter password.");
				$('#PasswordPageModalBox').find("div.error").show();
				return false;
			}
			$('#PasswordPageModalBox').find("div.error").hide();
			$('#PasswordPageModalBox').find(".progress_bar_submit").attr('disabled', true);
			$('#PasswordPageModalBox').find(".ajax_progress_bar").show();
			$.ajax({
				type: "POST",
				url: base_url + "valid-page-pwd/",
				data: {cms_id: cms_page_id, password: $("#cms_pwd_page_input").val()},
				success: function (msg) {
					msg_spt = msg.split(ajax_separator);
					if (msg_spt[0] == ajax_success) {
						window.location.href = redirect_url;
					} else {
						$('#PasswordPageModalBox').find("div.error").html("Invalid password.");
						$('#PasswordPageModalBox').find("div.error").show();
						$("#cms_pwd_page_input").val("");
						return false;
					}
				},
				complete: function (msg) {
					$('#PasswordPageModalBox').find(".progress_bar_submit").attr('disabled', false);
					$('#PasswordPageModalBox').find(".ajax_progress_bar").hide();
				}
			});
		});
	}
