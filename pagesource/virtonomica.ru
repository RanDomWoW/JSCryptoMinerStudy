        <script>
            var text = ["многопользовательская","увлекательная","умная","нестандартная","интеллектуальная","многогранная","уникальная","полезная","реалистичная","захватывающая"];
            var i = 0;
            $("#place").text(text[i]);

            setInterval(function() {
                $("#place").delay(700).animate({ // первоначальная задержка
                    opacity: 0
                }, 400, function() { // время затухания перед сменой
                    i++;                       
                    if (i==text.length) i=0;
                    $("#place").text(text[i]);                                         
                    $("#place").css("top","-100px");
                    $("#place").animate({
                        top: "+=100",
                        opacity: 1,                                                
                    }, 1000,'easeOutBounce', function() { // время анимации
                        // end
                    });
                });    
            }, 3500) // сумма всех задержек + ещё чё то
            
