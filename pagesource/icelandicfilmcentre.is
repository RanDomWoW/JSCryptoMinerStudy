<script>
window.db = {
    now : function(){return new Date().getTime()},
    get  : function(key) {
        var entry = JSON.parse(localStorage.getItem(key)||"0");
        if (entry.ttl && entry.ttl + entry.now < db.now()) {
            localStorage.removeItem(key);
            entry = 0;
        }
        return entry ? entry.value : 0;
    },
    set : function( key, value, ttl ) {
        localStorage.setItem( key, JSON.stringify({
            ttl   : ttl || 0,
            now   : db.now(),
            value : value
        }) );
    }
};
(function(){
var bodyElm = document.querySelector('body');
var bgholder = document.querySelector('.backgroundholder');
var bgtitle = bgholder.querySelector('.stilltitle');
if ( !bodyElm.classList.contains('home') ) {
    var bgimg = window.db.get('still');
    if (bgimg) {
        bgimg = bgimg.split(';');
        bgholder.style.backgroundImage = 'url("'+ bgimg[0] +'")';
        bgtitle.innerHTML = bgimg[1];
    }
}
})();
</script>

  
  

