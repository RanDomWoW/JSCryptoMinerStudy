			var TNZCookie = function() {
				var pub = {};

				// http://www.w3schools.com/js/js_cookies.asp
				pub.set = function(cname, cvalue, exdays) {
				    var d = new Date();
				    d.setTime(d.getTime() + (exdays*24*60*60*1000));
				    var expires = "expires="+d.toUTCString();
				    //var secure = TNZ.URL.isHTTPS() ? ';secure' : ''; Insecure cookies till site is fully https
				    var secure = '';
				    document.cookie = cname + "=" + cvalue + "; " + expires + secure + " ;domain=.newzealand.com;path=/";
				};

				pub.get = function(cname) {
				    var name = cname + "=";
				    var ca = document.cookie.split(';');
				    for(var i=0; i<ca.length; i++) {
				        var c = ca[i];
				        while (c.charAt(0)==' ') c = c.substring(1);
				        if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
				    }
				    return undefined;
				};

				pub.delete = function(cname) {
				    var d = new Date();
				    d.setTime(d.getTime() - 1);
				    var expires = "expires="+d.toUTCString();
				    var secure = TNZ.URL.isHTTPS() ? ';secure' : '';
				    document.cookie = cname + "=; " + expires + secure + " ;domain=.newzealand.com;path=/";
				};

				return pub;
			}();
		</script> <script>
			
