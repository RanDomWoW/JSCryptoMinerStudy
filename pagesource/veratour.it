            $("a.limiter-toggle").click(function () {
                var target = $(this).attr("limiter-target");
                $("#" + target + "-limited-text").toggle();
                $("#" + target + "-full-text").toggle();
                if ($.trim($(this).html()) == $(this).attr("closed-text")) {
                    $(this).html($(this).attr("open-text"));
                } else {
                    $(this).html($(this).attr("closed-text"));
                }
                if ($(this).html() == $(this).attr("closed-text")) {
                    //
                }
            });


            //create events to hide and show actions
            (function ($) {
                $.each(['show', 'hide'], function (i, ev) {
                    var el = $.fn[ev];
                    $.fn[ev] = function () {
                        this.trigger(ev);
                        return el.apply(this, arguments);
                    };
                });
            })(jQuery);

        });
    </script>
