    onAdLoad: function(win, doc, placement) {
      setTimeout(function() {
        var adFrame = win.frameElement;
        var adSpan = doc.getElementById("adSpan");
        var adDiv = doc.getElementById("adDiv");

        if (adSpan) {
          if (adFrame == document.getElementById('ad-panorama')) {
            // Never resze panorama...
          } else {
            var w = doc.querySelector('td').offsetWidth;
            var h = Math.max(adDiv.offsetHeight, adSpan.offsetHeight);

            adFrame.style.width = w + "px";
            adFrame.style.height = h + "px";
          }
        } else {
          console.log("Could not find adSpan");
        }
      }, 100);
