    getAdHtml: function(slot) {
      var set = (this.flagged || this.noas) ? "sub" : "all";
      if (set == 'all' && !this[set][slot]) {
        set = 'sub';
      }
      return this[set][slot].replace(new RegExp('INSERT_RANDOM_NUMBER', 'g'), this.pvid);
