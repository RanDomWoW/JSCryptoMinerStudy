<script>
    function loginYoxla()    {

        var pass=$('input[name=passl]').val();
        var email=$('input[name=emaill]').val();
        if ($.trim(pass).length < 1) {
            $('.info').html('<br/>Şifrəni daxil.');
            return false;
        }

        if (validateEmail(email)) {
            $('.info').html('<br/>Göndərildi');
            login();
            return false;
            window.location.reload(true);
        }
        else {
            $('.info').html('<br/>E-poçt ünvanı düzgün deyil.');
            return false;
        }

        function validateEmail(sEmail) {
            var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
            if (filter.test(sEmail)) {
                return true;
            }
            else {
                $('.info').html('<br/>E-poçt ünvanı düzgün deyil.');
            }
        };
    };
    function login()
    {
        $.ajax({
            type:"POST",
            url:"login.php",
            data:{"email":$('input[name=emaill]').val(),"pass":$('input[name=passl]').val()},
            success:function(data){
                if(data=="")
                {
                    location.reload();
                }
                else
                {
                    $('.info').html(data);
                }

            }
        });
    };
</script>

