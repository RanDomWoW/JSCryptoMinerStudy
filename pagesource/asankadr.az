                return false;
            }
            if ($.trim(email).length <= 5) {
                $('#responcepmain').html('<br/><span style="color: red;">E-poçt ünvanını daxil edin.</span><br/>');
                return false;
 }


            if ($.trim(pass).length <= 1) {
