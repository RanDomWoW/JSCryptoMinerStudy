<script>
      function muraciet()

      {
          var namemur=$('input[name=namemur]').val();
          var emailmur=$('input[name=emailmur]').val();
          var phonemur=$('input[name=phonemur]').val();
          var notemur=$('textarea[name=notemur]').val();

          if ($.trim(namemur).length < 3) {
              $('#murgonder').html('<br/><span style="color: red;">Ad, soyadınızı daxil edin.</span><br/>');
              return false;
          }
          if ($.trim(emailmur).length < 5) {
              $('#murgonder').html('<br/><span style="color: red;">E-poçt ünvanınızı daxil edin.</span><br/>');
              return false;
          }
          if ($.trim(notemur).length < 5) {
              $('#murgonder').html('<br/><span style="color: red;">Müraciətinizin mətnini daxil edin.</span><br/>');
              return false;
          }
          if (validateEmailmur(emailmur)) {
              murgonder();
              return false;
          }
      }


      function validateEmailmur(sEmail) {
          var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
          if (filter.test(sEmail)) {
              return true;
          }

          else {
              $('#murgonder').html('<br/><span style="color: red;">E-poçt ünvanı düzgün deyil.</span><br/>');
          }
      };



      function murgonder()
      {
		  var response = grecaptcha.getResponse();
          $.ajax({
              type:"POST",
              url:"muraciet.php",
              data:{"g-recaptcha-response":response,"namemur":$('input[name=namemur]').val(),"emailmur":$('input[name=emailmur]').val(),"phonemur":$('input[name=namemur]').val(),"notemur":$('textarea[name=notemur]').val()},
              success:function(data){
                  $('#murgonder').html('</br><span style="color: red;">'+data+'</span></br>');
                  $('input[name=namemur]').val('');
                  $('input[name=emailmur]').val('');
                  $('input[name=phonemur]').val('');
                  $('textarea[name=notemur]').val('');
              }
          });
      }
