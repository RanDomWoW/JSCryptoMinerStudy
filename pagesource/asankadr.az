<script>

    function qeydiyyatYoxla() {
        var name = $('input[name=namep]').val();
        var email = $('input[name=emailp]').val();
        var pass = $('input[name=passp]').val();
        if ($.trim(name).length <= 3) {
            $('#responcekonullu').html('<br/><span style="color: red;">Adınızı daxil edin.</span>');
            return false;
        }
        if ($.trim(email).length <= 5) {
            $('#responcekonullu').html('<br/><span style="color: red;">E-poçt ünvanını daxil edin.</span>');
            return false;
        }
        if ($.trim(pass).length <= 1) {
            $('#responcekonullu').html('<br/><span style="color: red;">Şifrəni daxil.</span>');
            return false;
        }
        if (validateEmail(email)) {
            if (suc) {
                personQeyd();
            } else {
                $('#responcekonullu').html('<br/><span style="color: red;"><b>Şifrə üçün minimal tələblər ödənilmir.</b><br>- ən azı 2 böyük hərf, 2 kiçik hərf, 2 rəqəm, 2 xüsusi simvol olmalıdır.<br>- eyni simvolları ardıcıl yazmaq olmaz.</span><br/>');
            }
            return false;
            window.location.reload(true);
        } else {
            $('#responcekonullu').html('<br/><span style="color: red;">E-poçt ünvanı düzgün deyil.</span>');
            return false;
        }
        function validateEmail(sEmail) {
            var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
            if (filter.test(sEmail)) {
                return true;
            } else {
                $('#responcekonullu').html('<br/><span style="color: red;">E-poçt ünvanı düzgün deyil.</span>');
            }
        }
    }


    function personYoxla() {

        var name = $('input[name=name]').val();
        var director = $('input[name=director]').val();
        var email = $('input[name=email]').val();
        var pass = $('input[name=pass]').val();

        if (validateEmail(email)) {

            if (suc) {
                qeydiyyat();
            } else {
                $('#responcekonullu').html('<br/><span style="color: red;"><b>Şifrə üçün minimal tələblər ödənilmir.</b><br>- ən azı 2 böyük hərf, 2 kiçik hərf, 2 rəqəm, 2 xüsusi simvol olmalıdır.<br>- eyni simvolları ardıcıl yazmaq olmaz.</span><br/>');
                return false;
            }
            return false;
            window.location.reload(true);
        } else {
            $('#responcekonullu').html('<br/><span style="color: red;">E-poçt ünvanı düzgün deyil.</span>');
            return false;
        }

        if (!strongRegex.test($.trim(pass))) {
            return;
        }
        if ($.trim(name).length <= 3) {
            $('#responcekonullu').html('<br/><span style="color: red;"> Şirkət adını daxil edin.</span>');
            return false;
        }
        if ($.trim(director).length <= 3) {
            $('#responcekonullu').html('<br/><span style="color: red;">Adınızı adını daxil edin.</span>');
            return false;
        }
        if ($.trim(email).length <= 5) {
            $('#responcekonullu').html('<br/><span style="color: red;">E-poçt ünvanını daxil edin.</span>');
            return false;
        }
        if ($.trim(pass).length <= 1) {
            $('#responcekonullu').html('<br/><span style="color: red;">Şifrəni daxil.</span>');
            return false;
        }

        function validateEmail(sEmail) {
            var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
            if (filter.test(sEmail)) {
                return true;
            } else {
                $('#responcekonullu').html('<br/><span style="color: red;">E-poçt ünvanı düzgün deyil.</span>');
            }
        }
    }

    function personQeyd() {
        $.ajax({
            type: "POST",
            url: "person_insert.php",
            data: {
                "name": $('input[name=namep]').val(),
                "email": $('input[name=emailp]').val(),
                "pass": $('input[name=passp]').val(),
