<form id="qr_login_ioop" action="qrcodeAuth" method="post" autocomplete="off">
<input type="hidden" id="qrcode_user_id" name="qrcodeUserId"/>
<input type="hidden" id="rcode" name="rcode"/>
</form>
<script type="text/javascript" src="res/base/plugin/jquery/jquery.min.js"></script>
<script type="text/javascript" src="res/base/plugin/jquery-cookie/jquery.cookie.js"></script>
<script type="text/javascript" src="res/base/plugin/jquery-qrcode/jquery.qrcode.min.js"></script>
<script type="text/javascript" src="res/skin/default/script/login_v3.5.js?ver=20170900"></script>
<script type="text/javascript" src="res/skin/default/script/DES3.js?ver=20170900"></script>
<script type="text/javascript">
$(function(){
    //通用初始化
    LOGIN.initFrame();
    $("#account").focus();
    if ($("#codeImg").length!=0) {
		$("#imagescode").val("1");
	}
    //处理记住密码复选框选中状态
    if($.cookie("remPassord") == "true") { 
        $("#rember_pass_chk").addClass("checked");
        $("#account").val($.cookie("userName")); 
        $("#password").val($.cookie("passWord"));
    }
    $("#rember_pass_chk,#rember_pass_txt").on("click", function(){
        $("#rember_pass_chk").toggleClass("checked");
    });
    //清除错误提示效果
    $("#account,#password").on("keydown", function(){
        $(this).parent().removeClass("error");
    });
    //绑定动态发送按钮
    $("#lfrm_input_dypd").on("click", function(){
    	alert("动态密码服务暂停，请您通过账号密码进行登录。");
		return false;
        //LOGIN.dynamicPassword.send();
    });
    //登录
    $("#logn_form_submit").on("click",function(){
        LOGIN.loginFormSubmit();
    });
    $(document).on("keydown", function(e){
        if(e.keyCode==13){
            LOGIN.loginFormSubmit();
        }
    });
    //快速体验
    $("#logn_tbar_exper").on("click", function(){
        $("#lcnt_tab_exper").click();
    });
    //内容标签切换
    $("#logn_cont_tab li").on("click", function(){
        var $this = $(this),
            _tid = $(this).attr("tid"),
            _ctab = $("#logn_cont_tab li.current"),
            _cblk = $("#logn_cont_main .logn-cont-blk.current");
        if(_tid && _cblk.length > 0){
            $this.addClass("current").siblings().removeClass("current");
            if(_tid != _ctab.attr("tid")){
                _cblk.removeClass("current").slideUp(500);
                $("#logn_cont_"+_tid).addClass("current").slideDown(800);
            }
        }
    });
    //核心功能切换
    $("#lcnt_func_head li").on("click", function(){
        var $this = $(this),
            _code = $this.attr("code");
        if(_code){
            $this.addClass("current").siblings().removeClass("current");
            $("#lcfu_itm_"+_code).addClass("current").siblings().removeClass("current");
        }
    });
    //绑定角色体验跳转
    $("#logn_cont_exper .lcnt-exper-item").on("click", function(){
        var _user = $(this).attr("uname"),
            _pass = $(this).attr("upass");
        if(_user && _pass){
            $("#account").val(_user);
            $("#password").val(_pass);
            $(window).scrollTop(0);
            LOGIN.loginFormSubmit();
        }
    });
    //首页头部图片切换
    LOGIN.slider.init();
    //特性效果初始化
    LOGIN.feat.init();
    //滚动后显示回到顶部
    LOGIN.showScrollTop();
    //显示消息提示窗口
    //LOGIN.showMsgLayer();
    
    function uuid() {
    var s = [];
    var hexDigits = "0123456789abcdef";
    for (var i = 0; i < 36; i++) {
        s[i] = hexDigits.substr(Math.floor(Math.random() * 0x10), 1);
    }
    s[14] = "4";  
    s[19] = hexDigits.substr((s[19] & 0x3) | 0x8, 1);  
    s[8] = s[13] = s[18] = s[23] = "-";
 
    var uuid = s.join("");
    return uuid;
    }
    var UUID;
    var timer;
    $("#qrcode_login").on("click", function(){
       window.clearInterval(timer);
       $("#qrcode_img").empty();
       UUID = uuid();
       //生成二维码
       $("#qrcode_img").qrcode({
            render:"table",//设置渲染方式 
            width:200, //设置宽度   
            height:200,  //设置高度   
            typeNumber:-1,      //计算模式    
            correctLevel:0,//纠错等级  
            background:"#ffffff",//背景颜色  
            foreground:"#000000", //前景颜色
            text:"ctnma://type=2&uuid="+UUID
       });
       $("#logining_text").text("扫描成功");
 	   if($("#qr_code_1").hasClass("tab-hidden")){
 	   	  $("#qr_code_1").removeClass("tab-hidden");
 	   }
 	   if(!$("#qr_code_2").hasClass("tab-hidden")){
 	      $("#qr_code_2").addClass("tab-hidden");
 	   }
 	   //定时请求后台  
 	   timer = setTimeout(function(){continueTimer();}, 1000);
 	});
    
    $("#back_to_code").on("click", function(){
       window.clearInterval(timer);
       $("#qrcode_img").empty();
       UUID = uuid();
       //生成二维码
       $("#qrcode_img").qrcode({
            render:"canvas",//设置渲染方式 
            width:200, //设置宽度   
            height:200,  //设置高度   
            typeNumber:-1,      //计算模式    
            correctLevel:0,//纠错等级  
            background:"#ffffff",//背景颜色  
            foreground:"#000000", //前景颜色
            text:"ctnma://type=2&uuid="+UUID
       });
       $("#logining_text").text("扫描成功");
  	   if($("#qr_code_1").hasClass("tab-hidden")){
  	   	  $("#qr_code_1").removeClass("tab-hidden");
  	   }
  	   if(!$("#qr_code_2").hasClass("tab-hidden")){
  	      $("#qr_code_2").addClass("tab-hidden");
  	   }
  	   //定时请求后台  
  	   timer = setTimeout(function(){continueTimer();}, 1000);
  	});
    
    function continueTimer(){
    $.ajax({
		type: "POST",
		data: {
			'uuid':UUID
		},
		dataType: "json",
		url: "longConnectionCheck",
		async: false,
		success: function(data){
			    if(data.STATE == '0'){
			        if(!$("#qr_code_1").hasClass("tab-hidden")){
   	                    $("#qr_code_1").addClass("tab-hidden");
                    }
                    if($("#qr_code_2").hasClass("tab-hidden")){
                        $("#qr_code_2").removeClass("tab-hidden");
                    }
                    setTimeout(function(){continueTimer();}, 1000);
			    }else if(data.STATE == '1'){
			        window.clearInterval(timer);//去掉定时器
			        $("#logining_text").text("正在登录");
                    $("#qrcode_user_id").val(data.USER_ID);
                    $("#rcode").val(data.rcode);
                    $("#qr_login_ioop").submit();
			    }else{
		    	    setTimeout(function(){continueTimer();}, 1000);
		    }
		},
		error: function(XMLHttpRequest, textStatus, errorThrown){
			window.clearInterval(timer);//去掉定时器
		}
	});
    
    }
});
function changeCode(){
	var codeImg = $("#codeImg");
	$("#codeImg").attr("src","authcode.jsp?v="+new Date().getTime());
}
</script>
