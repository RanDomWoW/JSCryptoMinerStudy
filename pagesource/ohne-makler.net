<script type="text/javascript">
$(function() {
    $("#find_cities").autocomplete({
	    source: "/geodb/cities/find/",
		minLength:2,
		max:10,
		scroll:true,
		autoFocus:true
	});
});

$(function() {
	$("#submit").click(function(e) {
		if (!$("#find_cities").val()) {
			return true;
		}

		e.preventDefault();
		$.ajax({
			type: "HEAD",
			url: "/geodb/check/"+encodeURIComponent($("#find_cities").val())+"/",
			success: function(data, status, xhr) {
				window.location.href = "/immobilie/list/?q="+encodeURIComponent($("#find_cities").val());
			},
			error: function(data, status, xhr) {
				$('#invalid_city').modal().css({
					'margin-top': '150px'
				});
			}
		});
	});
});


$(function() {
	$("#form2").submit(function(e) {
		e.preventDefault();
		$.ajax({
			type: "HEAD",
			url: "/immobilie/id/?id="+$("#object_id").val(),
			success: function(data, status, xhr) {
				window.location.href = xhr.getResponseHeader("Location");
			},
			error: function(data, status, xhr) {
				$("#id_msg").addClass("hidden");
				$("#id_error").removeClass("hidden");
			}
		});
	});
});
</script>

  

  
