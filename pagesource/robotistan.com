}
});
}
else {
window.criteo_q.push(
{event: "setAccount", account: criteoPartnerId},
{event: "setSiteType", type: criteoDeviceType},
{event: "setEmail", email: MEMBER_INFO.MAIL},
{event: "viewHome", ecpplugin: "tsoft"});
}
