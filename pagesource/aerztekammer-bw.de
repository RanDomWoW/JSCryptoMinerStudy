<!-- Skript für Ajax-Suche -->
<script>
      var http;
      var http_target;
		function waitforit(value,field,target,limit,element) {
			var waiting;
			var rec;
			/*if (field == "querybox") { rec = "recommendations" } else {rec = "recommendations_box"}
			document.getElementById(field).onblur = hide_recommendations(rec);*/
			if (waiting) {
				window.clearTimeout(waiting);
			}
			waiting = window.setTimeout(function() { get_recommendations(value,field,target,limit,element); }, 300);
		}
		function hide_recommendations(qbox) {
			window.setTimeout("document.getElementById('"+qbox+"').style.display='none';", 800);
		}
      function get_recommendations(value,field,target,limit,element) {
        if (navigator.appName=="Microsoft Internet Explorer") {
          http = new ActiveXObject("Microsoft.XMLHTTP");
        } else {
          http = new XMLHttpRequest();
        }
        http.open('get','/search_ajax/get_list.php?search=laekbw&phrase='+value+'&limit='+limit+'&field='+field+'&target='+target+'&element='+element);
        http.onreadystatechange=answer_recommendations;
        http.send(null);
        http_target=target;
      }
      function answer_recommendations() {
        if ((http.readyState==4) && (http.status==200)) {
          document.getElementById(http_target).innerHTML=http.responseText;
          document.getElementById(http_target).style.display = "block";
        }
      }
</script>	
