				$("body").addClass("portal-logged-in");
			}
			else{
				$("#login_portal")
					.attr("action","https://" + location.hostname + "/cas/login")
					.show();
				$("body").addClass("portal-not-logged-in");
			}
			var serviceUrl = "/10aerzte/00portal/index.php";
			var portString = "";
			if(location.port && location.port != 443 && location.protocol === "https"){
				portString = ":" + location.port
			}
			$("#casService").attr("value", "https://" + location.hostname + portString + serviceUrl);
                        var portalSettingsUrl = "https://" + location.hostname + portString + "/10aerzte/00portal/";
                        if( isKammerMitglied()) {
                                portalSettingsUrl += "stammdatenverwaltung";
                        } else {
                                portalSettingsUrl += "benutzerverwaltung";
                        }
                        $(".portal-settings").attr("href", portalSettingsUrl);
		});
                function isKammerMitglied() {
                        var mnr = getMnr();
                        if(mnr) {
                                return true;
                        }
                        return false;
                }
                function getMnr() {
                        var userdata = getUserDataFromCookies();
                        if( userdata) {
                                var mnr = userdata.mnr;
                                if( mnr != undefined && mnr != null) {
                                        return mnr;
                                }
                        }
                        return false;
                }
                function getUserDataFromCookies() {
                        if( $.cookie('userdata')) {
                                var userdata = $.parseJSON($.cookie('userdata'));
                                if( userdata != undefined && userdata != null) {
                                        return userdata;
                                }
                        }
                        return false;
                }
	</script>
	
	
