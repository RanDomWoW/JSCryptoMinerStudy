    <script>
    $(document).ready(function(){
        $('[data-toggle="tooltip"]').tooltip();
        $('.locations-map').css('opacity', 100).css('margin-top', '10px');
    });

	$("#gameJumbotron").carousel({
		interval: false
	});

	$("#gameJumbotron").bind('slide.bs.carousel', function(e) {
		$("#gameName").html($("img", e.relatedTarget).first().attr("alt"));

	});

	$("#gameCarousel").carousel({
		interval: false
	});

	$(".featured-game").on("mouseover", function(e){
        var featureId = $("#" + e.currentTarget.id).data("featureid");

        /* 3 is see all games, we don't do anything here */
        if (featureId != 3) {
            var carouselIndex = $("#" + e.currentTarget.id).data("id");
            var name = $("#" + e.currentTarget.id).data("gamename");
            var slug = $("#" + e.currentTarget.id).data("gameslug");
            var featureText = $("#" + e.currentTarget.id).data("featuretext");
            var featureType = "";

            switch (featureId) {
                case 0: featureType = "Most Popular"; break;
                case 1: featureType = "Staff"; break;
                case 2: featureType = "Community Favorite"; break;
            }

    		$("#gameJumbotron").carousel(carouselIndex);
            $("#featured-game-description").html(featureText);
            $("#featured-game-link").attr("href", "/order?game=" + slug);
            $("#featured-game-name").text(name);
            $("#feature-type").text(featureType);
            $("#featured-game-box").css("background-image", 'url("/img/game-icons/' + slug + '-boxart.jpg")');

            $("#featured-game-box-column").css("display","block");
            $("#featured-game-content-column").addClass("col-md-9");
            $("#featured-game-content-column").removeClass("col-md-12");
            $("#all-games-header").css("display","none");
            $("#featured-game-header").css("display","block");
        } else {
            var carouselIndex = $("#" + e.currentTarget.id).data("id");
            var featureText = $("#" + e.currentTarget.id).data("featuretext");
            $("#featured-game-description").html(featureText);

            $("#featured-game-box-column").css("display","none");
            $("#featured-game-content-column").removeClass("col-md-9");
            $("#featured-game-content-column").addClass("col-md-12");
            $("#all-games-header").css("display","block");
            $("#featured-game-header").css("display","none");
            $("#gameJumbotron").carousel(carouselIndex);
            $("#featured-game-link").attr("href", "/order");
        }
	});
    </script>

