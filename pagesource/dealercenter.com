<script type="text/javascript">window.addEventListener("DOMContentLoaded", function() {
if ($('#dcportalIframeDiv').length) {
var qs_referral = parseQueryFromUrl(window.location);
var qs_key = Object.keys(qs_referral)[0];

var iframe_src = $('#dcportalIframeDiv iframe').attr('src');

$('#dcportalIframeDiv iframe').attr('src', iframe_src.split("?")[0] +'?'+ qs_key +'='+qs_referral[qs_key]);
}
});

function parseQueryFromUrl() {
		var url = window.location.search.replace('?', '');
		var query = {};
		var a = (url[0] === '?' ? url.substr(1) : url).split('&');
		for (var i = 0; i < a.length; i++) {
			var b = a[i].split('=');
			query[decodeURIComponent(b[0])] = decodeURIComponent(b[1] || '');
		}
		return query;
	}</script>
