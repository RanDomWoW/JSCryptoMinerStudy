<script type="text/javascript" defer="defer">
    jQuery(document).ready(function() {
        jQuery('#keywordSearch').submit(function(event) {
            var keywords = jQuery("#keywords");
            if (jQuery(keywords).val() === jQuery(keywords)[0].title) {
                jQuery(keywords).val("");
            }

            keywords = jQuery(keywords).val();
            keywords = keywords.trim();
            keywords = encodeURIComponent(encode(keywords));

            if (keywords !== "") {
                var url = 'http://schwabjobs.com/ListJobs/ByKeyword/' + keywords + "/";

                document.location = url;
            } else {
                document.location = 'http://schwabjobs.com' + '/ListJobs/All';
            }

            event.preventDefault();
        });

        jQuery("#keywords").focus(function () {
            if (jQuery(this).val() == jQuery(this)[0].title) {
                jQuery(this).val("");
                jQuery(this).removeClass("defaultTextActive");
            }
        });

        jQuery("#keywords").blur(function () {
            if (jQuery(this).val() == "" || jQuery(this).val() == jQuery(this)[0].title) {
                jQuery(this).val(jQuery(this)[0].title);
                jQuery(this).addClass("defaultTextActive");
            }
        });
        jQuery("#keywords").blur();
    });

    function encode(val) {
        return val
            .replace(/&/gi, "{amp}")
            .replace(/\?/gi, "{qm}")
            .replace(/#/gi, "{hash}")
            .replace(/%/gi, "{percent}")
            .replace(/\*/gi, "{asterisk}")
            .replace(/\//gi, "{fs}")
            .replace(/\\/gi, "{bs}")
            .replace(/:/gi, "{colon}")
            .replace(/`/gi, "{backtick}")
            .replace(/\]/gi, "{rightbracket}")
            .replace(/\[/gi, "{leftbracket}");
    }
</script>
