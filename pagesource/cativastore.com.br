<script type="text/javascript">
var newsletterForm = new VarienForm('newsletter-validate');
function submitForm(gender) {
    if(typeof newsletterForm !== "undefined" && newsletterForm.validator.validate()) {
        sendVPVEvent('/inscricao-realizada','Novo assinante');
        
        var form = $j('.block-subscribe form');
        form.addClass('disabled');
        form.find('input[name=gender]').val(gender);

        $j.post(form.attr('action'), form.serialize());
    } else {
        $j.featherlight("<div class='center-content'><h2>Ooops!</h2><p>Ocorreu um erro. Por favor recarregue a página e tente de novo.</p></div>");
    }
}

function subscribeClose() {
    $j('.block-subscribe').fadeOut('fast',function(){
        $j('.block-subscribe').removeClass('popup-overlay').prependTo('.footer-info').slideDown('fast');
    });
    setPopupCookie();
}

// register cookie to close popup
function setPopupCookie() {
    expiry = new Date();
    expiry.setTime(expiry.getTime()+(365*24*60*60*1000));
    document.cookie = 'popup_close=1; expires=' + expiry.toGMTString();
}

// show accordingly block after doc ready
$j(document).ready(function() {
    if(Mage.Cookies.get('popup_close')) {
        // if already closed offer
        $j('.block-subscribe').removeClass('popup-overlay').prependTo('.footer-info').slideDown('fast');
    }
});
</script>
