<script>
    var promo = document.querySelector('.promo');
    var promoList = document.querySelector('.promo-list');
    var listItem = document.querySelectorAll('.promo-list__item');
    var teasers = document.querySelectorAll('.promo-teaser');
    var videoLink = document.querySelectorAll('.promo-video-link');
    var videos = document.querySelectorAll('.promo-video video');
    var isScroll = listItem.length > 4;
    var last = teasers.length - 1;
    var isVideoOpen = false;
    var timer;
    var next = 1;
    var classes = {
        item: 'promo-list__item_active',
        teaser: 'promo-teaser_active',
        video: 'promo-list__item_video'
    };

    $(document).ready(function () {
        if (isScroll) $(promoList).customScrollbar();
    });

    function setTeaser(num) {
        var id = 'teaser-' + num;
        var activeItem = document.querySelector('.' + classes.item);
        var activeContent = document.querySelector('.' + classes.teaser);

        if (activeItem) activeItem.classList.remove(classes.item);
        if (activeContent) activeContent.classList.remove(classes.teaser);
        document.getElementById(id).classList.add(classes.item);
        document.getElementById(id + '-content').classList.add(classes.teaser);

        if (isScroll) { $(promoList).customScrollbar("scrollTo", '#' + id); }
        next = num < last ? parseInt(num) + 1 : 0;
    }

    function startTimer() {
        if (last) { 
            clearInterval(timer);
            timer = setInterval(function () {
                setTeaser(next);
            }, 15000);
        }
    }

    $(promo).hover(function (e) {
        clearInterval(timer);
    }, function (e) {
        if (!isVideoOpen) startTimer();
    });

    $('.promo-list__item').on('click', function () {
        var num = this.getAttribute('data-num');
        setTeaser(num);
        if (this.classList.contains(classes.video)) {
            loadVideo(num);
        }
    })

    $(videoLink).click(function (e) {
        e.preventDefault();
        var promoVideo = $('.promo-video video')[0];
        promoVideo.pause();
        clearInterval(timer);
        isVideoOpen = true;
        $.fancybox({
            'type': 'iframe',
            'width': 700,
            'height': 394,
            'title': 'Как заключать пари на сайте БК «Лига Ставок»',
            'href': $(this).data('href'),
            'beforeClose': function () {
                promoVideo.play();
                startTimer();
                isVideoOpen = false;
            },
            'padding': [60, 20, 20, 20]
        });
    });

    function loadVideo(num) {
        var video = $('#teaser-' + num + '-content video')[0];
        for (var i = 0; i < videos.length; i++) { videos[i].pause(); }
        video.load();
        video.play();
    }

    startTimer();
</script>



