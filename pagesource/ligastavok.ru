<script>
    (function () {
        $(document).ready(function () {
            $('#listline').on('click', '.livetv', function () {
                setTimeout(function () {
                    setBetslipHeight(true);
                }, 300);
            });

            $('#livetv').on('click', '.close', function () {
                setBetslipHeight(false);
            });
        });

        var header = document.querySelector('.header');
        var fixedBar = document.querySelector('.fixed-sidebar');

        function setBetslipHeight(liveTV) {
            var liveTVH = 0;
            var liveTVMsgH = 0;
            var headerH = header.offsetHeight;
            var cartHeadH = 60;
            var controlsH = 30;
            var licenseH = 66;

            if (liveTV) {
                liveTVH = $('#livetv').is(':visible') ? 203 : 0;
                liveTVMsgH = $('#livetv_stream_mobile').is(':visible') || $('#livetv_stream_not_available').is(':visible') || $('#livetv_kinescope_not_authenticated').is(':visible') ? 135 : 0;
            }

            var betslipHeight = window.innerHeight - [headerH, liveTVH, liveTVMsgH, controlsH, licenseH, cartHeadH].reduce(function (el1, el2) {
                return el1 + el2;
            }, 0);

            $('.cardView').each(function () {
                this.style.maxHeight = betslipHeight + 'px';
                this.style.overflowY = 'auto';
            });
        }

        function setFixedBlocks() {
            var width = (window.innerWidth > 0) ? window.innerWidth : screen.width;
            var pageH = document.querySelector('body').clientHeight;

            if (width >= 1280 && window.pageYOffset <= pageH - window.innerHeight) {
                fixedBar.style.position = 'fixed';
                fixedBar.style.top = '100px';
            } else {
                fixedBar.style.position = 'absolute';
                fixedBar.style.top = 0;
            }

            width >= 1280 ? header.style.position = 'fixed' :  header.style.position = 'absolute';
        }

        function set() {
            setBetslipHeight(false);
            setFixedBlocks();
        }

        set();
        window.addEventListener('resize', set);
        window.addEventListener('scroll', setFixedBlocks);
    })();
</script>

