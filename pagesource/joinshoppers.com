<script type="text/javascript">
$(document).ready(function () {
     var browser = 'chrome', faIcon="fa fa-chrome";
     if (navigator.userAgent.search("Firefox") >= 0) {
          browser = 'firefox';
          faIcon = "fa fa-firefox";
     }
     $('.fa-browser').addClass(faIcon);
     $('.header-navbar').addClass('fixed');
     $( window ).resize(function() {
               createSwiper();
     });
     function createSwiper(){
          var width = $('.container').width();
          var height = 350;
          if(width < 484) height = 430;
          $('.swiper-container').css({width:width+'px', height: height + 'px'});
         //initialize swiper when document ready
         var mySwiper = new Swiper ('.swiper-container', {
           // Optional parameters
           direction: 'horizontal',
           spaceBetween: 30,
           loop: true,
           //paginationType: 'bullets',
           // If we need pagination
           pagination: {
                el: '.swiper-pagination',
                clickable: true
           },
           navigation: {
                nextEl: '.swipe-right',
                prevEl: '.swiper-button-prev',
           },

          });
     }
     createSwiper();
     $('.btn-install').click(function(e){

          if (navigator.userAgent.search("Chrome") >= 0) {
               //code goes here
               window.open("https://chrome.google.com/webstore/detail/shoppers/aaiolimgbncdaldgbbjkidiijidchhjo");
          }
          else if (navigator.userAgent.search("Firefox") >= 0) {
               //code goes here
               window.open("https://addons.mozilla.org/en-GB/firefox/addon/shoppers/?src=userprofile");
          }
     });
     $('.navbar-right li a').click(function(e){

          var target=  $(this).attr('data-target');
          if(typeof target !== "undefined") e.preventDefault();
          //$('#'+target).offset();
          var sT = 50 + $("#"+target).outerHeight(true); // get the fixedbar height
          // compute the correct offset and scroll to it.
          window.scrollTo(0,sT);
     })
});
</script>
