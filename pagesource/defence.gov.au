<!-- Wordpress feeds -->
<script>
    $(document).ready(function(){
            var storiesUrl = "http://news.defence.gov.au/rssfeed/?url=http://images.defence.gov.au/imagefeed/imagefeed.aspx?search=defhomepage&PreviewSize=1000";
            $.ajax({
                type: 'GET',
                url: storiesUrl,
                async: true,
                contentType: "application/json",
                dataType: 'jsonp'
            }).done(function(json) {
        
                var maxItem = 5;
				var arrayLen = json['item'].length > maxItem ? maxItem : json['item'].length;
                var markup = '<div class="carousel">';

                for (var i = 0; i < arrayLen; i++) {
                    var currentItem = json['item'][i];
                    markup += '<div class="slide">';
                    markup += '<a target = "blank" class="imageModal" href="' + currentItem.medialink + '" rel="gal">';
                    markup += '<img class="gm scale paddingtop1 margin1" src="' + currentItem.medialink + '" alt="' + currentItem.title + '"/>';
                    markup += '</a>';
                    markup += '<div class="slidedesc">';
                    markup += '<h3 class="margin1"><a href="' + currentItem.link + '">' + currentItem.title + '<a/></h3>';
                    markup += '<div>' + /*DWIC.Utilities.decodeNumericEntities*/ (currentItem.description) + '</div>';
                    markup += '</div>';
                    markup += '</div>';
                }
                // close list
                markup += '</div></div></div>';
				$("#newsList").html(markup).hide(); 
                // append generated markup
                if($.browser.webkit || $.browser.mozilla) {
					$(window).load(function() {						
						$("#newsList").show(); 
						var gm = Math.max.apply(null, $(".gm").map(function ()
						{
							return $(this).height();
						}).get());						
						$("#newsList").carousel({ })    
						$("#newsListLoading").hide();   
						$("#newsList").attr('style', 'position: relative; overflow: hidden; width: auto; min-height:' + gm + 'px;');
						$(".imageModal").nyroModal();
					});
				} else {
					$("#newsList").html(markup).show();
					$("#newsList").carousel({ })
				}

            }).fail(function(e) {
                console.log(e.message)
            });
		 }); 
	
// loads a feed given a URL and a jQuery object
 
	function LoadFeed(url,element)
	{
		$.ajax({
		   type: 'GET',
			url: url,
			async: true,
			contentType: "application/json",
			dataType: 'jsonp'
		}).done(function(json){
			//console.dir(json);
			// should we be using a templating framework like KnockoutJS or AngularJs here rather than emitting HTML?
			// construct the markup for the slides
			var arrayLen = json['item'].length;
			//console.log("array len: "+arrayLen);
			var markup = "<ul class='informed greyDark'>";
			for (var i=0; i<arrayLen;i++) {
				var currentItem = json['item'][i];
				markup += '<li><a href="'+currentItem.link+'" title="'+currentItem.title+'">'+currentItem.title+'</a></li>';
			}
			// close list
			markup += '</ul>';
			// append generated markup
			element.html(markup);	
		}).fail(function(e){
			console.log(e.message);
		});
	}
    $(document).ready(function(){
        //$("#newsCarousel").carousel({autoplay:true, time:5, speed:0.5, keyboard:true });
		/* Set up main news Carousel */
		/* Set up New Releases list  #newReleases */
		var newStoriesUrl = "http://news.defence.gov.au/rssfeed/?url=http://news.defence.gov.au/stories/feed&top=8";
		LoadFeed(newStoriesUrl,$("#storiesContent"));
		var newReleasesUrl = "http://news.defence.gov.au/rssfeed/?url=http://news.defence.gov.au/category/media-release/feed&top=8";
		LoadFeed(newReleasesUrl,$("#newReleases"));
	});

	
	
</script>    



