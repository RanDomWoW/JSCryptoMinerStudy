                            <script>
								$(document).ready(function() {
									var $searchOver = $('#header .searchOver'),
										$input = $searchOver.find('input');
 									
									// Menu Bar in Compact mode / Open Search box
									$('#header .util .btn_search').on('click', function() {
										$searchOver.fadeIn(300);
										$input.focus();
										
										resetHeader();
									});
									
									// Menu Bar in compact mode / Close Search box
									$('#header .searchOver .searchOver_closer').on('click', function() {
										$input.val('');
										$('#header .searchSec input').val('');
										$searchOver.fadeOut(300);
										
										resetHeader();
									});
									
									// Header Border Style Reset
									resetHeader = function() {
										if($('#header').hasClass('searchFail')) {
											$('#header').removeClass('searchFail');
										};
									};
									
									checkResize = function() {
										var $width = $(window).width()
										
										if($width >= 1201) {
											$input.val('');
											$('header .searchSec input').val('');
											$searchOver.hide();
										}
									};
									
									$(window).resize(checkResize);
									
									// Body click Search Box Close
									$('#container, #footer').click(function() {
										$('#header .searchOver .searchOver_closer').trigger('click');
									});

									// Menu Bar in compact mode / Value Enter Trigger
									$('#header .searchOver input').on('keydown', function(key) {
										if ( key.keyCode == 13 ){
											var $mobileSearchVal = $(this).val();
											
											$('#header .searchSec input').val($mobileSearchVal);
											submitTrigger();
										} else if ( key.keycode == 27 ) {
											console.log('test')
											$('#header .searchOver .searchOver_Closer').trigger('click');
										}
									});
									
									// Menu Bar in full mode / Value Enter Trigger
									$('#header .searchSec input').keydown(function(key) {
										if(key.keyCode == 13) {
											submitTrigger();
										}
									});

									submitTrigger = function() {
										if($('#header .searchSec input').val() == '') {
											$('#header').addClass('searchFail');
										} else {
											$('#header .searchSec .btn_search').trigger('click');
										}
									};
									
									// Validation Search Value Check
									$('#header input').bind('keyup change', function() {
										if($(this).val() == '') {
											$('#header').addClass('searchFail');
										} else {
											$('#header').removeClass('searchFail');
										}
									});
										
									// Search Value Push
									$('#header .searchSec .btn_search').on('click', function() {
										var $searchVal = $('#header .searchSec input').val();
										//console.log($searchVal);
										location.href = "/Lucene?stext=" + encodeURIComponent($searchVal);
									});
								});
                            </script>
