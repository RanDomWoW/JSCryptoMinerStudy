            player.play();
            ga('send', 'event', 'Movie', 'Play - [PC] Auto-Start')
        });

        player.on('ended', function (data) {
            $(".vimeobanner").find('.frmvimeo').hide();
            $(".vimeobanner").find('.poster').show();
