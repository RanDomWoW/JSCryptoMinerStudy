<script>
    //동영상 닫기
    // 비디오 팝업
    var videoPop = $("#popup-pbvideo").imLayerFixedPopup();

    $(".btn-play-main").on("click", function ($e) {
        $e.preventDefault();
        ga('send', 'event', 'Movie', 'Play - [Mobile] Click View Button')
        $("#wrap").css("width", "100%");
        mp4url = $(this).attr('data-url');

        if (mp4url != undefined) {
            //비메오 영상 일 경우
            if (mp4url.indexOf("vimeo.com") > -1) {
                videofocus = $(this);
                $("#vimeoVideo").attr("src", mp4url);
                var vimeoPlayer = new Vimeo.Player($("#vimeoVideo"));
                $("#popup-wrap1").show();
                $("#popup-wrap1 .popup-layer").focus();
                $("#popup-wrap1 #vimeoVideo").imRatioSize({ maxTri: 958, maxTar: 465 });

                vimeoPlayer.on('ended', function (data) {
                    ga('send', 'event', 'Movie', 'Play - [Mobile] Done')
                });
                vimeoPlayer.on('pause', function (data) {
                    ga('send', 'event', 'Movie', 'Play - [Mobile] Stop')
                });
                vimeoPlayer.on('play', function (data) {
                    ga('send', 'event', 'Movie', 'Play - [Mobile] Start')
                });

                return false;

            }
        }
    });

    $("body").on("click", "#popup-wrap1 .popup-dim, #popup-wrap1 .btn_close", function ($e) {
        var vimeoPlayer = new Vimeo.Player($("#vimeoVideo"));
        $e.preventDefault();
        $("#popup-wrap1").hide();
        vimeoPlayer.setCurrentTime(0);
        vimeoPlayer.pause();
        videofocus.focus();
        return false;
    });
</script>
