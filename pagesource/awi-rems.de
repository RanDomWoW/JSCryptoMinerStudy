    document.getElementById('User_Form_Auth-credential').onkeypress = function (e) {
        
        if (isCapslock(e)) {
            document.getElementById('User_Form_Auth-capsLockIndicator').style.visibility = 'visible';
        }
        else {
            document.getElementById('User_Form_Auth-capsLockIndicator').style.visibility = 'hidden';
        }
    }
    
    function isCapslock(e){
        
        e = (e) ? e : window.event;
        
        var charCode = false;
        if (e.which) {
            charCode = e.which;
        } else if (e.keyCode) {
            charCode = e.keyCode;
        }
        
        var shifton = false;
        if (e.shiftKey) {
            shifton = e.shiftKey;
        } else if (e.modifiers) {
            shifton = !!(e.modifiers & 4);
        }
        
        if (charCode >= 97 && charCode <= 122 && shifton) {
            return true;
        }
        
        if (charCode >= 65 && charCode <= 90 && !shifton) {
            return true;
        }
        
        return false;
    }
    
    
