<script type="text/javascript" >
  function showTime(objD,getItem){
     var hh = objD.getHours();
     if(hh<10) hh = '0' + hh;
     var mm = objD.getMinutes();
     if(mm<10) mm = '0' + mm;
     var ss = objD.getSeconds();
     if(ss<10) ss = '0' + ss;
     var items = {'hour':hh, 'minute':mm, 'minutes':mm, 'second':ss, 'seconds':ss};
     if(getItem) {
        str = items[getItem].toString();
     } else {
        str = hh + ":" + mm + ":" + ss;
     }
     return(str);
  }
  function tick(){
     var today = new Date();
     $('#hourone').html( showTime(today,'hour').substr(0,1) );
     $('#hourtwo').html( showTime(today,'hour').substr(1) );
     $('#minuteone').html( showTime(today,'minute').substr(0,1) );
     $('#minutetwo').html( showTime(today,'minute').substr(1) );
     $('#secondone').html( showTime(today,'second').substr(0,1) );
     $('#secondtwo').html( showTime(today,'second').substr(1) );
     window.setTimeout("tick()", 1000);
  }
  tick();
          
   
