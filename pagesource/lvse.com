 //add 20120924 gaopeng
 function swap_site_mode(el_selecor,mode){
   $(el_selecor).find('li[logo_url]').each(function(){
	var othis = this;
	var logo_url = $(this).attr('logo_url');
	var nav_a = $(this).find('a');
        
	var title = $(nav_a).html();
	var link  = $(nav_a).attr("href");
	var old_css;
	old_css = $(this).data('old_css') || {};
	if (typeof(old_css['width']) == 'undefined'){
	$.each(['width','height','background-image','padding','border','margin-top','margin-bottom','margin-left','margin-right','cursor'],function(i,v){
	old_css[v] = $(othis).css(v);
	});
	}   
	$(nav_a).css({"visibility":"hidden"});
	$('.top_site_line').css({"display":"none"});
	$(this).attr({"title": title})
	.click(function(){
	$(this).find('a').get(0).click();
	})
	.data("cur_mode","logo")
	.data("old_css",old_css)
	.css({ 
	"background-image":"url(" + logo_url + ")",
	"width":"124px",
	"height":"44px",
	"padding":"2px",
	"border" : "1px solid #E0E0E0",
	"background-repeat":"no-repeat",
	"background-position":"center center",
	"margin":"5px 4px 5px 5px",
	"cursor" :  ($.browser.msie  ? "hand" : "pointer")
	});
	
	});
}
