$(function(){
   $('#lvse_site_search input').focus(function(){
      var search_val = $.trim($(this).val());
      if (search_val == '输入关键字搜索'){
         $(this).val('');
      }
   }).keydown(function(event){
     if(event.keyCode == 13) {
        $('#lvse_site_search span').click();
        return false; 
     }
   });
   
   $('#lvse_site_search span').click(function(){
      var search_val = $.trim($('#lvse_site_search input').val());
      if (!/\S/.test(search_val) || search_val == '输入关键字搜索'){
         alert("请输入关键字搜索！");
      }else{
         window.location.href = 'http://www.lvse.com/search/site/' + encodeURIComponent(search_val.replace('/',''));
      }
   });
});
