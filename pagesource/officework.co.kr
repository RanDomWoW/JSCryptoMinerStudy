function createLog(toPage){
	$.postJSON("http://susoft.co.kr/officeon/everyone/officework.createWebLog", makeLogParam(toPage), resultCreateLog);
}
function makeLogParam(toPage){
	var param = new Object();
	param.accessToken = accessToken;
	param.deviceType = deviceType;
	param.fromUrl = fromUrl;
	param.toPage = toPage;
	param.site = site;
	return param;
}

function resultCreateLog(data){
	$.cookie('officeWorkCookie',data);
}

function doResize(){
	var h = $(window).height();    	
	winHeight = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight;
	$("#MainBody").height(winHeight);
	$("body").height(winHeight);
	$('.container').css("height", (h-180) + "px");
	$(".bx-viewport").css("height", (h-180) + "px");
// 	$(".bx-wrapper").css("height", (h-180) + "px");
// 	$("#HomepageBody").css("height", (h-180) + "px");

	var bodyHeight = $("body").outerHeight();
	var bodyWidth = $("body").outerWidth();
	
	var top	= (bodyHeight - 365)/2;
	var left = (bodyWidth - 800)/2;
	
	if(top < 0 ) top = 0;
	if(left < 0 ) left = 0;
	
	$('#preHomepagePopup').css('top', top);
	$('#preHomepagePopup').css('left', left);
}

