function setOffsetList(paramOffsetList){
	offsetList = paramOffsetList;
}
function movePostion(positionId){
	var position = offsetList[positionId];
	var padding = offsetList.padding;
	var top = position.top + padding;

	var menuHight = $("#"+ oldPageName +"menu").height();

	//top -= menuHight+140;
	if(menuOverScrollYn === false){
		top -= menuHight+210;
	}else{
		top -=  (menuHight*2)+210;
	}
	
	$("#MainBody").animate({scrollTop:top}, 1000);
	subMenuCookie(positionId);
}

function movePostion_bottom(sMenuId , positionId){
	if(oldPageName == sMenuId) {
		if(sMenuId == 'support') {
			onClickSubMenu(sMenuId , positionId);
		}else {
			movePostion(positionId);		
		}
	} else {
		
		onClickSubMenu(sMenuId , positionId);	
	}
}


function checkScrollerHeight(){
	var scrollerHeight = $("#MainBody").scrollTop();

	if(scrollerHeight > 90){
		if(menuOverScrollYn === false){
			menuHight = $("#"+ menuId +"menu").height();
			var mainMidHeight = $("#mainMid").height();
			
			$("#mainHeader").css("position", "absolute");
			$("#mainHeader").css("top", "-39px");
			$("#HeaderMenuBar").css("display", "none");
			
			$("#SubPageDiv").css("top", (menuHight)+"px");
			$("#mainFooterback").css("top", (menuHight)+"px");
			$("#mainMid").css("height", (mainMidHeight + menuHight) +"px");
			$("#Allmenudiv").css("top", "40px");
			$("#Allmenudiv").css("backgroundcolor", "rgba(0,0,0,0.3)");

			menuOverScrollYn = true;
		}
	}else{
		if(menuOverScrollYn === true){
			menuHight = $("#"+ menuId +"menu").height();
			var mainMidHeight = $("#mainMid").height();
			
			$("#mainHeader").css("position", "relative");
			$("#mainHeader").css("top", "0px");
			$("#HeaderMenuBar").css("display", "block");
			
			$("#SubPageDiv").css("top", "0px");
			$("#mainFooterback").css("top", "0px");
			$("#mainMid").css("height", (mainMidHeight - menuHight)+"px");
			$("#Allmenudiv").css("top", "80px");
			$("#mainMid").css("top", "0px");
	
			menuOverScrollYn = false;
		}
	}
}

