function onClickSubMenu(menu , positionId){
	if(positionId ==="top")
		gotoTop();
	if(menu === 'null')
		return;
	if(oldPageName === menu)
		return;
	makeSubFrame(menu , positionId);
	__changePage(menu);
	
}
function makeSubFrame(frameId , positionId){
	var h = $(window).height();  
	var frameEl = $("#"+frameId);
	var toPage = "";
	
	if(frameId !== 'tour'){
		$("#tour").attr("src", "");	
	}
	
	
	if(typeof frameEl === 'undefined' || frameEl.length === 0){
		var innerHtml = "<iframe id='"+frameId+"' name='"+frameId+"' frameborder='0' scrolling='no' style=' z-index: 90; display:none;width:100%;background-color: white;'></iframe>";
		$("#SubPageDiv").append(innerHtml);
	}
	
	if(frameId === 'strength'){
		$("#"+frameId).attr("src", "/officework/officework.homepageStrength.jsp?positionId="+positionId);
		$("#"+frameId).css("margin-top", -103 + "px");
		$("#mainFooterback").css("display","block");
		$("#"+frameId).css("height", 9660+ "px");
		toPage = 'STRENGTH';
	}else if(frameId === 'epgroupware'){
		$("#"+frameId).css("margin-top", -75 + "px");
		$("#"+frameId).attr("src", "/officework/officework.homepageEPGroupware.jsp?positionId="+positionId);
		$("#mainFooterback").css("display","block");

		
		/* 	}else if(frameId === 'spec'){
		$("#"+frameId).attr("src", "/officeon/everyone/officework.homepageSpec?positionId="+positionId);
 */		$("#"+frameId).css("height", 9500+ "px");
 		toPage = 'FUNCTION';
 	}else if(frameId === 'tour'){
		$("#"+frameId).attr("src", "/officework/officework.homepageTour.jsp?positionId="+positionId);
		$("#"+frameId).css("height", 1000+ "px");
		$("#"+frameId).css("margin-top", 14 + "px");
		$("#mainFooterback").css("display","block");
		toPage = 'LOOK';
 	}else if(frameId === 'support'){
		$("#"+frameId).attr("src", "/officework/officework.homepageSupportInquiry.jsp?positionId="+positionId);
		$("#"+frameId).css("height", 1000+ "px");
		$("#"+frameId).css("margin-top", -10 + "px");
		$("#mainFooterback").css("display","block");
		toPage = 'INQUIRE';
 	}else if(frameId ==='demo'){
		$("#"+frameId).attr("src","/officework/officework.homepagedemo.jsp?positionId="+positionId);
		$("#"+frameId).css("height", 1100+ "px");
		$("#"+frameId).css("margin-top", -45 + "px");
		$("#mainFooterback").css("display","block");
		toPage = '';
	}else if(frameId ==='price'){
		$("#"+frameId).attr("src","/officework/officework.homepageprice.jsp?positionId="+positionId);	
		$("#"+frameId).css("height", 1700+ "px");
		$("#"+frameId).css("margin-top", -45 + "px");
		$("#mainFooterback").css("display","block");
		toPage = '';
	}else if(frameId ==='home'){
		$("#mainFooterback").css("display","none");
		toPage = 'HOME';
	}else if(frameId ==='privacy'){
		$("#"+frameId).attr("src","/officework/officework.privacy.jsp?positionId="+positionId);	
		$("#"+frameId).css("height", 1000+ "px");
		$("#"+frameId).css("margin-top", 14 + "px");
		$("#mainFooterback").css("display","block");
		toPage = '';
	}
	createLog(toPage);
}

