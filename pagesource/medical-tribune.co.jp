<script type="text/javascript">
jQuery(function($){
var main = $('div.col-main');
var side = $('div.col-sub');
var abs_class = "pos_abs";
var fix_class = "pos_fix";
var btm_class = "bottom_set";
var main_width = 0;
var main_height = 0;
var side_height = 0;
var main_delay_height = 0;
var side_delay_height = 0;
var win_height  = 0;
var offset;
if(main.length && side.length){
var get_height = function(obj){
var height_tmp = 0;
obj.children(':not(script)').each(function(){
if($(this).is(':not(style)')){
height_tmp += $(this).outerHeight(true);
}
});
return height_tmp;
};
var setting = function(){
if(window.matchMedia('(min-width:1201px)').matches){
if(side_delay_height == 0){
side_height = get_height(side);
}else if(side_height != side_delay_height){
side_height = side_delay_height;
side_delay_height = 0;
}
if(main_delay_height == 0){
main_height = get_height(main);
}else if(main_height != main_delay_height){
main_height = main_delay_height;
main_delay_height = 0;
}
main_width = main.outerWidth();
win_height = $(window).height();
offset = main.offset();
}
};
var reset = function(){
if(side.hasClass(btm_class)){
side.removeClass(btm_class);
}
if(side.hasClass(fix_class)){
side.removeClass(fix_class).css({
bottom: '',
left: ''
});
}
};
var scroll_animate = function(setting_flg){
if(setting_flg){
setting();
}
if(window.matchMedia('(min-width:1201px)').matches && main_height > side_height && (win_height - offset.top) < side_height){
var scroll_top = 0;
scroll_top = $("html,body").scrollTop();
if(!scroll_top){
$.each($("html,body"), function(){
scroll_top = $(this).scrollTop();
});
}
if((scroll_top + win_height) > (side_height + offset.top)){
if((scroll_top + win_height) < (main_height + offset.top)){
if(!side.hasClass(fix_class)){
side.addClass(fix_class);
}
side.css({
left: main_width + offset.left
});
if(side.hasClass(btm_class)){
side.removeClass(btm_class);
}
}else{
if(!side.hasClass(btm_class)){
side.addClass(btm_class);
}
if(side.hasClass(fix_class)){
side.removeClass(fix_class).css({
left: ''
});
}
}
}else{
reset();
}
}else{
reset();
}
};
// read more click event
var read_more = $('a.read-more-btn');
if(read_more.length){
read_more.click(function(){
setTimeout(function(){
scroll_animate(true);
}, 200);
});
}
// resize event
var resize_timer = false;
$(window).resize(function(){
if(resize_timer !== false){
clearTimeout(resize_timer);
}
resize_timer = setTimeout(function(){
scroll_animate(true);
}, 200);
});
// scroll event
$(window).scroll(function(){
scroll_animate();
});
// delay action
var delay_count = 0;
var delay_stop  = 20;
var delay_interval = setInterval(function(){
side_delay_height = get_height(side);
main_delay_height = get_height(main);
if(main_height == main_delay_height && side_height == side_delay_height){
delay_count++;
}else{
delay_count = 0;
scroll_animate(true);
}
if(delay_count >= delay_stop){
clearInterval(delay_interval);
side_delay_height = 0;
main_delay_height = 0;
}
}, 1000);
// 初回実行
scroll_animate(true);
}
});
</script>
