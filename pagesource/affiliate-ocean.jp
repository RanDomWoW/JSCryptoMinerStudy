		<script>

    // 列の表示非表示
    $(".chkbox_col_visible").on('switchChange.bootstrapSwitch', function(){
        var target = $(this).data("target");
        if(this.checked){
            $("."+target).css("display","");
            $.cookie($(this).attr("id"), null);
        }else{
            $("."+target).css("display","none");
            $.cookie($(this).attr("id"), "1");
        }
    });

    // 列の表示非表示
    $(".chkbox_col_visible").each(function(){
        if($.cookie($(this).attr("id"))=="1"){
            $(this).prop('checked', false).trigger('change');
        }
    })
    $(document).ajaxComplete(function() {
        $(".chkbox_col_visible").each(function(){
            if($.cookie($(this).attr("id"))=="1"){
                $(this).prop('checked', false).trigger('change');
            }
        })
    });
    $(function(){
        $(".chkbox_col_visible").on('ifChanged',function(){
            if(this.checked){
                $.cookie($(this).attr("id"), null, { path: "/" });
            }else{
                $.cookie($(this).attr("id"), "1", { path: "/" });
            }
            return;
        });
    });
</script>



    	<script>
    $(function(){
        // 日選択関連
        change_date();
        $("#preset_date").change(function(){ change_date(); });
        function change_date(){
            var selected_val = $("#preset_date option:selected").val();
            if(selected_val == "0"){
                // enable date input form.
                $("#from_date").prop("readonly", false);
                $("#to_date").prop("readonly", false);
            }else{
                var start = "2018-03-19";
                var end = start;
                switch(selected_val){
                    case "1":
                        // this month
                        start = "2018-03-01";
                        end = "2018-03-19";
                        break;
                    case "2" :
                        // last month
                        start = "2018-02-01";
                        end = "2018-02-28";
                        break;
                    case "3" :
                        // last 30 days
                        start = "2018-02-17";
                        end = "2018-03-19";
                        break;
                    case "4" :
                        // last 7 days
                        start = "2018-03-12";
                        end = "2018-03-19";
                        break;
                    case "5" :
                        // today
                        start = "2018-03-19";
                        end = start;
                        break;
                    case "6" :
                        // yesterday
                        start = "2018-03-18";
                        end = start;
                        break;
                    case "7" :
                        // 先々月
                        start = "2018-01-01";
                        end = "2018-01-31";
                        break;
                    case "8" :
                        // 直近3ヶ月
                        start = "2017-12-01";
                        end = "2018-03-19";
                        break;
                    case "9" :
                        // all
                        start = "2016-01-01";
                        end = "2018-03-19";
                        break;
                }
                $("#from_date").val(start);
                $("#to_date").val(end);
                $("#from_date").prop("readonly", true);
                $("#to_date").prop("readonly", true);
            }
        }

        // 月選択関連
        change_month();
        $("#preset_month").change(function(){ change_month(); });
        function change_month(){
            var selected_val = $("#preset_month option:selected").val();
            if(selected_val == "0"){
                // enable date input form.
                $("#from_month").prop("readonly", false);
                $("#to_month").prop("readonly", false);
            }else{
                var start = "2017-09";
                var end = "2018-03";
                switch(selected_val){
                    case "1":
                        // last 6 month
                        start = "2017-09";
                        end = "2018-03";
                        break;
                    case "2":
                        // last 12 month
                        start = "2017-03";
                        end = "2018-03";
                        break;
                    case "9" :
                        // all
                        start = "2015-01";
                        end = "2018-03";
                        break;
                }

                $("#from_month").val(start);
                $("#to_month").val(end);
                // disable date input form.
                $("#from_month").prop("readonly", true);
                $("#to_month").prop("readonly", true);
            }
        }

    });

</script>
