<!-- Для поиска по комиссионной техники -->
$(function() {

    $.ajax({
		url:'/',
        method: "POST",
        data: {subaction : 'getCountCart'},
        dataType: "json",
		success: function(response) {
            if(response.data) {
                $("#basket_count").html(response.data);
                $("#basket_count").show();
            } else {
                $("#basket_count").hide();
            }
        }
	});


    $.ajax({
        url:'/',
        method: "POST",
        data: {subaction : 'getUserName'},
        dataType: "json",
        success: function(response) {

            if(!response.isUser) {
				$(".loginIn").show().html('Войти');
            } else {
                $(".profileIn").show().html(response.userName);
            }
        }
    });


	$("form#head_search button").on("click", function(e) {
		var partOfSite = $("form#head_search").find("a.cur").data('value');

		if(partOfSite == 'tehnoskarb') {
            e.preventDefault();

            var search = $("form#head_search").find("input#searchbar").val();

            if(search != '') {
                location.href = encodeURI("/katalog-komissionnojj-tekhniki/c-all/filter/search=" + search);
            }

		}
	});
});

</script>


