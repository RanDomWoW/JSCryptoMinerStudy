    $(function($){

      var headerForm = function(){
        var $header = $('#header'),_label  = 'js-scrolled';
        ($( window ).scrollTop() < 120)?$header.removeClass(_label):$header.addClass(_label);
      }
      $(window).on('scroll', _.throttle(headerForm, 1000));

      
      var _delay = 10000,
          slideItems = Array.prototype.slice.call($('.slider__radio')),
          showNextImage = function (){
            var now = parseInt($('input[name="slider"]:checked').val(), 10),
                next = slideItems.length===now?1:now+1;
            $('#slide'+next).prop('checked', true);
          },
          reload = setInterval(showNextImage, _delay);
      $('#slide1').prop('checked', true);

      $('#active').on('click', 'label', function(){
        clearInterval(reload);
        reload = setInterval(showNextImage, _delay);
      });
      
      $('.js-anchor').on('click', function(e){
        e.preventDefault();
        var _offset = 80,
            targetBody = ( $('html').scrollTop() > 0 )?$('html'):$('body');
            targetPos  = parseInt($(this.hash).eq(0).offset().top, 10) - _offset;
        targetBody.animate({scrollTop: targetPos},'slow','easeOutQuint');
      });
    })
  </script>

