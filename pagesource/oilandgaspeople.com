			doSearchLightbox();
			var url = '/oil-and-gas-jobs-search/s/?sMe=yp&jtm=' + $(this).val()
			window.location.href = url;
		});
		$('#browseJobsDropDowns').on('change', '#hotJobsBrowse_country', function(){
			doSearchLightbox();
			var url = '/oil-and-gas-jobs-search/s/?sMe=yp&country=' + $(this).val()
			window.location.href = url;
		});		
	});
	
	function doSearchLightbox(){
		$('#hotJobsListContainer').cycle('destroy');
		$('#v2_hotJobs #searchLightbox').fadeIn('fast');
	}
</script>

