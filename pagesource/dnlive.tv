<script type="text/javascript">
    $(function () {
        var $slider = $(".video-control-slider");
        $slider.slider({
            orientation: "horizontal",
        range: "min",
        min: 0,
        max: 100,
        value: 80,
        slide: function (event, ui) {
            changeMyVolume(ui.value);
        }
    });
    var currentVolume = 80;

    function changeMyVolume(volume, notrecord) {

        var calVol = Math.floor(((volume - 0) / (100 - 0)) * 100);
        $slider.find(".video-control-slider-main").css("width", volume);

        calVol = calVol > 0 ? calVol : 0;
        if (!notrecord) {
            currentVolume = calVol;
        }
        $slider.find(".video-control-slider-main>i").attr("title", '音量：' + calVol + '%');
        if (calVol > 0) {
            $(".video-control-volume").removeClass("mute");

        }
        if (window.ChangeVolume) {
            ChangeVolume(calVol);
        }
    }
    $(".video-control-volume>.icon").click(function () {
        var $this = $(this).parents(".video-control-volume");
        $this.toggleClass("mute");
        var myVolume = 0;
        if (!$this.hasClass("mute")) {
            myVolume = currentVolume;

        }

        $slider.slider("value", myVolume);
        if (window.ChangeVolume) {
            ChangeVolume(myVolume);
        }

    });
    $(".video-control-damun").click(function () {
        var isshow = true;
        if (!$(this).hasClass("open")) {
            isshow = false;
        }
        if (window.barrageShowHandler) {
            window.barrageShowHandler(isshow);
        }
    })
    });

</script>
