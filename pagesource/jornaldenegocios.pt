                        .build();

                    COF.pubApi.configMastheadCheckSizes(_mastheadCheckSizes);
                });


            var kwpag = [];
            kwpag.push('homepage');
                    kwpag.push('Mercados');
                    kwpag.push('empresas');
                    kwpag.push('economia');
                    kwpag.push('banca');
                    kwpag.push('acções');
                    kwpag.push('noticias');
                    kwpag.push('negócios');
