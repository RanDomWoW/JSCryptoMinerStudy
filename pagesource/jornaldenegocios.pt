                    kwpag.push('negócios online');
                    kwpag.push('trabalho em portugal');
                    kwpag.push('emprego em portugal');
                    kwpag.push('ganhar dinheiro');
                    kwpag.push('pme');
                    kwpag.push('inovação');
                    kwpag.push('empreendedorismo');



            function adsRenderEnded (adsEvent) {

                var slotElementID = adsEvent.slot.getSlotElementId();
                if (!adsEvent.isEmpty && slotElementID != 'OutOfPage') {
                    if (slotElementID.indexOf('BTFhorz') > -1 || slotElementID.indexOf('horzATF') > -1) {
                        if ($('#' + slotElementID).siblings('div.pub').length == 0) {
                            $('#' + slotElementID).after('<div class="pub">pub</div>');
                        }
                        if (slotElementID.indexOf("horzATF") > -1) {
                            //quando há masthead, o MREC fica em baixo da caixa das últimas notícias.

                            if ($(".mrecToMove").length > 0 && $("#ultimasHp_move").length > 0) {
                                $("#ultimasHp_move").insertBefore($(".mrecToMove"));
                            }
                        }
                    }
                    else if (slotElementID.indexOf('BTFvert') > -1 || slotElementID.indexOf('vertATF') > -1) {
                        if ($('#' + slotElementID).parent().siblings('div.pub').length == 0)
                            $('#' + slotElementID).parent().after('<div class="pub">pub</div>');
                    }
                }

                //validacao para carregar o script do optinmonster
                if ((adsEvent.isEmpty && slotElementID == 'OutOfPage') || (adsEvent.isEmpty && slotElementID.indexOf('horzATF') > -1)) {
                    indexOptin++;
                    if (indexOptin == 2 && typeof loadOptinMonster === 'function' && loadOptinMonster() !== undefined) {
                        loadOptinMonster();
                    }
                }
            }

            var indexOptin = 0;
            googletag.cmd.push(function () {
                googletag.pubads()
                    .setTargeting('end', [window.location.href.match(/([^\?]*)\/*$/)[0]])
                    .setTargeting('kw', kwpag);

                googletag.pubads().addEventListener('slotRenderEnded', function (event) {
                    adsRenderEnded(event);
                });


                googletag.pubads().enableSingleRequest();
                googletag.pubads().enableAsyncRendering();
                googletag.pubads().collapseEmptyDivs(true, true);
                googletag.pubads().setCentering(true);
                googletag.companionAds().setRefreshUnfilledSlots(true);
                googletag.pubads().disableInitialLoad();

                googletag.enableServices();

                COF.pubApi.renderOOP($('#div-gpt-ad-1468495125935-0'));

                
                COF.pubApi.renderMasthead($('#div-gpt-ad-1468493133235-0'), '', 'horzATF', 'ATF', false)
                

                
                COF.pubApi.renderMrec($('#div-gpt-ad-1468493588384-0'), '', 'vertATF', 'ATF', false);
                

                googletag.pubads().refresh();
            });
        });

    </script>



