                                                           $(document).ready(function() {

            $('#send-feedback').click(function(){
                $('.pr_feedback-error').hide();
                $.ajax({
                type: "PUT",
                url:  base_url+'/ajax/sendfeedback',
                data: {name: $('#user-name').val(), feedback: $('#feedback-text').val() },
                success: function(data)
                {
                if(data != 'success')
                {
                    $('.pr_feedback').html('<div class="alert alert-danger">' + data + '</div>');
                    $('.pr_feedback').show();
                }
                else
                {
                    $('.modal-body').html('<div class="alert alert-success">Hvala što ste podelili utisak sa nama.</div> ');
                    $('.pr_btn-feedback').hide();
                    $('#send-feedback').hide();

                }
                }
                });
            });
    });
    </script>




