    <script type="text/template" id="add-to-cart-result-template">{{! //<![CDATA[ }}<div class="cart-add-to-result">
    <div class="cart-last-added-item">
        {{#cartProduct}}
        <div class="cart-product">
                <div class="cart-product-img">
                    <a href="{{url}}">
                        <img class="lazyload" src="data:image/gif;base64,R0lGODlhAQABAIABAP///wAAACwAAAAAAQABAAACAkQBADs=" data-sizes="auto" data-srcset="{{#img}}{{img}} 240w, {{/img}}{{#img480}}{{img480}} 480w, {{/img480}}{{#img720}}{{img720}} 720w{{/img720}}" alt="{{label}}">
                    </a>
                </div>
                <div class="cart-product-details">
                    <h3>Gute Wahl! Der Artikel liegt nun im Warenkorb</h3>
                    <span class="product-name">{{name}}</span><br>
                    Menge: {{qty}}{{#deliveryTime}}<br>
                    Lieferzeit: <span class="delivery-time-value">{{deliveryTime}}</span>{{/deliveryTime}}
                </div>
            {{/cartProduct}}
        </div>
    </div>
    <div class="buttons-free-shipping-notice-wrapper">
        <div class="buttons">
            <a href="/checkout/cart/" class="button">Zum Warenkorb</a>
            <span data-close>Weitershoppen</span>
        </div>
        {{#freeShippingNotice}}
        <div class="free-shipping-notice callout secondary">
            {{{freeShippingNotice}}}
        </div>
        {{/freeShippingNotice}}
    </div>
    {{#hasCrossSellingProducts}}
    <div class="box-crosssell box-cross-sell category-products small-up-1 medium-up-3 small-collapse">
        <h2 class="heading-text">Uns solltest du unbedingt <em>kennenlernen</em></h2>
        {{#crossSellingProducts}}
        <div class="card product product-name-image column column-block">
            <div class="card-divider" data-equalizer-watch><h3><a href="{{url}}">{{name}}</a></h3></div>
            <div class="card-section product-img">
                <a href="{{url}}">
                    <img class="lazyload" src="data:image/gif;base64,R0lGODlhAQABAIABAP///wAAACwAAAAAAQABAAACAkQBADs=" data-sizes="auto" data-srcset="{{#img}}{{img}} 240w, {{/img}}{{#img480}}{{img480}} 480w, {{/img480}}{{#img720}}{{img720}} 720w{{/img720}}" alt="{{label}}">
                </a>
            </div>
            <div class="card-section">
                <div class="details">
                    <div class="rating-prices">
                        {{#ratingSummary}}
                            <span class="rating">
                    <span class="rating-stars" title="{{ratingSummary}}%" data-rating-summary="{{ratingSummary}}"><span style="width:{{ratingSummary}}%"></span></span><span class="rating-count">({{reviewsCount}}<span class="show-for-sr"> Bewertung(en)</span>)</span>
                </span>
                        {{/ratingSummary}}
                        {{{priceHtml}}}
                        {{#basePriceLabel}}<span class="baseprice-box">{{{basePriceLabel}}}</span>{{/basePriceLabel}}
                    </div>
                </div>
                {{#addToCart}}
                    <form action="{{addUrl}}" method="post" class="ajax-add-to-cart">
                        <fieldset{{#fieldsetAttributes}} {{{fieldsetAttributes}}}{{/fieldsetAttributes}}>
                            <div class="actions">
                                <div class="qty-wrapper">
                                    <div class="input-group">
                                        <div class="input-group-button"><button class="button secondary">-</button></div>
                                        <input class="input-group-field" type="number" name="qty" value="{{defaultQty}}">
                                        <div class="input-group-button"><button class="button secondary">+</button></div>
                                    </div>
                                </div>
                                <div class="buttons">
                                    <button class="button btn-cart">Warenkorb</button>
                                </div>
                            </div>
                        </fieldset>
                    </form>
                {{/addToCart}}
                {{^addToCart}}
                    <div class="actions">
                        <div class="buttons">
                            <button class="button" onclick="window.location.href='{{url}}'">Details</button>
                        </div>
                    </div>
                {{/addToCart}}
            </div>
        </div>
    {{/crossSellingProducts}}
    </div>
    {{/hasCrossSellingProducts}}
</div>{{! //]]> }}</script><div class="off-canvas position-top" id="add-to-cart-result-off-canvas" data-off-canvas></div>    <div class="wrapper off-canvas-content">
