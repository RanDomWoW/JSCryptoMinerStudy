<script>

  function cdtd2() {
	var xmas = new Date("June 25, 2016 12:00:00"); 
	var now = new Date(); 
	var timeDiff = xmas.getTime() - now.getTime(); 
	if (timeDiff <= 0) { 
		clearTimeout(timer);
		var countarea_x = document.getElementsByClassName("count");
		var j;
		for (j = 0; j < countarea_x.length; j++) {
			countarea_x[j].style.display = "none"; 
		}
	
	} 
	var seconds = Math.floor(timeDiff / 1000); var minutes = Math.floor(seconds / 60);
	 var hours = Math.floor(minutes / 60); var days = Math.floor(hours / 24); 
	 hours %= 24; minutes %= 60; seconds %= 60;
	 
	 if(days < 10 && days >= 0 )
	 {days='0'+days}
	 
	 if(hours < 10 && hours >= 0 )
	 {hours='0'+hours}
	 
	 if(minutes < 10 && minutes >= 0 )
	 {minutes='0'+minutes}
	 
	 if(seconds < 10 && seconds >= 0 )
	 {seconds='0'+seconds}
	 
	var day_x = document.getElementsByClassName("daysBox2");
	var hour_x = document.getElementsByClassName("hoursBox2");
	var min_x = document.getElementsByClassName("minsBox2");
	var sec_x = document.getElementsByClassName("secsBox2");
	var i;
	for (i = 0; i < day_x.length; i++) {
		day_x[i].innerHTML = days+' <span><br />DAYS</span>'; 
		hour_x[i].innerHTML = hours+' <span><br />HOURS</span>'; 
		min_x[i].innerHTML = minutes+' <span><br />MIN</span>'; 
		sec_x[i].innerHTML = seconds+' <span><br />SEC</span>';
	}
	  
	 /*document.getElementById("daysBox").innerHTML = days+' <span>DAYS</span>'; 
	 document.getElementById("hoursBox").innerHTML = hours+' <span>HOURS</span>'; 
	 document.getElementById("minsBox").innerHTML = minutes+' <span>MIN</span>'; 
	 document.getElementById("secsBox").innerHTML = seconds+' <span>SEC</span>'; */
	 var timer = setTimeout('cdtd2()',1000);
  } 
  
 </script>


