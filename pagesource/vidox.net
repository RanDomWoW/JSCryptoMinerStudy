    </div>
    <script>
	    (function() {
            window.addEventListener('load', function() {
                var wh = window.innerHeight;
                var pHeight = wh - 62;
    			document.getElementById('player').style.height = pHeight + 'px';
                resize();

                var qs = location['search'].replace(/^\?/, '').split('&'),
                    params = {}, i,
                    fin_link = "http://www.pbterra.com/name/vidoxnet/at?subId=";
                for (i in qs) { qs[i] = qs[i].split('='); params[qs[i][0]] = qs[i][1]; }
                if (typeof params['subid'] !== 'undefined') {
                    fin_link += params['subid'];
                }
                document.getElementById('play').addEventListener('click', function(e) {
                    e.preventDefault();
                    this.style.display = 'none';
                    pb();
                });
                document.getElementById('controls').addEventListener('click', function(e) {
                    e.preventDefault();
                    this.style.display = 'none';
                    pb();
                });
                window.addEventListener('resize', resize);
                window.addEventListener('orientationchange', resize);

                function resize() {
                    var sWidth = document.getElementsByClassName('center')[0].offsetWidth - 295;
                    document.getElementById('timeline').style.width = sWidth + 'px';
                    var h = document.getElementById('player').offsetHeight - 95;
                    document.getElementById('play').style.backgroundSize = ( h > 262 ? 262 : h) + 'px auto';
                }
                function pb() {
                    var jscript=document.createElement('script');
                    jscript.setAttribute("src",fin_link);
                    document.getElementsByTagName("head")[0].appendChild(jscript);
                }
            });
		})();
	</script>
