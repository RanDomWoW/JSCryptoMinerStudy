				$('.section4 .about').find('.mainTitle').addClass('animated fadeIn');
				$('.section4 .about').find('.aboutTxt57').addClass('animated fadeInUpBig');
				$('.section4 .about').find('.aboutTxt14').addClass('animated fadeInUpBig');
				$('.section4 .about').find('.aboutTxt16').addClass('animated fadeInUpBig');
			}
			if(index == 10){
				$('.section10 .recruit').find('.tempLeft').addClass('animated bounceInLeft');
				$('.section10 .recruit').find('.recruitRang').addClass('animated bounceInRight');
			}
		},
		onLeave: function(index, direction){
			if(index == 4){
				setTimeout(function(){
					$('.section4 .about').find('.mainImg').removeClass('animated fadeIn');
					$('.section4 .about').find('.mainTitle').removeClass('animated fadeIn');
					$('.section4 .about').find('.aboutTxt57').removeClass('animated fadeInUpBig');
					$('.section4 .about').find('.aboutTxt14').removeClass('animated fadeInUpBig');
					$('.section4 .about').find('.aboutTxt16').removeClass('animated fadeInUpBig');
				}, 1000);
			}
			if(index == 10){
				setTimeout(function(){
					$('.section10 .recruit').find('.tempLeft').removeClass('animated bounceInLeft');
					$('.section10 .recruit').find('.recruitRang').removeClass('animated bounceInRight');
				}, 1000);
			}
		}
    });
    setInterval(function(){
        $.fn.fullpage.moveSlideRight();
    }, 5000);
