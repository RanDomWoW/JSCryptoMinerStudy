$(function(){

	/* 团队 */
	$(".teamList li").hover(function(){
		if($(window).width()>1250){
			$(this).stop().addClass("active").removeClass("small").animate({width:'17.7989%'},'fast','swing',function(){
				$(this).find(".teamText").stop().fadeIn();
			})
			.siblings().removeClass("active").addClass("small").stop().animate({width:'10.73369%'},'fast','swing').find(".teamText").stop().fadeOut();
		}
	});
	var ww = $(window).width();
	var teamML = 0;
	var max;
	$(window).on("resize",function(){
		if($(window).width()<=1250){
			$(".teamList li").removeClass("active small").removeAttr("style").find(".teamText").removeAttr("style");
		}
		$(".teamList ul").css({"marginLeft":"0"});
		$(".clickBtn.pre").addClass("disabled");
		$(".clickBtn.next").removeClass("disabled");
		ww = $(window).width();
		teamML = 0;
		if(ww<=1250){ max = 100; }
		if(ww <= 600){ max = 300; }
	});
	if(ww<=1250){ max = 100; }
	if(ww <= 600){ max = 300; }
	$(".clickBtn.pre").addClass("disabled");
	$(".clickBtn.next").removeClass("disabled");

	$(".clickBtn").on("click",function(){
		var step = 100;

		if($(this).hasClass('next')){
			teamML += step;
		}else{
			teamML -= step;
		}
		teamML = teamML<=0?0:teamML;
		teamML = teamML>=max?max:teamML;
		$(".clickBtn").removeClass("disabled");
		if(teamML == 0){
			$(".clickBtn.pre").addClass("disabled");
		}
		if(teamML == max){
			$(".clickBtn.next").addClass("disabled");
		}
		$(".teamList ul").animate({"marginLeft":"-"+teamML+"%"})
	});
	/* 团队 end */

	var navHtml = $("#topNav").html();
	$(".topMain").html(navHtml);
	$(".topMain").each(function(){
		var highLight = $(this).attr("data-nav");
		$(this).find(".topLink").find("a[href='"+highLight+"']").addClass("link0090ff");
	});
	$(".spotCon").on("mouseover",function(){
		var index=$(this).index();
		if(index==0){
			$(".fp-prev").click();
		}else{
			$(".fp-next").click();
		}
	});
