$('#tijiao').on('click',(function(){
	var formurl = "/index.php?m=&c=Index&a=message";
	var name =$('#name').val();
	if(name==''){
		alert("姓名不能为空！");
		return false();
	}
	var phone =$('#phone').val();
	if(phone == ''){
		alert("手机号不能为空！");
		return false();
	}else{
		if (!/^1[34578]\d{9}$/.test(phone)) {
	        alert("手机号有误！");
	        return false();
	    }
	}
	var address = $('#address').val();
	if(address==''){
		alert("居住地不能为空！");
		return false();
	}
	var income = $('#income').val();
	if(income==''){
		alert("月收入不能为空！");
		return false();
	}
	var loan = $('#loan').val();
	if(loan==''){
		alert("贷款金额不能为空！");
		return false();
	}
	var captcha = $('#captcha').val();
	if(captcha==''){
		alert("验证码不能为空！");
		return false();
	}
	var profession = $('input[name="profession"]:checked').val();
	$.ajax({
        url: formurl,
        type: "post",
        data: {name:name,phone:phone,address:address,income:income,loan:loan,profession:profession,captcha:captcha},
        dataType: "json",
        success: function(data) {
        	alert(data.info);
        	location.reload();
        },
        error: function(data) {
        	alert(data.info);
        }
    });
}));
