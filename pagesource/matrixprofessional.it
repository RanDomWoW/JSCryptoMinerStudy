<script>
    /*salonfinder popup window hide*/
    jQuery('html').click(function () {
        jQuery('.nearest-salon-map').hide();
        jQuery('.header-nearest-salon').show();
    });

    jQuery('.header-nearest-salon, .nearest-salon-map-details').click(function (event) {
        event.stopPropagation();
    });

    

    $("#findLoc").on("keyup", function (event) {
        if (event.which == 13)
            $(".salon-find-inputBtn").trigger("click");
    });

    if (jQuery('.hdnIsSalonFinder').val() == "True") {
        jQuery("#siteContent").show();
        jQuery("#mobsiteContent").show();
        //jQuery(".header-nearest-salon-layer").hide();
        LoadWhereToGetIt();
    } else {
        jQuery("#siteContent").hide();
        jQuery("#mobsiteContent").hide();
        //jQuery(".header-nearest-salon-layer").show();

        function getCurrentPosition() {
            navigator.geolocation.getCurrentPosition(latlng, geoFail, { timeout: 10000 })
        }

        function latlng(value) {
            var obj = value;
            jQuery.cookie('geofail', "")
            var lat = obj.coords.latitude
            var lng = obj.coords.longitude

            getzipcode(lat, lng);
        }

        function geoFail() {
            if (!jQuery.cookie('geofail')) {
                jQuery.cookie('geofail', true)
                /*Currently not using geoFail functionality */
                // alert("Oops! There is no nearest Salon near your location. Check our various range of Salons using area Zip Code");
            }
            var lat = jQuery('#lat').val();
            var lng = jQuery('#long').val();
            getzipcode(lat, lng);

        }


        function getzipcode(lat, lng) {
            var geoc = new google.maps.Geocoder();

            var latlng = new google.maps.LatLng(lat, lng);
            geoc.geocode({
                'latLng': latlng
            }, function (res, stat) {
                var valid = false;
                var zipCode = "";
                if (stat === google.maps.GeocoderStatus.OK) {
                    for (var K in res) {
                        if (res[K] && jQuery.inArray('postal_code', res[K].types) >= 0) {
                            zipCode = res[K].address_components[0].long_name >> 0;
                            valid = true;
                            break;
                        }
                    }

                    jQuery(".nearest-salon-map").hide();
                    jQuery(".nearest-salon-option").click(function () {

                        jQuery(".header-nearest-salon").hide();
                        jQuery(".nearest-salon-map").show();
                        jQuery(".menu-bg").css("position", "inherit");
                        if (jQuery(window).width() > 768) {
                            jQuery(".carousel-inner").css("marginTop", "-70px");
                        }
                        initializeDefault(lat, lng);
                        google.maps.event.addDomListener(window, 'load', initializeDefault);
                    });
                    jQuery(".nearest-salon-detail").click(function () {
                        jQuery(".nearest-salon-map").hide();
                        jQuery(".header-nearest-salon").show();
                        jQuery(".menu-bg").css("position", "fixed");
                        jQuery(".carousel-inner").css("marginTop", "0");
                    });


                    var StoreInfo = {
                        "radius": 50,
                        "zipcode": "",
                        "latitude": lat,
                        "longitude": lng,
                        "storesperpage": 1,
                        "pagenum": 1,
                        "brand": 'matrix'
                    };

                    //ajaxgetsalon(StoreInfo)

                }
            });
        }

        function ajaxgetsalon(StoreInfo) {
            
            var apiurl = "";
            jQuery.ajax({
                type: "POST",
                url: apiurl,
                data: ProcessDATA(StoreInfo),
                dataType: 'json',
                success: function (data, status, xhr) {
                    if (data[0].Stores.length) {
                        var salonName = data[0].Stores[0].SalonName;
                        var address = data[0].Stores[0].Address;
                        var City = data[0].Stores[0].City;
                        var State = data[0].Stores[0].State;
                        var Country = data[0].Stores[0].Country;
                        var ZipCode = data[0].Stores[0].ZipCode;
                        latitude = data[0].Stores[0].Latitude;
                        longitude = data[0].Stores[0].Longitude;
                        var distance = data[0].Stores[0].Distance;
                        var phone = data[0].Stores[0].Phone;
                        var workingHours = data[0].Stores[0].workingHours;
                        jQuery("address").html('<strong>' + salonName + '</strong> <p>' + address + '</br>' + City + ', ' + State + ' ' + ZipCode + '</p><span class="glyphicon glyphicon-earphone" aria-hidden="true"></span> ' + phone + '<br /><span aria-hidden="true" class="glyphicon glyphicon-time"></span> ' + workingHours + '<br>');
                        jQuery(".plush-salon").text(salonName + " " + ZipCode);
                    } else if (StoreInfo.radius < 100) {
                        StoreInfo.radius += 50;
                        ajaxgetsalon(StoreInfo);
                    } else {
                        getzipcode(jQuery('#lat').val(), jQuery('#long').val());
                    }
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    jQuery(".pageloader").hide();
                }
            });
        }

        var browserAppName = "";
        function ProcessDATA(data) {
            if (browserAppName == "") {
                browserAppName = navigator.detect;
            }
            if (browserAppName == "MSIE 7.0" || browserAppName == "MSIE 8.0" || browserAppName == "MSIE 9.0") {
                var currentDomain = GetDomain(window.location.href);
                var apiDomain = GetDomain(jQuery("#hdnDomainName").val());
                if (currentDomain != null && apiDomain != null && currentDomain != apiDomain) {
                    return JSON.stringify(data);
                }
            }
            return data;
        }
        function GetDomain(url) {
            var m = url.match(/:\/\/(.[^/]+)/);
            return m ? m[0].replace("://", "") : null;
        }
        function initializeDefault(latitude, longitude) {
            //Loading Default map based on latitude & Longitude
            var mapProp = {
                center: new google.maps.LatLng(latitude, longitude),
                zoom: 8,
                mapTypeControl: true,
                mapTypeControlOptions: {
                    style: google.maps.MapTypeControlStyle.DROPDOWN_MENU
                },
                mapTypeId: google.maps.MapTypeId.ROADMAP
            };
            var map = new google.maps.Map(document.getElementById("headerMapDiv2"), mapProp);
            var map1 = new google.maps.Map(document.getElementById("headerMapDiv1"), mapProp);
            // Default Map Loading ended
            // setting  marker tip at location
            var myCenter = new google.maps.LatLng(latitude, longitude);
            var marker = new google.maps.Marker({
                position: myCenter,
                //animation: google.maps.Animation.BOUNCE
            });
            var marker1 = new google.maps.Marker({
                position: myCenter,
            });
            marker.setMap(map);
            marker1.setMap(map1);
            // setting marker tip ended
            // setting display text at marker tip started
            var infowindow = new google.maps.InfoWindow({
                content: "Salon Address"
            });
            var infowindow1 = new google.maps.InfoWindow({
                content: "Salon Address"
            });
            google.maps.event.addListener(marker, 'click', function () {
                infowindow.open(map, marker);
            });
            google.maps.event.addListener(marker1, 'click', function () {
                infowindow1.open(map1, marker1);
            });
            // display text part at marker tip ended
        }
    }

    //WhereToGetIt   start
    function getBaseURLBuy() {
        return location.protocol + "//" + location.hostname + (location.port && ":" + location.port) + "/";
    }

    //Setting the onclick event for show the salon finder popup
    function LoadWhereToGetIt() {
        var locatorUrl = $(".hdnWheretoGetUrl").val();
        var oncl;
        IsSmartphone() ? oncl = "openPopup('" + locatorUrl + "', jQuery('#MbfindLoc').val(), 'Find a Salon',true)" : oncl = "openPopup('" + locatorUrl + "', jQuery('#findLoc').val(), 'Find a Salon',true)";
        jQuery(".jswhere-btn").attr("onclick", oncl);
    }

    function IsSmartphone() {
		return (DetectUagent("android") || DetectUagent("blackberry") || DetectUagent("iphone") || DetectUagent("opera") || DetectUagent("palm") || DetectUagent("generic") || DetectUagent("ipod") || DetectUagent("ipad") || DetectUagent("symbian") || DetectUagent("iemobile"));
    }

    function DetectUagent(name) {
        return (navigator.userAgent.toLowerCase().search(name) > -1);
    }
    if (!IsSmartphone()) {
        jQuery('#mobileSite').hide();
    }


  var hdrSearchTextMb= document.getElementById('MbfindLoc');
  function hideheaderSearchTextMb(defaultText) {
        if (hdrSearchTextMb.value == defaultText)
            hdrSearchTextMb.value = "";
    }

	function showheaderSearchTextMb(defaultText) {
        if (hdrSearchTextMb.value == "") {
            hdrSearchTextMb.value = defaultText;
        }
    }

	function searchValidate(postalcode, defaultText) {
        if (hdrSearchTextMb.value == defaultText) {
            alert("Please enter Zip/Postal code");
            hdrSearchTextMb.focus();
            return false;
        }
        else
            return true;
    }

        var hdrSearchText = document.getElementById('findLoc');
      function hideheaderSearchText(defaultText) {
        if (hdrSearchText.value == defaultText)
            hdrSearchText.value = "";
    }

    function showheaderSearchText(defaultText) {
        if (hdrSearchText.value == "") {
            hdrSearchText.value = defaultText;
        }
    }
    function searchValidate(postalcode, defaultText) {
        if (hdrSearchText.value == defaultText) {
            alert("Please enter Zip/Postal code");
            hdrSearchText.focus();
            return false;
        }
        else
            return true;
    }

    function openPopup(locatorUrl, postalCode, defaultText, disableValidation) {
        var returnVal = disableValidation ? true : searchValidate(postalCode, defaultText);
        var useIFrameTemplate = "false";
        var salonLocatorPageURL = "";

        if (returnVal && useIFrameTemplate == 'false') {
            var custompagesFolder = "";
            var browser = navigator.appName;
            var browser = navigator.appName;
            var b_version = navigator.appVersion;
            var version = parseFloat(b_version);

            var tempIframe = document.createElement('iframe');
            tempIframe.id = "modal";
            tempIframe.name = "modal";
            tempIframe.frameborder = "0"
            tempIframe.scrolling = "yes"
            tempIframe.marginheight = "0"
            tempIframe.marginwidth = "0"
            tempIframe.height = '700';

            findDIv = document.getElementById("newVal");
            if ((navigator.appVersion.substr(22, 2) == "6.") || (navigator.appVersion.substr(22, 2) == "5."))
                tempIframe.allowTransparency = false;
            else
                tempIframe.allowTransparency = true;

            tempIframe.src = locatorUrl;
            if (postalCode != '' && postalCode != defaultText)
                tempIframe.src += "?addressline=" + postalCode + "&form=locator_search";
            jQuery('#modal-content').html('');
            jQuery('#modal-content').html(tempIframe);
			 if ('False' == 'True')
                bvLocConverisonEvent_Click();
        }
        else if (returnVal && useIFrameTemplate == 'true') {
            var href = salonLocatorPageURL;
            if (postalCode != '' && postalCode != defaultText)
                href += "?addressline=" + postalCode + "&form=locator_search";

            if ('False' == 'True')
                bvLocConverisonEvent_Click();
            window.location.href = href;
        }
    }

    jQuery(document).ready(function () {
        //Salon finder click for GTM
        jQuery('.salon-finder-btn-tracker-header-dsk, .salon-finder-btn-tracker-header-mb').click(function () {
            var sfValue;
            IsSmartphone() ? sfValue = jQuery('#MbfindLoc').val() : sfValue = jQuery('#findLoc').val();
