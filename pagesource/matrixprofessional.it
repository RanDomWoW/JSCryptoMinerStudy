    //Language Popup - Begin
    jQuery(".language-switch").click(function (e) {
        if ("1" == 0)
            $.cookie('languagePopupTrigger', true, { expires: 1 });
        else
            $.cookie('languagePopupTrigger', true, { expires: parseInt("1") });

        var url = window.location.href;
        var languageSwitch = jQuery(this).attr("data-language");
        var isUrlModified = false;
        $(".language-switch").each(function () {
            if (url.indexOf("/" + jQuery(this).attr("data-language") + "/") != -1) {
                url = url.replace(("/" + jQuery(this).attr("data-language") + "/"), ("/" + languageSwitch + "/"));
                isUrlModified = true;
            }
            else if (url.indexOf("/" + jQuery(this).attr("data-language")) != -1) {
                url = url.replace(("/" + jQuery(this).attr("data-language")), ("/" + languageSwitch));
                isUrlModified = true;
            }
        })

        if (!isUrlModified) {
            url = window.location.href = document.location.origin + "/" + languageSwitch + document.location.pathname + window.location.search;
        }
        //alert(url);
        window.location.href = url;
    })

    if (jQuery('.hdnIsLanguagePopupEnabled').val() == "True") {
        if (typeof $.cookie('languagePopupTrigger') === 'undefined') {
            jQuery(".language-popup").modal('show');
        }
