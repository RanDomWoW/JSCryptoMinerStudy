    }

    function getHref(el) {
        var linkUrl = $(el).attr('href');
        var searchString = IsSmartphone() ? $('#MbfindLoc').val() : $('#findLoc').val();
        if (linkUrl.indexOf('?') > 0)
            linkUrl = linkUrl + '&addressline=' + searchString;
        else
            linkUrl = linkUrl + '?addressline=' + searchString;
        $(el).attr('href', linkUrl);
    }
</script>


               
