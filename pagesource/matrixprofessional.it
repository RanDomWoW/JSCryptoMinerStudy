            function onYouTubePlayerAPIReady() {
                var len = jQuery(".youtubevideo_count").val();
                for (i = 1; i <= len; i++) {
                    player[i] = new YT.Player("youtubeplayer_" + i + "", {
                        videoId: jQuery("#embeb-" + i).val(),
                        playerVars: {
                            rel: 0,
                            controls: 0,
                        },
                        events: {
                            'onReady': onPlayerReady(event, i),
                        }
                    });
                }
                jQuery('iframe').each(function () {
                    var url = jQuery(this).attr("src");
                    if (jQuery(this).attr("src").indexOf("?") > 0) {
                        jQuery(this).attr({
                            "src": url + "&wmode=transparent",
                            "wmode": "Opaque"
                        });
                    }
                    else {
                        jQuery(this).attr({
                            "src": url + "?wmode=transparent",
                            "wmode": "Opaque"
                        });
                    }
                });
            }
            function onPlayerReady(event, i) {
                var playButton = document.getElementById("play-button_" + i + "");
                var that = this;
                var aaa = YT.PlayerState
                playButton.addEventListener("click", function (click_event) {
                    var url = jQuery("#youtubeplayer_" + i).attr('src').replace('controls=0', 'controls=1&autoplay=1');
                    jQuery("#youtubeplayer_" + i).attr('src', url);
                    if (that.is_playing) {
                        player[i].pauseVideo();
                        jQuery('.carousel-control.left, .carousel-control.right').css('color', '#fff');
                    } else {
                        player[i].playVideo();
                        jQuery("#play-button_" + i + "").hide();
                        jQuery(".play-button_" + i + "").hide();
                        jQuery("#thevideo_" + i + "").show();
                        jQuery('.carousel-control.left, .carousel-control.right').css('color', '#fff');
                        jQuery('.carousel-indicators').hide();
                        jQuery(".vidcap_" + i + "").hide();
                        jQuery('.matrix-prod-total-results.left-space').hide();
                        jQuery('.matrix-breadcrumb').hide();
                        setTimeout(function () {
                            jQuery(".carousel").carousel('pause');
                        }, 1000);
                    }
                });
                jQuery(".left.carousel-control").on('click', function () {
                    var len = jQuery(".youtubevideo_count").val();
                    for (j = 1; j <= len; j++) {
                        if (1 == player[j].getPlayerState()) {
                            player[j].pauseVideo();
                        }
                        var itemlen = jQuery('.home-banner-slider .item').length;
                        var itemindex = jQuery('.home-banner-slider .active').index('.home-banner-slider .item') - 1;
                        var leftindex = -1;
                        if (leftindex === itemindex) {
                            if (jQuery('.home-banner-slider .item').eq(itemlen - 1).find('iframe').hasClass('embed-responsive-item')) {
                                if (jQuery('.home-banner-slider .item').eq(itemlen - 1).find('iframe').attr('src').indexOf('autoplay=1') >= 1) {
                                    jQuery('.carousel-control.left, .carousel-control.right').css('color', '#fff');
                                    jQuery('.carousel-indicators').hide();
                                }
                                else {
                                    jQuery('.carousel-control.left, .carousel-control.right').css('color', '#000');
                                    jQuery('.carousel-indicators').show();
                                }
                            }
                        }
                        else if (jQuery('.home-banner-slider .item').eq(itemindex).find('iframe').hasClass('embed-responsive-item')) {
                            if (jQuery('.home-banner-slider .item').eq(itemindex).find('iframe').attr('src').indexOf('autoplay=1') >= 1) {
                                jQuery('.carousel-control.left, .carousel-control.right').css('color', '#fff');
                                jQuery('.carousel-indicators').hide();
                            }
                            else {
                                jQuery('.carousel-control.left, .carousel-control.right').css('color', '#000');
                                jQuery('.carousel-indicators').show();
                            }
                        }
                        else {
                            jQuery('.carousel-control.left, .carousel-control.right').css('color', '#000');
                            jQuery('.carousel-indicators').show();
                        }
                    }
                });
                jQuery(".right.carousel-control,.carousel-indicators li").on('click', function () {

                    var len = jQuery(".youtubevideo_count").val();
                    var rindex = jQuery('.home-banner-slider .active').index('.home-banner-slider .item') + 1;
                    if (jQuery('.home-banner-slider .item').eq(rindex).find('iframe').hasClass('embed-responsive-item')) {
