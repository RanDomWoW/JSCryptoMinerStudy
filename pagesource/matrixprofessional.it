    // product listing clear
    function prodList(classname, indexmodule) {
        var width = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
        jQuery('.' + classname).css('clear', 'none');
        if (width > 768) {

            if (indexmodule == undefined) {
                indexmodule = 4;
            }

            jQuery('.' + classname).each(function (index, item) {
                if ((index + parseInt(indexmodule)) % parseInt(indexmodule) == 0) {
                    jQuery(item).css('clear', 'both');
                }

            });

        }
        else {
            if (indexmodule == undefined) {
                indexmodule = 3;
            }
            jQuery('.' + classname).each(function (index, item) {
                if ((index + parseInt(indexmodule)) % parseInt(indexmodule) == 0) {
                    jQuery(item).css('clear', 'both');
                }
            });

        }

    }

