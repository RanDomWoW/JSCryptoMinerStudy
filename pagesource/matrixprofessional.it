                        }
                    }
                    else {
                        jQuery('.carousel-control.left, .carousel-control.right').css('color', '#000');
                        jQuery('.carousel-indicators').show();
                    }
                    for (m = 1; m <= len; m++) {
                        if (1 == player[m].getPlayerState()) {
                            player[m].pauseVideo();
                            jQuery('.carousel-control.left, .carousel-control.right').css('color', '#000');
                        }
                    }
                });
            }
            function onPlayerStateChange(event) {
