    <button class="btn btn-primary btn-mini ban-number">Ban</button>
    <% } else if(state == 'GETTING_SMS') { %>
      <button class="btn btn-primary btn-mini refuse-order">Refuse</button>
      <button class="btn btn-primary btn-mini ban-number">Ban</button>
      <% } else if(state == 'SMS_GOT') { %>
        <button class="btn btn-primary btn-mini show-full-sms">Full SMS</button>
        <button class="btn btn-primary btn-mini get-rep-sms">Addit. SMS</button>
        <button class="btn btn-primary btn-mini close-order">Close</button>
        <% } else if(state == 'TIME_EXPIRY') { %>
            <button class="btn btn-primary btn-mini dont-receive-sms">Did't receive SMS?</button>
            <button class="btn btn-primary btn-mini close-order">Close</button>
        <% } else { %>
          <button class="btn btn-primary btn-mini close-order">Close</button>
