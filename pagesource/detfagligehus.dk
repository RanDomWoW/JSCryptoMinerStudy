<script type="text/javascript">
    var subnodes = {}; //notes displayed in tabs menu

    function findSitemapNode(nodeid, nodes) {
        for (var i in nodes) {
            if (nodes[i].id === nodeid)
                return nodes[i];
            else {
                var next = findSitemapNode(nodeid, nodes[i].childs)
                if (next) return next;
            }
        }
    }


    $(function () {
        var nodeIds = ['1218','1260','1261'];
        var fnd = 0;
        for (var i in nodeIds) {
            var node = findSitemapNode(nodeIds[i], window.sitemap);
            if (node != null) {
                $("#ulFaaHjealpTab").append('<li><a href="javascript:void 0' + node.id + '" id="tab' + node.id + '"'+(fnd==0?' class="sel"':'')+'>' + node.name + '</a></li>');
                subnodes[node.id] = node;
                fnd++;
            }
        }

        var selPath = (window.location.hash === "" ? "/" + nodeIds[0] : window.location.hash.substring(1)).split('/');
        if (selPath.length > 1) {
            changeTab(selPath[1]);
        }

        $(".tabs li a").click(function () {
            $(".tabs li a").removeClass('sel');
            changeTab($(this)[0].id.substring(3));
            $(this).addClass('sel');
        });

        $("#selMySituation").change(function () {
            var val = $("#selMySituation option:selected").val();
            if ("-1" == val) {
                $("#selIWantToKnow").prop('disabled', true);
            }
            else {
                var node = findSitemapNode(val, subnodes);
                if (node!=null) {
                    $("#selIWantToKnow").find('option').remove().end().append('<option value="-1">Klik OK, eller vælg et emne</option>').prop('disabled', false);
                    $("#btnGoToHyperSearch").attr("href", node.url);
                    for (var i in node.childs) {
                        $("#selIWantToKnow").append('<option value="' + node.childs[i].id + '">' + node.childs[i].name + '</option>');
                    }
                }
            }
        });

        $("#selIWantToKnow").change(function () {
            var val = $("#selIWantToKnow option:selected").val();
            if ("-1" == val) {
            }
            else {
                var node = findSitemapNode(val, subnodes);
                if (node != null) {
                    $("#btnGoToHyperSearch").attr("href", node.url);
                }
            }
        });
    });

    function changeTab(nodeid) {
        $("#tab" + nodeid).addClass('sel');
        if (subnodes[nodeid] != null) {
            $("#btnGoToHyperSearch").attr("href", subnodes[nodeid].url);
            $("#selIWantToKnow").prop('disabled', true);
            $("#selMySituation").find('option').remove().end().append('<option value="-1">Vælg en situation</option>');
            for (var i in subnodes[nodeid].childs) {
                $("#selMySituation").append('<option value="' + subnodes[nodeid].childs[i].id + '">' + subnodes[nodeid].childs[i].name + '</option>');
            }
        }
    }
</script>



