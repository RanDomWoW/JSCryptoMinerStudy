</div>

<div class="blivmedlemcon2">
    <div id="blivmedlemspar">
        <div class="pris">
            <h3 style="font-size:20px;border-bottom:1px solid gray;padding-bottom:10px;">DIN pris pr. md:<span style="float:right;font-size:26px;"><span class="dinpris">527</span> kr.</span></h3>
			<p class="overflyt" style="display:none;">Ved at skifte <span class="skiftfra"></span> sparer du op til <span class="dinbesparelse">6.000</span> kr. om året.</p>
            <p class="gratisfagforening" style="display:none;">Du får gratis fagforening i 3 måneder, når du både melder dig ind i a-kasse og fagforening.</p>
            <p class="overflyt" style="display:none;">Find din præcise besparelse ved at sammenligne med din nuværende kontingentopkrævning.</p>
            <p class="uddannelse" style="display:none;">Er du under uddannelse, er medlemskab af fagforeningen gratis. Medlemskab af a-kassen er også gratis, hvis du opfylder betingelserne. Er du nyuddannet, bliver du opkrævet kontingent til både fagforening og a-kasse.</p>
        </div>
        <div class="buttons">
            <a href="/om-os/priser-og-vilkaar/priser/" style="float:left">Se priser og vilkår</a>
            <a href="/bliv-medlem/" id="blivmedlembtn" class="stdbuttonCTA" style="float:right;">Jeg vil gerne vide mere</a>
        </div>
        <div style="clear:both;"></div>
    </div>
    <div id="blivmedlemspar_fejl">
        <div class="pris">
            <p>Du skal vælge et medlemskab for at vi kan beregne din pris.</p>
        </div>
    </div>
</div>

<script type="text/javascript">
    function CalculatePrice()
    {
        var medlemskab = $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option:selected").text().match(/([0-9]+)/);
        medlemskab = medlemskab != null ? parseInt(medlemskab[0]) : null;

        var tilkob = $("label[for='" + $("#divTilkob input[type='checkbox']:checked").attr("id") + "']").text().match(/([0-9]+)/);
        tilkob = tilkob != null ? parseInt(tilkob[0]) : 0;

        $("#blivmedlemspar_fejl").hide();

        var situation = $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Situation").val();
        var medlemskab2 = $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option:selected").val();      // select.val() er i stykker, fordi vi har wrappet med span - brug i stedet option:selected


        if (medlemskab != null) {
            if (medlemskab2.indexOf("FFA") > -1 && situation != "uddannelse")
                $(".gratisfagforening").show();
            else
                $(".gratisfagforening").hide();

            if (situation == "uddannelse")
                $(".uddannelse").show();
            else
                $(".uddannelse").hide();

            $(".overflyt").hide();
            // overflytning
            if ($("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_MedlemAf").val() != "" && $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_MedlemAf").val() != "Anden fagforening") {

                // beregn besparelse
                if (situation != "uddannelse" && medlemskab2.indexOf("FS") == -1 && medlemskab2.indexOf("AD") == -1) {
                    var af = $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_MedlemAf").val().split(";");
                    var pris_af = 0;

                    if (medlemskab2.indexOf("FF") > -1)
                        pris_af += parseInt(af[4]) || 0;
                    if (medlemskab2.indexOf("AF") > -1)
                        pris_af += parseInt(af[5]) || 0;
                    if (medlemskab2.indexOf("AD") > -1)
                        pris_af += parseInt(af[6]) || 0;


                    var besparelse = (pris_af - medlemskab) * 12;
                    if (besparelse > 0) {
                        //$(".skiftfra").html("fra " + $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_MedlemAf option:selected").text());
                        $(".dinbesparelse").html(besparelse);
                        $(".overflyt").show();
                    }
                }
            }

            $(".dinpris").html(medlemskab + tilkob);
            if (situation == "uddannelse")
                $(".dinpris").html("fra " + $(".dinpris").html());

            $("#blivmedlemspar").show();
        } else {
            $("#blivmedlemspar").hide();
            $("#blivmedlemspar_fejl").show();
        }
    }

    function UpdateAddons()
    {
        var situation = $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Situation").val();
        var medlemskab = $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option:selected").val();      // select.val() er i stykker, fordi vi har wrappet med span - brug i stedet option:selected
        
        $("#divTilkob input[type='checkbox']").prop("checked", false);      // nulstil valg
        $("#divTilkob div").hide();

        console.log(medlemskab);
        console.log(medlemskab.indexOf("FF"));
        switch (situation) {
            case "ledig":
            case "lonmodtager":
                if (medlemskab.indexOf("FF") > -1) {
                    if (medlemskab.indexOf("A") > -1) {
                        $("#divTilkob_OpkrMdr label").optionSetPrice(10);
                        $("#divTilkob_OpkrMdr").show();
                        $("#divTilkob_OpkrMdr_Ulykke label").optionSetPrice(30);
                        $("#divTilkob_OpkrMdr_Ulykke").show();
                    } else {
                        $("#divTilkob_UlykkeFF label").optionSetPrice(30);
                        $("#divTilkob_UlykkeFF").show();
                    }
                }

                break;
            case "uddannelse":
                if (medlemskab.indexOf("FF") > -1) {
                    $("#divTilkob_Ulykke label").optionSetPrice(20);
                    $("#divTilkob_Ulykke").show();
                }
                break;
        }
    }

    function FilterMemberships()
    {
        var situation = $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Situation").val();
        console.log(situation);

        $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab").val("");                             // nulstil valg
        $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option[value!='']").map(function() { return $(this).parent("span").length === 0 ? this : null; }).wrap("<span style='display:none;' />").hide();            // skjul alle valgmuligheder

        switch (situation) {
            case "ledig":
            case "lonmodtager":
                $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option[value='FFAF']").unwrap().show().optionSetPrice(473 + 69);
                $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option[value='FFAD']").unwrap().show().optionSetPrice(356 + 69);
                $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option[value='FF']").unwrap().show().optionSetPrice(69);
                $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option[value='AF']").unwrap().show().optionSetPrice(473);
                $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option[value='AD']").unwrap().show().optionSetPrice(356);
                break;
            case "uddannelse":
                $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option[value='FFAF']").unwrap().show().optionSetRemark("fra 0 kr.");
                $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option[value='FFAD']").unwrap().show().optionSetRemark("fra 0 kr.");
                $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option[value='FF']").unwrap().show().optionSetPrice(0);
                $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option[value='AF']").unwrap().show().optionSetRemark("fra 0 kr.");
                $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option[value='AD']").unwrap().show().optionSetRemark("fra 0 kr.");
                break;
            case "selvstaendig":
                $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option[value='FSAF']").unwrap().show().optionSetPrice(473 + 45);
                $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option[value='AF']").unwrap().show().optionSetPrice(473);
                break;
        }

        UpdateAddons();
    }

    $(window).ready(function () {
        $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option[value!='']").map(function () { return $(this).parent("span").length === 0 ? this : null; }).wrap("<span style='display:none;' />").hide();            // skjul alle valgmuligheder

        // Tillad kun ét tilkøb
        var $unq = $("#divTilkob input[type='checkbox']");
        $unq.click(function () {
            $unq.filter(":checked").not(this).prop("checked", false);
        });

        // standardværdier
        $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Situation").val(getParameterByName("s") || "lonmodtager");
        FilterMemberships();
        $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab").val(getParameterByName("m") || "FFAF");
        UpdateAddons();
        if (getParameterByName("t") != null)
            $("#divTilkob input[value=" + getParameterByName("t") + "]").prop("checked", true);
        if (getParameterByName("o") != null)
            $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_MedlemAf").val(getParameterByName("o"));
        else
            $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_MedlemAf option:eq(2)").prop("selected", true);
        CalculatePrice();

        $("#blivmedlembtn").click(function () {
            var s = $(this).attr("href");
            s += "#";
            s += "s=" + $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Situation").val();
            s += "&m=" + $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_Medlemskab option:selected").val();
            s += "&t=" + $("#divTilkob input:checked").val();
            s += "&o=" + $("#ContentPlaceHolderDefault_SiteMasterCPH_BlivMedlemLight_6_BlivMedlemInit_MedlemAf").val();

            top.location.href = s;
            return false;
        });
    });

    jQuery.fn.extend({
        optionSetPrice: function (price) {
            return this.each(function () {
                $(this).text($(this).text().replace(/ \(.*\)$/m, ""));
                if (price != null) {
                    $(this).text($(this).text() + " (" + price + " kr.)");
                }
            });
        },
        optionSetRemark: function (text) {
            return this.each(function () {
                $(this).text($(this).text().replace(/ \(.*\)$/m, ""));
                $(this).text($(this).text() + " (" + text + ")");
            });
        }
    });

    function getParameterByName(name) {
        name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
        var regex = new RegExp("[#&]" + name + "=([^&]*)"),
            results = regex.exec(location.hash);
        return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
    }

</script>

