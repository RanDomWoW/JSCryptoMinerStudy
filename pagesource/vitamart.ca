var mytooltip;

function tooltip_timeout(divid)
{
   mytooltip=setTimeout(function() {
   	 hideTooltip(divid);
   }, 1000);
}

function showTooltip(e,divid)
{
	clearTimeout(mytooltip);
    _w = 110;
    bodyWidth = document.body.clientWidth;
    bodyHeight = Math.max( window.innerHeight, document.body.clientHeight );

    pX = e.clientX + 15;
    if (pX + _w >= bodyWidth) {
        pX -= pX + _w - bodyWidth + 10;
    }

    pY1=100;

    pY=e.clientY;

  /*  if ((bodyHeight-pY) < 100)
    {
       pY=e.clientY-100;
    }
    else
    {
       pY=e.clientY+15;
       pY1=-100;
    } */
    pY+=15;
    if (pX > 900 && divid!="help_coupon")
    {
        x=250;
        y=100;

        showTooltip1(e,divid,x,y);
    }
    else
    {
        if (divid=="help_coupon")
           pX-=250;

		if (divid=="help_deliver_tonight")
		{
           pY-=160;
		   pX-=80;
		}
		
        $('#'+divid).css({position: 'absolute', top: pY, left: pX, display: 'block', visibility: 'visible'});
        e.stopPropagation();
    }
}

function showTooltip1(e,divid,xx,yy)
{
    _w = 110;
    bodyWidth = document.body.clientWidth;
    pX = e.clientX + 15;
    if (pX + _w >= bodyWidth) {
        pX -= pX + _w - bodyWidth + 10;
    }
    pX-=xx;

    pY=e.clientY;

    if (divid=="ajax_mini_cart")
       pX-=200;

    if(divid=="help_coupon")
    {
       pX-=150;
//       pY-=150;
    }
    pY+=10;

    $('#'+divid).css({position: 'absolute', top: pY, left: pX, display: 'block', visibility: 'visible'});
    e.stopPropagation();
}

function hideTooltip(divid)
{
    $('#'+divid).css({display: 'none', visibility: 'hidden'});
}

