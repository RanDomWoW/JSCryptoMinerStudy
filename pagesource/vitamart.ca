<script type="text/javascript">
/**
* Call this function when a user clicks on a product link. This function
uses the event
* callback datalayer variable to handle navigation after the ecommerce data
has been sent
* to Google Analytics.
* @param  productObj An object representing a product.
*/
function productClicked(pname,pcode,price,brand,cat,variant,pid) {
dataLayer.push({
'event': 'productClick',
'ecommerce': {
'click': {
'actionField': {'list': cat},// Optional list property.
'products': [{
'name': pname,
'id': pcode,
'price': price,
'brand': brand,
'category': cat,
'variant': variant
}]
}
},
'eventCallback': function() {
document.location.href = "product.php?productid="+pid;
}
});
}
</script>
