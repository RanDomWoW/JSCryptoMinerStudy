a{text-decoration:none}a:hover{text-decoration:underline}img,a img{border:none;overflow:hidden}select{padding:0px}textarea{resize:none}
b,strong,label,h1,h2,h3,h4,h5,h6,th{font-family:'Roboto-Medium';font-weight:normal}textarea{resize:none}

.fluid,.holder,.header,.nav,.footer,.head,.body,.foot,.row,.form,.flbl,.field,.output{float:left;width:100%;height:auto}

.container{width:1030px;height:auto;margin:auto}

.fl{float:left;width:auto;height:auto}
.fr{float:right;width:auto;height:auto}

.topbar{background-color:#444;padding:9px 0px;color:#fff;font-size:11px;font-family:'Roboto-Medium';text-transform:uppercase}
