}
@media (max-width: 1100px){
.container{width:768px}
}
@media (max-width: 1024px){
.fixed-img,.ads{display:none}
.container{width:100%}.topbar{padding:9px 10px}.header{padding:10px}
}
@media (max-width: 768px){
.fl,.fr{width:100%}
