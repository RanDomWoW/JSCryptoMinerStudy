<script type="text/javascript">
$(function(){
var RequestModal = {
	centerModal: function() {
		var w = $(window).width();
		var h = $(window).height();
		var cw = $("#request-modal-content").outerWidth(true);
		var ch = $("#request-modal-content").outerHeight(true);
		if(ch >= h){ 
			ch = h;
			$("#request-modal-content").css({"height": h});
		} else {
			$("#request-modal-content").css({"height": ""});
		}
		$("#request-modal-content").css({"left": ((w - cw)/2) + "px","top": ((h - ch)/2) + "px"});
	},
	init: function() {
		$("a.request-modal-open").click(function(){
			$("#request-modal-content label").removeClass("active");
			$("#request-modal-content #sale").parent().addClass("active");
			$("#request-modal-content #sale").prop('checked', true);
			$("#request-modal-content #sale").val(true);
			$("#request-modal-content #mansion").parent().addClass("active");
			$("#request-modal-content #mansion").prop('checked', true);
			$("#request-modal-content #mansion").val(true);
			
			$("body").append('<div id="request-modal-overlay"></div>');
			$("#request-modal-overlay").fadeIn("slow");
			RequestModal.centerModal();
			$("#request-modal-content").fadeIn("slow");
			$("#request-modal-overlay, #request-modal-close").unbind().click(function(){
				$("#request-modal-content, #request-modal-overlay").fadeOut("slow",function(){
					$('#request-modal-overlay').remove();
				});
			});
		});
		$(window).resize(RequestModal.centerModal);
		$( "#request-modal-content #btn_inquery" ).click(function() {
			url = "/request/"+$('input:radio[name=contents]:checked').attr("id") + "-" + $('input:radio[name=type]:checked').attr("id");
			window.location.href = url;
		});
		$("#request-modal-content input:radio[name=contents]").change(function() { $(".radio_contents label").removeClass("active"); $(this).parent().addClass("active"); });
		$("#request-modal-content input:radio[name=type]").change(function() { $(".radio_type label").removeClass("active"); $(this).parent().addClass("active"); });
	}
};
RequestModal.init();
});
