<script type="text/javascript">
	$(function() {
		function toggle_sticky(id) {
			$('#'+id).toggleClass("active");
			$('#'+id+'_view').toggleClass("active");
			if ($('.sticky>a').hasClass('active')) {
				$('.sticky_references').show();
				$('.sticky_views').show();
			} else {
				$('.sticky_references').hide();
				$('.sticky_views').hide();
			}
		};
		$('.sticky').on('click', '.icon-tag', function(event) {
			var id = this.id;
			toggle_sticky(id);
		});
		$('.sticky_views').on('click', '.close', function(event) {
			var id = $(this).parent().attr('id').replace('_view','');
			toggle_sticky(id);
		});
		$('#bookmark_add').click(function(event) {
			JikaBookmarks.pushBook(JikaBookmarks.cur_property);
		});
		$('.sticky_views').on('click', '.delete', function(event) {
			JikaBookmarks.removeUrl(this);
		});
	});
</script>


