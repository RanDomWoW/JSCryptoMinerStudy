<script type="text/javascript">
var exdays = 3;
// console.log(exdays);
var availableUtms = [
    "DORM1","DORM2","CREDITS","foam","optimise",];
window.setCookie = function(cname, cvalue, exdays,domain) {
    // console.log(cname);
    // console.log(cvalue);
    // console.log(exdays);
    // console.log(domain);
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = "expires="+d.toUTCString();
    document.cookie = cname + "=" + cvalue + "; " + expires+"; domain="+domain+"; path=/";
}

function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) != -1) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}

function cokfunc(){

    if(!getCookie('disableFragranceModal')){
        // console.log(getCookie('secs'));
        if(getCookie('secs') > 1000){
            var cookiesinr = getCookie('secs')-1000;
            window.setCookie('secs',cookiesinr,30);
        }
        setTimeout(cokfunc, 1000);
    }
};

jQuery(document).ready(function($){

    var subId;

    // for handling request shipment notification url
    if(window.location.href.indexOf("utm_content") > -1) {
        function getParameterByName(name, url) {
            if (!url) {
              url = window.location.href;
            }
            name = name.replace(/[\[\]]/g, "\\$&");
            var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
                results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';
            return decodeURIComponent(results[2].replace(/\+/g, " "));
        }
        subId = getParameterByName('utm_content');
        // console.log(subId);
    }

    for(i=0;i< availableUtms.length; i++){
        if(subId == availableUtms[i]){

            window.setCookie('disableFragranceModal','true',30,Mage.Cookies.domain);
            window.setCookie('newarrivalNotification','true',30,Mage.Cookies.domain);
            window.setCookie('utmRequested',subId,30,Mage.Cookies.domain);
            break;
        }
    }
