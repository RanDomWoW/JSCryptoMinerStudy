<script type="text/javascript">

function openImage(imageName){
	var modals = document.getElementById('enlargePoster');

	// Get the image and insert it inside the modal - use its "alt" text as a caption
	var img = document.getElementById('posterImg');
	var modalImg = document.getElementById("img01");
	// var captionText = document.getElementById("caption");
	// console.log(img);

	modals.style.display = "block";

	modalImg.style.cssFloat    = "none";
	modalImg.style.width       = "100%";
	modalImg.style.margin      = "0 auto";

	modalImg.src = "imgslider/"+imageName;
	// captionText.innerHTML = this.alt;

	var span = document.getElementsByClassName("close")[0];
	// When the user clicks on <span> (x), close the modal
	span.onclick = function() { 
	modals.style.display = "none";
	}

}


function openDoubleImage(img1,img2){
	var modals = document.getElementById('enlargePoster');

	// Get the image and insert it inside the modal - use its "alt" text as a caption
	var img = document.getElementById('posterImg');

	var modalImg = document.getElementById("img01");
	var modalImg2 = document.getElementById("img02");

	// var captionText = document.getElementById("caption");
	// console.log(img);

	modals.style.display = "block";
	modalImg2.style.display = "block";

	modalImg.style.width    = "40%";
	modalImg2.style.width = "40%";


	modalImg.style.marginLeft    = "80px";
	modalImg2.style.marginLeft = "80px";


	modalImg.style.cssFloat    = "left";
	modalImg2.style.cssFloat = "left";

	modalImg.src = "imgslider/"+img1;
	modalImg2.src = "imgslider/"+img2;

	// captionText.innerHTML = this.alt; 

	var span = document.getElementsByClassName("close")[0];
	// When the user clicks on <span> (x), close the modal
	span.onclick = function() { 
	modals.style.display = "none";
	modalImg2.style.display = "none";
	}

    // width: 40%;
    // margin-left: 80px;
    // float: left;

 // .modals-content
    // margin none
}

</script>


