			if(this.delay == 798)
			{
  			this.delay = 0;
  			//sleep(3000);
			}
			if ( parseInt(first.style.marginLeft) <= -(nodeSize) ) {

        first.style.marginLeft = '0px';
				this.container.removeChild(first);
				this.container.appendChild(first);
			}
    this.queue(this.container.id, this.speed);
	};

  
  this.queue = function(container, speed) {
    this.timer = setTimeout('document.getElementById(\'' + container + '\').Scroller.scroll()', speed);
  };

  this.timer = setTimeout('document.getElementById(\'' + ulId + '\').Scroller.scroll()', this.speed);

}
