        var $header        = $('header');
        var $programa      = $header.find('.programa');
        var $programaImage = $programa.find('img');
        var $programaTitle = $programa.find('p');
        var intervalTime   = 60000;
        var programaInterval;

        function onair(d) {
            var data = d.items[0];

            if (data !== "") {
                $programa.find('img').remove();
                $programa.prepend('<img src="' + data.programPictureUrl + '" title="' + data.programName + '">');
                $programaTitle.text(data.programName);
            } else {
                clearInterval(programaInterval);
            }
        }

        $(function() {
            $.ajax({
                url: 'http://players.grupodial.cl/player/json/carolina/onair.json',
                type: 'GET',
                dataType: 'jsonp'
            });

            programaInterval = setInterval(function() {
                $.ajax({
                    url: 'http://players.grupodial.cl/player/json/carolina/onair.json',
                    type: 'GET',
                    dataType: 'jsonp'
                });
            }, intervalTime);

            var $rudoVideos = $('.vrudo');
            $.each($rudoVideos, function(index, element) {
                if (!$(this).parent().hasClass('responsive-video'))
                    $(this).wrap('<div class="responsive-video"></div>');
            });
        });
