<script type="text/javascript">
$(function(){
if($('#vote-ajax').length != 0){
var frm = $('#vote-ajax');
function RenderAnswers(data){
var poll = '<div class="poll"><dl>';
$.each(data.answers_smarty, function(i, item){
poll += '\
<dt>&mdash; '+item+'</dt>\
<dd>\
<div class="num">'+data.answers_w[i]+'% ('+data.answers_num[i]+' голосов)</div>\
<div class="pollline"><div style="width: '+data.answers_w[i]+'%;"></div></div>\
</dd>\
';
}); 
poll += '</dl></div>';
$(frm).append(poll+'<span class="votes-all">Всего голосов: '+data.all_num+'</span><a href="/polls/" class="all-more-info ml10">Все опросы</a>');
};
if($('.poll', frm).length != 0){
var p_id = $('.poll', frm).attr("data-id");
$.ajax({
type: 'get',
dataType: 'json',
url: '/polls/vote/?id='+p_id,
success: function(data){
if(data.length != 0){
RenderAnswers(data);
};
}
});
};
$('#vote-ajax').submit( function(){
$('.btn', frm).addClass('disable');
$.ajax({
type: 'post',
dataType: 'json',
url: '/polls/vote/',
data: frm.serialize(),
success: function(data){
if(data.length != 0){
$('.vote-variants, .submit-button, .all-more-info', frm).hide();
RenderAnswers(data);
};
}
});
return false;
});
};
});
</script>

