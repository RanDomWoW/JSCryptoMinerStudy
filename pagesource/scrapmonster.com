$("#file").change(function() {
	
			var file = $('#file').get(0).files[0];
			var imagefile = file.type;
			
			var match= ["image/jpeg","image/png","image/jpg","image/gif"];
			if(!((imagefile==match[0]) || (imagefile==match[1]) || (imagefile==match[2])|| (imagefile==match[3])))
			{
				
				alert("Only JPEG, PNG and GIF images are allowed");
				return false;
			}
			else
			{
				
				var numFiles = $('#file').get(0).files.length;
			    for (i = 0; i < numFiles; i++) {
					var file = this.files[i];
					
					var reader = new FileReader();
					reader.onloadend = function(e) 
					{
						abc += 1;
						$('#image_preview').append('<span id="delete'+ abc +'" class="mar-rt-10"><img  id="previewing'+ abc +'" src="'+ e.target.result+'" class="previewing"/></span>');
						$("#delete" + abc).append($("<img/>", {
							id: 'deleteimg',
							src: 'https://www.scrapmonster.com/images/delete.png',
							alt: 'delete',
							title :'Delete'
							}).click(function() {
							$(this).parent().remove();
						}));
						
				
					}
					reader.readAsDataURL(file); 
				
				}
			}
	});
	
