    var wppb_map;
    var marker_group = {
        "centers": [],
        "situations": [],
        "control": [],
        "buvets": []
    },infoWindowModal = [];

    function initMap() {
        wppb_map = new google.maps.Map(document.getElementById('mapMain'), {
            zoom: 11,
            center: {lat: 50.450070, lng: 30.522385}
        });


        /*
        [MIGX_id] => 2
        [mNumber] => 2
        [coliforms] => 1
        [coli] => 1
        [enterococci] => 1
        [scent] => 1
        [coloration] => 16
        [turbidity] => 0,34
        [taste] => 1
        [chrol] => 0,95
        */

        var infowindow = new google.maps.InfoWindow();
        infoWindowModal = infowindow;

        for (var i = 0; i < list['centers'].length; ++i) {
            configureMarkersAndInfoWindows('centers', list['centers'][i]['latitude'], list['centers'][i]['longitude'], list['centers'][i]['caption'], '', '', list['centers'][i]['address'], '', '', list['centers'][i]['phone'], list['centers'][i]['tcos']);
        }


        for (var i = 0; i < list['situations'].length; ++i) {
            configureMarkersAndInfoWindows('situations', list['situations'][i]['latitude'], list['situations'][i]['longitude'], list['situations'][i]['caption'], '', '', list['situations'][i]['address'], list['situations'][i]['dateSituation'], list['situations'][i]['timeSituation'], '');
        }


        for (var i = 0; i < list['control'].length; ++i) {
            configureMarkersAndInfoWindows('control', list['control'][i]['latitude'], list['control'][i]['longitude'], list['control'][i]['caption'], '', '', '', '', '', '', '', list['control'][i]['urlQuality']);
        }


        for (var i = 0; i < list['buvets'].length; ++i) {
            configureMarkersAndInfoWindows('buvets', list['buvets'][i]['latitude'], list['buvets'][i]['longitude'], list['buvets'][i]['caption'], list['buvets'][i]['image'], '', '', '', '', '', '');
        }

        function configureMarkersAndInfoWindows(type, lat, lng, title, image, information, address, date, time, phone, tcos, url ) {

            var icon = 'https://maps.google.com/mapfiles/kml/pal3/icon21.png', link = '<a href="laboratorіya-kontrolyu-yakostі-stіchnix-vod">контроль якості</a>';
			//var self = this;

			if (tcos == 1){
				icon = 'https://maps.google.com/mapfiles/kml/pal2/icon6.png';
			}

			 if (url != ''){
                link = '<a href="'+url+'">контроль якості</a>';
            }


            switch (type){
                case 'centers':
                    icon = icon;
                    break;
                case 'situations':
                    icon = "https://vodokanal.kiev.ua/template/danger.png";
                    break;
                case 'control':
                    icon = "https://vodokanal.kiev.ua/template/qauality.png";
                    break;
                case 'buvets':
                    icon = "https://vodokanal.kiev.ua/template/buvet.png";
                    break;
            }

            var marker = new google.maps.Marker(
                {
                    position: {lat: parseFloat(lat), lng: parseFloat(lng)},
                    map: wppb_map,
                    title: title,
                    icon: icon
                }
            );



            google.maps.event.addListener(marker, 'click', function () {
                infowindow.close();
                switch (type){
                    case 'centers':
                        infowindow.setContent('<p>Назва установи: '+title+'</p>' +
                            '<p>Адреса: '+address+'</p>' +
                            '<p>Номер тел.:'+phone+'</p>');
                        break;
                    case 'situations':
                        infowindow.setContent('<p  class="infoStreet">'+title+'</p>' +
                            '<p>Адреса: '+address+'</p>' +
                            '<p>Дата події: '+date+'</p>' +
                            '<p>Термін виправлення: '+time+' дні</p>');
                        break;
                    case 'control':
                        infowindow.setContent('' +
                            '<p class="infoStreet">Адреса:'+title+'</p> ' +
                            '<p class="infoBottom">Перейти на сторінку '+link+'</p>');
                        break;
                    case 'buvets':
                        infowindow.setContent('<p style="font-weight: 400; margin-bottom: 10px;">Бювет по вулиці: '+title+'</p><p style="font-weight: 400; margin-bottom: 10px;">Відповідальний Київводфонд.</p><div style="max-width:150px;"><img style="width:100%" src="/assets/images/'+image+'"></div>');
                        break;
                }

                infowindow.open(wppb_map, marker);

            });




            marker_group[type].push(marker);
        }
    }

</script>

