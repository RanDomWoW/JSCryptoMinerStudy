	function search(){
		var standingid=$("#selstanding").val();
		var standingvalue=$("#selstanding").find(':selected').text();

		var disciplineid=$("#seldiscipline").val();
		var disciplinevalue=$("#seldiscipline").find(':selected').text();
		
		
		
		var s,e,t;
		s=disciplinevalue.indexOf("[");
		e=disciplinevalue.indexOf("]");
		t=disciplinevalue.substr(s,e);
		disciplinevalue=disciplinevalue.replace(t,"");

		if(standingid=='select'){
			alert('Please select your qualification.');
			return;
		}
	
		if(disciplineid=='select'){
			alert('Please select your programme.');
			return;
		}
		var url="";
		url+=window.location.protocol+"//"+window.document.domain+((window.document.domain.indexOf('lpu')==-1)?"/lpu2016/distance":"/programmes");
		url+="/"+$("#selstanding").find(":selected").text().trim();
		url+="/"+$("#seldiscipline").find(":selected").attr('data-name');
		url+="/"+disciplinevalue;
		//alert(url);
		window.location.href=url;
	} 
			


</script>
