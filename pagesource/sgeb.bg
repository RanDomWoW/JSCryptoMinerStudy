        <p class="title">Бисквитки/Cookies</p>
        <span>
                                Страницата на Банка Societe Generale Експресбанк използва Бисквитки /Cookies.            
        </span>
        <ul>
            <li>
                <a target="_blank" href="http://www.sgeb.bg/byrzi-vryzki/biskvitki-cookies.html">Прочети повече.</a>
            </li>
            <li>
                <a href="javasript:;" id="cookie_agr">Разбрах, затвори.</a>
            </li>
        </ul>
        <div class="clearfix"></div>
    </div>
</div>
<script>
    $(function(){
        checkCookieAgr();
    });
    $(document).on("click","#cookie_agr , #cookie_warn a.close",function(e) {
	e.preventDefault();
        $('#cookie_warn').fadeOut('fast');
        setCookie('cookie_agr', 1, 30); // 30days
    });
    
    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays*24*60*60*1000));  // 30days
        //d.setTime(d.getTime() + (30*1000)); // FOR TEST MAKE IT 30sec
        var expires = "expires="+d.toUTCString();
        document.cookie = cname + "=" + cvalue + "; " + expires + "; path=/";
    }

    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i=0; i<ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1);
            if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
        }
        return "";
    }


    function checkCookieAgr() {
        var cookie_agr = getCookie("cookie_agr");
        if (cookie_agr != '1') {                
            $('#cookie_warn').css('display' , 'block');
        } 
    }
</script>
