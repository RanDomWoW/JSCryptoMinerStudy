                                ng-model="form.data.email"
                                placeholder="Email"
                                required>
                    </fieldset>
                    <fieldset class="form-group">
                        <label>Password</label>
                        <input
                                name="password"
                                class="form-control"
                                ng-class="{ 'has-error animated shake': form.registerForm.$submitted && form.registerForm.password.$invalid }"
                                type="password"
                                ng-model="form.data.password"
                                placeholder="Password"
                                required>
                    </fieldset>
                    <fieldset class="form-group">
                        <label>Confirm Password</label>
                        <input
                                name="confirm_password"
                                class="form-control"
                                ng-class="{ 'has-error animated shake': form.registerForm.$submitted && form.registerForm.confirm_password.$invalid }"
                                type="password"
                                ng-model="form.data.password_confirmation"
                                placeholder="Confirm Password"
                                required>
                    </fieldset>
                </div>
            </div>
        </div>
        <div><small ng-bind="form.message" class="hertext-red signup-invalid-message font-ubuntu"></small></div>
