            function openCompartilhar(pag, titulo) {
                var url = pag;
                var title = titulo;
                var cqdialog = $('#compartilhamento').html('<iframe style="border: 0px; width:100%; height:100%; min-height: 700px; z-index: 100 !important;" src="/page/compartilhar/?url=' + url + '&title=' + title + '" scrolling = "auto" ></iframe>')
                    .dialog({
                        position: 'center',
                        autoOpen: false,
                        modal: true,
                        width: 800,
                        title: title,
                        minWidth: 500,
                        resizable: true,
                    }).width(760).height(550);
                cqdialog.dialog('open');
            }
