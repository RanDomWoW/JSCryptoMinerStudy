<script>
	$.ajax({
		url: 'https://forum.cofe.ru/quickinfo.php',
		dataType: "json",
		beforeSend: function(){
		},
		success: function(data, textStatus){
			var cofe_threads = '';
			var cofe_activities = '';
			var cofe_blogs = '';

			$.each(data.activities, function(key, activity)
			{
				cofe_activities += '<li>';
				cofe_activities += '<a href="http://forum.cofe.ru/showthread.php?t=' + activity.threadid + '&p=' + activity.postid + '#post' + activity.postid + '" target="_blanck">"' + activity.username + '" ответил(-а) в теме "' + activity.title + '"</a>';
				cofe_activities += '</li>';
			});

			$.each(data.threads, function(key, thread)
			{
				cofe_threads += '<li>';
				cofe_threads += '<a href="http://forum.cofe.ru/showthread.php?t=' + thread.threadid + '" style="padding: 0px 0px 0px 0px;" target="_blanck">' + thread.title;
				cofe_threads += '<div class="entry-comments-views">';
				cofe_threads += '<span class="td-sp td-sp-ico-view"></span><span class="td-nr-views-111">' + thread.views + '</span>' + ' <span class="td-sp td-sp-ico-comments td-fake-click"></span>' + thread.replycount;
				cofe_threads += '</div>' + '</a>';
				cofe_threads += '</li>';
			});

			$.each(data.blogs, function(key, blog)
			{
				cofe_blogs += '<li>';
				cofe_blogs += '<a href="http://forum.cofe.ru/entry.php?b=' + blog.blogid + '" style="padding: 0px 0px 0px 0px;" target="_blanck">' + blog.title;
				cofe_blogs += '<div class="entry-comments-views">';
				cofe_blogs += '<span class="td-sp td-sp-ico-view"></span><span class="td-nr-views-111">' + blog.views + '</span>' + ' <span class="td-sp td-sp-ico-comments td-fake-click"></span>' + blog.comments_visible;
				cofe_blogs += '</div>' + '</a>';
				cofe_blogs += '</li>';
			});

			$('.cofe-activities').html(cofe_activities);
			$('.cofe-threads').html(cofe_threads);
			$('.cofe-blogs').html(cofe_blogs);
		},
		error: function(xhr, status, error){
		}
	});
</script>

