<div class="modal fade moe-modal moe-modal-amazon moe-modal-amazoninfo" id="moe-Amazon">
  <div class="modal-dialog modal-lg">
    <div class="modal-content" data-template="#template-amazon">
    </div>
  </div>
</div>

<script id="template-amazon" type="x-tmpl-mustache">
  <div class="modal-header">
    <button type="button" class="close modal-title" data-dismiss="modal"><span>✕</span></button>
    <h4 class="modal-title">您选中的商品</h4>
  </div>
  <div class="modal-body">
    <div>
      <div class="moe-title">
        <h4>{{title}}</h4>
        <h5>{{category}}</h5>
        <div class="clearfix">
          {{#variation}}
          <div class="moe-amazonitem-value">
            型号：
            <div class="left">{{key}}</div>
            <div class="center"></div>
            <div class="right">{{value}}</div>
          </div>
          {{/variation}}
          {{#hasVariation}}
          <div class="moe-amazonitem-value">
            <a class="moe-amazonitem-tip"
               href="javascript:;"
               data-toggle="tooltip"
               data-placement="bottom"
               data-original-title="若显示的商品型号与您想要购买的不同，可以在amazon网站上选择您想要购买的商品型号和尺寸，将其加入购物车，再从购物车中点击该商品，即可跳转到正确的链接地址。">
               <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span> 我该如何变更型号？
            </a>
          </div>
          {{/hasVariation}}
          </div>
        </div>
      </div>
    <ul class="nav nav-tabs moe-amazon-tabs">
      {{#availConditions.new}}
      <li role="presentation" z-class="active: poi.amazon.condition == 'new'">
        <a href="javascript:void(0);" z-onclick="@.switchList('new', 0)">全新</a>
      </li>
      {{/availConditions.new}}
      {{#availConditions.used}}
      <li role="presentation" z-class="active: poi.amazon.condition == 'used'">
        <a href="javascript:void(0);" z-onclick="@.switchList('used', 0)">中古品</a>
      </li>
      {{/availConditions.used}}
      {{#availConditions.collectible}}
      <li role="presentation" z-class="active: poi.amazon.condition == 'collectible'">
        <a href="javascript:void(0);" z-onclick="@.switchList('collectible', 0)">コレクター商品</a>
      </li>
      {{/availConditions.collectible}}
    </ul>
    <table class="table moe-amazonitem">
      <thead>
        <tr>
          <th class="moe-amazonitem-seller">商家</th>
          <th class="moe-amazonitem-comments">描述</th>
          <th class="moe-amazonitem-price">价格</th>
          <th class="moe-amazonitem-cart"></th>
        </tr>
      </thead>
      <tbody>
        {{#products}}
        <tr>
          <td>
            {{{getSeller}}}
            {{#sellerRating}}
              <br />
              {{#ratingStr}}
              <span class="moe-amazonitem-rate">
                {{getRatingStr}}
              </span>
              {{/ratingStr}}
              <span class="moe-amazonitem-rate omni-text">
                {{#highRate}}
                好评率{{highRate}}%
                {{/highRate}}
                {{#total}}
                <br/>
                共{{total}}条反馈
                {{/total}}
              </span>
            {{/sellerRating}}
          </td>
          <td>
            {{#comments}}
              <div class="moe-amazonitem-comments omni-text">
                {{comments}}
              </div>
            {{/comments}}
            {{{conditionDetailPill}}}
            {{#comments}}
              {{#badges}}
                <span class="moe-amazonitem-deliver">
                  <i></i> 由亚马逊日本发货
                </span>
              {{/badges}}
              {{^badges}}
                {{#sendFrom}}
                  <span class="moe-amazonitem-deliver">
                    发货源：{{sendFrom}}
                  </span>
                {{/sendFrom}}
              {{/badges}}
            {{/comments}}
            {{^comments}}
              <span class="moe-amazonitem-deliver">
                <i></i> 由亚马逊日本发货
              </span>
            {{/comments}}
          </td>
          <td>
            <span class="moe-amazonitem-itemprice omni-text" z-text="@.formatPrice({{price}})"></span>
            <br z-class="hidden: {{shipCharge}} == 0" />
            <span class="moe-amazonitem-japanprice" z-class="hidden: {{shipCharge}} < 1">
              日本国内运费
              <br z-class="hidden: {{shipCharge}} == 0" />
              <span z-text="@.formatPrice({{shipCharge}})"></span>
            </span>
            <br z-class="hidden: {{shipCharge}} > 0" />
            <span class="moe-amazonitem-curprice">免手续费</span>
          </td>
          <td>
            <button class="btn btn-default btn-sm onmiblue" type="button"
                    ajax-url="/cart/addAmazon"
                    method="post"
                    data-params="pid: {{pid}}, merchantId: '{{merchantId}}', url: '{{url}}', ship_charge:'{{shipCharge}}', price:'{{price}}', condition_text:'{{conditionText}}', comments:'{{comments}}'"
                    ajax-cb="$('#moe-Amazon').modal('hide');poi.updateCart(@.badge+@.req_badge);
                             if(@.overload) poi.msg('您购买的部分商品超过单个商品最大购买限制，请去购物车确认商品数量', 'danger'); if(@.message) poi.msg(@.message);">
              加入购物车
            </button>
          </td>
        </tr>
        {{/products}}
      </tbody>
    </table>
  </div>
  <div class="modal-footer">
    <div z-class="hidden: !@.pageTotal" class="moe-amazon-pagination">
      <ul class="pagination">
        <li>
          <a href="javascript:void(0)" z-onclick="@.switchList('', 1)" aria-label="Previous">
            <span aria-hidden="true" z-onclick="">&laquo;</span>
          </a>
        </li>
        {{#pageSelect}}
        <li z-class="active: @.pageCur == {{.}}">
          <a href="javascript:void(0)" z-onclick="@.switchList('', {{.}})">{{.}}</a>
        </li>
        {{/pageSelect}}
        <li>
          <a href="javascript:void(0)" z-onclick="@.switchList('', {{pageTotal}})">
            <span aria-hidden="true">&raquo;</span>
          </a>
        </li>
      </ul>
  </div>
</script>


