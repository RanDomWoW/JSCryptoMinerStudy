<script>

var omniRequestApply;

$(function () {

  omniRequestApply = (function () {
    var vm = new Vue({
      el: '#omni-request-apply',
      data: {
        site: '',
        url: '',
        link: '',
        title: '',
        variation: '',
        amount: '',
        remark: '',
        guid: '',
        unit_price: '',
        host: '',
        show_flag: false,
        show_host: false,

        is_yahoo: false,
        win_mode: true,
        immediately: false,
        unit_price_placeholder: '',
        win_price: 0,
        title_placeholder: '',
        amount_placeholder: 99,
        yahoo_price: 0,
      },
      mounted: function () {
        $('#omni-request-apply').on('hidden.bs.modal', this.clear);
        this.clear();
      },
      methods: {
        open: function (config) {
          var _this = this;

          this.is_yahoo = !!config['is_yahoo'];

          if(config['url'] !== undefined) { this.url = config['url']; this.link = config['url']; }
          if(config['site'] !== undefined) this.site = config['site'];
          if(config['title'] !== undefined) this.title = config['title'];
          if(config['variation'] !== undefined) this.variation = config['variation'];
          if(config['amount'] !== undefined) this.amount = config['amount'];
          if(config['remark'] !== undefined) this.remark = config['remark'];
          if(config['guid'] !== undefined) this.guid = config['guid'];
          if(config['unit_price'] !== undefined && this.unit_price > 0) this.unit_price = config['unit_price'];
          if(config['host'] !== undefined) {
            this.show_host = true;
            this.host = config['host'].host;
            if(config['host'].status == 1) {
              this.show_flag = true;
            } else {
              this.show_flag = false;
            }
          } else {
            this.show_host = false;
            this.host = '';
          }

          $('#omni-request-apply').modal('show');

          this.title_placeholder = '获取标题中';
          var api = '';
          if(this.is_yahoo) {
            api = 'fetchYahooAuction';
          } else {
            api = 'fetchTitle';
          }
          $.post('/index/' + api, {url: _this.url}, function (data, status, xhr){
            if(_this.url == config['url']) {
              _this.title_placeholder = '';
              if(data.title && _this.title == ''){
                _this.title = data.title;
              }
            }
            if(_this.is_yahoo) {
              if(data.price) {
                _this.unit_price_placeholder = '本次出价不可少于 ' + data.price + ' 日元';
              }
              _this.yahoo_price = data.price;
              if(data.winPrice > 0) {
                _this.win_price = data.winPrice;
              }
              if(data.winPrice == data.price) {
                _this.win_mode = false;
                _this.unit_price_placeholder = '一口价 ' + data.price + ' 円';
                _this.unit_price = data.price;
              }
              if(data.quantity > 0) {
                _this.amount_placeholder = data.quantity;
                if(_this.amount_placeholder == 1) {
                  _this.amount = 1; 
                }
              }
            }
          }).fail(function (event, data){
            _this.title_placeholder = '抓取URL标题失败，您可在此自行填上商品名供自己和工作人员参考';
            if(_this.is_yahoo) {
              $('#omni-request-apply').modal('hide');
              return;
            }
          });
        },
        clear: function () {
          this.site = '';
          this.url = '';
          this.link = '';
          this.title = '';
          this.variation = '';
          this.amount = '';
          this.remark = '';
          this.guid = '';
          this.unit_price = '';
          this.host = '';
          this.show_flag = false;
          this.show_host = false;

          this.is_yahoo = false;
          this.win_mode = true;
          this.immediately = false;
          this.unit_price_placeholder = '';
          this.win_price = 0;
          this.title_placeholder = '';
          this.amount_placeholder = 99;
          this.yahoo_price = 0;
        },
        auction: function () {
          if((this.win_price != 0) && this.win_mode == 1 && this.unit_price >= this.win_price) {
            poi.msg('竞拍出价需小于一口价：' + this.win_price + ' 円', 'danger');
            return;
          }
          var postdata = $('#omni-request-apply').parent('form').serializeArray();
          postdata.push({
            name: 'from',
            value: 'yahoo_auction',
          });
          $.post('/checkout', postdata, function () {
            var form = document.createElement('form');
            form.method = 'post';
            form.action = '/checkout';

            var addInput = function (name, value) {
              var input = document.createElement('input');
              input.type = 'hidden';
              input.name = name;
              input.value = value;
              form.appendChild(input);
            };

            for(var i = 0; i < postdata.length; ++i) {
              addInput(postdata[i].name, postdata[i].value);
            }
            addInput('_token', '5ab1e6a0b8f6d7.68342813');

            document.body.appendChild(form);
            form.submit();
            document.body.removeChild(form);
          });
        },
      },
      filters: {
        encodeURI: function (val) {
          return encodeURI(val);
        }
      },
    });

    return {
      open: vm.open,
      clear: vm.clear,
      close: function () {
        $('#omni-request-apply').modal('hide');
      }
    };

  })();

  $('body').on('click', '.switch-button', function () {
    $(this).find('input').click();
  });

});
</script></form>

<script>
$(document).ready(function() {
  $('#moe-ItemData').zBind(poi.item_list);

  // Amazon
  poi.amazon = {
    // 亚马逊图标/商家名 输出
    getSeller: function () {
      if(this.hasOwnProperty('sellerImg'))
      {
        return '<img src="' + this.sellerImg + '"></img>';
      }
      else {
        return (this.seller.toLowerCase() === 'amazon') ? '<div class="moe-amazon-logo"></div>' : this.seller;
      }
    },
    // 过滤输出“新規出品者”
    getRatingStr: function () {
      if(this == '新規出品者') return this;
    },
    // 品相药丸
    conditionDetailPill: function (){
      var classList = {
        '新品':       'new',
        'ほぼ新品':   'nearlynew',
        '非常に良い': 'beyondfine',
        '良い':       'fine',
        '可':         'commonly'
      };
      if(this.hasOwnProperty('conditionDetail'))
      {
        return '<span class="moe-pill moe-pill-' + (classList[this.conditionDetail] || 'new') + '">' + this.conditionDetail + '</span>';
      }
      else
      {
        return '';
      }
    },
    hasVariation: function (){
      return this.hasOwnProperty('variation');
    },
    // 输出分页模板数据
    pageSelect: function () {
      var result = [];
      for(i = 1; i <= this.pageTotal; ++i) {
        result.push(i);
      }
      return result;
    },
    // 货币格式化输出
    formatPrice: function(price) {
      return poi.formatPrice(price) + ' 円／' + poi.formatPrice(Math.ceil(price * poi.exchange)) + ' 元';
    },
    // 获取新列表
    switchList: function(condition, page) {
      condition = condition || this.condition;
      page = (page < 1) ? 1 : page;
      if(this.condition == condition && this.pageCur == page) {
        return;
      }
      $('.moe-amazon-shadow').addClass('moe-shadow-loading');
      // 向表单写入新url
      var url = 'http://www.amazon.co.jp/gp/offer-listing/' + this.asin;
      url += '?condition=' + condition;
      url += '&startIndex=' + ((page - 1) * 10);
      // 直接改动用户输入跳转
      $('#crawl-form input[name=url]').val(url);
      $('#crawl-form').submit();
      $('#moe-Amazon .modal-dialog').addClass('moe-shadow-loading');
    },
    // 刷新拟态框
    // 单独抽出需要数据
    refreshPage: function(result) {
      // url
      this.url = result.url;
      this.pid = result.asininfo.pid;
      this.asin = result.asininfo.asin;
      // condition
      this.condition = result.asininfo.condition;
      // 当前页数
      this.pageCur = result.asininfo.pageCur;
      // 总页数
      this.pageTotal = result.asininfo.pageTotal;
      // 商品信息
      this.title = result.asininfo.title;
      // 品相信息
      this.availConditions = {};
      for(var key in result.asininfo.availConditions) {
          this.availConditions[result.asininfo.availConditions[key].toLowerCase()] = 1;
      }
      // 型号信息
      this.variation = [];
      if (result.asininfo.hasOwnProperty('variation'))
      {
        for(var i in result.asininfo.variation.attrNames) {
          this.variation.push({
            key: result.asininfo.variation.attrNames[i],
            value: result.asininfo.variation.attrValues[i]
          });
        }
      }
      // 商家列表
      this.products = result.asininfo.products;
      for(var i in this.products) {
        this.products[i].index = parseInt(i);
      }
      // 同步
      this.$apply();
      $('#moe-Amazon').scrollTop(0);
    }
  };
  // 绑定
  $('#moe-Amazon').zBind(poi.amazon);

  // 查询事件
  $('#crawl-form').on('ajaxDone', function(event, result) {
    if(result.buy_request) {
      var remark = '';
      if(result.site.status == 0) {
        remark = '您提交的网站为首次业务，您的订单可能在审核完毕后更新购物须知。'
      } else {
        remark = result.site.remark;
      }
      omniRequestApply.open({
        url: $('#crawl-form input[name=url]').val(),
        site: result.site.remark,
        host: result.site,
        is_yahoo: !!result.yahoo_auction_flag,
        crawl_result: result,
      });
    }
    // 亚马逊结果
    else if( result.is_amazon ) {
      redirectAmazonToBrowserSite(result.url);
    }
    // 其他
    else {
      if(poi.currency == poi.consts_origin['currency']['jpy']) {
        result.items.forEach(function (item) { item.price = item.jpy_price });
      }
      poi.item_list.items = result.items;
      poi.item_list.url = result.url;
      poi.item_list.$apply();
      $('#moe-ItemData').modal();
    }
  });

  //featured-sites
  (function() {
    var slideFeatureSiteContainer = (function() {
      function curry(property, setFunc) {
        var list = $('.moe-featured-sites-wrapper')[0];

        return function(number) {
          list.style[property] = setFunc(number);
          $(list).height($(list).children('.container[data-id=' + number + ']').height());
        }
      }

      if (navigator.userAgent.indexOf("MSIE 8.0") > 0 || navigator.userAgent.indexOf("MSIE 9.0") > 0) {
        return curry('left',function(number) {
          if(number)
            return "-" + number + "00%";
          return 0;
        });
      }

      return curry('transform', function(number) {
        if (number)
          return "translateX(-" + number + "00%)";
        return "";
      });
    })();
    var slideFeatureSiteNavi = function(number) {
      $('.moe-featured-sites-navi .moe-featured-sites-category li').removeClass('active');
      $('.moe-featured-sites-navi .moe-featured-sites-category li[data-id='+number+']').addClass('active');
    };
    $('.moe-featured-sites-navi .moe-featured-sites-category li').click(function() {
      var index = $(this).data('id');
      slideFeatureSiteContainer(index);
      slideFeatureSiteNavi(index);
    });

    $('.moe-featured-sites-navi .moe-featured-sites-category li:first-child').click();

  })();

  //
  //bulletin
  var moeBulletinTime = 5000;

  var $moeBulletin = $('.moe-announce-list');
  var moeBulletinHeight = $('.moe-announce-wallpaper').height();
  var moeBulletinCount = $('.moe-announce-list li').length;
  var moeBulletinIndex = 1;
  var moeBulletinTimer = 0;

  var moeBulletinGo = function (timeout) { moeBulletinTimer = setTimeout(moeBulletinRoll, timeout !== undefined ? timeout : moeBulletinTime); };
  var moeBulletinStop = function () { clearTimeout(moeBulletinTimer); };

  // rool
  var moeBulletinRoll = function () {
    if (moeBulletinIndex >= moeBulletinCount) {
      $moeBulletin.css({'margin-top': '0px' });
      moeBulletinIndex = 0;
      moeBulletinGo(0);
    } else {
      $moeBulletin.css({'margin-top': '-' + (moeBulletinIndex * moeBulletinHeight) + 'px' });
      ++moeBulletinIndex;
      moeBulletinGo();
    }
  };

  // when hover
  $moeBulletin
    .on('mouseenter', 'li', function (){ moeBulletinStop(); })
    .on('mouseleave', 'li', function (){ moeBulletinGo(); });

  // init
  if(moeBulletinCount > 1) {
    moeBulletinGo();
  }

  // modal
  $('a.moe-announce-content').click(function() {
    $('#moeBulletin h4.modal-title').html(
      $(this).find('~ div.title').html() +
      ' <span class="time">' + $(this).find('~ div.time').html() + '</span>'
    );
    $('#moeBulletin .modal-body')
      .css({'white-space': 'pre-wrap'})
      .html($(this).find('~ div.content').html());
    $('#moeBulletin').modal();
  });

  // more
  var moeAnnounceMoreTimer = 0;
  var moeAnnounceMoreOpen = false;

  $('.moe-announce-more-button').on('click', function () {
    if(!moeAnnounceMoreOpen) {
      $('.moe-announce-more').show();
      clearTimeout(moeAnnounceMoreTimer);
      moeAnnounceMoreTimer = setTimeout(function (){ $('.moe-announce-more').hide(); moeAnnounceMoreOpen = false; }, 3000);
      moeAnnounceMoreOpen = true;
    } else {
      $('.moe-announce-more').hide();
      clearTimeout(moeAnnounceMoreTimer);
      moeAnnounceMoreOpen = false;
    }
  });

  $('.moe-announce-more')
    .on('mouseleave', function (){
      moeAnnounceMoreTimer = setTimeout(function (){ $('.moe-announce-more').hide(); moeAnnounceMoreOpen = false; }, 3000);
    })
    .on('mouseenter', function (){
      clearTimeout(moeAnnounceMoreTimer);
    });

  $('#crawl-form').on('submit', function (e) {
    var $url = $('#crawl-form input[name=url]');
    var value = $url.val();
    if(value == '') {
      poi.msg('不能提交空url', 'danger');
      return false;
    }
    redirectAmazonToBrowserSite(value);
    // 填充协议前缀
    if(!(/^https?\:\/\//i).test(value)) {
      $url.val('http://' + value);
    }
  });
  
  function redirectAmazonToBrowserSite(url)
  {
    if((/^https?\:\/\/www\.amazon\.co\.jp\//i).test(url)) {
      document.location.href = url.replace(/^https?\:\/\/www\.amazon\.co\.jp\//i, poi.browser_host + '/amazon/');
    }
    return false;
  }
});
</script>
<script>
  $(function(){
    var searchUrl = getUrlParam('url');
    if(searchUrl){
      $("#crawl-form input[name='url']").val(searchUrl);
      $("#crawl-form button[type='submit']").click();
      //强制关闭首次进入客服动画
      $('body').css({
        overflow: ''
      });
      $('.omni-new-customer-service-cover').fadeOut();
    }
    //匹配查询参数
    function getUrlParam(name){
      var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
      var r = window.location.search.substr(1).match(reg);
      if(r!=null)return decodeURI(r[2]); return false;
    }
  })
