<script>
$(function(){
  var source_site_names = {"4":"tora","5":"cq","3":"surugaya","1":"amazon","6":"animate","7":"melon","10":"mandarake","12":"movic","99":"request","100":"self","11":"hmv","17":"amiami"};
  var fee_type_names = {"0":"商品费用","1":"日本国内运费","2":"国际运费","4":"商品补款","6":"运单补款","10":"代购手续费","11":"包装手续费","12":"国际结算手续费","20":"仓储保管费","30":"付款手续费","50":"意向金","51":"大数量商品订单","52":"预约代购手续费","53":"支付转账手续费","54":"折扣","55":"申请单商品费用","60":"其他","70":"拼团优惠","97":"优惠券抵扣","98":"活动优惠","99":"积分抵扣"};
  poi.item_list = {
    exchange: 0.0615,
    is_cart: 0,
    items: [],
    currency_unit: function () {
      if(poi.currency == poi.consts_origin['currency']['rmb']) {
        return '元';
      } else {
        return '円'
      }
    },
    index: function(){
      var i;
      for( i in poi.item_list.items ){
        if( poi.item_list.items[i] == this ) return i;
      }
      return -1;
    },
    check: function(index, value){
      poi.item_list.items[index].checked = value;
      poi.item_list.$apply();
    },
    check_all: function(value){
      var i;
      for( i in this.items ){
        if( this.items[i].product.in_stock == 0 || this.items[i].limit == 0 ) continue;
        this.items[i].checked = value;
      }
      poi.item_list.$apply();
    },
    buyable: function () {
      return this.product.in_stock != 0 && this.limit != 0;
    },
    in_stock: function() {
      return this.product.in_stock != 0;
    },
    update: function(index, amount){
      var item = poi.item_list.items[index];
      var i;
      var quotient = amount / item.amount;
      item.price = 0;
      for (i in item.fees) {
        item.fees[i].sum *= quotient;
        item.fees[i].rmb = Math.ceil(item.fees[i].sum * poi.exchange);
        if(poi.currency == poi.consts_origin['currency']['rmb']) {
          item.price += item.fees[i].rmb;
        } else {
          item.price += item.fees[i].sum;
        }
      }
      item.price = parseInt(item.price);
      item.amount = amount;
      poi.item_list.$apply();
    },
    remove: function(index){
      poi.item_list.items.splice(index, 1);
      poi.item_list.$apply();
    },
    all_checked: function(){
      var i;
      for( i in this.items ){
        if( !this.items[i].checked && this.items[i].product.in_stock != 0 && this.items[i].limit != 0 ) return false;
      }
      return true;
    },
    total_rmb: function(){
      var i, sum = 0;
      for( i in this.items ){
        if( !this.items[i].checked || this.items[i].product.in_stock == 0 ) continue;
        sum += this.items[i].price;
      }
      return sum;
    },
    source_site_class: function(){
      var i = this.product.source_site;
      return "source_site_" + source_site_names[i.toString()];
    },
    fee_type_name: function() {
      var i = this.fee_type;
      return fee_type_names[parseInt(i)];
    },
    formatted_fee: function(){
      return poi.formatFee(this);
    },
    formated_unit_price: function() {
      return poi.formatPrice(this.unit_price) + ' 円';
    },
    no_poundage: function() {
      var noPoundage = true;
      for(var i = 0; i < this.fees.length; ++i) {
        if(this.fees[i].fee_type == 10)
        {
          noPoundage = false;
        }
      }
      return noPoundage ? '【免手续费】' : '';
    }
  };
});
</script>

