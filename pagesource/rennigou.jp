<script>
	$(function(){
		if(poi.bulletins.stick && poi.bulletins.stick.length > 0) {
			poi.bulletins_is_read = localStorage['rng-notice'] == poi.bulletins.stick[0].update_time;
		} else {
			poi.bulletins_is_read = true;
		}
		$('.poipoi-global-notice .close').click(function(){
			$('.poipoi-global-notice').removeClass('show');
		});

		if(!poi.bulletins_is_read) {
			$('.omni-new-header .bulletins-case').append('<span class="point"></span>');
			// $('.omni-new-header .item.notifiy .badge').removeClass('hidden');
		}
		if(poi.bulletins.length == 0) {
			$('.omni-new-header li.bulletins').hide();
		}

		$('.omni-new-header .bulletins-case').on('click', function () {
			$('.header-box .notice').slideDown();
			$('.omni-new-header .bulletins-case .point').addClass('hidden');
			// $('.omni-new-header .item.notifiy .badge').addClass('hidden');
			if(poi.bulletins.stick.length > 0) {
				localStorage['rng-notice'] = poi.bulletins.stick[0].update_time;
			}
			dropdownHide();
			maskHide();
		});

		new Vue({
			el: '.header-box .notice',
			data: {
				bulletins: poi.bulletins && poi.bulletins.stick && poi.bulletins.stick.length > 0 ? poi.bulletins.stick : [{
					'bulletin_title': '',
					'update_time': '',
					'bulletin_content': '<div class="text-center">当前没有任何公告</div>',
				}],
				page: 0,
			},
			computed: {
				_notice:function() {
					var o = this.bulletins;
					this.bulletins.forEach(function(e) {
						e.bulletin_content = e.bulletin_content.replace(/([^"])(https?:\/\/[^\s/$.?#].[^\s]*)/ig, '$1 <a href="$2" target="_blank">$2</a>');
					}, this);
					return o;
				},
			},
			methods: {
				to: function (page) {
					if(this.page + page < 0) this.page = this.bulletins.length - 1;
					else if(this.page + page >= this.bulletins.length) this.page = 0;
					else this.page += page;
				},
				close: function () {
					$('.header-box .notice').slideUp();
				},
			}
		});

	});
</script>

