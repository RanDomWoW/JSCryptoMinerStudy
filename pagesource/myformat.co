function googleAdShowBlock(caPub, blockId, renderTo, styleWidthHeight, autoFormat) {
    if(!document.getElementById(renderTo)){
        return false;
    }
    if(styleWidthHeight){
        styleWidthHeight = 'display:inline-block;' + styleWidthHeight; 
    } else {
        styleWidthHeight = 'display:block;'; 
    }
    document.getElementById(renderTo).innerHTML = 
        '<ins class="adsbygoogle" style="' + styleWidthHeight + 
        '" data-ad-client="ca-pub-' + caPub + '" data-ad-slot="' + blockId + 
        '"  ' + autoFormat  + '></ins>';
	(adsbygoogle = window.adsbygoogle || []).push({}); 
}

function addGoogleScript() {
    var s = document.createElement("script");
    s.type = "text/javascript";
    s.src = "//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js";
    s.async = "async";
    jQuery('head').append(s);
}

function googlePageLevel(caPub) {
    (adsbygoogle = window.adsbygoogle || []).push({
        google_ad_client: "ca-pub-" + caPub,
        enable_page_level_ads: true
      });
}

function addManyChatBot(jsId, widgetId) {
