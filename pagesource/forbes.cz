    	showQuote = false;
	}
	if (window.location.hash == "#citat-dne") {
		showQuote = true;
	}

	function displayQuote() {
		$(document).ready(function() {
			$('body .container').hide();
		});
		$('body > div.citat').show();
		var link = 'http://www.forbes.cz/citat-dne/';
		$('body > div.citat').html(
			'<iframe id="zttifrm" src="'+link+'" style="height:100%; width:100%; border:none;"></iframe>'
		);
		//document.getElementById('zttifrm').src = link + '?ztt=tgs' + Math.random().toString(36).substring(2, 15) + Math.random().toString(36).substring(2, 15);
		window.location.hash = "citat-dne";
	}

	function closeQuote() {
		$('body > div.citat > iframe').remove();
		$(document).ready(function() {
			$('body .container').show();
		});
		$('body > div.citat').hide();
		var pushUrl = docCookies.getItem('forbes-day-link');
		if (showQuote) {
			this.window.history.replaceState(null, null, pushUrl);
		}
		ga('send', 'pageview', window.location.pathname + window.location.search);
	}

	</script>

