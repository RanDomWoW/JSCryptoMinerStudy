	var currentShown = 0;
	
	var hiddenBlocks = [];
	
	// Set currently shown image content.
	function setCurrentShown(which) {
		var imageContent, newsContent;
		if (which != currentShown) {
			// -1 is the default value for 'none'.
			if (currentShown!=-1) {
				imageContent = document.getElementById('imageContent'+currentShown);
				if (imageContent) {
					newsContent = document.getElementById('footerNews'+currentShown);
					if (newsContent)
						newsContent.parentNode.removeChild(newsContent);
					else {
						newsContent = hiddenBlocks['footerNews'+currentShown];
						hiddenBlocks['footerNews'+currentShown] = null;
					}
					imageContent.parentNode.insertBefore(newsContent, imageContent);
					imageContent.parentNode.removeChild(imageContent);
					hiddenBlocks['imageContent'+currentShown] = imageContent;
					newsContent.style.display = 'block';
				}
			}
			imageContent = document.getElementById('imageContent'+which);
			// if target image content doesn't exist in document, try to find in the hidden cache.
			if (!imageContent) {
				imageContent = hiddenBlocks['imageContent'+which];
				hiddenBlocks['imageContent'+which] = null;
			}
			if (imageContent) {
				currentShown = which;
				newsContent = document.getElementById('footerNews'+currentShown);
				newsContent.parentNode.insertBefore(imageContent, newsContent);
				newsContent.parentNode.removeChild(newsContent);
				hiddenBlocks['footerNews'+currentShown] = newsContent;
				imageContent.style.display = 'block';
			} else {
				// not found : set currentShown to -1.
				currentShown = -1;
			}
		}
	}
	function onMouseOverImage() {
		which = this.which;
//		setCurrentShown(which);
		if (isIE)
			document.getElementById('show'+which).style.display = 'inline';
	}
	function onMouseOutImage() {
		which = this.which;
		if (isIE)
			document.getElementById('show'+which).style.display = 'none';
	}
	function enableImageMouseOvers() {
		for (var i=0; i<3; i++) {
			document.getElementById('img'+i).which = i;
			document.getElementById('img'+i).onmouseover = onMouseOverImage;
			document.getElementById('img'+i).onmouseout = onMouseOutImage;
		}
	}
	function disableImageMouseOvers() {
		for (var i=0; i<3; i++) {
			document.getElementById('img'+i).onmouseover = null;
			document.getElementById('img'+i).onmouseout = null;
		}
	}

	/* Minimal Ajax Code */
	function createRequestObject() {
		var ro;
		var browser = navigator.appName;
		if(browser == "Microsoft Internet Explorer"){
			ro = new ActiveXObject("Microsoft.XMLHTTP");
		} else {
			ro = new XMLHttpRequest();
		}
		return ro;
	}
	var http = createRequestObject();
	var onDataReady;
	function onReadyStateChange() {
		if (http.readyState==4) {
			onDataReady(http.responseText);
		}
	}
	function queryServer(method, url, queryString, callback) {
		onDataReady = callback;
		http.open('post', url, true);
	  http.setRequestHeader('Content-Type','application/x-www-form-urlencoded');
		http.send(queryString);
		http.onreadystatechange = onReadyStateChange;
	}
	
	
	function reloadImages() {
		restoreImages();
		disableImageMouseOvers();
		currSeq = currSeq + 1;
		currStorySeq = currStorySeq + 1;
		queryServer('get', 'image_block.php?seq='+currSeq+'&sseq='+currStorySeq, '', function(responseText) {
			loadContents(responseText);
			enableImageMouseOvers();
			setCurrentShown(0);
		} );
	}
	
	function repeatlyRefresh() {
		reloadImages();
		setTimeout("repeatlyRefresh()",30000);
	}
	
	function restoreImages() {
		setCurrentShown(-1);
		for (var i in hiddenBlocks) {
			if (hiddenBlocks[i]) {
				hiddenBlocks[i].style.display = 'none';
				document.getElementById('hiddenArea').appendChild(hiddenBlocks[i]);
			}
		}
		// Does this cause memory leak? I hope javascript does GC.
		hiddenBlocks = new Array();
		return false;
	}
	

