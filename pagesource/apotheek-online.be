	function checkVat( country, taxvat )
	{
		if (taxvat.val().length <= 7) {
			return;
		}
		var parameters = {
			country : country,
			taxvat  : taxvat.val()
		};
		var url = 'https://www.apotheek-online.be/masetax/taxvat/checkVat/';
		jQuery.post(url, parameters, function( data ) {
			if (taxvat.data('qtip')) {
				taxvat.removeClass('valid-taxvat');
				taxvat.removeClass('invalid-taxvat');
				taxvat.qtip('destroy');
			}
			if (data.valid) {
				taxvat.addClass('valid-taxvat');
			} else {
				taxvat.addClass('invalid-taxvat');
			}
			if (jQuery('input[name="billing[vat_id]"]').is(':visible')) {
				taxvat.qtip({
					overwrite : true,
					content: {
						text: data.response
					},
					show : {
						ready : true,
						when : false
					},
					hide : 'keyup',
					position: {
						corner: {
							tooltip : 'leftMiddle', // Use the corner...
							target  : 'rightMiddle' // ...and opposite corner
						},
						adjust: {
							screen: true
						}
					},
					style: {
						width: 'auto',
						padding: 0,
						background: '#002a42',
						color: '#FFFFFF',
						textAlign: 'left',
						border: {
							width: 10,
							color: '#002A42'
						},
						tip: true, // Give it a speech bubble tip with automatic corner detection
						name: 'dark' // Inherit the rest of the attributes from the preset dark style
					}
				});
			}
			if (data.valid) {
				setTimeout(function() {
					taxvat.qtip('hide');
					taxvat.removeClass('valid-taxvat');
					taxvat.removeClass('invalid-taxvat');
				}, 5000);
			}
		}, 'json');
	}
