<script>
var timer = false;
var ip = 0;
var sw = 0;
var si = 0;
var ir = new Array();
var load_cnt = new Array();;

jQuery(function(){
  jQuery(window).bind('load resize',function(){

    if (timer !== false) { clearTimeout(timer); }

    sw = jQuery('#block-system-main').width();
    si = 'span.inline-img-assist';
    var i=0;

    timer = setTimeout(function() {
      jQuery(si).each(function(){
        var si_idx = jQuery('span.inline-img-assist').eq(i);
        var sii = jQuery('span.inline-img-assist img').eq(i);
        var img = new Image();
        img.src = sii.attr('src');

        if (load_cnt[i] != 1) {
          ir[i] = sii.width() / 730;
          load_cnt[i] = 1;
        }

        if (ir[i] > 1) { ir[i] = 1; }

        ip = sw * ir[i];

        jQuery(si_idx).width(ip);
        i++;
      });
    }, 200);

  });
});
</script>  <script src="https://www.cyclowired.jp/sites/default/files/js/js_FbpwIZNwgzwEuuL4Q2HOM07BOSCY5LxL_gwSK4ohQBM.js"></script>
