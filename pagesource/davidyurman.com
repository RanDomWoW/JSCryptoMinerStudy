<script type="text/javascript">
    var $jq = jQuery.noConflict();
	$jq(function() {
		mobileInit();
		var $nav = $jq("#nav-mobile");
		if (!isIE()) {
			console.log('ISIE');
			$nav.mmenu({
				zposition: "front",
				isMenu: false
			});
		}
		/*in mobile, search button click*/
		$jq(".mobile-search").on("click", function() {
			if ($jq(".mobile-search").is(':visible')) {
				$jq(this).hide();
				$jq(".header-search-bar-mobile").slideDown();
				$jq("#middle").css('padding-top', '110px');
			}
		});
		/*get children menu*/
		var getMenuChild = function() {
			console.log('getmenuchild');
			var currentPath = decodeURIComponent(window.location.pathname);
			var category = [];
			var isParentTitleClicked = $jq("div.menu-parent-title[data-target='" + currentPath + "']").closest('.menu-parent')[0];
			if (isParentTitleClicked != undefined) {
				//console.log("currentPath in getmenuchild if 1111 "+currentPath);
				category.push($jq("div.menu-parent-title[data-target='" + currentPath + "']").closest('.menu-parent')[0]);
				$jq("ul.menu-child-items li.menu-landing-pageLink[data-target^='" + currentPath + "']").addClass('highlight');
			} else {
				var categoryStr1, categoryStr2;
				categoryStr1 = $jq("ul.menu-child-items li[data-target^='" + currentPath + "']").closest('.menu-parent')[0];
				categoryStr2 = $jq("ul.menu-child-items li[data-target^='" + currentPath + "']").closest('.categoryLink')[0];
				if (categoryStr1) {
					category.push($jq("ul.menu-child-items li.menu-landing-pageLink[data-target^='" + currentPath + "']").closest('.menu-parent')[0]);
					$jq("ul.menu-child-items li.menu-landing-pageLink[data-target^='" + currentPath + "']").addClass('highlight');
				} else {
					category.push($jq("ul.menu-child-items li a[data-target^='" + currentPath + "']").closest('.menu-parent')[0]);
					$jq("ul.menu-child-items li a[data-target^='" + currentPath + "']").addClass('highlight');
					var dataActiveGroup = $jq("ul.menu-child-items li a[data-target^='" + currentPath + "']").closest('.menu-parent').attr("data-active-group");
					console.log("dataActiveGroup " + dataActiveGroup);
				}
				if (categoryStr2) {
					category.push($jq("ul.menu-child-items li.menu-landing-pageLink[data-target^='" + currentPath + "']").closest('.categoryLink')[0]);
					$jq("ul.menu-child-items li.menu-landing-pageLink[data-target^='" + currentPath + "']").addClass('highlight');
				} else {
					category.push($jq("ul.menu-child-items li a[data-target^='" + currentPath + "']").closest('.categoryLink')[0]);
					$jq("ul.menu-child-items li a[data-target^='" + currentPath + "']").addClass('highlight');
				}
			}
			return category;
		}
		var getOpenMenu = function() {
			return $jq.cookie("menuOpen");
		};
		var shouldMenuOpen = function() {
			return typeof(getOpenMenu()) != "undefined";
		};
		var getActiveGroup = function() {
			return $jq.cookie("activeGroupSelected");
		}
		var isActiveGroupSelected = function() {
			return typeof(getActiveGroup()) != "undefined";
		};
		if (shouldMenuOpen()) {
			console.log('shouldMenuOpen')
			/* - RK Hamberger Changes */
			//$jq("a#menu-toggle").trigger("click"); // triggers mmenu on page load when menuOpen cookie is present - which is for category landing pages
			var parentCategory = getMenuChild();
			$jq(parentCategory[0]).toggleClass('menu-closed menu-open');
			if (isActiveGroupSelected()) {
				var dataActiveGroup = getActiveGroup();
				console.log("dataActiveGroup " + dataActiveGroup);
				if (dataActiveGroup && dataActiveGroup != 'category') {
					$jq(parentCategory[0]).attr("data-active-group", dataActiveGroup);
				}
				$jq.removeCookie('activeGroupSelected', {
					path: '/'
				});
			}
			if (parentCategory[1]) {
				$jq(parentCategory[1]).toggleClass('openSubCategory');
			}
			$jq.removeCookie('menuOpen', {
				path: '/'
			});
		}
		/* CHAT WITH US TEXT CHANGE FOR THE CHATCONTAINER ELEMENT ON PAGE LOAD START */
		$nav.on('opening.mm', function() {
			console.log("opening mmenu ");
			var chatLink = $nav.find("#lpButtonDiv-customerservice").children("div");
			if (chatLink && chatLink.length !== 0) {
				console.log(chatLink.text());
				var chatTxt = '';
				chatTxt = chatLink.text();
				if (chatTxt == 'CHAT AVAILABLE') {
					chatLink.text('CHAT WITH US');
				}
				//chatLink.css('color', 'rgba(255,255,255,0.5)');
			}
			//$jq('#nav-mobile').scrollTop($jq(".MenCont").offset().top);
		});
		/* ENabling Contact Customercare service section - Hamburger changes */
		var hostname = '';
		hostname = $jq('html').attr("lang");
		hostname = hostname.toLowerCase();
		if (hostname == 'en') {
			$jq('ul.menu-contactus').removeClass("hide");
		};
		// clicking event for Category landing page link
		$nav.on('click', '.menu-landing-pageLink', function(e) { 
			e.preventDefault();
			e.stopPropagation();
			var target = $jq(this).data("target");
			var categoryName = $jq(this).text().replace(/ /g, "");
			$jq.cookie("menuOpen", categoryName, {
				path: '/'
			});
			window.location.href = decodeURIComponent(target);
		});
		$nav.on('click', '.menu-parent-title', function(e) {
			e.preventDefault();
			e.stopPropagation();
			var $parent = $jq(this).closest('.menu-parent');
			// filtering for opened panel to close before opening a new panel - RK Hamberger Changes
			$parent.addClass("menu-current");
			$jq('.menu-parent').filter(function() {
				if ($jq(this).hasClass("menu-open") && !$jq(this).hasClass('menu-current')) {
					$jq(this).toggleClass('menu-closed menu-open');
				}
				$jq(this).children().removeClass("menu-highlight");
			});
			$parent.toggleClass('menu-closed menu-open');
			$parent.removeClass('menu-current'); // removing current class
			console.log("scrolltop: " + $jq(this).offset().top);
			//$jq('.menu-panel').scrollTop($jq(this).offset().top); //scrolling up to expanded top section for L1 categories
			if ($jq(this).parent().hasClass("menu-open")) {
				$jq(this).addClass("menu-highlight");
			}
			//scrolling up to expanded top section for L1 categories
			$jq('.menu-panel').animate({
				scrollTop: $jq(this).offset().top
			}, 'slow');
		});
		$nav.on('click', '.menu-child-items .menu-child-link', function(e) {
			e.preventDefault();
			e.stopPropagation();
			//var $parent = $jq(this).closest('.categoryLink');
			var $parent = $jq(this);
			// filtering for opened subcategory panel to close before opening a new subcategory panel - RK Hamberger Changes START
			$parent.addClass("menu-currentSubCategory");
			$jq('.menu-parent.menu-open li.categoryLink').filter(function() {
				if ($jq(this).hasClass("openSubCategory") && !$jq(this).hasClass('menu-currentSubCategory')) {
					$jq(this).toggleClass('openSubCategory');
					//console.log("closing opened subcategory ");
				}
			});
			$parent.removeClass('menu-currentSubCategory');
			$parent.toggleClass('openSubCategory');
		});
		$nav.on('click', '.categoryLink ul li a.cppLink, .menu-child-items li a.cppLink', function(e) { // clicking event for Category Product page link
			e.preventDefault();
			e.stopPropagation();
			var target = $jq(this).attr("data-target");
			var categoryName = $jq(this).text().replace(/ /g, "");
			$jq.cookie("menuOpen", categoryName, {
				path: '/'
			});
			var activeGroupStr = $jq(".menu-parent.menu-open").attr("data-active-group");
			$jq.cookie("activeGroupSelected", activeGroupStr, {
				path: '/'
			});
			window.location.href = decodeURIComponent(target);
		});
		$nav.on('click', '.menu-child-groups li', function() {
			var $link = $jq(this),
				$parent = $link.closest('.menu-parent'),
				groupName = $link.data('open');
			$parent.attr('data-active-group', groupName);
		});
	});

	function closeMenu(activeMenu) {
		if (activeMenu.length == 1) {
			var navDrop = activeMenu.find('.nav-drop');
			menuHide(activeMenu, navDrop);
		}
	}

	function menuShow($container, $drop) {
		$container.data('state', 'open');
		if (Evo.isMobileMenu()) {
			$drop.fadeIn('slow');
		} else {
			$drop.show();
		}
		$container.addClass('active');
	}

	function menuHide($container, $drop) {
		$container.data('state', 'closed');
		if (Evo.isMobileMenu()) {
			$drop.fadeOut('slow');
		} else {
			$drop.hide();
		}
		$container.removeClass('active');
	};

	function mobileInit() {
		/* Make touch events faster */
		FastClick.attach(document.body);
	}

	function isIE(userAgent) {
		userAgent = userAgent || navigator.userAgent;
		return userAgent.indexOf("MSIE ") > -1 || userAgent.indexOf("Trident/") > -1 || userAgent.indexOf("Edge/") > -1;
	}
