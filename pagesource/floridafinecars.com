			$(document).on("click", "#b-items__cars-one-img-check, label.label-list", clickToAddCompare);
			$(document).on("click", "label.label-grid, label.label-gridtwo", clickGridToAddCompare);
			$(document).on("click", ".compareitemdelete", clickCompareItemDelete);
			$(document).on('click', "label.checkboxreadytocompare", clickReadyToCompare);
