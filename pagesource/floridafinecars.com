		function loadYoutubeReviewsVideos() {
			if (!is.undefined($('#youtubereviewsvideos').html())) {
				var youtubelist = ['/images/youtubereviews/chevysilverado.jpg', '/images/youtubereviews/goldenoportunity.jpg', '/images/youtubereviews/ram.jpg', '/images/youtubereviews/fordfocus.jpg', '/images/youtubereviews/nissanaltima.jpg', '/images/youtubereviews/chevytrax.jpg'];
				var fancyboxes = $('#youtubereviewsvideos a.fancytube img');
				for (var i = 0; i < fancyboxes.length; i++) {
					var currentFancyBox = fancyboxes[i];
					$(currentFancyBox).attr('src', youtubelist[i]);
				}
			}
		}

		function clickReadyToCompare() {
			setTimeout(function () {
				var checkedtocompare = $('input.checkboxreadytocompare:checked').length;

				if (checkedtocompare <= 1)
					$('.compareselected').addClass('disabled').prop('disabled', 'disabled');
				else
					$('.compareselected').removeClass('disabled').removeAttr('disabled');
			}, 10);
		}

		/*related with compare function*/
		function clickCompareItemDelete() {
			var target = $(this);
			var sn = target.parents('li').data().stocknumber;
			deleteToCompareList(sn);
		}

		function updateCheckboxFromCompare() {
			if (JSON.parse(localStorage.getItem('stocklistcompare')) === null) {
				var itemschecked = $('form.formcheckboxcompare input:checkbox:checked');
				for (var i = 0; i < itemschecked.length; i++) {
					$(itemschecked[i]).prop('checked', false);
					$('#checkgrid' + $(itemschecked[i]).data().stocknumber).prop('checked', false);
					$('#checkdetail' + $(itemschecked[i]).data().stocknumber).prop('checked', false);
				}
			}

			var liststocks = JSON.parse(localStorage.getItem('stocklistcompare'));

			if (liststocks == null)
				return false;

			if (liststocks.length == 0)
				return false;

			for (var i = 0; i < liststocks.length; i++) {
				var current = liststocks[i];
				$("#checklist" + current.StockNumber).prop('checked', true);
				$('#checkgrid' + current.StockNumber).prop('checked', true);
				$("#checkdetail" + current.StockNumber).prop('checked', true);
			}
		}

		function clickGridToAddCompare() {
			var target = $(this);
			var sn = target.data().stocknumber;
			$(String.format('label.label-list[for="checklist{0}"]', sn)).click();
		}

		function clickToAddCompare() {
			var target = $(this);

			setTimeout(function () { addToCompareList(target, target.data().name) }, 10);
		}

		function addToCompareList(target, type) {

			var action = $("#" + target.prop('for')).is(':checked');
			var stocknumber = target.data().stocknumber;
			if (action == false) {
				deleteToCompareList(stocknumber);
				return;
			}

			var stockexist = verifyIsAddedToCompare(stocknumber);

			if (stockexist)
				return;

			var thumbnaildata = '';
			var titledata = '';
			var pricedata = '';
			var linkdata = '';
			var imagemaindata = '';

			if (type == 'detail') {
				$('#checkdetail' + stocknumber).prop('checked', true);
				thumbnaildata = $('#vehiclethumbnaildata').html();
				imagemaindata = $('#imagemaindata').html();

				var imageInTable = "";

				if (!is.null(imagemaindata)) {
					var imagedir = imagemaindata.substring(imagemaindata.lastIndexOf('/') + 1);
					var imagesplit = imagedir.split('.');

					if (imagesplit[0].length > 2) {
						imageInTable = imagemaindata.replace(imagedir, String.format("{0}-250x180.{1}", imagesplit[0], imagesplit[1]));
					}
				}
				else {
					imageInTable = '/images/intransit.png';
				}

				thumbnaildata = imageInTable.includes('https')? imageInTable : imageInTable.replace("http", "https");

				titledata = $('.vehicletitledata').html();
				pricedata = $('.vehiclepricedata').html();
				linkdata = document.location.pathname;
			}
			else {
				$('#checkgrid' + stocknumber).prop('checked', true);
				var currentItem = $(target.parents('div.b-items__cell'));

				thumbnaildata = currentItem.find('img.img-responsive').attr('src');
				titledata = currentItem.find('.b-items__cell-info-title h2 a').html();
				pricedata = currentItem.find('.b-items__cell-info-price').html();
				linkdata = currentItem.find('.b-items__cars-one-img a').attr('href');
			}

			appendToCompareList(stocknumber,
								thumbnaildata,
								titledata,
								pricedata,
								linkdata
								);

			/*adding to localstorage*/
			if (JSON.parse(localStorage.getItem('stocklistcompare')) === null) {
				var arraylist = [];

				arraylist.push(
				{
					StockNumber: stocknumber,
					ImageLink: thumbnaildata,
					Description: titledata,
					Price: pricedata,
					Url: linkdata
				});

				localStorage.setItem('stocklistcompare', JSON.stringify(arraylist));

				enableBtnCompare(arraylist.length);
			}
			else {
				var currentlist = JSON.parse(localStorage.getItem('stocklistcompare'));

				currentlist.push(
				{
					StockNumber: stocknumber,
					ImageLink: thumbnaildata,
					Description: titledata,
					Price: pricedata,
					Url: linkdata
				});

				localStorage.setItem('stocklistcompare', JSON.stringify(currentlist));

				enableBtnCompare(currentlist.length);
			}

			ga('send', 'pageview', String.format('/vehiclesaved?qs={0}&category=vehSaved', titledata));
		}

		function appendToCompareList(sn, src, desc, price, url) {

			var templatecompareli = '<li class="col-md-12 col-xs-12 no-padding compareitemcheck{3}" data-stocknumber="{3}">\
					<div class="col-md-1 col-xs-1 no-padding ">\
						<div class="btn-group checkboxreadycompare" data-toggle="buttons">\
							<label class="btn btn-default btn-checkbox checkboxreadytocompare active">\
								<input type="checkbox" checked="checked" class="checkboxreadytocompare" id="checkboxreadycompares{3}">\
								<span class="fa fa-check"></span>\
							</label>\
						</div>\
					</div>\
					<div class="col-md-3 col-xs-3 no-padding text-center"><img class="compareitemimage" src="{0}" height=40 width=50 alt="{0}"/></div>\
					<div class="col-md-7 col-xs-7 no-padding"><span class="compareitemdescription"><a href="{4}">{1}</a></span><span class="compareitemprice">{2}</span></div>\
					<div class="col-md-1 col-xs-1 no-padding compareitemdelete"><i class="fa fa-close"></i></div>\
				</li>';

			var templateli = String.format(templatecompareli, src, desc, price, sn, url);

			if ($('.contentcomparesection ul li:first').length == 0)
				$('.contentcomparesection ul').append(templateli);
			else
				$('.contentcomparesection ul li:first').before(templateli);
		}

		function deleteToCompareList(sn) {
			var found = false;
			var currentList = JSON.parse(localStorage.getItem('stocklistcompare'));
			for (var i = 0; i < currentList.length; i++) {
				if (currentList[i].StockNumber == sn) {
					currentList.splice(i, 1);
					found = true;
					$(String.format('.compareitemcheck{0}', sn)).remove();
					$("#checklist" + sn).prop('checked', false);
					$('#checkgrid' + sn).prop('checked', false);
					$("#checkdetail" + sn).prop('checked', false);
					break;
				}
			}

			if (found) {
				localStorage.setItem('stocklistcompare', JSON.stringify(currentList));
				enableBtnCompare(currentList.length);
			}
		}

		function verifyIsAddedToCompare(stocknumber) {
			if (JSON.parse(localStorage.getItem('stocklistcompare')) === null)
				return false;

			var liststocks = JSON.parse(localStorage.getItem('stocklistcompare'));

			if (liststocks == null)
				return false;

			if (liststocks.length == 0)
				return false;

			for (var i = 0; i < liststocks.length; i++) {
				var current = liststocks[i];
				if (current.StockNumber == stocknumber) {
					return true;
					break;
				}
			}

			return false;
		}

		function enableBtnCompare(total) {
			if (total > 0) {
				$('#compare-btn span.w3-badge').html(total);
				$('#compare-btn span.labeltitle').html("Saved");
				$('div.titlecompare').html(String.format("{0} Vehicles saved", total));

				if ($('#compare-btn').hasClass('hidden') && !($('div.comparesonlist').hasClass('comparesonlist-show'))) {
					$('#compare-btn').removeClass('hidden');
				}
			}
			else {
				$('#compare-btn span.w3-badge').html('');
				$('#compare-btn span.labeltitle').html('')
				$('div.titlecompare').html('');
				$('#compare-btn').addClass('hidden');
				$('div.comparesonlist').removeClass('comparesonlist-show');
			}
		}

		function loadCompareVehicles() {
			if (JSON.parse(localStorage.getItem('stocklistcompare')) === null)
				return false;

			var liststocks = JSON.parse(localStorage.getItem('stocklistcompare'));

			if (liststocks.length == 0)
				return false;

			enableBtnCompare(liststocks.length);

			for (var i = 0; i < liststocks.length; i++) {
				var current = liststocks[i];
				appendToCompareList(current.StockNumber, current.ImageLink, current.Description, current.Price, current.Url);
			}
		}

		/*end to relate with compare feature*/
	</script>
