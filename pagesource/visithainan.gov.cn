	if(cs2>=gs2)
	{cs2=0}
	 var left=$(".win2>li").eq(cs2).position().left;		 
     $(".win2").animate({left:-left});
	 $(".t22 div").removeClass("on");
	 $(".t22 div:eq(" + cs2 + ")").addClass("on");
	 cs2=cs2+1;
	
}; 
var MyMar=setInterval(goto2, 5000);//每秒执行一次goto 


$(".t22 div").mouseover(function() {
    clearInterval(MyMar);//停止	
                    });
$(".t22 div").mouseout(function() {
    MyMar=setInterval(goto2, 5000);//开始
                    });
					
$(".t22 div").each(function(index){

$(this).hover(
  function () {
   $(this).addClass("on");
    
   
  },
  function () {   
	$(".t22 div").removeClass("on");
	$(".t22 div:eq(" + cs2 + ")").addClass("on");
      });  
	  
    $(this).click(function(){	
	 cs2=index;
	 var lef2=$(".win2>li").eq(cs2).position().left;
		 
     $(".win2").animate({left:-lef2});
	 $(".t22 div").removeClass("on");
	 $(this).addClass("on");	 
	});
   
  });



});

</script>

