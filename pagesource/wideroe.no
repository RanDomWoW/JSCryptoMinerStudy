            }

            h4 {
              font-size: 14px;
              font-weight: bold;
              line-height: 20px;
              margin: 0;
            }

            h5 {
              font-size: 12px;
              font-weight: bold;
              line-height: 20px;
              margin: 0 0 0 0;
            }

            h6 {
              font-size: 11px;
              font-weight: bold;
              line-height: 20px;
              margin: 0 0 20px 0;
            }

            /* BODYTEXT */
            p,
            address {
              margin: 0 0 20px 0;
            }

            address {
              font-size: 12px;
            }
            address strong {
              font-size: 14px;
            }

            * strong,
            strong {
              font-weight: bold;
            }

            * em,
            em {
              font-style: italic;
            }

           
            /* CONTEXT */
            #context {
              color: #cccccc;
            }
            #context a {
              color: #8dc73f;
              color: #b9e5fb;
            }
            #context a:hover {
