    if (userAgent.indexOf("compatible") > -1 && userAgent.indexOf("MSIE") > -1 && !isOpera) {
        return "IE";
    }; //判断是否IE浏览器
}
var mb = myBrowser();


if ("Safari" == mb) {
   $('#videostart img').removeClass('bbbimg');
}
</script>

