    <script>
        // 自定义滚动 project-scroll
        (function ($) {
            $(window).on("load", function () {

                $.mCustomScrollbar.defaults.scrollButtons.enable = true; //enable scrolling buttons by default
                $.mCustomScrollbar.defaults.axis = "yx"; //enable 2 axis scrollbars by default

                $(".project-scroll").mCustomScrollbar({ theme: "light-thin" });

            });

            // 点击展开，去掉滚动条
            $(".arrival-guide-btn").on("click", function () {
                if (!$(this).hasClass("active")) {
                    $(".project-scroll").mCustomScrollbar("destroy");//销毁
                } else {
                    $(".project-scroll").mCustomScrollbar();//更新
                }
                $(this).toggleClass("active");
                $(".arrival-guide-content").toggleClass("active");
            });

        })(jQuery);
		</script>
    
