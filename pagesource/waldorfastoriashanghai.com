    <script type="text/javascript">
        var begindate, endDate;
        $(function () {

            //初始化 日期选择控件
            $('.inp').empty().datetimepicker({
                lang: 'ch',
                timepicker: false,
                format: 'Y-m-d',
                validateOnBlur: false,
                minDate: null
            });

            begindate = $('#beginDate_d').empty().datetimepicker({
                lang: 'ch',
                timepicker: false,
                format: 'Y-m-d',
                validateOnBlur: false,
                minDate: null,
                onSelectDate: function () {
                    $('#beginDate').val(begindate.val());
                }
            });

            endDate = $('#endDate_d').empty().datetimepicker({
                lang: 'ch',
                timepicker: false,
                format: 'Y-m-d',
                validateOnBlur: false,
                minDate: null,
                onSelectDate: function () {
                    $('#endDate').val(endDate.val());
                }
            });


            //begindate=$('#beginDate').empty().datetimepicker({
            //    lang: 'ch',
            //    timepicker: false,
            //    format: 'Y-m-d',
            //    validateOnBlur: false,
            //    minDate: null,
            //    onSelectDate: function () {
            //        $('#beginDate').prev('input.inp').val(begindate.val());
            //    }
            //});

            //endDate=$('#endDate').empty().datetimepicker({
            //    lang: 'ch',
            //    timepicker: false,
            //    format: 'Y-m-d',
            //    validateOnBlur: false,
            //    minDate: null,
            //    onSelectDate: function () {
            //        $('#endDate').prev('input.inp').val(endDate.val());
            //    }
            //});


            //绑定预定按钮
            $('#btnBooking').bind('click', function () {
                var bookingUrl = $('#btnBooking').attr('data-url'); //预定地址

                if ($("#beginDate").val() != "" && $("#endDate").val() != "") {
                    var sDate = new Date($("#beginDate").val().replace("-", "/").replace("-", "/"));
                    var eDate = new Date($("#endDate").val().replace("-", "/").replace("-", "/"));

                    if (checkDate(sDate, eDate)) {
                        bookingUrl += "&inputModule=HOTEL_SEARCH" + "&arrivalDay=" + sDate.getDate() + "&arrivalMonth=" + (sDate.getMonth() + 1) + "&arrivalYear=" + sDate.getFullYear() + "&departureDay=" + eDate.getDate() + "&departureMonth=" + (eDate.getMonth() + 1) + "&departureYear=" + eDate.getFullYear() + "&redeemPts=false&promo_code=";

                        window.location.href = bookingUrl;
                    }
                    else {
                        alert("离开日期须大于开始日期，请重新选择日期!");
                        return;
                    }

                }
                else {
                    alert("入住日期或离开日期不能为空，请选择日期!");
                    return;
                }


            });
        });

        //比较日期大小
        function checkDate(sDate, eDate) {
            if (eDate > sDate) {
                return true;
            }
            return false;
        }

        

    </script>

    

