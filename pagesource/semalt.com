<script>
(function() {
	'use strict';
	
	var $win = $(window);
	
	$(document).on('size_fixed', function() {
		var $panel = $('#hsmap');
		var $overlay = $panel.find('.hsmap_black');
		var $overlayInner = $overlay.find('div');
		var $allItems = $('.hsmap_item');
		var panelHeight = $panel.height();
		var invertZones = [];
		var screenTops = [];
		var startTop = $('#screen_autoseo').offset().top;
		var isPanelShown = false;
		
		calcZones();
		
		$('.hsmap_item')
			.mouseover(function() {
				var itemNum = $(this).attr('data-item');
				$('.hsmap_item[data-item="' + itemNum + '"]').addClass('hover');
			})
			.mouseout(function() {
				var itemNum = $(this).attr('data-item');
				$('.hsmap_item[data-item="' + itemNum + '"]').removeClass('hover');
			})
			.click(function() {
				var screen = $(this).attr('data-screen');
				var scrollTop = $('#screen_' + screen).offset().top;
				$('html, body').stop().animate({
					scrollTop: scrollTop
				}, 700, 'easeInOutCirc');
			});
		
		$win.scroll(tryChange);
		$win.resize(function() {
			calcZones();
			tryChange();
		});
		
		function tryChange() {
			var scrollTop = $win.scrollTop();
			if (scrollTop + panelHeight >= startTop) {
				if (!isPanelShown) {
					$panel.stop().fadeIn();
					isPanelShown = true;
				}
			} else {
				if (isPanelShown) {
					$panel.stop().fadeOut();
					isPanelShown = false;
				}
			}
			
			var panelTop = $panel.offset().top;
			var found = false;
			$.each(invertZones, function(i, edges) {
				if (panelTop + panelHeight > edges.top && panelTop < edges.bottom) {
					var top = panelTop + panelHeight - edges.top;
					var offset = panelHeight - top;
					var height = edges.bottom - panelTop;
					$overlay.css({
						marginTop: offset
					});
					$overlayInner.css({
						marginTop: -offset,
						height: height
					});
					found = true;
					return false;
				}
			});
			$overlay.toggle(found);
			
			for (var i = screenTops.length - 1; i >= 0; i--) {
				if (scrollTop >= screenTops[i]) {
					$allItems.removeClass('current').filter('.hsitem' + (i + 1)).addClass('current');
					break;
				}
			}
		}
		
		function calcZones() {
			var SCREEN_TOP_OFFSET = 400;
			screenTops = [];
			$overlay.find('.hsmap_item').each(function() {
				var screen = $(this).attr('data-screen');
				var top = $('#screen_' + screen).offset().top - SCREEN_TOP_OFFSET;
				screenTops.push(top);
			});
			var lastTop = $('#aguency_guard').offset().top - SCREEN_TOP_OFFSET;
			screenTops.push(lastTop);
			
			
			invertZones = [];
			$('.home_pack_append, #aguency_guard, #footer2015').each(function() {
				var $area = $(this);
				var top = $area.offset().top;
				var bottom = top + $area.outerHeight(true);
				invertZones.push({
					top: top,
					bottom: bottom
				});
			});
		}
	});
})();
</script>

