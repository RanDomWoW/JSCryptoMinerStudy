    $('.header-semalt-main').delegate('#envelope','click',function(){
        $('.popup', '.popup-holder').hide();
        if($('.nfblock').is(":visible") == true) {
            $('.nfblock').fadeOut(200);
            $('.show_all_cat').hide();
            return;
        }
        $.ajax({
            url: 'https://semalt.com/ajax/notification/get_notification.php',
            xhrFields: { withCredentials: true},
            headers: {'X-Requested-With': 'XMLHttpRequest'},
            type: 'POST',
            cache: false,
            data: {'q':'notification_unreed', 'id_categ':1 },
            success: function(data) {
                $('.nfblock').fadeIn(200);
                data = data.split('__11__',3);
                if(data[1]=='notification_unreed'){
                    $('.nfblock_main').html(data[0]);
                    add_scroll3();
                    $('.nfblock .nfblock1 .nfremove').addClass('nfnone');
                }
                notice = $.parseJSON(data[2]);
            }
        });
    });
    function add_scroll3(){
        if($('.nfblock1').length > 3){
            $('.nfblock_main').addClass('scrollable-area');
            jcf.customForms.replaceAll();
        }
        else {
            if($('.nfblock1').length == 3){
                var hh = $('.nfblock1').eq(0).height()+$('.nfblock1').eq(1).height()+$('.nfblock1').eq(2).height()+6;
                if(hh>270){
                    $('.nfblock_main').addClass('scrollable-area');
                    jcf.customForms.replaceAll();
                    return;
                }
            }
            $('.nfblock_main').css('width', 360).removeClass('scrollable-area').next('.vscrollbar').hide(); //.unwrap();
        }
    }
    function fixedHeader () {
        var header = $('.header-semalt-main');
        var headerContacts = $('.header-contacts');
        var button_type = $('.product-button a').attr('data-act-button');

        if ($(window).scrollTop() > 80) {
            header.addClass('fixed');
            headerContacts.addClass('fixed');
            $('.buttonLogin').hide();
            $('.getReportButton').show();


        }
        else {
            header.removeClass('fixed');
            headerContacts.removeClass('fixed');
            $('.buttonLogin').show();
            $('.getReportButton').hide();
        }
    }


    $(document).ready(function(){
                    fixedHeader();
            $(document).scroll(function(){
                fixedHeader();
                act_view();
            });
            if ($(window).scrollTop() > 80) {
                act_view();
            }
        
        $(document.body).delegate('.product-button a', 'click', act_click);

        var viewed = false;
        var clicked = false;

        function act_view() {
            if(viewed === true) return false;
            var button_type = $('.product-button a').attr('data-act-button');
            ActProductButton.log('view',button_type);
            viewed = true;
        }

        function act_click() {
            if(clicked === true) return false;
            var button_type = $('.product-button a').attr('data-act-button');
            ActProductButton.log('click',button_type);
            clicked = true
        }
    });



