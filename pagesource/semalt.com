function bindDiscoverEvents2() {
    var url = $('#site_start_wow').val();
    if(url.length == 0) {
        $('.site_error').text( ll.i3033 ).slideDown();
        $('#site_start_wow').addClass('borderRed');
        return;
    }
    if(!ValidUrl(url)) { // check if string is url
        $('.site_error').text(ll.i3036 ).slideDown();
        $('#site_start_wow').addClass('borderRed');
        return;
    }
    $('.site_error').slideUp();
    $('#site_start_wow').removeClass('borderRed');

    document.location.href = 'http://semalt.com/?s='+url;
    /*else { document.location.href = 'http://semalt.com/addproject/?url='+url;}*/

    function ValidUrl(str) {
        var pattern = new RegExp('^(https?:\\/\\/)?'+ // protocol
            '((([a-z\\d]([a-z\\d-]*[a-z\\d])*)\\.)+[a-z]{2,}|'+ // domain name
            '((\\d{1,3}\\.){3}\\d{1,3}))'+ // OR ip (v4) address
            '(\\:\\d+)?(\\/[-a-z\\d%_.~+]*)*'+ // port and path
            '(\\?[;&a-z\\d%_.~+=-]*)?'+ // query string
            '(\\#[-a-z\\d_]*)?$','i'); // fragment locator
        if(!pattern.test(str)) {
            return false;
        } else {
            return true;
        }
    }

}
