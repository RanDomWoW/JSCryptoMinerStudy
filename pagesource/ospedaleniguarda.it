              <script type="text/javascript">
                /* <![CDATA[ */
                function resizeHeader()
                {
                  var altezzaheader = $(window).outerHeight();
                  var altezzavideo = Math.round($(window).outerWidth()*9/16);
                  if (altezzaheader > altezzavideo) {
                    var altezzaheader1 = altezzavideo;
                    $("header").css({"min-height": "700px", "height": altezzaheader1});
                  } else {
                    $("header").css({"min-height": altezzaheader, "height": "auto"});
                  }
                  $("#showcase").css({"top": "0", "width": $(window).outerWidth(), "margin-left": "0"});
                  $("#showcase iframe").css("top", ($(window).outerHeight() - ($(window).outerWidth()*9/16))/2);
                  $("#showcase .copertina").css("height", $(window).outerWidth()*0.5625);
                  if ($("header").outerHeight() <= 700) {
                    // devo fissare altezza minima
                    $("header").css({"min-height": "700px", "height": "700px"});
                    if ($(window).outerWidth()/$(window).outerHeight() < 16/9) {
                      // verticale
                      $("#showcase").css({"top": "0", "width": $(window).outerHeight()*16/9, "margin-left": -($(window).outerHeight()*16/9 - $(window).outerWidth())/2});
                      $("#showcase iframe").css("top", "0");
                      $("#showcase .copertina").css("top", "0");
                    } else {
                      // orizzontale
                      $("#showcase").css({"top": "0", "width": $(window).outerWidth(), "margin-left": "0"});
                      $("#showcase iframe").css("top", ($(window).outerHeight() - ($(window).outerWidth()*9/16))/2);
                      $("#showcase .copertina").css("top", ($(window).outerHeight() - ($(window).outerWidth()*9/16))/2);
                    }
                  }
                }
                $(document).ready(function(){
                  resizeHeader();
                });
                $(window).load(function(){
                  resizeHeader();
                  setTimeout(function(){
                    $('#cookie-policy').fadeOut(800);
                  }, 6000);
                });
                $(window).resize(function(){
                  resizeHeader();
                });

                var iframe = document.querySelector('#showcase iframe');
                var player = new Vimeo.Player(iframe);

                player.on('play', function() {
                    console.log('played the video!');
                });

                var partito = 0;
                player.on('progress', function(data) {
                  if (partito == 0) {
                    console.log('playing now the video! ' + data.seconds);
                    if (data.seconds >= 0.5) {
                      console.log('progress ' + data.seconds);
                      player.on('timeupdate', function(data1) {
                        if (data1.seconds >= 0.5) {
                          $('#showcase .copertina').css('opacity', 0);
                          partito = 1;
                          console.log('timeupdate ' + data1.seconds);
                          player.off('timeupdate');
                          player.off('progress', onPlay);
                        }
                      });
                    }
                  }
                });

                player.getVideoTitle().then(function(title) {
                    console.log('title:', title);
                });
                /* ]]> */
              </script>
            
