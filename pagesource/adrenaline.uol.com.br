		SELECT 
		mod_sys_arquivo.id,
		mod_sys_arquivo.id_referencia,
		mod_sys_arquivo.titulo_seo,
		mod_sys_arquivo.url_amigavel,
		mod_sys_arquivo.texto_seo,
		mod_sys_arquivo.idsysusu,
		mod_sys_arquivo.destaque,
		mod_sys_arquivo.cadastrado_por,
		mod_sys_arquivo.autor_alternativo,
		mod_sys_arquivo.local,
		mod_sys_arquivo.numeroUnico,
		mod_sys_arquivo.nome,
		mod_sys_arquivo.texto,
		mod_sys_arquivo.lista_secao,
		mod_sys_arquivo.view_total,
		mod_sys_arquivo.data_publicacao,
		mod_sys_arquivo.stat
		 , mod_analise.produto_recomendado AS analise_produto_recomendado  , mod_analise.nota AS analise_nota  , mod_analise.nome AS analise_nome  , mod_analise.produto AS analise_produto  , mod_produto.id AS produto_id  , mod_produto.numeroUnico AS produto_numeroUnico  , mod_produto.nome AS produto_nome  , mod_produto.lista_secao AS produto_lista_secao  , mod_produto.tipo_de_item AS produto_tipo_de_item  , mod_tipo_de_item_categoria.nome as tipo_de_item_categoria_nome FROM sys_arquivo AS mod_sys_arquivo LEFT JOIN analise AS mod_analise ON ( mod_analise.id = mod_sys_arquivo.id_referencia AND mod_sys_arquivo.local = 'analise') LEFT JOIN produto AS mod_produto ON ( mod_produto.id = mod_analise.produto ) LEFT JOIN tipo_de_item_categoria AS mod_tipo_de_item_categoria ON (mod_tipo_de_item_categoria.id = mod_produto.tipo_de_item) 
																		WHERE 
																			mod_sys_arquivo.stat='1' AND 
																			mod_sys_arquivo.exclusivo_bc='0' AND 
																			mod_sys_arquivo.local NOT IN ('produto','chamada','links_adrenaline','perguntas_frequentes','sit_galerias') AND 
																			mod_sys_arquivo.data_publicacao BETWEEN '2018-03-14 00:00:00' AND '2018-03-18 00:03:30' 
																		ORDER BY 
