<script>

    //-- Init
    function defer(method) { if (window.jQuery) method(); else setTimeout(function () { defer(method); }, 50); }
    if (typeof defer == 'function') { defer(function () { homepageInit(); }); }
    //--

    var player;
    var animationTimeout;
    var firstRun = true;
    var youtubeVideoId = "";

    function homepageInit() {

        $(function () {

            if (firstRun) {
                //console.log("=== Ratiolizer First Run ===");
                $('.image-overlay').fadeIn(400);
                ratiolizer();
                window.firstRun = false;
            }

            if ($('#backgroundPlayer').length && !$.TMNLkit.CurrentBreakpoint.isMobile()) {
                //console.log("=== Found Video Player ID ===");
                //console.log("=== Desktop Device Detected ===");
                importYoutubeApi();
            } else {
                if ($.TMNLkit.CurrentBreakpoint.isMobile()) {
                    //console.log("=== Mobile version setup ===");
                    $('.image-overlay').fadeIn(400, function() {
                        $('.image-overlay.image-full').css('display', 'block');
                    });
                    window.firstRun = false;
                } else {
                    //console.log("=== Desktop version setup ===");
                }
                $('#container-full-link').removeClass('wait-True');
            }

            window.onresize = function (event) {
                setTimeout(function () {
                    ratiolizer();                    
                }, 10);
            };

        });
    };

    function arrowScroll(target) {
       //console.log(target);
        if (target != "#" || target != "") {
            $('html, body').animate({
                scrollTop: $(target).offset().top
            }, 500);
        }
    }

    function ratiolizer() {
        var top = ($('.hero-view').height() - $('.media-container').outerHeight()) / 2;
        $(".media-container").attr('style', 'top:' + top + 'px');
        var left = ($('.hero-view').width() - $('.media-container').outerWidth()) / 2;
        $('.image-overlay').css({ 'margin-left': left + 'px' });
        $('#backgroundPlayer').css({ 'margin-left': (left - 20) + 'px' });
    }

    //
    // Youtube Script
    //
    function importYoutubeApi() {
        // Import the youtube api
        // http://www.youtube.com/player_api or https://www.youtube.com/iframe_api
        if (typeof window.youtube_api_init == 'undefined') {
            var tag = document.createElement('script');
            tag.src = "https://www.youtube.com/player_api";
            var firstScriptTag = document.getElementsByTagName('script')[0];
            firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
        }
    }

    function onYouTubePlayerAPIReady() {
        //console.log("=== Youtube:: onYouTubePlayerAPIReady function called ===");
        player = new window.YT.Player('backgroundPlayer', {
            playerVars: {
                'autoplay': 0,
                'controls': 0,
                'autohide': 1,
                'wmode': 'opaque',
                'showinfo': 0,
                'modestbranding': 1,
                'disablekb': 1,
                'loop': 1,
                'rel': 0,
                'fs': 0,
                'cc_load_policy': 0,
                'iv_load_policy': 3,
                'enablejsapi': 1
                //'playlist': 'DI_aoV0HSec,aQM9oDHA0O0,qE-misYnt3Q,xRDe-smLIJY,CZF8lucwRsM,5Ph2Gd7JXAo'
            },
            videoId: youtubeVideoId,
            width: 1903,
            height: 1050,
            events: {
                'onReady': onPlayerReady,
                'onStateChange': onPlayerStateChange,
                'onError': onPlayerError
            }
        });

        //Timeout if youtube is taking to long
        //Start Animation
        animationTimeout = setTimeout(function () {
            //console.log("=== Animation Timeout Called ===");
            $('#container-full-link').removeClass('wait-True');
        }, 2500);
    }

    function onPlayerReady(event) {
        //console.log("=== Youtube:: Player Ready ===");

        player.mute();
        player.playVideo();
        clearTimeout(animationTimeout);
    }

    function onPlayerStateChange(event) {
        if (event.data === 1) {
            removeDrop();
        }

        if (event.data === window.YT.PlayerState.ENDED) {
            player.seekTo(0);
        }
    }

    function onPlayerError(event) {
        //console.log("=== Youtube:: Player Error ===");
        //console.log(event);
    }

    //Remove drop image and show video.
    var c = 0;
    function removeDrop() {
        if (c != 1) {
            //console.log("=== Remove Drop ===");
            $(".image-overlay").fadeOut("1000");
            $('#container-full-link').removeClass('wait-True');
            c++;
        }
    }
