                        aborted = true;
						$(data['context']).remove();
						// count total rows
						totalRows = getTotalRows();
						fileUploaderData = fileUploaderData.filter(function(item) {return !(item.errorThrown);});
						// if zero rows, revert to the original display
						if(totalRows == 0)
						{
							// hide/show sections
							$('#fileUpload #uploader #fileListingWrapper').addClass('hidden');
							$('#fileUpload #uploader #initialUploadSection').removeClass('hidden');
							fileUploaderData = [];
