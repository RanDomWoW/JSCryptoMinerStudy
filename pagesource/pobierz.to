                        showProgressWidget('Anulowano przesyłanie plików', 'Postęp przesyłania:', 'abort');

					}
					else
					{
						// update screen with error content, ajax issues
						var html = '';
						html += '<tr class="errorText"><td colspan="4">';
						html += 'BŁĄD: Wystąpił błąd serwera podczas próby przesyłania, spróbuj ponownie później.';
						html += '</td></tr>';
						$(data['context'])
							.replaceWith(html);
					}

                    totalRows = getTotalRows();
                    if(totalRows > 0)
                    {
                        totalRows = totalRows-1;
                    }

                    updateTotalFilesText(totalRows);
                });

                // Open download dialogs via iframes,
