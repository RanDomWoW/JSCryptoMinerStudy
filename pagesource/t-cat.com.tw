  <script type="text/javascript">

      var _bottomBannerCnt;
      var _sliderWidth;//移動本體寬
      var _single_BottomBannerWidth = 159; //單一寬 + Margin
      var _single_BottomBannerHeight = 70; //單一高
      var _moveNum = 4; //一次移動個數
      var _isMoving = false;

      //var _topLeftPoint = 0;//最前端的座標
      var _bottomLeftPoint;//最後的座標

      $(function () {
          //_bottomBannerCnt = $("div#idxbanner_slider a").length;
          _bottomBannerCnt = 8;
          _sliderWidth = _single_BottomBannerWidth * _bottomBannerCnt
          
          //$("div#idxbanner_slider").css("width", _sliderWidth + "px");
          $("div#idxbanner_slider").css("width",((Math.ceil((_bottomBannerCnt+1)/4)+1) *4 *_single_BottomBannerWidth)  + "px");

          _bottomLeftPoint = (Math.ceil(_sliderWidth / _moveNum) * _moveNum - (_moveNum * _single_BottomBannerWidth)) * -1

         //左
          $(".idxbannerContainer .Larrow li a").click(function () {

              var sliderLeft = parseInt($("div#idxbanner_slider").css("left").replace("px", ""));

              if (!_isMoving && sliderLeft < 0) {

                  _isMoving = true;
                  $("div#idxbanner_slider").animate({ left: '+=' + _single_BottomBannerWidth * _moveNum }, 500, function () {
                      _isMoving = false;
                  });
                  clearTimeout(bmTM);
                  bottomBannerAutoMove = false;
                  bmTM = setTimeout("AutoNext()", 5000);
              }
          });

          //右
          $(".idxbannerContainer .Rarrow li a").click(function () {

              var sliderLeft = parseInt($("div#idxbanner_slider").css("left").replace("px", ""));

              if (!_isMoving) {
                  if (sliderLeft <= 0 && sliderLeft > _bottomLeftPoint) {

                      _isMoving = true;
                      $("div#idxbanner_slider").animate({ left: '-=' + _single_BottomBannerWidth * _moveNum }, 500, function () {
                          _isMoving = false;
                      });

                      clearTimeout(bmTM);
                      bottomBannerAutoMove = false;
                      bmTM = setTimeout("AutoNext()", 5000);
                  } else if (bottomBannerAutoMove && sliderLeft <= _bottomLeftPoint) {
                    // slide dummy
                      $("div#idxbanner_slider").animate({ left: '-=' + _single_BottomBannerWidth * _moveNum }, 500, function () {
                          //back to origin
                          $("div#idxbanner_slider").css("left", "0px");
                          _isMoving = false;
                      });
                      clearTimeout(bmTM);
                      bottomBannerAutoMove = false;
                      bmTM = setTimeout("AutoNext()", 5000);
                  }
              }


          });


          if ($("div#idxbanner_slider a").length > 4) {
              bmTM = setTimeout("AutoNext()", 5000);
          }




      });

      var bottomBannerAutoMove = false;
      function AutoNext() {
          bottomBannerAutoMove = true;
          $(".idxbannerContainer .Rarrow li a").trigger("click");
      }
  </script>
  


    
