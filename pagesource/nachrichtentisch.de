             <script>
     function lesen() {
                function refresh() {
                    $( "#datumzeit" ).load( "datumzeit.php" );
                    $( "#zeit_sm" ).load( "zeit.php" );
                   countdown( "countdown", 5, 0 );
                    nrfeeds = 0;
                    for (var i = 0; i < arr.length; i++) {
       
                         
                    $("#container-" + arr[i]).load("medienmobil" + numb + prev + "/" + arr[i] + ".php",
                                    function () {
                                    nrfeeds++;
                                    equalheight('.main .feed');
                                  
                                    if (nrfeeds == arr.length) {
                                         $('.tooltip').tooltipster({
                                           contentAsHTML: true,
                                           animation: 'grow',
                                           position: 'bottom',
                                           delay: 400,
                                           interactive: true,
                                           trigger: (prev == 1) ? 'hover' : 'click',
                                           timer: 15000
                                           });
                              $(window).keypress(function() {
      $('.tooltip').tooltipster('hide');
   });
    $(document).on('click', '.close', function() {
    $('.tooltip').tooltipster('hide');
});
                               }
                                    });
                    }
                }
                    function storageAvailable() {
                            try {
                                var a = window.localStorage;
                                if (a) return true; else return false;
                            } catch (e) {
                                return false;
                            }
                        }
                       defaults = ["tagesschau", "dlf", "spiegel", "heute",
                           "br", "sz", "inforadio", "ndr", "mdr", "wdr", "swrinfo", "ntv", "euronews", "faz", "welt","rp", "dw", "focus",
							"tagesspiegel", "berliner", "waz", "merkur", "fr",
                           "taz", "bild", "diezeit", "stern", "aus_nzz", "cicero",  "aus_cnn", "aus_skynews", "aus_orf", "aus_srf", "aus_reuters_us",  								"aus_aljazeera"];
                       if (storageAvailable()) {
                           str = localStorage.getItem("items");
                           numb = localStorage.getItem("numb");
                           prev = localStorage.getItem("prev");
         
                           
                           if (str) {
                               $("#ausgabe").val(str);
                               arr = JSON.parse(str);
                           } else {
                               arr = defaults;
                                numb = "10";
                               prev = "0";
                           }
                           if (!numb) numb = "10";
                           if (!prev) prev = "0";
                       } else {
                           arr = defaults;
                             numb = "10";
                               prev = "0";
                                          }
                       // arr should now contain the original array
                       $("#col").empty();
                       for (var i = 0; i < arr.length; i++) {
                           // create a container for the current feed
                           var container = $('<div id="container-' + arr[i] +
                               '" class="feed"></div>');
                           // insert this container into the container for feeds
                           $("#col").append(container);
                       }
