	<script type="text/javascript">
	  var isNoServiceFee = false;
	  var isEms93 = true;
	  var servFee = isNoServiceFee ? 0 : 150;
	  var emsRate = 0.95;
	  var isSf95 = false;
      var sfRate = 0.95;
	  $(".set-language").click(function(){
		  var language = $(this).attr('data');
		  $.cookie('lang',language,{expires:30, path:'/'});
	  });
	  $(".price-more").click(function(){
	      $(".data-more").fadeIn();
		  $(this).hide();
	  });
	  $("#price_sub").click(function(){
		  var weight = $("input[name='computer_weight']");
		  var country = $(".computer-country");
		  var weightpreg = /^\d+(\.\d{1,})?$/;
		  if( !weightpreg.test(weight.val()) || weight.val()<=0 ){
			  layer.tips("请输入合法的重量", weight , {guide: 2, time: 2});
		  }else if(weight.val()>30000){
			  layer.tips("重量不能超过30000g", weight , {guide: 2, time: 2});
	      }else if(!country.val()){
		      var country_select = $(".selectFruits");
			  layer.tips("请选择转运目的地", country_select , {guide: 2, time: 2});
		  }else{
			  $.ajax( {
	   			type: 'POST',  contentType: "application/json", url: '/ajax', data: JSON.stringify({
					'go': 'Ticket/getShipFeeAll', 'data': {'weight':weight.val(), 'country':country.val() }
	   			}), dataType: 'json'
	   		  }).done( function( json ) {
		   		var c_v = country.val();
		   		var ratioTo = 0.00;
		   		switch(c_v){
		   		case 'China' : ratioTo = ratioToCNY;yuan='人民币';break;
		   		case 'Hong Kong' : ratioTo = ratioToHKD;yuan='港币';break;
		   		case 'Taiwan' : ratioTo = ratioToTWD;yuan='新台币';break;
		   		case 'Singapore' : ratioTo = ratioToSGD;yuan='新加坡元';break;
		   		default : ratioTo = ratioToUSD;yuan='美元';break;
		   		}

	   			if(json.data){
					var fee = json.data.fee;
					var real_fee = [];
					var c_i = json.data.country_info;
					var html = '';
					var way = ['EMS(国际特快专递)运费','航空件运费', '经济航空件(SAL)运费','海运件运费'];
					if(isEms93) {
						way[0] = 'EMS(国际特快专递)运费（95折）';
					}
					if(isSf95) {
	                    way[3] = '海运件运费（95折）';
	                }
					for(var i=0; i<4; i++){
						real_fee[i] = fee[i];
                        if( ( i == 0 && c_i.enable_ems != 0 ) ||
                            ( i == 1 && c_i.enable_air != 0 ) ||
                            ( i == 2 && c_i.enable_sal != 0 ) ||
                            ( i == 3 && c_i.enable_sf  != 0 ) ) {
                            var className = (i == 0)? '':'data-more';
                            var td1 = '<td>{0}&nbsp;&nbsp;<span>{1}日元</span></td>'.Format(way[i], fee[i]);
                            if (isEms93 && ( i == 0 && c_i.enable_ems != 0 )) {
                                real_fee[i] = parseInt(fee[i]*emsRate);
                                td1 = '<td>{0}&nbsp;&nbsp;<span style="text-decoration: line-through;">{1}</span>&nbsp;<span>{2}日元</span></td>'.Format(way[i], fee[i], real_fee[i]);
                            }
                            if (isSf95 && ( i == 3 && c_i.enable_sf  != 0 )) {
	                          	real_fee[i] = parseInt(fee[i]*sfRate);
	                          	td1 = '<td>{0}&nbsp;&nbsp;<span style="text-decoration: line-through;">{1}</span>&nbsp;<span>{2}日元</span></td>'.Format(way[i], fee[i], real_fee[i]);
	                      	}
                            var td2 = '<td>手续费&nbsp;&nbsp;<span>150日元</span></td>';
                            if (isNoServiceFee) {
                                td2 = '<td>免手续费&nbsp;&nbsp;<span style="text-decoration: line-through;">150日元</span>&nbsp;<span>0日元</span></td>';
                            }
                            var td3 = '<td>运费合计&nbsp;&nbsp;<span>{0}日元</span> (约<span>{1}</span>{2})</td>'.Format(servFee+parseInt(real_fee[i]), ((servFee+parseInt(real_fee[i]))*ratioTo).toFixed(2), yuan);
                            html += '<tr class="{0}">{1}{2}{3}</tr>'.Format(className, td1, td2, td3);
                        }
	                }
					$("#price_data").html(html);
					$(".price-more").show();
	   	        }else{
	   	        	layer.msg("数据有误，请稍后重试",2,-1);
	   	        }
	   		  });
		  }
	  });
	</script>
