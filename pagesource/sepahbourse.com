    function drawChart(TopTitle, Isin , index, trends)
    {
        try
        {
            var top = -1 * parseInt(index) * ChartHeight ;
            $(".AllChartsDiv").css("top" , top);
            if($("#ChartContinerItem"+ index).hasClass("Drawed"))return;
            $("#ChartContinerItem"+ index).addClass("Drawed");

            if ( CurrentISIN == Isin) return;

            CurrentISIN = Isin;
            MyAjax.get
            (   
                resourceURL,{ ISIN: Isin },'imgAjaxLoaderISINChart',
                function (response) {
                    var ArrayTradePrice = new Array(response.DataPrice.length);
                    for (var i = 0; i < response.DataPrice.length; i++) {
                        ArrayTradePrice[i] = parseInt(response.DataPrice[i].TradePrice);
                    }
                    ShowChart(TopTitle, ArrayTradePrice,  'ChartContinerItem' + index , trends);
                }
            );
        }
        catch(exc)
        {
            $("#imgAjaxLoaderISINChart").css("display","none");
        }
    }
            
