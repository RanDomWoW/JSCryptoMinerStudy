    var $resource = "/DesktopModules/Tadbir/MarketSummary/GetMarketSummaryHandler.ashx?ModuleId=10996"; 
    var TabId = 7258;
    var TimerAjaxUpdateDelay = 60000 ;
    var first = true;
    var AnnimateSpeed = 20000;
    var UseAnnimate = false;

    function GetMarketSummaryData() 
    {
        $.post(
            $resource,
            function ($x) {

                $x.MarketSummaryInfo.NavigateToDetails = function() {
                    window.location = "/default.aspx?tabid=" + TabId + "&ctl=details&isin=" + this.LowestChangeValue;
                };

                $x.MarketSummaryInfo.TseIndex       = AddComma($x.MarketSummaryInfo.TseIndex);
                $x.MarketSummaryInfo.TradedVolum    = AddComma($x.MarketSummaryInfo.TradedVolume);
                $x.MarketSummaryInfo.TradedValue    = AddComma($x.MarketSummaryInfo.TradedValue);
                $x.MarketSummaryInfo.MostTradedVolumeClosingPrice   = AddComma($x.MarketSummaryInfo.MostTradedVolumeClosingPrice);
                $x.MarketSummaryInfo.MostTradedValueClosingPrice    = AddComma($x.MarketSummaryInfo.MostTradedValueClosingPrice);

                $x.MarketSummaryInfo.MostTradedValue    = AddComma($x.MarketSummaryInfo.MostTradedValue);
                $x.MarketSummaryInfo.MostTradedVolume   = AddComma($x.MarketSummaryInfo.MostTradedVolume);

                ko.cleanNode(document.getElementById("MSummary"));
                ko.applyBindings($x.MarketSummaryInfo , document.getElementById("MSummary"));

                if( UseAnnimate && first )
                    AnnimateView();
                first = false;
            }
        );
    }

    GetMarketSummaryData();

    if (UseAnnimate && new Date().getHours() < 13) {
        setInterval(GetMarketSummaryData, TimerAjaxUpdateDelay);
    }

    function AnnimateView() {
        var $target = $(".MSummaryMain");
        $target.clearQueue();
        $target.css('left', -1 * $target.width() ) ;
        
        $target.animate 
            (
                 { left: $(".MSummaryMainFix").width()  }
                 , AnnimateSpeed
                 , 'linear'
                 ,function () {
                     AnnimateView();
                 }
            );
    }

    function AddComma(valueToAdd)
    {
        if(valueToAdd)
            return valueToAdd.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g , "$1," );
        return "";
    }
    
    String.prototype.toPersianDigit = function (a) 
    {
        return this.replace(/\d+/g, 
			function (digit) {
			    var enDigitArr = [], peDigitArr = [];
			    for (var i = 0; i < digit.length; i++) {
			        enDigitArr.push(digit.charCodeAt(i));
			    }
			    for (var j = 0; j < enDigitArr.length; j++) {
			        peDigitArr.push(String.fromCharCode(enDigitArr[j] + ((!!a && a == true) ? 1584 : 1728)));
			    }
			    return peDigitArr.join('');
			});
    };

    $("#lnkFullScreen").attr("href", '/Default.aspx?tabid=7258&mid=10996&DnnPrintMode=True&SkinSrc=[G]Skins%2f_default%2fNo+Skin&ContainerSrc=[G]Containers%2f_default%2fNo+Container');

</script>

