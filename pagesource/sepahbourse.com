                  }
                  ,tooltip:
                  {
                      backgroundColor: { linearGradient: [0, 0, 0, 50], stops: [[0, 'rgba(96, 96, 96, .8)'], [1, 'rgba(16, 16, 16, .8)']] },
                      borderWidth: 0, style: { color: '#FFF' },
                      formatter: function () { return '<span style="font-family:tahoma;direction:ltr;" class="DataValueInGrid">' + 'time:' + ' ' +  Highcharts.dateFormat('%H:%M', this.x)  + ' -> ' + this.y + '</span>'; }
                  }
                  ,plotOptions:
                     {
                         area: 
                         {
                              fillColor: {linearGradient: [0, 0, 0, 300],stops: [[0, Highcharts.getOptions().colors[0]],[1, 'rgba(2,0,0,0)']]}
                             ,lineWidth: ChartLineWidth
                             ,marker: {enabled: false,states: {hover: {enabled: true,radius: 5}}}
                             ,shadow: false
                             ,states: {hover: {lineWidth: 1 }}
                         }
                         ,line:
                         { 
                              dataLabels: { color: 'red' }
                             ,lineWidth: ChartLineWidth
                             ,marker: {enabled: false,states: {hover: {enabled: true,radius: 5}}}
                             ,shadow: false
                             ,states: {hover: {lineWidth: 1 }}
                         }
                         ,spline: 
                         { 
                             dataLabels: { color: 'red' }
                             ,lineWidth: ChartLineWidth
                             ,shadow: false
                             ,states: { hover: { lineWidth: 3} }
                             ,marker: { enabled: false, states: { hover: { enabled: true, radius: 5}}}

                             ,pointInterval: parseInt(trends) * 1000 
                             ,pointStart: Date.UTC(2011, 8, 9, 8, 30 , 0)
                         }
                     }
                  , series:
                     [{ 
                         name: '<span style="font-family:tahoma">' + TopTitle + '</span>'
                        ,data: objArrayTradePrice 
                        ,lineColor: ChartLineColor 
                     }]
	              ,navigation: { menuItemStyle: { fontSize: '20px'} }
                  ,legend:
                  {
                      enabled: true, layout: 'horizontal', verticalAlign: 'top', borderWidth: 0, borderColor: 'white'
                     , itemStyle: { color: ChartTitleColor }
                     , itemHoverStyle: { color: '#FFF' }
                     , itemHiddenStyle: { color: '#333' }
                  }
              });
              log = true;
          }
          catch(err){
             log = true;
          }

          log = true;
     }
     

     setInterval(CallDrawChart,180000);
     
     function CallDrawChart()
     {
         CurrentISIN ='';
         $("#ChartContinerItem"+ $(".indexItem.Current").attr("index")).removeClass("Drawed");
         drawChart($(".indexItem.Current").attr("title") , $(".indexItem.Current").attr("ISIN") , $(".indexItem.Current").attr("index") , $(".indexItem.Current").attr("trends")  );
     }

     function OffDraw(SymbolISIN)
     {

        CurrentISIN = SymbolISIN ;
     }

     $("#indexItem0").trigger('mouseover');

});

