<script type="text/template" class="template" id="skillMenuTemplate">
    <% _.each( rc, function( listItem ){ %>
        <% var reviewCount %>
        <a class="trackable"  data-gacategory="HOME_SKILLS" data-gaaction="product_card_click" data-galabel="<%-  listItem.courseId %>" href="https://www.shawacademy.com/courses/<%- listItem.taxonomy.skill[0] %>/<%- listItem.slug %>">
        <div class="course-card-1">
        <img class="course-img" src="https://dt9ph4xofvj87.cloudfront.net/user/sites/shawacademy.com/themes/mytheme/images/courses/<%- listItem.courseId %>/slider-card.png" alt="<%- listItem.courseName %>">
        <div class="card-info">
        <h6> <%- listItem.courseNameLine1 %> </h6>
        <p> <%- _.unescape(listItem.courseNameLine2) %></p>
        </div>
        <p class="card-time"> <img src="https://dt9ph4xofvj87.cloudfront.net/user/sites/shawacademy.com/themes/mytheme/images/icons/generic/line/black/time.svg" alt="Clock Icon" width="11" height="11"> <%- listItem.courseDuration %> </p>
        <div class="card-graduates">
          <% if ((listItem.totalGraduates * 0.02) == 0) { %>
            <%  reviewCount = 53 %>
          <% } else { %>
             <% reviewCount =  Math.round(listItem.totalGraduates * 0.02).toLocaleString() %>
         <% } %>
          <h6> <%- listItem.totalGraduates.toLocaleString() %> </h6>
          <img src="https://dt9ph4xofvj87.cloudfront.net/user/sites/shawacademy.com/themes/mytheme/images/userIcon.svg" alt="User Icon" width="12" height="12">
        </div>
        <div class="card-rating">
          <% for (i=0; i < Math.floor(listItem.averageRating); ++i ) { %>
            <img class="star-icon" src="https://dt9ph4xofvj87.cloudfront.net/user/sites/shawacademy.com/themes/mytheme/images/starIcon.svg" alt="Rating Icon" width="14" height="14">
          <% } %>
          <% if ( Math.ceil(listItem.averageRating) > listItem.averageRating ) { %>
            <img class="star-icon" src="https://dt9ph4xofvj87.cloudfront.net/user/sites/shawacademy.com/themes/mytheme/images/starIconHalf.svg" alt="Rating Icon" width="14" height="14">
            <% } %>
          </div>
        </div>
        </a>
    <% }); %>
</script>

