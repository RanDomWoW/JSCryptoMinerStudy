function selperso(feed_id)
{
	xmlhttp=GetXmlHttpObject();
	if (xmlhttp==null)
	  { alert ("Browser does not support HTTP Request"); return; }
	//alert(document.getElementById('checkbox_Si').checked);
	var url="bbd-inc/update-perso.php";
	url=url+"?feed_id="+feed_id;
	url=url+"&sid="+Math.random();
	xmlhttp.onreadystatechange=stateChanged;
	xmlhttp.open("GET",url,true);
	xmlhttp.send(null);
}

function stateChanged()
{ if (xmlhttp.readyState==4)
	{ 
		var feed_id = xmlhttp.responseText;

		if(document.getElementById("follow-"+feed_id).innerHTML=='Suivre ce blog') { document.getElementById("follow-"+feed_id).innerHTML = 'Ne plus suivre ce blog';  var persocount = parseInt(document.getElementById("persocount").innerHTML); document.getElementById("persocount").innerHTML = (persocount+1); } else { document.getElementById("follow-"+feed_id).innerHTML = 'Suivre ce blog'; var persocount = parseInt(document.getElementById("persocount").innerHTML); document.getElementById("persocount").innerHTML = (persocount-1); }

		if(document.getElementById("feed"+feed_id+"-cat666").style.display == 'none') document.getElementById("feed"+feed_id+"-cat666").style.display = ''; else document.getElementById("feed"+feed_id+"-cat666").style.display = 'none';

	}
}







function GetXmlHttpObject()
{
if (window.XMLHttpRequest)
  {
  // code for IE7+, Firefox, Chrome, Opera, Safari
  return new XMLHttpRequest();
  }
if (window.ActiveXObject)
  {
  // code for IE6, IE5
  return new ActiveXObject("Microsoft.XMLHTTP");
  }
return null;
}


