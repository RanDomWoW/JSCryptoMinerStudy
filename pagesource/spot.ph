    <script type="text/javascript">

        var eventMethod=window.addEventListener?"addEventListener":"attachEvent";
        var eventer=window[eventMethod];
        var messageEvent=eventMethod=="attachEvent"?"onmessage":"message";

        eventer(messageEvent,function(event){

            if (event.origin !== "https://www.spot.ph")
            return;

            var message = event.data;

            if (message[0] !== "expanding-catfish")
            if (message[0] !== "expanding-catfish-close")
            return;

            var frames=document.getElementsByTagName('iframe');

            for(var i=0;i<frames.length;i++){
                if(frames[i].contentWindow===event.source){
                    $iframeID = frames[i].id;
                    $('iframe[id*="'+$iframeID+'"]').css('height',message[1]);
                    break;
                }
            }
        }, false);

    </script>


<script type="text/javascript">
    var autoCompleteClicked = function(t, el) {
        var searchValue = $(t).attr('data-title');

        $(el).val(searchValue);
        $(el).next('.result-autocomplete').removeClass('result-border');
        $(el).next('.result-autocomplete').html('');
    };

    var headerSearchCallback = function(response) {
        var
          resultCard = document.querySelector('.autocomplete-card'),
          resultData = JSON.parse(response),
          autocompleteResult = Object.keys(resultData.data).map(function (key) {return resultData.data[key]}),
          searchTextBox = $('.search-form [name="search"]');

        if(!resultData.error) {
            resultData.total = typeof resultData.total === 'undefined' ? 0 : resultData.total;

            if(resultData.total) {
                searchTextBox.next('.result-autocomplete').html('');

                forEach(function(i){
                    var
                      tempHolder = $(resultCard).clone(),
                      icon = '';

                    if(i.custom_1 == 'article') {
                        icon = 'icon-note';
                    } else if(i.custom_1 == 'establishments') {
                        icon = 'icon-home';
                    } else if(i.custom_1 == 'events') {
                        icon = 'icon-star';
                    }

                    tempHolder.attr('data-title', decodeEntities(i.title));
                    tempHolder.find('.figure').addClass(icon);
                    tempHolder.find('[data-title-primary]').html(decodeEntities(i.title));
                    tempHolder.find('[data-title-secondary]').html(decodeEntities(i.blurb));
                    tempHolder.removeClass('none');
                    tempHolder.attr('href', i.link);

                    searchTextBox.next('.result-autocomplete').append(tempHolder);

                }, autocompleteResult.slice(0,3));

                searchTextBox.next('.result-autocomplete').addClass('result-border').removeClass('none');
            } else {
                searchTextBox.parents('.input-group').removeClass('input-active');
                searchTextBox.next('.result-autocomplete').removeClass('result-border');
                searchTextBox.next('.result-autocomplete').addClass('none');
            }
        }
    };

    $(document).ready(function() {
        var
            searchTextBox = document.querySelector('.search-form [name="search"]'),
            searchForm = document.querySelector('.search-form[data-search-form]');

        searchTextBox.addEventListener('keypress', function(e){
            setTimeout(function(){
                var data = $(searchForm).serialize();

                search(data, headerSearchCallback);

                 $(searchTextBox).parents('.input-group').addClass('input-active');
            }, 800);
        });

        searchTextBox.addEventListener('blur', function(e){
            if(e.relatedTarget == null) {
                $(searchTextBox).parents('.input-group').removeClass('input-active');
                $(searchTextBox).next('.result-autocomplete').removeClass('result-border');
                $(searchTextBox).next('.result-autocomplete').addClass('none');
            }
        });

    });
</script>

<script type="text/javascript">
$(document).ready(function(){
    var
      channelArticles = null,
      menuClicked;

    if($('.nav-articles .nav-article').find('.body .title').html() == ''){
        var tempNavCard = $('.nav-articles').clone();
    }

    $('.nav-header .menu .menu-item').on('click', function(event){
        event.preventDefault();
        var
            id = this.getAttribute('data-child-id'),
            parentId = this.getAttribute('data-parent-id'),
            viewMoreTitle = $('.menu[data-id="'+parentId+'"]').find('[data-child-id="'+id+'"] a').html();

            $.get( "https://json.summitmedia-digital.com/spot/v1/channel/get/"+id+"/0/3" , function( data ) {
                var articleMenu = data;
                var tempContainer = '';

                $.each(articleMenu,function(key,article){

                    tempNavCard.find('[data-url]').attr('href', makeRefTag('.nav_featured',article.url,'featured','nav',key+1));
                    tempNavCard.find('[data-image]').attr('src', urlSubdomain(article.image));
                    tempNavCard.find('[data-title]').html(decodeEntities(article.title));

                    if (!article.sat_url) {
                        tempContainer += $(tempNavCard).html();
                    }

                });

                $('.nav-articles').html(tempContainer);

            });
            if(id.indexOf('__top-list') > 0){
                id = 'top-list';
            }
        $('.modal-content a.nav-viewmore').attr('href',"https://www.spot.ph/"+parentId+'/'+id+"?ref=nav_featured");
        $('.modal-content a.nav-viewmore span.selected-channel').html(viewMoreTitle);

    });

    $('[data-menu]').on('click', function(event){
        event.preventDefault();

        var
          target = getElement(this.getAttribute('data-target')),
          id = this.getAttribute('data-id'),
          toggleClass = this.getAttribute('data-toggle-class'),
          subMenu = getElement('.menu[data-id="'+id+'"]'),
          viewMoreTitle = $('[data-id="'+id+'"]').find('span.text').html();

        if(id == menuClicked) {
            target.classList.toggle(toggleClass);
            this.classList.toggle('icon-up');
            this.classList.toggle('menu-active');
        } else {
            forEach(function(el){ el.classList.remove('menu-active'); el.classList.remove('icon-up'); }, getElement('[data-menu]'));

            target.classList.add(toggleClass);
            this.classList.add('icon-up');
            this.classList.add('menu-active');
        }

        forEach(function(el){ el.classList.add('none'); }, getElement('.menu[data-id]'));

        subMenu.classList.toggle('none');

        menuClicked = id;

        $.get( apiUrl+"channel/get/"+id+"/0/3" , function( data ) {
            var articleMenu = data;
            var tempContainer = '';

            $.each(articleMenu,function(key,article){

                tempNavCard.find('[data-url]').attr('href', makeRefTag('.nav_featured',article.url,'featured','nav',key+1));
                tempNavCard.find('[data-image]').attr('src', urlSubdomain(article.image));
                tempNavCard.find('[data-title]').html(decodeEntities(article.title));

                if (!article.sat_url) {
                    tempContainer += $(tempNavCard).html();
                }

            });

            $('.nav-articles').html(tempContainer);

        });

        $('.modal-content a.nav-viewmore').attr('href',"https://www.spot.ph/"+id+"?ref=nav_featured");
        $('.modal-content a.nav-viewmore span.selected-channel').html(viewMoreTitle);

    });

    $(document).mouseup(function(e) {
        var container = $(".modal-menu"),
            hamburgerMenu = $('[data-menu]');
        if (!hamburgerMenu.is(e.target) && hamburgerMenu.has(e.target).length === 0 && !container.is(e.target) && container.has(e.target).length === 0) {
            container.removeClass('modal-show');
            hamburgerMenu.removeClass('icon-up');
            hamburgerMenu.removeClass('menu-active');
        }
    });
});
</script>


    
