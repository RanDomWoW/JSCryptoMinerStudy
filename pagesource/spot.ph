        generateFeed(featured, '.section-best-of-manila .small-featured', breakerTemplate, false, true, false, '2_a','headline');

        $('.widget-featured, .section-top-10-list, .section-top-10-list').removeClass('hide');

            /* first 12 feeds */
            apiConnect(feedAPI+feedCounter+'/'+feedChunk, 'GET', 'json', '', '', true).then(function(values) {
                $count = values.length/4;
 				for(i=0; i<Math.ceil($count); i++){

 					feed = values.splice(0, 4);
 					generateFeed(feed, '.content .widget-feed:eq('+i+') .feed', feedTemplate, false, true, false, 'feed','home',(feedCounter+1));
 				}

                feedCounter++;

                $('.content .widget-feed').removeClass('hide');

                $('img.lazy').lazyload({threshold:300}).removeClass('lazy');

            loading = false;
            }); /* first 12 feeds */

    }); /* featured */


    $(document).on('click','#lmBtn', function(){
        loading = true;

        apiConnect(feedAPI+feedCounter+'/'+feedChunk, 'GET', 'json', '', '', true).then(function(values){

            feedCounter++;

            generateFeed(values, '.content .widget-feed:last .feed', feedTemplate, false, true, false, 'feed','home',feedCounter);
            $('img.lazy').lazyload({threshold:300}).removeClass('lazy');

            if (feedCounter > limit){
                $('#lmBtn').attr('href',$('#lmBtn').data('hrefLatestPage')).addClass('gtm-feedArchive').text('View Archives').removeAttr('id');
                return;
            };

            loading = false;
        })

        return false;
    });


    $(window).on('scroll', function(){

        var currentPosition = $(window).scrollTop() + $(window).height() + loadmoreThreshold;
        var targetPosition = loadmoreTarget.offset().top;

        if(currentPosition >= targetPosition && loading === false) {
           loadmoreTarget.trigger('click');
       }
    });



});
</script>


