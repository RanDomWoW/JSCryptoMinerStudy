function getTime(dateStr){
  if(dateStr==null||dateStr==""){
    return;
  }
  var timeStr;
  if(dateStr.indexOf(" ")!=-1){
    var array1 = dateStr.split(" ");
    var arrayDate = array1[0].split("-");
    var dateTime = new Date(arrayDate[0]+"/"+arrayDate[1]+"/"+arrayDate[2]+" "+array1[1]);
    timeStr = dateTime.getTime();
  }else{
    var arrayDate = dateStr.split("-");
    var dateTime = new Date(arrayDate[0]+"/"+arrayDate[1]+"/"+arrayDate[2]);
    timeStr = dateTime.getTime();
  }
  return timeStr;
}
$.ajax({
  url :"/eportalapply/jsp/queryCurrentTime.jsp",
  type :"post",
  dataType:"html",
  async:false,
