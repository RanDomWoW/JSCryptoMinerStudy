    <script type="text/javascript">
    $(document).on('keypress',function(event){
        if( $("#q").is(":focus")&&event.keyCode==13 ){
             globSearch();
            }
        });
        $(function(){
            //搜索
            $('.search_content').attr('value','请输入关键字');
            $('.search_content').css('color','#aeaeae');
            $('.hot_search').click(function(){
                if($('.hot_search_content').css('display') == 'none'){
                    $('.hot_search_content').show();
                }else{
                    $('.hot_search_content').hide();
                }
            })
            $('.hot_search_content a').click(function(){
                $('.search_content').attr('value',$(this).text());
                $('.search_content').css('color','#424242');
                $('.hot_search_content').hide();
            })
            $('.search_content').focus(function(){
                if($(this).attr('value') == '请输入关键字'){
                    $(this).attr('value','');
                }
                $(this).css('color','#424242');
            })
            $('.search_content').blur(function(){
                if($(this).attr('value') == ''){
                    $(this).attr('value','请输入关键字');
                    $(this).css('color','#aeaeae');
                }else{
                    $(this).css('color','#424242');
                }
            })
            
            //分行网站、集团单位
            $('.sub_menu').hover(function(){
                $('.branch').stop().slideToggle(300);
            })
            $('.jtdv_menu').hover(function(){
                $('.jtdv_con').stop().slideToggle(300);
            })
        })
        function globSearch(){
            var reg = new RegExp(/^[\w\u4e00-\u9fa5]+$/i);
            var q = trim($("#q").val());
            if(q != ""){
                var flag = reg.test(q);
            if(!flag){
               alert('关键字中存在非法字符，请重新输入！','提示信息');
               $('#q').focus();
                    return false;
                }
            }
            if(q == '请输入关键字'){
                q="";
                $("#q").attr('value','');
            }
         window.open('/search.html?q='+q);
        }
        function trim(s) {
            if(typeof(s) != "undefined"){
                return s.replace(/^\s*/, "").replace(/\s*$/, "");
            }
        }
