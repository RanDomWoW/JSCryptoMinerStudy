        <script>
        // аякс поиск
            $(document).ready(function() {
                
                
                
                function getCookie(name) {
                  var matches = document.cookie.match(new RegExp(
                    "(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
                  ));
                  return matches ? decodeURIComponent(matches[1]) : undefined;
                }
                
                if(getCookie('popup')!=1){
                    document.cookie="popup=1";
                    
                    $("#header_promo").removeClass("active");
                    $("#header_promo [data-toggletext]").text("Закрыть");
                    $("#header_promo [data-toggletext]").data("toggletext")=="Открыть";
                }
        
        
        
            // Кнопка 
            	// Живой поиск
            	$(".sisea-search-form input").keyup(function() {
            		if(this.value.length > 2) { // Пользователь набирает больше 2 символов в строке поиска
            			// скрывает/отображает с результаты за пределами окна
            			$(document).click(function(event){ // скрываем
            				if ($(event.target).closest(".site-search-results").length) return;
            				$(".site-search-results").slideUp("fast");
            				//event.stopPropagation();
            			});
            			$('#search').click( function() { // отображаем
            				$(".site-search-results").slideDown("fast");
            				return false;
            			});	
            			// ajax запрос загрузка результатов поиска от страницы и показ контейнера
            			$("#site-search-results").load("/search-results/",$(".sisea-search-form").serialize()).slideDown("fast");
            		}
            		else {
            		    // Если набрано меньше 2 символов, скрыть контейнер (CSS display:none;)
            			$("#site-search-results").slideUp("fast"); 
            		}
            	});
            });
        // успешно обновили календарь
            function update_calendar_date(data){
                $("#calendar").html(data);
                $("#calendar").removeClass('active');
            }
            
        // обновляем календарь
            function update_calendar(button){
                $("#calendar").addClass('active');
                
                var monthRu ='январь,февраль,март,апрель,май,июнь,июль,август,сентябрь,октябрь,ноябрь,декабрь'.split(',');
                var date    =new Date($("[data-сalendar_date]").attr('data-сalendar_date'));
                if($(button).attr('data-ajax__сalendar')=='prev_month'){
                    date.setMonth(date.getMonth()-1);
                }
                if($(button).attr('data-ajax__сalendar')=='next_month'){
                    date.setMonth(date.getMonth()+1);
                }
                var day         =date.getDate();
                var month       =date.getMonth()+1;
                var year        =date.getFullYear();
                var new_date    =year+"/"+month+"/"+day;
                var arr_data    ={'date':new_date};
                $("[data-сalendar_date]").attr('data-сalendar_date',new_date);
        
                $("[data-сalendar_date]").html(monthRu[month-1]);
                $("[data-сalendar_date_year]").html(year);
        
                ajax('/assets/action/calendar.php',arr_data,'update_calendar_date');
            }
            
        // аякс обновление календаря
            $("[data-ajax__сalendar]").click(function(){
                update_calendar(this);
            });
        

        // слайдеры
        	$(document).ready(function() {
                $('#guides').bxSlider({
                    slideWidth:282,
                    minSlides: 1,
                    maxSlides: 4,
                    moveSlides: 1,
