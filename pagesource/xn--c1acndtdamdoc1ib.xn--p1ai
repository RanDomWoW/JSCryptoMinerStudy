                });
                 $('#last_comments').bxSlider({
                    pause:10000,
        	        controls:false,
        	        pager:false,
        	        auto:true
                });
                $('#partners').bxSlider({
                    slideWidth:540,
                    minSlides: 1,
                    maxSlides: 2,
                    moveSlides: 1,
                    slideMargin:30,
        	        controls:false,
        	        infiniteLoop:true,
        	        pager:true,
        	        auto:true
                });
                if($("[data-ajax__сalendar]").length>0){
                    update_calendar();
                }
            });
        </script>
