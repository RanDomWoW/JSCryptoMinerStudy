<script type="text/html" id="default-offer-template">
    <div data-bind="foreach:filteredOffers">
        <div class="span4" data-bind="css:{'new-row':(($index()%3)==0)}">
            <div class="offer-pod default-offer-template" data-bind="with:$data">
                <a class="offer-img" data-bind="attr: { 'href': ClickThroughUrl }" >
                    <div class="offer-img-row">
                        <img data-bind="attr: { 'src': ImageUrl, 'alt': ProductName, 'title':ProductName }">
                        <div class="offer-strapline" data-bind="visible:OfferStrapline"><span class="text" data-bind="text:OfferStrapline">&nbsp;</span></div>
                        <div class="offer-img-text">
                            <div class="one-line-text offer-starrating" data-bind="visible: HasStarRating">
                                <i class="classification icon-sprite icon-class_small icon-sprite-white" data-bind=" css: StarRatingClass"></i>
                            </div>
                            <div class="one-line-text" data-bind="text: ProductName">&nbsp;</div>
                            <div class="one-line-text" data-bind="text: DestinationText">&nbsp;</div>
                        </div>
                    </div>
                </a>
                <div class="offer-details">
                    <div class="offer-detail-row">
                        <div class="offer-duration one-line-text">
                            Price per person for <span data-bind="text: Duration">2</span> nights
                        </div>
                        <div class="offer-boardbasis-text one-line-text" data-bind="text: BoardBasisText">&nbsp;</div>
                        <div class="offer-room-text one-line-text" data-bind="text: RoomInfoText">&nbsp;</div>
                    </div>
                    <div class="offer-price-row">
                        <div class="half">
                            <div data-bind="html: InfoLine1">&nbsp;</div>
                        </div>
                        <div class="half offer-price">
                            <div class="offer-price-from one-line-text">From</div>
                            <div class="offer-price-price one-line-text"><span class="offer-price-unit">&pound;</span><span class="offer-price-value" data-bind="text: FromPrice">&nbsp;</span></div>
                        </div>
                        <span class="block-clear">&nbsp;</span>
                    </div>
                    <div class="offer-actions-row">
                        <a class="number offer-tel offer-action" data-bind="visible: !HasClickThroughUrl, attr: { 'title': TelephoneNumber, 'href': 'Tel:' + TelephoneNumber }">Call <span data-bind="text:TelephoneNumber">&nbsp;</span></a>
                        <a class="offer-link offer-action" data-bind="visible: HasClickThroughUrl, attr: { 'href': ClickThroughUrl }">View this offer </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>

