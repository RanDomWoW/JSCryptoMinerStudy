    <script type="text/javascript">
        $(function () {
            var loginInterval;
            $("#js-auth-button").bind({
                mouseenter: function() {
                    loginInterval = setInterval(toggleLbUp, 250);
                },
                mouseleave: function() {
                    $(this).removeClass("lb-up");
                    clearInterval(loginInterval);
                },
                click: function() {
                    var top = $(this).offset().top,
                        left = $(this).offset().left;

                    $('#js-auth-block').css({
                        left: left,
                        top: top
                    }).show();

                    $(this).removeClass("lb-up");
                    clearInterval(loginInterval);

                    $('.authorize-fon').fadeIn();
                }
            });

            function toggleLbUp(){
                $(".login-button").toggleClass("lb-up");
            }

            //scripts.js
            hideByOutClick($(".login-button"), function(){
                $('#js-auth-block').hide();
                $('.authorize-fon').hide();
            });

            $('#login-form').submit(function(){
                if (!$('#login').val() || !$('#pass').val()) {
                    return false;
                }
                $.post(
                    '/ajax/login.php',
                    $('#login-form').serializeArray(),
                    function(data){
                        if (data.success && data.must_change_login) {
                            document.location.href = '/register/change-login';
                        } else if (data.success) {
                            document.location.reload(true);
                        } else {
                            if (data.need_confirm) {
                                document.location.href = '/register/notice/?id=' + data.need_confirm;
                            } else if (data.need_complete_reg) {
                                document.location.href = '/register/complete';
                            }
                            else {
                                alert(data.message);
                            }
                        }
                    },
                    'json'
                );

                return false;
            });

        });
    </script>


