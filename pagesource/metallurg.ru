<script type="text/javascript">
    $(function() {
        var blockWidth = $('.main-playoff .wrap:first').width();
        var slideLock = false;

        var checkButtons = function() {
            var currentLeft = $('.main-playoff .wrap ul').position().left;

            if (currentLeft >= 0) {
                $('.playoffLeft').hide();
            }
            else {
                $('.playoffLeft').show();
            }

            if (currentLeft <= blockWidth * ($('.main-playoff .wrap ul li').length - 1) * -1) {
                $('.playoffRight').hide();
            }
            else {
                $('.playoffRight').show();
            }
        };

        $('.playoffLeft').click(function() {
            if (slideLock) {
                return;
            }
            slideLock = true;
            $('.main-playoff .wrap ul').animate({
                left: '+=' + blockWidth
            }, 500, function() {
                slideLock = false;
                checkButtons();
            });
        });
        $('#right').click(function() {
            if (slideLock) {
                return;
            }
            slideLock = true;
            $('.main-playoff .wrap ul').animate({
                left: '-='  + blockWidth
            }, 500, function() {
                slideLock = false;
                checkButtons();
            });
        });

        checkButtons();
    });
</script>


