    <script>
        $(function () {
            $(document).ready(function(){
                var waitInterval,
                    topTimer,
                    rotationTimeout,
                    setCurrent = function() {
                        if ($(this).parent().hasClass('active')) {
                            return false;
                        }
                        $(this).parent().addClass('active').siblings().removeClass('active');
                        var activeLink = $(".actual-list .active").find("a");
                        setLink(activeLink);


                        var index = $(this).index('.actual-list a');
                        $('.actual-detail .actual-detail-element').eq(index).addClass("active").siblings().removeClass("active");

                        var currentImg = $('.actual-detail .actual-detail-element').eq(index).find("img"),
                            currentSrc = currentImg.attr("src");
                        if (currentSrc.indexOf('blank.gif') != -1) {
                            currentImg.attr("src", currentImg.data("src"));
                        }

                        clearTimeout(topTimer);
                        topTimer = setTimeout(function(){
                            $('.actual-detail .actual-detail-element.active').addClass("top").siblings().removeClass("top");
                        }, 700); // время css анимации
                    };

                waitInterval = setInterval(function() {
                    // если высота блока больше 50, значит шрифты еще не подгрузились
                    if ($(".head-menu-block > ul").height() == 50) {
                        // Убираем блоки с заголовками новостей, которые не влезли в область
                        var maxH = $(".actual-list").height();
                        $(".actual-list li").each(function(){
                            if($(this).position().top + $(this).height() > maxH) {
                                $(this).remove();
                            }
                        });
                        // Убираем соответсвующие блоки с детальной информацией
                        $(".actual-detail .actual-detail-element").eq($('.actual-list li').length - 1).nextAll().remove();

                        initNews();
                        initSecondBlockNews();
                        clearInterval(waitInterval);
                    }
                }, 50);

                function initSecondBlockNews() {
                    // Новости клуба
                    $(".info-news-item-detail").each(function(){
                        var topOffset = $(this).height() - $(this).find(".ini-head").outerHeight();
                        $(this).css({
                            "top": topOffset
                        });
                        $(this).data("offset", topOffset);
                    });
                    $('.ini-head').addClass('showed');

                    $(".info-news-item").hover(function(){
                        $(this).find(".info-news-item-detail").stop(true);
                        $(this).find(".info-news-item-detail").animate({"top": 0},200);
                    },function(){
                        var topOffset = $(this).find(".info-news-item-detail").data("offset");
                        $(this).find(".info-news-item-detail").animate({"top": topOffset},200);
                    });

                    // Новости хоккея
                    var timerTool;
                    $(".mn-inform-right ul > li > a").hover(function(){

                        $(this).next().show();
                        if (Modernizr.cssanimations){
                            $(this).next().removeClass("fadeOutLeft").addClass("fadeInRight"); //next = .news-link-detail
                        }

                    },function(){
                        var toolTip = $(this).next();

                        if (Modernizr.cssanimations){
                            toolTip.removeClass("fadeInRight").addClass("fadeOutLeft");
                            timerTool = setTimeout(function(){
                                toolTip.hide();
                            },300);
                        } else {
                            $(this).next().hide();
                        }

                    });
                }

                function initNews() {
                    clearInterval(rotationTimeout);
                    $('.actual-list a').click(function(){
                        if (rotationTimeout) {
                            clearInterval(rotationTimeout);
                        }
                        setCurrent.call(this);
                        return false;
                    });
                    rotationTimeout = setInterval(function() {
                        var current = 0;
                        $('.actual-list li').each(function(n) {
                            if ($(this).is('.active')) {
                                current = n;
                            }
                        });
                        if (current == $('.actual-list li').length - 1) {
                            current = 0;
                        }
                        else {
                            current += 1;
                        }
                        $('.actual-list li').eq(current).find('a').each(function() {
                            setCurrent.call(this);
                        });
                    }, 3000);
                }
                function setLink(activeLink, href, newid, material) {
                    var link = $(".main-actual-news-link");
                    link.attr('href', activeLink.attr('href'));
                    link.data('newid', activeLink.data('newid'));
                    link.data('material', activeLink.data('materila'));
                }
            });
        });
    </script>



