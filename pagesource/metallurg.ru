<script type="text/javascript">
    var matchLine;
    $(document).ready(function() {

            matchLine = new MatchLine({
                target: $(".match-lister-block"),
                startFrom: 0,
                visibleElement: 4,
                beforeNext: function(){
                    // stop multi clicking
                    if( $(".match-view ul:animated").length || $(this).hasClass("disabled") || $(this).hasClass("disabled_ajax")) {
                        return false;
                    }

                    var maxOffset = ($('.match-view .match-item').length - 5) * this.step;
                    if (maxOffset + this.currentListOffset <= this.step && matchNav.next) {
                        //не даем грузить более 25 матчей
                        if ($('.match-item').length > 24) {
                            return false;
                        }
                        if (matches != matchNav.next) {
                            $('.match-next').removeClass('disabled');
                            loadMatches('next');
                            return false;
                        }
                    } else {
                        this.slideNext();
                        return false;
                    }

                },
                beforePrev: function(){
                    // stop multi clicking

                    var leftOffset = $('.match-view ul').position().left;
                    if (-leftOffset <=  this.step) {
                        if ($('.match-item').length > 24) {
                            $('.match-prev').addClass('disabled');
                            return false;
                        }
                        if (matchNav.prev) {
                            $('.match-prev').removeClass('disabled');
                            loadMatches('prev');
                            return false;
                        }
                    } else
                    {
                    this.slidePrev();
                    }
                }
            });


        if (matchNav.prev) {
            $('.match-prev').removeClass('disabled');
        }

        if (matchNav.next)
        {
            $('.match-next').removeClass('disabled');
        }
    });

    var timer_line;

    function loadMatches(dir) {
        clearTimeout(timer_line);
        var data = {};
        if (dir == 'prev') {
            data.key = matchNav.prev;
        }
        else {
            data.key = matchNav.next;
        }
        data.dir = dir;
        $('.match-nav').removeClass('disabled_ajax');
        $(".match-" + dir ).addClass('disabled_ajax');
        $.ajax({
            'type'          : 'get',
            'url'           : '/ajax/getMatchLine.php',
            'data'          : data,
            'dataType'      : 'json',
            'cache'         : false,
            'success'       : function (data) {
                $(".match-" + dir ).removeClass('disabled_ajax');
                if (dir == 'prev') {
                    $('.match-view ul').prepend(data.blocks);
                    $('.match-dropdown-view').prepend(data.modals);
                    leftOffset = (matchNav.prev - data.prev + 1) * matchLine.step;
                    $('.match-view ul').css('left', -leftOffset);
                    matchNav.prev = data.prev;
                } else {
                    $('.match-view ul').append(data.blocks);
                    $('.match-dropdown-view').append(data.modals);
                    matchNav.next = data.next;

                }
                matchLine.moveMatchSlider(dir);
            }
        });
        timer_line = setTimeout(function(){
            $(".match-" + dir ).removeClass('disabled_ajax');
        }, 300);
    }
</script>
