<script language="JavaScript" type="text/javascript">
$(document).ready(function() { 
    $('.calndr').datepicker({
        format: "yyyy-mm-d",
    })
    //Listen for the change even on the input
    .change(dateChanged)
    .on('changeDate', dateChanged);
});

function dateChanged(ev) { console.log(ev);

    var getfulldates=JSON.stringify(ev.date);

    
    var spdate=getfulldates.split("-");
    var year= spdate[0].substring(1, spdate[0].length);
    var month= spdate[1];
    var tempdate= spdate[2];
    var sptempdate =tempdate.split("T");
    var getday=sptempdate[0];

    var today = new Date(month+'/'+getday+'/'+year);
    var tomorrow = new Date(today);
     tomorrow.setDate(today.getDate()+2);
     tomorrow.toLocaleDateString();

     var getfulldates=JSON.stringify(tomorrow);

    var spdate=getfulldates.split("-");
    var year= spdate[0].substring(1, spdate[0].length);
    var month= spdate[1];
    var tempdate= spdate[2];
    var sptempdate =tempdate.split("T");
    var getday=sptempdate[0];

    window.location="http://www.ngex.com/events/public/index.php?day="+getday+"&month="+month+"&year="+year;
    
    
}
</script>
