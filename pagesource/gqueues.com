    <script type="text/javascript">

        var panels = $('.gq-homem-section-2-container .gq-homem-section');
        var currPanel = 0;
        var DURATION = 13000;


        var showPanel = function(panelIndex){

            var viewWidth = $(window).width();

            $('.gq-homem-section-2-container .gq-homem-section').each(function(index){
                var newLeft = ((index) * viewWidth) - (panelIndex * viewWidth);
                $(this).css("left", newLeft);
            });

            $(".gq-homem-panel-selector li.selected").removeClass('selected');
            $(".gq-homem-panel-selector li").eq(panelIndex).addClass('selected');

        };



        var init = function(){

            showPanel(currPanel);

            var slidePanels = setInterval(function(){

                currPanel = ((currPanel + 1) % panels.length);
                showPanel(currPanel);

            }, DURATION);


            $('.gq-homem-section-2-container').click(function(){
                currPanel = ((currPanel + 1) % panels.length);
                showPanel(currPanel);
                clearInterval(slidePanels);
            });

            $('.gq-homem-panel-selector li').click(function(){
                showPanel($(this).index());
                clearInterval(slidePanels);
            });

        };


        init();

    </script>

