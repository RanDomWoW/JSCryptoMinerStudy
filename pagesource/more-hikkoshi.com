<!--checklist -->
<script>
//変数storageにlocalStorageを格納
var storage = (function(){ return window.localStorage;})();
//.checkCashの総数をカウント
var classnum = $(".checkCash").length;

// var success = document.getElementById('btn-success');
// var clear = document.getElementById('btn-default');

// function setClick(){
// success.addEventListener('click',function set(),true);
// clear.addEventListener('click',function cle(),true);
// }

//データを格納するファンクション
function setchecked() {
  //格納配列を定義
  var checklist = {};
  for (var i=0; i < classnum; i++) {
    var k = "#" + $(".checkCash:eq("+i+")").attr("id");
    var v = $(".checkCash:eq("+i+")").prop("checked");
    //配列に変数を格納
    checklist[k] = ""+v+"";
  };
  localStorage.setItem("checklist", JSON.stringify(checklist)); 
    //日付を設定
      //今日の日付データを変数hidukeに格納
      var date=new Date(); 
      // 1桁の数字を0埋めで2桁にする
      var toDoubleDigits = function(num) {
        num += "";
        if (num.length === 1) {
          num = "0" + num;
        }
        return num;     
      };
      //年・月・日・曜日を取得する
      var year = date.getFullYear();
      var month = toDoubleDigits(date.getMonth() + 1);
      var day = toDoubleDigits(date.getDate());
      var hour = toDoubleDigits(date.getHours());
      var minute = toDoubleDigits(date.getMinutes());
      var todaytime = "最終保存日："+year+"/"+month+"/"+day+" "+hour+":"+minute;
  localStorage.setItem("checkedtime",todaytime);
  $(".checkedTime").html("<i class='fa fabiconCheck fa-check-circle'></i>"+todaytime);
  $(".checkedTime").removeClass("checkNot");
  $(".checkedTime").addClass("checkYes");
}

//格納されたデータを実行
function dochecked() {
  var answer = JSON.parse(localStorage.getItem("checklist"));
  console.log(answer);
  if (answer) {
    var dokeys = Object.keys(answer);
    console.log(dokeys);

    for (var i=0; i < classnum; i++) {
      var dokey = dokeys[i];
      var dovalue = answer[dokey];
      console.log(dovalue);
      if (dovalue === "true") {
        $(dokey).attr("checked",true);
      }else{
        $(dokey).attr("checked",false);     
      }
    };
  };
};
dochecked();

//データを格納するファンクションを実行
$(".btn-success-foot").click(function set() {
  if (navigator.cookieEnabled) {
    setchecked();
    alert("チェック項目が保存されました。引越し作業が進むごとにチェックしていってくださいね。ブックマークやホーム画面に登録してご利用ください。");
  } else {
    alert("ブラウザの cookie がオフになっています。cookie をオンにしてください。");
  }
}
);

//データをクリアする
$(".btn-default-foot").click(function cle() {
  var res = confirm("本当にチェック項目をリセットしますか？");
  if (res == true) {
    storage.clear();
    $(".checkCash").attr("checked",false);
    alert("チェック項目がリセットされました。");


    //日付を設定
    $(".checkedTime").html("<i class='fa fabiconChecknot fa-check-circle'></i>最終保存日：チェックリストを保存していません");
    $(".checkedTime").addClass("checkNot");
    $(".checkedTime").removeClass("checkYes");

  } else{
    return;
  }
}
);

//ブラウザを開いた時に、最終更新日を通知
var checkedtime = localStorage.getItem("checkedtime"); 
(function(){
  if (checkedtime) {
    $("h1").after("<p class='checkedTime checkYes'><i class='fa fabiconCheck fa-check-circle'></i>"+checkedtime+"</p>");
  }else{
    $("h1").after("<p class='checkedTime checkNot'><i class='fa fabiconChecknot fa-check-circle'></i>最終保存日：チェックリストを保存していません</p>");      
  }
}());

//保存ボタン
var successBtn=$('.backtosuccess');
    successBtn.hide();
setTimeout(function() {
    $(successBtn).fadeIn();
}, 2000);

</script>
