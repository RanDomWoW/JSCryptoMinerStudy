		$.fn.indexRotator = function(options) {
			
			var autoratate = false;
			var pane = $(this);
			var image = pane.find('.news-image-placeholder > a > img');
			var globalContainer = pane.find('#news-pane-container');
			var textBlock = $('.news-anons');
			var announce = pane.find('.news-anons > .news-text');
			var name = pane.find('.news-anons > a:first');
			var date = pane.find('.news-anons > .date');
			var links = pane.find('#lf-news-list');
			
			for(var i in options.articles) {
				var tmp = options.articles[i];
				links.append('<a counter="' + i + '" href="' + tmp.url + '" class="item"><div class="date">' + tmp.time + '</div><span>' + tmp.name + '</span><div class="clr"></div></a>');
			}
			
			function ShowItem(item) {
				var article = options.articles[item.attr('counter')];
				links.find('.active').removeClass('active');
				item.addClass('active');
				image.attr('src', article.img).parent().attr('href', article.url);
				name.html(article.name).attr('href', article.url);
				announce.html(article.announce);
				date.html(article.fullTime);
				globalContainer.hide().fadeIn(500);
			}
			
			function StopAutorotate() {
				if(autoratate) {
					clearInterval(autoratate);
					autoratate = false;
				}
			}
			
			function SetAutorotate() {
				if(!autoratate) {
					autoratate = setInterval(function() {	
						var item = links.find('.active');
						item = item.next();
						if(item[0] == undefined) {
							item = links.find('a:first');
						}
						ShowItem(item);
					}, 5000);
				}
			}
			
			pane.show();
			
			links.find('a').each(function(i) {
				if(i == 0) {
					ShowItem($(this));
				}
				$(this).hover(
					function() {
						StopAutorotate();
						ShowItem($(this));
					}
					,
					function() {
						return;
					}
				)
			});
			
			SetAutorotate();
			
			pane.hover(
				function() {
					return false;
				}
				,
				function() {
					SetAutorotate();
				}
			);
			
		}
		
