				jQuery('li#nav-design').attr('style', '');
				jQuery('li#nav-resources').attr('style', '');
			}
		}
		
		function IsMobile() {
		   if(window.innerWidth <= 600) {
			 return true;
		   } else {
			 return false;
		   }
