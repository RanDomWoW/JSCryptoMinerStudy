 reviewNavigator = function() {
    currentColumn = 1;
    var columns = $('.review--frame-container ul').length;

    // what should the new Left Position be
    var getNewPosition = function(){
        var w = 100;
        var newPos = -1 * ( currentColumn - 1 ) * w + "%";
        return newPos;
    };

    var changeColumn = function(direction){
        // change the current Column to the next or previous one
        currentColumn = direction > 0 ? currentColumn + 1 : currentColumn - 1;
        selectedItem = currentColumn - 1;
        $('.review--frame-container').animate({
            left: getNewPosition()
        });
    }

    var containerHeight = function(){
        newHeight = $('.review--frame-container [rel=' + currentColumn+ ']').height();
        $('.reviews-container').css('height', newHeight + 'px');
    }

    $('a.next').on('click', function(evt){
        evt.preventDefault();
        if(currentColumn < columns) {
            changeColumn(1);
        }
        containerHeight();
    });
    $('a.previous').on('click', function(evt){
        evt.preventDefault();
        if(currentColumn > 1) {
            changeColumn(-1);
        }
        containerHeight();
    });

}
</script>
