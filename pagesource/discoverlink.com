jQuery(document).ready(function($){


		
		// parallax fot top image
		$(document).ready(function () {
			$(window).scroll(function () {
				var s = $(window).scrollTop();
				/* parallax effect fot top image */
				$("#slider-paralax img").css("top",(s/2)  + "px");
			})
		})
		
		
		
		//fixed menu for homepage
		if ($(window).width() > 768) {
		
			   $(window).bind('scroll', function() {
			   var navHeight = $( '.ls-wp-fullwidth-container' ).height();
					 if ($(window).scrollTop() > navHeight) {
						 $('#header').addClass('header-scrolled');
						 $('body').addClass('body-with-header-scrolled');
					 }
					 else {
						 $('#header').removeClass('header-scrolled');
						 $('body').removeClass('body-with-header-scrolled');
					 }
				});
		}

	
	
	
	//testimonials quote signs
	$( ".avia-testimonial-content p" ).wrapInner( "<span class='testimonial_text_p'></span>");
	$( ".avia-testimonial-content p" ).prepend( "<span class='l-quote'>&nbsp; </span>" );
	$( ".avia-testimonial-content p" ).append( "<span class='r-quote'>&nbsp; </span>" );
	

	/* fix lack of elements for styling properly clients circles on front page */
	$( "#home-clients .slide-entry" ).wrapInner( "<div class='client_img'></div>");
	
	/* fix different height of testimonials on front page*/
	var TestimonialsMaxHeight = Math.max.apply(null, $("div.avia-testimonial").map(function ()
		{
			return $(this).height();
		}).get());
	$("div.avia-testimonial-wrapper").css("height",TestimonialsMaxHeight);
	
	
	/* fix height of title */
	if (($(window).width() > 420) && (($(window).width() < 768)) ){
		var succesLineHeight= $(".second-line").height();
		$(".second-line .line-title").css("line-height",succesLineHeight + 'px');
		var succesLineHeight= $(".third-line").height();
		$(".third-line .line-title").css("line-height",succesLineHeight + 'px');
	}
	
	if ($(window).width() > 768) {
			/* fix different height */
		var secMaxHeight = Math.max.apply(null, $(".challenges, .solutions").map(function ()
			{
				return $(this).height();
			}).get());
		$(".challenges, .solutions").css("height",secMaxHeight);
	};
	
	
	/*  open .pdf files in new browser page */
		$('a[href$=".pdf"]').click(function() {
			window.open(this.href);
			return false;
			});
});


