$('#txt').stop().animate({fontSize: '+=1px'},300);

});

$('.amoin').click(function () {

$('#txt').stop().animate({fontSize: '-=1px'},300);

});

});





$(document).ready(function () {

var taille = 14;

var augmentation = 1;

var tailleMax = 25;

var tailleMin= 10;

$('.aplus').click(function () {

taille +=1;

if (taille >= tailleMax)

{

taille = tailleMax;

}

$('#txt').stop().animate({fontSize: taille+"px"},300);

});

$('.amoin').click(function () {

taille -=1;

if (taille <= tailleMin)

{

taille = tailleMin;

}

$('#txt').stop().animate({fontSize: taille+"px"},300);

});

});
</script>



