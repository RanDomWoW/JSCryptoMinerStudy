        <script language="javascript">
            function showMsgDiv() {
                showDullDiv();
                showAppendedDiv();
                var divTag = document.getElementById('appendedDiv');
                var divData = '<div class="dwindowClsee" align="center" id="appendedDivInner">'
                divData += '<div class="Create_heading">Warning</div>'
                divData += '<div align="center" id="divContent">'
                divData += '<table width="90%" cellspacing="0" cellpadding="1" border="0">'
                divData += '<tr><td height="10"></td></tr>'
                for (i = 0; i < errorMsg.length; i++) {
                    divData += '<tr><td height="25" align="left" style="font-family:Arial;font-size:12px;">&nbsp;-&nbsp;' + errorMsg[i] + '</td></tr>'
                }
                divData += '<tr><td height="10"></td></tr>'
                divData += '</table>'
                divData += '</div>'
                divData += '<div><input class="clsBtn" type="button" value="Close" onclick="closeDiv(\'appendedDiv\')"></div>'
                divData += '<div>&nbsp;</div>';
                divData += '</div>'
                divTag.innerHTML = divData
                document.getElementById('appendedDivInner').style.display = 'block'
                setDivPosition('appendedDivInner')
            }
            function fillFrm(obj, n, val, cls) {
                if (typeof obj != 'undedined' && typeof n != 'undedined') {
                    if (n == 1) {
                        if (trim(obj.value) == val) {
                            obj.value = '';
                            obj.className = cls
                        }
                    } else if (n == 2) {
                        if (trim(obj.value) == val || trim(obj.value) == '') {
                            obj.className = cls + '_dull'
                            obj.value = val;
                        }
                    }
                }
            }
            function showFillPass(obj, name, n) {
                if (typeof obj != 'undedined' && typeof name != 'undedined') {
                    if ((name == 'userpassTemp' || name == 'confpassTemp') && (trim(obj.value) != '')) {
                    } else {
                        obj.style.display = 'none'
                        document.getElementById(name).style.display = ''
                        if (n == 1)
                            document.getElementById(name).focus();
                    }
                    if (name == 'userpassTemp')
                        document.getElementById(name).value = 'Password'
                    else if (name == 'confpassTemp')
                        document.getElementById(name).value = 'Confirm Password'
                }
            }
            function validate(email) {
                var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
                if (reg.test(email) == false)
                    return false;
                else
                    return true;
            }
            function chkFrmFields(frmObj) {
                var flag = 0;
                var msg = 'All fields are compulsory. Please Enter following fields.';
                var f1 = trim(frmObj.fname.value);
                var f2 = trim(frmObj.lname.value);
                var f3 = trim(frmObj.email.value);
                var f4 = trim(frmObj.username.value);
                var f5 = frmObj.userpass.value;
                var f6 = frmObj.confpass.value;
                //var f7 = frmObj.course.value;
                var f7 = frmObj.SecurityCode.value;
                var f8 = trim(frmObj.phone1.value);
                var f9 = trim(frmObj.phone2.value);
                var f10 = trim(frmObj.phone3.value);
                if (f1 == '' || f1 == 'First Name') {
                    ++flag;
                    msg += '\n\t Your first name';
                }
                if (f2 == '' || f2 == 'Last Name') {
                    ++flag;
                    msg += '\n\t Your last name';
                }
                if (f3 == '' || f3 == 'email' || validate(f3) == false) {
                    ++flag;
                    msg += '\n\t Your email address';
                }
                if (f8 == '' || f8 == 'Phone' || f9 == '' || f9 == 'Phone' || f10 == '' || f10 == 'Phone') {
                    ++flag;
                    msg += '\n\t 10 digit phone number';
                }
                if (f4 == '' || f4 == 'User Name') {
                    ++flag;
                    msg += '\n\t User name';
                }
                if (f5 == '') {
                    ++flag;
                    msg += '\n\t Password';
                }
                if (f5 != '' && f5.length < 4) {
                    ++flag;
                    msg += '\n\t Password should be alteat 4 character long.';
                }
                if (f6 == '') {
                    ++flag;
                    msg += '\n\t Confirm Password';
                }
                //if(f7 == ''){ ++flag; msg += '\n\t Course'; }	
                if (f7 == '' || f7 == 'Security Code') {
                    ++flag;
                    msg += '\n\t Security Code';
                }
                if (flag > 0) {
                    alert(msg);
                    return false;
                }
                if (f5 != f6) {
                    alert('Password and Confirm password should be same.');
                    return false;
                }
            }
            function clickte() {
                keyworditem = document.getElementById('search').value;
                if (keyworditem == "" || keyworditem == 'MBA, MCA, GRE, GMAT, CBSE, NTSE, IELTS ...') {
                    keyworditem = "";
                    alert("Please enter Keyword/Topic");
                    return false;
                }
                if (keyworditem != "") {
                    var searchvale = keyworditem.toLowerCase();
                    var strReplaceAll = searchvale;
                    var intIndexOfMatch = strReplaceAll.indexOf(" ");
                    while (intIndexOfMatch != -1) {
                        strReplaceAll = strReplaceAll.replace(" ", "-");
                        intIndexOfMatch = strReplaceAll.indexOf(" ");
                    }
                    strReplaceAll = strReplaceAll.replace("c#", "c-sharp");
                    strReplaceAll = strReplaceAll.replace("&", "and");
                    keyworditem = strReplaceAll;
                }
                categoritem = document.getElementById('cat').value;
                if (categoritem == "") {
                    categoritem = "all";
                    window.location = '//www.tcyonline.com/tests/' + keyworditem;
                } else {
                    window.location = '//www.tcyonline.com/tests/' + keyworditem + '/' + categoritem + '/1';
                }
            }
            function setDivPosition(obj) {
                var myHeight = window.innerHeight ? window.innerHeight : (document.body.clientHeight ? document.body.clientHeight : document.documentElement.clientHeight)
                var objDiv = document.getElementById(obj)
                var clientWid = document.documentElement.clientWidth ? document.documentElement.clientWidth : document.body.clientWidth;
                var clientHei = document.body.clientHeight ? document.body.clientHeight : document.documentElement.clientHeight
                objDiv.style.left = ((clientWid - objDiv.offsetWidth) / 2) + 'px';
                var ignoreMe = document.documentElement.scrollTop ? document.documentElement.scrollTop : document.body.scrollTop
                objDiv.style.top = (myHeight - objDiv.offsetHeight) / 2 + 'px';
            }
        </script>
