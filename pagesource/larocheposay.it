			}
		};
		var oYTrackingOptions = {
			onStart:{
				callback: oYTTrackingFct.onStartViewVideo
			},
			onPercent:{		
				25:	{
					callback: oYTTrackingFct.onPercentViewVideo,
					args:{percent:'25%'}
				},
				50:{
					callback: oYTTrackingFct.onPercentViewVideo,
					args:{percent:'50%'}
				},
				75:	{
					callback: oYTTrackingFct.onPercentViewVideo,
