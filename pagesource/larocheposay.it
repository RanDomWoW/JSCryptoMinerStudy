							onSubmitNewsletterFooter(false);
						}
						function onSubmitNewsletterFooter(bIsInscription){
							var msg_error = "";
							var bSubmitForm = true;
							if (jQueryObject('#formNewsletterFooter [name="mail"]').length >0){
								if(!controlValeurEmailObligatoire(jQueryObject('#formNewsletterFooter [name="mail"]').get(0),false)){
									bSubmitForm = false
									msg_error = NewsletterFooter_NeedEmail;
								}
							} else {
								bSubmitForm = false
							}
							if (msg_error == ""){
								msg_error = NewsletterFooter_ErrorDefault;
							}
							if (bSubmitForm) {
								//popin confirm
								if (bIsInscription){
									if (sLRPInter_NewsletterConfirmWithChck == '1'){
										openPopinFunction('popinLRPInter','popinNewsletterFooterConfirm','','','','')
										if(TemplateNewsletterConfirmWithChck==undefined){
											TemplateNewsletterConfirmWithChck = AOXJsManager.getNewTemplateAOX(jQueryObject('#formTemplateNewsletterConfirmWithChck'));									
										}
									}else {
										popConfirmMessage('',NewsletterFooter_PopinConfirmInscriptionTitle,jQueryObject("<textarea/>").html(NewsletterFooter_PopinConfirmInscriptionMsg).text(),'sendNewsletterFooterAJAXInscription','');
									}
								} else {
									popConfirmMessage('',NewsletterFooter_PopinConfirmDesinscriptionTitle,NewsletterFooter_PopinConfirmDesinscriptionMsg,'sendNewsletterFooterAJAXDesincription','');
								}
							} else {
								//popin erreur
								popAlertMessage(msg_error,'');
							}
						}
						function checksNewsletterConfirmWithChck(ctrl){
							if (typeof TemplateNewsletterConfirmWithChck != 'undefined') {
								var result = TemplateNewsletterConfirmWithChck.check();
								if (result.error) {
									bSubmitIsLock = false;
									AOXJsManager.tools.event.restoreEventHandler(ctrl,"click");
									TemplateNewsletterConfirmWithChck.processErrors(result); 
								} else {
									jQueryObject('#popinLRPInter').overlay().close();							
									sendNewsletterFooterAJAXInscription();
								}
							}
						}
						function sendNewsletterFooterAJAXInscription(){
							//MKI 2017/04/05 - modifs gtm altima CDO
							if (typeof AOXJsManager != 'undefined' && typeof bSiteUseTagManagerAltima != 'undefined' && bSiteUseTagManagerAltima &&
								(typeof gtmEventName != 'undefined') && gtmEventName.length > 0) {								
								AOXJsManager.getGTMTracking().pushEvent_Std(gtmEventName, {category: 'Footer', action:'Newsletter', label:'Register_Validate'});
							}

							sendNewsletterHeaderAJAX(true);
						}
						function sendNewsletterFooterAJAXDesincription(){
							sendNewsletterHeaderAJAX(false);
						}
						function sendNewsletterHeaderAJAX(bIsInscription){
							
							//closePopinMessage();
							
							// MBR - 06/08/2015 - Modif suite au passage v4 : il existe deux sortes de popin pour les messages.
							//if( jQueryObject('#popinMessageConfirm').length > 0 ){ jQueryObject('#popinMessageConfirm').overlay().close(); }
							//if( jQueryObject('#popinMessageAlert').length > 0 ){ jQueryObject('#popinMessageAlert').overlay().close(); }
							
							var strAttributes = "";
							if (bIsInscription) {
								strAttributes += "<Service ServiceName='ServiceAOX_InscriptionNewsletter'>";
								strAttributes += " <ServiceParam ServiceParamName='FrontOfficeUser.Mail'>" + jQueryObject('#formNewsletterFooter [name="mail"]').val() + "</ServiceParam>";
								strAttributes += " <ServiceParam ServiceParamName='AdminActionResultMode'>" + 1 + "</ServiceParam>";
								if (NewsletterFooter_WaitingValidation=='1') {
									strAttributes += " <ServiceParam ServiceParamName='MailAboutNewsletter.SendMail_Html_Code'>HTMLT_V3_MAIL_NWL_INSC_WAIT</ServiceParam>";
									//sujet : AOX_Newsletter_WaitingValidationSubject
									//!!!indiquer Media Confirmation pour redirect à partir de l'URL dans le mail d'optin, sinon vérifier Media HTMLT_SUBSCRIBE_UNSUBSCRIBE_NEWSLETTER existe
									strAttributes += " <ServiceParam ServiceParamName='MailAboutNewsletter.ServiceConfirmationMediaAppCode'>HTMLT_V3_NEWLSTER_CONFIRM_RESULT</ServiceParam>";
									strAttributes += " <ServiceParam ServiceParamName='MailAboutNewsletter.ServiceConfirmationMailMediaAppCode'>HTMLT_V3_MAIL_NWL_INSC_CONF</ServiceParam>";
								} else {
									strAttributes += " <ServiceParam ServiceParamName='MailAboutNewsletter.SendMail_Html_Code'>HTMLT_V3_MAIL_NWL_INSC_CONF</ServiceParam>";
									//AOX_Newsletter_SubscriptionConfirmSubject
								}
								strAttributes += "	<ServiceParam ServiceParamName='UserProperty.AcceptNL_push'>1</ServiceParam>";
								strAttributes += "	<ServiceParam ServiceParamName='AllowCreateOrUpdateUserProperties'>1</ServiceParam>";
								strAttributes += "</Service>";
							} else {
								strAttributes += "<Service ServiceName='ServiceAOX_UpdateFOUserAcceptNewsletter'>";
								strAttributes += " <ServiceParam ServiceParamName='FrontOfficeUser.Mail'>" + jQueryObject('#formNewsletterFooter [name="mail"]').val() + "</ServiceParam>";
								strAttributes += " <ServiceParam ServiceParamName='FrontOfficeUser.AcceptNewsletter'>0</ServiceParam>";
								strAttributes += " <ServiceParam ServiceParamName='UserProperty.AcceptNL_push'>0</ServiceParam>";
								strAttributes += " <ServiceParam ServiceParamName='AllowCreateOrUpdateUserProperties'>1</ServiceParam>";
								strAttributes += " <ServiceParam ServiceParamName='AdminActionResultMode'>" + 1 + "</ServiceParam>";
								if (NewsletterFooter_WaitingValidation=='1') {
									// pas de 'waiting unsubscribe' -> forçage en non Waiting
									strAttributes += " <ServiceParam ServiceParamName='AcceptNewsletter_waitingValidation'>0</ServiceParam>";
									strAttributes += " <ServiceParam ServiceParamName='MailAboutNewsletter.SendMail_Html_Code'>HTMLT_V3_MAIL_NWL_UNSC_CONF</ServiceParam>";
									//sujet : AOX_Newsletter_WaitingValidationSubject_Unsubscribe
								} else {
									strAttributes += " <ServiceParam ServiceParamName='MailAboutNewsletter.SendMail_Html_Code'>HTMLT_V3_MAIL_NWL_UNSC_CONF</ServiceParam>";
									//sujet : AOX_Newsletter_UnsubscriptionConfirmSubject
								}
								strAttributes += "</Service>";
							}

							if (typeof addGoogleTrackingEvent == 'function' && typeof bSiteUseTagManagerBy55 != 'undefined') {
								var sNewsletterEventText = ''
								if (bIsInscription) {
									sNewsletterEventText = 'newsletterSubscription';
								}else{
									sNewsletterEventText = 'newsletterUnSubscription';
								}

								//category, action, opt_label, opt_value, opt_noninteraction,oDataLayerConfigComplement
								var oConfig = {
									event:sNewsletterEventText,
									'eventCategory':null,
									'eventAction':null,
									'eventLabel':null,
									'eventNoninteraction':null,
									'pageCategory':sTrackingPageCategory
								}
								addGoogleTrackingEvent('', '', '', null,null,oConfig);
							}

							/* Appel du service en AJAX */
							jQueryObject.post(sUrlAdminAction,{ServiceRequestMode:1,ServicesXML:escape(strAttributes)}, function(data){
								if(jQueryObject(data).children('AjaxXmlResult').length > 0){
									var oXMLResult = jQueryObject(data).children('AjaxXmlResult')[0];                
									var sSucceed = '';
									var oTextResult = '';
									if (bIsInscription) {
										sSucceed = jQueryObject(oXMLResult).find("ServiceResult[ServiceName='ServiceAOX_InscriptionNewsLetter']").attr('eServiceResult');
										oTextResult = jQueryObject(oXMLResult).find("ServiceResult[ServiceName='ServiceAOX_InscriptionNewsLetter']").find('TextResult').first().text();
									} else {
										sSucceed = jQueryObject(oXMLResult).find("ServiceResult[ServiceName='ServiceAOX_UpdateFOUserAcceptNewsletter']").attr('eServiceResult');
										oTextResult = jQueryObject(oXMLResult).find("ServiceResult[ServiceName='ServiceAOX_UpdateFOUserAcceptNewsletter']").find('TextResult').first().text();
									}
									//retour de ServiceAOX_UpdateFOUserAcceptNewsletter -> présence de <div> qui ne passe pas avec le popAlertMessage
									oTextResult = oTextResult.replace('<div>', '');
									oTextResult = oTextResult.replace('</div>', '');

									//fermer popin + popin alert (!! au temps de réaction de la fermeture -> ajout latence
									//jQueryObject('#popinLRPInter').overlay().close();
									setTimeout(function(){
										if (sSucceed=="success"){
											popAlertMessage(oTextResult +'<br />','');
										} else if (sSucceed=="failed"){
											popAlertMessage(oTextResult +'<br />','');
										} else {
											popAlertMessage(NewsletterFooter_ErrorDefault +'<br />','');
										}
									}, 600);
								};
							});
						}
