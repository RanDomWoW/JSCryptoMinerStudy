<script type="text/javascript">
jQuery(function(){
    
    var curHref = window.location.href;
    if(-1 < curHref.indexOf(".html")){
        var hrefArr = curHref.split("/");
        var pid = hrefArr[4];
    }else{
        var hrefArr = curHref.split("=");
        var pid = hrefArr[1];
    }
    jQuery('.ind_jia a').each(function(){
        var pHref = jQuery(this).attr("href");
       
        if(-1 != pHref.indexOf(pid)){
            var liObj = jQuery(this).parent();
            liObj .addClass('w_new').removeClass('w_old');
           //liObj .find('*.n_old').addClass('n_new').removeClass('n_old');
	    liObj .find('*.n_old_t').addClass('n_new_t1').removeClass('n_old_t');
            return;
        }

    })
 	var timer = null;
	jQuery('.ind_jia').hover(function(){
                var _this = jQuery(this);
                _this.addClass('w_new').removeClass('w_old');		
		_this.find('*.n_old_t').addClass('n_new_t').removeClass('n_old_t');
		timer = setTimeout(function(){
                _this.find('*.n_old').addClass('n_new').removeClass('n_old');
			
		},500);

          /*      var _this = jQuery(this);

                _this.addClass('w_new').removeClass('w_old');
		_this.find('*.n_old_t').addClass('n_new_t').removeClass('n_old_t');
			
		_this.find('*.n_old').addClass('n_new').removeClass('n_old');*/
	},function(){
		jQuery(this).removeClass('w_new').addClass('w_old');
		jQuery(this).find('*.n_new').removeClass('n_new').addClass('n_old');
		if(-1 != jQuery(this).find("a").attr("href").indexOf(pid)){
		    return;
		}else{
			jQuery(this).find('*.n_new_t').removeClass('n_new_t').addClass('n_old_t');     	
		}
                clearTimeout(timer);

	  }

	);

})
</script>




