                var EMPTY_SEARCH_STRING = "search...";
            
                function perform_site_search(){
                    var search_query = $("#search_query_field").val();
                    if ( search_query != EMPTY_SEARCH_STRING ){
                        search_query = validate_search_field();
                        if ( search_query.length > 0 ){
                            $("#search-box-form").submit();
                        }
                    }
                }
                
                function reset_search_field(){
                    $("#search_query_field").focus();
                    $("#search_query_field").val("");
                }
                
                function validate_search_field(){
                    $("#search_query_field").val($("#search_query_field").val().replace(/[^a-zA-Z 0-9 -]+/g,""));
                    return $("#search_query_field").val();
                }
            
                $(document).ready(function(){
                
                    $("#search-box-form input").keydown(function (e) {
                        if (e.keyCode == 13) {
                            e.preventDefault();
                            return false;
                        }
                    });
                
                    $("#search_box_magnifier").click(function(){
                        perform_site_search();
                    });
                    $("#search_box_reset").click(function(){
                        reset_search_field();
                    });
                    $("#search_query_field").focus(function(){
                        if ($("#search_query_field").val() == EMPTY_SEARCH_STRING) {
                            $("#search_query_field").val("");
                        }
                    });
                    $("#search_query_field").blur(function(){
                        if ($.trim($("#search_query_field").val()).length == 0){
                            $("#search_query_field").val(EMPTY_SEARCH_STRING);
                        }
                    });
                    $("#search_query_field").keyup(function(e) {
