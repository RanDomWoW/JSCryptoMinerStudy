			if ($('nazione')){
				if ($('nazione').value.length > 0) n++;
				var nazioneval = $('nazione').value;
			} else {
				var nazioneval = '';
			}

			if ($('anno')){
				if ($('anno').value.length > 0) n++;
				var annoval = $('anno').value;
			} else {
				var annoval = '';
			}

