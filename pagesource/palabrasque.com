body,td,th {
	font-family: Verdana, Geneva, sans-serif;
	font-size: 1.2em;
	font-weight: normal;
}

#rounded-corner{font-family:"Lucida Sans Unicode", "Lucida Grande", Sans-Serif;font-size:12px;width:310px;text-align:left;border-collapse:collapse;margin:0px; margin-top:10;}
#rounded-corner thead th.rounded-company{background:#b9c9fe url("http://www.palabrasque.com/css/left.png") left -1px no-repeat;}
#rounded-corner thead th.rounded-q4{background:#b9c9fe url("http://www.palabrasque.com/css/right.png") right -1px no-repeat;}
#rounded-corner th{font-weight:normal;font-size:1em;color:#039;background:#b9c9fe;padding:2px;}
#rounded-corner td{background:#e8edff;border-top:1px solid #fff;color:#669;padding:2px;}

#rounded-corner tfoot td.rounded-foot-left{background:#e8edff url("http://www.palabrasque.com/css/botleft.png") left bottom no-repeat;}
#rounded-corner tfoot td.rounded-foot-right{background:#e8edff url("http://www.palabrasque.com/css/botright.png") right bottom no-repeat;}
#rounded-corner tbody tr:hover td{background:#d0dafd;}

#letra-grande {};
#letra-grande td{}

.resalta {
	color:#B35FFE; text-shadow: 0px -1px 4px white, 0px -2px 10px yellow, 0px -10px 20px #ff8000, 0px -18px 40px red;
}



h1, h2, h3, p, dt, dd, td, li, div {font-family: Arial, Helvetica, sans-serif;}
h1{margin:0px;letter-height: 0px; margin-bottom:0px;text-align:center;padding:0px 2% 0px 2%;background-color:#b9c9fe;color:#000055;font-weight:bold;}
h2 {background-color: #eeeeee;padding:6px;margin:0px;} 
h3 {margin:0px;margin-bottom:0px;background-color: #eeeeee;padding:4px;}
h4 {margin:0px;margin-bottom:0px;}
body {margin:0; padding:0; height:100%; text-align:left;background:#ffffff;}
hr {width: 95%;border: 1px dashed rgb(0, 0, 203);}
a:hover {background-color:yellow;} 
.line1 {background-color: #eeeeee;padding:2px 3px 0px 2px;margin:1px;}   
.c{font-size:smaller;color:#999999;}
.d{font-size:small;color:#777777;}
.top{}
.top a:link {text-decoration: none; color:#000044;}
.top a:visited {text-decoration: none; color:#000044;}
.top a:active {text-decoration: none; color:#000044;}
.top a:hover {text-decoration: underline;color:#000044;background-color:#b9c9fe; }
.e{color:red;font-weight:bold;} 
.rig {position:absolute;right:5px;width:120px;}
.lef {position:absolute;left:5px;width:160px;}
.mid {position:absolute;left:170px;right:30px;padding:2%}
.m{margin:5px 1% 0px 1%;padding:2%}
.f{margin:5px 1% 0px 1%;text-align:center;}
.r{position: absolute;top:5px;left:10px;width: 130px;background:#ffffcc;margin:0px;padding:3px;height:610px;}    
li.inl {display:inline;padding-left:1em;padding-right:1em;border-right:0px;}
li.last {display:inline;padding-left:1em;padding-right:1em;border-right:0px;}  
