                console.log($(e.target).attr('id'));
                if ($(e.target).attr('id') == 'popup_newsltr') {
                    $(this).hide();
                }
            });

            if ($(window).width() < 768) {
                // TODO: find a better fix for iOS scrolling the window trying to center the popup input
                $('#popup_newsltr input').on('focus', function () {
                    $('html, body, #page-wrapper').css({
                        'height': $(window).height(),
                        'overflow': 'hidden'
                    });
                });
                $('#popup_newsltr input').on('blur', function () {
                    $('html, body, #page-wrapper').css({
                        'height': 'auto',
                        'overflow': 'initial'
                    });
                    $('html, body, #page-wrapper').animate({
                        scrollTop: 0
                    });
                });
            }

            $('#popup_newsltr button').on('click touchend', function (e) {
                if ($('#popup_newsltr input').val()) {
                    e.preventDefault();
                    $.ajax({
                        method: 'post',
                        url: window.location.origin + '/newsletter/subscriber/new/',
                        data: {email: $('#popup_newsltr input').val()}
                    }).success(function () {
                        $('#popup_newsltr .form').hide();
                        var el = $('<p></p>');
                        el.addClass('newsletter-promo-code');
                        el.text('ENTER CODE: 10OFF');
                        $('#popup_newsltr .content').append(el);
                    }).error(function () {
                        console.log('An error was experienced whilst attempting to subscribe to newsletter');
                    });
                }

            });
        });

    });
</script>
