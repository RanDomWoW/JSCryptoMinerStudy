<script type="text/javascript">
//EMAR-CARD


function emarCard(){
  
    
  sendAlonePaths=["basket"];

  $.get('/ajax/Ajax_Basket/getBasketDetails', "json_basket", function (response) {
    var basketItems = response["basket_items"]
    var cart = [];
    for (var i = 0; i < basketItems.length; i++) {
      item = {};
      item["price"] = basketItems[i]["product"]["my_price"];
      item["item"] = basketItems[i]["product"]["product_reference"];
      item["quantity"] = parseInt(basketItems[i]["product_quantity"]);
      cart.push(item);
    }
    ScarabQueue.push(['cart', cart]);
    ScarabQueue.push(['go']);
  }, 'json');
  
}
$( document ).ready(function() {
  emarCard();
});


</script>


<script>
  function addToWishlist(productId){
      //e.preventDefault();
      var product_id = productId
      var attributes = $('[name^="attributes['+product_id+']"]:first').serialize();
      var data = { ajax: true, product_id: product_id, attributes: attributes }

      $.get('/ajax/Ajax_Wishlist/addToWishlist', data, function (response) {
        if (response.added_to_wishlist) {
          var animationEnd = 'webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend';

          //$("#wishlist-button-"+productId).attr('class', 'pull-right fa fa-heart fa-lg');
          $("#wishlist-button-"+productId).attr('onclick', 'deleteFromWishlist('+productId+')');
          $("#wishlist-button-"+productId).addClass("animated flash").one(animationEnd, function() {
            $(this).removeClass('animated flash');
          });

          $(".wishlist-navbar-icon").addClass("animated swing").one(animationEnd, function() {
            $(this).removeClass('animated swing');
          });

          //$("#wishlist-button-"+productId+'-mobile').attr('class', 'fa fa-heart fa-lg pull-right');
          $("#wishlist-button-"+productId+'-mobile').attr('onclick', 'deleteFromWishlist('+productId+')');
          $("#wishlist-button-"+productId+'-mobile').addClass("animated flash").one(animationEnd, function() {
            $(this).removeClass('animated flash');
          });
          

        }
      }, 'json');
    }

    function deleteFromWishlist(productId){
      var data = {'product_id': productId};

      $.post('/ajax/Ajax_Wishlist/deleteFromWishlist', data, function (response) {
        if (response.deleted_from_wishlist) {
          $.post('/ajax/Ajax_Wishlist/getWishlist', data, function (response) {
            $('.productlisting_container').html(response.wishlist);
            var animationEnd = 'webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend';

            //$("#wishlist-button-"+productId).attr('class', 'pull-right fa fa-heart-o fa-lg');
            $("#wishlist-button-"+productId).attr('onclick', 'addToWishlist('+productId+')');
            $("#wishlist-button-"+productId).addClass("animated flash").one(animationEnd, function() {
              $(this).removeClass('animated flash');
            });

            //$("#wishlist-button-"+productId+'-mobile').attr('class', 'fa fa-heart-o fa-lg pull-right');
            $("#wishlist-button-"+productId+'-mobile').attr('onclick', 'addToWishlist('+productId+')');
            $("#wishlist-button-"+productId+'-mobile').addClass("animated flash").one(animationEnd, function() {
              $(this).removeClass('animated flash');
            });


          }, 'json');
        } else {
          //$.frontend.alert('Unable to deleted product from wish list', 'Error');
        }
      }, 'json');
    }


    function addToWishlistDetail(productId){
      //e.preventDefault();
      var product_id = productId
      var attributes = $('[name^="attributes['+product_id+']"]:first').serialize();
      var data = { ajax: true, product_id: product_id, attributes: attributes }
      $.frontend.rpc.get('/ajax/Ajax_Wishlist/addToWishlist', data, function (response) {
        if (response.added_to_wishlist) {


          $("#wishlist-button").text("Added to Wishlist");
          var animationEnd = 'webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend';

          $("#wishlist-button").addClass("animated flash").one(animationEnd, function() {
            $(this).removeClass('animated flash');
          });

          /*
          if ($(window).width() < 778) {
            $(".notifyMe").show(0).delay(3200).hide(0);
          }
          */
        }
      }, 'json');

    }


  </script>
  


