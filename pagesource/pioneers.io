<script>
$(function() {
    var mq = window.matchMedia( "only screen and (min-width: 1024px)" );
    if (mq.matches) {
        $('.home-tile .unit').hover(function () {
            var unit = $(this).data('unit');
            $('.hero-heading').hide();
            $('.' + unit).show();
        }, function () {
            $('.hero-heading').hide();
            $('.unit-00').show();
        });


        setTimeout(function() {
            $('.unit-00').show();
            $('video.unit-00')[0].play();

            var shapes = document.querySelectorAll('.header__pattern .shape');
            Object.keys(shapes).forEach(function (key) {
                var shape = shapes[key];
                //line elements don't have getTotalLength()
                if (shape.tagName != 'line') {
                    shape.style.strokeDasharray = shape.getTotalLength() / 0.5;
                    shape.style.animationDelay = (Math.random() * -15) + 's';
                }
                /* shape.addEventListener('click', function () {
                 this.classList.toggle('active');
                 }.bind(shape));*/
            });
        }, 200);
    } else {
        $('.hero-video').hide();
        $('.hero-cover').show();
        $('.hero-heading.unit-00').show();
    }
});

</script>
