	    $(function() {
			// Fixed Nav
			$(window).scroll(function () {
				var scrollTop = $(window).scrollTop();
				if (scrollTop > 100) {
					$("#header_bottom_nav").addClass("fixedNav").stop();
				} else {
					$("#header_bottom_nav").removeClass("fixedNav").stop();
				}
			});
			// Search Grow
			$("#search_box").click(function() {
			    $(this).addClass("growSearch").animate({"width": "200px"}, "700");
			});
			// Slider
			$(".info_open").on("click", function() {
				$("#header_slider_wrapper").slideToggle(700);
				$(".info_open").text('Information');
				$(this).toggleClass("info_close");
				$(".info_close").text('Close Information');
				return false;
			});
			$(".more_links").on("click", function() {
				$("#footer_slider_bg").slideToggle(1200);
				$(".more_links").text('More Links');
				$(this).toggleClass("close_links");
				$(".close_links").text('Close Links');
				return false;
			});
			// Accordion
	        $(".open").on("click", function() {
	            $(this).next().slideToggle('slow');
				$(this).children(".plus_sign").toggleClass("minus_sign");
	            return false;
	        });
			// Tabs
			$(".tab-click").on('click', function() {
				var link = $(this).attr('href');
				$(".tab-click").parent().removeClass("active");
				$(this).parent().addClass("active");
				$(".tab").hide();
				$(link).show();
				return false;
			});
		});
		 /* Responsive Accordian to Tabs */	
			$(document).ready(function () {					
				$('#tabs_nav').children('li').first().children('a').addClass('active')					
					.next().addClass('is-open').show();			
				$('#tabs_nav').on('click', 'li > a', function() {			
				  if (!$(this).hasClass('active')) {							  	
					$('#tabs_nav .is-open').removeClass('is-open').hide();
					$(this).next().toggleClass('is-open').toggle();
			
					$('#tabs_nav').find('.active').removeClass('active');
					$(this).addClass('active');
				  } else {					
					$('#tabs_nav .is-open').removeClass('is-open').hide();
					$(this).removeClass('active');
				  }
			   });
			   
			   var firstheight = $("#tabs_nav li:first").find(".info_tabs_content").height();
			   $("#tabs_nav").css("padding-bottom",firstheight);
			   $("#tabs_nav li").click(function() {
					var height = $(this).find(".info_tabs_content").height();
					$("#tabs_nav").css("padding-bottom",height);	
				});
			});
	
		</script>

