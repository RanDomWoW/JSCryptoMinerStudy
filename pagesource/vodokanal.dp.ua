<script>
    function billingRegister() {
        switch ($('#billing-kind input:checked').val()) {
            case 'pp':
                window.location.href = 'http://cpp.vodokanal.dp.ua/';
                break;
            case 'ent':
                window.location.href = 'http://cent.dneprvodokanal.dp.ua/pindex.php?q=cabinet/enterprises/register';
                break;
        }
    }

    function billingLogin() {

        var checkValue = function (e, p) {
            var v = e.val();
            if (!p.test(v)) {
                v = false;
                e.animate({'background': '#fbb'}, 'fast')
                .animate({'background': '#fff'}, 'slow');
            }
            return v;
        };

        var e = $('#billing-auth input');
        var kind = $('#billing input[name="kind"]:checked').val();
        var pattern = {
            pp: /^[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?$/,
            ent: /^(abn)[0-9]{0,10}$/
        };
        var login = checkValue(e.filter('[name="login"]'), pattern[kind]);
        var password = checkValue(e.filter('[name="password"]'), /^(.*){1,20}$/);

        if (login && password) {
            switch (kind) {
                case 'pp':
                    $.ajax({
                        type: 'POST',
                        url: '/try_auth/try_auth_mcpp.php',
                        dataType: 'json',
                        data: {
                            'login': login,
                            'password': password
                        },
                        beforeSend: function () {
                            // скрыть кнопки
                        },
                        complete: function () {
                            // показать кнопки
                        },
                        success: function (response) {
                            console.log(response);
                            if (response.result.hasOwnProperty('code') && response.result.code == 200) {
                                var aid = response.aid;
                                var path = 'http://cpp.vodokanal.dp.ua/index.html?MCPP_AID=';
                                window.location.href = path + aid;
                            } else {
                                errorTryAuth(result.text);
                            }
                        },
                        error: function (e) {
                            errorTryAuth("Помилка серверу.<br>Спробуйте пізніше чи зверніться до адміністратора сайту");
                        }
                    });
                    break;
                case 'ent':
                    $.ajax({
                        type: 'POST',
                        url: '/try_auth/try_auth_cent.php',
                        dataType: 'json',
                        data: {
                            'login': login,
                            'password': password
                        },
                        beforeSend: function () {
                            // скрыть кнопки
                        },
                        complete: function () {
                            // показать кнопки
                        },
                        success: function (response) {
                            if (response.result.hasOwnProperty('code') && response.result.code == 200) {
                                $('[name=CENT_AID]').val(response.aid);
                                $('#aid').submit();
                            } else {
                                errorTryAuth(result.state.text);
                            }
                        },
                        error: function (e) {
                            errorTryAuth("Помилка серверу.<br>Спробуйте пізніше чи зверніться до адміністратора сайту");
                        }
                    });
                    break;
            }
        } else {
            var format = '';
            switch (kind) {
                case 'pp':
                    format = 'email@mail.com';
                    break;
                case 'ent':
                    format = 'abn0000'
                    break;
                default:
            }
            errorTryAuth("Заполните корректно поля: <br> <i>'Имя пользователя'</i> в формате " + format + "<br> и <i>'Пароль'</i>");
        }
    }

    var errorTryAuth = function (text) {
        $('#modal_window h1').css('color', 'red');
        $('#vidgets_error').html(text);
        modalWindow();
    };

    var modalWindow = function () {
        $('#overlay').fadeIn(400, function () {
            $('#modal_window').css('display', 'block').animate({opacity: 1, top: '50%'}, 200);
        });

        $('#modal_close, #overlay').click(function () {
            $('#modal_window').animate({
                    opacity: 0, top: '45%'
                },
                200,
                function () {
                    $(this).css('display', 'none');
                    $('#overlay').fadeOut(400);
                });
        });

    }
</script>
