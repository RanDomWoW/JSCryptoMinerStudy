<script>
    $(document).ready(function () {
        var msg = function () {
            this.resp = {
                full_name: "",
                address: "",
                phone: "",
                email: "",
                text: ""
            };

            this.set = function (key, val) {
                this.resp[key] = val;
            };
            this.reset = function (key, val) {
                this.resp[key] = '';
            };
            this.get = function (key) {
                return this.resp[key];
            };
            this.getAll = function () {
                return this.resp;
            };
        }
        dataMsg = new msg();

        $('[data-target=target]').blur(function () {
            var name = $(this).attr("name");
            var value = $(this).val();
            dataMsg.set(name, value);
        });

        $('[data-target=sendMsg]').click(function () {
            var data = dataMsg.getAll();
            for (var key in data) {
                if (!data[key]) {
                    $('#modal_window h1').css('color', 'red');
                    $('#vidgets_error').html('Для отправки сообщения заполните <br> корректно все поля.');
                    return modalWindow();
                }
            }
            sendMsg(data);
        });

        var sendMsg = function (data) {
            $.post(
                "/inf/services/message_ru.php",
                {
                    data: data
                },
                function (resp) {
                    var answer = JSON.parse(resp),
                        txt = answer.text,
                        len = txt.length;
                    if (len < 100) {
                        $('#modal_window h1').css('color', 'red');
                        $('#vidgets_error').html(txt);
                        modalWindow();
                    } else {
                        $('textarea').val('');
                        dataMsg.set('text', '')
                        $('#modal_window h1').css('color', 'lime');
                        $('#vidgets_error').html(txt);
                        modalWindow();
                    }
                }
            );
        }

        $('#feedback :input').keypress(function (keyEvent) {
            var codeKey = keyEvent.keyCode
            var fieldName = $(this).attr('name');
            switch (fieldName) {
                case 'full_name':
                    if (!((codeKey > 1040 && codeKey < 1103) || codeKey == 32 || codeKey == 1028 || codeKey == 1030 || codeKey == 1103 || codeKey == 1105 || codeKey == 1108 || codeKey == 1110)) {
                        event.preventDefault();
                    }
                    break;
                case 'address':
                    if ((codeKey > 64 && codeKey < 91) || (codeKey > 96 && codeKey < 123)) {
                        event.preventDefault();
                    }
                    break;
                case 'phone':
                    if (!(codeKey > 48 && codeKey < 58) || $('.num').val().length > 12) {
                        event.preventDefault();
                    }
                    break;
                case 'email':
                    if ((codeKey > 1039 && codeKey < 1102) || codeKey == 1025 || codeKey == 1105 || codeKey == 1110) {
                        event.preventDefault();
                    }
                    break;
                default:
                    return;
            }
        });

    });
</script>
