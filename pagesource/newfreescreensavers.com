<script language="javascript">
$(document).ready(function(){
    $('#curr_lang_div').click(function(){$('#langs_div').css("display", "block"); return false;});
    $('body').click(function(){$('#langs_div').css("display", "none");});

	$('#langs_div a').click(function () { 
		var str;
		var cook_var;
		str=''+$(this).attr('id'); 
		cook_var = getCookie("exp_locale");
		if (cook_var != str){
			var expire_date = new Date;
	       	 	expire_date.setTime(expire_date.getTime() + (3600*24*31*1000));
			time = expire_date.toGMTString();
			setCookie("exp_locale", str, time);
		}
   	 });
function setCookie (name, value, expires, path, domain, secure) {
      document.cookie = name + "=" + escape(value) +
        ((expires) ? "; expires=" + expires : "") +
        "; path=/" +
        ((domain) ? "; domain=" + domain : "") +
        ((secure) ? "; secure" : "");
}



function getCookie(name) {
	var cookie = " " + document.cookie;
	var search = " " + name + "=";
	var setStr = null;
	var offset = 0;
	var end = 0;
	if (cookie.length > 0) {
		offset = cookie.indexOf(search);
		if (offset != -1) {
			offset += search.length;
			end = cookie.indexOf(";", offset)
			if (end == -1) {
				end = cookie.length;
			}
			setStr = unescape(cookie.substring(offset, end));
		}
	}
	return(setStr);
}
});
</script>
