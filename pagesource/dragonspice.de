					document.getElementById("f.search.param").parentElement.focus();
				document.getElementById("f.search.param").focus();*/
			}</script> <script type="text/javascript" src="https://www.dragonspice.de/out/dgragonspice/src/js/widgets/oxajax.js?1465230175"></script> <script type="text/javascript" src="https://www.dragonspice.de/out/dgragonspice/src/js/widgets/oxcountdown.js?1465230176"></script> <script type="text/javascript" src="https://www.dragonspice.de/out/dgragonspice/src/js/widgets/oxminibasket.js?1465230177"></script> <script type="text/javascript" src="https://www.dragonspice.de/out/dgragonspice/src/js/widgets/oxmodalpopup.js?1465465975"></script> <script type="text/javascript">$( '#miniBasket' ).oxMiniBasket();</script> <!--[if (gte IE 6)&(lte IE 8)]> <script type="text/javascript" src="https://www.dragonspice.de/out/dgragonspice/src/js/libs/IE9.js"></script> <![endif]--> </body> </html>