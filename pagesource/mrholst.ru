    .bkg_shad {
        background: #000000;
        opacity: 0.5;
        position: fixed;
        left: 0px;
        top: 0px;
        width: 100%;
        height: 100%;
        display: none;
        z-index: 20;
    }

    .oferta_view {
        display: none;
        width: 800px;
        border: 1px solid #ffffff;
        background: #FFFFFF;
        overflow: auto;
        border-radius: 5px;
        padding: 15px;
        position: fixed;
        left: 50%;
        top: 3%;
        margin-left: -400px;
        color: #000000;
        font-size: 11px;
        font-family: ms sans serif;
        text-shadow: none;
        z-index: 25;
    }

</style>
<script type="text/javascript">
    $(document).ready(function () {
        $('.oferta').click(function () {
            $.ajax({
                url: '/l/oferta.php',
                type: 'POST',
                success: function (data) {
                    $('.oferta_view').html(data);
                    $('.bkg_shad').fadeIn();
                    $('.oferta_view').fadeIn();

                    $('.bkg_shad').click(function () {
                        $('.bkg_shad').fadeOut();
                        $('.oferta_view').fadeOut();
                    });
                }
            });
        });
    });
</script>


