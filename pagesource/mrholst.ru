<script>
    function navigation_scroll() {

        var scroll = $(document).scrollTop();
        if (scroll > 190) {
            $('.topstock').addClass('fixedstock');
        }
        else {
            $('.topstock').removeClass('fixedstock');
        }

    }

    function scrollmenutop() {

//	$('html, body').animate({ scrollTop: $('#section-baget').offset().top - 190}, 500);

    }

    $(document).ready(function () {

        navigation_scroll();

        
        /**Промокод**/

        $(".promoform").submit(function (event) {
            event.preventDefault();

            if ($(this).find('input[name="phone"]').val() != '') {
                $.post("promocode.php", $(this).serialize(), function (result) {

                    if (result == 1) {
                        alert('Вы не ввели номер телефона');
                    } else if (result == 2) {
                        alert('Вы не ввели промокод');
                    } else if (result == 3) {
                        alert('Введенный вами промокод не существует. Введите корректный промокод или позвоните по телефону 8(800)700 93-89 (Бесплатно по всей России)');
                    } else {
                        $.fancybox({
                            content: result
                        });
                        $('.promoform')[0].reset();
                    }
                });
            } else {
                alert('Вы не ввели номер телефона');
            }
        });

        /**Промокод**/


    });

    $(document).scroll(function () {

        navigation_scroll();

    });
</script>

