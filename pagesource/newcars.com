<script type="text/javascript">
var qq_dropdowns, cc_dropdowns;


var Local = {};

var mvc = document.forms.Quote;


Local.onJSLoaded = function () {

  qq_dropdowns = new DD.MakesModels(999,999,0,'qq_make_select','qq_model_select','qq_year_toggle','',{id:'qq_make_id',onchange:'Local.make(qq_dropdowns,this.value);',disabled:'true'},{id:'qq_model_id',onchange:'Local.model(qq_dropdowns,this.value);',disabled:'true'},{id:'qq_year',name:'qq_year',onclick:'qq_dropdowns.year=this.value;this.checked=true;'});
  cc_dropdowns = new DD.MakesModels(999,999,0,'cc_make_select','cc_model_select','cc_year_toggle','',{name:'cc_make_id',id:'cc_make_id',onchange:'Local.make(cc_dropdowns,this.value);',disabled:'true'},{name:'cc_model_id',id:'cc_model_id',onchange:'Local.model(cc_dropdowns,this.value);',disabled:'true'},{id:'cc_year',name:'cc_year'});
  QU.MMpreload();



  Watermarks.addInput(Com.$('qq_zipcode'), 'ZIP');
};

Local.DDinit = function () {
  qq_dropdowns.getMakes();
  cc_dropdowns.getMakes();
};

Local.turnON = function (obj) {
  if (obj == null) obj = qq_dropdowns;
	if (obj == qq_dropdowns) {
	Com.$('qq_make_id').disabled = false;
	if (parseInt(obj.mki, 10) !== 999) Com.$('qq_model_id').disabled = false;
	Com.$('qq_submit').disabled = false;
  }
  else {
	Com.$('cc_make_id').disabled = false;
	if (parseInt(obj.mki, 10) !== 999) Com.$('cc_model_id').disabled = false;
	Com.$('cc_submit').disabled = false;
  }
};

Local.turnOFF = function (obj) {
    // emptied because we don't need to turn stuff off
};

Local.make = function (obj, make_id) {
	obj.mki = parseInt(make_id, 10);
	obj.mdi = 999;
	obj.year = 0;
	obj.getModels();
};

Local.model = function (obj, model_id) {
	obj.mdi = parseInt(model_id, 10);
	obj.year = 0;
	obj.getModelYears();
};

Local.redirect = function () {
  var sel =  Com.$('cc_make_id').selectedIndex;
  var make = Com.$('cc_make_id').options[sel].text;
  sel = Com.$('cc_model_id').selectedIndex;
  var model = Com.$('cc_model_id').options[sel].text;
  if ((make != "Select a Make") && (model != "Select a Model")) {
	  make = make.toLowerCase().replace(/[\s-]/g,'')
	  model = model.toLowerCase().replace(/[\s-]/g,'')
	  window.location = "/" + make + "/" + model + "/index.html";
  }
  else alert('Please select a Make and Model.');
};

// Dealer Search

var dealerSearch = new MakeZipcodeSelector({
  make_element_id: 'find_dealer_make',
  zipcode_element_id: 'find_dealer_zipcode',
  submit_element_id: 'find_dealer_submit',
  connector: new DealerSearchConnector()
});


</script>


