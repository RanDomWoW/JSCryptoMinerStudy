function in_array(value, array) { for(var i = 0; i < array.length; i++) if(array[i] == value) return true; return false; }			
$(".timer").each(function( index ) { 
  var ts = new Date($(this).attr("data-time"));
  var span = $(this).find("span"); 
  $(this).countdown({timestamp: ts, callback: function(days, hours, minutes, seconds){
    var time_left = "";
    var hs = [1,21]; var hsa = [2,3,4,22,23,24];
    if (hours != 0){
      if (in_array(hours,hs)) time_left += hours + " час "; 
      else {
        if (in_array(hours,hsa)) time_left += hours + " часа "; 
        else time_left += hours + " часов "; 
      }  
    }  
    var ma = [1,21,31,41,51]; var mta = [2,3,4,22,23,24,32,33,34,42,43,44,52,53,54]; 
    if (in_array(minutes,ma)) time_left += minutes + " минута"; 
    else{
      if (in_array(minutes,mta)) time_left += minutes + " минуты"; 
      else time_left += minutes + " минут"; 
    }  
    span.html(time_left);
  }});
});
			
