        <h1 id="ControlBoxHeader">Maze Generator</h1>
        <div>
            <div class="ControlBox">
                <div class="ShapeSection">
                    <label for="ShapeDropDownList" id="ShapeLabel">Shape:</label>
                    <select name="ShapeDropDownList" id="ShapeDropDownList">
	<option selected="selected" value="1">Rectangular</option>
	<option value="2">Circular</option>
	<option value="3">Triangular</option>
	<option value="4">Hexagonal</option>

</select>
                </div>
                <table class="Settings">
                    
                            <tr>
                                <td class="SettingLabel">
                                    <label for="S1TesselationDropDownList" id="S1StyleLabel">Style:</label>
                                </td>
                                <td class="SettingValue">
                                    <select name="S1TesselationDropDownList" id="S1TesselationDropDownList">
	<option selected="selected" value="1">Orthogonal (Square cells)</option>
	<option value="2">Sigma (Hexagonal cells)</option>
	<option value="3">Delta (Triangular cells)</option>

</select>
                                </td>
                            </tr>
                            <tr>
                                <td class="SettingLabel">
                                    <label for="S1WidthTextBox" id="S1WidthLabel">Width:</label>
                                </td>
                                <td class="SettingValue">
                                    <input name="S1WidthTextBox" type="text" value="20" maxlength="5" id="S1WidthTextBox" />
                                    (2 to 200 cells)
                                </td>
                            </tr>
                            <tr>
                                <td class="SettingLabel">
                                    <label for="S1HeightTextBox" id="S1HeightLabel">Height:</label>
                                </td>
                                <td class="SettingValue">
                                    <input name="S1HeightTextBox" type="text" value="20" maxlength="5" id="S1HeightTextBox" />
                                    (2 to 200 cells)
                                </td>
                            </tr>
                            <tr>
                                <td class="SettingLabel">
                                    <label for="S1InnerWidthTextBox" id="S1InnerWidthLabel">Inner width:</label>
                                </td>
                                <td class="SettingValue">
                                    <input name="S1InnerWidthTextBox" type="text" value="0" maxlength="5" id="S1InnerWidthTextBox" />
                                    (0 or 2 to width - 2 cells)
                                </td>
                            </tr>
                            <tr>
                                <td class="SettingLabel">
                                    <label for="S1InnerHeightTextBox" id="S1InnerHeightLabel">Inner height:</label>
                                </td>
                                <td class="SettingValue">
                                    <input name="S1InnerHeightTextBox" type="text" value="0" maxlength="5" id="S1InnerHeightTextBox" />
                                    (0 or 2 to height - 2 cells)
                                </td>
                            </tr>
                            <tr>
                                <td class="SettingLabel">
                                    <label for="S1StartsAtDropDownList" id="S1StartsAtLabel">Starts at:</label>
                                </td>
                                <td class="SettingValue">
                                    <select name="S1StartsAtDropDownList" id="S1StartsAtDropDownList">
	<option selected="selected" value="1">Top</option>
	<option value="2">Bottom or inner room</option>

</select>
                                </td>
                            </tr>
                        
                    <tr>
                        <td class="SettingLabel">
                            Advanced:
                        </td>
                        <td class="SettingValue">
                            <label for="AlgorithmParameter1TextBox" id="AlgorithmParameter1Label">E:</label>
                            <input name="AlgorithmParameter1TextBox" type="text" value="50" maxlength="5" id="AlgorithmParameter1TextBox" />
                            (0 to 100), 
                            <label for="AlgorithmParameter2TextBox" id="AlgorithmParameter2Label">R:</label>
                            <input name="AlgorithmParameter2TextBox" type="text" value="100" maxlength="5" id="AlgorithmParameter2TextBox" />
                            (0 to 100)
                        </td>
                    </tr>
                </table>
