<script>
  if (window.matchMedia('(max-width: 600px)').matches) {
    $('#form-toggle .button-container .toggle-keyword-form:first').text('#hashtag/keyword');
  } else {
    $('#form-toggle .button-container .toggle-keyword-form:first').text('#hashtag');
  }
</script>

<script>
  var hashtagTerm = '',
      accountTerm = '',
      keywordTerm = '',
      mentionTerm = '',
      urlTerm = '';
  $('#form-toggle .button-container').on('click', 'a', function(e) {
    eventTrack({
      category: 'General',
      action: 'search--switchProduct',
      properties: {
        from: $('#form-toggle .button-container a.active').text().replace(/[^a-z0-9]/gi, '').toLowerCase(),
        to: $(this).text().replace(/[^a-z0-9]/gi, '').toLowerCase()
      }
    });

    if ($('#form-toggle .active').text().indexOf('hashtag') > -1) {
      hashtagTerm = $('.keyword-tracking-form input#search').val();
    } else if ($('#form-toggle .active').text().indexOf('account') > -1) {
      accountTerm = $('.account-tracking-form input#search').val();
    } else if ($('#form-toggle .active').text().indexOf('keyword') > -1) {
      keywordTerm = $('.keyword-tracking-form input#search').val();
    } else if ($('#form-toggle .active').text().indexOf('mention') > -1) {
      mentionTerm = $('.keyword-tracking-form input#search').val();
    } else if ($('#form-toggle .active').text().indexOf('URL') > -1) {
      urlTerm = $('.keyword-tracking-form input#search').val();
    }

    $('#form-toggle .button-container a').removeClass('active');
    $(this).addClass('active');

    if ($(this).attr('class').indexOf('keyword') > -1) {
      $('.preview-forms-container form').removeClass('form-active');
      $('.keyword-tracking-form').addClass('form-active');
    } else if ($(this).attr('class').indexOf('account') > -1) {
      $('.preview-forms-container form').removeClass('form-active');
      $('.account-tracking-form').addClass('form-active');
    }

    if ($(this).text().indexOf('hashtag') > -1) {
      $('.keyword-tracking-form input#search').attr('placeholder', 'Enter a #hashtag to track');
      $('.keyword-tracking-form input#search').val(hashtagTerm);
      $('.keyword-tracking-form input#search').focus();
      document.getElementById('live-example').setAttribute('href', '/realtime/5J2coj/SEO');
    } else if ($(this).text().indexOf('account') > -1) {
      if ($('.account-tracking-form input[type=radio]:checked').attr('value') == 'twitter') {
        $('.account-tracking-form input#search').attr('placeholder', 'Enter a Twitter @account to analyze its activity');
      } else if ($('.account-tracking-form input[type=radio]:checked').attr('value') == 'instagram') {
        $('.account-tracking-form input#search').attr('placeholder', 'Enter an Instagram @account to analyze its activity');
      } else if ($('.account-tracking-form input[type=radio]:checked').attr('value') == 'facebook') {
        $('.account-tracking-form input#search').attr('placeholder', 'Enter a Facebook @page to analyze its activity');
      }
      $('.account-tracking-form input#search').val(accountTerm);
      $('.account-tracking-form input#search').focus();
    } else if ($(this).text().indexOf('keyword') > -1) {
      $('.keyword-tracking-form input#search').attr('placeholder', 'Enter a keyword to track');
      $('.keyword-tracking-form input#search').val(keywordTerm);
      $('.keyword-tracking-form input#search').focus();
      document.getElementById('live-example').setAttribute('href', '/realtime/BXrv5B/audi');
    } else if ($(this).text().indexOf('mention') > -1) {
      $('.keyword-tracking-form input#search').attr('placeholder', 'Enter a username to track its @mentions');
      $('.keyword-tracking-form input#search').val(mentionTerm);
      $('.keyword-tracking-form input#search').focus();
      document.getElementById('live-example').setAttribute('href', '/realtime/BXrv5B/audi');
    } else if ($(this).text().indexOf('URL') > -1) {
      $('.keyword-tracking-form input#search').attr('placeholder', 'Enter a URL to track');
      $('.keyword-tracking-form input#search').val(urlTerm);
      $('.keyword-tracking-form input#search').focus();
      document.getElementById('live-example').setAttribute('href', '/realtime/qEkBao/Giltcom');
    }

    if (!$(this).is('.toggle-keyword-form:first')) {
      $('label#ig-box').hide();
    } else {
      $('label#ig-box').show();
    }

    e.preventDefault();
  });
</script>

