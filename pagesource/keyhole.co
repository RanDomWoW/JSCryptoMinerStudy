<script>
  function autoSearch() {
    $('.account-tracking-form input#search').on('keyup', throttle(function(){
      var searchTerm = $(this).val();
      var socialnetwork = $('.account-tracking-form .platform-select input[type=radio]:checked').attr('value');
      // check if searchterm is a URL and parse socialnetwork and username
      if (isURL(searchTerm)) {
        socialnetwork = extractPlatformFromURL(searchTerm);
        searchTerm = extractUsernameFromURL(searchTerm);
      }

      var url = ws + '/tracking/' + socialnetwork + '/autocomplete.json';
      var request = {'screen_name': searchTerm, 'user': apiKey};

      $('.account-tracking-form .suggestions').remove();
      if (searchTerm != '' && searchTerm != ' ' && searchTerm != undefined) {
        $.post(url, request)
          .done(function(data) {
            var suggestions = data.response.data;
            if (!suggestions)
              suggestions = [{full_name: "No suggestions found.", id: 0}];

            if (suggestions.length >= 5) {
              suggestions = suggestions.slice(0,4);
            }

            $('.account-tracking-form .search-box').append('<div class="suggestions ' + socialnetwork + '"></div>');

            for (var i = 0; i < suggestions.length; i++) {
              var suggestion = suggestions[i];
              if (suggestion.id == 0){
                $('.account-tracking-form .suggestions').append('<a class="suggestion no-suggestion"><div class="suggestion--account-info"><h4 class="suggestion--account-name">' + suggestion.full_name + '</h4></div></a>');
              } else if (socialnetwork == 'twitter') {
                $('.account-tracking-form .suggestions').append('<a href="#" class="suggestion" data-id="' + suggestion.username + '"><img src="' + suggestion.picture + '"><div class="suggestion--account-info"><h4 class="suggestion--account-name">' + suggestion.full_name + '</h4><div class="suggestion--account-meta"><span class="suggestion--account-username">@' + suggestion.username + '</span> &centerdot; <span class="suggestion--followers">' + numFormat(suggestion.followed_by) + '</span> followers</div></div></a>');
              } else if (socialnetwork == 'instagram') {
                $('.account-tracking-form .suggestions').append('<a href="#" class="suggestion" data-id="' + suggestion.username + '"><img src="' + suggestion.picture + '"><div class="suggestion--account-info"><h4 class="suggestion--account-name">@' + suggestion.username + '</h4><div class="suggestion--account-meta"><span class="suggestion--account-username">' + suggestion.full_name + '</span></div></div></a>');
              } else if (socialnetwork == 'facebook') {
                $('.account-tracking-form .suggestions').append('<a href="#" class="suggestion" data-id="' + suggestion.id + '"><img src="' + suggestion.picture + '"><div class="suggestion--account-info"><h4 class="suggestion--account-name">' + suggestion.name + '</h4><div class="suggestion--account-meta"><span class="suggestion--category">' + suggestion.category + '</span> &centerdot; <span class="suggestion--likes">' + numFormat(suggestion.likes) + '</span> likes</div></div></a>');
              } else if (socialnetwork == 'youtube') {
                $('.account-tracking-form .suggestions').append('<a href="#" class="suggestion" data-id="' + suggestion.id + '"><img src="' + suggestion.picture + '"><div class="suggestion--account-info"><h4 class="suggestion--account-name">' + suggestion.screen_name + '</h4><div class="suggestion--account-meta"><span class="suggestion--category">' + suggestion.full_name + '</span></div></div><input class="snippet" type="hidden" value="'+ escapeHtml(suggestion.snippet) + '"/></a>');
              }

              if (suggestion.is_verified == true) {
                $('.account-tracking-form .suggestions .suggestion:last-child h4.suggestion--account-name').append('<img class="verified" src="/img/icons/verified.png">');
              }
            }

            $('.suggestion').first().focus();

            // navigate suggestions using up/down arrow keys
            $('.suggestions').keydown(function(e) {
              if (e.keyCode == 39 || e.keyCode == 40 || e.keyCode == 9) {
                $(".suggestion:focus").next().focus();
                eventTrack({
                  'category': 'Account Tracking',
                  'action': 'search__autocomplete--moveDown',
                  'properties': {
                    'search': $('.account-tracking-form input#search').val(),
                    'platform': socialnetwork,
                  }
                });
              } else if (e.keyCode == 37 || e.keyCode == 38) {
                $(".suggestion:focus").prev().focus();
                eventTrack({
                  'category': 'Account Tracking',
                  'action': 'search__autocomplete--moveUp',
                  'properties': {
                    'search': $('.account-tracking-form input#search').val(),
                    'platform': socialnetwork,
                  }
                });
              } else if (e.keyCode == 13) {
                magicSearch($('.suggestion:focus').data('id'), socialnetwork);
              } else if (e.keyCode == 8 || e.keyCode == 46) {
                $('.account-tracking-form input#search').val($('.account-tracking-form input#search').val().slice(0, -1)).focus();
              } else {
                $('.account-tracking-form input#search').val($('.account-tracking-form input#search').val() + String.fromCharCode(e.keyCode).toLowerCase()).focus();
              }
              e.preventDefault();
            });

            // hidden form submit when clicking suggestion
            $('.suggestions .suggestion').off('click').on('click', function(e) {
              if (!$(this).hasClass('no-suggestion'))
                if ( socialnetwork == 'youtube' ) {
                  magicSearch($(this).data('id'), socialnetwork, $(this).children('.snippet').val());
                } else {
                  magicSearch($(this).data('id'), socialnetwork);
                }
              e.preventDefault();
            });

            function magicSearch(searchTerm, socialnetwork, snippet) {
              var innerSearch = searchTerm;
              var innerSocialNetwork = socialnetwork;

              eventTrack({
                'category': 'Account Tracking',
                'action': 'search__autocomplete--click',
                'properties': {
                  'search': $('.account-tracking-form input#search').val(),
                  'platform': socialnetwork,
                  'account': searchTerm
                }
              });

              var form = document.createElement("form");
              form.setAttribute("method", "post");
              form.setAttribute("action", "/account-tracking/preview");

              var searchInput = document.createElement('input');
              searchInput.type = 'hidden';
              searchInput.name = 'search';
              searchInput.value = innerSearch;
              form.appendChild(searchInput);

              var networkInput = document.createElement('input');
              networkInput.type = 'hidden';
              networkInput.name = 'platform_acc';
              networkInput.value = innerSocialNetwork;
              form.appendChild(networkInput);

              if ( snippet !== null ) {
                var snippetInput = document.createElement('input');
                snippetInput.type = 'hidden';
                snippetInput.name = 'snippet';
                snippetInput.value = snippet;
                form.appendChild(snippetInput);
              }

              document.body.appendChild(form);
              form.submit();
              document.body.removeChild(form);
            }
          });
      }
    }));

    $('body').on('click', function() {
      $('.suggestions').remove();
    });
  }

  $(document).ready(function() {
    autoSearch();
  });
</script>

