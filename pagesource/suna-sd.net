<script>
$(function() {

var isFeedbackSent=$.cookie('isFeedbackSent');

if(isFeedbackSent){

         $( "#feedbackbutton_text" ).text('شكرا لمشاركتك!');
         $( "#feedback_button" ).addClass('feedback_sent');
        }
 
else    {
        $( "#feedback_button" ).on('click',showFeedback); 
        $( "#close_feedback_form" ).on('click',hideFeedback);
        $('#send_feedback').on('click',send_feedback);
        }
$('.radio').on('click',function () {
    
    $(this).find('input').removeAttr('checked');
    $(this).siblings().removeClass('checked');
    
    $(this).find('input').attr('checked',true);
    $(this).addClass('checked');
});


});

    var overall_quality="";
    var ease_of_use="";
    var news_quality="";
    var extra_feedback="";
function showFeedback () {
   
    $(this).animate({'bottom':'-50px'},700,'easeOutQuart');

   
    $( "#feedback_form" ).animate({'bottom':'-10px'},900,'easeOutQuart'); 

}
function hideFeedback () {
  
    $("#feedback_button").animate({'bottom':'-10px'},1000,'easeOutQuart');
    $( "#feedback_form" ).animate({'bottom':'-570px'},1000,'easeOutQuart'); 
    }

function send_feedback () {
        
    overall_quality=$('input[name="overall_quality"]:checked').attr('value');
    ease_of_use=$('input[name="ease_of_use"]:checked').val();
    news_quality=$('input[name="news_quality"]:checked').val();
    extra_feedback=$('#extra_feedback').val();
    
        
    if(!(overall_quality&&ease_of_use&&news_quality)){
    
        alert('أجب عن جميع الأسئلة من فضلك!');

    }
    else{   

        showAjaxIndecators();
        doAjaxSend();
    
        } /*end else*/

    }

function doAjaxSend () {
            data="overall_quality="+overall_quality+"&ease_of_use="+ease_of_use+"&news_quality="+news_quality+"&extra_feedback="+extra_feedback;
            $.ajax({
            
            type: 'POST',
            url:'http://www.suna-sd.net/feedback/update_db',
            data:data,
            success: function(html) {
         
                feedbackDone();

            }
    } );    /*end ajax stuff*/
}    

function showAjaxIndecators () {
    
    $('.question,hr,#send_feedback').fadeOut();
    $( "#feedback_form" ).animate({'height':'100px'},900,'easeOutQuart',function(){

            $('#ajax_info').fadeIn();

    });
      
    
  
   }   
function feedbackDone() {
            $('#ajax_info img,#ajax_message').fadeOut();
            $('#ajax_message').text('شكرا لك! تم إرسال البيانات!');
            $('#ajax_message').fadeIn();
           $.cookie('isFeedbackSent',true);
