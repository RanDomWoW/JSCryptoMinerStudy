<script type="text/javascript">
        // <![CDATA[

        var dropdown = function () {
                $(".ui.dropdown").dropdown({
                        on: "hover"
                });
                //$(".menu.toggle").click(function () {
                //    $(".menu.toggle").removeClass("active");
                //        $(this).toggleClass("active");
                //});
                $(".ui.checkbox").checkbox();
                $("#menu_toggle").on("click", function () {
                        $("menu_toggle").transition('horizontal flip', '1000ms').transition('horizontal flip', '1000ms')
                })
        }

        $(document).ready(function () {
                console.log("ready!");
                dropdown();

                function getTarget(e) {
                        console.log("Finding event target");
                        if (!e) {
                                e = window.event;
                        }
                        return e.target || e.srcElement;

                }

                function itemDone(e) {
                        var target, targetID,
                                target = getTarget(e);
                        targetID = target.id;
                        console.log(targetID);
                }


                var parentItems = document.getElementsByClassName('parent item');
                $(parentItems).on("click", function (e) {
                        var index = $(parentItems).index(this);
                        console.log('event fired');
                        e.preventDefault();
                        var showChildMenu = $(parentItems[index]).next();
                        showChildMenu.transition({
                                animation: "slide down",
                                onComplete: function () {
                                        if (showChildMenu.hasClass("hidden")) {
                                                showChildMenu.hide();
                                                console.log('show');
                                        } else {
                                                showChildMenu.show();
                                                console.log('hide');
                                        }
                                }
                        });

                })

                $("#main_navigation .menu.toggle").on("click", function (e) {

                        e.preventDefault();

                        var menu = $("#main_navigation .ui.vertical.menu");
                        menu.transition({
                                animation: "slide down",
                                onComplete: function () {
                                        if (menu.hasClass("hidden")) {
                                                menu.hide();
                                        } else {
                                                menu.show();
                                        }
                                }
                        });
                })

				let btn = document.getElementById('searchButton');

                function launchModal() {
                    $('#search-modal')
                        .modal({
                            autofocus: true,
                            closable: true,
                            inverted: true,
                            transition: 'fade up'
                        })
                        .modal('show');
                }
                if (btn)
                    btn.addEventListener('click', launchModal, false);
                else
                    console.log('this btn doesn\'t exist.');
	
                $(window).on("resize", function () {
                        var viewportWidth = $(window).width();
                        if (viewportWidth > 515)
                                $("#footer__top-part").removeClass("doubling stackable");
                        else
                                $("#footer__top-part").addClass("doubling stackable");
                }).resize();
                //lazy load images
                function init() {
                        var imgDefer = document.getElementsByTagName('img');
                        for (var i = 0; i < imgDefer.length; i++) {
                                if (imgDefer[i].getAttribute('data-src'))
                                        imgDefer[i].setAttribute('src', imgDefer[i].getAttribute('data-src'));
                        }
                }
                window.onload = init();
			//autocomplete
            jQuery('#searchInput').qc({
                program: 'https://search.np.edu/s/suggest.json',
                alpha: '0.5',
                show: '10',
                sort: '0',
                length: '2',
                datasets: {
                    organic: {
                        name: 'Suggestions',
                        collection: 'npc-search',
                        profile: '_default',
                        show: '10'
                    },
                    people: {
                        name: 'People',
                        collection: 'npc-staff-custom',
                        profile: 'auto-completion',
                        show: '3'
                        , template: {
                            suggestion: jQuery('#auto-completion-people').text()
                        }
                    },
                    courses: {
                        name: 'Courses',
                        collection: 'npc-courses-custom',
                        profile: 'auto-completion',
                        show: '5'
                        , template: {
                            suggestion: jQuery('#auto-completion-courses').text()
                        }
                    }
                }

            });
                
		});
      // ]]>

</script>
