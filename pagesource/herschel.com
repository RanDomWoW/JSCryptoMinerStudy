<script type="text/template" id="hsco-cart-item-template">
  <% _.each(products, function(model) { %>
  <div class="row row--vertical-center m-b-2 text-cta hsco-row-model bfx-minicart-product" data-carturl="<%= model.cartUrl %>">
    <div class="col-xs-2">
      <a href="<%= model.link %>" data-gwp="<%=model.giftWithPurchase%>">
        <img class="bfx-product-image" src="<%= model.image_path %>.sthumbnails.330.330.jpg" alt="<%= model.display_name %>">
      </a>
    </div>
    <div class="col-xs-4">
      <a class="link-hover--grey-light" href="<%= model.link %>" data-gwp="<%=model.giftWithPurchase%>">
        <p class="m-y-0 bfx-product-name"><%= model.display_name %></p>
        <% if (!model.oneSize){%>
        <p class="m-y-0 bfx-product-desc">(<%= model.size %>)</p>
        <%}%>
        <% if (model.to){%>
        <p class="m-y-0">To: <%= model.to %></p>
        <%}%>
        <p class="m-y-0 text-copy2 bfx-product-color"><%= model.color_name %></p>
        <p class="bfx-sku" style="display:none"><%=model.sku%></p>
      </a>
    </div>
    <div class="col-xs-5">
      <div class="quantity-selector" <% if (model.to) {%>style='display:none;'<% } %>>
      <button class="button quantity-selector__button quantity-selector__button--more hsco-cart-item-more link-hover--white" data-sku="<%=model.sku%>" <% if (model.giftWithPurchase) {%>style='visibility:hidden;'<% } %>><i class="btb bt-angle-up"></i></button>
      <input data-gwp="<%=model.giftWithPurchase%>" type="text" onkeypress='return event.charCode >= 48 && event.charCode <= 57' value="<%= _.isUndefined(model.quantity)?1:model.quantity %>" name="quantity-<%=model.sku%>" class="quantity-selector__input hsco-cart-quantity-selector" <% if (model.giftWithPurchase) {%>disabled readonly<% } %>/>
      <button class="button quantity-selector__button quantity-selector__button--less hsco-cart-item-less link-hover--white" data-sku="<%=model.sku%>" <% if (model.giftWithPurchase) {%>style='visibility:hidden;'<% } %>><i class="btb bt-angle-down"></i></button>
    </div>
    <% if (!model.to) {%>x<% } %> <span class="bfx-price bfx-list-price"><%= model.purchaseDisplay %></span>
    </div>
    <div class="hidden bfx-minicart-product-qty"><%= _.isUndefined(model.quantity)?1:model.quantity %></div>
    <div class="col-xs-1">
      <a href="#remove" class="pull-right hsco-delete-item" data-sku="<%=model.sku%>" data-url="<%= model.cartUrl %>"><i class="btb bt-times" data-sku="<%=model.sku%>"></i></a>
    </div>

  </div>
  <% }, products); %>
</script>
