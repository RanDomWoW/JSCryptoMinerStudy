<script type="text/template" id="hsco-wishlist-item-template">
  <% _.each(products, function(model) { %>
  <div class="row row--vertical-center text-copy3 hsco-wishlist-row" data-wishlisturl="<%= model.cartUrl%>">
    <div class="col-xs-2">
      <a href="/bin/herschel/product/sku-to-page?sku=<%= model.sku %>">
        <img src="<%= model.image_path %>.sthumbnails.330.330.jpg" alt="<%= model.display_name %>">
        </a>
    </div>
    <div class="col-xs-4">
      <a class="link-hover--grey-light" href="/bin/herschel/product/sku-to-page?sku=<%= model.sku %>">

      <p class="m-y-0 text-medium"><%= model.display_name %></p>
      <% if (!model.oneSize){%>
      <p class="m-y-0">(<%= model.size %>)</p>
      <%}%>
      <p class="m-y-0"><%= model.color_name %></p>
      <p class="m-y-0 bfx-price bfx-product-subtotal"><%= model.purchaseDisplay %></p>
        </a>
    </div>
    <div class="col-xs-4 text-white">
      <button class="button button--primary wishlist-item__cart hsco-wishlist-move-cart" data-sku="<%=model.sku%>" data-url="<%= model.moveToCartUrl %>"><i class="btb bt-shopping-cart"></i></button>
    </div>
    <div class="col-xs-2">
      <a href="#" class="pull-right hsco-wishlist-delete-item" data-sku="<%=model.sku%>"><i class="btb bt-times" data-sku="<%=model.sku%>"></i></a>
    </div>
  </div>
  <% }); %>
</script>

