         <script type="text/javascript">

	        $(document).ready(function ()
	        {
	        	basePageInit();
	            $('#js_action').val('true');

	            $('#login').click(function(e)
	            {
	            	e.preventDefault();
	                $('#action').val('login');
	                $('#form').submit();
	            });

                $("#slider").easySlider({
                        controlsShow: false,
                        firstShow: false,
                        lastId: false,
                        lastShow: false,
                        vertical: true,
                        speed: 800,
                        auto: true,
                        pause: 4000,
                        continuous: true
                });


                $('#account_login').keydown(function(e)
                {
                    var key = window.e ? window.e.keyCode : e.which;
                    if(key == KEY.RETURN)
                    {
                        $('#account_password').focus();
                    }
                    return true;
                });

                $('#account_password').keydown(function(e)
                {
                    var key = window.e ? window.e.keyCode : e.which;
                    if(key == KEY.RETURN)
                    {
                        e.preventDefault();
                        $('#action').val('login');
                        $('#form').submit();
                    }
                    return true;
                });

                $('#account_password').keyup(function(e){
                    if($('#account_password').val().length > 16) {
                        e.preventDefault();

                        var str = $('#account_password').val();
                        $('#account_password').val(str.substring(0, 16));

                        $( "#dialog-message" ).dialog({
                            modal: true,
                            buttons: {
                                Ok: function() {
                                    $(this).dialog("close");
                                }
                            }
                        });
                    }
                });

	            $('#lost_password_link').click(function(e)
	            {
	                e.preventDefault();
	                $('#action').val('lost_password');
	                $('#form').submit();
	            });
	        });
        </script>
