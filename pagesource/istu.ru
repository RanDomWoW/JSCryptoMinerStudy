<script type="text/javascript">
	
	var news={}, gallery={}, posts={},info={},big={};
	var depth = (checkBrowser())? 2 : 0;
	var blocks = [];
	
	function createObject(obj, id) {
		obj.current = 0;
		obj.target = $(id);
		
		//obj.arr = $(id + " .moduletable:not(:has(.slider_block_node), :has(.category-module)), " + id + " .slider_block_node, " + id + " .category-module li").get();
		obj.arr = $(id + " .moduletable:not(:has(.slider_block_node), :has(.category-module), :has(.junewsultra)), " + id + " .slider_block_node, " + id + " .category-module li, " + id + " .jn").get();
		removeElements(obj.arr);
		blocks.push(obj);
		$(id)[0].style.display = "block";
		return obj;
		
	}
	
	var timeShow = 10000;
	var delay = 350;
	
	createObject(news, "#slider_news_block");
	setTimeout(function() {if (news.arr.length>1) setInterval(function() {flipNode(news)}, timeShow)}, delay);
	
	createObject(posts, "#slider_posts_block");
	setTimeout(function() {if (posts.arr.length>1) setInterval(function() {flipNode(posts)}, timeShow)}, delay*2);
	
	createObject(gallery, "#slider_gallery_block");
	setTimeout(function() {if (gallery.arr.length>1) setInterval(function() {flipNode(gallery)}, timeShow)}, delay*3);
	
	createObject(info, "#slider_info_block");
	setTimeout(function() {if (info.arr.length>1) setInterval(function() {flipNode(info)}, timeShow)}, delay*4);
	
	createObject(big, "#slider_big_block");
	if (big.arr.length>1) setInterval(function() {flipNode(big)}, 15000);
	
	function removeElements (delarr) {
	
		for (var i = 1; i < delarr.length; i++) {
			delarr[i].parentNode.removeChild(delarr[i]);
			delarr[i].style.display="block";
		}
	}
	
	function changeNode(obj) {
		obj.arr[obj.current].parentNode.removeChild(obj.arr[obj.current]);
		obj.current = (obj.current==obj.arr.length-1)? 0 : obj.current + 1;
		obj.target[0].appendChild(obj.arr[obj.current]);
	}
	
	function flipNode(obj) {
		//return;
		obj.current = (obj.current==obj.arr.length-1)? 0 : obj.current + 1;
		
		$(obj.target).flippy({
			verso : obj.arr[obj.current],
			direction : "TOP",
			duration : "750",
			depth: depth,
			color_target: "#305BB6",
			onFinish: function () {
				obj.target[0].style.overflow="hidden";
			}
		});
		
	}
	
	function checkBrowser () {	
		var useragent=navigator.userAgent;
		var navigatorname;
		if (useragent.indexOf('MSIE')!= -1) {
			navigatorname="MSIE";
		} else if (useragent.indexOf('Gecko')!= -1)	{
			if (useragent.indexOf('Chrome')!= -1)
			navigatorname="Google Chrome";
			else navigatorname="Mozilla";
			return true;
		} else if (useragent.indexOf('Mozilla')!= -1){
			navigatorname="old Netscape or Mozilla";
		} else if (useragent.indexOf('Opera')!= -1)	{
			navigatorname="Opera";
		
		}
		return false;
	}
</script>      
