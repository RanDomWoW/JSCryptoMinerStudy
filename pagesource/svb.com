<script type="text/javascript">
    function ShowLogin() {
        $(".fancybox[href='#svb_login']").trigger('click');
    }

    function CloseLogin() {

    }

    function ValidateLogin()
    {
        if(($("input#loginDlgUserName").val() == "") || ($("input#loginDlgPassword").val() == "")) {
            return false;
        }
        else{
            return true;
        }
    }

    function ValidatePassword()
    {
        var newPassword = $("input#Password1").val();
        if (newPassword != $("input#Password2").val()) {
            alert("Passwords do not match.");
            return false;
        }
        if (newPassword == "") {
            alert("Passwords cannot be blank.");
            return false;
        }
        if (newPassword.match(/['\#"%\s]/)) {
            alert("Passwords cannot contain spaces, tabs, single quotes, double quotes, percent signs or pound signs.");
            return false;
        }
        if (newPassword.length < 7) {
            alert("Password must contain at least seven characters.");
            return false;
        }

        return true;
    }
    
    mylab.wait(function() {
        $(document).ready(function () {
            $("#loginDlgUserName").focus(function(e) {
                if($("#loginDlgUserName").val() == "E-mail") {
                    $("#loginDlgUserName").val("");
                }
            });

            $("#loginDlgUserName").blur(function() {
                if($("#loginDlgUserName").val() == "") {
                    $("#loginDlgUserName").val("E-mail");
                }
            });

            $("#loginDlgPasswordClear").show();
            $("#loginDlgPassword").hide();

            $("#loginDlgPasswordClear").focus(function() {
                $("#loginDlgPasswordClear").hide();
                $("#loginDlgPassword").show();
                $("#loginDlgPassword").focus();
            });

            $("#loginDlgPassword").blur(function() {
                if($("#loginDlgPassword").val() == "") {
                    $("#loginDlgPasswordClear").show();
                    $("#loginDlgPassword").hide();
                }
            });

            
        });
    });
</script>

