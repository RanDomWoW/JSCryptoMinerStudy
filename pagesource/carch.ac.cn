<script type="text/javascript">
$(function() {
	var sWidth = $("#focus").width(); //获取焦点图的宽度（显示面积）
	var len = $("#focus ul li").length; //获取焦点图个数
	var index = 0;
	var picTimer;
	
	
	//以下代码添加数字按钮和按钮后的半透明条，还有上一页、下一页两个按钮
	var btn = "<div class='btnBg'></div><div class='btn'>";
	for(var i=0; i < len; i++) {
		btn += "<span></span>";
	}
	btn += "</div>";
	//btn += "<div class='preNext pre'></div><div class='preNext next'></div>";
	$("#focus").append(btn);
	$("#focus .btnBg").css("opacity",0.5);

	
	//为小按钮添加鼠标滑入事件，以显示相应的内容
	$("#focus .btn span").css("opacity",0.4).mouseenter(function() {
		index = $("#focus .btn span").index(this);
		showPics(index);
	}).eq(0).trigger("mouseenter");


	//本例为左右滚动，即所有li元素都是在同一排向左浮动，所以这里需要计算出外围ul元素的宽度
	$("#focus ul").css("width",sWidth * (len));
	
	//鼠标滑上焦点图时停止自动播放，滑出时开始自动播放
	$("#focus").hover(function() {
		clearInterval(picTimer);
	},function() {
		picTimer = setInterval(function() {
			showPics(index);
			index++;
			if(index == len) {index = 0;}
		},4000); //此4000代表自动播放的间隔，单位：毫秒
	}).trigger("mouseleave");
	
	//显示图片函数，根据接收的index值显示相应的内容
	function showPics(index) { //普通切换
		
		var nowLeft = -index*sWidth; //根据index值计算ul元素的left值
		$("#focus ul").stop(true,false).animate({"left":nowLeft},300); //通过animate()调整ul元素滚动到计算出的position
		//$("#focus .btn span").removeClass("on").eq(index).addClass("on"); //为当前的按钮切换到选中的效果
		$("#focus .btn span").stop(true,false).animate({"opacity":"0.4"},300).eq(index).stop(true,false).animate({"opacity":"1"},300); //为当前的按钮切换到选中的效果
	}
	
	//友情链接图片滚动
	
	$("#colee_left2").html($("#colee_left1").html());
	var tmp_left_value = parseInt($("#colee_left").scrollLeft());
	var tmp_width = parseInt($("#colee_left2").offset().left) - parseInt($("#colee_left1").offset().left) - 1;
	var itmp = 0;
	function show_link(){
	
		//tmp_left_value = parseInt($("#colee_left").scrollLeft());				
		
		if(itmp > tmp_width)  //offsetWidth 是对象的可见宽度
		{
			//tmp_left_value = tmp_left_value - tmp_width;
			itmp = 0;
			$("#colee_left").scrollLeft(itmp); //是对象的实际内容的宽，不包边线宽度
		}	
		else
		{
			//tmp_left_value = tmp_left_value + 1;
			itmp = itmp + 1;
			$("#colee_left").scrollLeft(itmp); //是对象的实际内容的宽，不包边线宽度
			
		}
		
	}
	
	var MyMar3=setInterval(function() {show_link();},30);
	$("#colee_left").hover(function(){
		clearInterval(MyMar3);
	},function(){
		MyMar3=setInterval(function() {show_link();},30);
	});
	

	//国重快讯的图片切换
	
	var count_8=$("#pictures_change8 div").length;
	if(count_8 > 1)
	{
		var tmpi_8 = 0;
		var tmpTimer_8 ;
	
		$("#pictures_change8 div:not(:first-child)").hide();
				
		$("#pictures_change8").hover(function() {
        
            clearInterval(tmpTimer_8);
					
        },function(){		
				
			tmpTimer_8 = setInterval(function() {showChg_8();},3000);
				
		});
		
	
	
		function showChg_8()
    	{	
			tmpi_8 = tmpi_8 + 1;
			if(tmpi_8 >= count_8)
			{
				tmpi_8 = 0;
			}
		
			$("#pictures_change8 div").filter(":visible").fadeOut(1000).parent().children().eq(tmpi_8).fadeIn(1000);
       
    	}
	
		$("#pictures_change8").trigger('mouseout');
	
	}
	
	//banner图片切换
	var count_9=$("#pictures_change9 div").length;
	if(count_9 > 1)
	{
		var tmpi_9 = 0;
		var tmpTimer_9 ;
	
		$("#pictures_change9 div:not(:first-child)").hide();
				
		$("#pictures_change9").hover(function() {
        
            clearInterval(tmpTimer_9);
					
        },function(){		
				
			tmpTimer_9 = setInterval(function() {showChg_9();},3000);
				
		});
		
	
	
		function showChg_9()
    	{	
			tmpi_9 = tmpi_9 + 1;
			if(tmpi_9 >= count_9)
			{
				tmpi_9 = 0;
			}
		
			$("#pictures_change9 div").filter(":visible").fadeOut(1000).parent().children().eq(tmpi_9).fadeIn(1000);
       
    	}
	
		$("#pictures_change9").trigger('mouseout');
	
	}
	
	
});

</script>

