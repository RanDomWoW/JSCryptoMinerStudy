	$(document).ready(function(){
		IMF_createAreaSelect();
		IMF_updatePrograms();
	});
	
	function IMF_createAreaSelect() {
		$('#imf-area-wrapper').html('<label for="imf-area">Area of study</label><select id="imf-area" name="area" onchange="IMF_updatePrograms()">'+area_options_html+'</select>');
	}
	
	function IMF_updatePrograms() {
		var area_id = $('#imf-area').val();
		if (area_id == '') {
			// If no area was selected, show the full program list.
			$('#imf-program').html(all_program_options_html);
		} else {
			// Otherwise show only that area's programs.
			$('#imf-program').html(area_program_options_html[area_id]);
		}
	}
</script>

