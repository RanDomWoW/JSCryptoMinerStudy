    function validateEmail(email)
    {
        var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return re.test(email);
    }

    function submitAction()
    {
        var form = document.getElementById("user-pass");

        var name = $("#edit_name").val();
        var email = "";
        var display = "Results: ";
        if (validateEmail($("#edit_name").val()))
        {
            name = "";
            email = $("#edit_name").val();

            display = display + "Error in Username, try your ID number.";
            $("#dlgResetPassError").dialog("open");
            var dlgResult = "<td>" + display + "</td>"; //$.x_displayDlg(display);
            $("#dlgResetPassError").append(dlgResult);
        }
        else
        {
            //var result = ($.x_ResetUserPassword(name, email));
            //BUG39690
            $.ajax({
                type: "GET",
                url: "sites/all/libraries/pages/home_pages/ajax_functions.php",
                data: {
                    func: "ResetUserPassword",
                    name: name,
                    email: email
                },
                success: function (result) {
            jsonObj = eval("(" + result + ")");
            if (jsonObj == "00")
            {
                display = display + "Successfully Reset User Password.";

                $("#dlgResetPass").dialog("open");
                var dlgResult = "<td>" + display + "</td>"; //$.x_displayDlg(display);
                //alert(display);
                $("#trResetResult").append(dlgResult);
            }
            else
            {
                display = display + "Error in Reset User Password . Please try again later.";
                $("#dlgResetPass").dialog("open");
                var dlgResult = "<td>" + display + "</td>"; //$.x_displayDlg(display);
                //alert(display);
                $("#trResetResult").append(dlgResult);
            }
        }
            });


        }
        $("#dlgReset").fadeOut();

    }

    $("#dlgResetPass").dialog({
        autoOpen: false,
