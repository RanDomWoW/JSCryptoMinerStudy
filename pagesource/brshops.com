<script type="text/javascript">
$(document).ready( function(){

		var paginacao = $('.paginacao');

		if( paginacao.length ){
					var boxIten = $(paginacao[1]).find('tr > td:last-child');
					var qtyItens = $(paginacao[1]).find('tr > td:first-child b:last-child').html();
					var pages = boxIten.find('.numero_pagina');

					//Esconde texto paginacao
					boxIten.find('.link_anterior, .link_proxima').hide();

					//Inicio do controle
					if( qtyItens > 150 ){
							var allPages = Math.ceil(qtyItens/150);
							if( allPages > 10 ){
									console.log(allPages)
									var url = window.location.origin;
									var category = window.location.pathname;
									var search = window.location.search;
									if( search.match('page') ){
											var NumberSearch = parseInt(search.split('=').slice(-1));
											console.log(NumberSearch)
									}else {
											var NumberSearch = 1;
									}
									var NumberSearchPlus = parseInt(NumberSearch) + 1;
									var NumberSearchMinus = parseInt(NumberSearch) - 1;

									if( search === '' || search === undefined || search === null || NumberSearch === 1 ){
											boxIten.find('.numero_pagina:nth-child(1n+6)').hide();
									}else if(  NumberSearch <= 4 ) {
											boxIten.find('.numero_pagina:nth-child(1n+7)').hide();
									}else if( NumberSearch > 4 && NumberSearch < allPages ){
											boxIten.find('.numero_pagina').hide();
											boxIten.prepend(
												`<a href="`+url+category+`" class="numero_pagina" title=" Página 1 ">
														<u>1</u>
												</a>
												<span class="separacao">...</span>
												<a href="`+url+category+search+`&page=`+NumberSearchMinus+`" class="numero_pagina" title=" Página `+NumberSearchMinus+` ">
														<u>`+NumberSearchMinus+`</u>
												</a>`);
											boxIten.append(
												`<a href="`+url+category+search+`&page=`+NumberSearchPlus+`" class="numero_pagina" title=" Página `+NumberSearchPlus+` ">
														<u>`+NumberSearchPlus+`</u>
												</a>
												<span class="separacao">...</span>
												<a href="`+url+category+search+`&page=`+allPages+`" class="numero_pagina" title=" Página `+allPages+` ">
														<u>`+allPages+`</u>
												</a>`);
									}else if( search == '?page='+allPages || NumberSearch === allPages ){
											boxIten.find('.numero_pagina:nth-child(1n+6)').hide();
											boxIten.find('.pagina_atual').before(
												`<span class="separacao">...</span>`);
											boxIten.prepend(
												`<a href="`+url+category+`" class="numero_pagina" title=" Página 1 ">
														<u>1</u>
												</a>
												<span class="separacao">...</span>`);
									}

							}
					}
		}

});

</script>

