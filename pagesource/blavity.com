ileUrl&#34;:&#34;http://facebook.com/NamonEugene&#34;,&#34;fbname&#34;:null,&#34;fbid&#34;:null,&#34;fbUserPicture&#34;:null,&#34;fbAccesToken&#34;:&#34;&#34;,&#34;isFbAutoPublish&#34;:false,&#34;isfbconnect&#34;:false,&#34;created_at&#34;:&#34;2017-01-05T16:04:17.137Z&#34;,&#34;updated_at&#34;:&#34;2017-01-05T16:04:17.137Z&#34;,&#34;profileImagePreference&#34;:&#34;https://res.cloudinary.com/blavity/image/upload/v1500032504/hjeit3khdkyhcklfrdn0&#34;,&#34;user_status&#34;:&#34;active&#34;}],&#34;slug&#34;:&#34;the-trouble-with-classifying-bruno-mars-as-a-cultural-appropriator&#34;,&#34;categories&#34;:[{&#34;_id&#34;:&#34;57ce5b43dbf888cd7733011c&#34;,&#34;term_id&#34;:9508,&#34;updatedAt&#34;:&#34;2016-09-06T05:59:31.577Z&#34;,&#34;description&#34;:&#34;&#34;,&#34;taxonomy&#34;:&#34;category&#34;,&#34;slug&#34;:&#34;community-submitted&#34;,&#34;name&#34;:&#34;Community Submitted&#34;,&#34;__v&#34;:0,&#34;createdAt&#34;:&#34;2016-09-06T05:59:31.577Z&#34;,&#34;deleted&#34;:false,&#34;deletedAt&#34;:null,&#34;deleted_at&#34;:null,&#34;updated_at&#34;:&#34;2016-10-30T19:56:10.141Z&#34;,&#34;created_at&#34;:&#34;2016-10-30T19:56:10.141Z&#34;},{&#34;_id&#34;:&#34;57ce5b43dbf888cd773300f9&#34;,&#34;term_id&#34;:95,&#34;updatedAt&#34;:&#34;2016-09-06T05:59:31.554Z&#34;,&#34;description&#34;:&#34;&#34;,&#34;taxonomy&#34;:&#34;category&#34;,&#34;slug&#34;:&#34;opinion&#34;,&#34;name&#34;:&#34;Opinion&#34;,&#34;__v&#34;:0,&#34;createdAt&#34;:&#34;2016-09-06T05:59:31.554Z&#34;,&#34;deleted&#34;:false,&#34;deletedAt&#34;:null,&#34;deleted_at&#34;:null,&#34;updated_at&#34;:&#34;2016-11-03T21:06:16.562Z&#34;,&#34;created_at&#34;:&#34;2016-11-03T21:06:16.562Z&#34;}],&#34;tags&#34;:[{&#34;_id&#34;:&#34;5aa7e2c531506f4ebc90552d&#34;,&#34;deletedAt&#34;:null,&#34;deleted&#34;:false,&#34;name&#34;:&#34;Bruno mars&#34;,&#34;slug&#34;:&#34;Bruno-mars&#34;,&#34;__v&#34;:0,&#34;deleted_at&#34;:null,&#34;updated_at&#34;:&#34;2018-03-13T14:40:05.112Z&#34;,&#34;created_at&#34;:&#34;2018-03-