<script type="text/javascript">
    jQuery(document).ready(function($) {
        $('#billing_customer_address').change(function(){
            if($(this).val() == '')
            {
                $('.billing_full_info').show();
                $('.sipping_this_address').hide();
                $('.billing_save_in_address_book').show();
            }else{
                $('.billing_full_info').hide();
                $('.sipping_this_address').show();
                $('.billing_save_in_address_book').hide();
            }
        });
        $('#billing_use_for_shipping_yes').click(function(){
            var length = $("[id='billing_use_for_shipping_yes']:checked").length; 
            switch(length)
            {
                case 1:
                    $('#billing_use_for_shipping').val('1');
                break;
                case 0:
                    $('#billing_use_for_shipping').val('0');
                break
            }
        }); 
        $('#billing_use_for_shipping_yes').click(function(){
            var length = $("[id='billing_use_for_shipping_yes']:checked").length; 
            switch(length)
            {
                case 1:
                    $('.opc_ship_to_adress').hide();
                break;
                case 0:
                    $('.opc_ship_to_adress').show();
                break
            }
        });
        $('#shipping_customer_address').change(function(){
            if($(this).val() == '')
            {
                $('#ship_form').show();
            }else{
                $('#ship_form').hide();
            }
        });
        $('#customer_account_create').click(function(){
            var length = $("[id='customer_account_create']:checked").length; 
            switch(length)
            {
                case 1:
                    $('#register-customer-password').show();
                break;
                case 0:
                    $('#register-customer-password').hide();
                break
            }
        }); 
        //ajax update for mobile
        $('select[ajax="selectcountry"],#billing_postcode,#billing_region,#state_id,#billing_city,#billing_customer_address,#shipping_customer_address,#billing_telephone').change(function(){
            if($(this).val() != '')
            {
                url = $('#mst-address-request').val()+'fancycheckout/offcanvascheckout/updatebillship';
                if(typeof ajax_request !== 'undefined')
                ajax_request.abort();
                ajax_request = $.ajax({
                	type: "POST",
                	url: url,
                	data: $("#offcanvas-checkout").serialize(),
                	cache: false,
                    beforeSend:  function() {
                        if($(window).width()<651){
                            $('.loading-shipping-address').show();
                            $('.cart-step-content').css('opacity','0.2');
                            $('.shipping-step-content').css('opacity','0.2');
                            $('.review-step').css('opacity','0.2');
                        }
                    },
                    success: function(html){
                        //ken
                        if($(window).width()<651){
                            $('.loading_image').hide();
                            $('.cart-step-content').css('opacity','1');
                            $('.shipping-step-content').css('opacity','1');
                            $('.review-step').css('opacity','1');
                        }
                        var _json=$.parseJSON(html); 
                        $('.review-step-shipping').html(_json.shipping_method);                        
                        $('.review-step-payment').html(_json.payment_method);
                        $('.review-step-review').html(_json.review_order);
                    },
                    complete: function(data){
                        //custom 11
                        url = $('#mst-address-request').val()+'fancycheckout/offcanvascheckout/updatebillship';
                        if(typeof ajax_request !== 'undefined')
                        ajax_request.abort();
                        ajax_request = $.ajax({
                        	type: "POST",
                        	url: url,
                        	data: $("#offcanvas-checkout").serialize(),
                        	cache: false,
                            beforeSend:  function() {
                                if($(window).width()<651){
                                    $('.loading-shipping-address').show();
                                    $('.cart-step-content').css('opacity','0.2');
                                    $('.shipping-step-content').css('opacity','0.2');
                                    $('.review-step').css('opacity','0.2');
                                }
                            },
                            success: function(html){
                                //ken
                                if($(window).width()<651){
                                    $('.loading_image').hide();
                                    $('.cart-step-content').css('opacity','1');
                                    $('.shipping-step-content').css('opacity','1');
                                    $('.review-step').css('opacity','1');
                                }
                                var _json=$.parseJSON(html); 
                                $('.review-step-shipping').html(_json.shipping_method);                        
                                $('.review-step-payment').html(_json.payment_method);
                                $('.review-step-review').html(_json.review_order);
                            },
                            complete: function(data){
                                
                            }
                        });
                    }
                });
            }
        });
    });
</script>
