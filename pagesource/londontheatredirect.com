        (function () {
            'use strict';

            initEventImageLink.$inject = ['jquery'];
            ltd.push(initEventImageLink);

            function initEventImageLink($) {
                $(".events-container-item__link").click(function(){
                    var $this = $(this);

                    var productObj = {
                        'EventName' : $this.data('event-name'),
                        'EventId' : $this.data('event-id'),
                        'Brand' : $this.data('venue-name'),
                        'Category' : $this.data('category'),
                        'URL' : $this.attr('href')
                    };

                    RecordProductClick(productObj);
                });

                function RecordProductClick(productObj)
                {
                    dataLayer.push({
                        'event': 'productClick',
                        'ecommerce': {
                            'click': {
                                'actionField': {
                                    'list': 'Homepage - Most Popular'
                                },
                                'products': [{
                                    'name': productObj.EventName, // Name or ID is required.
                                    'id': productObj.EventId,
                                    'brand': productObj.Brand,
                                    'category': productObj.Category, // name of the category is the same as in impression
                                }]
                            }
                        },
                        'eventCallback': function() {
                            document.location = productObj.URL
                        }
                    });
                }
            }
        }());
    </script>


