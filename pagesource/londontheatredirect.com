    <script>
        function bookingBoxCheck(e) {
            if (!document.getElementsByClassName) return true;
            var CLASS_EMPTY = 'header-booking-box__input--error';
            var inputs = document.getElementsByClassName('header-booking-box__input');

            var valid = false;
            var invalid = [];
            for (var i = 0; i < inputs.length; i++)
            {
                if (inputs[i].value) valid = true;
                else invalid.push(inputs[i]);
            }

            if (valid) {
                for (var i = 0; i < inputs.length; i++) {
                    inputs[i].classList.remove(CLASS_EMPTY);
                }
            }
            else {
                for (var i = 0; i < inputs.length; i++) {
                    invalid[i].classList.add(CLASS_EMPTY);
                }
            }

            return valid;
        }


        (function () {
            if (!document.getElementsByClassName) return true;

            var button = document.getElementsByClassName('header-booking-box__submit')[0];
            if (!button) return true;

            button.addEventListener('touchstart', bookingBoxCheck);

        }());
    </script>



                

