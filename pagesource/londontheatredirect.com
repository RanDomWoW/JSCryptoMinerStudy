<script>
    (function () {
        'use strict';

        initTP.$inject = ['jquery'];
        ltd.push(initTP);

        function initTP($)
        {
            var LTD = window.LTD;
            var LOCALE = LTD && LTD.lang && LTD.lang.locale || 'en-GB';
            var BP_VERTICAL = 768;
            var $btnPrev = $('.tp-review-prev'),
                $btnNext = $('.tp-review-next'),
                $btnBottom = $('.tp-review-bottom-next'),
                $container = $btnNext.closest('.tp-service-review-container'),
                $list = $container.find('.tp-service-reviews'),
                start = $list.children().length,
                index = 0,
                step = 3,
                itemW = $list.parent().width() / 3 || 0,
                length = parseInt($btnNext.data('length')) || 18,
                reviewCount = parseInt($btnNext.data('reviewCount')) || 0,
                url = $btnNext.data('url') || null,
                isVertical = false,
                stored = [];

            setVertical();
            setMinWidth();
            $(window).on('resize', function () {
                setVertical();
                if (isVertical) return;
                if (stored.length)
                {
                    for (var i=0; i<stored.length; i++) $list.append(stored[i]);
                }
                var bckItemW = itemW;
                itemW = $list.parent().width() / 3 || 0;
                if (itemW > bckItemW) setMinWidth();
            });

            $btnPrev.click(onClickPrev);
            $btnNext.click(onClickNext);
            $btnBottom.click(onClickBottom);

            function AddItem(item, key)
            {
                this.find('.tp-review-name').text(item.ConsumerName);
                this.find('.tp-review-header').text(item.Title);
                this.find('.tp-review-content').text(ellipsis(item.Text, 100));

                var date = new Date(item.CreatedAt);
                if (date.getTime())
                {
                    this.find('.tp-service-published-meta').attr('content', item.CreatedAt);
                    this.find('.tp-service-published').text(date.toLocaleString(LOCALE, {
                        day: 'numeric',
                        month: 'long'
                    }));
                }

                this.find('.tp-star-rating')
                    .removeClass('tp-count-1 tp-count-2 tp-count-3 tp-count-4 tp-count-5')
                    .addClass([ 'tp-count', item.Stars || 1 ].join('-'));

                this.appendTo($list);
            }

            function ellipsis(text, l) { return (text.length > l) ? text.substr(0, l) + '\u2026' : text; }

            function getHeight() { height = $list.prop('scrollHeight'); return height; }

            function onClickBottom(e)
            {
                e.preventDefault();
                index += step;
                if (stored.length > step) addItems();
                else
                {
                    addItems();
                    $.get( url, { start: start, length: length }, success, 'json');
                }

                if (start >= reviewCount) $btnBottom.addClass('disabled');

                function addItems()
                {
                    var h = $list.height();

                    for (var i=0; i< step; i++) addItem();

                    function addItem()
                    {
                        if (!stored.length) return;
                        var item = stored.shift();
                        h += item.height;
                        item.css('opacity', 0);
                        $list.animate({ height: h + 'px' }, 100, function () {
                            $list.append(item);
                            item.animate({ opacity: 1 }, 400);
                        });
                    }
                }
            }

            function onClickPrev(e)
            {
                e.preventDefault();
                index -= step;
                if (index <= 0) index = 0;
                setOffset();
                setClass();
            }

            function onClickNext(e)
            {
                e.preventDefault();
                index += step;
                var bp = $list.children().length - step;
                if (index >= bp) index = bp;
                setOffset();
                setClass();

                if (url && index >= bp - (step*2)) $.get( url, { start: start, length: length }, success, 'json');
            }

            function setClass()
            {
                if (index >= $list.children().length - step) $btnNext.removeClass('active');
                else $btnNext.addClass('active');

                if (index <= 0) $btnPrev.removeClass('active');
                else $btnPrev.addClass('active');
            }

            function setMinWidth() { if (!isVertical) $list.css('min-width', ($list.children().length * itemW + 500) + 'px'); }

            function setOffset() { $list.animate({ left: (index * -itemW) }, 'slow'); }

            function setVertical()
            {
                if (window.innerWidth <= BP_VERTICAL)
                {
                    isVertical = true;
                    index = 0;
                    var children = $list.children();
                    for (var i=step; i<children.length; i++) stored.push($(children[i]).detach());
                }
                else
                {
                    isVertical = false;
                }
            }

            function scrollBottom() { $list.animate({ scrollTop: height || getHeight() }, 'slow'); }

            function success(data)
            {
                if (isArray(data) && data.length)
                {
                    var template = $list.children().last().clone();

                    for (var i=0; i<data.length; i++) AddItem.call(template.clone(), data[i], i);
                    setMinWidth();
                    start += data.length;
                }
            }

            function isArray(value) { return Object.prototype.toString.call( value ) === '[object Array]'; }

        }
    }());
</script>



                
