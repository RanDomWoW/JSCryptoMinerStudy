			        	}else if(getCookie('setview')){
			        	    clearCookie('setview');
			        	    window.location.reload();
			        	}
			        }
			    },
			    onComplete: function () {
			    	/*渐变圆canvas*/
			    	setCanvas();
			    	
			    }
			});
			
			var resize_flag = false;
			$(window).resize(function(){
				if(!resize_flag&&!os.isPc){
					resize_flag = true;
					/*高小于宽  横屏*/
					if($(window).height()<$(window).width()){
					    setCookie('setview',true,1200*3600);
					    window.location.reload();
					}else if(getCookie('setview')){
					    clearCookie('setview');
					    window.location.reload();
					}
					setTimeout(function(){resize_flag = false;},100);
				}
			});
			
			/*fullpage*/
