				    		opacity: 1
				    	},1000,function(){
				    		$('.hover-tips').addClass('cur');
				    		$('.next-view').addClass('cur');
				    	});
				    }
				}).on('circle-animation-end',function(){
					if(!getCookie('setpop')){
						setTimeout(function(){
							$('.arc-box').fadeOut();
							$('.pop-link-bg').fadeIn();
							$('.pop-link-box').show(0,function(){
								$('.pop-link-box').addClass('ishow');
							});
							setCookie('setpop',true,1*60*60);
						},1000);
					}
				});
