				$('.arc-box .line-box .line').hover(function(){
					if(os.isPc){
						var _index=$(this).index();
						$('.hover-tips').remove();
						$('.arc-box .line-text .text').eq(_index).show(0,function(){
							$(this).addClass('cur').siblings().removeClass('cur').hide();
						});
					}
					
				},function(){
					if(os.isPc){
						$('.arc-box .line-text .text').removeClass('cur').hide();
					}
					
				});
			}

			/*鼠标移动视觉差动画*/
			var $body = $(window);
			var WIDTH = $body.width();
			var HEIGHT = $body.height();
			var $bg = $('.box1').find('.bg');
			var $bg2 = $('.box1').find('.bg2');
			function moveAd(e) {
			  var xPer = e.clientX / WIDTH;
			  var yPer = e.clientY / HEIGHT;
			  var translateX = -5 + (xPer * 30);
			  var translateY = -5 + (yPer * 30);

			  $bg.css({
			    transform: 'translateX(' + translateX + 'px) translateY(' + translateY + 'px)'
			  });

			  $bg2.css({
			    transform: 'translateX(-' + translateX + 'px) translateY(-' + translateY + 'px)'
			  });
			}



			/*防抖动函数*/
			function debounce(func, wait, immediate) {
			    var timeout;
			    return function() {
			        var context = this, args = arguments;
			        var later = function() {
			            timeout = null;
			            if (!immediate) func.apply(context, args);
			        };
			        var callNow = immediate && !timeout;
			        clearTimeout(timeout);
			        timeout = setTimeout(later, wait);
			        if (callNow) func.apply(context, args);
			    };
			};
			var myEfficientFn = debounce(function() {
			    setCanvas();
			}, 500); 

			/*改变窗口后重绘画布*/
			$(window).resize(function(){
				if(_csize>$(window).width()||_csize>$(window).height()||_csize<Math.ceil($(window).height()*0.9042)||_csize<Math.ceil($(window).width()*0.9042)){
					$('.arc-box').removeClass('active');
					myEfficientFn();
				}else{
					$('.arc-box').css({
						marginTop: Math.ceil(($(window).height()-_csize)*0.5),
						marginLeft: Math.ceil(($(window).width()-_csize)*0.5)
					});
				}
