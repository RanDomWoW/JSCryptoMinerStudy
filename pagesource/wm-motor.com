<script type="text/javascript">
	$(function(){
		var _navLen=$('.bocweb-header .bocweb-nav li');
		var _delay=0;
		$('.bocweb-header .bocweb-nav li').each(function(){
			$(this).css({
				'transition-delay' : _delay+'s'
			});
			_delay+=0.1;
		});
		$('.bocweb-menu').click(function(){
			$(this).toggleClass('active');
			if($('.bocweb-header').hasClass('h-show')){
				$('.bocweb-header').removeClass('h-show');
				if($('.bocweb-header').hasClass('istop')){
					$('.bocweb-header').addClass('ishide');
				}
				$('.bocweb-header .bocweb-nav').fadeOut(function(){});
			}else{
				$('.bocweb-header .bocweb-nav').fadeIn(function(){
					$('.bocweb-header').addClass('h-show');
				});
			}
		});
		var setHt='';
		var setHt2='';
		$('.bocweb-header .bocweb-nav li').hover(function(){
			var _this=$(this);
			clearTimeout(setHt);
			if(!$('.is1100').is(':hidden')){
				setHt=setTimeout(function(){
					_this.find('.hide').stop(false,true).slideDown();
				},600);
			}
		},function(){
			clearTimeout(setHt);
			if(!$('.is1100').is(':hidden')){
				$('.bocweb-header .bocweb-nav li .hide').hide();
			}
		});
		$('.bocweb-header .bocweb-nav li .sub-hide').hover(function(){
			var _this=$(this);
			clearTimeout(setHt2);
			if(!$('.is1100').is(':hidden')){
				_this.find('.link-box').stop(false,true).slideDown();
				if($(this).index()==0){
					$(this).parents('li').find('.line').css({
						width: '199.5%'
					});
				}
			}
		},function(){
			clearTimeout(setHt2);
			if(!$('.is1100').is(':hidden')){
				$('.bocweb-header .bocweb-nav li .link-box').hide();
				$('.bocweb-header .bocweb-nav li .line').removeAttr('style');
			}
		});
		$('.bocweb-header .bocweb-nav li>a').click(function(){
			if($('.is1100').is(':hidden')){
				if($(this).parent().find('.hide').is(':hidden')){
					$(this).parent().addClass('active');
					$(this).parent().find('.hide').stop(false,true).slideDown().parent().siblings().find('.hide').stop(false,true).slideUp();
					$(this).parent().siblings().find('.sub-hide').removeClass('active').find('.link-box').stop(false,true).slideUp();
				}else{
					$(this).parent().removeClass('active');
					$(this).parent().find('.hide').stop(false,true).slideUp().parent().siblings().find('.hide').stop(false,true).slideUp();
					$('.bocweb-header .bocweb-nav li').find('.sub-hide').removeClass('active').find('.link-box').stop(false,true).slideUp();
				}
				
			}
		});
		$(document).on('click','.bocweb-header .bocweb-nav li .sub-hide .show',function(){
			if($('.is1100').is(':hidden')){
				if($(this).parent().find('.link-box').is(':hidden')){
					$(this).parent().addClass('active');
					$(this).parent().find('.link-box').stop(false,true).slideDown();
				}else{
					$(this).parent().removeClass('active');
					$(this).parent().find('.link-box').stop(false,true).slideUp();
				}
				$('.bocweb-header .bocweb-nav li').find('.sub-hide').removeClass('active');
				$('.bocweb-header .bocweb-nav li').find('hide').removeAttr('style');
				$('.bocweb-header .bocweb-nav li').find('link-box').removeAttr('style');

			}
			return false;
		});
		$(window).resize(function(){
			if(!$('.is1100').is(':hidden')){
				$('.bocweb-header .bocweb-nav').fadeIn(function(){
					if(!$('.bocweb-header').hasClass('h-show')){
						$('.bocweb-header .bocweb-nav').removeAttr('style');
					}
				});
				if($('.bocweb-header').hasClass('istop')){
					$('.bocweb-header').addClass('ishide');
				}
			}
		});

		/*判断是否是360极速模式*/
		function isChrome(){
			var ua = navigator.userAgent.toLowerCase();

			return ua.indexOf("chrome") > 1;
		}

		function _mime(option, value) {
			var mimeTypes = navigator.mimeTypes;
			for (var mt in mimeTypes) {
				if (mimeTypes[mt][option] == value) {
					return true;
				}
			}
			return false;
		}

		var is360 = _mime("type", "application/vnd.chromium.remoting-viewer");
		if (isChrome() && is360) { 
			$('body').css({
				'-webkit-text-stroke-width': '0.2px'
			});
		}
	});
