				$('.arc-box').css({
					width: _csize,
					height: _csize,
					marginTop: Math.ceil(($(window).height()-_csize)*0.5),
					marginLeft: Math.ceil(($(window).width()-_csize)*0.5)
				});
				$('.arc-box .circle').circleProgress({
				    value: 1,
				    size: _csize,
				    startAngle: -0.5*Math.PI,
				    lineCap: 'round',
				    thickness: Math.ceil(_csize*0.0648),
				    fill: { gradient: ["#29ffdb", "#b6ff64"] }
				}).on('circle-animation-progress', function(event, stepValue) {
				    if(stepValue>0.3&&!$('.arc-box').hasClass('active')){
				    	$('.arc-box').addClass('active');
