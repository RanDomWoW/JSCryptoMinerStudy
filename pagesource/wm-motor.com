			        $('.box3 .video').eq(1).prepend('<video loop name="media" webkit-playsinline playsinline x5-playsinline x5-video-player-type="h5" ><source src="http://static.wm-motor.com/web/swf/2.mp4?v=v13"></video>');
			        $('.box3 .video').eq(2).prepend('<video loop name="media" webkit-playsinline playsinline x5-playsinline x5-video-player-type="h5" ><source src="http://static.wm-motor.com/web/swf/3.mp4?v=v13"></video>');
	                if(os.isPc){
			        	$('.box1').on('mousemove', moveAd);
			        }else{
			        	/*判断横竖屏*/
			        	/*高小于宽  横屏*/

			        	if($(window).height()<$(window).width()){
			        	    setCookie('setview',true,1200*3600);
