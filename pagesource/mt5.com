                ));
                matches =  matches ? decodeURIComponent(matches[1]) : undefined;

                if (matches !== undefined) {
                    top_news_containers_cookie[type] = matches.split('_');
                }
            }

            if (type in top_news_containers_cookie) {
                if (top_news_containers_cookie[type].indexOf(top_news_containers[i].dataset.id) !== -1) {
                    top_news_containers[i].style.display = 'none';
                    top_news_containers[i].dataset.status = 'hide';
                    top_news_containers_something_was_hide = true;
                }
            }
            //top_news_containers[i].style.display = 'none';
        }

        if (top_news_containers_something_was_hide) {
            document.querySelector('#top_news .show').setAttribute('style', '');
        }

        function main_top_news_show() {
            $(event.target.parentNode).fadeOut();
            $('#top_news [data-status="hide"]').fadeIn();
        }
