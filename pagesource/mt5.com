Date.prototype.getSunday = function(month, number) {
    var count = 0;
    month = month || this.getUTCMonth();
    number = number || 1;
    var d = new Date(this.getUTCFullYear(), month, 1, 0, 0, 0),
        nd = new Date(this.getUTCFullYear(), month + 1, 0);
    var day = d.getDay();
    var j = day;
    for(var i = 1; i <= nd.getDate(); i++) {
        if(j === 0) {
            count ++;
        }
        if(count === number) {
            break;
        }
        (j === 6) ? j = 0: j++;
    }
    d.setDate(i);
    return d;
};
Date.prototype.getFirstSunday = function(month) {
    month = month || this.getUTCMonth();
    return this.getSunday(month, 1);
};
Date.prototype.getLastSunday = function(month) {
    month = month || this.getUTCMonth();
    var dayName, j,
        day = new Date(this.getUTCFullYear(), month, 1, 0, 0, 0),
        lastDay = new Date(this.getUTCFullYear(), month + 1, 0);

    dayName = lastDay.getDay();
    j = lastDay.getDate();
    do {
        j--;
        dayName--;
    } while(dayName > 0);
    day.setDate(j);
    return day;
};

function tradingSessions(o) {

    var timers = new Array(),
        self = this;

    var cityTime = {
        all: {
            name: o.i18n.pacificName,
            offset: 12,
            savingTime: false,
            isAmericanDST: false
        },
        id: {
            name: o.i18n.id,
            offset: 7,
            savingTime: false,
            isAmericanDST: false
        },
        ms: {
            name: o.i18n.ms,
            offset: 8,
            savingTime: false,
            isAmericanDST: false
        },
        de: {
            name: o.i18n.de,
            offset: 1,
            savingTime: true,
            isAmericanDST: false
        },
        cn: {
            name: o.i18n.cn,
            offset: 8,
            savingTime: false,
            isAmericanDST: false
        },
        ar: {
            name: o.i18n.ar,
            offset: 4,
            savingTime: false,
            isAmericanDST: false
        },
        vi: {
            name: o.i18n.vi,
            offset: 7,
            savingTime: false,
            isAmericanDST: false
        },
        th: {
            name: o.i18n.th,
            offset: 7,
            savingTime: false,
            isAmericanDST: false
        },
        bd: {
            name: o.i18n.bd,
            offset: 6,
            savingTime: false,
            isAmericanDST: false
        },
        home: {
            name: o.i18n.home,
            currentTime: true
        }
    };


    this.sessionSchedule = {
        pacifiс: cityTime[o.lang] || cityTime.all,
        asian: {
            name: o.i18n.asianName,
            offset: 9,
            savingTime: false,
            isAmericanDST: false
        },
        moscow: {
            name: o.i18n.moscowName,
            offset: 3,
            savingTime: false,
            isAmericanDST: false
        },
        european: {
            name: o.i18n.europeanName,
            offset: 0,
            savingTime: true,
            isAmericanDST: false
        },
        american: {
            name: o.i18n.americanName,
            offset: -5,
            savingTime: true,
            isAmericanDST: true
        }
    };

    var parseDate = function(val) {
        var date, time;
        val = val.split(' ');
        date = val[0];
        time = val[1];
        date = date.split('-');
        time = time.split(':');
        return {
            year: parseInt(date[0]),
            month: date[1] - 1,
            day: parseInt(date[2]),
            hour: parseInt(time[0]),
            minute: parseInt(time[1]),
            second: parseInt(time[2])
        };
    };

    var checkDST = function(isAmerican) {
        var result = false,
            dateObj = new Date(),
            month = self.serverTime.getUTCMonth(),
            day = self.serverTime.getUTCDate();
        if(month > 2 && month < 9) {
            result = true;
        }
        else if(month < 2 && month > 10) {
            result = false;
        }
        else {
            switch(month) {
                //март переход на летнее время
                case 2 :
                    if(isAmerican) {
                        result = (day >= dateObj.getSunday(2, 2).getDate());
                    }
                    else {
                        result = (day >= dateObj.getLastSunday(2).getDate());
                    }
                    break;
                //октябрь, переход на зимнее время в Европе
                case 9 :
                    if(!isAmerican) {
                        result = (day <= dateObj.getLastSunday(9).getDate());
                    }
                    else {
                        result = true;
                    }
                    break;
                //ноябрь, переход на зимнее время в США
                case 10 :
                    if(isAmerican) {
                        result = (day <= dateObj.getFirstSunday(10).getDate());
                    }
                    break;
            }
        }

        return result;
    };

    var setOffsetTimers = function(o) {
        var time, isDaylightSaving;
        if(o.currentTime) {
            return new Date();
        }
        isDaylightSaving = checkDST(o.isAmericanDST);
        time = self.serverTime.getTime() + o.offset * 3600000;
        time = (isDaylightSaving && o.savingTime) ? time + 3600000 : time;
        return new Date(time);
    };

    var formatTimeOutput = function(val) {
        return (val < 10) ? '0' + val : val;
    };

    this.utcDate = parseDate(o.currentTime);
    this.serverTime = new Date(Date.UTC(this.utcDate.year, this.utcDate.month, this.utcDate.day,
                                        this.utcDate.hour, this.utcDate.minute, this.utcDate.second));

    this.setTimers = function() {
        $.each(self.sessionSchedule, function() {
            timers.push(setOffsetTimers(this));
        });
    };

    this.killTimers = function() {
        for(var i = 0; i < timers.length; i++) {
            timers[i] = null;
        };
        timers = new Array();
    }

    this.renderSchedule = function() {
        var container = $('.session-schedule'), i = 0;
        container.find('*').remove();
        $.each(self.sessionSchedule, function() {
            var markup;
            var dateText = '';

            if(this.currentTime) {
                dateText = formatTimeOutput(timers[i].getHours()) + ':' + formatTimeOutput(timers[i].getMinutes());
            }
            else {
                dateText = formatTimeOutput(timers[i].getUTCHours()) + ':' + formatTimeOutput(timers[i].getUTCMinutes());
            }

            markup = $('<div class="session-schedule-item"></div>');
            markup.append($('<div class="session-city"></div>').text(this.name));
            markup.append($('<div class="session-city-time">' +
                '<div class="session-clock passive"></div>' +
                '<div class="current-session-time">' + dateText +
                '</div>' + '</div>'
            ));
            container.append(markup);
            i++;
        });
    };

    this.refreshSchedule = function(interval) {
        var newTime, oldTime = self.serverTime.getTime();
        interval = interval || 60000;
        newTime = oldTime + interval;
        self.serverTime.setTime(newTime);
        self.killTimers();
        self.setTimers();
        self.renderSchedule();
    };

    this.getSeconds = function () {
        return self.serverTime.getSeconds();
    };

}
