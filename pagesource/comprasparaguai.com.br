<script type="text/javascript">
function adicionar_lista_desejo(produto_pk){
    var url = '/lista-desejos/adicionar-produto/';
    var dados = { 'produto_pk' : produto_pk };

    $.post(url, dados, function(data){
        var adicionado = '#desejo_'+produto_pk;
        $('.wishlist a').html('Lista de desejos ('+data['qtd']+')');
        if (!data['erro']) {
            
                $(adicionado).addClass('adicionado-desejo').html(data['msg']);
            
        }
        else {
            alert(data['msg']);
        }
        });
}
</script>

