				data: {
					email: email,
					conversao: 'quadro-receber-novidades-home-site',
					csrfmiddlewaretoken: '1uWwZCJ2TaQtNPgNPS08YgP7teGIPvdu'
				},
				dataType: 'json',

				success: function (response) {

					$('#mensagemAviso').html(response.message);
					$('#mensagemAviso').show();

					if (response.status) {
						$('#mensagemAviso').removeClass('alert-warning');
						$('#mensagemAviso').addClass('alert-success');
						$('form#newsletter').hide()
					}
					else {
						$('#mensagemAviso').removeClass('alert-success');
						$('#mensagemAviso').addClass('alert-warning');
					}
				},

