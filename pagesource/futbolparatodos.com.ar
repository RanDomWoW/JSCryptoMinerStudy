        </p>
    </div>
</div>            </div>
        </div>
        <div class="loader page-loader">
            <img src="http://www.futbolparatodos.com.ar/assets/img/loader.gif" alt="Loader">
        </div>
        <div class="loader global-loader">
            <img src="http://www.futbolparatodos.com.ar/assets/img/loader.gif" alt="Loader">
        </div>
        <!-- Match information modal -->
        <div class="modal fade" id="match-information-modal" tabindex="-1" role="dialog" aria-labelledby="match-information-modal-label">
        </div>
        <!-- Match information modal -->
        <!-- Generic modal -->
        <div class="modal fade" id="generic-modal" tabindex="-1" role="dialog" aria-labelledby="generic-modal-label">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <span class="glyphicon glyphicon-alert modal-icon" aria-hidden="true"></span>
                        <span class="modal-title" id="generic-modal-label">NOTIFICACIÓN</span>
                    </div>
                    <div class="modal-body">
                        <div id="message-generic-modal"></div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="button  button-danger" data-dismiss="modal">CERRAR</button>
                    </div>
                </div>
            </div>
        </div>
        <!-- End generic modal -->
        <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
        <script type="text/javascript" src="http://www.futbolparatodos.com.ar/assets/js/bootstrap/modal.js"></script>
        <script type="text/javascript">
            var image_url;
            var sending_request = false;
            var active_session = false;
            var tag = 'general';
            var notification_delay_time = 0;
            var status_change_messages = {
                '1': 'Ha comenzado el partido entre {team1} y {team2}.',
                '6': 'Ha comenzado el segundo tiempo del partido entre {team1} y {team2}.',
                '8': 'Ha comenzado el primer tiempo suplementario del partido entre {team1} y {team2}.',
                '10': 'Ha comenzado el segundo tiempo suplementario del partido entre {team1} y {team2}.',
                '12': 'Ha comenzado la definición por penales del partido entre {team1} y {team2}.',
                '5': 'Ha finalizado el primer tiempo del partido entre {team1} y {team2}.',
                '7': 'Ha finalizado el segundo tiempo del partido entre {team1} y {team2}.',
                '9': 'Ha finalizado el primer tiempo suplementario del partido entre {team1} y {team2}.',
                '11': 'Ha finalizado el segundo tiempo suplementario del partido entre {team1} y {team2}.',
                '2': 'Ha finalizado el partido entre {team1} y {team2}.',
                '3': 'El partido entre {team1} y {team2} ha sido suspendido.',
                '4': 'El partido entre {team1} y {team2} ha sido postergado.',
            };
            var extra_messages = {
                WINNER: 'El encuentro lo ha ganado {team} por {goals1} a {goals2}.',
                TIE: 'El encuentro ha terminado empatado {goals1} a {goals2}.',
                WINNING: 'El encuentro lo está ganando {team} por {goals1} a {goals2}.',
                TYING: 'El encuentro está empatado {goals1} a {goals2}.',
                GOAL: 'Gol de {team}.'
            };

            function pageUnderConstructionModal(){
                showGenericModal('La página solicitada se encuentra en construcción. Intente nuevamente dentro de unos días.');
            }

            function resetNotificationDelayTime() {
                notification_delay_time = 0;
            }

            function increaseNotificationDelayTime() {
                notification_delay_time += 400;
            }

            function clearhashFromURL() {
                if (window.history && history.pushState) {
                    window.history.pushState("", document.title, window.location.pathname);
                } else {
                    var scroll = {
                        top: document.body.scrollTop,
                        left: document.body.scrollLeft
                    };
                    window.location.hash = '';
                    document.body.scrollTop = scroll.top;
                    document.body.scrollLeft = scroll.left;
                }
            }

            if (window.location.hash && window.location.hash == '#_=_') {
                clearhashFromURL();
            }

            function showGenericModal(message) {
                $('#message-generic-modal').html(message);
                $('#generic-modal').modal('toggle');
            }

            function activeUpdateMatches() {
                $('.update-matches').click(function() {
                    $('.page-loader').css('display', 'block');
                    current_tag = $(this);
                    data_match = $(this).data('match');
                    $.ajax({
                        url: "http://www.futbolparatodos.com.ar/partidos",
                        data: data_match,
                        cache: false,
                        success: function(result) {
                            current_tag.parent().parent().parent().html(result);
                            activeUpdateMatches();
                            activeShowMatchInformation();
                            $('.page-loader').css('display', 'none');
                        },
                        error: function(err) {
                            showGenericModal('Error al mostrar fecha.');
                            $('.page-loader').css('display', 'none');
                        }
                    });
                });
            }

            activeUpdateMatches();

            function activeShowMatchInformation() {
                $('.match-row, .key-match').click(function() {
                    showMatchInformation({
                        match: $(this).data('match'),
                        league: $(this).data('league')
                    });
                });
            }

            function showMatchInformation(data) {
                $('.page-loader').css('display', 'block');
                current_tag = $(this);
                $.ajax({
                    url: "http://www.futbolparatodos.com.ar/informacion-partido",
                    data: data,
                    cache: false,
                    success: function(result) {
                        window.location.hash = 'partido-' + data.match;
                        $('#match-information-modal').html(result);
                        $('#match-information-modal').modal('toggle');
                        $('.page-loader').css('display', 'none');
                    },
                    error: function(err) {
                        showGenericModal('Error al mostrar el partido.');
                        $('.page-loader').css('display', 'none');
                    }
                });
            }

            function addExtraMessage(message, updated_match, is_goal) {
                goals_teams = getTeamsGoals(updated_match);
                local_goals = goals_teams.local;
                visitor_goals = goals_teams.visitor;

                if (local_goals != visitor_goals) {
                    if (updated_match.status.id == '2') {
                        extra_message = extra_messages.WINNER;
                    } else if (is_goal || (updated_match.status.id != '1' && updated_match.status.id != '3' && updated_match.status.id != '4')) {
                        extra_message = extra_messages.WINNING;
                    } else {
                        return message;
                    }
                    if (local_goals > visitor_goals) {
                        extra_message = extra_message.replace('{team}', updated_match.local.name);
                        extra_message = extra_message.replace('{goals1}', updated_match.local.goals);
                        extra_message = extra_message.replace('{goals2}', updated_match.visitor.goals);
                    } else {
                        extra_message = extra_message.replace('{team}', updated_match.visitor.name);
                        extra_message = extra_message.replace('{goals1}', updated_match.visitor.goals);
                        extra_message = extra_message.replace('{goals2}', updated_match.local.goals);
                    }
                } else {
                    if (updated_match.status.id == '2') {
                        extra_message = extra_messages.TIE;
                    } else if (is_goal || (updated_match.status.id != '1' && updated_match.status.id != '3' && updated_match.status.id != '4')) {
                        extra_message = extra_messages.TYING;
                    } else {
                        return message;
                    }
                    extra_message = extra_message.replace('{goals1}', updated_match.local.goals);
                    extra_message = extra_message.replace('{goals2}', updated_match.visitor.goals);
                }

                return message += ' ' + extra_message;
            }

            function addGoalsScorers(condition, updated_match, match_modal) {
                css_class = (condition == 'local') ? 'pull-left' : 'pull-right';
                template = '<div class="clearfix"><div class="goal-minutes ' + css_class + '"><b>{minutes} {time}</b></div><div class="author-goal ' + css_class + '">{player} ({type})</div></div>';

                html_result = '';
                html = '';
                goalscorers = updated_match[condition].goalscorers;
                goalscorers = goalscorers ? goalscorers : [];
                for (var i = 0; i < goalscorers.length; i++) {
                    author = goalscorers[i];
                    html = template.replace('{minutes}', author.minutes);
                    html = html.replace('{time}', author.time);
                    html = html.replace('{player}', author.player);
                    html = html.replace('{type}', author.type);

                    html_result += html;
                }

                match_modal.find('.authors-' + condition + '-goals').html(html_result);
            }

            function setAndNotifyGoals(condition, updated_match, match_row, match_modal) {
                goal_difference = getGoalDifference(condition, updated_match, match_row);
                key_match_box = $('.key-match-' + updated_match['id']);

                if (goal_difference > 0) {
                    for (var n = 0; n < goal_difference; n++) {
                        message = extra_messages.GOAL;
                        message = message.replace('{team}', updated_match[condition].name);

                        if (n == goal_difference - 1 && getGoalDifference(condition == 'local' ? 'visitor' : 'local', updated_match, match_row) == 0) {
                            message = addExtraMessage(message, updated_match, true);
                        }

                        notify(message);
                    }

                    match_row.find('.' + condition + '-goals').addClass('annotation');
                    match_modal.find('.' + condition + '-goals').addClass('annotation');
                    key_match_box.find('.' + condition + '-goals').addClass('annotation');

                }

                match_modal.find('.' + condition + '-goals').text(updated_match[condition].goals);
                match_row.find('.' + condition + '-goals').text(updated_match[condition].goals);

                
                key_match_box.find('.' + condition + '-goals').text(updated_match[condition].goals);
            }



            function getGoalDifference(condition, updated_match, match_row) {
                current_goals = match_row.find('.' + condition + '-goals').text();
                current_goals = isNaN(current_goals) ? current_goals.replace(/\s/g, '') : parseInt(current_goals);

                if (current_goals != updated_match[condition].goals) {
                    if (current_goals == '-') {
                        current_goals = 0;
                    }
                    if (havePenalties(updated_match[condition].goals)) {
                        updated_penalty_goals = getPenaltyGoals(updated_match[condition].goals);
                        current_penalty_goals = getPenaltyGoals(current_goals);
                        return updated_penalty_goals - current_penalty_goals;
                    }
                    return updated_match[condition].goals - current_goals;
                } else {
                    return 0;
                }
            }

            function getPenaltyGoals(goals) {
                var reg_exp = /\(([^)]+)\)/;
                penalty_goals = reg_exp.exec(goals);
                if (!penalty_goals || !penalty_goals[1]) {
                    return 0;
                }
                return parseInt(penalty_goals[1]);
            }

            function havePenalties(goals) {
                var reg_exp = /\(([^)]+)\)/;
                penalty_goals = reg_exp.exec(goals);
                return penalty_goals && penalty_goals[1];
            }

            function getTeamsGoals(updated_match) {
                if (havePenalties(updated_match.local.goals) ||
                    havePenalties(updated_match.visitor.goals)) {
                    return {
                        local: getPenaltyGoals(updated_match.local.goals),
                        visitor: getPenaltyGoals(updated_match.visitor.goals)
                    }
                }
                return {
                    local: updated_match.local.goals,
                    visitor: updated_match.visitor.goals
                }
            }

            function timeFinalized(status_match) {
                return status_match == '5' || status_match == '7' || status_match == '9' || status_match == '11' ||
                    status_match == '2' || status_match == '3' || status_match == '4';
            }

            function updateGoals(updated_match, match_row, match_modal) {
                setAndNotifyGoals('local', updated_match, match_row, match_modal);
                setAndNotifyGoals('visitor', updated_match, match_row, match_modal);

                addGoalsScorers('local', updated_match, match_modal);
                addGoalsScorers('visitor', updated_match, match_modal);
            }

            function updateMatchStatus(updated_match, match_row, match_modal) {
                if (updated_match.status.id != match_row.data('status')) {
                    message = status_change_messages[updated_match.status.id + ''];
                    message = message.replace('{team1}', updated_match.local.name);
                    message = message.replace('{team2}', updated_match.visitor.name);

                    message = addExtraMessage(message, updated_match);

                    match_row.data('status', updated_match.status.id);
                    match_row.find('.match-time').replaceWith(updated_match.status.html);
                    match_modal.find('.match-time').replaceWith(updated_match.status.html);

                    notify(message);
                }
            }

            function updateMatchesResults() {
                var matches_to_update = [];
                $('.match-row').each(function() {
                    if ($(this).data('status') != '2') {
                        match_to_update = {};
                        match_to_update.id = $(this).data("match");
                        match_to_update.league = $(this).data("league");
                        matches_to_update.push(match_to_update);
                    }
                });

                if (matches_to_update.length == 0)
                    return;

                $.ajax({
                    url: "http://www.futbolparatodos.com.ar/partidos/resultados",
                    data: {
                        matches: matches_to_update
                    },
                    cache: false,
                    success: function(result) {
                        updated_matches = JSON.parse(result);
                        for (var i = 0; i < updated_matches.length; i++) {
                            updated_match = updated_matches[i];

                            match_row = $('tr[data-match="' + updated_match.id + '"]');
                            match_modal = $('.modal-body[data-match="' + updated_match.id + '"]');

                            if (timeFinalized(updated_match.status.id)) {
                                updateGoals(updated_match, match_row, match_modal);
                                updateMatchStatus(updated_match, match_row, match_modal);
                            } else {
                                updateMatchStatus(updated_match, match_row, match_modal);
                                updateGoals(updated_match, match_row, match_modal);
                            }

                            setTimeout(function() {
                                $('.local-goals, .visitor-goals').each(function() {
                                    $(this).removeClass('annotation');
                                });
                            }, 15000);

                            resetNotificationDelayTime();
                        };
                    },
                    error: function(err) {
                        console.log('Error al actualizar los partidos.');
                    }
                });
            }

            function notify(message) {
                setTimeout(function() {
                    if (!("Notification" in window)) {
                        alert("Este browser no soporta notificaciones de escritorio");
                    } else if (Notification.permission === "granted") {
                        var notification = new Notification('Notificación Fútbol para Todos', {
                            body: message,
                        });
                    } else if (Notification.permission !== 'denied') {
                        Notification.requestPermission(function(permission) {
                            if (permission === "granted") {
                                var notification = new Notification('Notificación Fútbol para Todos', {
                                    body: message,
                                });
                            }
                        });
                    }
                }, notification_delay_time);
                increaseNotificationDelayTime();
            }

            $(function() {

                $('#match-information-modal').on('hidden.bs.modal', function() {
                    clearhashFromURL();
                })

                $('#button-close-notification').click(function() {
                    $(this).parent().hide();
                });

                if (window.location.hash && window.location.hash.indexOf('partido-') !== -1) {
                    match_id = window.location.hash.split('-')[1];
                    league = $('tr[data-match="' + match_id + '"]').trigger('click').data('league');
                    showMatchInformation({
                        match: match_id,
                        league: league || tag
                    });
                }

                activeShowMatchInformation();

                setInterval(updateMatchesResults, 30000);

                $(".navbar-toggle").click(function() {
                    $(".menu").slideToggle();
                });
            });
        </script>
