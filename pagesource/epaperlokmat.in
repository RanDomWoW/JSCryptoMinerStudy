function getissuebydate(artdate){
	
	var realdate = artdate;
	 
	var splitdate = realdate.split("-");
	console.log(splitdate);
	Year = splitdate[0];
    month = splitdate[1];
    date = splitdate[2];
	
	
	var htmlvalfontpage='';
    $.ajax({
        url: "http://epaperlokmat.in/eNewspaper/OutSourcingData.php?operation=getIssuebyDate&epaperid=LOK&date="+artdate,
        dataType: 'json',
        data: {
            'operation': "getIssuebyDate",
            'issueid': artdate
        },
       	success: function(result)
			{
				if(result.length>0)
				{
					$("#mydata").html("");
					$("#section-linetriangle-2").html("");
					
					for(var i=0;i<result.length;i++)
					{
						
						var date=result[i].date;
