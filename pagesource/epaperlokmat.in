						var issueid=result[i].issueid;
						var editionname = result[i].edition_description;
						var edname = encodeURIComponent(editionname);
						var editionid = result[i].edition_id;
						var edidsplit = editionid.split("_");
						var imgname = result[i].imagename;
						var pathsplit = imgname.split("/");
						
						if(artdate <= datecheckr)
	{
		var imgpath = "http://epaper.lokmat.com.s3-website.ap-south-1.amazonaws.com/eNewspaper/News/"+edidsplit[0]+"/"+edidsplit[1]+pathsplit[1]+"/"+pathsplit[2]+"/"+pathsplit[3]+"/"+pathsplit[4]+"/"+pathsplit[5]+"/"+pathsplit[6];
		var link = "http://epaperlokmat.in/Archive/main-editions/"+edname+"/"+newdate+"/1";
	}
	else{
		var imgpath = "http://epaperlokmat.in/eNewspaper/News/"+edidsplit[0]+"/"+edidsplit[1]+pathsplit[1]+"/"+pathsplit[2]+"/"+pathsplit[3]+"/"+pathsplit[4]+"/"+pathsplit[5]+"/"+pathsplit[6];
		var link = "http://epaperlokmat.in/main-editions/"+edname+"/"+newdate+"/1";
	}
						
