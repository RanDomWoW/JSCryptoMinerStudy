    $("a[lid="+returnID+"]").attr("style","background:#DADAD8;border:1px solid #DADAD8");
  else
     $("a[lid]").eq(0).attr("style","background:#DADAD8;border:1px solid #DADAD8");
}

 sortid_uid();
    </script>

