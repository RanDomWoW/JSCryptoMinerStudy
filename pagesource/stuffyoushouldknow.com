	var infinite = new Waypoint.Infinite({
		element: $('.infinite-container')[0],
		onBeforePageLoad: function () {
			HSW.ads.refresh();
			$('#infinite-loading').removeClass('hidden');
		},
		onAfterPageLoad: function ($item) {
			$('#infinite-loading').addClass('hidden');
			//Disable lazy load on AJAX-loaded page
			$.each($item.find('img[data-src]'), function () {
				$(this).attr('src', $(this).attr('data-src'));
			});
		}
	});

	if (HSW.utilities.isMobile() === false) {
		//Make right rail bottom ad sticky
		$(document).ready(function(){
			$("#ad-wrap-rectangleRailBottom").sticky({
				topSpacing:10,
				bottomSpacing:400
			});
		});
	}
