			<script>
    scrollEventTrack();

    function scrollEventTrack(){

        $(window).scroll(function() {   
            var scrollDistance = getScrollDistance();

            for (var i = dataLayer.length-1; i > 0; i--) {
               if (dataLayer[i].eventCategory == 'Scroll') {
                   var scrollDistancePrev = dataLayer[i].eventLabel;
                   break;
               }
            }


            if(typeof scrollDistancePrev == "undefined" || (typeof scrollDistancePrev != "undefined" && scrollDistancePrev != scrollDistance)){
               var isOptanonSet = !!getCookie('OptanonConsent');
               var isCategoryAccepted = false;

               if(isOptanonSet) {
                   var cookieValue = getCookie('OptanonConsent');   
                      if(checkOptanonCategory(cookieValue, 2) && checkOptanonCategory(cookieValue, 4) && checkOptanonCategory(cookieValue, 5)){
                          isCategoryAccepted = true;                      
                      }
                   }

                   if((!isOptanonSet || isCategoryAccepted) && scrollDistance >= 25){
                      pushScrollEventTrack(scrollDistance);
                   }
            }             
        });
    }

    function getScrollDistance(){
        var scrollPercent = 100 * $(window).scrollTop() / ($(document).height() - $(window).height());
        var scrollDistance;

        if(scrollPercent < 25)
            scrollDistance = 0;
        else if(scrollPercent >= 25 && scrollPercent < 50)
            scrollDistance = 25;
        else if(scrollPercent >= 50 && scrollPercent < 75)
            scrollDistance = 50;
        else if(scrollPercent >= 75 && scrollPercent < 100)
            scrollDistance = 75;
        else if(scrollPercent >= 100)
            scrollDistance = 100;

        return scrollDistance;
    }

    function pushScrollEventTrack(scrollDistance){
        /* crée un nouvel élément (true) ou  l'update uniquement (false)*/
        var newTag = true;

        /* UPDATE du Tag existant*/
        /* On veut juste en rajouter à chaque fois. pour maj décommenter le code suivant */
        /*
        for (var i in dataLayer) {
            if (dataLayer[i].eventCategory == 'Scroll') {
                dataLayer[i].eventLabel = scrollDistance;
                newTag = false;
                break;
            }
        }
        */

        /* AJOUT d'un nouveau Tag*/
        if(newTag){
            dataLayer.push({
                'event': 'GAevent',
                'eventCategory': 'Scroll',
                'eventAction': jQuery("input[name='gtm_type_page']").val(),
                'eventLabel':scrollDistance
            });
        }
    }

   function checkOptanonCategory(cookieValue, category){
        var optanonConsent = new RegExp('[\?&]?' + 'groups' + '=([^]*)').exec(cookieValue);

        if (optanonConsent && optanonConsent.length > 1) {
            optanonConsent = optanonConsent[1];
            optanonConsent = optanonConsent.replace(/%3A/g,":");
            optanonConsent = optanonConsent.replace(/%2C/g,",");
            
            console.log('optanonConsent',optanonConsent);

            if(optanonConsent.indexOf(category+":1") > -1 ){
                return true;
            } else {
                return false;
            }
        }
        return false;
    }

    function getCookie(name) {
        var value = "; " + document.cookie;
        var parts = value.split("; " + name + "=");
        if (parts.length == 2) return parts.pop().split(";").shift();
    }
 
</script>
