    <script>
        $( document ).ready(function() {

            var urlAction = '';

            $('#btnVestibular').on('click', function () {
                $('#hidTipoVestibular').val("VESTIBULAR");
            });
            $('#btnReingresso').on('click', function () {
                $('#hidTipoVestibular').val("REINGRESSO");
            });
            $('#btnTransferencia').on('click', function () {
                $('#hidTipoVestibular').val("TRANSFERENCIA");
            });

        });

        function validacao () {

            if($('input[name="nome"]').val().length === 0) {
                $('input[name="nome"]').val($('.register-form-mobile').find('input[name="nome"]').val());
            }
            if($('input[name="cpf"]').val().length === 0) {
                $('input[name="cpf"]').val($('.register-form-mobile').find('input[name="cpf"]').val());
            }
            if($('input[name="email"]').val().length === 0) {
                $('input[name="email"]').val($('.register-form-mobile').find('input[name="email"]').val());
            }
            if($('input[name="celular"]').val().length === 0) {
                $('input[name="celular"]').val($('.register-form-mobile').find('input[name="celular"]').val());
            }

            var urlParameters = 'nome=' + $('input[name="nome"]').val() + '&cpf=' + $('input[name="cpf"]').val() + '&mail=' + $('input[name="email"]').val() + '&celular=' + $('input[name="celular"]').val();
            var urlPreInscricao = 'http://fsg.br/pre-inscricao-vestibular/?nome=' + $('input[name="nome"]').val() + '&email=' + $('input[name="email"]').val() + '&phone=' + $('input[name="celular"]').val();

            if (
                $('input[name="nome"]').val().length === 0 ||
                $('input[name="cpf"]').val().length === 0 ||
                $('input[name="email"]').val().length === 0 ||
                $('input[name="celular"]').val().length === 0
                ) {
                    alert('Você deve preencher todos os campos para prosseguir!');
                    return;
            }

            if ($('#hidTipoVestibular').val() == 'VESTIBULAR') {
                urlAction = 'http://inscricao.fsg.br/?' + urlParameters;
                var data_array = [
                    { name: 'identificador', value: 'capa_vestibular_2018_1' },
                    { name: 'token_rdstation', value: 'da1f79379df778566ad3e0b911299213' },
                    { name: 'name', value: $('input[name="nome"]').val() },
                    { name: 'email', value: $('input[name="email"]').val() },
                    { name: 'phone', value: $('input[name="celular"]').val() },
                    { name: 'cpf', value: $('input[name="cpf"]').val() },
                ];

                RdIntegration.post(data_array, function() {
                    window.location.href = urlAction;
                });
            } else if ($('#hidTipoVestibular').val() == 'REINGRESSO') {
                urlAction = 'http://reingresso.fsg.br/index.php' + '?' + urlParameters;
                var data_array = [
                    { name: 'identificador', value: 'capa_reingresso_2018_1' },
                    { name: 'token_rdstation', value: 'da1f79379df778566ad3e0b911299213' },
                    { name: 'name', value: $('input[name="nome"]').val() },
                    { name: 'email', value: $('input[name="email"]').val() },
                    { name: 'phone', value: $('input[name="celular"]').val() },
                    { name: 'cpf', value: $('input[name="cpf"]').val() },
                ];

                RdIntegration.post(data_array, function() {
                    window.location.href = urlAction;
                });
            } else if ($('#hidTipoVestibular').val() == 'TRANSFERENCIA') {
                urlAction = 'http://transferencia.fsg.br/index.php' + '?' + urlParameters;
                var data_array = [
                    { name: 'identificador', value: 'capa_transferencia_2018_1' },
                    { name: 'token_rdstation', value: 'da1f79379df778566ad3e0b911299213' },
                    { name: 'name', value: $('input[name="nome"]').val() },
                    { name: 'email', value: $('input[name="email"]').val() },
                    { name: 'phone', value: $('input[name="celular"]').val() },
                    { name: 'cpf', value: $('input[name="cpf"]').val() },
                ];

                RdIntegration.post(data_array, function() {
                    window.location.href = urlAction;
                });
            }

        }

    </script>

