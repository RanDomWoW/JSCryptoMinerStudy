                  <script type="text/javascript" src="http://www.fsg.br/assets/js/base64.js"></script>
                  <script type="text/javascript" src="http://www.fsg.br/assets/js/jquery.min.js"></script>
                  <script type="text/javascript">
                      $(document).ready(function() {
                        var uri_base_path = 'http://www.fsg.br/';
                        $('form#frmLogin input#apsweb_usr, form#frmLogin input#senha').focus(function(){
                          var menu = $('li.dropdown.open-aluno');
                          if (!menu.hasClass('open')) {
                            $('li.dropdown.open-aluno').addClass('open');
                          }
                        });
                        // Recuperação de senha
                        $('a#recuperar-senha').click(function(){
                          $('#frmLogin').slideUp();
                          $('#frmResetPass').slideDown();
                          return false;
                        });
                        $('#cancelar-recuperacao').click(function(){
                          fecharAlerta('.msg-recuperar');
                          $('#frmResetPass').slideUp();
                          $('#frmLogin').slideDown();
                          $('.camposLogin').slideDown('Slow');
                          $('#btnSubmitLogin').slideDown('Slow');
                          $('#escolha').hide();
                        });
                        $('#frmResetPass').submit(function(){
                          if ($('#userResetPass').val().length > 0 && $('#emailPass').val().length > 0) {
                            $('.msg-recuperar').html('Aguarde...').slideDown('Slow');
                            $('#btnSubmitResetPass').attr('disabled', true);
                            $.ajax({
                              url: uri_base_path + 'php/login/sendResetPass.php',
                              type: 'POST',
                              data: $('#frmResetPass').serialize(),
                              success: function(data) {
                                $('.msg-recuperar').html(data).slideDown('Slow');
                                $('#btnSubmitResetPass').attr('disabled', false);
                              }
                            });
                          } else {
                            $('#btnSubmitResetPass').attr('disabled', false);
                            $('.msg-recuperar').html('Preencha os campos corretamente.').slideDown('Slow');
                          }
                          return false;
                        });
                        // Login
                        $('#buttonProfessor').click(function(){
                          $('#frmLogin').attr('action', 'https://portal.fsg.br/index.php5?irPage=login&irModulo=professor');
                          frmLogin.submit();
                        });

                        $('#buttonAluno').click(function(){
                          $('#frmLogin').attr('action', 'https://portal.fsg.br/index.php5?irPage=login&irModulo=aluno');
                          frmLogin.submit();
                        });

                        $('#cancelar').click(function(){
                          $('.camposLogin').slideDown('Slow');
                          $('#btnSubmitLogin').slideDown('Slow');
                          $('#escolha').hide();
                        });
                        $('#frmLogin').submit(function(){
                          if ($('#apsweb_usr').val().length > 0 && $('#senha').val().length > 0) {
                            var dados = $(this).serialize();
                            $('#apsweb_passwdSend').val(
                              Base64.encode($('#senha').val())
                            );
                            $.ajax({
                              type: 'POST',
                              url: uri_base_path + 'php/login/carregaPortalCorreto.php',
                              data: dados,
                              dataType: 'json',
                              success: function (data) {
                                var radioUnidade = $('form#frmLogin [name="apsweb_unidade"]:checked');

                                if (radioUnidade.length && radioUnidade.val() == 'FTSG') {
                                  $('form#frmLogin input#apsweb_empuni').val('12,2');
                                } else {
                                  if (null !== data.unidadeEmpresa) {
                                    $('form#frmLogin input#apsweb_empuni').val(data.unidadeEmpresa);
                                  } else {
                                    $('form#frmLogin input#apsweb_empuni').val('1,11');
                                  }
                                }

                                if (data.portal == 0) {
                                  $('.msg').slideDown('Slow');
                                } else if (data.portal == 1) {
                                  $('#frmLogin').attr('action', 'https://portal.fsg.br/index.php5?irPage=login&irModulo=aluno');
                                   frmLogin.submit();
                                } else if (data.portal == 2) {
                                  $('.camposLogin').hide();
                                  $('#btnSubmitLogin').hide('Slow');
                                  $('#escolha').slideDown('Slow');
                                  if ($('#frmLogin .msg').is(':visible')) {
                                    $('#frmLogin .msg').slideUp('Slow');
                                  }
                                } else if (data.portal == 3) {
                                  $('#frmLogin').attr('action','https://portal.fsg.br/index.php5?irPage=login&irModulo=professor');
                                  frmLogin.submit();
                                }
                              }
                            });
                          } else {
                            $('.msg').slideDown('Slow');
                          }
                          return false;
                        });
                        });
                      function fecharAlerta(alerta) {
                        $(alerta).slideUp('Slow');
                      }
                  </script>
