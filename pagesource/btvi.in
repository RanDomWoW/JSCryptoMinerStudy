<script type="text/javascript" >

jQuery(document).ready(function(){
    jQuery("#asia").css("display","block");   
    
    jQuery(".menu-link").each(function(){
      menu_text = jQuery(this).attr("id");
      //console.log(menu_text);
      //console.log(menu_text.toUpperCase()+"   "+current_link);
      if(menu_text.toUpperCase().trim()=="HOME")
      {
        jQuery(".menu-link .active").removeClass("active");
        jQuery(this).addClass("active");
      }    
    });

    jQuery("#myCarousel3 ol li").on('click', function(e) {
        var activeTab = jQuery(this).text();
        var url = jQuery(this).data('hrefurl');
        jQuery("#carousel-heading").parent('a').prop("href", url);
        jQuery("#carousel-heading").text(activeTab);
    });

  

  });

</script>
