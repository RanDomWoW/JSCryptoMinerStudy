			$shopping_list = $('.shoppings-list');
			$cat_label_bt = $('.cat_label_bt');
			
			$search_show = $('.search_show');
			$search_show.click(function(){
				if($cat_menu_fake_check.prop("checked") == true){
					$cat_menu_fake_check.prop("checked", false);
					$shop_menu_fake_check.prop("checked", false);
					$cat_list.show();
					$shopping_list.hide();
				}
				
			});
			
			$('li.shoppings a').click(function(){
				$cat_list.hide();
				$shopping_list.show();
			});
			
			$('.back-menu .back-bt-label').click(function(){
				$cat_list.show()
				$shopping_list.hide();
			});
			
			$('.back-menu .close-bt-label').click(function(){
				$shop_menu_fake_check.prop("checked", false);
				$cat_list.show();
				$shopping_list.hide();
			});
			
			$cat_label_bt.click(function(){
				if($cat_menu_fake_check.prop("checked") == true){
					$cat_menu_fake_check.prop("checked", false);
				}else{
					$cat_menu_fake_check.prop("checked", true);
				}
				$shop_menu_fake_check.prop("checked", false);
				$cat_list.show();
				$shopping_list.hide();
			});
			
		</script>

