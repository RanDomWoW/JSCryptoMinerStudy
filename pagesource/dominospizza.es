<script type="text/javascript">

    $(document).ready(function () {

        //CartaPizzaView.init();

        $('#ProductoFiltro').on('change', function () {
            $('#masa-container').removeClass("show");
            $.ajax({
                type: "POST",
                url: '/Producto/Productofiltros',
                data: { filtro: $('#ProductoFiltro option:selected').val() },
                success: function (data) {
                    $("#CartaPizzas").html(data);
                },

            });
        });

        $('#promo-bebidas').on('click', function (evt) {
            if (typeof ga != "undefined") { ga('send', 'event', 'Banner_Promocion_Bebidas', 'Ir a oferta Pie', 'vegetales_cast.jpg'); }
            var Accion = '2';
            var Detalle_Accion = 'Inspirame';
            switch (Accion) {
                case '1':
                    if (Detalle_Accion != "") {
                        window.open(Detalle_Accion, '_blank');
                    }
                    //Link Externo

                    break;

                case '2':
                    evt.preventDefault();
                    //Link Interno
                    if (Detalle_Accion != "") {
                        window.location.href = '/' + Detalle_Accion;
                    }

                    break;

                case '3':
                    //Cambio de Armando
                    //Promocion
                    evt.preventDefault();
                    //var url = "/promociones";
                    var url = '/promociones#' + Detalle_Accion;
                    window.location.href = url;

                    break;

                case '4':
                    //Producto
                    evt.preventDefault();
                    if (Detalle_Accion == null || Detalle_Accion == undefined || Detalle_Accion == "") {
                        window.location.href = '/carta-de-pizzas';
                    } else {
                        Detalle_Accion = Detalle_Accion.replace(" ", "-");
                        var url = '/carta-de-pizzas/' + Detalle_Accion;
                        window.location.href = url;
                    }

                    break;
                case '5':
                    //Promoción para llevar
                    evt.preventDefault();
                    var url = '/promociones-pizza-para-llevar#' + Detalle_Accion;
                    window.location.href = url;
            }
        });



    });

    // esto ya está en cartaPizza.js - lo dejo comentado

    //$('#pizzas-container #title > span').on('click', function (evt) {
    //    $(this).toggleMasas(evt);
    //});

