    $('#banner-carousel img.play').on('click', function () {
        var videoContainer = $("#SeccionBlackVideo .video");
        var videoUrl = $(this).data('videourl');
        $('#SeccionBlackVideo').css("height", $('body').height() - $('#SeccionBlackVideo').css("padding-top").replace("px", ""));
        $('#SeccionBlackVideo').show();
        videoContainer.append('<iframe width="711" height="400" src="' + videoUrl + '" frameborder="0" allowfullscreen style="margin: 0 auto; position: absolute; left: 0; right: 0;"></iframe>');

        //$("#SeccionWhite").removeClass('lightbox black video');

        var closeButton = $("#SeccionBlackVideo .ico-close-white");

        closeButton.on('click', function () {
            var iframe = videoContainer.find('iframe');
            iframe.remove();
            $('#SeccionBlackVideo').hide();
        });
    });

    function imgClick(tipoAccion, detalleAccion, nombreImagen) {
        if (typeof ga != "undefined") { ga('send', 'event', 'Carrusel', 'Ir a oferta', nombreImagen); }
        switch (tipoAccion) {
            case '1':
                if (detalleAccion != "") {
                    window.open(detalleAccion, '_blank');
                }
                //Link Externo
                break;
            case '2':
                //Link Interno
                if (detalleAccion != "") {
                    window.location.href = '/' + detalleAccion;
                }
                break;
            case '3':
                //Promocion domicilio
                window.location.href = '/promociones#' + detalleAccion;
                break;
            case '4':
                //Producto
                var url;
                if (!detalleAccion || detalleAccion == "") { //(detalleAccion == null || detalleAccion == undefined || detalleAccion == "") {
                    url = '/carta-de-pizzas';
                    window.location.href = url;
                } else {
                    detalleAccion = detalleAccion.replace(" ", "-");
                    url = '/carta-de-pizzas/' + detalleAccion;
                    window.location.href = url;
                }
                break;
            case '5':
                //Promoción para llevar
                window.location.href = '/promociones-pizza-para-llevar#' + detalleAccion;
                break;
        }
    }

    /*Timeout de Banner Movil*/
    setTimeout(function () {
        CerrarBannerPopupMovil();
    }, 5000);

</script>




