 var ArticleData='';
	 $('div.articleBody').on('copy', function (e) {
		e.stopPropagation();
		e.preventDefault();
		var url = decodeURI(window.location.href);
		var title = $(document).find('title').text();
		var cd = e.originalEvent.clipboardData;
		ArticleData = title + '\n' + url;
		cd.setData('text/plain',ArticleData);
	});
    $('div.articleBody').selectionSharer();
</script>

