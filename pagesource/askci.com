    <script type="text/javascript">

        var isLoading = 0;
        $(function() {
            isLoading = 0;

        });

        function AddMoreNewsList() {
            if (isLoading == 1) {
                return;
            }
            $("#AddMoreNewsButton").val("加载中...");
            isLoading = 1;
            $.ajax({
                type: "POST",
                url: "/Home/AddMoreNewsList",
                data: { lastDate: $("#NewsLastDateHidden").val() },
                dataType: "json",
                success: function (data) {
                    isLoading = 0;
                    if (data == null || data.rv == null || data.rv.Flag != 1 || data.listNews == null) {
                        $("#AddMoreNewsButton").val("加载更多");
                        return;
                    }

                    if (data.listNews.length < 1) {
                        isLoading = 1;
                        $("#AddMoreNewsButton").val("没有更多了");
                        return;
                    }

                    var strHtml = "";
                    for (var i = 0; i < data.listNews.length; i++) {
                        var item = data.listNews[i];
                        if (item == null) {
                            continue;
                        }

                        strHtml = strHtml + '<div class="ct_b_l_list">';

                        var newsLogo = item.NewsLogo == null ? "" : item.NewsLogo;
                        var hClassName = "";
                        if (newsLogo != "") {
                            strHtml = strHtml + '<a href="' + item.NewsSavePath + '" class="enlarger ct_b_l_l_img fl" title="' + item.NewsTitle + '"  target="_blank" >';
                            strHtml = strHtml + '<img  class="enlargeimg" src="' + newsLogo + '" alt="' + item.NewsTitle + '"/>';
                            strHtml = strHtml + '</a>';
                            strHtml = strHtml + '<div class="ct_b_l_l_tbox ltboxwidth" >';
                        } else {
                            strHtml = strHtml + '<div class="ct_b_l_l_tbox " >';
                            hClassName = "hfzheifght";
                        }

                        strHtml = strHtml + '<a href="' + item.NewsSavePath + '"  target="_blank" title="' + item.NewsTitle + '">';
                        strHtml = strHtml + '<h1  class="tm ' + hClassName + '">' + item.NewsTitle + '</h1>';
                        strHtml = strHtml + '</a>';
                        strHtml = strHtml + '<p class="ct_b_l_l_tb_p">';
                        strHtml = strHtml + item.NewsContent + '<a href="' + item.NewsSavePath + '" title="' + item.NewsTitle + '  target="_blank" >【详情】</a>';
                        strHtml = strHtml + '</p>';

                        strHtml = strHtml + '<div class="ct_b_l_l_tb_hotf">';
                        strHtml = strHtml + '<span class="ct_b_l_l_tb_hftm fl">' + item.StrCreateDate + ' </span>';
                        strHtml = strHtml + '<div class="ct_b_l_l_tb_hfnav fr">';
                        if (item.ListTag != null && item.ListTag.length > 0) {
                            for (var k = 0; k < item.ListTag.length; k++) {
                                var tag = item.ListTag[k];
                                if (item.ListTag[k] == null) {
                                    continue;
                                }
                                var tagClassName = "";
                                if (k % 3 == 0) {
                                    tagClassName = "bakg03";
                                }
                                if (k % 3 == 1) {
                                    tagClassName = "bakg01";
                                }
                                if (k % 3 == 2) {
                                    tagClassName = "bakg02";
                                }
                                strHtml = strHtml + '<a class="' + tagClassName + '" href="' + tag.TagUrl + '"  target="_blank" title="' + tag.TagName + '">' + tag.TagName + '</a>';

                            }
                        }
                        strHtml = strHtml + '</div>';
                        strHtml = strHtml + '</div>';
                        strHtml = strHtml + '<div class="clear"></div>';


                        strHtml = strHtml + '</div>';
                        strHtml = strHtml + '</div>';

                    }
                    $("#AddMoreNewsButton").before(strHtml);

                    if (data.lastDate != null) {
                        $("#NewsLastDateHidden").val(data.lastDate);
                    }


                    $("#AddMoreNewsButton").val("加载更多");

                    gdHeight();

                    ////刷新百度分享
                    //window._bd_share_main.init();


                },
                beforeSend: function () {
                },
                complete: function () {
                },
                error: function (xmlHttpRequest, textStatus, errorThrown) {
                    //var msg = "状态：" + textStatus + ";出错提示：" + errorThrown;
                    //alert(msg);
                    isLoading = 0;
                    $("#AddMoreNewsButton").val("加载更多");
                }
                //timeout: 20000   
            });
        }

    </script>



<script type="text/javascript">
    function FeedbackClick() {
        var feedbackContent = $("#FeedbackContent").val();
        if (feedbackContent == "") {
            alert("内容不能为空");
            return;
        }
        var feedbackUrl = window.location.href;

        $("#SaveBtn").attr("disabled", "disabled");
        $.ajax({
            type: "POST",
            url: "/Feedback/Add",
            data: { FeedbackContent: feedbackContent, FeedbackUrl: feedbackUrl },
            dataType: "json",
            success: function(s) {
                $("#SaveBtn").removeAttr("disabled");
                if (confirm(s.ReturnMsg)) {
                    $("#bgheise").attr("style", "display:none;");
                    $("#login_abroad_box").addClass("rem_lo");
                    $(".rem_lo").removeClass("boxblock");
                }
            },
            beforeSend: function() {
            },
            complete: function() {
            },
            error: function(xmlHttpRequest, textStatus, errorThrown) {
                var msg = "状态：" + textStatus + ";出错提示：" + errorThrown;
                alert(msg);
                $("#SaveBtn").removeAttr("disabled");
            },
            timeout: 50000
        });
    }
</script>
