    <script type="text/javascript">
        $(function() {
            //header
            $('.header-opensoy').click(function(event) {
                event.preventDefault();
                $(this).closest('li').find('.header-soy').toggleClass('active');
            });

            var div = $('div')
            if (div.hasClass('b41-nav-lista')) {
                $(".header ").addClass('header-static-scroll');
                $(".menu-mobile-open ").addClass('header-static-scroll');

                function headerStatic() {
                    var altouno=$(".b41-cnt").innerHeight();
                    var altodos=$(".b41-nav").innerHeight();               
                    var st = $(window).scrollTop();
                    var headerScroll = $('.b41-nav-lista');
                    if (st >= altouno) {
                       headerScroll.addClass('b41-header-sticky');
                       $(".b41-cnt").css('margin-bottom', altodos);
                    }
                    else if(st <= altouno){
                        headerScroll.removeClass('b41-header-sticky');
                        // $(".header ").removeClass('header-static-scroll');
                        $(".b41-cnt").removeAttr('style');
                    }
                }
            }

           
            $(window).scroll(headerStatic);

            $(window).resize(function(event) {
               $(window).scroll(headerStatic);
            });

             //INICIO B5
            $('.b5-input input').focus(function() {
                $(this).closest('.b5-input').find('.b5-label').addClass('active');
            })
            $('.b5-input input').focusout(function() {
                if ($(this).val().length > 0) {
                    $(this).closest('.b5-input').find('.b5-label').addClass('active');
                } else {
                    $(this).closest('.b5-input').find('.b5-label').removeClass('active');
                }
            })
            //FIN B5
            $("form").validationEngine('attach', {
                promptPosition: "topLeft",
                autoHidePrompt: true,
                autoPositionUpdate: false,
                autoHideDelay: 2000,
                binded: false,
                scroll: false,
                validateNonVisibleFields: true,
                showOneMessage: true
            });
