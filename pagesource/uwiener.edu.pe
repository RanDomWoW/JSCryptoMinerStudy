            //     if (!valid) {
            //         // alert('error');
            //     } else {
            //         // alert('no-error');
            //     }
            // });

            $('#btn-enviar-suscripcion').click(function(event) {
                event.preventDefault();
                 var valid = $(this).closest('form').validationEngine('validate');
                 var form = $(this).closest('form').serialize();

                if (!valid) {
                    // alert('error');
                } else {
                    // alert('no-error');
                    $.ajax({
                        url: '/guardar-suscripcion',
                        type: 'POST',
                        dataType: 'json',
                        data: form,
                        success: function(response){
                            if (!response.status) {
                                alert(response.message);

                            } else {

                                location.href = '/gracias/'
                            }
                        }
                    })
                }       
                
            });

        });

         

        
