<script language="javascript" type="text/javascript">

    

    var evnTracker = 0;

    $(".dropdown-toggle").bind('keydown', function (e) {

        if (e.which >= 65 && e.which <= 90) { // tab
 
            var keyChar = String.fromCharCode(e.keyCode);
            e.preventDefault();

            if (evnTracker == 0)
            {
                $(this).parent().find('.dropdown-toggle').click();
            }

            var selectedItem = $(this).parent().find('.dropdown-menu a').filter(function () { return $(this).text().indexOf(keyChar) === 0; }).first();
            selectedItem.focus();

        }
    });

    $(".dropdown-menu").bind('keydown', function (event) {

        var keyChar = String.fromCharCode(event.keyCode);
        var selectedItem = $(this).find('a').filter(function () { return $(this).text().indexOf(keyChar) === 0; }).first();
        selectedItem.focus();

        if (event.keyCode == 9)
        {
            event.preventDefault();
        }
    });

    //// On dropdown close
    $(document).on('hidden.bs.dropdown', function (event) {
        var dropdown = $(event.target);
        // Set aria-expanded to false
        dropdown.find('.dropdown-menu').attr('aria-expanded', false);

        // Set focus back to dropdown toggle
        dropdown.find('.dropdown-toggle').focus();
        evnTracker = 0

    });


    // On dropdown open
    $(document).on('shown.bs.dropdown', function (event) {
        var dropdown = $(event.target);
        evnTracker = 1

        // Set aria-expanded to true
        dropdown.find('.dropdown-menu').attr('aria-expanded', true);

    });

</script>

