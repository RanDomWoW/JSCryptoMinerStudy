			$(window).on('load scroll',function(){
				$('.fade > *').each(function(){
					var elemPos = $(this).offset().top;
					var scroll = $(window).scrollTop();
					var windowHeight = $(window).height();
					if (scroll > elemPos - windowHeight + 100){
						$(this).addClass('fadein');
					}
				});
			});
		});
	</script>
		
