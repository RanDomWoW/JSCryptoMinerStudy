    function NavigateUrl(url, hash) {

        var fullUrl = 'http://' + location.host + url;

        try {
            if (window.location.href.split('#', 1000)[0] == fullUrl) {
                window.location.hash = hash;
                location.reload();
            }
            else {

                window.location.replace(fullUrl + hash);
            }
        }
        catch (e) {
            alert(e.message);
        }

    }
</script>



