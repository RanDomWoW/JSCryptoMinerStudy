	height: 150px;
	background-color: #FFF;
	border-bottom: 1px solid #DDD;
	box-sizing: border-box;
}

li.isotope-item:last-child {
	border-bottom: none;
}

.isotope-item a.link_title {
	position: absolute;
	overflow: hidden;
	top: 0;
	left: 0;
	height: 35px;
	display: block;
	padding-left: 20px;
	font-family: opensans, 'sans-serif' !important;
	font-size: 20px !important;
	line-height: 35px;
	font-weight: 400;
	color: #052E5E;
}

.isotope-item div.entry-content {
	position: absolute;
	top: 40px;
	height: 100px;
	padding: 0 30px 0 20px;
	overflow: hidden;
	line-height: 20px;
	color: #6D6D6D;
	width: 95%;
}

.isotope-item div.entry-content p {
	font-size: 14px;
}

.isotope-item a.vc_read_more {
	position: absolute;
	width: 32px;
	height: 32px;
	bottom: 0;
	right: 0;
	background: #a60000;
	font-size: 0 !important;
	color: #a60000;
	-webkit-transition: all .3s;
	-moz-transition: all .3s;
	-o-transition: all .3s;
	-ms-transition: all .3s;
	transition: all .3s;
}

.isotope-item a.vc_read_more:hover {
	background: #DA6000;
	color: #DA6000;
}

.isotope-item a.vc_read_more:before {
	content: '+';
	position: absolute;
	left: 50%;
	top: 50%;
	height: 20px;
	width: 20px;
	margin-left: -9px;
	margin-top: -21px;
	color: #FFF;
	font-family: opensans, sans-serif;
	font-size: 30px;
}

a.showallnews {
	float: right;
	width: 50%;
	background-color: #FFF;
	border-color: #D8D8D8;
	border-right-width: 20px;
	border-bottom-width: 20px;
	border-left-width: 10px;
	border-style: solid;
	padding: 1.5px 10px;
	box-sizing: border-box;
	font-size: 16px;
	font-weight: bold;
	color: #999;
}

@media only screen and (max-width: 767px) {
