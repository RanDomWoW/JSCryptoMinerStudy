      function jumpToPage(maxpage, url) {
          var page = $('#jumptopagenumber').val();

          var ival = parseInt(page);
          if (isNaN(ival)) {
              alert("Page numbers must be numeric");
              return;
          }

          if (ival > maxpage) {
              ival = maxpage;
          }

          if (ival < 1)
              ival = 1;

          ival = ival - 1;
          if (ival < 0)
              ival = 0;

          window.location.href = url + "" + ival;
      }
