    <script>

        // Global variable
        var is_touch = document.ontouchmove === null;
        var root_domain = 'vesti-ukr.com';
        var addthis_load = false;
        var addthis_config = {
            pubid: 'ra-56546f3b582746ad'
        };

        // Global function
        var addthis_init = function (){
            if(typeof addthis !== 'undefined' && addthis_load) {
                addthis.update('share', 'url', document.URL);
                addthis.update('share', 'title', document.title);
                addthis.update('share', 'description', $('meta[name=description]').attr('content'));
                if(typeof addthis.layers.refresh === 'function') {
                    addthis.layers.refresh();
                }
            }else{
                $.getScript('https://s7.addthis.com/js/300/addthis_widget.js#async=1', function () {
                    addthis.init();
                    addthis_load = true;
                });
            }
        };

        function show_imgs($elems) {
            $elems.each( function() {
                var $this = $( this );
                var src = $this.data( 'src' );
                if( $this.closest( $this.closest( '.img-wrap' ).length == 0 ) && src != '' ) {
                    $this.wrap( '<div class="img-wrap" style="background-image:url(' + src + ')"></div>' );
                }
            } );
        }

        function sunsite_action_banners(data) {
            var $place, w = $(window).width();
            var is_mobile = ( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) );
            banners_data = data;

            if( data.length == 0 ) {
                return false;
            }

            for( var i in data ) {
                $place = false;
                $( 'div[data-place="' + i + '"]' ).empty().each(function(){
                    var $this = $(this);
                    var s = $this.data('screen') || '';

                    if (s == '') {
                        $place = $this;
                    } else {
                        s = s.split(',');
                        if (w > parseInt(s[0]) && w <= parseInt(s[1])) {
                            $place = $this;
                        }
                    }
                });

                if ($place) {
                    for( var j in data[i] ) {
                        if (!(data[i][j].screen_width[0] < w && data[i][j].screen_width[1] >= w)) {
                            data[i].splice(j, 1);
                        }
                    }

                    if (data[i].length > 1) {
                        n =  Math.floor(Math.random()*data[i].length);
                        if (parseInt(data[i][n].type) == 14827) {
                            $place.append( '<div class="banner"><a href="' + data[i][n].url + '" target="_blank"><img src="https://vesti-ukr.com/pub/banners/' + data[i][n].file + '"></a></div>' );
                        } else {
                            $place.append( '<div class="banner">' + data[i][n].banner_code + '</div>' );
                        }
                    } else if (typeof data[i][0] != 'undefined') {
                        $place.append( '<div class="banner">' + data[i][0].banner_code + '</div>' );
                    }
                }

            }
        }

        var size_grid = [0,320,480,680,980,1200];
        var grid_point = 0;
        load_srcs(['/user/js/jquery-3.2.1.min.js'], function(){
            $(function(){
                $(window)
                    .resize(fix_sidebar_position)
                    .on(is_touch ? 'touchmove' : 'wheel', fixed_header);
                grid_point = get_grid_point();
                $(window).resize(function() {
                    grid_point = get_grid_point(true);
                });
                $('body:eq(0)').on('sizemode.change', function(event){
                    if (typeof banners_data == 'object') {
                        sunsite_action_banners(banners_data);
                        // console.log('w:' + grid_point);
                    }
                });
                sunsite_inits['img_wrap'] = function() {show_imgs($('img[data-src]'));}
                show_imgs($('img[data-src]'));

                // $.getScript('https://cdn.gravitec.net/sites/vesti/client.js');
            });

            sunsite_inits['add_this'] = addthis_init;
            addthis_init();
        });

        /* Response trigger */
        function get_grid_point(trigger) {
            var trigger = trigger || false;
            var w = $(window).width();
            var new_point = 0;
            for(var i in size_grid) {
                if (w > size_grid[i]) {
                    new_point = size_grid[i];
                }
            }

            if (grid_point != new_point && trigger) {
                grid_point = new_point;
                $('body:eq(0)').trigger('sizemode.change');
            }

            return new_point;
        }

        function fix_sidebar_position () {
            if ($('#js-main-slider').length > 0 && $('#js-sidebar').length > 0) {
                $('#js-sidebar').css('margin-top', 0);
                // $('#js-sidebar').css('margin-top', '-' + (Math.abs($('#js-main-slider').offset().top-$('#js-sidebar').offset().top) +
                // ($(window).width() >= 1050 && $('#js-sidebar').find('.currency-weather').length > 0 ? $('#js-sidebar').find('.currency-weather').outerHeight() : 32)) + 'px');
                $('#js-sidebar').css('margin-top', '-' + (Math.abs($('#js-main-slider').offset().top-$('#js-sidebar').offset().top)) + 'px');
            }
        }

        function fixed_header() {
            if ($(document).scrollTop() >= ($(window).width() > 980 ? 50 : 0)) {
                if (!$('#header').hasClass('fixed')) {
                    $('#header').addClass('fixed');
                }
            } else {
                if($('#header').hasClass('fixed')) {
                    $('#header').removeClass('fixed');
                }
            }
        }
