<div id="heatmap" ng-app="myApp" ng-controller="heatmap" ng-init="orderBy = 'value'; reverse = 'desc'">
	<div class="card">
		<iframe src="/plugins/ticker" width="100%" height="90" frameborder="none" style="border-bottom: 1px solid #d9d9d9;"></iframe>
		<div class="card-body">

			<div class="form-inline pull-right">

				<select class="form-control" ng-model="orderBy">
					<option value="symbol">Stock Symbol</option>
					<option value="last">Last Price</option>
					<option value="change">Percentage Change</option>
					<option value="value">Traded Value</option>
					<option value="trades">Number of Trades</option>
				</select>

				<select class="form-control" ng-model="reverse">
					<option value="asc">Ascending</option>
					<option value="desc">Descending</option>
				</select>

			</div>

			<h4 class="card-title">Stock Heatmap</h4>
			<h5 class="card-subtitle">Monitor all stock price movement in realtime</h4>

			<div class="clearfix"></div>

			<hr/>

			<div ng-if=" ! stocks">Loading...</div>

			<div ng-if="stocks" style="border: 1px solid #000;">
				<div class="card-group" style="margin-bottom: 0;" ng-repeat="stock in stocks" ng-init="outerIndex = $index" ng-if="$index % 15 == 0">
					<a href="/chart/{{stock.symbol}}" target="_blank" class="card card-stock" ng-repeat="stock in stocks | orderBy : orderBy : reverse == 'desc' | limitTo : 15 : outerIndex" ng-class="{'bg-danger': stock.change < 0,'bg-success': stock.change > 0,'bg-secondary': stock.change == 0}">
						<div class="card-body" style="font-size: 12px; padding: 5px;">
							<h5 class="card-title text-white font-weight-bold" title="{{stock.name}}" style="font-size: 14px; margin-bottom: 5px;">{{stock.symbol}}</h5>
							<h6 class="card-subtitle text-white font-weight-bold" title="Last Price" style="font-size: 12px; margin-bottom: 5px;">{{stock.last | price}}</h6>
							<h6 class="card-subtitle mb-0 text-white font-weight-bold" title="Percentage Change" style="font-size: 12px;">{{stock.change | numeraljs:'0,0.00'}}%</h6>
							<small>
								<span class="text-white text-uppercase pull-left" title="Traded Value">{{stock.value | value}}</span>
								<span class="text-white text-uppercase pull-right" title="Number of Trades">{{stock.trades | numeraljs:'0,0'}}</span>
							</small>
						</div>
					</a>
				</div>
			</div>

			<!-- <p class="card-text pull-right"><small class="text-muted">Last updated 3 mins ago</small></p> -->

		</div>
	</div>
</div>                
