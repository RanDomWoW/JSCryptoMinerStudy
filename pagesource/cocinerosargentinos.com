                    //$(wrapper).append('<li class="ui-state-default"><span class="ordenar"><i class="fa fa-sort"></i></span><span class="eliminar remove_field"><i class="fa fa-trash"></i></span><input type="text" placeholder="100grs de Azucar" class="form-control input-xl vacios ingredientes" onkeypress="keypressingredientes(event, this)" name="mytext[]" style="width: 100%;" /></li>');
