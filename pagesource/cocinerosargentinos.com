                $('#modalLogin').on('shown.bs.modal', function () {
                    $("#login_email").focus();
                });
                return;
            } else {
            	$("#modalRegistro").modal();
            	
            	$('#modalRegistro').on('shown.bs.modal', function () {
        			$("#registro-nombre").focus();
        		});
            }
        }
        
        function miPerfil() {
            $("#modalMiPerfil").modal();
            	
            $('#modalMiPerfil').on('shown.bs.modal', function () {
