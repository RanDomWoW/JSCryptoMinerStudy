            function removerIng(objeto) {
                var numItems = $('.ingredientes').length
                if(numItems>1){
                    $(objeto).parent('li').remove();
                }else{
                    alert("Debe haber al menos un ingrediente");
                }  
            }
            
            function removerPaso(objeto) {
                var numItems = $('.paso').length
                if(numItems>1){
                    $(objeto).parent('li').remove();
                }else{
                    alert("Debe haber al menos un paso");
                }  
            }
            
            
            function keypresspasos(e, objeto){
                if(e.which == 13) {
                        
                    if(e.shiftKey){
                        auto_grow(_this);
                    }else{
                        var wrapper = $("#pasos"); //Fields wrapper
                        //$(wrapper).append('<li class="ui-state-default"><span class="ordenar"><i class="fa fa-sort"></i></span><span class="eliminar remove_field"><i class="fa fa-trash"></i></span><input type="text" placeholder="100grs de Azucar" class="form-control input-xl vacios ingredientes" onkeypress="keypressingredientes(event, this)" name="mytext[]" style="width: 100%;" /></li>');
