                        $( "#pasos" ).sortable();
                        _this = $(objeto).parent("li").next().find("textarea");
                        _this.focus();
                        _this.val();
                        //auto_grow(_this);
                        var texto = $(objeto).val();
                        var cantidad = texto.length-1;
                        nuevotexto = texto.substring(0,cantidad);
                        $(objeto).val(nuevotexto);
                        $(wrapper).on("click",".remove_field", function(e){ //user click on remove text
                            e.preventDefault(); $(this).parent('li').remove();
                        });
                        auto_grow(_this);
                    }
                }else{
                    auto_grow(objeto);
                }
                
            }
            
            function auto_grow(element) {
                $(element).css({height: "5px"});
                $(element).css({height: $(element).prop('scrollHeight')+"px"});
                /*
                element.style.height = "5px";
                element.style.height = (element.scrollHeight)+"px";*/
            }
