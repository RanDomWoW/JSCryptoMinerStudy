  <script>
    $(document).ready(function () {
      //accordion script
      $(".accordion-header").on("click", function(e) {
        if(!$(this).hasClass('active')){
          $('.accordion').find('.accordion-header').removeClass('active').next('.accordion-body').slideUp();
        }

        $(this).toggleClass("active").next('.accordion-body').slideToggle();
      });

      //dropdown script
      $(".js-sub-dropdown").on("click", function(){
        $(".mobile-nav__dropdown").toggleClass("show-submenu");
      });

      $('.js-mobile-nav-toggle, .closebtn').on('click',function(){
        $('#mySidenav').toggleClass('open');
      });

      //Readmore for product page
      $('.read-more').on('click', function(e){
        e.preventDefault();

        var $hiddenDiv = $(this).prev('.product-features');
        $hiddenDiv.toggleClass('hidden');
        if(!$hiddenDiv.hasClass('hidden')){
          $(this).text('Read less -');
        }else{
          $(this).text('Read more +');
        }
      });

      //open external links in new tabs
      jQuery('a[href^="http"]').not('a[href^="'+$(location).attr('hostname')+'"]').attr('target', '_blank');

      //collection mobile filter
      var $categoryHead = $('.filter-block .color-head');
      if($(window).width() <= 749){
        $categoryHead.on('click',function(e){
          e.preventDefault();
          var $ul = $(this).next('ul');
          var $icon = $(this).find('i.fa');
          if(!$ul.is(':visible')){
              $icon.removeClass('fa-caret-down').addClass('fa-caret-up');
          }else{
            $icon.removeClass('fa-caret-up').addClass('fa-caret-down');
          }
          $ul.slideToggle(300);
        });
      }

      //quote slider
      $('.quote-slider').slick({
        autoplay: true,
        autoplaySpeed: 3000,
        arrows: false,
        dots: false,
        centerMode: true,
        slidesToShow: 1,
        fade: true
      });

      //safety slider
      $('.safety-slider').slick({
        autoplay: true,
        autoplaySpeed: 3000,
        arrows: true,
        dots: false,
        centerMode: true,
        slidesToShow: 1,
        slidesToScroll: 1,
        fade: true,
        infinite: true,
        draggable: true,
        accessibility: true,
        swipeToSlide: true,
        mobileFirst: true
      });

      //faq sidebar script
      $('.faq-sidebar a.help-links').on('click',function(e){
        e.preventDefault();

        var targetId = '#'+$(this).prop('href').split('#')[1];

        $('.faqs .accordion-group').removeClass('is-active');
        $(targetId).addClass('is-active');

        $('.faq-sidebar a.help-links').removeClass('is-active');
        $(this).addClass('is-active');
      });

      //thumbnail slider
      var $thumbDiv = $('.thumbnails-wrapper');

      $('button.slider-nav').on('click',function(e){
          e.preventDefault();

          var $activeItem = $thumbDiv.find('a.active-thumb').parent('li');
          var overideThumbFunctions = $thumbDiv.find(".cycle-variants").length;

          var useThumbs = $activeItem.length;
          var $newItem = null;

          if($(this).hasClass('slider-left')){

              if(useThumbs && overideThumbFunctions==0){
                $newItem= $activeItem.prev('li');
                if($newItem.length==0){
                  $newItem = $thumbDiv.find('ul li:last-child');
                }
              }
              else{
                productDetails.previous();
              }

          }else{
            if(useThumbs && overideThumbFunctions==0){
              $newItem = $activeItem.next('li');

              console.log($newItem.length);
              if($newItem.length==0){
                $newItem = $thumbDiv.find('ul li:first-child');
              }
            }
            else{
              productDetails.next();
            }
          }

          if($newItem!=null){
            $thumbDiv.find('a').removeClass('active-thumb');
            $newItem = $newItem.find('a');
            $newItem.addClass('active-thumb');
            $('.product-single img.product-featured-img').prop('src',$newItem.prop('href'));
            $('.product-single img.zoomImg').prop('src',$newItem.data('zoom'));
          }

      });


    });
  </script>
