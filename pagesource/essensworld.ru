    $.fn.followTo = function () {
        var that = this;
        var top = 210;
        var offsetBot = 360;

        $(window).scroll(function (e) {
            var height = that.innerHeight();
            var maxTop = $(document).innerHeight() - offsetBot - height;

            if ($(window).scrollTop() > maxTop - top) {
                that.css({
                    position: 'absolute',
                    top: maxTop
                });
            } else {
                that.css({
                    position: 'fixed',
                    top: top
                });
            }
        });
    };

    $('.banner_right,.banner_left').followTo();

