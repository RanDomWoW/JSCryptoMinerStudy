<script type="text/javascript">
			var widthSlider=$('#main-slider').width();
			var countSlider=$('#main-slider .slider-content li').length
			var sliderPos=0;
			var startPost=0;
			var sliderItemsWidth=0;
			var timerSlider;
			
			if(countSlider*355>widthSlider)
			{
				var items=$('#main-slider .slider-content li');
				var ilosc=items.length;
				
				if($('#main-slider-container').width()<=320) { plus=20;  }
				else if($('#main-slider-container').width()<=760) { plus=25;  }
				else if($('#main-slider-container').width()<=850) { plus=88;  }
				else if($('#main-slider-container').width()<=1200) { plus=44;  }
				else { plus=60; }
				
				for(a=0;a<items.length;a++) { sliderItemsWidth+=$(items[a]).width()+plus; }
				
				
				for(a=0;a<Math.min(4, ilosc);a++) { $('#main-slider .slider-content li').eq(a).clone().addClass('cloned').appendTo($('#main-slider .slider-content ul')); }
				for(a=0;a<Math.min(4, ilosc);a++) { sliderPos+=$('#main-slider .slider-content li').eq(a).width()+plus; $('#main-slider .slider-content li').eq($('#main-slider .slider-content li').length-1-a).clone().addClass('cloned').prependTo($('#main-slider .slider-content ul')); }
				$('#main-slider .slider-content ul').css({ left: -sliderPos+'px' });
				startPost=sliderPos;
				timerSlider=setTimeout('scrollSliderStep(1)', 10000);
			}
			else
			{
				$('#main-slider .prev-next').hide();
			}
			
			$(document).ready(function(){
				$('#main-slider .prev-next .prev a').click(function(){
					scrollSliderStep(-1);
					return false;
				});
				$('#main-slider .prev-next .next a').click(function(){
					scrollSliderStep(1);
					return false;
				});
				$('#main-slider').hammer().on("swipeleft", function(ev) {
					scrollSliderStep(1);
					ev.stopPropagation();

				});
				$('#main-slider').hammer().on("swiperight", function(ev) {
					scrollSliderStep(-1);
					ev.stopPropagation();
				});
			
				$('#main-slider').hover(function(){
					try { clearTimeout(timerSlider); } catch (e) {}
				}, function(){
					timerSlider=setTimeout('scrollSliderStep(1)', 10000);
				})
			});
			
			function scrollSliderStep(direction)
			{
				try { clearTimeout(timerSlider); } catch (e) {}
				timerSlider=setTimeout('scrollSliderStep(1)', 10000);
				
				if($('#main-slider .slider-content ul:animated').length==0)
				{
					if($('#main-slider-container').width()<=320) { change=direction*(304+20);  }
					else if($('#main-slider-container').width()<=760) { change=direction*(304+25);  }
					else if($('#main-slider-container').width()<=850) { change=direction*(608+88);  }
					else if($('#main-slider-container').width()<=1200) { change=direction*(1000+44);  }
					else { change=direction*($('#main-slider-container').width()+60); }
					
					if(sliderPos+change>sliderItemsWidth+startPost)
					{
						sliderPos-=sliderItemsWidth;
						$('#main-slider .slider-content ul').css({ left: -sliderPos+'px' });
					}
					else if(sliderPos+change<0)
					{
						sliderPos+=sliderItemsWidth;
						$('#main-slider .slider-content ul').css({ left: -sliderPos+'px' });
					}
					sliderPos+=change;
					
					$('#main-slider .slider-content ul').animate({ left: -sliderPos+'px' }, 1000);
					
					try { clearTimeout(timerSlider); } catch (e) {}
					timerSlider=setTimeout('scrollSliderStep(1)', 10000);
				}
			}

</script>
