<script>

      jQuery(document).ready(function($) {
      var ajaxurl = '/wp-admin/admin-ajax.php';

        function stopForm(e) {
          e.preventDefault();
          return false;

        }

        function errorForm(form, originalButtonText, errorMsg) {
          var errorMsg = errorMsg || 'Error. Please try again.';
          var userInput = $(form).find('input[name=email]').val();
          $(form).find('.btn').html(originalButtonText);
          $(form).find('input[name=email]').val(errorMsg).parent('.input-groups').addClass('signup-error');
          $(form).find('input[name=email]').bind('focus blur', function(e) {
            $(this).val(userInput);
            $(this).unbind('focus blur');
            $(form).find('.btn').removeClass('disabled').removeAttr('disabled');

          });
        }

        $("#signupformtop").submit(function(e) {
          e.preventDefault();
          var form = this;

          if ($(form).find('input[name=email]').val()) {
            var $el = $(form).parent('');
            /*$(form).find('.signup-error').removeClass('signup-error');*/
            var originalButtonText = $(form).find('.btn').html();
            $(form).find('.btn').html('&nbsp; <i class="fa fa-refresh fa-spin" style="margin-left:0px;"></i>');
            $(form).find('.btn').addClass('disabled').attr('disabled', 'disabled');
            $(form).bind('submit', stopForm);
            var email = $(form).find('input[name=email]').val();
            // _etmc.push(["setOrgId", "6153121"]);
            // _etmc.push(["setUserInfo", {"email" : email }]);
            // _etmc.push(["trackPageView"]);
            // 
            $.ajax({
              method: 'POST',
              url: 'http://52.5.80.158/email-signup',
              data: {
                email: email,
                list: 872
              },
              success: function(data) {

            $.cookie('1sale_newsletter_signedup', 'true', { expires: 365, path: '/' });
                      $.cookie('1sale_eparam', btoa(encodeURIComponent(email)), { expires: 365, path: '/' });

                if (data.code === 200) {

                  if (data.status === true) {

                    $(form).before('<h4 class="" style="font-size:14px;">Signup Success! We\'ll deliver deals straight to your inbox.</h4>');
                    $('.copytext').remove();
                    $(form).remove();
                    $(form).find('.input-group-btns').remove();
                
      	          _etmc.push(["setUserInfo", {"email": email }]);


                    ga('send', 'event', 'Header Email Signup', data.email, {
                      'nonInteraction': 1
                    });
                    setTimeout(function(){
                      closeIframe();
                    },2500);
                    
                    
                  } else {
                    errorForm(form, originalButtonText);
                  }
                } else if (data.code === 202) {
                  // console.log(data.info.results[0].ErrorCode);
                  if (data.info.results[0].ErrorCode == '12014') {
                    errorForm(form, originalButtonText, "Already Subscribed.");
                    setCookieAndHideForm(email);
                  } else {
                    errorForm(form, originalButtonText);
                  }
                }
                $(form).unbind('submit', stopForm);
                $(form).find('.btn').removeClass('disabled').removeAttr('disabled');
              }


            });
          }


        });
        
        $('#closeform').click(function(e){
          e.preventDefault();
          setCookieAndHideForm();
        })

      });
      function setCookieAndHideForm(email){
        if(email){
  
          jQuery.cookie('1sale_eparam', btoa(email), {
            expires: 365, path:'/'
          });
          _etmc.push(["setUserInfo", {"email": email }]);

        }
        jQuery.cookie('1sale_newsletter_signedup', 'true', {
          expires: 365, path:'/'
        });
        closeIframe();
      }
      function closeIframe(){
          $("#header-bar-under").animate({
            'margin-top': '-50px',
          }, 500, function(){
            $(this).remove();
          });
      }
