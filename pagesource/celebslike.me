<script type="text/javascript">
    (function () {
        var viewType = parseInt($("#initialView").val(), 10);
        var imgUrl = $("#initialImageUrl").val();
        var selectedRegionX = $("#initialSelectedRegionX").val();
        var selectedRegionY = $("#initialSelectedRegionY").val();
        var selectedRegionKey;
        if (selectedRegionX === "" || selectedRegionY === "") {
            selectedRegionKey = null;
        } else {
            selectedRegionKey = {
                x: parseFloat(selectedRegionX),
                y: parseFloat(selectedRegionY)
            };
        }

        var viewClasses = {};
        viewClasses[clm.routing.Views.LANDING] = "clm-view-landing";
        viewClasses[clm.routing.Views.SELECTION] = "clm-view-selection";
        viewClasses[clm.routing.Views.RESULT] = "clm-view-result";

        $(window).on("clm.viewchange", function(e, d) {
            var currentViewType = d.currentViewType;

            $.each(viewClasses, function(viewId, className) {
                $("body").removeClass(className);
            });

            $("body").addClass(viewClasses[currentViewType]);
        });

        if (viewType === clm.routing.Views.LANDING) {
            clm.routing.router.navigate(clm.routing.Views.LANDING, {}, false);
        } else if (viewType === clm.routing.Views.SELECTION) {
            clm.routing.router.navigate(
                clm.routing.Views.SELECTION,
                {
                    externalUrl: imgUrl,
                    selectedRectangleKey: selectedRegionKey
                },
                false);
        }

        var opts = {
            lines: 11 // The number of lines to draw
            , length: 35 // The length of each line
            , width: 10 // The line thickness
            , radius: 30 // The radius of the inner circle
            , scale: 0.25 // Scales overall size of the spinner
            , corners: 1 // Corner roundness (0..1)
            , color: '#fff' // #rgb or #rrggbb or array of colors
            , opacity: 0.1 // Opacity of the lines
            , rotate: 0 // The rotation offset
            , direction: 1 // 1: clockwise, -1: counterclockwise
            , speed: 1.6 // Rounds per second
            , trail: 54 // Afterglow percentage
            , fps: 20 // Frames per second when using setTimeout() as a fallback for CSS
            , zIndex: 2e9 // The z-index (defaults to 2000000000)
            , className: 'spinner' // The CSS class to assign to the spinner
            , top: '-100%' // Top position relative to parent
            , left: '50%' // Left position relative to parent
            , shadow: false // Whether to render a shadow
            , hwaccel: false // Whether to use hardware acceleration
            , position: 'absolute' // Element positioning
        };
        // let it spin forever
        var analyzingSpinner = new Spinner(opts).spin(document.querySelector("#loadingBlock > p"));
        var imageLoadingSpinner = new Spinner(opts).spin(document.querySelector("#imageLoadingBlock > p"));
        var uploadingSpinner = new Spinner(opts).spin(document.querySelector("#uploadingBlock > p"));

    }());
</script>







