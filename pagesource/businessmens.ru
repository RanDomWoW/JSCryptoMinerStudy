<script type="text/javascript">jQuery(function ($) {
$(document).ready(function() {
    
    var params = {"json":1,"position":"bottom","url":"/","double":0,"exclude":[]};
    
    if ($('#bnr_w0').is(':visible') == false) {
        return false;
    }
    
    var loadFunc = function() {

        $.get('http://old.businessmens.ru/bnr/load', params)
        .done(function(response) {
            
            if (response.banners) {
                
                var content = '';
                
                for (var key in response.banners) {
                
                    var banner = response.banners[key];
                    
                    if (content) {
                        content += '<div style="height: 30px; width:100%;"></div>';
                    } 
                    
                    if (banner.type == 'image') {
                        
                        content += (response.pixel ? '<img style="width: 1px; height: 1px" src="' + banner.pixel + '" /></a>' : '') +
                             '<a target="_blank" href="http://old.businessmens.ru' + banner.link + '">' +
                             '<img style="max-width: 100%" src="' + banner.image + '" /></a>'
                         ;
                         
                    } else if (banner.type == 'html5') {
                        
                        content += '<iframe src="' + banner.src + '"' +
                         'frameborder="0" border="0" scrolling="no" ' +
                         'allowtransparency="true" width="100%" height="180" />';
                        
                    } else if (banner.type == 'content') {
                        content += banner.content;
                    }
                }
                
                if (content) {
                    $('#bnr_w0').html(content);    
                }
                
                if (response.sticky && screenSize() == 'wide') {
                    $('#bnr_w0').sticky({bottomSpacing:447});
                }
                
                if (0) {
                    params.exclude.push(banner.uid);
                    
                    setTimeout(loadFunc, 0 * 1000);    
                }
                
            } else {
                
                if (params.exclude.length == 0) {
                    $('#bnr_w0').remove();    
                }
            }
        })
        .fail(function() {
            
            if (params.exclude.length == 0) {
                $('#bnr_w0').remove();    
            }
            
        });
        
    }; 
    
    setTimeout(loadFunc, 1000);

});
