<script>

    function EndRequestHandler(sender, args) {



        RemoveNotify();
        $('.MyGridViewGenerate').DataTable({
            dom: 'Bfrtip',
            stateSave: true,
            "lengthMenu": [[40, 50, 60, -1], [40, 50, 60, "All"]],
            "iDisplayLength": 50,
            buttons: [{
                extend: 'excelHtml5',
                title: 'Excel Export',
                text: 'خروجی اکسل',
                exportOptions: {
                    format: {
                        body: function (data, column, row) {
                            //if it is html, return the text of the html instead of html
                            if (/<\/?[^>]*>/.test(data)) {
                                return $(data).text();
                            } else {
                                return data;
                            }
                        }
                    }
                },
                customize: function (xlsx) {
                    if ($("#hfdatetime").length) {
                        var sheet = xlsx.xl.worksheets['sheet1.xml'];
                        var numrows = 5;
                        var clR = $('row', sheet);

                        //update Row
                        clR.each(function () {
                            var attr = $(this).attr('r');
                            var ind = parseInt(attr);
                            ind = ind + numrows;
                            $(this).attr("r", ind);
                        });

                        // Create row before data
                        $('row c ', sheet).each(function () {
                            var attr = $(this).attr('r');
                            var pre = attr.substring(0, 1);
                            var ind = parseInt(attr.substring(1, attr.length));
                            ind = ind + numrows;
                            $(this).attr("r", pre + ind);
                        });

                        function Addrow(index, data) {
                            var msg = '<row r="' + index + '">'
                            for (var i = 0; i < data.length; i++) {
                                var key = data[i].key;
                                var value = data[i].value;
                                msg += '<c t="inlineStr" r="' + key + index + '">';
                                msg += '<is>';
                                msg += '<t>' + value + '</t>';
                                msg += '</is>';
                                msg += '</c>';
                            }
                            msg += '</row>';
                            return msg;
                        }


                        //insert
                        var r1 = Addrow(1, [{ key: 'A', value: $("#hfdatetime").val() }]);

                        sheet.childNodes[0].childNodes[1].innerHTML = r1 + sheet.childNodes[0].childNodes[1].innerHTML;


                        $('row c[r^="A1"]', sheet).attr('s', '48');
                        $('row c[r^="A2"]', sheet).attr('s', '48');

                        $('row c[r^="A3"]', sheet).attr('s', '48');

                    }
                }
            },
                {
                    extend: 'print',
                    autoPrint: false,
                    footer: true,
                    customize: function (win) {
                        //$(win.document.body)
                        //    .css('font-size', '10pt')
                        //    .prepend(
                        //        '<img src="http://datatables.net/media/images/logo-fade.png" style="position:absolute; top:0; left:0;" />'
                        //    );

                        $(win.document.body).addClass('PrintBody');


                        $(win.document.body).find('table')
                            .addClass('PrintCompact')
                            .css('font-size', 'inherit');



                        if ($("#hfdatetime").length) {
                            $(win.document.body).prepend("<div class=' '> " + $("#hfdatetime").val() + "</div>");
                        }
                    }
                }
            ],
            "aaSorting": [],
            //stateSave: true,
            language: {
                processing: "لطفا کمی صبر کنید",
                search: "جستجو :&nbsp;",
                lengthMenu: "",
                info: "",

                infoPostFix: "", "bPaginate": true,
                emptyTable: "هیچ اطلاعاتی وجود ندارد",
                "sPaginationType": "full_numbers",
                "sDom": '<"top"flp>rt<"bottom"i><"clear">',
                paginate: {
                    first: "ایتدا",
                    previous: "صفحه قبلی",
                    next: "بعدی",
                    last: "آخر"
                },
                buttons: {
                    excel: 'خروجی اکسل',
                    print: 'چاپ'
                }

            },
            fixedHeader: {
                header: true,
                footer: true
            }
        });



    }
    $(document).ready(function () {


        try {
            var prm = Sys.WebForms.PageRequestManager.getInstance();

            prm.add_endRequest(function () {
                EndRequestHandler();
            });
        }
        catch (e) { };



        EndRequestHandler();
        $('.dataTables_filter input').addClass('form-control dtfsize');

    })
</script>


