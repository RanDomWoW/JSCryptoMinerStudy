<script>



    (function () {

        // Hide Header on on scroll down
        var didScroll;
        var lastScrollTop = 0;
        var delta = 5;
        var navbarHeight = $('.rd-navbar-panel-canvas').outerHeight();


        function hasScrolled() {
            var st = $(this).scrollTop();

            // Make sure they scroll more than delta
            if (Math.abs(lastScrollTop - st) <= delta)
                return;


            if (st > lastScrollTop && st > navbarHeight) {
                // Scroll Down
                $('.rd-navbar-wrap .rd-navbar-panel-canvas').removeClass('nav-down').addClass('nav-up');
                $('.rd-navbar-wrap .rd-navbar-panel').removeClass('nav-down_parent').addClass('nav-up_parent');

            } else {
                // Scroll Up
                if (st + $(window).height() < $(document).height()) {
                    $('.rd-navbar-wrap .rd-navbar-panel-canvas').removeClass('nav-up').addClass('nav-down');
                    $('.rd-navbar-wrap .rd-navbar-panel').removeClass('nav-up_parent').addClass('nav-down_parent');
                }
            }

            lastScrollTop = st;
        }

        $(window).scroll(function () {
            didScroll = true;

            hasScrolled();
        });

    })();







