	jQuery(document).ready(function(){	
		jQuery("#weatherList > div:gt(0)").hide();
		
		jQuery.get("/renew/common/weather.jsp", function(xmlData){ 
			parseWeatherXML(xmlData);
			
		},"xml");
		var weatherList = new Array();
		var filterLocal = ['143', '138', '136', '130', '283', '279', '272', '281', '137', '273', '278', '276', '277', '271', '115'];
		function existFilterLocal(id) {
			for(var i = 0; i < filterLocal.length; i++)
			{
				if (filterLocal[i] == id) return i;
			}
			return -1;
		}
		function parseWeatherXML(xmlData) {
			jQuery(xmlData).find("current > weather > local").each(function(){
				weather = {};
				var icon = jQuery(this).attr("icon");
				var id = jQuery(this).attr("stn_id");
				if (icon == '') return;		
				weather.ta = jQuery(this).attr("ta");
				weather.desc = jQuery(this).attr("desc");
				weather.icon = icon;
				weather.stn_id = id;
				weather.local = jQuery(this).text();
				if (existFilterLocal(id) >= 0) {
					weatherList[id] = weather;
				}									
			});	
			
			printWeather();
			weatherSlide();
		}
		
		function printWeather() {
			var d = new Date();
			var week = new Array('일', '월', '화', '수', '목', '금', '토');
			var today = d.getFullYear()+'.'+  (d.getMonth() + 1) + '.' + d.getDate() +' ('+week[d.getDay()] + ')';
			
			var html = "";
			jQuery.each(filterLocal, function(i, local){
				if (weatherList.hasOwnProperty(local) == false) return; 
				var weather = weatherList[local];
				html = html + '<div class="weather" style="display:'+((i==0)?'block':'none')+'">'
						+ '<p class="weather_date">' + today + '<br/>' + weather.local + ' ' + weather.ta + '˚C</p>'
						+ '<p class="weather_img"><img src="/images/top2015/weather/NB' + weather.icon + '.jpg" alt="" style="width:38px"/></p>'
						+ '</div>';
			});	
			jQuery("#weatherList").html(html);
		}
		
		
		function weatherSlide() {
			setInterval(function() {
				jQuery('#weatherList > div:first')
					.hide()
			    	.next()
				    .show()
				    .end()
				    .appendTo('#weatherList');
			}, 5000);
		}
	});
	
