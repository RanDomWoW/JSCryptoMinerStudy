	var dos_cover_url, dos_content, dos_excerpt;

	if (thisPost['regular-body']){
		dos_content = $('<div></div>').append(thisPost['regular-body']).find('p').first().html();
		dos_cover_url = $('<div></div>').append(thisPost['regular-body']).find('img').first().attr('src');
	}else if(thisPost['photo-url-500']){
		dos_cover_url =  thisPost['photo-url-500'];
		dos_content = $('<div></div>').append(thisPost['photo-caption']).find('p').first().html();
	}
	if (dos_content){
		dos_excerpt = dos_content.substring(0,50)+'...';
	}else{
		dos_excerpt = '科學歷史研究員外出考察中。:)';
	}
	$('#dos_title').html(dos_excerpt);

	if (!dos_cover_url){
		dos_cover_url = noimg;
		$('#dos_cover').html('<img src="'+noimg+'" style="width:100%; padding:50px 20px;">');
	}else{
		$('#dos_cover').html('<img src="'+dos_cover_url+'" style="width:100%">');
	}

	// Pansci Tile
	$('.ps_block')
	.hover(
	    function(){
	        $(this).addClass('hover');
	    },
	    function(){
	        $(this).removeClass('hover');
	    }
	);

	$('.know_bean')
	.hover(
	    function(){
	        $(this).addClass('hover');
	    },
	    function(){
	        $(this).removeClass('hover');
	    }
	);

})
</script>
