<script charset="utf-8" type="text/javascript">
var flg ='0';
if($('.d-gender-btn--men').hasClass('is-current')){
	var gender = 'MEN';
}else{
	var gender = 'WOMEN';
}

RR.jsonCallback = function(){

if( flg == 0 || flg == 1 ){
	// START ENGAGE CODE
	// Select the correct content slider
	var content_div = $('.d-gender-btn--men').hasClass('is-current') ? $('.d-slider.content--men')[0] : $('.d-slider.content--women')[0];

	// Confirm that there is a response before proceeding
	if (RR.data.JSON.content) {
		// Get the RR object
		var placementObject = RR.data.JSON.content;
		// Create an array to hold the banner lis
		var bannerArray = [];
		// For each item in the response, create an li element

		var cnt = 0;

		for (key in RR.data.JSON.content) {
			cnt++;
			bannerArray.push('<li><a href="' + placementObject[key].clickthrough_URL + '"data-gtm="TOP BANNER '+ gender + ':'+ cnt +'"><img src="' + placementObject[key].asset_image_URL + '" width="982" height="400"></a></li>');


		}
		// Get the slider and switch out its contents

		slider_ul = $(content_div).find(".d-slider-hero")[0];
		slider_ul.innerHTML = bannerArray.join("\n");


	}

	$(content_div).find(".d-slider-hero").bxSlider({ // 自動再生
	      auto: true,

	      pager: true,
	      mode: 'fade',
	      responsive: false,
	      speed: 2000,
	      controls: false,
	      pager: true,

	  });
}
flg++ ;
	// END ENGAGE CODE



if($('.d-gender-btn--men').hasClass('is-current')){
var section = $('#recommend_items_mens');
}else{
var section = $('#recommend_items_womens');
}
var ul = $(section).find('ul.c-itemList');
$(ul).html('');
if(!RR.data.JSON.placements[0]){
$(section).hide();
return;
}


var items = RR.data.JSON.placements[0].items;
var message = RR.data.JSON.placements[0].strategy_message;
if(message){
$(section).find('.strategy-message').text(message);
}

var cnt = 0;
var now = new Date();
var hide_start = new Date('2018/1/26 12:00:00 +0900');
var hide_end = new Date('2018/1/26 14:00:00 +0900');

if(items && items.length > 0 && (now.getTime() < hide_start.getTime() || now.getTime() >= hide_end.getTime())){
$.each(items, function(i, e) {
cnt++ ;
var price = this.price.replace( /￥|,/g , "");

var sale_price = this.sale_price.replace( /￥|,/g , "");
var off_parcentage = null;
if(Number(sale_price) < Number(price)){
	off_parcentage = Math.floor((price-sale_price)/price*100);
}

if($('.d-gender-btn--men').hasClass('is-current')){
	var gender = 'MEN';
}else{
	var gender = 'WOMEN';
}

var html = '';
html += '<li class="item">';
html += '<div class="thumb"><a href="'+this.link_url+'" data-gtm="TOP RECOMMEND '+ gender + ':'+ cnt +'"><img src="'+this.img_url+'?resize=237%3A284&output-quality=80" data-original="'+this.img_url+'" alt="'+this.brand+'" class="d-slider-lazy"></a></div>';
html += '<div class="data">';
html += '<div class="name">'+this.brand+'</div>';
if(off_parcentage){
html += '<div class="price discount">'+this.sale_price+' '+off_parcentage+'%OFF</div>';
}else{
html += '<div class="price">'+this.price+'</div>';
}
html += '</div></li>';
$(ul).append(html);
});
}else{
$(section).hide();
}
};
</script>



