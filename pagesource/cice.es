                                                    dataLayer.push({'eAction':'clase_prueba_footer', 'eLabel': '', 'event':'conversion','modalidad':'PRESENCIAL','titulacion':''});
                                                    $('#form-prueba-clase-gratis-lightbox input[name="formulario"]').val('SOLICITA PRUEBA GRATUITA - BOTTOM');
                                                }


                                        });

                                    $('#form-prueba-clase-gratis-lightbox').submit(function() {
                                           if ($('#form-prueba-clase-gratis-lightbox').valid()) {
                                                    //alert ($('#form-prueba-clase-gratis-lightbox #boton').val());
                                                   if ($('#form-prueba-clase-gratis-lightbox #boton').val()=='boton_superior') dataLayer.push({ 'eAction':'clase_prueba_top_ok','eLabel':document.title,'event':'conversion','modalidad':'PRESENCIAL','titulacion':''});
                                                   if ($('#form-prueba-clase-gratis-lightbox #boton').val()=='boton_footer') dataLayer.push({ 'eAction':'clase_prueba_footer_ok','eLabel':document.title,'event':'conversion','modalidad':'PRESENCIAL','titulacion':''});
                                                   return true;
                                           } else {
                                                    //alert ($('#form-prueba-clase-gratis-lightbox #boton').val());
                                                   if ($('#form-prueba-clase-gratis-lightbox #boton').val()=='boton_superior') dataLayer.push({ 'eAction':'clase_prueba_top_error','eLabel':document.title,'event':'conversion','modalidad':'PRESENCIAL','titulacion':''});
                                                   if ($('#form-prueba-clase-gratis-lightbox #boton').val()=='boton_footer') dataLayer.push({ 'eAction':'clase_prueba_footer_error','eLabel':document.title,'event':'conversion','modalidad':'PRESENCIAL','titulacion':''});
