    var sliderArray = [];

    $(document).ready(function () {
        function createSliderBox() {
            return {
                i: 0,
                max: 0,
                width: 240,
                slideTime: 1000,
                autoSlideTime: 5000,
                init: function () {
                    var t = this;
                    t.max = $("#slideContainer26 li").length;
