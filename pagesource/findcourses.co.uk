                    $("#slideContainer26 li").css("display", "block");
                    $("#slideContainer26 ul").mouseenter(function () { t.stopAutoSlide(); });
                    $("#slideContainer26 ul").mouseleave(function () { t.stopAutoSlide(); t.autoSlide(); });
                },
                next: function () { this.stopAutoSlide(); this.i++; this._slide(); },
                prev: function () { this.stopAutoSlide(); this.i--; this._slide(); },
                _autoTimer: null,
                stopAutoSlide: function () { clearTimeout(this._autoTimer); },
                autoSlide: function () {
                    var t = this;
                    t._autoTimer = setInterval(function () {
                        t.i++;
                        t._slide();
                    }, t.autoSlideTime);
                },
                _slide: function () {

                    var t = this;
                    if (t.i > t.max - 1)
                        t.i = 0;
                    else if (t.i < 0)
                        t.i = t.max - 1;

                    $("#slideContainer26 ul").animate({ left: -(t.width * t.i) }, { duration: t.slideTime, queue: false });
                }
            };
        }

        var sliderBox = createSliderBox();

        sliderArray[26] = sliderBox;
        sliderArray[26].init();
        sliderArray[26].autoSlide();
    });

