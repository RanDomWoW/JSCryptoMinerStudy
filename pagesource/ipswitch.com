<div class="white-modal search" ng-controller="siteSearchController" id="scrollContainer" data-document-culture="en-US">
    <div class="close-container">
        <div class="close-icon close-modal">
            <svg>
                <use xlink:href="/library/img/common.svg#close-icon-circle"></use></svg>
        </div>
    </div>
    <div class="white-modal-inner">
        <div class="input-container">
            <input class="search-field" type="text" placeholder='Type to search...' ng-model="siteSearch.SearchText" ng-change="onSearchTextChanged()" on-enter="submitSearch()" onblur="ga('create', 'UA-18536655-2', 'auto'); ga('set', 'page', '/search-field-tracking'); ga('send', { hitType: 'pageview', page: '/search-field-tracking?searchText=' + this.value });" />
            <span ng-show="initialLoad">Search Ipswitch</span>
            <span ng-show="!initialLoad">{{results.length}}/{{totalResults}} {{totalResults === 1 ? 'Resource' : 'Resources'}}</span>
        </div>

        <div class="search-results" infinite-scroll="nextPage()" infinite-scroll-disabled="isBusy" infinite-scroll-container="#scrollContainer" infinite-scroll-immediate-check="false">
            <div class="result-item slide-top" ng-repeat="result in results">
                <a ng-href="{{result.url}}">
                    <div class="result-image" ng-style="{'background-image': 'url(' + result.image + ')'}">
                    </div>
                    <div class="result-info">
                        <div class="result-title">
                            {{result.title}}
                        </div>
                        <div class="result-summary" ng-bind-html="result.content">
                        </div>
                    </div>
                </a>
            </div>
        </div>
    </div>
</div>
