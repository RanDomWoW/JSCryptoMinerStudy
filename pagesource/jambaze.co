--------------------------------------------------------------*/
.themecountry-social-sharing {
	clear: both;
	width: 100%;
	text-align: left;
	margin: 15px 0 15px 0;
}

.themecountry-social-sharing ul {
	list-style: none;
	margin: 0;
	padding: 0;
}

.themecountry-social-sharing li {
	display: inline-block;
	margin: 0 10px 0 0;
}

.themecountry-social-sharing li:last-child {
	margin-right: 0;
}

.themecountry-social-sharing li a {
	-webkit-font-smoothing: antialiased;
	-webkit-transition: background-color .2s ease-in-out;
	-moz-transition: background-color .2s ease-in-out;
	-o-transition: background-color .2s ease-in-out;
	transition: background-color .2s ease-in-out;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	position: relative;
	overflow: hidden;
	font-size: 18px;
	width: 130px;
	height: 30px;
	text-align: left;
	text-decoration: none;
	line-height: 30px;
	display: block;
	color: #ffffff;
	padding: 0 10px;
	font-weight: bold;
	border-radius: 2px;
}

.themecountry-social-sharing li a:before {
	content: '';
	width: 0;
	height: 0;
	position: absolute;
	top: 0;
	left: 0;
	z-index: 1;
	-webkit-transition: all 0.5s ease;
	-moz-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	-ms-transition: all 0.5s ease;
	transition: all 0.5s ease;
}

.themecountry-social-sharing li a i {
	font-size: 18px;
	line-height: 30px;
	position: relative;
	z-index: 2;
}

.themecountry-social-sharing li a span {
	position: relative;
	z-index: 2;
	font-size: 14px;
	float: right;
}

.themecountry-social-sharing .facebook a {
