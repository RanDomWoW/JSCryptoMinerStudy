      var $pageLength = $sliderArea.find('.read-wrap').length;
      var li = "";

      //pagerの要素を作る
      if ($pageLength > 1) {
        $sliderArea.find('.pager-custom').css('display','block');
        for (var k = 0; k < $pageLength; k++) {
          var pageNo = k+1;
          li += '<li class="reason-pager"><a data-slide-index="' + k +'" href="">理由<br>その' + pageNo + '</a></li>';
        }
        $sliderArea.find('.bx-pager').append($('<ul />').append(li));
      }
        
        function setColor(index) {
               
            var $wrap = $("#reason-pager .bx-wrapper");
            $wrap.removeClass("blue");
            $wrap.removeClass("pink");
            $wrap.removeClass("green");
            $wrap.removeClass("purple");
            $wrap.removeClass("orange");
           
            switch(index) {
                case 0:
                    $wrap.addClass("blue");
                    break;
                case 1:
                    $wrap.addClass("pink");
                    break;
                case 2:
                    $wrap.addClass("green");
                    break;
                case 3:
                    $wrap.addClass("purple");
                    break;
                case 4:
                    $wrap.addClass("orange");
                    break;
            }

           
        }

        var s_index = -1;
      //bxSliderの設定
      $slider = $('#reason-slide').bxSlider({
        speed:1000,
        pagerCustom: '.bx-pager',
        touchEnabled: false,
        onSliderLoad: function(index){
            if( windowWidth<=windowSm ) setColor(0);
        },
        onSlideBefore: function(slide, oldIndex, newIndex){
            if( windowWidth<=windowSm ) setColor(newIndex);
            $sliderArea.addClass("movement");
        },
        onSlideAfter: function(slide, oldIndex, newIndex){
            $sliderArea.removeClass("movement");
            resolveClick();
        },
      });
      resolveClick = function() {
        if (s_index >-1) {
            $slider.goToSlide(s_index);
            s_index = -1;
        }
      };
      $(".reason-pager a").click(function(){
        if ($sliderArea.hasClass("movement")) {
            s_index = parseInt($(this).attr("data-slide-index"));
            if( windowWidth<=windowSm ) setColor(s_index);
        }
      });
    };
  });

})(jQuery);
</script>

