        function slideSwitch() {
            var $active = $('div.windowItem.active');

            if ($active.length == 0) $active = $('div.windowItem:last');

            // use this to pull the images in the order they appear in the markup
            var $next = $active.next().length ? $active.next()
                    : $('div.windowItem:first');

            $active.addClass('last-active');

            $next.css({ opacity: 0.0 })
                    .addClass('active')
                    .animate({ opacity: 1.0 }, 2000, function () {
                        $active.removeClass('active last-active');
                    });
        }

        $('document').ready(function () {
            // set timer for autoSlider
            //timer = setInterval(function () {
            //    autoSlide();
            //}, 7000);
            setInterval("slideSwitch()", 8000);
            // add click handler to slider controls
            $('div.controls span').live("click", function () {
                if ($(this).hasClass('active')) {
                    return false;
                }
                if (sliderFlag == false) {
                    sliderFlag = true;
                    clearInterval(timer);
                    showSlide($(this));
                }
            });
            setInterval("slideTwitter()", 7500);

        });

        function getDefaultValue(id) {
            if (id == 'tjtuul-tjtuul') { return 'Jouw emailadres'; }
        }

        $('#tjtuul-tjtuul').bind("blur", function () {
            var defaultValue = getDefaultValue($(this).attr('id'));

            var currentValueEntered = $(this).val();
            if (currentValueEntered == '') { $(this).val(defaultValue); currentValueEntered = defaultValue; }
        });
        $('#tjtuul-tjtuul').click(function () {

            var defaultValue = getDefaultValue($(this).attr('id'));

            // Remove default value
            if ($(this).val() == defaultValue) { $(this).val(''); }

        });

        function slideTwitter() {
            var $active = $('#twitterBox div.active');

            if ($active.length == 0) $active = $('#twitterBox div.twitterItem:last');

            // use this to pull the images in the order they appear in the markup
            var $next = $active.next().length ? $active.next()
                    : $('#twitterBox div.twitterItem:first');

            $active.addClass('last-active');

            $next.css({ opacity: 0.0 })
                    .addClass('active')
                    .animate({ opacity: 1.0 }, 2000, function () {
                        $active.removeClass('active last-active');
                    });
        }
        function validateEmail(email) {
            var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            return re.test(email);
        }

