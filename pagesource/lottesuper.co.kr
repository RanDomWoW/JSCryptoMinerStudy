<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="ko" lang="ko">
<head>
<meta http-equiv="Content-Type" content="text/html; CHARSET=utf-8" />
<title>롯데인터넷슈퍼</title>
<script type="text/javascript">

var tmp = window.location; 

function redirectUrl() {
	var param	= "";
	var _url	= "";
	var bool	= device_Checker();
	
	if (String(tmp).indexOf("?") > 0) {
		tmp = String(tmp).split("?");
		param = tmp[1];
	}
	if (param != "") {
		param = "?"+param;
	}
	
	//2014.10.13 sjkim77 프레시센터 전용 도메인 적용
	if (String(tmp).indexOf("www.lottefresh.com") != -1 || String(tmp).indexOf("www.lottefresh.co.kr") != -1) {
		if(param != "") {
			param = param + "&rdCd=10";
		} else {
			param = param + "?rdCd=10";
		}
	}
	
	_url = "http://www.lottesuper.co.kr/handler/Index-Start"+param;
	
	if(bool) { //단말기라면
		_url = "http://m.lottesuper.co.kr/handler/Index-Start"+param;
	}
	
	location.href = _url;
}

// device 체크 2014.11.11 SHOH5
function device_Checker() {
	var agent	= navigator.userAgent;
	var _device	= false;
	if (agent.match(/iPhone/) != null ||
		agent.match(/Android/) != null ||
		agent.match(/Windows Phone/) != null ||
		agent.match(/Opera Mobi/) != null ||
		agent.match(/BlackBerry/) != null ||
		agent.match(/SymbianOS/) != null ||
		agent.match(/Bada/) != null ||
		agent.match(/iPod/) != null ||
		agent.match(/iPad/) != null
	) {
		_device = true;
	}
	return _device;
}
</script>
</head>
<body onload="redirectUrl()">
</body>
</html>