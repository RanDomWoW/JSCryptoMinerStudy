    function mySlide(obj) {
        var list=obj.list,unit=obj.unit,left_arrow=obj.left_arrow,right_arrow=obj.right_arrow,unit_height=obj.unit_height,unit_width=obj.unit_width,view_count=obj.view_count,
                step_width=obj.step_width,isArrowHover=obj.isArrowHover,arrowHoverClass=obj.arrowHoverClass,speed_time=obj.speed_time;
        unit_height=unit_height||$(unit).height();
