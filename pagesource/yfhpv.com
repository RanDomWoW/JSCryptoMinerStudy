        /*  定义index页面常见问题自动滚动函数
         参数：
         selector:滚动box的id选择器
         time:执行周期时间
         */
        function auto_scroll(selector,time){
            var self=$(selector);
            function auto_play(){
                var height=self.prop("scrollHeight")-self.css("height").slice(0,-2)-self.css("paddingTop").slice(0,-2)-self.css("paddingBottom").slice(0,-2);
                var spead=time/height;
                var top=self.scrollTop();
                var cuur_time=(height-top)*spead;
                self.animate({scrollTop:height},cuur_time,"linear",function(){
                    self.animate({scrollTop:0},auto_play);
                });
            }
            auto_play();
            self.mouseenter(function(){
                self.stop();
            }).mouseleave(function(){
                auto_play();
            });
        }
        var ul_scroll_ready=true;
        var num_scroll_ready=true;
        $(document).scroll(function(){
            var myScrollTop=$(document).scrollTop();
            if(ul_scroll_ready){
                var ul_curr_top=$("#three_box_move").offset().top;
                if((ul_curr_top-myScrollTop)<300){
