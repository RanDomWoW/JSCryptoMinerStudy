	var insert_close_icon = function() {
		var cloned_a = $('<a href="#"></a>');

		cloned_a.attr('id', 'close-feedback');
		cloned_a.insertAfter($('#feedback h2'));
		cloned_a.text('');

		cloned_a.click(function() {
			$('#feedback').fadeOut(function() {
				$(this).html('');
				$('.show-feedback').show();
			});
			return false;
		});
	}

	$('#socialmedia').removeClass('hidden');

	$('body').off("click", ".show-feedback");
	$('body').on("click", ".show-feedback", function() {
		$(this).hide();

		if (! $("#feedback").length) {
			$("#socialmedia").after('<div id="feedback" style="display:none"></div>');
		}

		$("#feedback").hide();
		$("#feedback").html("");
		$.ajax({
			url: "/editorial-feedback.html",
			cache: false,
			headers: {leapajax : 'true'}
			
		}).done(function(content) {
			startIdx = content.search("<div class=\"html5-article\">");
			endIdx =  content.search("<!-- /.html5-article -->"); 
			content = content.substring(startIdx, endIdx);
			var container = $('<div></div>').appendTo(document.body).append(content);
			// all urls absolute
			var hrefContainer = ['src', 'href', 'action'];
			for (var i = 0; i < hrefContainer.length; i++) {
				var attr = hrefContainer[i];
				container.find('[' + attr + ']:not([' + attr + '^=#]):not([' + attr + '^=http]):not([' + attr + '^=mailto])').each(function() {
					if ($(this).attr(attr) != null && $(this).attr(attr).indexOf("/") != 0) {
						$(this).attr(attr, '/' + $(this).attr(attr));
					}
				});
			}

			var contentObj = $(content);
			var headline = contentObj.find('h1').html();
			var form = container.find('.feedback-form-ajax');

			$("#feedback").append('<h2>' + headline + '</h2>').append(form);
			container.remove();

			$('#feedback').hide().fadeIn(function() {
				var top = $("#feedback").offset().top;
				$('html, body').animate({
					scrollTop: top
				}, 500);
			});

		}).fail(function(data) {
			var feedback = $('#feedback');

			feedback.html('<h2>Bei Ihrer Anfrage ist ein Fehler aufgetreten!</h2>');

			insert_close_icon();

			feedback.hide().fadeIn(function() {
				var top = feedback.offset().top;
				$('html, body').animate({
					scrollTop : top
				}, 500);
			});
		});
		return false;
	});

	// handler to insert a close-icon into an erroneous feedback-request
