<script>
function community_live_block_content(type) {

	var html = '';
	var selected_type = '';

	if (type != 'comment' && type != 'topic') {
		return false;
	}
	
	selected_type = $('.sidebar-block .community-live-block:not(.sections) .menu .menu-item.selected').data('type');
	
	if (selected_type == type) {
		return false;
	}
	
	element_disable($('.sidebar-block .community-live-block:not(.sections)'));

	$.ajax({
		type     : 'post',
		url      : '/forum/ajax/stream/' + type + '/',
		dataType : 'json',
		success  : function(data, ans) {
			html = data.sText;
			$('.sidebar-block .community-live-block:not(.sections) .row:not(.menu.main)').remove();
			$('.sidebar-block .community-live-block:not(.sections) .rows').html(html);
			$('.sidebar-block .community-live-block:not(.sections) .rows footer').remove();
			$('.sidebar-block .community-live-block:not(.sections) .menu .menu-item').removeClass('selected');
			$('.sidebar-block .community-live-block:not(.sections) .menu .menu-item[data-type=' + type + ']').addClass('selected');
			element_disable($('.sidebar-block .community-live-block:not(.sections)'));
		}
	});
	
	return true;
	
}

$(document).ready(function() {
	
	$('.sidebar-block .community-live-block:not(.sections) .menu .menu-item').on('click', function() {
		community_live_block_content($(this).data('type'));
		return false;
	});
	
	$('.sidebar-block .community-live-block:not(.sections) .row.message').poshytip({
		className: 'infobox',
		alignTo: 'target',
		alignX: 'left',
		alignY: 'center',
		offsetX: 10,
		liveEvents: true,
		showTimeout: 1000
	});
	
});
