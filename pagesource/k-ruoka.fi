    <script type="text/ng-template" id="/static/js/once-site/templates/teaser-context-menu.html">
    <div class="rk-teaser-context-menu" ng-if="!editMode">
        <a id="context-menu-video-btn" role="button" class="btn btn-rk-transparent theme-primary-color ng-hide" ng-show="teaserData.hasVideo" ng-href="{{teaserData.getPageURL()}}?play=1" target="_self" title="Katso video">
            <i class="glyphicon glyphicon-play-circle f-g"></i>
        </a>

        <button id="context-menu-action-btn" type="button" class="btn btn-rk-transparent" ng-click="toggleContextMenu();">
            <i class="rk-icon rk-icon-caret" ng-class="{'active': isOpen}"></i>
        </button>


        <div class="rk-context-menu-container" ng-class="{'slide-in': isOpen, 'slide-out': !isOpen}" ng-hide="teaserData.isPreview">
            <div ng-if="!isEqual(teaserType, 'offer')" class="rk-context-mneu-header">Toiminnot:</div>
            <div ng-if="isEqual(teaserType, 'offer')" class="rk-context-mneu-header">Lisätiedot:</div>
            <div class="btn-group-vertical" role="group" aria-label="Toiminnot">
                <div ng-if="isEqual(teaserType, 'recipe')" favourite-recipe-button="" content-id="{{teaserData.id}}" toggle-successfully="closeContextMenu();"></div>
            </div>
            <ul ng-if="isEqual(teaserType, 'offer')" class="list-no-style rk-context-menu-info-list" ng-hide="xs">
                <li class="ng-hide" ng-show="!isEmpty(teaserData.description)">{{teaserData.description |textTruncate:70}}</li>
                <li class="ng-hide Py-xs" ng-show="!isEmpty(teaserData.additionalInfo2)">
                    <strong ng-bind="teaserData.additionalInfo2"></strong>
                </li>
                <li class="ng-hide" ng-show="!isEmpty(teaserData.normalPrice)">
                    Normaalihinta: {{teaserData.normalPrice}} &euro;
                </li>
                <li>Hinta voimassa</li>
                <li><span>{{(teaserData.validFrom || teaserData.startPublish) |date:'shortDate'}}</span><span ng-if="!isEmpty(teaserData.stopPublish)"> - {{teaserData.stopPublish |date:'shortDate'}}</span><span ng-if="isEmpty(teaserData.stopPublish)">&nbsp;alkaen</span></li>
            </ul>
        </div>
    </div>
    <div class="rk-context-menu-mask ng-hide" ng-show="showMask"></div>
</script>

