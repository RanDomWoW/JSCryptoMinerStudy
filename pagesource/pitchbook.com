<script type="text/javascript">
    jQuery(function($){
        var owl = $(".our-client-slider");
        owl.owlCarousel({
            loop           : true,
            nav            : true,
            navText        : ['&#xf104;', '&#xf105;'],
            dots           : false,
            center         : true,
            responsiveClass: true,
            responsive     : {
                320 : {
                    items : 1,
                    margin: 20
                },
                640 : {
                    items: 3
                },
                1024: {
                    items: 3
                },
                1200: {
                    items: 5
                }
            }
        });
        var centerItemContent = $('.owl-item.center .hidden-content').html();
        $('.active-slide-viewer').html(centerItemContent);
        owl.on('translated.owl.carousel', function(event){
            centerItemContent = $('.owl-item.center .hidden-content').html();
            $('.active-slide-viewer').html(centerItemContent);
        });

		function _clickCallback () {
            var elemIndex = $(this).index(),
                    centerIndex = owl.find('.owl-item.center').index();

            if ( !$(this).hasClass('center') ) {

                // 7 & 6 are numbers of items for setup clicked item to center position
                if ( elemIndex > centerIndex ) {
                    owl.trigger('to.owl.carousel', [elemIndex - 7, '300', true]);
                } else {
                    owl.trigger('to.owl.carousel', [elemIndex + 6, '300', true]);
                }

            }
		}

		owl.find('.owl-item').bind('click.owl-items', _clickCallback);

		owl.on('resized.owl.carousel', function () {
            owl.find('.owl-item')
					.unbind('click.owl-items')
					.bind('click.owl-items', _clickCallback);
		});
    });
