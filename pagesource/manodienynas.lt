                            <script type="text/javascript">
                                function dl_form1_check(form) {
                                set_rem_cookie();
                                var result;
                                with (form) {
                                result = true &&
                                        dl_validate_required(username, 'Neįvestas prisijungimo vardas') &&
                                        dl_validate_login(username, 'Neteisingas prisijungimo vardas') &&
                                        dl_validate_required(password, 'Neįvestas slaptažodis');
                                }

                                if (result){
                                var url = $(form).attr("action");
                                $.ajax({
                                type: "POST",
                                        url: url,
                                        data: $('form[name="dienynas_form1"]').serialize(),
                                        success: function(data) {
                                        data = $.parseJSON(data);
                                        if (data.url !== false) {
                                        document.location.href = data.url;
                                        return;
                                        } else {
                                        $('form[name="dienynas_form1"] .loader').hide();
                                        $('form[name="dienynas_form1"] #login_submit').show();
                                        }

                                        if (data.message) {
                                        console.log(data);
                                        showNotif(data.message, 'error');
                                        }
                                        },
                                        beforeSend: function() {
                                        $('form[name="dienynas_form1"] .loader').show();
                                        $('form[name="dienynas_form1"] #login_submit').hide();
                                        },
                                        error: function() {
                                        $('form[name="dienynas_form1"] #login_submit').show();
                                        $('form[name="dienynas_form1"] .loader').hide();
                                        }
                                });
                                }
                                }

                                function set_rem_cookie() {
                                    DeleteCookie('dienynas_login_username');
                                    DeleteCookie('dienynas_login_password');
                                    DeleteCookie('dienynas_remember_me');
                                }

                                function SetCookie(c_name, c_value) {
                                var today = new Date();
                                var oneYear = (1000 * 60 * 60 * 24 * 365);
                                var c_date = new Date(today.getTime() + oneYear);
                                document.cookie = c_name + '=' + escape(c_value) + '; expires=' + c_date.toGMTString();
                                }

                                function DeleteCookie(name) {
                                document.cookie = name + '=;expires=Thu, 01-Jan-70 00:00:01 GMT';
                                }

                            </script>
