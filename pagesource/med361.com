	//选项卡
	function xuanxiangka(obj,child,className,box_1){
		obj=$('#'+obj);
		
		obj.find('.'+child).mouseover(function(){
			$('.'+box_1+':visible').hide();
			index=obj.find('.'+child).index($(this));
			$('.'+box_1).eq(index).show();
			$(this).removeClass(className).siblings().addClass(className);
		})
		
	}
	function top_bottom_scroll(obj){
		var _wrap=$('#'+obj);//定义滚动区域 
		var _interval=3000;//定义滚动间隙时间 
		var _moving;//需要清除的动画 
		_wrap.hover(function(){ 
		clearInterval(_moving);//当鼠标在滚动区域中时,停止滚动 
		},function(){ 
		_moving=setInterval(function(){ 
		var _field=_wrap.find('li:first');//此变量不可放置于函数起始处,li:first取值是变化的 
		var _h=_field.height();//取得每次滚动高度 
		_field.animate({marginTop:-_h+'px'},600,function(){//通过取负margin值,隐藏第一行 
		_field.css('marginTop',0).appendTo(_wrap);//隐藏后,将该行的margin值置零,并插入到最后,实现无缝滚动 
		}) 
		},_interval)//滚动间隔时间取决于_interval 
		}).trigger('mouseleave');//函数载入时,模拟执行mouseleave,即自动滚动 
	}
	top_bottom_scroll('lineOne');
	top_bottom_scroll('lineTwo');
	top_bottom_scroll('lineThree');
	//鼠标一过，图片渐变
	$('.taitou_box_right dl').hover(function(){
		$(this).css('opacity','0.8');
	},function(){
		$(this).css('opacity','1');
	});
		
	
</script>
