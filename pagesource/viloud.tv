<script>
jQuery(document).ready(function($){
if ( $('body').hasClass('home')) {
/*setStats();
var intervalStats=setInterval(setStats, 3000);*/
}
if ( $('body').hasClass('page-id-889') || $('body').hasClass('page-id-1074')) {
currency="USD";
if(typeof geoip2 !== 'undefined')
{
geoip2.country(function (response)
{
if(((response || {}).continent || {}).code == "EU")
{
currency="EUR";
}
setPlans(currency);
}, function (error)
{
setPlans(currency);
});
}
else
{
setPlans(currency);
}
}
function setPlans(currency){
plans = null;
$('#period-checkbox').prop('checked', false);
jQuery.ajax({
type: "GET",
url: 'https://app.viloud.tv/1/plans',
dataType: 'json',
data: {},
success: function (obj, textstatus) {
if( !('error' in obj) ) {
plans = obj;
if(plans !== null){
period="annual";
setPrices(currency, period, plans);
$('#period-checkbox').change(function() {
if($(this).is(':checked')){
console.log("checked");
period="monthly";
}
else{
period="annual";
}
setPrices(currency, period, plans);
});
$('#monthly-billing-button').click(function() {
if(period!="monthly"){
console.log("monthly click");
$('#period-checkbox').prop('checked', true);
period="monthly";
setPrices(currency, period, plans);
}
});
$('#annual-billing-button').click(function() {
if(period!="annual"){
console.log("annual click");
$('#period-checkbox').prop('checked', false);
period="annual";
setPrices(currency, period, plans);
}
});                    
}
}
else {
console.log(obj.error);
}
}
});
}
function setPrices(currency, period, plans){
priceFree = "0";
pricePro = plans.pro.period[period].price[currency];
priceBusiness = plans.business.period[period].price[currency];
if( currency == "EUR" ){
$( ".plan-price" ).addClass( "price-eur" );
$( ".price-without-discount" ).addClass( "price-eur" );
}else{
$( ".plan-price" ).addClass( "price-usd" );
$( ".price-without-discount" ).addClass( "price-usd" );
}
if(period == "monthly"){
$( ".price-without-discount" ).css( "visibility", "hidden" );
$( ".billed-annually-text" ).css( "visibility", "hidden" );
$("#annual-billing-button").css("opacity", "0.3");
$("#monthly-billing-button").css("opacity", "1");
}
else{
$( ".price-without-discount" ).css( "visibility", "inherit" );
$( ".billed-annually-text" ).css( "visibility", "inherit" );
$( "#price-pro-without-discount" ).text( plans.pro.period.monthly.price[currency]);
$( "#price-business-without-discount" ).text( plans.business.period.monthly.price[currency]);
$("#monthly-billing-button").css("opacity", "0.3");
$("#annual-billing-button").css("opacity", "1");
}
$( "#priceFree" ).html(priceFree);
$( "#pricePro" ).html(pricePro);
$( "#priceBusiness" ).html(priceBusiness); 
$(".orderPro").attr("href", "https://app.viloud.tv/order?plan=pro&period="+period);
$(".orderBusiness").attr("href", "https://app.viloud.tv/order?plan=business&period="+period);
}
if ( $('body').hasClass('page-id-508')){
setStats();
var intervalStats=setInterval(setStats, 3000);
};
function setStats(){
jQuery.ajax({
type: "GET",
url: 'https://app.viloud.tv/1/stats',
dataType: 'json',
data: {},
success: function (obj, textstatus) {
if( !('error' in obj) ) {
stats = obj;
if(stats !== null){
var views=parseInt(stats.views);
var channels=parseInt(stats.channels);
$('#numCreatedChannels').html(numberWithCommas(channels));
$('#numVideosServed').html(numberWithCommas(views));
}
}
else {
console.log(obj.error);
}
}
});
};
function numberWithCommas(x) {
return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
}
});
