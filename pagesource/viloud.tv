/* The switch - the box around the slider */
.vl-switch {
position: relative;
display: inline-block;
width: 60px;
height: 34px;
box-shadow: inherit;
}
/* Hide default HTML checkbox */
.vl-switch input {display:none;}
/* The slider */
.vl-slider {
position: absolute;
cursor: pointer;
top: 0;
left: 0;
right: 0;
bottom: 0;
background-color: #ccc;
-webkit-transition: .4s;
transition: .4s;
}
.vl-slider:before {
position: absolute;
content: "";
height: 26px;
width: 26px;
left: 4px;
bottom: 4px;
background-color: white;
-webkit-transition: .4s;
transition: .4s;
}
input:checked + .vl-slider:before {
-webkit-transform: translateX(26px);
-ms-transform: translateX(26px);
transform: translateX(26px);
}
/* Rounded sliders */
.vl-slider.round {
border-radius: 34px;
}
.vl-slider.round:before {
border-radius: 50%;
}
