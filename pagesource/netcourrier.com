function ea_auth_ident_profiles_display(elt)
{
	var obj=ea_getid("div_login_profiles");
	if (!obj)
		return;
	var pos=ea_elt_pos2(elt);
	var left=pos[0];
	if (elt.scrollHeight>elt.offsetHeight)
		var top=pos[1]+elt.scrollHeight;
	else
		var top=pos[1]+elt.offsetHeight;
	obj.style.left=left+"px";
	obj.style.top=top+"px";
	ea_elt_display(obj,true,true);

	document.onmousedown=function(e) {
		var elt=ea_elt_get_from_event(e);
		while (elt) {
			if ((elt.id=="id_login")||(elt.id=="div_login_profiles"))
				return;
			elt=elt.parentNode;
		}
		try {
			e.preventDefault();
		} catch(e) {}
		ea_auth_ident_profiles_hide();
		return;
	}
}
function ea_auth_ident_profiles_hide()
{
	ea_elt_display_id("div_login_profiles",false,false);
	document.onmousedown=function(e) {
		return;
	}
}
function ea_auth_ident_profiles_click(login,domain)
{
	ea_id_setval("id_login",login);
	ea_id_setval_all("id_domain",domain);
	ea_elt_display_id("id_domain_other",false,false);
	var obj=ea_getid("id_domain_0");
	if (obj&&obj.parentNode)
		ea_elt_display(obj.parentNode,true,true);
	ea_id_setval("id_check_ident_cookie",true);
	var obj=ea_getid("id_pwd_"+login);
	if (obj)
		ea_id_setval("id_pwd",obj.value);
	ea_focus("id_pwd");
	ea_auth_ident_profiles_hide();
}
function ea_auth_login_keyup(e)
{   
	var elt=ea_getid("id_login");
	if (elt) {
		var c=elt.value.substr(elt.value.length-1);
		if (c=="@") {
			elt.value=elt.value.substr(0,elt.value.length-1);
			var obj=ea_getid("id_domain_0");
			if (!obj||(obj.style.display=="none")) {
				obj=ea_getid("id_domain_other");
				if (!obj||(obj.style.display=="none"))
					obj=ea_getid("id_domain");
			}
			if (obj&&(obj.style.display!="none"))
				ea_elt_focus(obj);
		}
	}   
	return;
}       
ea_focus("id_login");
