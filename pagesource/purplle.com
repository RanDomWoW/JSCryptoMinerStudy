          var params = {"event":"device_registration","eventType":"setEvent","user_id":user_id,"identifier":visitorppl,"device_token":playerID,"mode_device":"Mobile","timeStamp":timeStamp,"user_status":status}; 

         
          var params = JSON.stringify(params);
