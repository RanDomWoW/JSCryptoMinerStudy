        function registerEvent(playerID,status){

          var visitorppl = getCookie('visitorppl');
          var timeStamp = parseInt(new Date().getTime()/1000);
          var user_id = (window.user_id == 0 || window.user_id == undefined || window.user_id == '')?visitorppl:window.user_id;
          var http = new XMLHttpRequest();
