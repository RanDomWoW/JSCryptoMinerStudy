<script async='true' type='text/javascript'>//<![CDATA[
jQuery.noConflict();
jQuery(document).ready(function($){
	$('#PopularPosts2 ul li .item-title a').each(function(){
  		var titleString = $(this).text();
		var titleLength = titleString.length;
		
      	if(titleLength > 70){
    		$(this).text(titleString.substr(0,70) + "...");
        };
	});
});
