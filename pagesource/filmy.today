            $.fn.breaking_news = function () {
                $ele = $(this);
                $margin_left = 0;
                $type = 1;
                $twidth = $ele.width() - 950;
                $li = $ele.find('li:first');
                $this = $(this);
                $timer = '';
                var scroller = {
                    start: function () {
                        $timer = setInterval(function () {
                            if ($type === 1)
                                $margin_left = $margin_left + 1;
                            else
                                $margin_left = $margin_left - 1;
                            $li.css({'margin-left': -$margin_left});
                            if ($margin_left >= $twidth)
                                $type = 2;
                            if ($margin_left <= 0)
                                $type = 1;
                        }, 20);
                    },
                    pause: function () {
                        clearInterval($timer);
                    }
                };
                setTimeout(function () {
                    scroller.start();
                }, 3000);
                $ele.mouseenter(function () {
                    scroller.pause();
                });
                $ele.mouseleave(function () {
                    scroller.start();
                });
            };
        </script>
