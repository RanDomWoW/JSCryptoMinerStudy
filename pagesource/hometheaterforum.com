    findKeyWordsMatch: function(subCategory, filterKeys) {
        var filterKeysLength = filterKeys.length,
            keyWordsMatched = 0, // number of words that match
            keyWordsDescriptionMatched = 0; // number of words that match the description
        for (var filterKeyIndex = 0; filterKeyIndex < filterKeysLength; filterKeyIndex++) {
            var keyWordIndex = subCategory.nameLower.indexOf(filterKeys[filterKeyIndex].toLowerCase());
            if (keyWordIndex != -1 && filterKeys[filterKeyIndex].length > 0) {
                keyWordsMatched++;
            }
            var descriptionPlusTitle = subCategory.descriptionLower + " " + subCategory.nameLower;
            if (descriptionPlusTitle.indexOf(filterKeys[filterKeyIndex].toLowerCase()) != -1) {
                keyWordsDescriptionMatched++;
            }
        }
        var keyWordsFullMatchFound = false;
        if (keyWordsMatched == filterKeysLength && keyWordsMatched != 0) {
            keyWordsFullMatchFound = true;
        }
        var keyWordsDescriptionFullMatchFound = false;
        if (keyWordsDescriptionMatched == filterKeysLength && filterKeysLength != 0) {
            keyWordsDescriptionFullMatchFound = true;
        }
        return {
            "categoryTitle": keyWordsFullMatchFound,
            "categoryDescription": keyWordsDescriptionFullMatchFound
        }
    },
    findPositionsToHighlight: function(filterKeys, newStringTemp) {
        // run through each key word
        var filterKeysLength = filterKeys.length,
            newStringTempLower = newStringTemp.toLowerCase();
        positionsToHighlight = [];
        for (var filterKeyIndex = 0; filterKeyIndex < filterKeysLength; filterKeyIndex++) {
            var currentFilterKey = filterKeys[filterKeyIndex],
                currentFilterKeyLower = currentFilterKey.toLowerCase();
            if (newStringTempLower == currentFilterKeyLower) {
                applyHighlight = true;
                positionsToHighlight = 1;
                break;
            } else if (newStringTempLower.indexOf(currentFilterKeyLower) == 0) {
                applyHighlight = true;
                positionsToHighlight.push([0, currentFilterKeyLower.length]);
            } else if (newStringTempLower.indexOf(currentFilterKeyLower) > 0 && currentFilterKeyLower.length >= 3) {
                applyHighlight = true;
                positionsToHighlight.push([newStringTempLower.indexOf(currentFilterKeyLower), (newStringTempLower.indexOf(currentFilterKeyLower) + currentFilterKeyLower.length)]);
            }
        }
        if(positionsToHighlight.length == 0){
            positionsToHighlight = -1;
        }
        return positionsToHighlight;
    },
    combineTouchingPositions: function(positionsToHighlight){
      if (positionsToHighlight != -1 && positionsToHighlight != 1 && positionsToHighlight.length != 0) {
          positionsToHighlight = positionsToHighlight.sort(function(a, b) {
              return a[0] - b[0]
          });
          var positionsToHighlightTemp = [];
          for (var p = 0, pLen = positionsToHighlight.length; p < pLen; p++) {
              if (positionsToHighlightTemp.length == 0) {
                  positionsToHighlightTemp.push([positionsToHighlight[p][0], positionsToHighlight[p][1]]);
              } else {
                  var lastPositionData = positionsToHighlightTemp[positionsToHighlightTemp.length - 1];
                  if (positionsToHighlight[p][0] >= lastPositionData[0] && positionsToHighlight[p][0] <= lastPositionData[1] && positionsToHighlight[p][1] > lastPositionData[1]) {
                      positionsToHighlightTemp[positionsToHighlightTemp.length - 1][1] = positionsToHighlight[p][1];

                  } else if (positionsToHighlight[p][0] > lastPositionData[1]) {
                      positionsToHighlightTemp.push([positionsToHighlight[p][0], positionsToHighlight[p][1]]);
                  }
              }
          }
      } else if(positionsToHighlight == 1) {
          var positionsToHighlightTemp = 1;
      } else if(positionsToHighlight == -1){
          var positionsToHighlightTemp = -1;
      }
      return positionsToHighlightTemp;
    },
    highlightWordPositions: function(word, positionsToHighlightCombined){
      var newStringTempParts = [],
      applyHighlight = false;
      if(typeof positionsToHighlightCombined == "object"){
          positionsToHighlightCombinedLength = positionsToHighlightCombined.length;
      }
      if (positionsToHighlightCombined != -1 && positionsToHighlightCombined != 1 && typeof(positionsToHighlightCombined) == "object" && positionsToHighlightCombinedLength != 0) {
          applyHighlight = true;
          var tempString = "";
          // If there is text before highlighting starts, insert it
          if(positionsToHighlightCombined[0][0] != 0){
            tempString = tempString + word.substring(0, positionsToHighlightCombined[0][0]);
          }
          // insert middle parts (highlighted or not highlighted)
          for (var positionsToHighlightCombinedIndex = 0; positionsToHighlightCombinedIndex < positionsToHighlightCombinedLength; positionsToHighlightCombinedIndex++) {
              tempString = tempString + "<span class='forumListFilterHighlight'>" + word.substring(positionsToHighlightCombined[positionsToHighlightCombinedIndex][0], positionsToHighlightCombined[positionsToHighlightCombinedIndex][1]) + "</span>";
              if ((positionsToHighlightCombinedIndex + 1) < positionsToHighlightCombinedLength) {
                tempString = tempString + word.substring(positionsToHighlightCombined[positionsToHighlightCombinedIndex][1], positionsToHighlightCombined[positionsToHighlightCombinedIndex + 1][0]);
              }
          }
          // If there is text after the highlighting ends, insert it
          if(positionsToHighlightCombined[positionsToHighlightCombinedLength - 1][1] != positionsToHighlightCombinedLength - 1){
            tempString = tempString + word.substring(positionsToHighlightCombined[positionsToHighlightCombinedLength - 1][1]);
          }
          newStringTempParts.push(tempString);
      } else {
        if(positionsToHighlightCombined == 1){
          applyHighlight = true;
          newStringTempParts.push("<span class='forumListFilterHighlight'>" + word + "</span>");
        }else{
          newStringTempParts = [word];
        }
      }
      return {
        "highlightNeeded": applyHighlight,
        "highlightedWord": newStringTempParts.join(" ")
      };
    },
    init: function() {
        var forumListNodes = document.querySelectorAll(themeHouse.forumListFilter.domInfo.selectors.forumNodes);
        for (var i = 0, forumListNodesLen = forumListNodes.length; i < forumListNodesLen; i++) {
            if (forumListNodes[i].className.indexOf(themeHouse.forumListFilter.domInfo.forumCategoryPrefix) != -1) {
                if (themeHouse.forumListFilter.domInfo.selectors.forumNodeCategoryName == null) {
                    var categoryTextNode = forumListNodes[i];
                } else {
                    var categoryTextNode = forumListNodes[i].querySelector(themeHouse.forumListFilter.domInfo.selectors.forumNodeCategoryName);
                }
                themeHouse.forumListFilter.forumListCategories.push({
                    "ele": forumListNodes[i],
                    "name": themeHouse.forumListFilter.textDecode(categoryTextNode.innerHTML.trim()),
                    "state": true,
                    "categories": []
                });
            } else {
                if (themeHouse.forumListFilter.domInfo.selectors.forumNodeSubCategoryName == null) {
                    var subCatEle = forumListNodes[i];
                } else {
                    var subCatEle = forumListNodes[i].querySelector(themeHouse.forumListFilter.domInfo.selectors.forumNodeSubCategoryName);
                }
                if (themeHouse.forumListFilter.domInfo.selectors.forumNodeDescriptionName == null) {
                    var descriptionEle = subCatEle;
                } else {
                    var descriptionEle = subCatEle.parentElement;
                }
                if (subCatEle !== null) {
	                var catName = themeHouse.forumListFilter.stripHTML(themeHouse.forumListFilter.textDecode(subCatEle.innerHTML.trim()));
	                themeHouse.forumListFilter.forumListCategories[themeHouse.forumListFilter.forumListCategories.length - 1].categories.push({
	                    "ele": forumListNodes[i],
	                    "textEle": subCatEle,
	                    "name": catName,
	                    "nameLower": catName.toLowerCase(),
	                    "nameSplit": catName.split(/\s+?/),
	                    "descriptionLower": descriptionEle.getAttribute(themeHouse.forumListFilter.domInfo.forumSubCategoryDescriptionAttribute).toLowerCase(),
	                    "state": true,
	                    "highlightState": false
	                });
	        }
            }
        }
        themeHouse.forumListFilter.needsInit = false;
    },
    filter: function(filterKeyString) {
        if (themeHouse.forumListFilter.needsInit) {
            themeHouse.forumListFilter.init();
        }
        // If the filter key did not change, do no run
        if (themeHouse.forumListFilter.lastSearch != filterKeyString) {
            themeHouse.forumListFilter.lastSearch = filterKeyString;
            // Remove all commas, non-ASCII characters
            filterKeyString = filterKeyString.replace(new RegExp(",", 'g'), ""); // remove commas
            filterKeyString = themeHouse.forumListFilter.accentDecode(filterKeyString); // remove non-ASCII characters
            // Split the filter string into separate words
            var filterKeys = filterKeyString.trim().split(/\s+?/); // split into words
            //if filterKeys returns an empty string, reset it
            if (filterKeys[0] == "") {
                filterKeys = [];
            }
            var filterKeysLen = filterKeys.length; // number of words
            // Must keep track if results were found to know weather or not to display the no results box
            var resultsFound = false;
            // Loop through keep category
            for (var i = 0, forumListCategoriesLen = themeHouse.forumListFilter.forumListCategories.length; i < forumListCategoriesLen; i++) {
                var category = themeHouse.forumListFilter.forumListCategories[i];
                // Must keep track if a sub category was found to know weather or not to display the category
                var displayCategory = false;
                // Loop through each sub category
                for (var x = 0, forumListCategoriesSubLen = themeHouse.forumListFilter.forumListCategories[i].categories.length; x < forumListCategoriesSubLen; x++) {
                    var subCategory = themeHouse.forumListFilter.forumListCategories[i].categories[x];
                    // Find out if the Category Title and Category Description had full matches or not
                    // doKeyWordsMatch.categoryTitle = Match found (true | false)
                    // doKeyWordsMatch.categoryDescription = Match found (true | false)
                    var doKeyWordsMatch = themeHouse.forumListFilter.findKeyWordsMatch(subCategory, filterKeys);
                    // Must keep track if highlighting for this word must be applied
                    var applyHighlight = false;
                    if (doKeyWordsMatch.categoryTitle) { // full match of filter key input
                        resultsFound = true;
                        // Grab the sub category name and split it into separate words
                        var nameSplit = subCategory.nameSplit;
                        var nameSplitLen = nameSplit.length;
                        // Store the new sub category string parts here
                        var tempNameSplit = [];
                        // run through each word in the category title
                        for (var z = 0; z < nameSplitLen; z++) {
                            // The word to highlight
                            var newStringTemp = nameSplit[z];
                            // find the positions to highlight
                            var positionsToHighlight = themeHouse.forumListFilter.findPositionsToHighlight(filterKeys, newStringTemp);
                            // process the highlight positions - Connects highlight ranges that touch
                            var combinedPositionsToHighlight = themeHouse.forumListFilter.combineTouchingPositions(positionsToHighlight);
                            // Find out if highlighting must be applied, and if so get the highlighted word
                            // highlightedWordData.highlightNeeded = Highlight needed (true | false)
                            // highlightedWordData.highlightedWord = The highlighted word (String)
                            var highlightedWordData = themeHouse.forumListFilter.highlightWordPositions(newStringTemp, combinedPositionsToHighlight);
                            var highlightedWord = highlightedWordData.highlightedWord;
                            // If highlighting will be needed, set to true
                            if(highlightedWordData.highlightNeeded == true){
                              applyHighlight = true;
                            }
                            // Add the highlighted word to the sub category title parts
                            tempNameSplit.push(highlightedWord);
                        }
                        displayCategory = true;
                        if (subCategory.state == false) { // show sub category
                            themeHouse.forumListFilter.toggleVisibility(true, subCategory.ele);
                            themeHouse.forumListFilter.forumListCategories[i].categories[x].state = true;
                        }
                        if (applyHighlight) { // change sub cat html for highlighting if update is needed
                            subCategory.textEle.innerHTML = tempNameSplit.join(" ");
                            themeHouse.forumListFilter.forumListCategories[i].categories[x].highlightState = true;
                        } else if (subCategory.highlightState == true) { // remove highlighting
                            subCategory.textEle.innerHTML = subCategory.name;
                            themeHouse.forumListFilter.forumListCategories[i].categories[x].highlightState = false;
                        }
                    } else if (filterKeysLen != 0) { // key words found
                        if (themeHouse.forumListFilter.searchDescriptions && doKeyWordsMatch.categoryDescription) { // If search descriptions is enabled, and the description keywords fully matched the sub category description, display the category
                            displayCategory = true;
                            resultsFound = true;
                            // Grab the sub category name and split it into separate words
                            var nameSplit = subCategory.nameSplit;
                            var nameSplitLen = nameSplit.length;
                            // Store the new sub category string parts here
                            var tempNameSplit = [];
                            // run through each word in the category title
                            for (var z = 0; z < nameSplitLen; z++) {
                                // The word to highlight
                                var newStringTemp = nameSplit[z];
                                // find the positions to highlight
                                var positionsToHighlight = themeHouse.forumListFilter.findPositionsToHighlight(filterKeys, newStringTemp);
                                // process the highlight positions - Connects highlight ranges that touch
                                var combinedPositionsToHighlight = themeHouse.forumListFilter.combineTouchingPositions(positionsToHighlight);
                                // Find out if highlighting must be applied, and if so get the highlighted word
                                // highlightedWordData.highlightNeeded = Highlight needed (true | false)
                                // highlightedWordData.highlightedWord = The highlighted word (String)
                                var highlightedWordData = themeHouse.forumListFilter.highlightWordPositions(newStringTemp, combinedPositionsToHighlight);
                                var highlightedWord = highlightedWordData.highlightedWord;
                                // If highlighting will be needed, set to true
                                if(highlightedWordData.highlightNeeded == true){
                                  applyHighlight = true;
                                }
                                // Add the highlighted word to the sub category title parts
                                tempNameSplit.push(highlightedWord);
                            }
                            if (subCategory.state == false) { // description match found, display sub category
                                themeHouse.forumListFilter.toggleVisibility(true, subCategory.ele);
                                themeHouse.forumListFilter.forumListCategories[i].categories[x].state = true;
                            }
                            if (applyHighlight) { // change sub cat html for highlighting if update is needed
                                subCategory.textEle.innerHTML = tempNameSplit.join(" ");
                                themeHouse.forumListFilter.forumListCategories[i].categories[x].highlightState = true;
                            } else if (subCategory.highlightState == true) { // remove highlighting
                                subCategory.textEle.innerHTML = subCategory.name;
                                themeHouse.forumListFilter.forumListCategories[i].categories[x].highlightState = false;
                            }
                        } else {
                            if (subCategory.highlightState == true && subCategory.state == true) {
                                subCategory.textEle.innerHTML = subCategory.name;
                                themeHouse.forumListFilter.forumListCategories[i].categories[x].highlightState = false;
                                themeHouse.forumListFilter.toggleVisibility(false, subCategory.ele);
                                themeHouse.forumListFilter.forumListCategories[i].categories[x].state = false;
                            } else if (subCategory.state == true) {
                                themeHouse.forumListFilter.toggleVisibility(false, subCategory.ele);
                                subCategory.textEle.innerHTML = subCategory.name;
                                themeHouse.forumListFilter.forumListCategories[i].categories[x].state = false;
                            } else if (subCategory.highlightState == true) {
                                subCategory.textEle.innerHTML = subCategory.name;
                                themeHouse.forumListFilter.forumListCategories[i].categories[x].highlightState = false;
                            }
                        }
                    } else {
                        if (subCategory.state == false) { // no key words given -> show sub category
                            themeHouse.forumListFilter.toggleVisibility(true, subCategory.ele);
                            themeHouse.forumListFilter.forumListCategories[i].categories[x].state = true;
                        }
                        if (subCategory.highlightState == true) {
                            subCategory.textEle.innerHTML = subCategory.name;
                            themeHouse.forumListFilter.forumListCategories[i].categories[x].highlightState = false;
                        }
                    }
                }
                if (themeHouse.forumListFilter.hideEmptyCategories) {
                    if (displayCategory || filterKeyString == "") {
                        if (themeHouse.forumListFilter.forumListCategories[i].state == false) { // show category
                            themeHouse.forumListFilter.toggleVisibility(true, category.ele);
                            themeHouse.forumListFilter.forumListCategories[i].state = true;
                        }
                    } else {
                        if (themeHouse.forumListFilter.forumListCategories[i].state == true) { // hide category
                            themeHouse.forumListFilter.toggleVisibility(false, category.ele);
                            themeHouse.forumListFilter.forumListCategories[i].state = false;
                        }
                    }
                }
            }
            if (filterKeyString != "" && !resultsFound) {
                themeHouse.forumListFilter.showNoResultsBox(true);
            } else {
                themeHouse.forumListFilter.showNoResultsBox(false);
            }
        }
    },
    showNoResultsBox: function(show) {
        if (themeHouse.forumListFilter.noResultsEle == null) {
            themeHouse.forumListFilter.noResultsEle = document.querySelector(themeHouse.forumListFilter.domInfo.noResultsBoxId);
        }
        if (show && !themeHouse.forumListFilter.noResultsVisible) {
            themeHouse.forumListFilter.noResultsVisible = true;
            themeHouse.forumListFilter.toggleVisibility(true, themeHouse.forumListFilter.noResultsEle);
        } else if (!show && themeHouse.forumListFilter.noResultsVisible) {
            themeHouse.forumListFilter.noResultsVisible = false;
            themeHouse.forumListFilter.toggleVisibility(false, themeHouse.forumListFilter.noResultsEle);
        }
    }
};

document.addEventListener("DOMContentLoaded", function(event) {
  var forumListFilterSearch = document.getElementById("forumListFilterSearch");
  forumListFilterSearch.onkeyup = function(){
    themeHouse.forumListFilter.filter(forumListFilterSearch.value);
  };
  forumListFilterSearch.onchange = function(){
    themeHouse.forumListFilter.filter(forumListFilterSearch.value);
  };
});
</script>
