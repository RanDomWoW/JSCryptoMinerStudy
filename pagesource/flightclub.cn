<script>
var fn_img_lazy_load = function() {
	$("img.lazy").lazyload({
		failure_limit : 10,
		effect : "fadeIn"
	});
}

var news_list_pos = 30;
var fn_load_more_news = function() {
	var makeup_large = function(data) {
		var url = data.url;
		var pic = data.pic;
		var title = data.title;
		var senddate = data.senddate;
		var desc = data.description;

		var html = '<div class="large_item"> <div class="title"> <a target=_blank href="'+url+'">'+title+'</a> </div> <div class="pic"> <a target=_blank href="'+url+'"><img class="lazy" data-original="http://www2.flightclub.cn'+pic+'" src="http://www2.flightclub.cn/static/img/news_blank_bg_pic.png"></a> </div> <div class="bar"><div class="summary">'+(desc.length>0?'<i class="fa fa-quote-left"></i> ':'')+desc+'</div> <div class="more"><a target=_blank href="'+url+'" class="btn btn-primary">查看详情 <i class="fa fa-play"></i></a></div> <div class="cl"></div></div> <div class="cl"></div> </div> <div class="grey_bar"></div>';
		return html;
	}

	var makeup_normal = function(data) {
		var url = data.url;
		var pic = data.pic;
		var title = data.title;
		var senddate = data.senddate;
		var desc = data.description;

		var html = '<div class="normal_item"> <div class="pic"> <a target=_blank href="'+url+'"><img class="lazy" data-original="http://www2.flightclub.cn'+pic+'" src="http://www2.flightclub.cn/static/img/news_blank_bg_pic.png"></a> </div> <div class="info"> <div class="title"> <a target=_blank href="'+url+'">'+title+'</a> </div> <div class="summary">'+desc+'</div> <div class="text_bar"><i class="fa fa-clock-o"></i> '+senddate+'</div> </div> <div class="cl"></div> </div> <div class="grey_bar"></div>';
		return html;
	}

	var btn = $("#load_more_btn");
	var btn_old_html = btn.html();
	if (btn.hasClass("loading")) {
		return;
	}
	btn.attr("class", "button loading");
	btn.html("正在读取中，请稍等哦...");
	var api = "http://www.flightclub.cn/mapi/data/news_list_full/" + news_list_pos + "?" + Math.random();
	$.getJSON(api, function(json){
		btn.attr("class", "button");
		btn.html(btn_old_html);
		if (json && json.ret == "ok") {
			news_list_pos += json.msg.count;
			var html_list = "";
			$.each(json.msg.list, function(k, v){
				if (v.flag.indexOf("c") > -1) {
					html_list += makeup_large(v); 
				} else {
					html_list += makeup_normal(v); 
				}
			});
			$("#news_list").append(html_list);
			fn_img_lazy_load();
		}
	});
}
var slide_slick = function() {
	$(".slides").slick({
		dots: true,
		pauseOnDotsHover: true,
		autoplay: true,
		autoplaySpeed: 6000,
		infinite: true,
		arrows: false
	});
}
// on-load
$(function(){
	fn_img_lazy_load();
	fccn_sticky("home_ss");
	fccn_sticky("extra_list_prom");
	slide_slick();
	fccn_top_hover_switch($(".nav_and_search_bar"), $(".home_list").offset().top);
});
</script>
