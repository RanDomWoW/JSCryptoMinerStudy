<script type="text/javascript">
    $(".tool > li").hover(
            function () {
                $(this).find("a").first().addClass("toolitemhover");
            },
            function () {
                $(this).find("a").first().removeClass("toolitemhover");
            }
    );
    $(".subtool li").hover(
            function () {
                $(this).find("a").first().addClass("toolsubitemhover");
            },
            function () {
                $(this).find("a").first().removeClass("toolsubitemhover");
            }
    );
    $(".toolitemwithsub").hover(
            function () {
                var h = $(this).find("ul.subtool").height();
                var submenu = $(this).find("ul.subtool");
                if (submenu.attr("h") == "n") {
                    submenu.attr("h", h);
                }
                submenu.show().css("height", "0px").stop().animate({ height: submenu.attr("h") + "px" }, 300);
            },
            function () {
                var h = $(this).find("ul.subtool").height();
                $(this).find("ul.subtool").stop().animate({ height: "0px" }, 300, function () { $(this).hide() });
            }
    );
</script>
