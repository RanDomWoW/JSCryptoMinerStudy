<script>
// MovClassic 
var MovClassic = function (videoWrapper, controller, video) {
  this.videoParent = $('#u-Mov');
  this.closeBtn = $('#closeBtn');
  this.videoWrapper = videoWrapper;
  this.controller = controller;
  this.video = video;
  this.lock = false;
}
MovClassic.prototype = {
  mov_play: function () {
    this.videoWrapper.each(function (i, ele) {    
      var $video = $(this).find('video');
      var $vPlay = $(this).find('.v_play');
      var $siblingsVideo = $(this).siblings('.v_box').find('video');
      
      $(this).click(function () {
        if ($video.get(0).paused) {
          $video.get(0).play();
          if(checkClass($vPlay,'playIcon')){
            $vPlay.removeClass('playIcon').addClass('pauseIcon');
          }
          if ($siblingsVideo) {
            $siblingsVideo.get(0).pause();
          }
        } else {
          $video.get(0).pause();
          if (checkClass($vPlay, 'pauseIcon')) {
            $vPlay.removeClass('pauseIcon').addClass('playIcon')
          }
        }
      })



      $(this).hover(function () {
        if ($video.get(0).paused) {
          if (checkClass($vPlay, 'pauseIcon')) {
            $vPlay.removeClass('pauseIcon').addClass('playIcon').fadeIn();
          } else {
            $vPlay.addClass('playIcon').fadeIn();
          }
        } else {
          if (checkClass($vPlay, 'playIcon')) {
            $vPlay.removeClass('playIcon').addClass('pauseIcon').fadeIn();
          } else {
            $vPlay.addClass('pauseIcon').fadeIn();
          }
        }

      }, function () {
        $vPlay.removeClass('playIcon').removeClass('pauseIcon').fadeOut();
      })
    })
    function checkClass(ele, className) {
      return (ele.hasClass(className)) ? true : false;
    }

    return this;
  },
  mov_scroll: function (className, height) {
    // className为类名,height为参考线高度
    var _this = this;
    $(window).scroll(function () {
      if ($(this).scrollTop() > height) {
        _this.videoParent.addClass(className).find(_this.controller).addClass(
          className)
        if (!_this.lock) {
          _this.closeBtn.show()
        }
      } else {
        _this.videoParent.removeClass(className).find(_this.controller).removeClass(
          className)
        _this.videoWrapper.show();
        _this.videoParent.show();
        _this.closeBtn.hide();
        _this.lock = false
      }
    })
    return this;
  },
  mov_fullSize: function (bannerWrapper, className) {
    var _this = this,
      iniWidth = _this.videoWrapper.width(),
      iniHeight = _this.videoWrapper.height(),
      windowWidth = $(window).width(),
      windowHeight = $(window).height(),
      movWidth = windowWidth / 2,
      movHeight = windowHeight / 2;

    bannerWrapper.on('click', function () {
      $('#mask').show();
      _this.videoWrapper.width(movWidth).height(movHeight);
      _this.videoWrapper.addClass(className);
    })
    $('#mask').on('click', function () {
      $(this).hide();
      _this.videoWrapper.width(iniWidth).height(iniHeight);
      _this.videoWrapper.removeClass(className);
    })
    return this;

  },
  closeVideo: function () {
    var _this = this;
    _this.closeBtn.click(function () {
      _this.lock = true;
      _this.closeBtn.hide();
      _this.videoParent.hide();
      _this.videoWrapper.hide();
      _this.video.get(0).pause();
      _this.controller.show();

    })
    return this;
  }
}

var mov1 = new MovClassic($('.v_box'));
mov1.mov_play();
</script>
