<script>

jQuery(function($) {
	var element = document.getElementById('bottom-fixed-btn');
	var target = document.getElementById('toc_container');
	var target2 = document.getElementById('site-footer');
	// element.hide();
	
	if(element != null && target != null && target2 != null) {

		$(window).on('load scroll resize', function() {
			var elementH = element.clientHeight;
			var scTop = window.pageYOffset;
			var winH = window.innerHeight;
			
			var targetRect = target.getBoundingClientRect();
			var targetPos = window.pageYOffset + targetRect.top;
			var targetH = target.clientHeight;
			
			var target2Rect = target2.getBoundingClientRect();
			var target2Pos = window.pageYOffset + target2Rect.top;
			var target2H = target2.clientHeight;

			if(scTop > targetPos + targetH && scTop < target2Pos - elementH - winH) {
				element.classList.remove('fadeout');
				element.classList.add('fadein');
			} else {
				element.classList.remove('fadein');
				element.classList.add('fadeout');
			}
		});
	}
});


</script>

<script>

jQuery(function($) {

	var $element = $('.accordion');
	$element.on('click', function() {
		if(!$(this).next().is(':animated')) {
      if( $(this).next().css('display') == "block" ) {
        $(this).removeClass('on').next().slideUp();
      } else {
        $(this).addClass('on').next().slideDown();
      }
    }
	})
});

</script>

