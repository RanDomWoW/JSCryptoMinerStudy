	<script src="https://s.ytimg.com/yts/jsbin/player-en_US-vflFKNtIl/base.js" name="player/base"></script>

	<script language="javascript">
	var isMPOpen = false;
	var tm_id  = null;
	var tm_cnt = 0;
	var tm_max = 30;
	var html   = '';

	var yt_player;

	$("#mp_box").mouseover(function() {
	  if (typeof yt_player === "object")
			yt_player.unMute();
	}).mouseout(function() {
	  if (typeof yt_player === "object")
			yt_player.mute();
	});

	function onPlayerReady(e) {
		if (typeof yt_player === "object"){
			e.target.mute();

		  tm_id = setInterval(function(){
				var tm = parseInt(yt_player.getDuration()) - parseInt(yt_player.getCurrentTime());
				$("#mp_duration").html( tm + "초");
				$("#mp_duration").show();
		  }, 1000);
		}
	}

	function onPlayerStateChange(e) {
	  if (typeof YT === "object"){
			if (e.data == YT.PlayerState.PLAYING) {
				$("#mp_preview").hide();
			} else if (e.data == YT.PlayerState.ENDED) {
				resetDuraMP();
				closeMP();
			}
		}
	}

	function resetDuraMP() {
		if (tm_id != null) { clearInterval(tm_id); tm_id = null; }
		tm_cnt = 0;
		$("#mp_duration").hide();
	}

	function showDuraMP() {
		resetDuraMP();

		$("#mp_duration").html(tm_max + "초");
		$("#mp_duration").show();

	  tm_id = setInterval(function(){
			$("#mp_duration").html(((tm_max-1)-tm_cnt) + "초");
			tm_cnt++;
			if (tm_cnt >= tm_max) closeMP();
	  }, 1000);
	}

	function initMP() {
		html = '<a href="javascript:showsmallLanding();"><img src="/ads/images/banner/kia/03162018/KIA internet_Stinger_Sustain_RK_990x67_031318.jpg"></a>';

		resetDuraMP();

		if (typeof yt_player === "object")
			yt_player = null;

		$("#mp_preview").hide();
		$("#mp_frame").css("height", "67px");
		$("#mp_box").css("clip", "rect(0px, 990px, 67px, 0px)");
		$("#mp_box").html(html);
		$("#mp_btn_open").show();
		$("#mp_btn_close").hide();
		$("#mp_btn_detail").hide();

		$("#home_popup_frame").css("top", "740px");
	}

	function closeMP() {
		initMP();
		setCookie('mp', 'close', 1);
	}

	function openMP() {
		$("#mp_preview").show();

		html = '<div id="mp_video"><img id="mp_preview" src="/ads/images/banner/kia/03162018/KIA Internet_Stinger_Sustain_RK_990x557_031318.jpg"><iframe id="mp_video_holder" width="990" height="557" src="https://www.youtube.com/embed/43JjZzGch90?enablejsapi=1&amp;autoplay=1&amp;rel=0&amp;controls=0&amp;showinfo=0&amp;cc_lang_pref=en" frameborder="0" allowfullscreen></iframe></div><div id="mp_block" onclick="javascript:showLanding();"></div>';

		$("#mp_frame").css("height", "557px");
		$("#mp_box").css("clip", "rect(0px, 990px, 557px, 0px)");
		$("#mp_box").html(html);
		$("#mp_btn_open").hide();
		$("#mp_btn_close").show();
		$("#mp_btn_detail").show();

		if (typeof yt_player === "object")
			yt_player = null;

		$.getScript("//www.youtube.com/iframe_api", function() {
	    yt_int = setInterval(function(){
        if (typeof YT === "object"){
				    yt_player = new YT.Player('mp_video_holder', {
				        playerVars: {
						      modestbranding: 1,  // Hide the Youtube Logo
						      loop: 0,            // Run the video in a loop
						      fs: 0,              // Hide the full screen button
				        	cc_load_policty: 0,
				        	iv_load_policy: 3
				        },
				        events: {
									'onReady': onPlayerReady,
									'onStateChange': onPlayerStateChange
				        }
				    });
            clearInterval(yt_int);
	      }
	    }, 500);

	    preview_int = setInterval(function(){
				$("#mp_preview").hide();

				clearInterval(preview_int);
	    }, 3000);
		});

		$("#home_popup_frame").css("top", "1215px");
	}

	function showLanding() {
		closeMP();
		cklog("https%3A%2F%2Fwww.kia.com%2Fus%2Fen%2Fvehicle%2Fstinger%2F2018%2Foverview%3Futm_source%3Dvideo%26utm_medium%3Drk%26utm_campaign%3D2018mar%26utm_term%3Dmainexpand%26utm_content%3Dsustain");
		var hWnd = window.open('', "showLanding", "");
		if (hWnd != null) hWnd.location.href = "https://www.kia.com/us/en/vehicle/stinger/2018/overview?utm_source=video&utm_medium=rk&utm_campaign=2018mar&utm_term=mainexpand&utm_content=sustain";
		if (parseInt(navigator.appVersion) >= 4) hWnd.window.focus();
	}

	function showsmallLanding() {
		closeMP();
		cklog("https%3A%2F%2Fwww.kia.com%2Fus%2Fen%2Fvehicle%2Fstinger%2F2018%2Foverview%3Futm_source%3Dbanner%26utm_medium%3Drk%26utm_campaign%3D2018mar%26utm_term%3Dmainsmall%26utm_content%3Dsustain");
		var hWnd = window.open('', "showLanding", "");
		if (hWnd != null) hWnd.location.href = "https://www.kia.com/us/en/vehicle/stinger/2018/overview?utm_source=banner&utm_medium=rk&utm_campaign=2018mar&utm_term=mainsmall&utm_content=sustain";
		if (parseInt(navigator.appVersion) >= 4) hWnd.window.focus();
	}

	function cklog(url) {
		$.ajax({url:"/tools/cklog.php?url="+url});
	}

	initMP();

	if (getCookie('mp') == null || getCookie('mp') == "") {
		openMP();
		isMPOpen = true;
	}
	</script>
