<div class="email-section">
    <div class="col-sm-12">
        <h3>Sign up for inside Email Alerts & Start  saving!</h3>
        <p>Sign up and SAVE BIG!!! It's simple. Enter the details below and click the "EMAIL SIGN UP" button to start receiving factorydirect.ca emails. These emails could include amazing deals, flyers, contests, coupons, promotions, information and more!</p>
        <div class="fld">
            <input class="inp" data-val="true" data-val-required="The Email field is required." id="newsletter-email" name="Email" placeholder="Enter your email here..." type="text" value="" />
           
            <input type="button" value="Email sign up" id="newsletter-subscribe-button" class="btn" />
            
        </div>
        <div class="newsletter-validation">
            <span id="subscribe-loading-progress" style="display: none;" class="please-wait">Wait...</span>
            <span class="field-validation-valid" data-valmsg-for="Email" data-valmsg-replace="true"></span>
        </div>
        <div class="newsletter-result" id="newsletter-result-block"></div>
    </div>
</div>

<script type="text/javascript">
        function newsletter_subscribe(subscribe) {
            var subscribeProgress = $("#subscribe-loading-progress");
            subscribeProgress.show();
            

            if (!($("#newsletter-email").val())) {
                alert('Please enter email address');
                $('#newsletter-email').val('');
                $('#newsletter-email').focus();
                subscribeProgress.hide();
                return true;
            }

            if (!validateEmail($("#newsletter-email").val())) {
                alert('Please enter valid email address');
                $('#newsletter-email').val('');
                subscribeProgress.hide();
                return true;
            }

            var postData = {
                Email: $("#newsletter-email").val()
            };
            $.ajax({
                cache: false,
                type: "POST",
                url: "/InsiderEmailAlert",
                data: postData,
            success: function (data) {
                subscribeProgress.hide();
                window.location.href = "/ConfirmInsiderEmailAlert";
            },
            error: function (xhr, ajaxOptions, thrownError) {
                $('#newsletter-result-block').text('Failed to subscribe.');
                $('#newsletter-result-block').show();
                subscribeProgress.hide();
            }
        });
        }

        $(document).ready(function () {
            $('#newsletter-subscribe-button').click(function () {
                newsletter_subscribe('true');
            });
            $("#newsletter-email").keydown(function (event) {
                if (event.keyCode == 13) {
                    $("#newsletter-subscribe-button").click();
                    return false;
                }
            });
        });

        function validateEmail($email) {
            var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
            return filter.test($email);
        }
</script>
