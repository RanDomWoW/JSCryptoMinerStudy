<script>
    $(function () {
        
        //Check whether the latitude and longitude saved in cookie or not
        if (typeof $.cookie('fdlatitude') === 'undefined' || typeof $.cookie('fdlongitude') === 'undefined') {;
            if ("ip" == "ip")
                getLatLongFromDB();
            else
                getLocation();

            //Default location is brampton (L6P 0A1)
            //$('#storeDropDown').text(add3Dots("Brampton", 17));
            getLatLong('L1S 2J1', true);
        } else {
            if ($.cookie('fdlatitude') != '' && $.cookie('fdlongitude') != '') {
                getWareHouseList($.cookie('fdlatitude'), $.cookie('fdlongitude'), true);
            } else {
                getLocation();

                //Default location is brampton (L6P 0A1)
                //$('#storeDropDown').text(add3Dots("Brampton", 17));
                getLatLong('L1S 2J1', true);
            }
        }

        $('#storeDropDown').click(function () {
            $('#txtHeaderPostalCode').val('');
            $('#spnErrMessage').text('');
            $('#postalCodeContainer').slideToggle();
        });

        $('#btnHeaderPostalCode').click(function () {
            getLatLongByPincode();
            $('.hideStore').hide();
        });

        $('#txtHeaderPostalCode').keypress(function (e) {
            if (e.which == 13) {//Enter key pressed
                getLatLongByPincode();
                $('.hideStore').hide();
            }
        });

        $('#listHeaderAllStores').click(function () {
            $('#spnErrMessage').text('');
            $('.hideStore').slideToggle();
            $(this).hide();
        });

        $("#storeDropDownList").on("click", "li > a", function () {
            var id = $(this).attr('data-id');
            var name = $(this).attr('data-name');
            var lat = $(this).attr('data-lattitude');
            var lng = $(this).attr('data-longitude');

            $('#storeDropDown').text(add3Dots(name, 17));
            $.cookie('fdlatitude', lat, { expires: 9999 });
            $.cookie('fdlongitude', lng, { expires: 9999 });

            $('#postalCodeContainer').slideToggle();
            location.reload();
        });

        $('body').click(function (evt) {
            if (evt.target.id == "locationMenu")
                return;
            //For descendants of menu_content being clicked, remove this check if you do not want to put constraint on descendants.
            if ($(evt.target).closest('#locationMenu').length)
                return;

            //Do processing of click event here for every element except with id menu_content
            $('#postalCodeContainer').hide();
        });
    });

    function getLocation() {
        if (navigator.geolocation) {
            navigator.geolocation.getCurrentPosition(showPosition, showError);
        } else {
            getLatLongFromDB();
            console.log("Geolocation is not supported by this browser.");
        }
    }

    function showPosition(position) {
        $.cookie('fdlatitude', position.coords.latitude, { expires: 9999 });
        $.cookie('fdlongitude', position.coords.longitude, { expires: 9999 });
    }

    function showError(error) {
        getLatLongFromDB();
        //switch (error.code) {
        //    case error.PERMISSION_DENIED:
        //        console.log("User denied the request for Geolocation.");
        //        break;
        //    case error.POSITION_UNAVAILABLE:
        //        console.log("Location information is unavailable.");
        //        break;
        //    case error.TIMEOUT:
        //        console.log("The request to get user location timed out.");
        //        break;
        //    case error.UNKNOWN_ERROR:
        //        console.log("An unknown error occurred.");
        //        break;
        //}
    }

    function getLatLongByPincode() {
        var postalCode = $('#txtHeaderPostalCode').val();
        getLatLong(postalCode, false);
    }

    function getLatLong(postalCode, selected) {
        $('#spnErrMessage').text('');
        if (postalCode.trim() != '') {
            $.ajax({
                cache: false,
                type: 'POST',
                url: '/WareHouse/GetLocation',
                headers: { 'Cache-Control': 'max-age=180' },
                data: { zipPostalCode: postalCode },
                success: function (data) {
                    getWareHouseList(data.lat, data.lng, selected);

                    $.cookie('fdlatitude', data.lat, { expires: 9999 });
                    $.cookie('fdlongitude', data.lng, { expires: 9999 });
                }
            });
        } else {
            $('#spnErrMessage').text('Please enter a valid Postal Code.');
        }
    }

    function getLatLongFromDB() {
        $.ajax({
            type: 'POST',
            url: '/WareHouse/GetLatLongFromDB',
            headers: { 'Cache-Control': 'max-age=180' },
            success: function (data) {
                if (data.status) {
                    $.cookie('fdlatitude', data.lat, { expires: 9999 });
                    $.cookie('fdlongitude', data.lng, { expires: 9999 });
                }
                else { }
            }
        });
    }


    function getWareHouseList(lat, lng, selected) {
        var pid = $('#hdnProductId').val();
        $.ajax({
            cache: false,
            type: 'POST',
            url: '/WareHouse/GetAllWareHouseList',
            headers: { 'Cache-Control': 'max-age=180' },
            data: { 'lattitude': lat, 'longitude': lng },
            success: function (data) {
                $('#storeDropDownList').empty();
                var i = 0;
                var flag = false;
                for (var j in data.result) {
                    if (i == 0 && selected) {
                        $('#storeDropDown').text(add3Dots(data.result[j].Name, 17));
                    }
                    //Bind stores list
                    if (i < 4) {
                        $('#storeDropDownList').append('<li class="showStore"><a href="javascript:void(0);" data-id="' + data.result[j].Id + '" data-lattitude="' + data.result[j].Lattitude + '" data-longitude="' + data.result[j].Longitude + '" data-name="' + data.result[j].Name + '">' + data.result[j].Name + ' - ' + data.result[j].Address + '</a></li>');
                        flag = true;
                    } else {
                        $('#storeDropDownList').append('<li class="hideStore" style="display:none"><a href="javascript:void(0);" data-id="' + data.result[j].Id + '" data-lattitude="' + data.result[j].Lattitude + '" data-longitude="' + data.result[j].Longitude + '" data-name="' + data.result[j].Name + '">' + data.result[j].Name + ' - ' + data.result[j].Address + '</a></li>');
                    }
                    i++;
                }

                $.cookie('Warehouse.UserNearByStores', data.stores, { expires: 9999 });

                if (i == 0 || !flag) {
                    $('#spnErrMessage').text('No store(s) found.');
                }
                if (i > 4) {
                    $('#listHeaderAllStores').show();
                }
                $('#storeDropDownList').show();
            }
        });
    }

    function add3Dots(string, limit) {
        var dots = "...";
        if (string.length > limit) {
            string = string.substring(0, limit) + dots;
        }

        return string;
    }
</script>

