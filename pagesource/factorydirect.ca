<script type="text/javascript">
    function fileSelected() {
        var count = document.getElementById('barcodeToUpload').files.length;
        var file = document.getElementById('barcodeToUpload').files[0];
        var fileSize = 0;
        if (file.size > 1024 * 1024) {
            fileSize = (Math.round(file.size * 100 / (1024 * 1024)) / 100).toString() + 'MB';
            //resizeAndUpload(file);
        }
        else {
            fileSize = (Math.round(file.size * 100 / 1024) / 100).toString() + 'KB';
            //uploadFile();
        }
        uploadFile();
        //document.getElementById('details').innerHTML += 'Name: ' + file.name + '<br>Size: ' + fileSize + '<br>Type: ' + file.type;
        //document.getElementById('details').innerHTML += '<p>';
    }

    function uploadFile() {
        // Checking whether FormData is available in browser
        if (window.FormData !== undefined) {

            $(".barcodereaderloader").show();

            var fileUpload = $("#barcodeToUpload").get(0);
            var files = fileUpload.files;

            // Create FormData object
            var fileData = new FormData();
            fileData.append(files[0].name, files[0]);

            $.ajax({
                url: '/BarcodeScanner/UploadFile',
                type: "POST",
                contentType: false, // Not to set any content header
                processData: false, // Not to process data
                data: fileData,
                success: function (result) {
                    $(".barcodereaderloader").fadeOut("slow");
                    if (result == '-1' || result == '-2' || result != '') {
                        //-1: Invalid or corrupted file
                        //-2: exception caught
                    } else {
                        var redirectUrl = '/ProductSearch/Upcs?q=' + result;
                        window.location.href = redirectUrl;
                    }
                },
                error: function (err) {
                    $(".barcodereaderloader").fadeOut("slow");
                    //alert(err.statusText);
                }
            });
        } else {
            //FormData is not supported.
        }
    }

    function resizeAndUpload(file) {
        var reader = new FileReader();
        reader.onloadend = function () {
            EXIF.getData(file, function () {
                var orientation = EXIF.getTag(this, "Orientation");
                document.getElementById('fileDisplayArea').innerHTML += 'orientation: ' + orientation;
                document.getElementById('fileDisplayArea').innerHTML += '<p>';

                var canvas = document.createElement('canvas');
                var ctx = canvas.getContext("2d");
                var tempImg = new Image();
                tempImg.src = reader.result;
                tempImg.onload = function () {
                    var MAX_WIDTH = 450;
                    var MAX_HEIGHT = 550;
                    var tempW = tempImg.width;
                    var tempH = tempImg.height;
                    if (tempW > tempH) {
                        if (tempW > MAX_WIDTH) {
                            tempH *= MAX_WIDTH / tempW;
                            tempW = MAX_WIDTH;
                        }
                    } else {
                        if (tempH > MAX_HEIGHT) {
                            tempW *= MAX_HEIGHT / tempH;
                            tempH = MAX_HEIGHT;
                        }
                    }
                    canvas.width = tempW;
                    canvas.height = tempH;
                    ctx.save();

                    var width = canvas.width; var styleWidth = canvas.style.width;
                    var height = canvas.height; var styleHeight = canvas.style.height;

                    if (orientation) {
                        if (orientation > 4) {
                            canvas.width = height; canvas.style.width = styleHeight;
                            canvas.height = width; canvas.style.height = styleWidth;
                        }
                        switch (orientation) {
                            case 2: ctx.translate(width, 0); ctx.scale(-1, 1); break;
                            case 3: ctx.translate(width, height); ctx.rotate(Math.PI); break;
                            case 4: ctx.translate(0, height); ctx.scale(1, -1); break;
                            case 5: ctx.rotate(0.5 * Math.PI); ctx.scale(1, -1); break;
                            case 6: ctx.rotate(0.5 * Math.PI); ctx.translate(0, -height); break;
                            case 7: ctx.rotate(0.5 * Math.PI); ctx.translate(width, -height); ctx.scale(-1, 1); break;
                            case 8: ctx.rotate(-0.5 * Math.PI); ctx.translate(-width, 0); break;
                        }
                    }

                    ctx.drawImage(this, 0, 0, tempW, tempH);
                    ctx.restore();
                    var dataURL = canvas.toDataURL('image/png', 1.0);  //Full Quality: 1.0, Medium Quality: 0.5, Low Quality: 0.1
                    var image = dataURL.replace('data:image/png;base64,', '');

                    $.ajax({
                        type: 'POST',
                        url: '/BarcodeScanner/UploadImage',
                        data: '{ "imageData" : "' + image + '" }',
                        contentType: 'application/json; charset=utf-8',
                        dataType: 'json',
                        success: function (result) {
                            if (result == '-1' || result == '-2') {
                                //-1: Invalid or corrupted file
                                //-2: exception caught
                            } else {
                                var redirectUrl = '/ProductSearch/Upcs?q=' + String(result);
								document.getElementById('fileDisplayArea').innerHTML += 'result: ' + redirectUrl;
								document.getElementById('fileDisplayArea').innerHTML += '<p>';
                                //window.location.href = redirectUrl;
                            }
                        },
                        error: function (err) {
                            alert(err.statusText);
                        }
                    });
                }
                document.getElementById('imageresult').src = tempImg.src;//URL.createObjectURL(file);
            });
        }
        reader.readAsDataURL(file);
    }
    function searchSiteIOS(url, appUrl) {
        setTimeout(function () {
            // if pic2shop not installed yet, go to App Store
            window.location = appUrl;
        }, 25);
        try {
            window.location = url;
        } catch (e) {

        }
    }
</script>

