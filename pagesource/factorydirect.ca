<script type="text/javascript">
    function insider_alert_subscribe() {
        if (!($("#insider-popup-email").val())) {
            alert('Please enter email address');
            $('#insider-popup-email').val('');
            $('#insider-popup-email').focus();
            return true;
        }
        if (!validateEmailAddress($("#insider-popup-email").val())) {
            alert('Please enter valid email address');
            $('#insider-popup-email').val('');
            return true;
        }
        var subscribeProgress = $("#subscribe-loading-progress");
        $(".barcodereaderloader").show();
        var postData = {
            Email: $("#insider-popup-email").val(),
            Name: $("#insider-popup-Name").val(),
            Location: $("#insider-popup-Location").val(),
            PostalCode: $("#insider-popup-PostalCode").val(),
            Reference: $("#insider-popup-Reference").val(),
            SubscribeSource: $("#insider-popup-SubscribeSource").val(),
            SubscribeContact: $("#insider-popup-SubscribeContact").val()
        };
        $.ajax({
            cache: false,
            type: "POST",
            url: "/InsiderEmailAlert",
            data: postData,
            success: function (data) {
                $(".barcodereaderloader").show();
                window.location.href = "/ConfirmInsiderEmailAlert";
            },
            error: function (xhr, ajaxOptions, thrownError) {
                $('#insider-popup-result-block').text('Failed to subscribe.');
                $('#insider-popup-result-block').show();
                $(".barcodereaderloader").hide();
            }
        });
    }
    $(document).ready(function () {
        $('#insider-popup-button').click(function () {
            insider_alert_subscribe();
        });
        $('#insider-popup-email').addClass('margin-textbox');
        $('#insider-popup-Name').addClass('margin-textbox');
        $('#insider-popup-Location').addClass('margin-textbox');
        $('#insider-popup-PostalCode').addClass('margin-textbox');
        $('#insider-popup-Reference').addClass('margin-textbox');
        $('#insider-popup-SubscribeSource').addClass('margin-textbox');
        $('#insider-popup-SubscribeContact').addClass('margin-textbox');
    });

    function validateEmailAddress($email) {
        var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
        return filter.test($email);
    }
