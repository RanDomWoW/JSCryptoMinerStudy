<script type="text/template" id="search-product-card-tpl">
    <a href="/product/<%- slug %>" title="<%- title %>">
        <article class="card card--lite">
            <div class="card__image-container">
                <picture>
                    <img src="<%- image %>?w=150&h=150&auto=format"
                         alt="<%- altText %>"
                         class="search-card__img"
                         width="150"
                         height="150">
                </picture>
            </div>
            <div class="card__category-footer">
                <h6 class="card__title">
                    <%- title %>
                </h6>

                <div class="prices">
                    <span class="price price--reduced"><%= price %></span> <span class="price__suffix"><%- priceSuffix %></span>
                </div>
                <% if(rating > 0) {%>
                    <div class="review-stars">
                        <span class="review-star icon-glyph"><%= stars %></span>
                        <span class="text text--v5 review-star__count">(<%- reviewCount %>)</span>
                    </div>
                <% } %>
            </div>
        </article>
    </a>
</script>
<script type="text/template" id="search-article-card-tpl">
    <a href="/blog/posts/<%- slug %>" title="<%- title %>">
        <article class="card mini-article-card">
            <picture>
                <img
                    class="mini-article-card__image"
                    src="<%- image %>?w=300&h=300&fit=crop&auto=format"
                    alt="<%- title %>"
                    width="150" height="150">
            </picture>

             <span class="mini-article-card__category">
                <span class="mini-article-card__category-icon">
                    <svg version="1.1" xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16">
                        <title>guide</title>
                        <path fill="#444" d="M14 16h-12c-1.1 0-2-0.9-2-2v-12c0-1.1 0.9-2 2-2h12c1.1 0 2 0.9 2 2v12c0 1.1-0.9 2-2 2zM2 2v12h12v-12h-12z"></path>
                        <path fill="#444" d="M4 5h8v2h-8v-2z"></path>
                        <path fill="#444" d="M4 9h5v2h-5v-2z"></path>
                    </svg>
                </span>
                 <span><%- category %></span>
            </span>

            <h1 class="mini-article-card__title"><%- title %></h1>

            <footer class="mini-article-card__footer">
                <span class="mini-article-card__date"><%- published %></span>
                <% if (typeof(comments) !== 'undefined') { %>
                    <span class="mini-article-card__comments"><%- comments %></span>
                <% }%>
            </footer>
        </article>
    </a>
</script>
