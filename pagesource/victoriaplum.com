                    <div class="search-list__title">
                        <a href="/search?query=<%- suggestion.formatted %>" class="quick-search__suggestion" title="<%= suggestion.formatted %>">
                            <span><%= suggestion.unformatted %></span>
                        </a>
                    </div>

                    <div class="search-list__autofill i-paste-search" data-paste-term="<%= suggestion.formatted %>">
                        <div class="icon-glyph search-list__autofill-icon">
                            <svg version="1.1" xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16">
                                <title>arrow-diagonal</title>
                                <path d="M4.182 2.343v1.98h6.081l-8.627 8.627 1.414 1.414 8.627-8.627v6.081h1.98v-9.475z"></path>
                            </svg>
                        </div>
                    </div>
                </li>
            <% }); %>
        </ul>
    <% } %>
</script>
<script type="text/template" id="search-exact-match-tpl">
    <div class="search-panel__exact-match">
        <h2 class="search-panel__heading">We've found an exact match</h2>
        <div class="quick-search__match">
            <%= match %>
        </div>
    </div>
</script>
