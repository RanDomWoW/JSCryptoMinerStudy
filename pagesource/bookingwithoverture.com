    <script>
      $(document).ready(function(){
        // initalize fitvids
        $("#video").fitVids();
        // stop video on modal close
        $('.modal').each(function(){
          var src = $(this).find('iframe').attr('src');
          $(this).on('click', function(){
              $(this).find('iframe').attr('src', '');
              $(this).find('iframe').attr('src', src);
          });
        });
      });
    </script>

