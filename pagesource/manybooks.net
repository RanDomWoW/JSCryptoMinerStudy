<script type="text/javascript">
  var startTime = new Date(2013,1,1);
  var outputSpan = document.getElementById('downloadCount');
  setInterval(function () {
    outputSpan.innerHTML = addCommas((40000000 + ((new Date() - startTime)/1000)).toFixed());
  }, 2450);

  function addCommas(nStr)
  {
	nStr += '';
	x = nStr.split('.');
	x1 = x[0];
	x2 = x.length > 1 ? '.' + x[1] : '';
	var rgx = /(\d+)(\d{3})/;
	while (rgx.test(x1)) {
		x1 = x1.replace(rgx, '$1' + ',' + '$2');
	}
	return x1 + x2;
  }
