                function startTime() {
                    var today = new Date();
                    var dayNames = ['Даваа','Мягмар','Лхагва','Пүрэв','Баасан','Бямба','Ням' ];
                    var y = today.getFullYear();
                    var mo = today.getMonth() + 1;
                    var d = today.getDate();
                    var h = today.getHours();
                    var days = dayNames[today.getDay()-1];
                    h = checkTime(h);
