                    newsLoading = true;
                    console.log(Number($moreNewsTarget.find('div.event__item:last-child').attr('data-page')) + 1);
                    if (newsPage > Number($moreNewsTarget.find('div.event__item:last-child').attr('data-page')) + 1) {
                        newsEnd = true;
                        $('#refresher').remove();
                    }
                    if (!newsEnd) {
                        $.get('/a/list?size=5&page=' + newsPage, function (data) {
                            if (newsPage === 1) {
                                $moreNewsTarget.html(data);
                            }
                            else {
                                $moreNewsTarget.append(data);
                            }
                            newsPage++;
                        });
                    }
        }
            if(widthScreen < 720) {
                loadMoreNews();
                window.onscroll = throttle(function (event) {
                    console.log(1);
                    if ((window.innerHeight + window.pageYOffset) >= document.body.offsetHeight - 300) {
                        loadMoreNews();
                    }
                }, 500);
            }
        </script>

    </div>
</div>




