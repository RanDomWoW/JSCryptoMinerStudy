<script type="text/javascript">

    var canLoad = true

    function page_ajax_get(){
      var page = jQuery('.paged').last().attr('data-paged') || 2;
      var cat = null;
      var ajaxurl = 'http://www.300bar.com/wp/wp-admin/admin-ajax.php';

      canLoad = false

      jQuery.ajax({
