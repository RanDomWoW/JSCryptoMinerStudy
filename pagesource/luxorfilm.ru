        <script type="text/javascript">
            $(function () {
                function InitSlider() {
                    var hfMin = $("#hfMin");
                    var hfMax = $("#hfMax");

                    var slider = $("#ctl00_contentPlaceHolder_ctl00_DateSlider").slider({
                        range: true,
                        min: 9, // минимальный час
                        max: 26, // максимальный час
                        values: [hfMin.val(), hfMax.val()], // текущие выбранне значение инициализируем из хиденфилдов
                        slide: function (event, ui) {
                            if ((ui.values[1] - ui.values[0]) < 1) {
                                // Не даем инвертировать выбранные диапазон
                                return false;
                            }
                            // Запоминаем выбранные значение в хидены, что бы считывать их в постбеках
                            hfMin.val(ui.values[0]);
                            hfMax.val(ui.values[1]);

                            if (false)
                                __doPostBack('ctl00$contentPlaceHolder$ctl00$DateSlider');
                        }
                    });
                };

                // При загрузке страницы инициализируем слайдер
                InitSlider();

                // Для переинициализации скрипта после постбека
                var prm = Sys.WebForms.PageRequestManager.getInstance();
                prm.add_endRequest(InitSlider);
            });
        </script>
