<script>
    $(document).ready(function () {
        initBanners();
        initColorbox();
        initSlider();
        initGrid();
    });

    function initBanners() {
        $('.bposter').each(function () {
            var $el = $(this);
            var $bUrl = $el.find('table a')
            if ($bUrl.length)
                $el.find('.border-hover a').attr('href', $bUrl.attr('href'));
            else
                $el.remove();
        });

        var $grid = $('#grid');
        var $promo = $grid.find('li.promo');
        var $slider = $grid.find('li.slider-wrapper');
        var $banners = $grid.find('li.extra');
        var placeCount = $grid.find('li.standart-block:not(.extra)').length;

        // Если на странице есть промофильм, то он в 4 раза больше обычного 
        // постера
        if ($promo.length)
            placeCount += 3;

        // Если на странице есть слайдер, то он в 2 раза больше обычного 
        // постера
        if ($slider.length)
            placeCount += 1;

        // Показываем только тот баннер который влезает в свобоное место 
        // в конце списка баннеров
        var emptyPlaces = (6 - placeCount % 6);
        emptyPlaces = emptyPlaces === 6 ? 0 : emptyPlaces;
        $banners.filter('.extra:not([data-size=' + emptyPlaces + '])').remove();
    };

    function initColorbox() {
        $(".trailer-link").colorbox({
            iframe: true,
            close: "Закрыть",
            innerWidth: window.innerWidth * (45.3 / 100),
            innerHeight: window.innerWidth * (28 / 100)
        });
        $(".buy-ticket").colorbox({
            iframe: true,
            scrolling: false,
            close: "Закрыть",
            //innerWidth: window.innerWidth * (69.68 / 100),
            //innerHeight: window.innerWidth * (53.34 / 100)
            innerWidth: window.innerWidth * 0.6,
            innerHeight: window.innerWidth * 0.6 * (3 / 4)
        });
    };

    function initSlider() {
        $('.bxslider').bxSlider({
            auto: true,
            touchEnabled: false
        });
    };

    function initGrid() {
        heightResize();
        $(window).resize(heightResize);

        var $grid = $('#grid');
        var $promo = $grid.find('li.promo');
        var $slider = $grid.find('li.slider-wrapper');
        var $banner1 = $grid.find('li.b1');
        var $banner2 = $grid.find('li.b2');
        var $items = $grid.find('li.standart-block');
        var liCount = $items.length;

        var i = 1;
        $items.each(function () {
            if (($promo.length && liCount < 12 && i == 2) ||
                ($promo.length && liCount >= 12 && i == 8) ||
                ($slider.length && liCount < 12 && i == 8) ||
                ($slider.length && liCount >= 12 && i == 10))
                i++

            if (($banner1.length && i == 3) ||
                ($banner2.length && i == 11))
                i++

            $(this).data('foo', i++);
        });

        // Задаем расположение блоков с промофильмом, слайдером и баннерами
        // в зависимости от общего кол-ва элементов
        if ($promo.length)
            $promo.data('foo', liCount < 12 ? 2 : 8);
        if ($slider.length)
            $slider.data('foo', liCount < 12 ? 8 : 10);
        if ($banner1.length)
            $banner1.data('foo', 3);
        if ($banner2.length)
            $banner2.data('foo', 11);

        $grid.dylay({
            speed: 100,
            easing: '',
            selector: '>li'
        });

        $grid.dylay('sort', 'foo', 'asc');
    };

    function heightResize() {
        var $elms = $('#grid > li.standart-block');

        var $film = $elms.filter('.film');
        var $slider = $elms.filter('.slider-wrapper');
        var $promo = $elms.filter('.promo');
        var $banner = $elms.filter('.extra');

        var $hover = $elms.find('.border-hover');
        var $promoHover = $promo.find('.border-hover');
        var $bannerHover = $banner.find('.border-hover');

        var stdw = $film.length ? $film.width() : $slider.width();
        var sliderw = $film.length ? stdw * 2 : stdw;
        var promow = stdw * 2;
        var bannerw = stdw * +$banner.data('size');

        var stdh = $film.length ? Math.round(stdw * 1.2) : 380;
        var promoh = stdh * 2;
        var sliderh = stdh;
        var bannerh = stdh;

        $film.height(stdh);
        $promo.height(promoh);
        $promo.width(promow);
        $slider.height(sliderh);
        $slider.width(sliderw);
        $banner.height(bannerh);
        $banner.width(bannerw);

        $hover.height(stdh - 8);
        $hover.width(stdw - 8);

        $promoHover.height(promoh - 8);
        $promoHover.width(promow - 8);

        $bannerHover.height(bannerh - 8);
        $bannerHover.width(bannerw - 8);
    };
</script>

