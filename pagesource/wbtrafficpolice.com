                openSpeed: 150,
                closeEffect: 'elastic',
                closeSpeed: 150,
                closeBtn: true,
                arrows: true,
                nextClick: true,
                autoScale: true,
                type: 'image'
            });
                            $('.prev1').insertBefore('.topslider');
                $('.next1').insertAfter('.topslider');
                var owlBios = $('.topslider');
                owlBios.owlCarousel({
                    loop: true,
                    margin: 15,
                    nav: false,
                    dots: false,
                    lazyLoad: true,
                    animateOut: 'fadeOut 0.1s',
                    activeClass: 'activet',
                    autoplay: true,
                    autoplayTimeout: 5000,
                    autoplayHoverPause: false,
                    items: 1,
                    singleItem: true
                });
                $('.prev1').click(function () {
                    owlBios.trigger('prev.owl.carousel', [700]);
                });
                $('.next1').click(function () {
                    owlBios.trigger('next.owl.carousel', [700]);
                });
                $('.item').removeAttr('style');
                            var type = window.location.hash.substr(1);
            if (type == '') {
                var rht = $("#rytPartWeb").height();
                var lht = $("#lftPartWeb").height();
                if (lht > rht) {
                    $("#rytPartWeb").css('min-height', (lht - 20));
