#main-text {
    position: relative;
    margin: 40px 5px 0 5px;
    padding: 0 60px 10px;
    font-family: roboto;
    font-size: 12px;
    color: #777;
    background-color: #fff;
    line-height: 1.9em;
    border-radius: 4px;
    border: 1px solid #ddd;
}

#main-text h1, .h1 {
    font-size: 20px;
    display: block;
    color: #777;
    text-align: center;
    width: 100%;
    padding-top: 20px;
}

#main-text .col {
    padding: 15px 0;
    border-radius: 4px;
    margin: 10px;
}

#main-text ul {
    /*background-color: #EBEBEB;*/
    padding: 0 20px;
    border-radius: 20px;
    margin: 10px 90px 10px 20px;
}

#main-text ul li {
    margin: 0 0 6px 20px;
    font-size: 11px;
    line-height: 1.7em;
}

#main-text img {
    margin: 10px;
}

#main-text p {
    margin: 10px 20px 0 10px;
}

#main-text blockquote {
    background: #fff;
    font-family: serif;
    padding: 0 20px 15px 45px;
    margin: 1em auto .3em;
    position: relative;
    max-width: 80%;
    min-width: 250px;
    font-size: 20px;
    line-height: 1.2;
    text-align: justify;
    border-left: 15px solid #DDDDDD;
    border-right: 2px solid #DDDDDD;
}

#main-text blockquote::before {
    content: "\201C";
    /*Unicode for Left Double Quote*/
    font-family: Georgia, serif;
    font-size: 60px;
    font-weight: bold;
    color: #999;
    position: absolute;
    left: 10px;
    top: 5px;
}

#main-text blockquote::after {
    /*Reset to make sure*/
    content: "";
}

#main-text .pic {
    float: right;
    margin: 20px 20px 35px 40px;
    padding: 15px;
    background-color: #fff;
    border-radius: 2px;
    border: 1px solid #ddd;
}


@media (max-width: 991px) {
    #main-text {
        padding: 1em;
    }
    #main-text .col {
        padding: 15px;
        margin-left: 15px;
        margin-right: 15px;
    }
    #main-text .pic {
        width: 100%;
        margin: 20px auto;
        text-align: center;
    }
    #main-text .pic img {
        margin: 5px;
        float: none;
        position: relative;
    }
    #main-text ul {
        margin: 10px;
    }
}

@media (max-width: 767px) {
    #main-text ul {
        margin: 0;
        padding: 10px;
    }
}</style>
    

