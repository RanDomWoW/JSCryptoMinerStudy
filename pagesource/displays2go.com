.featured-cat {
	border-radius: 4px; 
	border: 1px solid #ddd;
	background: rgba(223,227,230,0.5);
	margin-bottom: 15px;
}
.featured-cat img {
	border-radius: 4px 4px 0 0;
}
.featured-cat > div {
	position: relative;
}
.featured-cat span {
	position: absolute; 
	bottom: 0; 
	left: 0; 
	right: 0; 
	width: 100%; 
	background: rgba(255, 255, 255, 0.9); 
	display: block; 
	padding: 10px;
	font-weight: bold;
	font-size: 16px;
}
.featured-cat ul:after {
	content: '';
	display: block;
	clear: both;
}
.featured-cat ul > li {
	width: 50%;
	float: left;
}
	.featured-cat ul > li:nth-child(even):after {
		content: '';
		display: block;
		clear: both;
	}
.featured-cat ul > li > a {
	display: inline-block;
	padding: 5px 0;
