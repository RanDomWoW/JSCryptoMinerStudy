      app.service('StatsService', function ($http) {
          this.getStats = function() {
              return $http({
                      method: 'GET',
                      url: "/api/landingPageStats/getLandingPageStats",
                      headers: {'Content-Type': 'application/x-www-form-urlencoded'}
                  })
                  .success(function (data, status) {
                      return data;
                  })
          }
      });



      app.controller('StatsCtrl', function ($scope, StatsService,$timeout) {

          StatsService.getStats()
          .then(function(result){
              $scope.statsObj = result.data;
              $timeout(function() {
                  animateNumbers();
              },0);
          })
      });

      app.controller('FavoriteSalonsCtrl', function ($scope, FavoriteUserSalonService, User,$timeout) {

        var loadFavSalons = function(){
            $scope.loadingFavSalonPromise = FavoriteUserSalonService.getFavoriteSalons()
            .then(function(response) {
                $scope.favoriteSalons = response.data;
            })
        };

        User.loadUser()
        .then(function() {
            var user = User.getLoadedUserObj()
            if (!user) {
                return;
            };
            $scope.userImgPath = User.getUserProfileImgPath(user);
       });

        loadFavSalons();

        $scope.getSalonImgPath = function(salon){
            if (salon && salon.imgID) {
                return '/data/'+salon.sid+'/coverImage/'+salon.imgID;
            } else {
                return '/data/placeholders/coverImage/placeholder.png';
            }
        }

        events.subscribe('/login', function() {
            loadFavSalons();
        });


        $scope.$on('favorieSalonRemoved', function(event, args) {
            $scope.favoriteSalons = _.filter($scope.favoriteSalons, function(salon){
                return salon.sid != args.sid;
            })
        });


    });



