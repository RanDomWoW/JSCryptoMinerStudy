    <!-- Fix ios11 bug on input cursor issue inside bootstrap modals -->
    <script type="text/javascript">
        var scrollTopDiff = 0;

        var selectedCategoryLink;


        $(document).on('show.bs.modal','.userAuthenticationModal', function() {
             scrollTopDiff = $('body').scrollTop();
             $(".body-content").fadeOut(300);
        })

        $(document).on('hide.bs.modal','.userAuthenticationModal', function() {
            $(".body-content").show();
            $(".body-content").css('opacity','.5');
            $('body').scrollTop(scrollTopDiff);
            $(".body-content").animate({
                 opacity: 1
             }, 500, function() {
                 // Animation complete.
             });
        });


        $(document).on('shown.bs.modal','#searchSalonModal', function() {
            $("body").addClass("search-on-mobile-open");
            $(".salonSearchInput").focus();
        });

        $(document).on('hide.bs.modal','#searchSalonModal', function() {
            $("body").removeClass("search-on-mobile-open");
        });


        var closeCatSelection = function() {
            $(".closeCatBtn").remove();
            var children=$('#salonTypeContainer').children();
            $.each(children, function( index, value ) {
                $(value).transition({ x: 0+'px', y: 0+'px', scale: 1, rotateX: '0deg' }, 600, 'snap');
                $(value).css("transform","");
                $(value).css('zIndex',0);
            });
            $('.categoryContainer').removeClass('isOpen');
            $(".categoryImageCont").removeClass("selected");
        };

        var reDrawCategoryTilePositions = function(animationDuration) {
            // var offset = +4;

            var animationDuration = animationDuration || 400;
            var children=$('#salonTypeContainer').children();

            var clickedElement = $(".categoryImageCont.selected");

            $.each(children, function( index, value ) {

                var left;
                var top;
                var scaleFactor = 1.5;
                if ($(window).width() < 767) {
                    var topOffset;
                    if ($(window).width() < 460) {
                        scaleFactor = 1.7;
                        topOffset = 20;
                    } else {
                        scaleFactor = 1.2;
                        topOffset = 0;
                    }
                    if (index <= 1) {
                        top = topOffset;
                    } else if (index <= 3) {
                        top = -$(value).height() + topOffset ;
                    } else if (index <= 5) {
                        top = - 2*($(value).height()) + topOffset;
                    } else if (index <= 7) {
                        top = - 3*($(value).height()) + topOffset;
                    };

                    left = (($("#salonTypeContainer").width() - $(value).width()) / 2);
                } else if ($(window).width() <= 992) {
                    if (index <= 1) {
                        top = 0;
                    } else if (index <= 3) {
                        top = -$(value).height() ;
                    } else if (index <= 5) {
                        top = - 2*($(value).height());
                    } else if (index <= 7) {
                        top = - 3*($(value).height());
                    };

                    scaleFactor = 1.2;

                    left = (($("#salonTypeContainer").width() - $(value).width()) / 2) - 15;
                } else {
                    if (index <= 3) {
                        top = $(value).find(".containerWithCloseBtn").innerHeight() / 4;
                    } else {
                        top = -$(value).find(".containerWithCloseBtn").innerHeight();
                    };
                    left = - ($(value).position().left) + $(value).width()/2.7 * scaleFactor;
                }

                selectedCategoryLink = $(clickedElement).find(".overlay").attr('href');

                if ($(value).find( ".categoryImageCont" )[0] == $(clickedElement)[0]) {
                    $(value).css('zIndex',100);
                    if ($(window).width() < 460) {
                        left = left * scaleFactor / 2;
                    }
                    if ($(window).width() <= 992){
                        if ($(value).position().left) {
                            left = left - $(value).position().left + 12
                        };
                    };

                    $(value).transition({ x: left+'px', y: top+'px', scale: scaleFactor, rotateX: '0deg' }, animationDuration, 'snap');

                    $(clickedElement).closest(".containerWithCloseBtn").append('<div class="closeCatBtn"><i class="fa fa-times" aria-hidden="true"></i></div>');

                } else {
                    if ($(window).width() <= 992){
                        left = left - $(value).position().left;
                    }
                    $(value).transition({ x: left+'px', y: top+'px', rotateX: '0deg' }, animationDuration, 'snap');
                }

            });
        }

        var triggerCategoryAnimation = function(clickedElement) {
            if ($('.categoryContainer').hasClass('isOpen')) {
                closeCatSelection();
            } else {
                $('.categoryContainer').addClass('isOpen');
                $(clickedElement).addClass("selected");

                if ($(window).width() <= 992) {
                    $('body, html').animate({
                        scrollTop: $(".selectionContainerInside").offset().top - 120
                    }, 300);
                } else {
                    $('body, html').animate({
                        scrollTop: $(".selectionContainerInside").offset().top - 190
                    }, 300);
                }
                reDrawCategoryTilePositions();
            };
        };


        $(window).on('resize', _.debounce(function(){
            if ($('.categoryContainer').hasClass('isOpen')) {
                var children=$('#salonTypeContainer').children();
                $.each(children, function( index, value ) {
                    $(value).transition({ x: 0+'px', y: 0+'px', scale: 1, rotateX: '0deg' }, 0, 'snap');
                    $(value).css("transform","");
                    $(value).css('zIndex',0);
                });
                reDrawCategoryTilePositions(0);
            }
        }, 400));


        $(document).on('click touch', '.closeCatBtn', function(){
            closeCatSelection();
        });


        $('.categoryImageCont:not(.noAnimation)').click(function (event) {
            event.preventDefault();
            triggerCategoryAnimation(this);
        });

        $(document).on('click touchend','body',function(ev){
            if (!ev.target.closest('.multiSelect') && $(".checkboxLayer").hasClass("show")) {
                setTimeout(function () {
                    $(".helperContainer .inputFilter").blur();
                    $(".checkboxLayer.show").removeClass("show");
                }, 200);
            };
        });



    </script>

