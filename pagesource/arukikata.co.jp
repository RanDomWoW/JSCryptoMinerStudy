jQuery(function() {
	jQuery('.guidebook_form').submit(function(e) {
		var domain = location.hostname;
		var guidebook_site = 'https://www.arukikata.co.jp/guidebook/';
		if(domain.match(/^m((\.|-).+)?\.arukikata\.co\.jp/))
			guidebook_site = 'https://m.arukikata.co.jp/guidebook/';
		$(this).attr('action', guidebook_site + 'search');
		
		if(!(jQuery(this).find('.book_country option:selected').val())) {
			if(jQuery(this).find('.book_area option:selected').val() == 'series')
				alert('シリーズを選択して下さい');
			else
				location.href = guidebook_site + jQuery(this).find('.book_area option:selected').val() + '/';
			return false;
		}
		
		if(jQuery(this).find('.book_area option:selected').val() == 'series') {
			location.href = guidebook_site + 'series/' + jQuery(this).find('.book_country option:selected').val() + '/';
			return false;
		}
		
		jQuery('.book_area').prop('disabled', true);
	});
	
	jQuery('.book_area').change(function(e) {
		var option;
		if(jQuery(this).val() == 'series') {
			option = ['<option value="">シリーズを選ぶ（必須）</option>'];
			Array.prototype.push.apply(option, 
				jQuery.map(guidebookform.series, function(value, key) {
					return '<option value="' + key + '">' + value + '</option>';
				})
			);
		}else if(jQuery(this).val() == 'japan') {
			option = ['<option value="">都道府県で絞り込む（任意）</option>'];
			Array.prototype.push.apply(option, 
				jQuery.map(guidebookform.prefectures, function(value, key) {
					return '<option value="' + key + '">' + value + '</option>';
				})
			);
			jQuery(this).parents('form').find('.book_country').attr('name', 'prefecture');
		}else if(jQuery(this).val()) {
			option = ['<option value="">国で絞り込む（任意）</option>'];
			Array.prototype.push.apply(option, 
				jQuery.map(guidebookform.countries[jQuery(this).val()], function(value, key) {
					return '<option value="' + key + '">' + value + '</option>';
				})
			);
			jQuery(this).parents('form').find('.book_country').attr('name', 'country');
		}
		jQuery(this).parents('form').find('.book_country').html(option.join(''));
	});
	
	jQuery('.book_area').change();
});
