        function count () {
          if ($timer.hasClass('hidden')) {
            $timer.hide().removeClass('hidden').fadeIn();
          }
          var currTime = parseInt(initSeconds - ((new Date().getTime()) / 1000));
          var daysLeft = parseInt(currTime/86400);
          var hoursLeft = parseInt(currTime/3600); 
          var minutsLeft = parseInt(currTime/60);
          var secondsLeft = parseInt(currTime);
          
          seconds = minutsLeft*60;
          seconds = secondsLeft-seconds;
          if (seconds < 0) {
            seconds = 0;
          }   
          
          minutes = hoursLeft*60;
          minutes = minutsLeft-minutes;
          if (minutes < 0) {
            minutes = 0;
          }
          
          hours = daysLeft*24;
          hours = hoursLeft-hours;
          if (hours < 0) {
            hours = 0;
          }
          
          if (daysLeft < 0) {
            daysLeft = 0;
          }
          
          if (currTime <= 0) {
            clearInterval(timer);
          }
          
          writeTime($minute, minutes);
          writeTime($hour, hours);
          
          
          if (daysLeft == 0) {
            writeTime($sec, seconds);
            $day.hide();
            $sec.show();
          } else {
            writeTime($day, daysLeft);
            $day.show();
            $sec.hide();
          }
        }
        
        var timer = setInterval(count, 1000);
        
        function writeTime ($elem, time) {
          if (time >= 10) {
            $elem.find('.time').text(time);
