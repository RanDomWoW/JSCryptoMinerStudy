    <script>
      function svg_mouseover(rgn) {
        $('.sa_SVGMap').data('rgn', rgn);
        $('.sa_SVGMap').each(function (){
          if ($(this).data('initialized') === true) {
            return;
          };
          $(this).data('initialized', true);
          $(this).tooltip({
            items: 'path',
            track: true,
            show: false,
            hide: false,
            content: function () {
              var $this = $(this);
              if ($this.closest('.emotionImage').length > 0 && !$this.closest('.emotionImage').hasClass('active')) {
                return "";
              }
              if ($this.attr('title')) {
                if ($this.attr('title') === 'X') {
                  return "";
                }
                return $this.attr('title');
              } else {
                var rgn = $('.sa_SVGMap').data('rgn');
                var len = rgn.length;
                var systemcode = rgn.substr(len - 2, len);
                if (systemcode === 'at' || systemcode === 'sy') {
                  systemcode = 'at';
                }
                $.ajax('/' + systemcode + ''+'/'+'uptodate'+'/'+'guide'+'/'+'ajax'+'/'+'5,de'+'/'+'_'+'/'+'objectname.jspon', {
                  data: {
                    objectId : rgn
                  },
                  async: false,
                  dataType: 'json',
                  success: function (data) {
                    $this.attr('title', data.name);
                  }, 
                  error: function () {
                    $this.attr('title','X');
                  }
                });
                if ($this.attr('title') === 'X') {
                  return "";
                }
                return $this.attr('title');
              }
            }
          });
        });
        
        $('#link' + rgn).addClass('hover').parent().addClass('hover');
        if (rgn === 'RGN16at') {
          $('.sa_SVGMap .tirol').attr('class', 'austria hoverable tirol hover');
        }
        if (rgn === 'RGN104ch') {
          $('.sa_SVGMap .eastswitzerland').attr('class', 'switzerland hoverable eastswitzerland hover');
        }
      }
      
      function svg_mouseout(rgn) {
        $('#link' + rgn).removeClass('hover').parent().removeClass('hover');
        if (rgn === 'RGN16at') {
          $('.sa_SVGMap .tirol').attr('class', 'austria hoverable tirol');
        }
        if (rgn === 'RGN104ch') {
          $('.sa_SVGMap .eastswitzerland').attr('class', 'switzerland hoverable eastswitzerland');
        }
      }
      
      function svg_click(rgn, lnk) {
        if (typeof trackClickMDD === 'function') {
          trackClickMDD('dest', 'map', rgn);
        }
        if (lnk) {
          window.location = lnk;
          return false;
        }
        return true;
      }
    </script>
