        <script>
            var productSliderData = [
                                {
                    index: 1,
                    position: 0,
                    price: '19.9',
                    // url: 'https://shop.tele2.at/wk/shop/residential/privat-complete/kundendaten.html?productId=10015'
                    url: '/privat/internet/internet-und-telefon'
                },
                                {
                    index: 2,
                    position: 50,
                    price: '24.9',
                    // url: 'https://shop.tele2.at/wk/shop/residential/privat-complete/kundendaten.html?productId=20008'
                    url: '/privat/internet/internet-und-telefon'
                },
                                {
                    index: 3,
                    position: 100,
                    price: '29.9',
                    // url: 'https://shop.tele2.at/wk/shop/residential/privat-complete/kundendaten.html?productId=20009'
                    url: '/privat/internet/internet-und-telefon'
                },
                            ];

            var options = {
                useEasing: true,
                useGrouping: true,
                separator: '.',
                decimal: ',',
                prefix: '',
                suffix: ''
            };

            function animatePriceTo(current, target) {
                var demo = new CountUp("product-slider-price", current, target, 1, 2, 1, options);
                demo.start();
            }

            function getRelativePosition(ui) {
                var sliderWidth = jQuery('#product-slider-draggable').width();
                var containerWidth = jQuery('#product-slider-containment').width() - sliderWidth;

                var pos = ui.position.left;
                var relativePosition = pos * 100 / containerWidth;
                return relativePosition;
            }

            function getRelativePositionForClick(event) {
                var x = event.pageX - jQuery('#product-slider-containment').offset().left;
                var sliderWidth = jQuery('#product-slider-draggable').width();
                var containerWidth = jQuery('#product-slider-containment').width() - sliderWidth;

                if (x <= sliderWidth) {
                    return 0;
                } else if (x > containerWidth) {
                    return 100;
                }
                return (x - sliderWidth / 2) * 100 / containerWidth;
            }

            function setSliderToRelativePosition(relativePosition) {
                var sliderWidth = jQuery('#product-slider-draggable').width();
                var containerWidth = jQuery('#product-slider-containment').width() - sliderWidth;
                var newPosition = containerWidth / 100 * relativePosition;
                jQuery('#product-slider-draggable').css('left', newPosition + 'px');
            }

            function getClosestProduct(relativePosition) {
                var closestDistance = 100;
                var closestProduct = 0;
                for (var i = 0; i < productSliderData.length; i++) {
                    var distance = Math.abs(productSliderData[i].position - relativePosition);
                    if (distance < closestDistance) {
                        closestDistance = distance;
                        closestProduct = productSliderData[i];
                    }
                }
                return closestProduct;
            }

            function setUrl(url) {
                if (url == null) {
                    jQuery('#product-slider-url').attr('href', '#');
                    jQuery('#product-url').addClass('disabled');
                } else {
                    jQuery('#product-slider-url').attr('href', url);
                    jQuery('#product-slider-url').removeClass('disabled');
                }
            }

            function activateProduct(product) {
                setUrl(product.url);
                currentPrice = product.price;
                jQuery('.circle').removeClass('active');
                jQuery('.circle-' + product.index).addClass('active');
            }

            function changeProduct(product, adaptPosition) {
                if (currentPrice != product.price) {
                    animatePriceTo(currentPrice, product.price);
                }
                if (adaptPosition) {
                    setSliderToRelativePosition(product.position);
                }
                activateProduct(product);
            }

            jQuery(function () {

                jQuery("#product-slider-draggable").draggable({
                    containment: "parent",
                    scroll: false,
                    drag: function (event, ui) {
                        var product = getClosestProduct(getRelativePosition(ui));
                        changeProduct(product, false);
                    },
                    stop: function (event, ui) {
                        var product = getClosestProduct(getRelativePosition(ui));
                        changeProduct(product, true);
                    }
                });

                jQuery("#product-slider-containment").click(function (event) {
                    var relativePosition = getRelativePositionForClick(event)
                    var product = getClosestProduct(relativePosition);
                    changeProduct(product, true);
                });

                                jQuery(".circle-1").click(function () {
                    changeProduct(productSliderData[0], true);
                });
                                jQuery(".circle-2").click(function () {
                    changeProduct(productSliderData[1], true);
                });
                                jQuery(".circle-3").click(function () {
                    changeProduct(productSliderData[2], true);
                });
                
                if (productSliderData[0]) {
                    activateProduct(productSliderData[0]);
                    jQuery('#product-slider-price').html((productSliderData[0].price + '0 ').replace('.', ','));
                }
            });

        </script>
