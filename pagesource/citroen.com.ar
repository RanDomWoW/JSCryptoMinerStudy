<script type="text/javascript">var url_supplements = new String("");
    $(function(){
        $('body a').on('click',function(e){
            if(url_supplements.length>0
                && $(this).attr('href').indexOf(url_supplements)==-1
                && $(this).attr('href').substring(0,1)!='/'
                && $(this).attr('href').substring(0,1)!='#'
            ){
                var url_supp = url_supplements;
                var url = $(this).attr('href');
                if(url.indexOf(location.href)==-1){
                    var pos_query = url.indexOf('?');
                    if(pos_query==-1){
                        var pos_achor = url.indexOf('#');
                        if(pos_achor==-1){
                            pos_query = url.length-1;
                        }else{
                            pos_query = pos_achor-1;
                        }
                        url_supp = '?'+url_supp;
                    }else{
                        url_supp = url_supp+'&';
                    }
                    url = url.substr(0,pos_query).concat(url_supp,url.substr(pos_query+1));
                    $(this).attr('href',url);
                }
            }
        });
    });

	function initImgDefer(){
	    var imgDefer = document.getElementsByClassName('imgDefer');
	    for (var i=0; i<imgDefer.length; i++){
	    if(imgDefer[i].getAttribute('data-src')){
	    imgDefer[i].setAttribute('src',imgDefer[i].getAttribute('data-src'));
	    }}}
	    if(window.addEventListener){
          window.addEventListener('load', initImgDefer)
        }else{
          window.attachEvent('onload', initImgDefer)
        }</script>

