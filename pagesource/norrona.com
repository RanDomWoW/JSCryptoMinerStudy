            <div class="col-12 text-center product__color_name"></div>
        </div>
        <!-- SIZE -->
        <div class="middlestep__message row grid-row">
            <div class="col-12 grid-item">
                <div>Please select your size</div>
            </div>
        </div>
        <div class="row grid-row product__size d-flex align-items-center py-2" data-selected-size-code="">
            <ul class="col-12 grid-item d-flex flex-row flex-nowrap justify-content-center">
                <li class="out-of-stock">
                    <a class="" role="button"
                       data-size-code=""
                       data-stockdate=""
                       data-itemsinstock=""
                       data-prop=""
                       data-displayname=""
                       data-enableredirect="false"></a>
                </li>
            </ul>
        </div>
        <!-- ADD TO BASKET -->
        <div class="row px-2 mt-4 product__shop" style="display: none;">
            <button type="button" class="col-12 btn btn-primary anim-btn" id="addBasket"
                    data-product-code=""
                    data-message-delay="2500"
                    data-ec-id=""
                    data-ec-name=""
                    data-ec-price=""
                    data-ec-brand=""
                    data-ec-variant=""
                    data-ec-category=""
                    data-ec-list=""
                    data-ec-position="">
                <span>
                    Add to basket
                </span>
                <span>
                    Adding to basket
                </span>
                <span>
                    Added to basket
                </span>
                <span>
                    Choose size
                </span>
            </button>
        </div>
        <!-- INVENTORY BUTTON -->
        <div class="row px-2 product__inventory" style="display: none;">
            <div class="col-12">
                <button class="btn btn btn-primary" type="button" id="storeinventory" data-size-dependency="true" data-url="">Show store inventory</button>
            </div>
        </div>
        <!-- SIZE FEEDBACK -->
        <div class="product__size_feedback mt-3" data-few-left="Only {items} left" data-out-of-stock="Out of stock" data-in-stock="In stock" data-available-from="Available from {date}">

        </div>
    </div>
</section>



