    $(function() {
      function customerSubscribed(el, name) {
        el = $(el);
        el.find('.newsletter-content').fadeOut(400);
        el.find('label').fadeOut(400);
        el.find('input').fadeOut(400);
        el.find('button').fadeOut(400);
        el.find('a').fadeOut(400);
        el.find('.back-to').fadeIn(400);

        var Offer_Code = (el.find('input[name="Offer_Code"]').val() || '');
        if (Offer_Code.indexOf('code:') > -1 ){
          Offer_Code = Offer_Code.replace('code:','');
          el.find('.newsletter-text').find('h2').html('Use this code at checkout for free overnight shipping: <b>'+Offer_Code+'</b>')
        }

        cookie('newsletter-hide', 1, { expires:365 });

        ga('send', 'event', 'Action', 'Submit', 'Newsletter Signup ');
        fbq('track', 'Lead');
      }

      var klaviyoBindings = [
          ['#email_signup_overlay', 'Overlay'],
          ['#email_signup_overlay_popup_desktop', 'Popup'],
          ['#email_signup_overlay_popup_mobile', 'Popup'],
          ['#footer_signup', 'Footer']
      ];

      for (var i=0; i < klaviyoBindings.length; i++) {
        $(function(element, name) {
          KlaviyoSubscribe.attachToForms(element, {
            success: function ($form) {
              if (typeof heap !== 'undefined') {
                heap.identify($form.find('input[name=email]').val().toLowerCase());
              }
              customerSubscribed(element, name);

              // CONVERSION EVENT FOR VWO
            	window._vis_opt_queue = window._vis_opt_queue || [];
            	window._vis_opt_queue.push(function(){
                _vis_opt_goal_conversion(200);
              });

              // GTM custom event
              if (typeof dataLayer !== 'undefined') {
                dataLayer.push({'event': 'newsletter_signup'});
              }
            },
            success_message: "Lovely. Stay tuned for the latest from Tamara Mellon."
          });
        }(klaviyoBindings[i][0], klaviyoBindings[i][1]));
      }

      // Check for PC codes
      var pc = cookie('pc');
      if (pc == 'TM2DAY') {
        $('.header-offer-text').text('Free 2-Day Shipping on All Orders.');
      } else if (pc == 'TM0DAY' || pc == 'TMSAMEDAY') {
        $('.header-offer-text').text('Free Same-Day Shipping on All Orders.');
      }
    });
