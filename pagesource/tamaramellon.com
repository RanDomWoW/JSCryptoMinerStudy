  function checkURLforOverlay() {
    // CHECK FOR OVERLAY VARIANT
    var match = urlquery('ov')
    // CHECK FOR OVERLAY CLICK BACKGROUND TO CLOSE OVERLAY
    var bgc = false
    // CHECK FOR OVERLAY DELAY SETTING
    var d = urlquery('ov_delay')
    if ( !d ){
      // IF !d IN URL, SET delay = 0 TO PASS BELOW
      var delay = 0
    } else {
      // ENSURE d IS A NUMBER
      if ( isNaN(d) ){
        var delay = 0
      } else {
        // d IS A NUMBER, SET TO VALUE IN URL
        var delay = +d
      }
    }
    // IF URL IS FOR FEARLESS LANDING PAGE, DO NOT SHOW OVERLAY
    var url = window.location.href
    if ( url.indexOf('sweepstakes-fearless') < 0 ){
      if( match == false ){
        showNewsletterOverlay(false,null,bgc,delay)
      } else {
        showNewsletterOverlay(true,match,bgc,delay)
      }
    }
  }

