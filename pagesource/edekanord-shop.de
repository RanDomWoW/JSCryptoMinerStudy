				<h4 class="panel-title">
	                <a data-toggle="collapse"
    	               data-parent="#search-suggestion-accordion-delivery"
                       href="#suggest-delivery-{{$index}}">
        	            <i class="icon"
                           ng-class="{'icon-minus' : $first, 'icon-plus' : !$first}"></i>
                        <span ng-bind="region.name"></span>
					</a>
				</h4>
			</div>
			<div edeka-collapse-icon-toggle
                 id="suggest-delivery-{{$index}}"
                 class="panel-collapse collapse"
                 ng-class="{in : $first}">
				<div class="panel-body">
					<div class="list-markets-search">
						<div class="row market" ng-repeat="store in region.markets">
								<div class="col-xs-4">
									<a href="{{store.storePath}}">
										<img ng-src="{{store.imagePath}}" class="img-responsive" alt="{{store.name}}">
									</a>
								</div>
								<div class="col-xs-8 market-desc">
									<address>
										<strong>{{store.name}}</strong> 
										<span ng-repeat="datum in store.data">{{datum}}<br></span>
									</address>
									<ul class="offers list-unstyled">
										<li ng-repeat="facility in store.facilities">{{facility}}</li>
									</ul>
									<div class="clearfix"></div>
								</div>
								<div class="col-xs-4">
									<a class="iconlink-arrow-right" href="{{store.storePath}}">Zur Marktseite</a>
								</div>
								<div class="col-xs-8 market-desc">
									<div class="inner">
										<ul class="list-unstyled list-inline services">
											<li ng-show="store.deliveryService">
												<i class="icon icon-truck"></i> 
												<span>Lieferservice</span>
											</li>
											<li ng-show="store.pickupService">
												<i class="icon icon-pocket"></i> 
												<span>Selbstabholung</span>
											</li>
										</ul>
									</div>
								</div>
							</div>
						</div>
				</div>
			</div>
		</div>
