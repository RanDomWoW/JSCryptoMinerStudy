        jQuery(document).ready(function() {
            jQuery('a.like_dislike').each(function() {
                jQuery(this).click(function() {

                    var thisBtn = jQuery(this);
                    var comment_id = thisBtn.attr("data-commentid");
                    var ratetype = thisBtn.attr("data-ratetype");
                    thisBtn.fadeTo('fast',0.5);

                    if (!thisBtn.hasClass('in_action') && readCookie("rated" + parseInt(comment_id)) != 'true') {
                        thisBtn.addClass('in_action');
                        jQuery.post(
                            "http://negarshop.ir/wp-admin/admin-ajax.php",
                            {
                                'action': 'like_dislike_ajax',
                                'commentid': parseInt(comment_id),
                                'ratetype': ratetype
                            },
                            function (response) {
                                thisBtn.parent().find('span').empty().text('' + response + '');

                                createCookie("rated" + parseInt(comment_id), "true", 'http://negarshop.ir/womens-watches-emilia/', 7);
                            }
                        );

                    }
                    thisBtn.removeClass('in_action');
                    thisBtn.fadeTo('fast',1);

                });
            });
        });
