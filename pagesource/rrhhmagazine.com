function validar(control) {
	if(comprueba_mail(control.elements["email"]) == false)
		return false;
	if(comprueba_contraseña(control.elements["password"]) == false)
		return false;
			
	return true;
}

function comprueba_mail(control)
{
	if(control.value.length == 0) {
		alert("Introduzca un e-mail");
		control.focus();
		control.select();
		return false;
	}
	if(control.value.search(/([a-zA-Z.\-])+@([a-zA-Z.]+)$/)==0)
		return true;
	else{
		alert("Introduzca un e-mail válido");
		control.focus();
		control.select();
		return false;
	}
}

function comprueba_contraseña(control)
{
	if(control.value.length == 0) {
		alert("Introduzca una clave");
		control.focus();
		control.select();
		return false;
	}
	else
		return true;
}

