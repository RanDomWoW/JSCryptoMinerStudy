$('body').on('click', '.match-article-tab li', function(e){
	
	var tab_type 		= $(this).find('a').html(),
		kickoffTS 		= $('.match-article-tab').attr('data-kickoffts'),
		matchendTS 		= kickoffTS + 7200,
		currentTS 		= e.timeStamp,
		match_status 	= '';
	
	record_match_status();
	
	ga('send', 'pageview', window.location.pathname + ' ' + tab_type);
	googletag.pubads().refresh();
});

function record_match_status(){
	
	var kickoffTS 		= $('.match-article-tab').attr('data-kickoffts') * 1000,
		matchendTS 		= kickoffTS + 7200,
		currentTS 		= $.now(),
		match_status 	= '';


	if (currentTS < kickoffTS) {
		match_status = 'pre';
	}
	else if (currentTS > kickoffTS && currentTS < matchendTS) {
		match_status = 'during';
	}
	else if (currentTS > matchendTS) {
		match_status = 'post';
	}

	
	//send match status (pre, during, post)
	if(window.location.hostname=='test.newstalk.ie'){
		ga('set', 'dimension6', match_status);	
	}
	else{
		var dimensionValue = 'SOME_DIMENSION_VALUE';
		ga('set', 'dimension5', match_status);
	}
}


