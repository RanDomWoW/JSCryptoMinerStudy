        if(1==1)
        {
            $("#freshManClose").show();
            $("#freshMan").slideDown();
            $(".nav-pop").css("top","234px");
            timeId = setInterval("Count()",1000);
        }

        var div_obj = $("#eshopVI_pop_div_cartform");
        var windowWidth = document.body.clientWidth;
        var windowHeight = document.body.scrollHeight;
        var popupHeight = 580;
        var popupWidth = 760;

        div_obj.css("top", 135);
        div_obj.css("left", windowWidth / 2- popupWidth / 2);

        $("#eshopVI_mask").css("width", windowWidth);
        $("#eshopVI_mask").css("height", windowHeight);
        div_obj.show();


        $("#ksbb-content-main").children().hide();
        $("#ksbb-content-left").find("a").hide();
                
        $("#ksbb-content-main").children().eq(0).show();
        $("#ksbb-content-left").find("a").eq(0).show();
        $(".ksbb-nav a").mouseover(function(){
            $(".ksbb-nav a").removeClass("on");
            $(this).addClass("on");
            $("#ksbb-content-main").children().hide();
            $("#ksbb-content-left").find("a").hide();
            $(".ksbb-nav a").each(function(index){
                if($(this).hasClass("on"))
                {
                    $("#ksbb-content-main").children().eq(index).show();
                    $("#ksbb-content-main").children().eq(index).find(".ksbb-content-img").each(function(){
                        $(this).attr('src', $(this).attr('data-original'));
                        $(this).removeAttr('data-original');
                    });
                    $("#ksbb-content-left").find("a").eq(index).show();
                    $("#ksbb-content-left").find("img").eq(index).attr('src', $("#ksbb-content-left").find("img").eq(index).attr('data-original'));
                    $("#ksbb-content-left").find("img").eq(index).removeAttr('data-original');
                }
            });
                    
                    
            stopKsbbInterval();         
        });
        $(".ksbb-nav a").mouseout(function(){
            continueKsbbInterval();         
        });
        $("#ksbb-content-left").mouseover(function(){
            stopKsbbInterval();         
        });
        $("#ksbb-content-left").mouseout(function(){
            continueKsbbInterval();         
        });
        $(".ksbb-content-right").mouseover(function(){
            stopKsbbInterval();         
        });
        $(".ksbb-content-right").mouseout(function(){
            continueKsbbInterval();         
        });
        toTop();
        lazyLoadPic();
    });
    function Count(){
        if(timeCount<=0){
            $("#freshMan").slideUp();
            $(".nav-pop").css("top","154px");
            clearInterval(timeId);
        }
        timeCount--;
    };
    $("#divTopMsgClose").click(function () {
        $("#divTopMsg").hide();
    });

    $("#freshManClose").click(function(){
        $("#freshMan").slideUp();
        $("#freshManClose").hide();
    });
    function lazyLoadPic() {
        var $this = $('img');
        $this.each(function () {
            var $self = $(this);
            if ($self.is('img')) {
                if ($self.attr('data-original')) {
                    $self.attr('src', $self.attr('data-original'));
                    $self.removeAttr('data-original');
                }
            }
        });
    }
    //轮播改变class触发事件 start
    setTimeout(function(){
        if (window.MutationObserver) {
            var observer = new MutationObserver(function (mrs) {
                if (mrs[0].attributeName == 'class')
                {
                
                    $("#bannerNavId a").each(function (i) {
                        if($(this).hasClass("activeSlide"))
                        {
                            //console.log(i);
                            $(".ppxl-content-left-list a").each(function (index) {
                                if (index == i) {
                                    $(this).css("color", "#9E0028");
                                }
                                else {
                                    $(this).css("color", "#000000");
                                }
                            });
                        }
                    });

                }
                

            });
            for (var i = 0; i < document.getElementById('bannerNavId').getElementsByTagName('a').length; i++)
            {
                observer.observe(document.getElementById('bannerNavId').getElementsByTagName('a').item(i), {
                    'attributes': true
                });
            }
        
        }},5000)

    //轮播改变class触发事件 end

</script>

