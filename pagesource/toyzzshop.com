                            <span class="fs-20" aria-hidden="true">önceki sayfa</span>
                        </a>
                    </li>

                    <li v-if="(this.params.currentPage - 3) > 0 && params.pageCount > 5"><a href="javascript:void(0);" class="fs-20">...</a></li>

                    <li
                        v-for="n in numbers"
                        v-if="n > 0"
                        @click="changePage(n)"
                        class="{{ n == params.currentPage ? 'active' : '' }}">
                        <a href="javascript:void(0);" class="fs-20">{{ n }}</a>
                    </li>

                    <li v-if="(params.pageCount - params.currentPage) >= 3"><a href="javascript:void(0);" class="fs-20">...</a></li>

                    <li
                        class="next {{ (params.currentPage) >= params.pageCount ? 'disabled' : '' }}"
                        @click="changePage(params.currentPage + 1)">
                        <a href="javascript:void(0);" aria-label="Sonraki">
                            <span class="fs-20" aria-hidden="true">sonraki sayfa</span>
                        </a>
                    </li>
                </ul>
