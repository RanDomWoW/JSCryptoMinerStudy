                    </li>
                    <li>
                        <span class="nth fs-20">{{params.currentPage}}/{{params.pageCount}}</span>
                    </li>
                    <li
                        class="next"
                        @click="changePage(params.currentPage + 1)">
                        <a href="javascript:void(0);" aria-label="Sonraki" class="fs-18 btn btn-outline {{(params.currentPage) >= params.pageCount ? 'btn-gray' : 'btn-black' }}">
                            sonraki
                        </a>
                    </li>
            </nav>
        </div>
    </div>
