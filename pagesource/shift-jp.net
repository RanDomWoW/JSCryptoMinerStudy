<script type="text/javascript">
	$(function() {
		$('.worksSec_list').slick({
			centerMode: true,
			centerPadding: 0,
			slidesToShow: 5,
			slidesToMove: 1,
			// variableWidth: true,
			prevArrow: '<a href="javascript: void(0);" class="worksSec_arrow worksSec_arrow-prev"></a>',
			nextArrow: '<a href="javascript: void(0);" class="worksSec_arrow worksSec_arrow-next"></a>',
			responsive: [
				{
					breakpoint: $$$.pcsp.getBreakpoint(),
					settings: {
						arrows: false,
						slidesToShow: 3,
						swipeToSlide: true,
					}
				}
			]
		});

		$$$.customerLogo = (function(){
			var _init = function(){
				$('.customers_logo').each(function(index, el) {
					var _width = 0;
					$(this).find('.customers_list').attr('style', '');
					$(this).find('.customers_item').each(function(index, el) {
						_width += $(this).width() + 60;
					});
					var _clone = $(this).find('.customers_item').clone().addClass('customers_item-clone');
					_cloneNum = Math.ceil($(window).width() / _width / 2) +1;
					for (var i = 0, l = _cloneNum; i < l; i++) {
						$(this).find('.customers_list').prepend(_clone.clone()).append(_clone.clone())
					}
					$(this).find('.customers_list')
						.css({
							width: _width * _cloneNum * 2 + _width,
							left: _width * -1
						})
						.data('width', _width);
				});
			}

			var _move = function(speed, reverse){
				if (reverse) {
					if (parseInt($(this).find('.customers_list').css('left')) > 0) {
						$(this).find('.customers_list').css({
							left: $(this).find('.customers_list').data('width') * -1
						})
					} else {
						$(this).find('.customers_list').css({
							left: '+=' + speed,
						});
					}
				} else {
					if (parseInt($(this).find('.customers_list').css('left')) < $(this).find('.customers_list').data('width') * -2) {
						$(this).find('.customers_list').css({
							left: $(this).find('.customers_list').data('width') * -1
						})
					} else {
						$(this).find('.customers_list').css({
							left: '-=' + speed,
						});
					}
				}
			}

			var _slide = function(){
				setInterval(function(){
					_move.call($('.customers_logo').eq(0), 1);
					_move.call($('.customers_logo').eq(1), 1, true);
					_move.call($('.customers_logo').eq(2), 2);
				}, 15)
			}

			return {
				init: _init,
				slide: _slide
			}
		}());

		$(window).on('load', function(event) {
			event.preventDefault();
			$$$.customerLogo.init();
			$$$.customerLogo.slide();
		});
	});
</script>

