<script type="text/javascript">
    $(document).ready(function(){
        $('.subscribe_form_ajax').on('submit', function(e) {
        if(jQueru(e.target).find('.recaptcha-holder').length == 0) {
          e.preventDefault();
          submitForm(e.target);
		}
		return false;
	});
    });

    function submitForm(form)
    {
        $.ajax({ url: "/?ACT=161",
            data: $(form).serialize(), // serializes the form's elements.
            global: true,
            cache: false,
            type: 'POST',
            success: function(res){
                if (res.success == 1)
                {
			  
              		if($('#ouibounce-modal').is(':visible')){
                            $('#thanks').show();
                            $('#subscriber-popup-field').hide();
                            $('#roundtable-modal-footer').hide();
                    }

                    if($('#ouibounce-modal').is(':visible') || $('#ouibounce-modal').length > 0){
                        setTimeout(function(){$('#ouibounce-modal').remove();}, 6000);
                    } else {$('#ouibounce-modal-rt').remove();}

                    $('.subscribe_form_ajax > .inline-subscribe').css('display', 'none');
                    $('.subscribe_form_ajax > .subscribe-success-message').css('display', 'block');
                    window.dataLayer = window.dataLayer || []; 
					window.dataLayer.push({ 'event' : 'AjaxEmailSubscribeSuccess', 'formId' : 'AjaxEmailSubscribe' });


                }
                else
                {
                	if($("#bad-email").length && $(form).find(".input-group").length) {
                        $("#bad-email").prependTo($(form).find(".input-group")).fadeIn('fast', function () {$(this).delay(1600).fadeOut('slow');});
                    } else {alert(res.message);}

                }
            }, error: function(XMLHttpRequest, textStatus, errorThrown)
            {
                alert('System Error. Please contact customer service.');
            }
        });
        return false;
    }
</script>

