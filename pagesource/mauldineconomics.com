        <script type="text/javascript"> 
 $( document ).ready(function() {
    var labels = ['days', 'hours', 'minutes', 'seconds'],
      setDate = moment.tz("2018-03-19 00:00:00", "US/Eastern").toDate(),
      template = _.template($('#rab-countdown-temp').html()),
      currDate = '00:00:00:00:00',
      nextDate = '00:00:00:00:00',
      parser = /([0-9]{2})/gi,
      $example = $('#rab-countdown');
    // Parse countdown string to an object
    function strfobj(str) {
      var parsed = str.match(parser),
        obj = {};
      labels.forEach(function(label, i) {
        obj[label] = parsed[i]
      });
      return obj;
    }
    // Return the time components that diffs
    function diff(obj1, obj2) {
      var diff = [];
      labels.forEach(function(key) {
        if (obj1[key] !== obj2[key]) {diff.push(key);}
      });
      return diff;
    }
    // Build the layout
    var initData = strfobj(currDate);
    labels.forEach(function(label, i) {
      $example.append(template({
        curr: initData[label],
        next: initData[label],
        label: label
      }));
    });
    // Starts the countdown
    $example.countdown(setDate, function(event) {
      var newDate = event.strftime('%d:%H:%M:%S Z'),
        data;
      if (newDate !== nextDate) {
        currDate = nextDate;
        nextDate = newDate;
        // Setup the data
        data = {
          'curr': strfobj(currDate),
          'next': strfobj(nextDate)
        };
        // Apply the new values to each node that changed
        diff(data.curr, data.next).forEach(function(label) {
          var selector = '.%s'.replace(/%s/, label),
              $node = $example.find(selector);
          // Update the node
          $node.removeClass('flip');
          $node.find('.curr').text(data.curr[label]);
          $node.find('.next').text(data.next[label]);
          // Wait for a repaint to then flip
          _.delay(function($node) {$node.addClass('flip');}, 50, $node);
        });
      }
    });
  }); 
  $('#rab-countdown').on('finish.countdown', function(event) {
    $(this).html('').parent().html('');       
     $('#home-banner-main-top-right').html('<a href="http://www.mauldineconomics.com/go/v37s9f/MEC" target="_blank"><img alt="Thousands of investors have picked Jared Dillian\'s ETF 20/20. Want in? Join for just $79 per year." src="//images.mauldineconomics.com/campaigns/ETF-2020-launch/etf-2020-homepage-banner-post.png" width="331" /></a>');       
});
</script> 
