      <script type="text/javascript">;(function($){
  'use strict';

  $(function(){
    var $videoContainer = $('.js-home-video');
    var $videoIframe = $('#js-home-video-iframe');
    var $scrollParent = $('#js-home-stories-scroll-parent');
    var showingVideoContainer = false;

    var closeVideo = function(){
      $videoIframe.attr('src', '');
      $videoContainer.slideUp(500);
      $videoContainer.removeClass('showing');
      showingVideoContainer = false;
    };

    $('.js-home-video-open').click(function(){
      // Effectively lazy-load the videos by not applying
      // any src to the iframe until needed
      var $this = $(this);
      var videoSrc = $this.data('video-src') + '?autoplay=true';

      var position = $this.position().left;
      var diff = ($this.outerWidth() + position) - $scrollParent.outerWidth();
      if (position < 0){
        // Overflows on the left; scroll so the element is at the left edge
        $scrollParent.animate({
          scrollLeft: position
        }, 'fast');
      }
      else if(diff > 0){
        // Overflows on the right; scroll so the element is at the right edge
        $scrollParent.animate({
          scrollLeft: $scrollParent.scrollLeft() + diff
        }, 'fast');
      }

      // Allowing clicking the same video link twice to close the video
      if($videoIframe.attr('src') === videoSrc){
        closeVideo();
        return;
      }

      if(showingVideoContainer){
        // Swap the src to the new video
        $videoIframe.attr('src', videoSrc);
      }
      else {
        // Show the container then load the video when ready
        $videoContainer.slideDown(500, function(){
          $videoIframe.attr('src', videoSrc);
        });
      }

      // This has to hapen after the slideDown is initiated
      $('html, body').animate({
        // HACK: account somewhat for the fixed header height
        // on mobile
        scrollTop: $(window).width() > 480 ?
          $videoContainer.offset().top :
          $videoContainer.offset().top - 20
      }, 500);

      $videoContainer.addClass('showing');
      showingVideoContainer = true;
    });

    $('.js-home-video-close').on('click', function(){
      if(showingVideoContainer){
        closeVideo();
      }
    });
  });
})(jQuery);
</script>

