tNode.insertBefore(c,d);function b(){var j;for(var h=0;h<f.length;h++){if(f[h].href&&f[h].href.indexOf(a)>-1){j=true}}if(j){c.media=g||"all"}else{setTimeout(b)}}b();return c}; loadCSS("http://www.beach-on-map.ru/img/0/css/m41.css"); </script> <noscript><link href="http://www.beach-on-map.ru/img/0/css/m41.css" rel="stylesheet" type="text/css" /></noscript> <script type="text/javascript"> var map_data = {"geoname":"Весь мир","video":"","hotelads":"http://www.hotelscombined.com/Place/Aitutaki.htm?a_aid=132060","scale":"2","flightads":"http://www.momondo.com/flights/ait/flights-to-aitutaki.html","items":[["1","17.5","Африка","1","africa","62","south-africa-cape-town-boulders-beach","","ЮАР, Кейптаун, Пляж Боулдерс-бич","1","1","9.1"],["15","-94","Центральная Америка","1","central-america","59","panama-san-blas-isla-diablo-beach","","Панама, Сан-Блас, Исла-Диабло, пляж","1","1","9.0"],["29","51","Западная Азия","1","western-asia","59","uae-khor-fakkan-beach-palms","","ОАЭ, Пляж Хор-Факкан, пальмы","1","1","8.0"],["-3","77","Индийский океан","1","indian-ocean","35","maldives-baa-atoll-fulhadhoo-beach-clear-water","","Мальдивы, Атолл Баа, Пляж Фулхадху, прозрачная вода","1","1","9.5"],["-10.169921","-169.118645","Океания","1","oceania","27","cook-islands-aitutaki-atoll-ee-island-ee-motu-day-trip","","Острова Кука, атолл Аитутаки, Ее Айленд (моту Ее), запас провианта на день","1","1","9.5"],["52","9","Европа","1","europe","63","italy-calabria-tropea-beach","","Италия, Калабрия, Пляж Тропея","1","1","8.7"],["18.5","-72","Карибы","1","caribbean","60","bahamas-exuma-pig-beach","","Багамы, Эксума, Пляж Пиг-бич","1","1","9.2"],["32","119","Восточная Азия","1","eastern-asia","39","china-guilin-yangshuo-village-li-river","","Китай, Гуйлинь, Деревня Яншо, река Ли","1","1","8.9"],["-24","-61","Южная Америка","1","south-america","36","colombia-cartagena-playa-blanca-beach-palm-over-water","","Колумбия, Картахена, Пляж Плайя Бланка, пальма над водой","1","1","9.1"],["52","-112","Северная Америка","1","north-america","28","usa-florida-key-biscayne-island-crandon-park-beach","","США, Флорида, Остров Ки-Бискейн, пляж Крэндон Парк","1","1","8.7"],["8","120","Юго-Восточная Азия","1","southeast-asia","35","thailand-samui-maenam-beach-view-to-east","","Таиланд, Самуи, Пляж Маенам, вид на восток","1","1","9.3"]],"center":["15","-1"]}; var pic_data = ""; var misc_data = ["index","Весь мир","","http://www.beach-on-map.com/img/2/cook-islands-aitutaki-atoll-ee-island-ee-motu-day-trip","jpg","2048"]; </script> <script async type="text/javascript" charset="UTF-8" src="http://www.beach-on-map.ru/img/0/js/m75.js"></script> <script type="application/ld+json">{"alternateName":"Весь мир","mainEntityOfPage":"http://www.beach-on-map.ru/","isFamilyFriendly":"1","text":"TOP50: Лучшие пляжи мира. » Весь мир. Ее Айленд (моту Ее) (Ee island - Ee motu) - лучший пляж на острове Аитутаки. Здесь нет ни отелей, ни ресторанов, ни магазинов, зато есть идеальный тропический пляж. Однако, лагуна очень мелкая, - нужно долго идти чтобы нормально искупаться. / читать дальше » / Рейтинг: 9.5 Фулхадху (Fulhadhoo) - пляж одноименного острова на Мальдивах (атолл Баа). Это место в своем роде уникально. Обычно на Мальдивах встречаются либо дорогие люксовые острова-отели, либо острова с местными жителями полностью застроенные домами. Здесь же остров относительно большой и деревня занимает лишь небольшую территорию. Все остальное - в вашем полном распоряжении. Несколько километров белого как мука песка, буйная тропическая растительность и песчаная коса, на которой днем вы можете купаться в прозрачной воде, а вечером встречать закат. Большую часть времени на пляже нет ни одного человека, - ни местных, ни туристов. И все это вы можете получить за относительно небольшие деньги. / читать дальше » / Рейтинг: 9.5 Фо Сизонс (Four Seasons Bora Bora) - отель с лучшими пляжами на Бора Бора. Отель дорогой, однако, определенно стоит своих денег. Рекомендуем детский пляж, расположенный на берегу уютной внутренней лагуны. Пляж: песок. / читать дальше » / Рейтинг: 9.4 Маенам (Maenam) - лучший пляж на острове Самуи в Тайланде, а также один из лучших пляжей в мире. Обилие пальм и песок, а также небольшое количество людей, - все компоненты идеальных тропиков сошлись здесь в идеальной пропорции. При этом есть возможность найти недорогое бунгало (от 600 бат) прямо на берегу, - отличное соотношение цена/качество! Наилучшая часть пляжа - возле кафе Shangrilah Bungalows. / читать дальше » / Рейтинг: 9.3 Нануйя Лайлай (Nanuya Lailai) - остров архипелага Ясава с лучшими пляжами на Фиджи. Самый лучший пляж - Голубая лагуна (Blue Lagoon). Сюда возят много экскурсий с соседних островов, поэтому днем здесь всегда много людей. Поэтому имеет смысл жить непосредственно на острове, чтобы иметь возможность посещать лагуну утром и вечером в отсутствии толп туристов. / читать дальше » / Рейтинг: 9.3 Финолху-Канифуши (Finolhu Kanifushi) - остров в атолле Баа на Мальдивах с уникальной песчаной косой, благодаря которой есть возможность совершать продолжительные прогулки в океане с посещением еще трех необитаемых островов. Везде есть отличные пляжи с высокими пальмами и без битых кораллов в воде, а большая лагуна хорошо подходит тем, кто любит заплыть подальше. / читать дальше » / Рейтинг: 9.3 Пиг-бич (Pig Beach) - небольшой необитаемый остров в группе Эксума на Багамах. Сам по-себе остров никому не интересен. Главное то, что здесь есть свиньи, с которыми можно поплавать в Карибском море. Хрюшек кормят как туристы, так и местные жители, однако, в последнее время их популяция немного сократилась. Впрочем, по другим сведениям взрослых особей местные жители просто понемногу подъедают, - иначе их станет слишком много. / читать дальше » / Рейтинг: 9.2 Батибу-Бэй (Batibou Bay) - лучший пляж на острове Доминика, Карибы. Это отличный урок тем, кто считает, что на Доминике нет хороших пляжей. Они таки есть, - только места знать надо :) Собственно здесь идеально все, - много пальм, песок белый, вода прозрачная без сильных волн и течений. И при всем этом относительно немного людей. Плата за вход составляет 5$ с человека. Также на пляже есть отличный бар - с выпивкой и вкусной едой. / читать дальше » / Рейтинг: 9.2 Уан Фут Айленд (моту Тапуаетай) (One Foot island - Tapuaetai motu) - необитаемый остров атолла Аитутаки. Сюда постоянно возят экскурсии из отелей, поэтому людей относительно много. Здешние пейзажи идеальны для фотосессии на фоне пальм, но купаться не слишком комфортно. / читать дальше » / Рейтинг: 9.2 Вомо (Vomo) - остров с лучшими пляжами на островах Маманука. Также остров отлично подходит для пеших прогулок по невысокому горному хребту. / читать дальше » / Рейтинг: 9.2 Дирекшн Айленд (Direction Island) - необитаемый остров атолла Килинг (Кокосовые острова). Здесь находится лучший пляж в регионе. Остров оборудован для проведения пикников и является популярным местом отдыха у туристов. / читать дальше » / Рейтинг: 9.2 Теахупоо (Teahupo'o) - потрясающий серферский пляж на юге острова Таити во Французской Полинезии. Сама по-себе волна здесь хорошая, но крайне сложная и подходит только для опытных спортсменов. Уникальность заключается в рифе, который частично гасит эту волну с края. Это дает возможность делать отличные фото находясь в центре событий, но при этом не подвергая себя опасности. Отсюда и множество расслабленных наблюдателей, которые совершенно спокойно находятся казалось бы на самом краю бездны. / читать дальше » / Рейтинг: 9.2 Кайо Ларго (Cayo Largo) - остров с лучшими пляжами на Кубе. Это туристическая резервация в чистом виде, однако, намного более приятная, чем Варадеро. Впрочем, и более дорогая. Лучший пляж - Плайя Сирена (Playa Sirena) - на западе острова. / читать дальше » / Рейтинг: 9.2 Амилла-Фуши (Amilla Fushi) - частный остров на атолле Баа, Мальдивы. Пляжи все отличные и довольно разнообразные. Есть участки с пальмами - скорее напоминающие Карибы, а есть участки побережья с буйным кустарником - более типичные для Мальдив. Заход в воду песчаный, но почти сразу начинается коралловое дно с отличным снорклингом. / читать дальше » / Рейтинг: 9.2 Дигура (Dhigurah) - местный остров в атолле Южный Ари на Мальдивах. Это один из лучших островов в стране, но пока еще не переполненный туристами. Остров очень длинный с идеальным пустынным пляжем на юге, переходящим в шикарную песчаную косу. Женщинам можно купаться в бикини, пусть и не везде. Это одно из лучших мест на Мальдивах для наблюдения за китовыми акулами в океане. При этом и на домашнем рифе также неплохой снорклинг. На острове есть недорогое кафе, магазины и несколько независимых дайв-центров, что поможет снизить и без того невысокие расходы на отдых. При желании здесь можно отдохнуть не сильно дороже Тайланда, при этом получив настоящие Мальдивы в их наилучшем виде. / читать дальше » / Рейтинг: 9.1 Атмосфе-Канифуши (Atmosphere Kanifushi) - остров-отель на атолле Лавияни на Мальдивах. Северный пляж великолепный - с пальмами и белым песком. И только в западной части побережья морское дно каменистое. Впрочем, остров длинный (2 км), поэтому всегда можно найти подходящее место. Имеются и незначительные недостатки. Так, например, деревья находятся далековато от берега. Кроме того, возле острова нет домашнего рифа. Но отель несколько раз в день организует отличные бесплатные снорклинг-экскурсии на лодке. / читать дальше » / Рейтинг: 9.1 Пляж Диана и мыс Коко Пойнт (Diana beach & Coco Point) - лучший пляж на острове Барбуда, а также на всех Карибах. Бесконечный белый песок и кокосовые пальмы - идеальный рецепт тропического уединения. Принято отдельно выделять мыс Коко Пойнт, который заканчивается превосходной песчаной отмелью. / читать дальше » / Рейтинг: 9.1 Уайт Бич (White beach) - лучший пляж на острове Боракай на Филиппинах. В высокий сезон здесь много людей, но пляжный отдых все равно превосходный. Пляж условно делится на три зоны - Станция 1, Станция 2 и Станция 3. При этом совершенно не принципиально где жить, так как вы без проблем сможете ходить куда угодно в течение дня. / читать дальше » / Рейтинг: 9.1 Фо-Сизонс (Landaa Giraavaru) - остров-отель Four Seasons атолла Баа на Мальдивах. Самая лучшая часть пляжа - коса Ландаа, уходящая далеко в океан. В остальном же это неплохое, но не слишком выдающееся место по-меркам региона. / читать дальше » / Рейтинг: 9.1 Диду-Финолу (Dhidhoofinolhu) - частный остров в атолле Южный Ари - довольно большой по Мальдивским меркам. Везде находится красивый и разнообразный пляж, дающий полное представление о том, какие могут быть Мальдивы. Морское дно песчаное с комфортной глубиной, и при этом нигде нет волнорезов. К северу находится длинная песчаная коса, позволяющая в отлив пройти до местного острова Дигура. / читать дальше » / Рейтинг: 9.1 Плайя Бланка (Playa Blanca) - превосходный пляж на Карибском побережье Колумбии (20 км к югу от Картахены) с классическим тропическим пейзажем, - белым песком и пальмами над водой. Определенный недостаток заключается в том, что днем сюда приезжает много людей из Картахены. Зато до 10:00 и после 16:00 пляж будет в вашем полном распоряжении. / читать дальше » / Рейтинг: 9.1 Боулдерс-бич (Boulders Beach) - пляж с пингвинами - единственный в Кейптауне, ЮАР. Колония возникла только в 1982 году, а сейчас уже насчитывает несколько тысяч особей. Кроме того на пляже много огромных валунов, которые сильно напоминают Сейшелы. Имеется несколько участков побережья, где можно купаться. Пингвины же обычно собираются на главном пляже, доступ на который людям запрещен. Вы можете только пройти по деревянному помосту (вход 10$) на смотровую площадку. Впрочем, иногда и в бесплатной зоне можно встретить случайных пингвинов. / читать дальше » / Рейтинг: 9.1 Пунта Кана (Punta Cana) - туристический регион с лучшими пляжами в Доминиканской республике. Большинство отелей находится в местечке под название Баваро (Bavaro), центром которого является пляж Плайя Эль Кортесито (Playa El Cortecito). / читать дальше » / Рейтинг: 9.0 Пиджин Пойнт (Pigeon Point) - лучший пляж на острове Тобаго, а также во всех Карибах. Фактически это полуостров, в западной части которого находится центральный пляж со всевозможными удобствами и развлечениями. Север и восток полуострова - более дикие с небольшим количеством отдыхающих. / читать дальше » / Рейтинг: 9.0 Виламенду (Vilamendhoo) - частный остров в атолле Алифу-Даалу на Мальдивах. Пляжные бунгало удачно находятся на некотором удалении от берега. Поэтому практически по всему периметру острова виден только идеальный пляж - с деревьями, пальмами свисающими над водой, белым песком и удобным заходом в воду, а на домашнем рифе отличный снорклинг. Из недостатков можно отметить то, что на севере острова нет хорошей глубины, а южный берег сильно размыт водой. / читать дальше » / Рейтинг: 9.0 Сонева-Кири южный (Soneva Kiri South Beach) - заброшенный второстепенный пляж отеля Сонева-Кири на острове Ко-Куд в Тайланде. Теперь любой желающий имеет возможность попасть на лучший пляж острова, - к тому же еще и безлюдный, поскольку поблизости нет ни отелей, ни ресторанов. В 300 метрах от берега находится небольшой остров Ko Raet. / читать дальше » / Рейтинг: 9.0 Лелеувия (Leleuvia) - коралловый остров на Фиджи недалеко от главного острова Вити Леву. Формально относится к группе островов Ломаивити. Здесь отличный пляж, хороший снорклинг и уютная атмосфера. / читать дальше » / Рейтинг: 9.0 Вайя (Waya) - остров на Фиджи в архипелаге Ясава. По совокупности факторов является самым оптимальным островом в архипелаге. Лучший пляж находится в бухте Likuliku Bay. / читать дальше » / Рейтинг: 9.0 Нанануира (Nananu-i-Ra) - небольшой остров на северо-востоке главного острова Вити-Леву на Фиджи. На острове есть несколько отличных пляжей, сравнимых с пляжами Ясавы и Маманук. При этом от большой земли добираться гораздо меньше, а цены гораздо ниже, чем на указанных удаленных островах. / читать дальше » / Рейтинг: 9.0 Кабо Сан-Хуан де Гиа (Cabo San Juan de Guia) - пляж в национальном парке Tayrona в Колумбии. Место исключительно красивое и колоритное. Здесь есть побережье с белым песком и пальмами, а также дополнительные бонусы в виде первозданных джунглей и покинутый город индейцев. Туристов, правда, очень много, но сильно они не мешают. Плавание в океане во многих местах опасное из-за сильных течений. / читать дальше » / Рейтинг: 9.0 Пазир Панджанг (Pasir Panjang) - лучший пляж на острове Кай Кечил в Индонезии. Впрочем, почти возле каждой деревни на западе острова есть хороший пляж. К сожалению, не везде есть хороший отель. / читать дальше » / Рейтинг: 9.0 Исла-Диабло (Isla Diablo) - бюджетный остров архипелага Сан-Блас в Панаме. Легендарные две пальмы над водой могут не дожить до вашего приезда, но вся красота останется. В Сан-Блас 365 островов и многие из них как на картинке. Поторопитесь пока здесь не понастроили отелей и терминалов для круизных лайнеров. Идиллия не будет продолжаться вечно. / читать дальше » / Рейтинг: 9.0 Кайо Коко (Cayo Coco) - острова с отличными пляжами возле северного побережья Кубы. Лучшая часть побережья находится на западе и называется Кайо Гильермо (Cayo Guillermo). Пляж Пилар (Playa Pilar) находится на крайней западной части Кайо Гильермо и является лучшим пляжем архипелага. Кайо Коко является туристической зоной, рассчитанной на иностранцев. Однако, в отличии от Варадеро здесь гораздо меньше отелей и пляжи более пустынные. / читать дальше » / Рейтинг: 8.9 Медхуфуши (Medhufushi) - частный остров в атолле Миму, Мальдивы. Практически вся береговая линия представляет из себя отличный пляж с пальмами, кустами и деревьями. К сожалению, в воде везде много водорослей и очень мелко. Только напротив бассейна находится более-менее приличный участок, где не надо далеко идти до глубины. Снорклинг вокруг острова также довольно посредственный, не смотря на каменистое дно. / читать дальше » / Рейтинг: 8.9 Яншо (Yangshuo) - культовая достопримечательность возле города Гуйлинь - родина бэкпекерского движения в Китае. Карстовые горы формируют уникальные пасторальные пейзажи, с которыми могут конкурировать только холмы Тосканы. / читать дальше » / Рейтинг: 8.9 Халавели (Halaveli) - остров-отель в атолле Северный Ари на Мальдивах. Пляжи везде идеальные с большим количеством высоких пальм и песчаным заходом в воду. В соответствии с общей тенденцией теперь остров защищен волнорезом почти со всех сторон, что немного портит вид на океан. На домашнем рифе хороший снорклинг - особенно за пределами волнореза. / читать дальше » / Рейтинг: 8.9 Кайо де Агуа (Cayo de Agua) - лучший пляж на островах Лос Рокес. Здесь относительно мало растительности, зато песчаные отмели необыкновенно красивы. / читать дальше » / Рейтинг: 8.9 Нгапали (Ngapali) - самый престижный пляж в Мьянме, а также один из лучших пляжей в Юго-Восточной Азии. Строительство отелей идет ударными темпами, однако, здесь все еще малолюдно по сравнению с курортами Тайланда. / читать дальше » / Рейтинг: 8.9 Лаукала (Laucala) - частный остров возле острова Тавеуни на Фиджи. Лучшие пляжи находятся на севере острова, и они идеально подходят как для купания, так и для снорклинга. / читать дальше » / Рейтинг: 8.9 Лизард Айленд Резорт (Lizard Island Resort) - отельный пляж на острове Лизард-айленд Большого Барьерного Рифа в Австралии. Хотя пляж и хорош, на острове есть и другие пляжи, - возможно, даже гораздо лучше. Эта часть острова привлекательна скорее своей элитностью, - для тех, разумеется, кто может себе это позволить. / читать дальше » / Рейтинг: 8.9 Голубая лагуна (Blue Lagoon) - лучший пляж на Перхентианских островах в Малайзии. Прозрачная вода, белый песок, буйная тропическая растительность и небольшое количество людей, - почти все компоненты идеального пляжа. Единственный недостаток - отсутствие пальм. Также пляж известен под названием Turtle Bay, потому что иногда черепахи приходят сюда откладывать яйца. / читать дальше » / Рейтинг: 8.9 Рити-Ра (Reethi Rah) - искусственный остров с самой протяженной береговой линией на Мальдивах, атолл Северный Мале (верх). При чем практически везде пляж идеальный во всех отношениях, - белый песок, пальмы и отличный заход в воду безо всяких камней и ракушек. Из недостатков можно отметить полное отсутствие снорклинга, так как в воде нет ни рыбок, ни кораллов. Кроме того по острову все время катаются многочисленные электромобили, что понравится далеко не каждому. / читать дальше » / Рейтинг: 8.9 Хилтон (Hilton Bora Bora) - отель на Бора Бора. Находится на не самом лучшем моту, с которого не видна гора Отеману. Кроме того водные бунгало идут вдоль пляжа, что несколько портит пейзаж. Из преимуществ можно отметить прекрасный тропический сад, примыкающий к пляжу. Рекомендуем селиться не в водном бунгало, а в доме в саду. Благодаря этому вы сможете сэкономить много денег и получите хороший вид. Пляж: песок. / читать дальше » / Рейтинг: 8.9 Гили-Ланканфуши (Gili Lankanfushi) - тот самый остров на Мальдивах (атолл Северный Мале) с двухэтажными водными виллами, внешне выдержанными в стилистике Тайской тюрьмы. Тем не менее пляжи здесь превосходные, - особенно северное побережье с пальмовой рощей и песчаной косой. Днем мимо острова регулярно пролетают гидросамолеты, что несколько неуместно для такого эксклюзивного отеля. / читать дальше » / Рейтинг: 8.9 Клонг-Чао (Klong Chao) - самый популярный пляж на западном побережье острова Ко-Куд в Тайланде. Тропический пейзаж с пальмами и отели проверенные временем привлекают сюда много пакетных туристов. Кроме того это лучшее место на острове, чтобы встречать закат. Песчаная коса на севере пляжа эффектно смотрится с воздуха и подходит тем, кто любит поплескаться на мелководье. / читать дальше » / Рейтинг: 8.9 Атлантис (Atlantis) - самый знаменитый пляжный отель рядом с островом Нассау на Багамах. По-сути это скорее роскошный аквапарк со всем что только можно придумать. Тут есть пирамида Майя, собственный Тадж-Махал, плавательные бассейны с огромными рыбами, водные горки, детские зоны и множество пляжей - океанских и во внутренней лагуне. Хотя бы раз в жизни здесь обязательно стоит побывать. / читать дальше » / Рейтинг: 8.9 Мистери-айленд (Mystery Island / Inyeug) - небольшой райский остров рядом с большим островом Анейтьюм в Вануату. Раз в несколько дней сюда приплывает круизный лайнер и высаживает сотни туристов (если немного отойти, что толпы не будет). В остальное время здесь нет вообще никого. Кроме собственно пляжного отдыха главные развлечения это ярмарка с местными товарами, а также плавание с гигантскими черепахами и снорклинг. / читать дальше » / Рейтинг: 8.9 Кайо-Мадриски (Madrisqui / Madriski) - один из лучших островов в архипелаге Лос-Рокес в Венесуэле. Здесь такой же белый песок и лазурная вода как и везде, но есть и кое-что уникальное. Во-первых, это деревья со спасительной тенью, - что редко встретишь на других островах Лос-Рокес. Во-вторых, здесь есть рыбацкая деревушка (Cayo Pirata), чудом уцелевшая после того, как острова получили статус Национального парка. Рекомендуем местные кафе с отличными и недорогими морепродуктами. / читать дальше » / Рейтинг: 8.9 Уайт Бэй (White Bay) - лучший пляж острова Джост Ван Дайк на Британских Виргинских Островах. В центральной части пляжа находится бар Soggy Dollar Bar, известный как место где был изобретен коктейль Painkiller. / читать дальше » / Рейтинг: 8.8 Муша-Кэй (Musha Cay) - частный остров в группе Эксума-Кэйс на Багамах. В отличии от большинства островов Эксумы здесь есть нормальный пляж с пальмами, а также великолепная песчаная коса. В остальном это обычный багамский остров, отличающийся только наличием невероятно дорогого отеля. / читать дальше » / Рейтинг: 8.8 Посмотреть остальные пляжи »","url":"http://www.beach-on-map.ru/","publisher":{"email":"andrew.v.stephen.jr@gmail.com","telephone":"+79168158196","name":"Andrew V. Stephen","@type":"Person"},"@type":"WebPage","significantLink":"http://www.hotelscombined.com/Place/Aitutaki.htm?a_aid=132060","inLanguage":"Russian","thumbnailUrl":"http://www.beach-on-map.com/img/2/cook-islands-aitutaki-atoll-ee-island-ee-motu-day-trip-1