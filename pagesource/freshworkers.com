$('#opennav').on('click',function(){
	
	if($('#mynav').hasClass('in collapse')){
		$('#mynav').removeClass('in collapse').css('height','0');
	}else{
		$('#mynav').addClass('in collapse').css('height','auto');
		}
});

$.validator.addMethod(
    "regex",
    function(value, element, regex) {
        var check = false;
        return this.optional(element) || regex.test(value);
    },
    "Please check your input."
);

$(document).ready(function(e) {	
	
	$('#buttom-subscribe').validate({
        rules: {
            username: {
				required: true,
				//regex: /[^a-zA-Z ]/,
            },
			useremail: {
				required: true,
				email:true
            },
			
		}, 
		
submitHandler: function(form) {
	
            var data = $(form).serialize();			
			
            // Use Ajax to submit form data			
					$.ajax({
						type:'POST',
						url:'/frontend/web/index.php?r=site%2Fvalidateemail',
						data:data,
						success:function(html){
							if(html == '1')
							{
								$('#ema-alr').css('display','block');
								$('#dom-not').css('display','none');
							}
							else if(html == '2')
							{
								$('#ema-alr').css('display','none');
								$('#dom-not').css('display','block');
							}
							else if(html == '3')
							{
								$('#ema-alr').css('display','none');
								$('#dom-not').css('display','none');
								window.location.href="/frontend/web/index.php?r=site%2Fthankyou"								
							}														
							 
							}
					});
  			} 				
	});
    
});

</script>