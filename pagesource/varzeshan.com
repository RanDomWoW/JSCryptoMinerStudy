    function checkRecoverData() {
        if (!checkRecoverEmail()) {
            return false;
        }
        if (!CheckRecoverEmailFormat()) {
            return false;
        }
        return true;
    }
    function checkRecoverEmail() {
        var element = $("#txtRecoverEmail");
        if (element.val() == '') {
            $("#forgetErrorMessages").html('ایمیل را وارد کنید');
            return false;
        } else {
            return true;
        }
    }
    function CheckRecoverEmailFormat() {
        var email = $("#txtRecoverEmail").val();
        var reg = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        var result = reg.test(email);
        if (result == false) {
            $("#forgetErrorMessages").html('ایمیل را درست وارد کنید');
            return false;
        }
        return true;
    }
