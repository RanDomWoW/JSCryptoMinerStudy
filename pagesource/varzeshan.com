<script>

    function sendForgotPassword() {
        if (!forgotPassCheckInside()) {
            return false;
        }

        $("#forgetErrorMessages").html('در حال ارسال لینک تغییر کلمه عبور...');
        $.ajax({
            type: "POST",
            url: "/var/Users/SendRecoverEmail",
            data: JSON.stringify({ Email: $("#txtForgotEmailControll").val() }),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            cache: false,
            success: function (data) {
                $("#loginMessages").html('');
                if (data === "1") {
                    $("#forgetErrorMessages").html('لینک تغییر کلمه عبور به ایمیل شما ارسال شد');
                    setTimeout(function () {
                        $("#txtForgotEmailControll").val('');
                        $('.errSignIn').remove();
                        $("#forgetErrorMessages").html('');
                        $("#mdlForgotPassword").modal('toggle');
                    }, 3000);

                } else if (data === "2") {
                    $("#forgetErrorMessages").html('ایمیل شما در سیستم ثبت نشده');
                } else {
                    $("#forgetErrorMessages").html('ارسال لینک تغییر کلمه عبور با مشکل مواجه شد. دوباره سعی کنید');
                }
            }
        });


    }

    function ajaxLogin() {
        if (!CheckLoginInside()) {
            return false;
        }

        var data = JSON.stringify({
            Email: $('#txtSignInEmailControll').val(),
            Password: $('#txtSignInPasswordControll').val(),
            IsRememberPassword: $('#chkRememberMe').prop('checked')
        });

        $.ajax({
            url: "/var/Users/AjaxLogin",
            type: "post",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            data: data,
            beforeSend: function () {
                if (window.currentRequest != null) {
                    window.currentRequest.abort();
                }
                $('.loginLoader').show();
                $('.loginKeys').hide();
            }, success: function (data) {
                if (data.m_Item1) {

                    window.location = window.location.pathname;

                } else {

                    $('.errSignIn').remove();
                    $('.loginLoader').hide();
                    $('.loginKeys').show();
                    $("#loginMessages").html('');
                    var errorItem = data.m_Item2;
                    setLoginError(errorItem);
                }
            }
        }

        );
    }
    function sendActivationCode() {
        $("#loginMessages").html('در حال ارسال لینک فعالسازی...');
        $.ajax({
            type: "POST",
            url: "/var/Users/SendActiveLink",
            data: JSON.stringify({ Email: $("#txtSignInEmailControll").val() }),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                if (data == "1") {
                    $("#loginMessages").html('لینک فعالسازی به ایمیل شما ارسال شد');

                } else if (data == "0") {
                    $("#loginMessages").html('ارسال لینک فعالسازی با مشکل مواجه شد. دوباره سعی کنید');
                }
            }
        });
    };

    function setLoginError(loginError) {
        if (loginError == "1") {
            $("#loginMessages").html('نام کاربری یا کلمه عبور را اشتباه وارد کرده اید');
        }
        if (loginError == "2") {
            $("#loginMessages").html('حساب شما غیرفعال می باشد  جهت فعالسازی <a class="sendActiveEmail" href="#" onclick="sendActivationCode()">کلیک کنید<a>');
        }
        if (loginError == "3") {
            $("#loginMessages").html('حساب شما مسدود می باشد');
        }
    }
    function setSignUpError(signUpError) {
        $("#SignUpMessages").html(signUpError);
    }
    function forgotPassCheckInside() {
        var error = false;
        if (!checkRequired($('#txtForgotEmailControll'))) {
            error = true;
        }
        if (error) {
            return false;
        }
        return true;
    };
    function CheckLoginInside() {
        var error = false;
        if (!checkRequired($('#txtSignInEmailControll'))) {
            error = true;

        }
        if (!checkRequired($('#txtSignInPasswordControll'))) {
            error = true;
        }
        if (error) {
            return false;
        }
        return true;
    };
    function checkRequired(inputClass) {
        var element = inputClass;
        inputClass.next('.errSignIn').remove();
        var message = 'لطفا فیلد مورد نظر را پرکنید';


        if (element.val() == '') {
            element.addClass('error');
            var errorItem = ' <label class="error errSignIn">' + message + '</label>';
            $(errorItem).insertAfter(inputClass);
            return false;
        } else {
            element.removeClass('error');
            return true;
        }
    }
    function checkRegex(inputClass, regextype) {
        var element = inputClass;


        inputClass.next('.errRegex').remove();

        var nameRegex = "", message = "";
        switch (regextype) {
            case "username":
                nameRegex = /^[\u0600-\u06FF ]{3,16}$/;
                message = 'لطفا نام  را فارسی وارد کنید ';
                break;
            case "userfamily":
                nameRegex = /^[\u0600-\u06FF ]{2,30}$/;
                message = 'لطفا نام خانوادگی را فارسی وارد کنید ';
                break;
            case "email":
                nameRegex = /^(("[\w-\s]+")|([\w-]+(?:\.[\w-]+)*)|("[\w-\s]+")([\w-]+(?:\.[\w-]+)*))(@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][0-9]\.|1[0-9]{2}\.|[0-9]{1,2}\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\]?$)/i;
                message = 'لطفا ایمیل را به درستی وارد کنید ';
                break;
            default:
                break;
        }

        if (!nameRegex.test(element.val()) && element.val().length > 0) {
            element.addClass('error');
            var errorItem = ' <label class="error errRegex">' + message + '</label>';
            $(errorItem).insertAfter(inputClass);
            return false;
        } else {
            if (checkRequired(inputClass, message)) {
                element.removeClass('error');
            }
            return true;
        }
    }
</script>


            

