            }else{
                $(".cartimg").attr('src','http://d16exz5qcbs0qr.cloudfront.net/js/workspace/slideshow/v1/queensberry/new/cart.png');  
            }
        }        
        
        function toggeleImage(myval){
            if (myval == 1){
                $(".pinImage").attr('src','http://workspace-cdn.s3.amazonaws.com/images/clientwebsite/share/pinterest-hover.png');
            }else{
                 $(".pinImage").attr('src','http://workspace-cdn.s3.amazonaws.com/images/clientwebsite/share/pinterest.png');
            }
        }
        
        function togglebuyimg(myval){
           
           if (myval == 1){
                $(".buythisimg").attr('src','http://workspace.queensberry.com/js/slideshow/queensberry/buy-hover.png');
            }else{
                 $(".buythisimg").attr('src','http://workspace.queensberry.com/js/slideshow/queensberry/buy.png');
            }
       }
       
       function toggledownloadimg(myval){
           if (myval == 1){
                $(".downloadimg").attr('src','http://d16exz5qcbs0qr.cloudfront.net/js/workspace/slideshow/v1/queensberry/down-load-hover.png');
            }else{
