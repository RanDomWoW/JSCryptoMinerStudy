    <script data-description="template for a error/success messages" id="tmp_messages" type="text/template">
        <%if(errorMessages.length > 0){%>
        <div class="msg-error mod-standard">
            <div class="mod-standard-inner">
                <div class="content">
                    <span class="gicon-action-white"></span>
                    <% errorMessages.forEach(function(message){%>
                    <p>
                        <%=message%>
                    </p>
                    <%});%>
                </div>
        </div>
        </div>
        <%}%>
        <%if(successMessages.length > 0){%>
        <div class="msg-success mod-standard">
            <div class="mod-standard-inner">
                <div class="content">
                    <span class="gicon-checkmark-white success"></span>
                    <% successMessages.forEach(function(message){%>
                    <p>
                        <%=message%>
                    </p>
                    <%});%>
                </div>
            </div>
        </div>
        <%}%>
    </script>
