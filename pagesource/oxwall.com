<style type="text/css" data-type="vc_custom-css">.hentry , .entry-content{
    margin:0;
}

.gc_row h1 , .hfeatures_col0 h2{
    font-size:2.625em;
    color:#000;
}

.gc_row {
    position:relative;
    z-index:1;
    margin-bottom:0;
}

.gc_row2 {
    position:relative;
    z-index:1;
    margin-bottom: 0;
    padding-bottom: 3em;
    padding-top: 3em;
}

.gc_row .wpb_content_element {
    margin-bottom:12px!important; 
}
.gc_col1 .vc_row {
    height:148px;
    width:45%;
    margin:0.750em 1.5em 0.750em 0;
    float:left;
    border:1px solid #ebebeb;
}

.gc_col1 .vc_row .wpb_column {
    height:148px;
    padding:0!important;
    opacity:0;
    background-color:rgba(0,0,0,0.6);
    -webkit-transition: all 0.3s linear 0s;
    -moz-transition: all 0.3s linear 0s;
    -ms-transition: all 0.3s linear 0s;
    -o-transition: all 0.3s linear 0s;
    transition: all 0.3s linear 0s;
}

.gc_col1 .gc_more .wpb_column {
    opacity:1!important;
    background-color: #dce0e3;
}

.gc_col1 .vc_row .wpb_column .wpb_text_column {
    padding-top:0.6em;
}

.gc_col1 {
    padding-right:0!important;
}

.gc_col2 .vc_row {
    margin:0.250em 0;
    height:148px;
    float: left;
}

.gc_col2 h2 {
    color:#000!important;
    padding-bottom:0.6em;
}

.gc_col2 p {
    font-size:17px;
    color:#828282;
    line-height:1.4;
}

.gc_col2 a , .hfeatures_col p a , .support_row a{
    margin-top:0.750em;
    background-color: #000;
    color: #fff;
    font-family: "hindbold";
    font-size: 0.813em;
    padding: 1.375em 2.6em;
    text-transform: uppercase;
    -webkit-transition: background 0.3s linear;
    -moz-transition: background 0.3s linear;
    -ms-transition: background 0.3s linear;
    -o-transition: background 0.3s linear;
    transition: background 0.3s linear;
}

.gc_col1 .vc_row a {
    top:50%;
    color: #000;
    font-family: "hindbold";
    background-color:#fff;
    font-size: 0.813em;
    padding: 1.375em 3em;
    text-transform: uppercase;
}

.gc_col2 a:hover , .hfeatures_col p a:hover , .support_row a:hover {
    background-color:#dce0e3;
    -webkit-transition: background 0.3s linear;
    -moz-transition: background 0.3s linear;
    -ms-transition: background 0.3s linear;
    -o-transition: background 0.3s linear;
    transition: background 0.3s linear;
}

.gc_col1 .vc_row:hover .wpb_column {
    opacity:1;
}

.h_features {
    position:relative;
    z-index:1;
    overflow:hidden;
    padding-top: 4.2em;
    padding-bottom: 4.5em; /*was 4em*/
    margin-bottom:0!important;
}

.hfeatures_col {
    background-color:#fff;
    width:32%;
    padding:2em;
    border: 1px solid #dce0e3;
    border-top: 3px solid #ef9411;
}

.hfeatures_col ul li{
    margin:0.875em 0;
    color:#828282;
}

.hfeatures_col ul li:first-child {
    margin-top:0;
}

.hfeatures_col ul li a {
    color:#ef9411;
    text-decoration:underline;
}

.hfeatures_col ul {
    margin:0!important;
    padding-left: 16px;
}

.hfeatures_col p{
    margin-bottom: 16px !important;
    margin-top: 42px;
}

.hfeatures_col0 h2 {
    margin:0;
}

.hfeatures_col0 p , .hfeatures_col0 i{
    line-height: 0.8;
    margin-left: -6px;
    margin-top: 0;
    margin-bottom:0;
    color: #edf1f3;
    font-family: hindbold;
    font-size: 104px;
    font-style:normal;
}

.hfeatures_col0 i{
    letter-spacing:3px;
    margin-left:0!important;
}

.support_row {
    position:relative;
    z-index:1;
    margin-bottom:420px;
    padding-top: 1em;
}

.support_row h2, .support_row h3{
    color:#000;
}

.support_row h2 {
    font-size:2.625em;
}

.support_row h3 {
    font-size:24px;
}

.support_row p , .support_row h2 , .support_row h3 {
    margin-top:12px;
    margin-bottom:12px;
}

.support_row p {
    color:#828282;
    line-height:1.4;
}
    
.support_row i {
    color:#ef9411;
}
body .support_row .support_col1>.vc_column-inner {
    background-position:bottom center!important;
}

.support_col2 {
    padding:2em;
}

.support_col2 hr{
    background-color: #ef9411;
    height: 3px;
    width:7%;
    margin-left:0;
}

.dots_row {
    height: 480px;
    position: absolute;
    top: -36px;
    width: 1024px;
}

.cta_row {
    width:100%;
    z-index:0;
    position:fixed;
    margin-bottom:0;
    overflow:hidden;
    height:420px;
    bottom:0;
}

.cta_row .wpb_text_column h2{
    font-size:2.625em;
    color:#fff;
    text-transform:uppercase;
    margin-bottom:1em!important;
    line-height:1.3;
}

.cta_row a{
    color: #000;
    font-family: "hindbold";
    background-color:#fff;
    font-size: 0.813em;
    padding: 1.375em 3em;
    text-transform: uppercase;
    -webkit-transition: background 0.3s linear;
    -moz-transition: background 0.3s linear;
    -ms-transition: background 0.3s linear;
    -o-transition: background 0.3s linear;
    transition: background 0.3s linear;
}

.cta_row a:hover{
    background-color:#ef9411;
    color:#fff;
    -webkit-transition: background 0.3s linear;
    -moz-transition: background 0.3s linear;
    -ms-transition: background 0.3s linear;
    -o-transition: background 0.3s linear;
    transition: background 0.3s linear;
}

.cta_footer p{
    text-transform:uppercase;
    padding-bottom:12px;
    font-size:0.8em;
}

