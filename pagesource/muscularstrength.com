$(function() {
  var a = $("#scott_menu");
  var b = a.children("li");
  var c = $("#scott_overlay");
  b.bind("click mouseenter", function() {
    var a = $(this);
    a.addClass("slided selected");
    a.children("div").css("z-index", "9999").stop(true, true).slideDown(100, function() {
      b.not(".slided").children("div").hide();
      a.removeClass("slided")
    })
  }).bind("mouseleave", function() {
    var a = $(this);
    a.removeClass("selected").children("div").css("z-index", "1")
  });
  a.bind("mouseenter", function() {
    var a = $(this);
    c.stop(true, true).slideUp(200, .6);
    a.addClass("hovered")
  }).bind("mouseleave dblclick", function() {
    var a = $(this);
    a.removeClass("hovered");
    c.stop(true, true).slideUp(200, 0);
    b.children("div").hide()
  })
})
</script>
