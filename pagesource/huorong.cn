			window.onresize=resizeBannerImage;//当窗口改变宽度时执行此函数
			function resizeBannerImage()
			   {
			    var winW =  document.body.offsetWidth
			     if( $(window).width() > 0 ) {
			        /* 这里是要执行的代码 */
			        $(".phoness").css('width',winW+'px')
			        $(".yx-rotaion").css('width',winW+'px')
			    }
			   }
			   $(".yx-rotaion").yx_rotaion({auto:true});

			 // 下载箭头的变化
			 $('.downs').hover(function(){
			 	$('.block').css("color","#ff9600");
				$('.downs .ico').css("backgroundPosition",-8);
			 },function(){
			 	$('.block').css("color","#ffaa00");
				$('.downs .ico').css("backgroundPosition",0);
			 })	
			  $('.trys').hover(function(){
			 	$('.try').css("color","#ff9600");
				$('.trys .ico').css("backgroundPosition",-8);
			 },function(){
			 	$('.try').css("color","#ffaa00");
				$('.trys .ico').css("backgroundPosition",0);
			 })	
			  $('.enters').hover(function(){
			 	$('.user').css("color","#ff9600");
				$('.enters .ico').css("backgroundPosition",-8);
			 },function(){
			 	$('.user').css("color","#ffaa00");
				$('.enters .ico').css("backgroundPosition",0);
			 })	
			  $('.downs').click(function(){
			   	$('.block').css("color","#f17f05");
			   	$('.downs .ico').css("backgroundPosition",-16);
			 })
			  $('.trys').click(function(){
			    $('.try').css("color","#f17f05");
				$('.trys .ico').css("backgroundPosition",-16);
			 })
			   $('.enters').click(function(){
			   	$('.user').css("color","#f17f05");
				$('.enters .ico').css("backgroundPosition",-16);
			   })

			// 主页内容滑动转换
			$(".showS ul li a").hover(function(){
				$(this).css('color','#fa0');
				$(this).prev().css("backgroundPosition",-6);
			},function(){
				$(this).css('color','#7a7a7a');
				$(this).prev().css("backgroundPosition",0);
			})
		}); 
    </script>
