<script type="text/javascript">
    var $mainVideo = null;
    var $slideoutContent = null;

    function scrollParallax() {
        if (null !== $mainVideo) {
            var $videoHeight = $('#headerVideo').height();
            var probe = 85 - $header.height();
            if ($slideoutContent.hasClass('in')) {
                probe += $slideoutContent.height();
            }
            switch (true) {
                case ($window.scrollTop() <= probe):
                    $mainVideo.css('transform', 'translate3d(0px,0px,0px)');
                    break;
                case ($window.scrollTop() > probe + $videoHeight):
                    $mainVideo.css('transform', 'translate3d(0px,' + $videoHeight + 'px,0px)');
                    break;
                default:
                    $mainVideo.css('transform', 'translate3d(0px,' + (($window.scrollTop() - probe) * .5) + 'px,0px)');
            }
        }
    }

    function resetmainVideo() {
        if (null !== $mainVideo) {
            $mainVideo.css('transform', 'translate3d(0px,0px,0px)');
        }
    }


    function renderVideoButton() {
        if (!!window.hasVideo) {
            var $videoWrapper = $('#video-wrapper');
            if (mainVideo.paused) {
                $videoWrapper.addClass('paused');
            } else {
                $videoWrapper.removeClass('paused')
            }
        }
    }


    var $videoProbeUpper;
    var $videoProbeLower;

    function startStopVideo() {
        if (!!window.hasVideo) {
            if (!!window.videoPopup) {
                if (!mainVideo.paused) {
                    mainVideo.pause();
                }
            } else {
                var scrollTop = $window.scrollTop();
                var upper = $videoProbeUpper.offset().top - scrollTop;
                var lower = $videoProbeLower.offset().top - scrollTop;
                var windowWidth = window.getRealWidth();
                var videoInViewport = (upper < $window.height() && lower > headerMin);
                if (mainVideo.paused && !window.pausedOnDemand) {
                    if (windowWidth > 767 && videoInViewport) {
                        mainVideo.play();
                        renderVideoButton();
                    }
                } else {
                    if (windowWidth <= 767 || !videoInViewport) {
                        mainVideo.pause();
                        renderVideoButton();
                    }
                }
                switch (true) {
                    case (windowWidth <= 767 && true === window.videoVisible):
                        $mainVideo.css('display', 'none');
                        window.videoVisible = false;
                        break;
                    case (windowWidth > 767 && false === window.videoVisible):
                        $mainVideo.css('display', 'block');
                        window.videoVisible = true;
                        break;
                }
            }
        }
    }

    var $bluerow = null;

    function buttonsSticky() {
        if (null !== $bluerow && window.getRealWidth() > 991) {
            var $scrollTop = $window.scrollTop();
            var probe = parseFloat($("#video-wrapper").css('padding-top')) + 30;
            var $slideoutContentHeight = $slideoutContent.height();
            var probe2 = 1400 + $slideoutContentHeight;
            if ($slideoutContent.hasClass('in')) {
                probe += $slideoutContentHeight;
            }
            switch (true) {
                case ($scrollTop <= probe):
                    $('#buttons-placeholder').css({display: 'none'});
                    $bluerow.removeClass('affix affix-trans')
                        .removeClass('unsticked');
                    break;
                case ($scrollTop > probe && $scrollTop <= probe2):
                    $('#buttons-placeholder').css({display: 'block'});
                    if ($bluerow.hasClass('unsticked')) {
                        $bluerow.addClass('affix affix-trans')
                            .removeClass('unsticked');
                    } else {
                        $bluerow.addClass('affix');
                    }
                    break;
                case ($scrollTop > probe2):
                    $bluerow.addClass('unsticked');
                    break;
            }
        }
    }

    function showSideBar() {
        var scrollTop = $window.scrollTop();
        var $frontpageSharebox = $('#frontpage-sharebox');

        if (scrollTop > 50 && window.getRealWidth() > 767) {
            $frontpageSharebox.animate({'right': '0px'}, 300);
        } else {
            $frontpageSharebox.stop(true).animate({'right': '-200px'}, 100);
        }
    }

    function doOnResize() {
        var windowWidth = window.getRealWidth();

        $window.off('scroll', scrollParallax);
        $window.off('scroll', showSideBar);
        if (windowWidth > 767) {
            $window.on('scroll', scrollParallax);
            $window.on('scroll', showSideBar);
        } else {
            resetmainVideo();
        }

        showSideBar();
        startStopVideo();

        $slideoutContent.off('shown.bs.collapse', scrollParallax);
        $slideoutContent.off('hidden.bs.collapse', scrollParallax);
        $slideoutContent.off('shown.bs.collapse', startStopVideo);
        $slideoutContent.off('hidden.bs.collapse', startStopVideo);
        $slideoutContent.off('show.bs.collapse', resetmainVideo);
        $slideoutContent.off('hide.bs.collapse', resetmainVideo);
        if ('function' === typeof onMapLoad) {
            $slideoutContent.off('show.bs.collapse', onMapLoad);
        }
        if (windowWidth > 767) {
            $slideoutContent.on('shown.bs.collapse', scrollParallax);
            $slideoutContent.on('hidden.bs.collapse', scrollParallax);
            $slideoutContent.on('shown.bs.collapse', startStopVideo);
            $slideoutContent.on('hidden.bs.collapse', startStopVideo);
            $slideoutContent.on('show.bs.collapse', resetmainVideo);
            $slideoutContent.on('hide.bs.collapse', resetmainVideo);
            if ('function' === typeof onMapLoad) {
                $slideoutContent.on('show.bs.collapse', onMapLoad);
            }
        }

        $window.off('scroll', buttonsSticky);
        if (windowWidth > 991) {
            $window.on('scroll', buttonsSticky);
        } else {
            $bluerow.addClass('unsticked');
        }

        $window.off('scroll', startStopVideo);
        if (windowWidth > 767) {
            $window.on('scroll', startStopVideo);
        }
    }

